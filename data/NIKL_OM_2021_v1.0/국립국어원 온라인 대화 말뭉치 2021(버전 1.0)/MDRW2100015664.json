      "utterance": [
        {
          "id": "MDRW2100015664.1.1",
          "form": "벌써? 이거 되게 핫한 대화다 ㅋㅋㅋㅋ",
          "original_form": "벌써? 이거 되게 핫한 대화다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015664.1.2",
          "form": "없지",
          "original_form": "없지",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015664.1.3",
          "form": "일단 다들 연애부터 하세요",
          "original_form": "일단 다들 연애부터 하세요",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015664.1.4",
          "form": "근데 내친구는 자기 동기가 벌써 결혼한애도 있더라",
          "original_form": "근데 내친구는 자기 동기가 벌써 결혼한애도 있더라",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015664.1.5",
          "form": "마져",
          "original_form": "마져",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015664.1.6",
          "form": "결혼식 가고 난리나던데",
          "original_form": "결혼식 가고 난리나던데",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015664.1.7",
          "form": "언니도 내년이면 결혼하는 사람 있을것같아",
          "original_form": "언니도 내년이면 결혼하는 사람 있을것같아",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015664.1.8",
          "form": "없어.... 친구가....",
          "original_form": "없어.... 친구가....",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.9",
          "form": "친구가 잇어야.....",
          "original_form": "친구가 잇어야.....",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.10",
          "form": "언니 주변에 연애하는 사람 많",
          "original_form": "언니 주변에 연애하는 사람 많",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.12",
          "form": "그리고 주변 name3.name4",
          "original_form": "그리고 주변 &name3&.&name4&",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.13",
          "form": "개가 결혼을 하지..",
          "original_form": "개가 결혼을 하지..",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.14",
          "form": "응 실버타운 예약이요",
          "original_form": "응 실버타운 예약이요",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.15",
          "form": "나는 사촌언니 결혼한거 보고",
          "original_form": "나는 사촌언니 결혼한거 보고",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.16",
          "form": "결혼은 무엇인가",
          "original_form": "결혼은 무엇인가",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.17",
          "form": "하고싶다가도 끔찍해보이는",
          "original_form": "하고싶다가도 끔찍해보이는",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.18",
          "form": "긍정? 부정?",
          "original_form": "긍정? 부정?",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015664.1.19",
          "form": "혼자.살고.싶어.지는",
          "original_form": "혼자.살고.싶어.지는",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015664.1.20",
          "form": "긍정 한 5퍼",
          "original_form": "긍정 한 5퍼",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015664.1.21",
          "form": "호오.....",
          "original_form": "호오.....",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015664.1.22",
          "form": "비혼이 답인가",
          "original_form": "비혼이 답인가",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015664.1.23",
          "form": "주말에 둘이서 차끌고 놀러다니는거",
          "original_form": "주말에 둘이서 차끌고 놀러다니는거",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015664.1.24",
          "form": "부러운데",
          "original_form": "부러운데",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015664.1.25",
          "form": "저것도 성향이 맞아야 좋지",
          "original_form": "저것도 성향이 맞아야 좋지",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015664.1.26",
          "form": "한쪽은 쉬고싶다? 이러면 바로 싸우는거",
          "original_form": "한쪽은 쉬고싶다? 이러면 바로 싸우는거",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015664.1.27",
          "form": "그리고 상대 집안까지 신경써야하잖아",
          "original_form": "그리고 상대 집안까지 신경써야하잖아",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015664.1.28",
          "form": "집안이야 알아보기 힘들다 쳐도",
          "original_form": "집안이야 알아보기 힘들다 쳐도",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015664.1.29",
          "form": "그냥 친구 많이 사귀어서 주말에 한명씩 만나서 노는게 답인듯",
          "original_form": "그냥 친구 많이 사귀어서 주말에 한명씩 만나서 노는게 답인듯",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015664.1.30",
          "form": "우오우",
          "original_form": "우오우",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015664.1.31",
          "form": "친구",
          "original_form": "친구",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015664.1.32",
          "form": "지인을 통해서, 동호회 정도인듯",
          "original_form": "지인을 통해서, 동호회 정도인듯",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015664.1.33",
          "form": "뭔가 확실히 깊은 관계는 아니여도 친구사귈필요성 느끼는중",
          "original_form": "뭔가 확실히 깊은 관계는 아니여도 친구사귈필요성 느끼는중",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015664.1.34",
          "form": "그러니까 서로 부지런하게 친구만들어서 서로 소개해주자",
          "original_form": "그러니까 서로 부지런하게 친구만들어서 서로 소개해주자",
          "speaker_id": "1",
          "time": "20210826 00:11"
        }
      ]
