        {
          "id": "MDRW2100015654.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100015654.1.2",
          "form": "성형에 관심있어?",
          "original_form": "성형에 관심있어?",
          "speaker_id": "1",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015654.1.3",
          "form": "아무래도 관심이 가긴해",
          "original_form": "아무래도 관심이 가긴해",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015654.1.4",
          "form": "나도 성형에 관심이있어",
          "original_form": "나도 성형에 관심이있어",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015654.1.5",
          "form": "어떤 성형?",
          "original_form": "어떤 성형?",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015654.1.6",
          "form": "이마!",
          "original_form": "이마!",
          "speaker_id": "1",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015654.1.7",
          "form": "아 이마에 필러 넣는거 아프다던데",
          "original_form": "아 이마에 필러 넣는거 아프다던데",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015654.1.8",
          "form": "아 정말? 이마가 뚜렷하면 머리묶을때 이뻐보여",
          "original_form": "아 정말? 이마가 뚜렷하면 머리묶을때 이뻐보여",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015654.1.9",
          "form": "나는 성형도 성형이지만 다이어트부터 하고싶더라",
          "original_form": "나는 성형도 성형이지만 다이어트부터 하고싶더라",
          "speaker_id": "2",
          "time": "20210826 01:40"
        },
        {
          "id": "MDRW2100015654.1.10",
          "form": "맞아 최고의 성형은 다이어트래",
          "original_form": "맞아 최고의 성형은 다이어트래",
          "speaker_id": "1",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015654.1.11",
          "form": "ㅋㅋㅋㅋ맞는말이다",
          "original_form": "ㅋㅋㅋㅋ맞는말이다",
          "speaker_id": "2",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015654.1.12",
          "form": "ㅋㅋㅋㅋㅋ다이어트하면 턱선이 돋보여서 이뻐보이더라",
          "original_form": "ㅋㅋㅋㅋㅋ다이어트하면 턱선이 돋보여서 이뻐보이더라",
          "speaker_id": "1",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015654.1.13",
          "form": "맞아 그리고 다이어트하면 인상부터도 다른것같아",
          "original_form": "맞아 그리고 다이어트하면 인상부터도 다른것같아",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015654.1.14",
          "form": "맞아 연예인도 다이어트하면 더 이뻐보이더라",
          "original_form": "맞아 연예인도 다이어트하면 더 이뻐보이더라",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015654.1.15",
          "form": "역시 다이어트가 답인가봐",
          "original_form": "역시 다이어트가 답인가봐",
          "speaker_id": "2",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015654.1.16",
          "form": "다이어트 시작해보자",
          "original_form": "다이어트 시작해보자",
          "speaker_id": "1",
          "time": "20210826 01:44"
        }
      ]
