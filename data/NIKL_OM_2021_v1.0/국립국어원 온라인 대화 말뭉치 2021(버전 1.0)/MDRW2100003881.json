      "utterance": [
        {
          "id": "MDRW2100003881.1.1",
          "form": "ㅎㅎ 그래야 우리가 밖에 나가지",
          "original_form": "ㅎㅎ 그래야 우리가 밖에 나가지",
          "speaker_id": "2",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003881.1.2",
          "form": "오오 태양아 솟아 올라라",
          "original_form": "오오 태양아 솟아 올라라",
          "speaker_id": "1",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003881.1.3",
          "form": "해 떠야 쾌적한데",
          "original_form": "해 떠야 쾌적한데",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003881.1.4",
          "form": "오전내내 어두컴컴해서 밤 같더니",
          "original_form": "오전내내 어두컴컴해서 밤 같더니",
          "speaker_id": "1",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003881.1.5",
          "form": "이제 해뜨는 것 같네 ㅎㅎ",
          "original_form": "이제 해뜨는 것 같네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003881.1.6",
          "form": "오오 과연 포장을 쉽게 하러 갈 수 있을 것인가",
          "original_form": "오오 과연 포장을 쉽게 하러 갈 수 있을 것인가",
          "speaker_id": "1",
          "time": "20210821 13:15"
        },
        {
          "id": "MDRW2100003881.1.7",
          "form": "땅바닥까지 다 말라야 애들도 운동시킬텐데",
          "original_form": "땅바닥까지 다 말라야 애들도 운동시킬텐데",
          "speaker_id": "2",
          "time": "20210821 13:15"
        },
        {
          "id": "MDRW2100003881.1.8",
          "form": "아마 땅까지 다 마르진 않을듯",
          "original_form": "아마 땅까지 다 마르진 않을듯",
          "speaker_id": "1",
          "time": "20210821 13:15"
        },
        {
          "id": "MDRW2100003881.1.9",
          "form": "그래도 비 그치면 좋겠다",
          "original_form": "그래도 비 그치면 좋겠다",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003881.1.10",
          "form": "ㅇㅇ 이제 해떠도 시원한데 굳이 비가 계속 내릴 필욘 없지",
          "original_form": "ㅇㅇ 이제 해떠도 시원한데 굳이 비가 계속 내릴 필욘 없지",
          "speaker_id": "1",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003881.1.11",
          "form": "하루면 충분해",
          "original_form": "하루면 충분해",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003881.1.12",
          "form": "ㅇㅇ 가끔 중간중간 하루 정도만 왔으면",
          "original_form": "ㅇㅇ 가끔 중간중간 하루 정도만 왔으면",
          "speaker_id": "1",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003881.1.13",
          "form": "요즘 너무 시도때도 없이 자주 내려 ㅠ",
          "original_form": "요즘 너무 시도때도 없이 자주 내려 ㅠ",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003881.1.14",
          "form": "와 해 뜬 것 봐봐",
          "original_form": "와 해 뜬 것 봐봐",
