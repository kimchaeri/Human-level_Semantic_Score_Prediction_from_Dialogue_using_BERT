      "utterance": [
        {
          "id": "MDRW2100015201.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015201.1.2",
          "form": "군대에 대해서는 할말이 별로 없으시려나요..!",
          "original_form": "군대에 대해서는 할말이 별로 없으시려나요..!",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015201.1.3",
          "form": "제가 군대를 안다녀와서ㅠㅠ",
          "original_form": "제가 군대를 안다녀와서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015201.1.4",
          "form": "그래도 동생이 훈련소갔다왔어요!",
          "original_form": "그래도 동생이 훈련소갔다왔어요!",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015201.1.5",
          "form": "아하 그러시군요ㅎㅎ",
          "original_form": "아하 그러시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015201.1.6",
          "form": "동생분은 아직 군인이신거에요?",
          "original_form": "동생분은 아직 군인이신거에요?",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015201.1.7",
          "form": "훈련소갔다가 공익하고있어요",
          "original_form": "훈련소갔다가 공익하고있어요",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015201.1.8",
          "form": "name1님은 군대 다녀오셨나요?",
          "original_form": "&name1&님은 군대 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015201.1.9",
          "form": "아하 공익이시구나!",
          "original_form": "아하 공익이시구나!",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015201.1.10",
          "form": "네 저는 12월쯤에 전역했어요!ㅎㅎ",
          "original_form": "네 저는 12월쯤에 전역했어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015201.1.11",
          "form": "고생많으셨겠어요ㅠㅠ",
          "original_form": "고생많으셨겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015201.1.12",
          "form": "동생은 하필 코로나 심할때가서 훈련도 거의 못받고 씻지도 못했다고 하더라고요",
          "original_form": "동생은 하필 코로나 심할때가서 훈련도 거의 못받고 씻지도 못했다고 하더라고요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015201.1.13",
          "form": "아이구 감사합니다ㅠㅠ",
          "original_form": "아이구 감사합니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015201.1.14",
          "form": "아 코로나심할때 훈련소였으면 그럴만 했겠네요ㅠ",
          "original_form": "아 코로나심할때 훈련소였으면 그럴만 했겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015201.1.15",
          "form": "군대생활은 어떠셨어요??",
          "original_form": "군대생활은 어떠셨어요??",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015201.1.16",
          "form": "저도 군생활때 코로나때문에 휴가도 못나오고 답답하긴했어요ㅠ",
          "original_form": "저도 군생활때 코로나때문에 휴가도 못나오고 답답하긴했어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015201.1.17",
          "form": "그랬겠네요ㅠㅠ많이 불편하셨겠다",
          "original_form": "그랬겠네요ㅠㅠ많이 불편하셨겠다",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015201.1.18",
          "form": "그래도 주변 사람들이 괜찮아서 어떻ㄱ게 잘 버텼네요ㅎㅎ",
          "original_form": "그래도 주변 사람들이 괜찮아서 어떻ㄱ게 잘 버텼네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015201.1.19",
          "form": "동기나 선임이나 다 괜찮았나보네요ㅎㅎ",
          "original_form": "동기나 선임이나 다 괜찮았나보네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015201.1.20",
          "form": "듣기로는 군대에 핸드폰 사용도 된다고 하더라고요",
          "original_form": "듣기로는 군대에 핸드폰 사용도 된다고 하더라고요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015201.1.21",
          "form": "넹 다 괜찮았어서 잘 버텼어요ㅎㅎ",
          "original_form": "넹 다 괜찮았어서 잘 버텼어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015201.1.22",
          "form": "제 친구들이 갈때만 해도 안됐어가지고",
          "original_form": "제 친구들이 갈때만 해도 안됐어가지고",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015201.1.23",
          "form": "이렇게 나이먹음이 느껴지네요ㅠ",
          "original_form": "이렇게 나이먹음이 느껴지네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015201.1.24",
          "form": "요즘에 일과끝나면 핸드폰 쓸수있어서 좋더라구요!",
          "original_form": "요즘에 일과끝나면 핸드폰 쓸수있어서 좋더라구요!",
          "speaker_id": "1",
          "time": "20210825 22:26"
        }
      ]
