      "utterance": [
        {
          "id": "MDRW2100030413.1.1",
          "form": "효과가 좋은거 같긴한데",
          "original_form": "효과가 좋은거 같긴한데",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030413.1.2",
          "form": "너무 강해",
          "original_form": "너무 강해",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030413.1.3",
          "form": "그게 피곤한걸 한방에 해결해주기위해 한병에 고용량이 들어있어서 그래",
          "original_form": "그게 피곤한걸 한방에 해결해주기위해 한병에 고용량이 들어있어서 그래",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030413.1.4",
          "form": "조금 속이 쓰린거 같기도",
          "original_form": "조금 속이 쓰린거 같기도",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030413.1.5",
          "form": "그래서 밥먹고 바로 먹는게 좋대",
          "original_form": "그래서 밥먹고 바로 먹는게 좋대",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030413.1.6",
          "form": "얼마주고샀어",
          "original_form": "얼마주고샀어",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030413.1.7",
          "form": "이게 원래 엄청 비싼거거든",
          "original_form": "이게 원래 엄청 비싼거거든",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030413.1.8",
          "form": "근데 제일싼 가격보다 만원싸게샀어",
          "original_form": "근데 제일싼 가격보다 만원싸게샀어",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030413.1.9",
          "form": "싸게줬네",
          "original_form": "싸게줬네",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030413.1.10",
          "form": "어느나라거야",
          "original_form": "어느나라거야",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030413.1.11",
          "form": "독일거야",
          "original_form": "독일거야",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030413.1.12",
          "form": "나도 내일 피곤하면 먹어봐야지",
          "original_form": "나도 내일 피곤하면 먹어봐야지",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030413.1.13",
          "form": "나중에 싸게 나오면 엄마 것도 사줘",
          "original_form": "나중에 싸게 나오면 엄마 것도 사줘",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030413.1.14",
          "form": "속쓰리다며ㅋㅋㅋㅋ",
          "original_form": "속쓰리다며ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030413.1.15",
          "form": "그렇긴한데 효과가 있는거같아",
          "original_form": "그렇긴한데 효과가 있는거같아",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030413.1.16",
          "form": "덜피곤해",
          "original_form": "덜피곤해",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030413.1.17",
          "form": "아직 안먹어봤는데 기대되네",
          "original_form": "아직 안먹어봤는데 기대되네",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030413.1.18",
          "form": "사줄거야말거야",
          "original_form": "사줄거야말거야",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030413.1.19",
          "form": "알겠어ㅋㅋㅋ",
          "original_form": "알겠어ㅋㅋㅋ",
