      "utterance": [
        {
          "id": "MDRW2100021752.1.1",
          "form": "질문해줘 ㅋㅋㅋ",
          "original_form": "질문해줘 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021752.1.2",
          "form": "요즘 뭐봐?!뭐들어?!",
          "original_form": "요즘 뭐봐?!뭐들어?!",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021752.1.3",
          "form": "요즘 오월의 청춘",
          "original_form": "요즘 오월의 청춘",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021752.1.4",
          "form": "영화야?",
          "original_form": "영화야?",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021752.1.5",
          "form": "드라마야! 아마 케이비에스?",
          "original_form": "드라마야! 아마 케이비에스?",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021752.1.6",
          "form": "누구나오는건데??",
          "original_form": "누구나오는건데??",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021752.1.7",
          "form": "고민시! 금새록!",
          "original_form": "고민시! 금새록!",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021752.1.8",
          "form": "여배들 귀여워 ㅜ",
          "original_form": "여배들 귀여워 ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021752.1.9",
          "form": "언제하는거야?",
          "original_form": "언제하는거야?",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021752.1.10",
          "form": "남배는ㅋㅋ그게중요해",
          "original_form": "남배는ㅋㅋ그게중요해",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021752.1.11",
          "form": "이도현? 이상이? 일걸?? ㅋㅋㅋ 올해완결난거임 ㅋㅋ",
          "original_form": "이도현? 이상이? 일걸?? ㅋㅋㅋ 올해완결난거임 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021752.1.12",
          "form": "여배들도 사랑해줘 ㅋㅋ",
          "original_form": "여배들도 사랑해줘 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021752.1.13",
          "form": "금새록은 알아!",
          "original_form": "금새록은 알아!",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021752.1.14",
          "form": "뭔내용이야?",
          "original_form": "뭔내용이야?",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021752.1.15",
          "form": "광주항쟁 ㅜ 어제 보는데 계엄령 떨어짐 ㅜ",
          "original_form": "광주항쟁 ㅜ 어제 보는데 계엄령 떨어짐 ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021752.1.16",
          "form": "아 ㅜㅜㅜㅜ 난 못볼내용이다ㅜㅜㅜ",
          "original_form": "아 ㅜㅜㅜㅜ 난 못볼내용이다ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021752.1.17",
          "form": "듣기만해도 가슴아픔",
          "original_form": "듣기만해도 가슴아픔",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021752.1.18",
          "form": "진짜 우리나라에서 얼마 안되내용이라 생각하니 믿기지가 않음 ㅜ",
          "original_form": "진짜 우리나라에서 얼마 안되내용이라 생각하니 믿기지가 않음 ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021752.1.19",
          "form": "그러니까ㅜㅜ 왜봄ㅜㅜ 딱봐도 새드엔딩일듯",
          "original_form": "그러니까ㅜㅜ 왜봄ㅜㅜ 딱봐도 새드엔딩일듯",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021752.1.20",
          "form": "난 드라마 거의 다봐..",
          "original_form": "난 드라마 거의 다봐..",
          "speaker_id": "1",
          "time": "20210828 22:25"
        }
      ]
