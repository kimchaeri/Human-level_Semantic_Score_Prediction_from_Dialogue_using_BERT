      "utterance": [
        {
          "id": "MDRW2100022767.1.1",
          "form": "일요일은 분리수거 하는 날~~",
          "original_form": "일요일은 분리수거 하는 날~~",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022767.1.2",
          "form": "분리수거 직접 하셨나요",
          "original_form": "분리수거 직접 하셨나요",
          "speaker_id": "2",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100022767.1.3",
          "form": "넹 제가 직접해요!!",
          "original_form": "넹 제가 직접해요!!",
          "speaker_id": "1",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100022767.1.4",
          "form": "유일하게 하는 집안일",
          "original_form": "유일하게 하는 집안일",
          "speaker_id": "1",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100022767.1.5",
          "form": "가족들이 아무렇게나 모아놔서 짜증나요",
          "original_form": "가족들이 아무렇게나 모아놔서 짜증나요",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022767.1.6",
          "form": "ㅋㅋㅋㅋㅋ아 담당이신가요",
          "original_form": "ㅋㅋㅋㅋㅋ아 담당이신가요",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022767.1.7",
          "form": "주 범인이 누굴까요",
          "original_form": "주 범인이 누굴까요",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022767.1.8",
          "form": "넹 제가 라벨 제거랑 이런거 다 해서 버려야해요",
          "original_form": "넹 제가 라벨 제거랑 이런거 다 해서 버려야해요",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022767.1.9",
          "form": "엄마랑 동생이요",
          "original_form": "엄마랑 동생이요",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022767.1.10",
          "form": "ㅋㅋㅋㅋㅋ 엄마두요? 아빠는요",
          "original_form": "ㅋㅋㅋㅋㅋ 엄마두요? 아빠는요",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022767.1.11",
          "form": "아빠는 쓰레기 자체를 배출을 안해요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아빠는 쓰레기 자체를 배출을 안해요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022767.1.12",
          "form": "그리고 막 제가 비닐류 플라스틱류 나눠놓아도",
          "original_form": "그리고 막 제가 비닐류 플라스틱류 나눠놓아도",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022767.1.13",
          "form": "나중가면 또 막 다 섞어서 넣어놔요",
          "original_form": "나중가면 또 막 다 섞어서 넣어놔요",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022767.1.14",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022767.1.15",
          "form": "ㅋㅋㅋㅋㅋ 비닐 플라스틱 증말 번거롭죠",
          "original_form": "ㅋㅋㅋㅋㅋ 비닐 플라스틱 증말 번거롭죠",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022767.1.16",
          "form": "그리고 동생이 음식 잘 사다먹는데",
          "original_form": "그리고 동생이 음식 잘 사다먹는데",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022767.1.17",
          "form": "먹고나서 잘 안씻고 내다놔요",
          "original_form": "먹고나서 잘 안씻고 내다놔요",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022767.1.18",
          "form": "그럼 냄 새 나 고",
          "original_form": "그럼 냄 새 나 고",
          "speaker_id": "1",
          "time": "20210829 18:26"
        },
        {
          "id": "MDRW2100022767.1.19",
          "form": "제손에 묻기도하고요",
          "original_form": "제손에 묻기도하고요",
          "speaker_id": "1",
          "time": "20210829 18:26"
        },
        {
          "id": "MDRW2100022767.1.20",
          "form": "너무 화가 난답니더",
          "original_form": "너무 화가 난답니더",
          "speaker_id": "1",
          "time": "20210829 18:26"
        },
        {
          "id": "MDRW2100022767.1.21",
          "form": "이런이런 ㅡㅡ 혼내줍시다",
          "original_form": "이런이런 ㅡㅡ 혼내줍시다",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022767.1.22",
          "form": "못사먹게 통장 압수!",
          "original_form": "못사먹게 통장 압수!",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022767.1.23",
          "form": "정말 집안일 하나도 안하는 건 동생이에요 한대 때리고싶네요!!",
          "original_form": "정말 집안일 하나도 안하는 건 동생이에요 한대 때리고싶네요!!",
          "speaker_id": "1",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022767.1.24",
          "form": "동생은 집에서 그럼 무슨일을 맡고있나요",
          "original_form": "동생은 집에서 그럼 무슨일을 맡고있나요",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022767.1.25",
          "form": "분리수거 처리비용 청구합시다",
          "original_form": "분리수거 처리비용 청구합시다",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022767.1.26",
          "form": "게임만 쳐해댑니다",
          "original_form": "게임만 쳐해댑니다",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022767.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022767.1.28",
          "form": "아 분리수거 안되는 코팅종이도 종이에 넣어놓고",
          "original_form": "아 분리수거 안되는 코팅종이도 종이에 넣어놓고",
          "speaker_id": "1",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022767.1.29",
          "form": "어떻게 혼내주지 ㅡㅡ  !",
          "original_form": "어떻게 혼내주지 ㅡㅡ  !",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022767.1.30",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022767.1.31",
          "form": "그럼 저는 그걸 또 쓰레기통에 다시 버려야하고!!",
          "original_form": "그럼 저는 그걸 또 쓰레기통에 다시 버려야하고!!",
          "speaker_id": "1",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022767.1.32",
          "form": "에휴 !!!",
          "original_form": "에휴 !!!",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022767.1.33",
          "form": "동생한테 하라하면 안되나요",
          "original_form": "동생한테 하라하면 안되나요",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022767.1.34",
          "form": "제가 그나마 유일하게 하는거 하나라",
          "original_form": "제가 그나마 유일하게 하는거 하나라",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022767.1.35",
          "form": "뺏기지않을거에요",
          "original_form": "뺏기지않을거에요",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022767.1.36",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022767.1.37",
          "form": "동생방에 동생 쓰레기는 다 갖다 버리고싶네요 호홋",
