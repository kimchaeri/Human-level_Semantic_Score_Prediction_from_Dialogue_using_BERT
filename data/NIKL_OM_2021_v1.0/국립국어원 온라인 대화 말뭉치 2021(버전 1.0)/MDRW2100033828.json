      "utterance": [
        {
          "id": "MDRW2100033828.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 11:09"
        },
        {
          "id": "MDRW2100033828.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210825 11:10"
        },
        {
          "id": "MDRW2100033828.1.3",
          "form": "요즘 당근마켓이 유행이잖아요 중고거래 자주 하시는 편이세요?",
          "original_form": "요즘 당근마켓이 유행이잖아요 중고거래 자주 하시는 편이세요?",
          "speaker_id": "1",
          "time": "20210825 11:15"
        },
        {
          "id": "MDRW2100033828.1.4",
          "form": "전에는 자주 했는데",
          "original_form": "전에는 자주 했는데",
          "speaker_id": "2",
          "time": "20210825 11:15"
        },
        {
          "id": "MDRW2100033828.1.5",
          "form": "비매너분들 만나고나니",
          "original_form": "비매너분들 만나고나니",
          "speaker_id": "2",
          "time": "20210825 11:16"
        },
        {
          "id": "MDRW2100033828.1.6",
          "form": "요샌 안하게 되네요",
          "original_form": "요샌 안하게 되네요",
          "speaker_id": "2",
          "time": "20210825 11:16"
        },
        {
          "id": "MDRW2100033828.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 11:16"
        },
        {
          "id": "MDRW2100033828.1.8",
          "form": "많이는 안해요 팔면 표같은걸 온라인장터에 팔았어요",
          "original_form": "많이는 안해요 팔면 표같은걸 온라인장터에 팔았어요",
          "speaker_id": "1",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100033828.1.9",
          "form": "표요?",
          "original_form": "표요?",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100033828.1.10",
          "form": "주로 어떤 표요?",
          "original_form": "주로 어떤 표요?",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100033828.1.11",
          "form": "네 음악회입장권요",
          "original_form": "네 음악회입장권요",
          "speaker_id": "1",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100033828.1.12",
          "form": "샀다가 못가면 팔았어요",
          "original_form": "샀다가 못가면 팔았어요",
          "speaker_id": "1",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100033828.1.13",
          "form": "아항!",
          "original_form": "아항!",
          "speaker_id": "2",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100033828.1.14",
          "form": "당근 통해 구입도 해보셨어요?",
          "original_form": "당근 통해 구입도 해보셨어요?",
          "speaker_id": "2",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100033828.1.15",
          "form": "아뇨 당근 눈팅만 하다가 어플 지웠어요",
          "original_form": "아뇨 당근 눈팅만 하다가 어플 지웠어요",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100033828.1.16",
          "form": "요즘에 다시 들어가고 싶은데",
          "original_form": "요즘에 다시 들어가고 싶은데",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100033828.1.17",
          "form": "제 폰 용량이 적어서 못깔고있어요",
          "original_form": "제 폰 용량이 적어서 못깔고있어요",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100033828.1.18",
          "form": "당근이 동네분들이랑 거래하는거라\r\n직거래 하는것도 편하고\r\n신뢰도 가서 좋은거 같아요",
          "original_form": "당근이 동네분들이랑 거래하는거라\r\n직거래 하는것도 편하고\r\n신뢰도 가서 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210825 11:22"
        },
        {
          "id": "MDRW2100033828.1.19",
          "form": "네 얼마전에 기사도났더라구요",
          "original_form": "네 얼마전에 기사도났더라구요",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033828.1.20",
          "form": "중고나라는 못믿는 이미지고 당근은 신뢰가는 이미지라는데 저도 그렇게 느꼈어요",
          "original_form": "중고나라는 못믿는 이미지고 당근은 신뢰가는 이미지라는데 저도 그렇게 느꼈어요",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033828.1.21",
          "form": "중고나라는 사기도 많더라구요",
          "original_form": "중고나라는 사기도 많더라구요",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033828.1.22",
          "form": "네 그렇다고 들었어요",
          "original_form": "네 그렇다고 들었어요",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033828.1.23",
          "form": "어제 뭔 게시물 보다가요. 한달 수입에 중고나라 소액사기를 포함시키는 사람 봤어요. 어이가없죠",
          "original_form": "어제 뭔 게시물 보다가요. 한달 수입에 중고나라 소액사기를 포함시키는 사람 봤어요. 어이가없죠",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033828.1.24",
          "form": "헉 정말 어이가 없네요",
          "original_form": "헉 정말 어이가 없네요",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100033828.1.25",
          "form": "네 아예 딴세상 사람 같죠",
          "original_form": "네 아예 딴세상 사람 같죠",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100033828.1.26",
          "form": "그런 생각을 갖고 사는 사람들이\r\n있다니 진짜 소름이네요ㅠ",
          "original_form": "그런 생각을 갖고 사는 사람들이\r\n있다니 진짜 소름이네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 11:40"
        }
      ]
    }
  ]
