          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004105.1.2",
          "form": "아 코로나 언제풀리냐",
          "original_form": "아 코로나 언제풀리냐",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.3",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.4",
          "form": "여행가고싶다 어행",
          "original_form": "여행가고싶다 어행",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.5",
          "form": "일본가고싶다",
          "original_form": "일본가고싶다",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.6",
          "form": "4당계 좀 에바",
          "original_form": "4당계 좀 에바",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.7",
          "form": "우리이거전문분야인듯",
          "original_form": "우리이거전문분야인듯",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.8",
          "form": "제주도가고싶다",
          "original_form": "제주도가고싶다",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.9",
          "form": "majja",
          "original_form": "majja",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.10",
          "form": "호캉스가고싶다",
          "original_form": "호캉스가고싶다",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.11",
          "form": "아니 근데",
          "original_form": "아니 근데",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.12",
          "form": "4단계 왜 안풀려",
          "original_form": "4단계 왜 안풀려",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.13",
          "form": "아사쿠사 실크푸딩 존맛",
          "original_form": "아사쿠사 실크푸딩 존맛",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.14",
          "form": "백신도 다 접종하고잇자나",
          "original_form": "백신도 다 접종하고잇자나",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.15",
          "form": "이건돼?",
          "original_form": "이건돼?",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.16",
          "form": "아 진짜 ㅠ",
          "original_form": "아 진짜 ㅠ",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.17",
          "form": "존맛은 왜되냐고",
          "original_form": "존맛은 왜되냐고",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.18",
          "form": "먹고싳다",
          "original_form": "먹고싳다",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.19",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.20",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004105.1.21",
          "form": "우리동네에",
          "original_form": "우리동네에",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004105.1.22",
          "form": "쿠루미 알아?",
          "original_form": "쿠루미 알아?",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004105.1.23",
          "form": "일본식 과자점인데",
          "original_form": "일본식 과자점인데",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004105.1.24",
          "form": "빙수도 팔더라고...?",
          "original_form": "빙수도 팔더라고...?",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004105.1.25",
          "form": "심지어 밀크티 빙수도 있어..",
          "original_form": "심지어 밀크티 빙수도 있어..",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
