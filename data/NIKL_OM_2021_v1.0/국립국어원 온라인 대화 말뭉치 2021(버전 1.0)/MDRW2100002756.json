      "utterance": [
        {
          "id": "MDRW2100002756.1.1",
          "form": "결혼은 하셨나요?",
          "original_form": "결혼은 하셨나요?",
          "speaker_id": "1",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002756.1.2",
          "form": "아니요 미혼이예요 그래서 가사일 부담이 없어요",
          "original_form": "아니요 미혼이예요 그래서 가사일 부담이 없어요",
          "speaker_id": "2",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002756.1.3",
          "form": "아~ 부러워요ㅠ",
          "original_form": "아~ 부러워요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002756.1.4",
          "form": "저는 전업입니다ㅠ",
          "original_form": "저는 전업입니다ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002756.1.5",
          "form": "아이고 고생많으세요",
          "original_form": "아이고 고생많으세요",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002756.1.6",
          "form": "지금은 둘째 낮잠 타임이라 쉬고있어요. 일어나면 다시 ㅠ전쟁터로ㅋㅋ",
          "original_form": "지금은 둘째 낮잠 타임이라 쉬고있어요. 일어나면 다시 ㅠ전쟁터로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002756.1.7",
          "form": "육아전쟁이 젤  힘들지요 ㅜ.ㅜ",
          "original_form": "육아전쟁이 젤  힘들지요 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002756.1.8",
          "form": "전쟁입니다 ㅋㅋ진짜ㅠㅠ",
          "original_form": "전쟁입니다 ㅋㅋ진짜ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002756.1.9",
          "form": "그래도 자는 모습보면 흐뭇하시겠어요",
          "original_form": "그래도 자는 모습보면 흐뭇하시겠어요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002756.1.10",
          "form": "잘때가 제일 천사죠ㅋㅋㅋ",
          "original_form": "잘때가 제일 천사죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002756.1.11",
          "form": "저절로 미소가 지어질듯요",
          "original_form": "저절로 미소가 지어질듯요",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002756.1.12",
          "form": "네~ ㅎㅎ 근데 터울이 7살인데도 싸우더라구요ㅠ",
          "original_form": "네~ ㅎㅎ 근데 터울이 7살인데도 싸우더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002756.1.13",
          "form": "저희 조카 터울도 7살인데 싸워요",
          "original_form": "저희 조카 터울도 7살인데 싸워요",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002756.1.14",
          "form": "애기가 언니를 때리네요ㅠㅋㅋㅋ",
          "original_form": "애기가 언니를 때리네요ㅠㅋㅋㅋ",
