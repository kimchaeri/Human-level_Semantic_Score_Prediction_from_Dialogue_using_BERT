      "utterance": [
        {
          "id": "MDRW2100007878.1.1",
          "form": "쇼핑. 저는 시계 사고파요",
          "original_form": "쇼핑. 저는 시계 사고파요",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007878.1.2",
          "form": "손목시계? 브랜드봐놓은거 있으세요?",
          "original_form": "손목시계? 브랜드봐놓은거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007878.1.3",
          "form": "ㅜㅜ 롤렉스요",
          "original_form": "ㅜㅜ 롤렉스요",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007878.1.4",
          "form": "오우 마뉘 고급이네요",
          "original_form": "오우 마뉘 고급이네요",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007878.1.5",
          "form": "얼마정도인가요",
          "original_form": "얼마정도인가요",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007878.1.6",
          "form": "전에 있었는데 팔았더니.. 지금은 그때보다 중고가가 더 비싸요ㅋ",
          "original_form": "전에 있었는데 팔았더니.. 지금은 그때보다 중고가가 더 비싸요ㅋ",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100007878.1.7",
          "form": "그럴수도 있군요",
          "original_form": "그럴수도 있군요",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100007878.1.8",
          "form": "저는 소소하게 양우산? 샀어요 ㅋㅋ",
          "original_form": "저는 소소하게 양우산? 샀어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100007878.1.9",
          "form": "요즘은 저도 쇼핑 안해요ㅜㅜ",
          "original_form": "요즘은 저도 쇼핑 안해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100007878.1.10",
          "form": "양우산.. 이제 가을인데",
          "original_form": "양우산.. 이제 가을인데",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100007878.1.11",
          "form": "그래도 그동안은 소소하게 잘썼어요",
          "original_form": "그래도 그동안은 소소하게 잘썼어요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100007878.1.12",
          "form": "비가 왔다 안왔다 할때가 많아서 ㅋㅋ",
          "original_form": "비가 왔다 안왔다 할때가 많아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100007878.1.13",
          "form": "아 사신지 좀 됐나보네요.",
          "original_form": "아 사신지 좀 됐나보네요.",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100007878.1.14",
          "form": "음 전 최근에 산 게 진짜 없네요ㅜ",
          "original_form": "음 전 최근에 산 게 진짜 없네요ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100007878.1.15",
          "form": "저는 어제 선풍기샀어요 신일꺼로",
          "original_form": "저는 어제 선풍기샀어요 신일꺼로",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100007878.1.16",
          "form": "세일해서요 ㅋㅋ",
          "original_form": "세일해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100007878.1.17",
          "form": "오...여름 다 갔는데., 요즘은 써큘 쓰던데요",
          "original_form": "오...여름 다 갔는데., 요즘은 써큘 쓰던데요",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100007878.1.18",
          "form": "여름이 다 가서 세일했나봐요",
          "original_form": "여름이 다 가서 세일했나봐요",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100007878.1.19",
          "form": "집에 써큘 너무 시끄러워서ㅜ",
          "original_form": "집에 써큘 너무 시끄러워서ㅜ",
