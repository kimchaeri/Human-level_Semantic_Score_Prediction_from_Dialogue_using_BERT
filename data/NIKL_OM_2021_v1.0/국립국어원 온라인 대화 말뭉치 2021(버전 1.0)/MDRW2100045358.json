      "utterance": [
        {
          "id": "MDRW2100045358.1.1",
          "form": "걔네는 그래서",
          "original_form": "걔네는 그래서",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045358.1.2",
          "form": "탄산수를 2리터짜리 페트에다 팔더라구",
          "original_form": "탄산수를 2리터짜리 페트에다 팔더라구",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045358.1.3",
          "form": "나도 지형이 그렇다고 듣긴 했는데",
          "original_form": "나도 지형이 그렇다고 듣긴 했는데",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045358.1.4",
          "form": "그럼 파는 생수는?",
          "original_form": "그럼 파는 생수는?",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045358.1.5",
          "form": "독일 놀러갔는데 영어는 안써잇지",
          "original_form": "독일 놀러갔는데 영어는 안써잇지",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045358.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045358.1.7",
          "form": "둘다 목끝까지 물 채워놔서 구분 안되지",
          "original_form": "둘다 목끝까지 물 채워놔서 구분 안되지",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045358.1.8",
          "form": "그래서 마트에서 생수 사먹을때 ㄹㅇ복불복 심정으로 고름...",
          "original_form": "그래서 마트에서 생수 사먹을때 ㄹㅇ복불복 심정으로 고름...",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045358.1.9",
          "form": "ㅇㅎ.....",
          "original_form": "ㅇㅎ.....",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045358.1.10",
          "form": "살기위해 마시다",
          "original_form": "살기위해 마시다",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045358.1.11",
          "form": "마시게됬구나",
          "original_form": "마시게됬구나",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045358.1.12",
          "form": "그당시만 해도 탄산수 걸리면 쓰고 이상하다고 다 버렸엇는데",
          "original_form": "그당시만 해도 탄산수 걸리면 쓰고 이상하다고 다 버렸엇는데",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045358.1.13",
          "form": "난 탄산수 처음 본게",
          "original_form": "난 탄산수 처음 본게",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045358.1.14",
