        },
        {
          "id": "MDRW2100035779.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100035779.1.3",
          "form": "혹시 향수에 관심 잇으세여?",
          "original_form": "혹시 향수에 관심 잇으세여?",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100035779.1.4",
          "form": "예전에는 조금 관심이 있었는데, 지금은 별로 없어요ㅎㅎ",
          "original_form": "예전에는 조금 관심이 있었는데, 지금은 별로 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100035779.1.5",
          "form": "오 집에 향수 없으세여?",
          "original_form": "오 집에 향수 없으세여?",
          "speaker_id": "1",
          "time": "20210830 01:46"
        },
        {
          "id": "MDRW2100035779.1.6",
          "form": "있을것가틍ㄴ데",
          "original_form": "있을것가틍ㄴ데",
          "speaker_id": "1",
          "time": "20210830 01:47"
        },
        {
          "id": "MDRW2100035779.1.7",
          "form": "2개 있어요ㅎㅎ\r\n둘 다 오래되었네요",
          "original_form": "2개 있어요ㅎㅎ\r\n둘 다 오래되었네요",
          "speaker_id": "2",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100035779.1.8",
          "form": "오 어디껀데용?",
          "original_form": "오 어디껀데용?",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100035779.1.9",
          "form": "바디샵이였던가 거기랑 하나는 랑방이요",
          "original_form": "바디샵이였던가 거기랑 하나는 랑방이요",
          "speaker_id": "2",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100035779.1.10",
          "form": "오... 유명한것만 있으시네여",
          "original_form": "오... 유명한것만 있으시네여",
          "speaker_id": "1",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100035779.1.11",
          "form": "아 그런가요ㅎㅎ",
          "original_form": "아 그런가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100035779.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100035779.1.13",
          "form": "저희집엔 엄마가 모아둔게 많아서 쪼금씩 쓰고잇어여.\r\nㅋㅋㅋ",
          "original_form": "저희집엔 엄마가 모아둔게 많아서 쪼금씩 쓰고잇어여.\r\nㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100035779.1.14",
          "form": "슬프게도 제 돈주고사면 욕할것같아여",
          "original_form": "슬프게도 제 돈주고사면 욕할것같아여",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100035779.1.15",
          "form": "왜 욕을하죠?",
          "original_form": "왜 욕을하죠?",
          "speaker_id": "2",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100035779.1.16",
          "form": "그만큼 많아서.. 욕먹어여.ㅠ\r\n요즘 향수가 막 사고싶더라고요 ㅋㅋ\r\n근데 향수가 참 비싼것같아요",
          "original_form": "그만큼 많아서.. 욕먹어여.ㅠ\r\n요즘 향수가 막 사고싶더라고요 ㅋㅋ\r\n근데 향수가 참 비싼것같아요",
          "speaker_id": "1",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100035779.1.17",
          "form": "아 정말 많으신가봐요",
          "original_form": "아 정말 많으신가봐요",
          "speaker_id": "2",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100035779.1.18",
          "form": "맞아요 향수들이 쓰는 양에 비해서 용량도 크고 비싼거 같아요ㅜㅜ",
          "original_form": "맞아요 향수들이 쓰는 양에 비해서 용량도 크고 비싼거 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 02:06"
        },
        {
          "id": "MDRW2100035779.1.19",
          "form": "ㅋㅋㅋㅋ 자랑은 아니엇지만.. 그렇게 되엇어여..\r\n어떤사람이 말하길\r\n향수는 절때 죽지않는다\r\n이러면서 오래쓰더라고요",
          "original_form": "ㅋㅋㅋㅋ 자랑은 아니엇지만.. 그렇게 되엇어여..\r\n어떤사람이 말하길\r\n향수는 절때 죽지않는다\r\n이러면서 오래쓰더라고요",
          "speaker_id": "1",
          "time": "20210830 02:06"
        }
      ]
