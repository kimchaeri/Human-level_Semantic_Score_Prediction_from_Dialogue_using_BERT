        },
        {
          "id": "MDRW2100037257.1.2",
          "form": "유부초밥도 맛있다 ㅋㅋ",
          "original_form": "유부초밥도 맛있다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037257.1.3",
          "form": "맛있제",
          "original_form": "맛있제",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037257.1.4",
          "form": "웅웅 둘이 같이 먹으니 색다르지만 ㅋㅋ",
          "original_form": "웅웅 둘이 같이 먹으니 색다르지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037257.1.5",
          "form": "ㅋㅋ 몸에좋은거 많이 들어갔다",
          "original_form": "ㅋㅋ 몸에좋은거 많이 들어갔다",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037257.1.6",
          "form": "저녁 메뉴가 아주 든든하네 ㅎㅎ",
          "original_form": "저녁 메뉴가 아주 든든하네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037257.1.7",
          "form": "든든하게 먹어야지",
          "original_form": "든든하게 먹어야지",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037257.1.8",
          "form": "그래맞당 ㅎㅎ",
          "original_form": "그래맞당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037257.1.9",
          "form": "반찬도 먹어라",
          "original_form": "반찬도 먹어라",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037257.1.10",
          "form": "이거만 먹을래",
          "original_form": "이거만 먹을래",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
