      "utterance": [
        {
          "id": "MDRW2100040074.1.1",
          "form": "반려동물 키워본 경험 있으세요?",
          "original_form": "반려동물 키워본 경험 있으세요?",
          "speaker_id": "1",
          "time": "20210904 05:52"
        },
        {
          "id": "MDRW2100040074.1.2",
          "form": "네네~저희 지금 햄스터 키우고 있어요~",
          "original_form": "네네~저희 지금 햄스터 키우고 있어요~",
          "speaker_id": "2",
          "time": "20210905 06:40"
        },
        {
          "id": "MDRW2100040074.1.3",
          "form": "햄스터 키우는 군요",
          "original_form": "햄스터 키우는 군요",
          "speaker_id": "1",
          "time": "20210905 07:06"
        },
        {
          "id": "MDRW2100040074.1.4",
          "form": "저는 키워본적 없는데 아이들이 좋아하죠?",
          "original_form": "저는 키워본적 없는데 아이들이 좋아하죠?",
          "speaker_id": "1",
          "time": "20210905 07:06"
        },
        {
          "id": "MDRW2100040074.1.5",
          "form": "네~강아지 고양이 얘기하다 전 도저히 자신없어서",
          "original_form": "네~강아지 고양이 얘기하다 전 도저히 자신없어서",
          "speaker_id": "2",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040074.1.6",
          "form": "어릴때 키워봤던 햄스터로 합의봤어요ㅜㅋㅋㅋ",
          "original_form": "어릴때 키워봤던 햄스터로 합의봤어요ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040074.1.7",
          "form": "저는 예전에 막내딸이 강아지 키우고 싶다고 해서 강아지를 키운적이 있어요",
          "original_form": "저는 예전에 막내딸이 강아지 키우고 싶다고 해서 강아지를 키운적이 있어요",
          "speaker_id": "1",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040074.1.8",
          "form": "말티즈였죠",
          "original_form": "말티즈였죠",
          "speaker_id": "1",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040074.1.9",
          "form": "엄청 좋아했겠어요~",
          "original_form": "엄청 좋아했겠어요~",
          "speaker_id": "2",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040074.1.10",
          "form": "항상 강아지얘기해요ㅠ",
          "original_form": "항상 강아지얘기해요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040074.1.11",
          "form": "아이들은 그런거 같아요",
          "original_form": "아이들은 그런거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040074.1.12",
          "form": "솔직히 강아지 고양이가 귀엽긴 하잖아요",
          "original_form": "솔직히 강아지 고양이가 귀엽긴 하잖아요",
          "speaker_id": "1",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040074.1.13",
          "form": "그렇죠~뒷감당이 엄마몫이라ㅠ",
          "original_form": "그렇죠~뒷감당이 엄마몫이라ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100040074.1.14",
          "form": "고민되는건..",
          "original_form": "고민되는건..",
          "speaker_id": "2",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100040074.1.15",
          "form": "동감합니다",
          "original_form": "동감합니다",
          "speaker_id": "1",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040074.1.16",
          "form": "막내딸이 본인이 다 하겠다고 약속하고 데려왔는데",
          "original_form": "막내딸이 본인이 다 하겠다고 약속하고 데려왔는데",
          "speaker_id": "1",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040074.1.17",
          "form": "결국 제 몫이 되었죠",
          "original_form": "결국 제 몫이 되었죠",
          "speaker_id": "1",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040074.1.18",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040074.1.19",
          "form": "솔직히 눈에 다 보이지만..",
          "original_form": "솔직히 눈에 다 보이지만..",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040074.1.20",
          "form": "ㅡ늘 져주게 되는거같아요ㅠ",
          "original_form": "ㅡ늘 져주게 되는거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100040074.1.21",
          "form": "엄마들은 그렇죠",
          "original_form": "엄마들은 그렇죠",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040074.1.22",
          "form": "물론 키우니까 귀엽고 예쁘긴 한데",
          "original_form": "물론 키우니까 귀엽고 예쁘긴 한데",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040074.1.23",
          "form": "그냥 아이하나 더 키우는 거랑 같아요",
          "original_form": "그냥 아이하나 더 키우는 거랑 같아요",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040074.1.24",
          "form": "돈도 그렇고 정성도 그렇고",
          "original_form": "돈도 그렇고 정성도 그렇고",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040074.1.25",
          "form": "맞아요ㅠ 예쁘긴한데..엄마도 좀 편하고 싶다느뉴ㅠ",
          "original_form": "맞아요ㅠ 예쁘긴한데..엄마도 좀 편하고 싶다느뉴ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040074.1.26",
          "form": "돈도 많이들죠ㅠ",
          "original_form": "돈도 많이들죠ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040074.1.27",
          "form": "그렇죠..아이들 키우기도 벅찬데",
          "original_form": "그렇죠..아이들 키우기도 벅찬데",
          "speaker_id": "1",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100040074.1.28",
          "form": "햄스터는 손이 좀 덜가나요?",
          "original_form": "햄스터는 손이 좀 덜가나요?",
          "speaker_id": "1",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100040074.1.29",
          "form": "네네~따로 운동시키거나 하지.않아도 되서~",
          "original_form": "네네~따로 운동시키거나 하지.않아도 되서~",
          "speaker_id": "2",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100040074.1.30",
          "form": "밥잘 챙겨주고 톱밥만 갈아주면되요^^",
          "original_form": "밥잘 챙겨주고 톱밥만 갈아주면되요^^",
          "speaker_id": "2",
          "time": "20210905 07:15"
        }
      ]
