      "utterance": [
        {
          "id": "MDRW2100021715.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021715.1.2",
          "form": "네네! 요즘은 엄두가 안나서 어딜 못가지만..",
          "original_form": "네네! 요즘은 엄두가 안나서 어딜 못가지만..",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021715.1.3",
          "form": "자주 다니시나요?",
          "original_form": "자주 다니시나요?",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021715.1.4",
          "form": "요즘엔 자주 못다니죠ㅠㅠ",
          "original_form": "요즘엔 자주 못다니죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021715.1.5",
          "form": "최근엔 그냥 사는 도시 안에서만 다니네요ㅋㅋㅋ",
          "original_form": "최근엔 그냥 사는 도시 안에서만 다니네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021715.1.6",
          "form": "어디가보셨나요",
          "original_form": "어디가보셨나요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021715.1.7",
          "form": "도시를 탐방해보자 해서 저 부산 살거든요 부산의 곳곳을 둘러봤어요",
          "original_form": "도시를 탐방해보자 해서 저 부산 살거든요 부산의 곳곳을 둘러봤어요",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021715.1.8",
          "form": "다대포도 가보고",
          "original_form": "다대포도 가보고",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021715.1.9",
          "form": "기장도 가보고",
          "original_form": "기장도 가보고",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021715.1.10",
          "form": "와 부산! 부산만 돌아도 엄청 볼데많을것같아요",
          "original_form": "와 부산! 부산만 돌아도 엄청 볼데많을것같아요",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021715.1.11",
          "form": "네 제가 또 부산 살면서도 잘 안돌아다녔더니 잘 몰랐던 곳들도 많았고 ㅋㅋ",
          "original_form": "네 제가 또 부산 살면서도 잘 안돌아다녔더니 잘 몰랐던 곳들도 많았고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021715.1.12",
          "form": "저는 해동용궁사가 기억에남네요",
          "original_form": "저는 해동용궁사가 기억에남네요",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021715.1.13",
          "form": "다른 지역분들은 용궁사를 그렇게 좋아하시더라구요",
          "original_form": "다른 지역분들은 용궁사를 그렇게 좋아하시더라구요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021715.1.14",
          "form": "마자요 절벽 경치가ㅎㅎ",
          "original_form": "마자요 절벽 경치가ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021715.1.15",
          "form": "부산에 자주 와보셨어요?",
          "original_form": "부산에 자주 와보셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021715.1.16",
          "form": "네 몇번가봤습니다!",
          "original_form": "네 몇번가봤습니다!",
          "speaker_id": "1",
          "time": "20210828 22:37"
        }
      ]
