        {
          "id": "MDRW2100008486.1.2",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008486.1.3",
          "form": "진짜로요",
          "original_form": "진짜로요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008486.1.4",
          "form": "연출이 진짜 최고였어요",
          "original_form": "연출이 진짜 최고였어요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008486.1.5",
          "form": "원래 그냥 저냥 챙겨보던 사람이었는대",
          "original_form": "원래 그냥 저냥 챙겨보던 사람이었는대",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008486.1.6",
          "form": "와 생방은 느낌이 또 다르더라고요",
          "original_form": "와 생방은 느낌이 또 다르더라고요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008486.1.7",
          "form": "그쵸 앞으로 무슨 일이 일어날지 모르는 그 긴장감",
          "original_form": "그쵸 앞으로 무슨 일이 일어날지 모르는 그 긴장감",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008486.1.8",
          "form": "노래 연출까지 합쳐져서 더 소름이었어요",
          "original_form": "노래 연출까지 합쳐져서 더 소름이었어요",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008486.1.9",
          "form": "하 정말로요",
          "original_form": "하 정말로요",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008486.1.10",
          "form": "점점 퀠리티도 좋아져서",
          "original_form": "점점 퀠리티도 좋아져서",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008486.1.11",
          "form": "대단하다 생각했어요",
          "original_form": "대단하다 생각했어요",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008486.1.12",
          "form": "이게 마크가 맞나?? 하고",
          "original_form": "이게 마크가 맞나?? 하고",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008486.1.13",
          "form": "분명 마크인데 캐릭터 얼굴에 표정이 막 있는듯한 느낌까지",
          "original_form": "분명 마크인데 캐릭터 얼굴에 표정이 막 있는듯한 느낌까지",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008486.1.14",
          "form": "최근 양잠시만 해도 음원까지 좋아서 완전 영화 보는 것 같아요",
          "original_form": "최근 양잠시만 해도 음원까지 좋아서 완전 영화 보는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008486.1.15",
          "form": "맞아요 ㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008486.1.16",
          "form": "양잠시는 진짜 노래가 엄청 좋았죠",
          "original_form": "양잠시는 진짜 노래가 엄청 좋았죠",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008486.1.17",
          "form": "저 음원 기다리고있잖아요",
          "original_form": "저 음원 기다리고있잖아요",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008486.1.18",
          "form": "저도 존버하고 있습니다!! ㅋㅋㅋㅋ",
          "original_form": "저도 존버하고 있습니다!! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008486.1.19",
          "form": "마크 컨텐츠 중에 어떤 걸 제일 좋아하시나요??",
          "original_form": "마크 컨텐츠 중에 어떤 걸 제일 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008486.1.20",
          "form": "흠.. 고민...",
          "original_form": "흠.. 고민...",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008486.1.21",
          "form": "저는 역시.. 마음의 고장 우정리...",
          "original_form": "저는 역시.. 마음의 고장 우정리...",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008486.1.22",
          "form": "너무 따뜻하고.. 과몰입했어요 ㅠㅠ",
          "original_form": "너무 따뜻하고.. 과몰입했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008486.1.23",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008486.1.24",
          "form": "저는 자매꽃이요!",
          "original_form": "저는 자매꽃이요!",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008486.1.25",
          "form": "화기애애한 분위기에서 느껴지는 기이함이 정말 인상깊었어요",
          "original_form": "화기애애한 분위기에서 느껴지는 기이함이 정말 인상깊었어요",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008486.1.26",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008486.1.27",
          "form": "자매꽃 저는 너무 매웠어요 ㅋㅋㅋㅋ",
          "original_form": "자매꽃 저는 너무 매웠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008486.1.28",
          "form": "쫄보라서 엄청 겁먹으면서 봤죠",
          "original_form": "쫄보라서 엄청 겁먹으면서 봤죠",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008486.1.29",
          "form": "반전요소 있는게 자빱컨텐츠 매력이죠",
          "original_form": "반전요소 있는게 자빱컨텐츠 매력이죠",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008486.1.30",
          "form": "빱라임씬도 보셨나요?",
          "original_form": "빱라임씬도 보셨나요?",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
