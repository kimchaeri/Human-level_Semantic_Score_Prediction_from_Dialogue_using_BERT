      },
      "utterance": [
        {
          "id": "MDRW2100028313.1.1",
          "form": "나 스파게티 먹으러 가고싶어.",
          "original_form": "나 스파게티 먹으러 가고싶어.",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028313.1.2",
          "form": "아 배고파 난 연어김밥",
          "original_form": "아 배고파 난 연어김밥",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028313.1.3",
          "form": "아사할것같아",
          "original_form": "아사할것같아",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028313.1.4",
          "form": "아사라니 너무 전문적이야.",
          "original_form": "아사라니 너무 전문적이야.",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028313.1.5",
          "form": "그리고 이제 고등어 먹었어",
          "original_form": "그리고 이제 고등어 먹었어",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100028313.1.6",
          "form": "난 오늘 저녁으로 룸메랑 떡볶이 먹었지.",
          "original_form": "난 오늘 저녁으로 룸메랑 떡볶이 먹었지.",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100028313.1.7",
          "form": "어디 떡볶이먹었는데",
          "original_form": "어디 떡볶이먹었는데",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100028313.1.8",
          "form": "엽떡 먹었어. 다 못먹음.",
          "original_form": "엽떡 먹었어. 다 못먹음.",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100028313.1.9",
          "form": "오 엽떡 맵지않나",
          "original_form": "오 엽떡 맵지않나",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100028313.1.10",
          "form": "순한맛으로 먹었지 당연히",
          "original_form": "순한맛으로 먹었지 당연히",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100028313.1.11",
          "form": "순한맛은 별로 안맵나?",
          "original_form": "순한맛은 별로 안맵나?",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100028313.1.12",
          "form": "응 걍 떡볶이맛이지 머.",
          "original_form": "응 걍 떡볶이맛이지 머.",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100028313.1.13",
          "form": "넌 무슨 떡볶이가 제일 좋아?",
          "original_form": "넌 무슨 떡볶이가 제일 좋아?",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100028313.1.14",
          "form": "난 그냥 떡볶이가 조아. 심플이즈 베스트. 하지만 궁중떡볶이도 조아하지.",
          "original_form": "난 그냥 떡볶이가 조아. 심플이즈 베스트. 하지만 궁중떡볶이도 조아하지.",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100028313.1.15",
          "form": "궁중떡볶이도 맛있지",
          "original_form": "궁중떡볶이도 맛있지",
          "speaker_id": "1",
          "time": "20210905 22:22"
        }
      ]
