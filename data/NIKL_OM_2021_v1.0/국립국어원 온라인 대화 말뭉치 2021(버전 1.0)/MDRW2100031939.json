      "utterance": [
        {
          "id": "MDRW2100031939.1.1",
          "form": "학창시절 전공은 뭐 공부하셨어요?",
          "original_form": "학창시절 전공은 뭐 공부하셨어요?",
          "speaker_id": "2",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100031939.1.2",
          "form": "컴퓨터공학이요 ㅎㅎ",
          "original_form": "컴퓨터공학이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:54"
        },
        {
          "id": "MDRW2100031939.1.3",
          "form": "아... 완전 어렵지않나요?",
          "original_form": "아... 완전 어렵지않나요?",
          "speaker_id": "2",
          "time": "20210905 17:55"
        },
        {
          "id": "MDRW2100031939.1.4",
          "form": "뭐... 하다보면 다 되요 ㅎㅎ",
          "original_form": "뭐... 하다보면 다 되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100031939.1.5",
          "form": "ㅋㅋ 그럼 공대다니신거죠?",
          "original_form": "ㅋㅋ 그럼 공대다니신거죠?",
          "speaker_id": "2",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100031939.1.6",
          "form": "네 공대요 ㅎㅎ",
          "original_form": "네 공대요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100031939.1.7",
          "form": "전 여대ㅋㅋ",
          "original_form": "전 여대ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100031939.1.8",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100031939.1.9",
          "form": "그럼 대학때 친구 여자들만 있겠네요?",
          "original_form": "그럼 대학때 친구 여자들만 있겠네요?",
          "speaker_id": "1",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100031939.1.10",
          "form": "네... ㅋㅋ동기를 군대보내보고싶었어요ㅋㅋ",
          "original_form": "네... ㅋㅋ동기를 군대보내보고싶었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100031939.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:08"
        },
        {
          "id": "MDRW2100031939.1.12",
          "form": "그럼 학교다닐때 씨씨도 못해보네요ㅜㅜ",
          "original_form": "그럼 학교다닐때 씨씨도 못해보네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 18:08"
        },
        {
          "id": "MDRW2100031939.1.13",
          "form": "ㅋㅋ학교엔 교수님과 청소아저씨들만이ㅋㅋ",
          "original_form": "ㅋㅋ학교엔 교수님과 청소아저씨들만이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:09"
        },
        {
          "id": "MDRW2100031939.1.14",
          "form": "공대나 다를바 없네요",
          "original_form": "공대나 다를바 없네요",
          "speaker_id": "1",
          "time": "20210905 18:10"
        },
        {
          "id": "MDRW2100031939.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:10"
        },
        {
          "id": "MDRW2100031939.1.16",
          "form": "ㅋㅋ 그래도 나름 복전도 하고 후회없는 대학생활이었어요",
          "original_form": "ㅋㅋ 그래도 나름 복전도 하고 후회없는 대학생활이었어요",
          "speaker_id": "2",
          "time": "20210905 18:12"
        },
        {
          "id": "MDRW2100031939.1.17",
          "form": "후회없이 보내셨음 된거죠 ㅋㅋ",
          "original_form": "후회없이 보내셨음 된거죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100031939.1.18",
          "form": "전 대학때 제대로 연애 못해본게 아쉽네요?ㅋㅋㅋ",
          "original_form": "전 대학때 제대로 연애 못해본게 아쉽네요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100031939.1.19",
          "form": "하긴 저도 지금 연애는 안부러운데 풋풋한 연애 부러워요",
          "original_form": "하긴 저도 지금 연애는 안부러운데 풋풋한 연애 부러워요",
          "speaker_id": "2",
          "time": "20210905 18:19"
        },
        {
          "id": "MDRW2100031939.1.20",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100031939.1.21",
          "form": "그때만 가능한",
          "original_form": "그때만 가능한",
