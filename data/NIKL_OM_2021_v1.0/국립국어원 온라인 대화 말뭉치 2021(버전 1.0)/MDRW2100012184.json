      "utterance": [
        {
          "id": "MDRW2100012184.1.1",
          "form": "하루종일 비가 내려요",
          "original_form": "하루종일 비가 내려요",
          "speaker_id": "2",
          "time": "20210824 20:06"
        },
        {
          "id": "MDRW2100012184.1.2",
          "form": "날씨가 진짜 이상하네요",
          "original_form": "날씨가 진짜 이상하네요",
          "speaker_id": "1",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012184.1.3",
          "form": "가을장마라는데 그런것도 있었나요ㅋㅋ",
          "original_form": "가을장마라는데 그런것도 있었나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012184.1.4",
          "form": "기후위기 때문에 새로 생긴듯해요",
          "original_form": "기후위기 때문에 새로 생긴듯해요",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012184.1.5",
          "form": "글고 이건 장마도 아니고 무슨 동남아 스콜이죠",
          "original_form": "글고 이건 장마도 아니고 무슨 동남아 스콜이죠",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012184.1.6",
          "form": "무섭게 내리더라고요",
          "original_form": "무섭게 내리더라고요",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012184.1.7",
          "form": "그죠 오늘 밖에 나왔는데 비는 그쳤는데 습기가 훅",
          "original_form": "그죠 오늘 밖에 나왔는데 비는 그쳤는데 습기가 훅",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012184.1.8",
          "form": "진짜 동남아인줄 알았어요",
          "original_form": "진짜 동남아인줄 알았어요",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012184.1.9",
          "form": "습기때문에 끄은적끄은적해서",
          "original_form": "습기때문에 끄은적끄은적해서",
          "speaker_id": "2",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100012184.1.10",
          "form": "덥게 느껴졌어요",
          "original_form": "덥게 느껴졌어요",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012184.1.11",
          "form": "습기 정말~~",
          "original_form": "습기 정말~~",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012184.1.12",
          "form": "이런날엔 보일라 잠깐 틀면 뽀소옹합니다",
          "original_form": "이런날엔 보일라 잠깐 틀면 뽀소옹합니다",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012184.1.13",
          "form": "오 그렇네요",
          "original_form": "오 그렇네요",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012184.1.14",
          "form": "왠지 뜨거운걸 틀기 싫어서 창문열고 바람을 맞고 있었어요",
          "original_form": "왠지 뜨거운걸 틀기 싫어서 창문열고 바람을 맞고 있었어요",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012184.1.15",
          "form": "습기를 더 끌어들이고 있었네요",
          "original_form": "습기를 더 끌어들이고 있었네요",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012184.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012184.1.17",
          "form": "ㅋㅋ 한번도전해보세요 곰팡이도 안슬고 좋읍니다",
          "original_form": "ㅋㅋ 한번도전해보세요 곰팡이도 안슬고 좋읍니다",
          "speaker_id": "1",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100012184.1.18",
          "form": "네 쫌이따가 한번 지져보겠습니다",
          "original_form": "네 쫌이따가 한번 지져보겠습니다",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012184.1.19",
          "form": "요새 정말 기후위기이긴 한가봐요",
          "original_form": "요새 정말 기후위기이긴 한가봐요",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012184.1.20",
          "form": "여름 더위가 이세상 더위가 아니었죠",
          "original_form": "여름 더위가 이세상 더위가 아니었죠",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012184.1.21",
          "form": "그니까요 겨울은 또얼마나 추울지..",
          "original_form": "그니까요 겨울은 또얼마나 추울지..",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100012184.1.22",
          "form": "여름에는 이대로 쪄질수도 있겠다 싶었는데요",
          "original_form": "여름에는 이대로 쪄질수도 있겠다 싶었는데요",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100012184.1.23",
          "form": "쪄지면서 구워지고 살균되는 온도였죠...",
          "original_form": "쪄지면서 구워지고 살균되는 온도였죠...",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012184.1.24",
          "form": "하지만 이제 곧 얼음계절이 온다는걸 우린 알고있습니다",
          "original_form": "하지만 이제 곧 얼음계절이 온다는걸 우린 알고있습니다",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012184.1.25",
          "form": "ㅋㅋㅋㅋㄱ얼음왕국이에요",
          "original_form": "ㅋㅋㅋㅋㄱ얼음왕국이에요",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012184.1.26",
          "form": "name2 지역에는 눈이 많이오나요?",
          "original_form": "&name2& 지역에는 눈이 많이오나요?",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012184.1.27",
          "form": "아뇨 평균적으로 오는거 같아요 아파트단지라서 눈도 빨리 치워지는 편이구",
          "original_form": "아뇨 평균적으로 오는거 같아요 아파트단지라서 눈도 빨리 치워지는 편이구",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012184.1.28",
          "form": "name1 동네는 어떤가요?",
          "original_form": "&name1& 동네는 어떤가요?",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012184.1.29",
          "form": "ㅎㅎ저는 한 5년에 한번쯤 눈을 봐요",
          "original_form": "ㅎㅎ저는 한 5년에 한번쯤 눈을 봐요",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012184.1.30",
          "form": "눈이 부족한 곳이군요 ㅋㅋ",
          "original_form": "눈이 부족한 곳이군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012184.1.31",
          "form": "겨울에도 좀 덜 추우셨음 좋겠네요",
          "original_form": "겨울에도 좀 덜 추우셨음 좋겠네요",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012184.1.32",
          "form": "ㅋㅋ 겨울이 확실히 덜춥긴한데요",
          "original_form": "ㅋㅋ 겨울이 확실히 덜춥긴한데요",
          "speaker_id": "1",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012184.1.33",
          "form": "여기 계속있으니 이 온도에 익숙해져서 눈많이 오는동네 가면 얼어죽을것 같아요..!",
          "original_form": "여기 계속있으니 이 온도에 익숙해져서 눈많이 오는동네 가면 얼어죽을것 같아요..!",
          "speaker_id": "1",
          "time": "20210824 20:22"
        },
        {
          "id": "MDRW2100012184.1.34",
          "form": "아 이제 날씨땜에 이사 못가시겠어요(?)",
          "original_form": "아 이제 날씨땜에 이사 못가시겠어요(?)",
          "speaker_id": "2",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100012184.1.35",
          "form": "여름vs 겨울 어느게 나으신가요",
          "original_form": "여름vs 겨울 어느게 나으신가요",
          "speaker_id": "2",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012184.1.36",
          "form": "그으래도...여름이 조오금 나아요",
          "original_form": "그으래도...여름이 조오금 나아요",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012184.1.37",
          "form": "겨울에는 찬바람 맞으면 생의 의지가 없어진달까..",
          "original_form": "겨울에는 찬바람 맞으면 생의 의지가 없어진달까..",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012184.1.38",
          "form": "동지! 저도 추위에 엄청 약해요",
          "original_form": "동지! 저도 추위에 엄청 약해요",
          "speaker_id": "2",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012184.1.39",
          "form": "지난 세월이 후회된달까",
          "original_form": "지난 세월이 후회된달까",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012184.1.40",
          "form": "찬바람 휘잉하면 상스러운 욕을 해요",
          "original_form": "찬바람 휘잉하면 상스러운 욕을 해요",
          "speaker_id": "2",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012184.1.41",
          "form": "내가 왜이리살아야하나 싶고",
          "original_form": "내가 왜이리살아야하나 싶고",
          "speaker_id": "1",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012184.1.42",
          "form": "ㅋㅋㄲㅋ",
          "original_form": "ㅋㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012184.1.43",
          "form": "ㅋㅋㅋ내가.. 무슨 부귀영화를 보겠다고 이 바람을 맞으며 나가고 있나",
          "original_form": "ㅋㅋㅋ내가.. 무슨 부귀영화를 보겠다고 이 바람을 맞으며 나가고 있나",
          "speaker_id": "2",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012184.1.44",
          "form": "내가 뭐하러 이추운날에 나와서 이러고잇나..",
          "original_form": "내가 뭐하러 이추운날에 나와서 이러고잇나..",
