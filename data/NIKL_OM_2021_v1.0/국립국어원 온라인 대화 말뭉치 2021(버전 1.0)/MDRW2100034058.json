        {
          "id": "MDRW2100034058.1.1",
          "form": "학교 다닐 땨",
          "original_form": "학교 다닐 땨",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100034058.1.2",
          "form": "발표 잘하셨어요?",
          "original_form": "발표 잘하셨어요?",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100034058.1.3",
          "form": "아뇨 발표 하기 싫어했죠",
          "original_form": "아뇨 발표 하기 싫어했죠",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100034058.1.4",
          "form": "저랑 같군요...",
          "original_form": "저랑 같군요...",
          "speaker_id": "2",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100034058.1.5",
          "form": "전 싫어했는데 잘하지도 못했어요 크크..",
          "original_form": "전 싫어했는데 잘하지도 못했어요 크크..",
          "speaker_id": "2",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100034058.1.6",
          "form": "ㅋㅋㅋ잘하는 애들은 따로 있더라구요",
          "original_form": "ㅋㅋㅋ잘하는 애들은 따로 있더라구요",
          "speaker_id": "1",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100034058.1.7",
          "form": "동아리 활동 하셨나요?",
          "original_form": "동아리 활동 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100034058.1.8",
          "form": "ㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100034058.1.9",
          "form": "네 저는 옷 만드는 동아리였어요",
          "original_form": "네 저는 옷 만드는 동아리였어요",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100034058.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100034058.1.11",
          "form": "와 신기하네요. 저는 게임 동아리요ㅋㅋ",
          "original_form": "와 신기하네요. 저는 게임 동아리요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100034058.1.12",
          "form": "뜨개질 같은거 하셨나요?",
          "original_form": "뜨개질 같은거 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100034058.1.13",
          "form": "아니요 재봉틀로요ㅋㅋㅋㅋㅋ",
          "original_form": "아니요 재봉틀로요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100034058.1.14",
          "form": "게임 동아리는 뭐하나요?",
          "original_form": "게임 동아리는 뭐하나요?",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100034058.1.15",
          "form": "그냥 게임해요ㅋㅋ 학교라 좋은 게임을 돌렸던건 아니구요",
          "original_form": "그냥 게임해요ㅋㅋ 학교라 좋은 게임을 돌렸던건 아니구요",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100034058.1.16",
          "form": "아하ㅋㅋㅋㅋㅋㅋ 좋은 동아리네요",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋ 좋은 동아리네요",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100034058.1.17",
          "form": "ㅋㅋㅋㅋ거의 크아만 했어요",
          "original_form": "ㅋㅋㅋㅋ거의 크아만 했어요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100034058.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ아 저도.크아 좋아하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 저도.크아 좋아하는데",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100034058.1.19",
          "form": "여러명이서 하면 재밌을 거 같아요",
          "original_form": "여러명이서 하면 재밌을 거 같아요",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100034058.1.20",
          "form": "축제때는 크아 대회도 했었어요ㅋㅋㅋ",
          "original_form": "축제때는 크아 대회도 했었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100034058.1.21",
          "form": "상품도 별거 없었는데 학교에서 게임하니까 다 좋아하더라구요",
          "original_form": "상품도 별거 없었는데 학교에서 게임하니까 다 좋아하더라구요",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100034058.1.22",
          "form": "헐ㅋㅋㅋㅋ 크아 대회요?",
          "original_form": "헐ㅋㅋㅋㅋ 크아 대회요?",
