        },
        {
          "id": "MDRW2100007123.1.2",
          "form": "오늘 열심히 했는데여",
          "original_form": "오늘 열심히 했는데여",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007123.1.3",
          "form": "제가 요리 다 했죠?",
          "original_form": "제가 요리 다 했죠?",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007123.1.4",
          "form": "요리는 오빠가 다했지",
          "original_form": "요리는 오빠가 다했지",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007123.1.5",
          "form": "칭찬해",
          "original_form": "칭찬해",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007123.1.6",
          "form": "일상이지 집안일은",
          "original_form": "일상이지 집안일은",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007123.1.7",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007123.1.8",
          "form": "내일 빨래해야되니까 안까먹게 말해줘 내일",
          "original_form": "내일 빨래해야되니까 안까먹게 말해줘 내일",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007123.1.9",
          "form": "빨래",
          "original_form": "빨래",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007123.1.10",
          "form": "언제 하시게요",
          "original_form": "언제 하시게요",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007123.1.11",
          "form": "퇴근하고 해야지",
          "original_form": "퇴근하고 해야지",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007123.1.12",
          "form": "바닥 좀 치워놔야 로봇청소기 돌리는데",
          "original_form": "바닥 좀 치워놔야 로봇청소기 돌리는데",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007123.1.13",
          "form": "오늘 자기전에 정리하고 출근할때 돌리고나감되지",
          "original_form": "오늘 자기전에 정리하고 출근할때 돌리고나감되지",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007123.1.14",
          "form": "그럼 본인이 집 정리하세요^^",
          "original_form": "그럼 본인이 집 정리하세요^^",
          "speaker_id": "2",
