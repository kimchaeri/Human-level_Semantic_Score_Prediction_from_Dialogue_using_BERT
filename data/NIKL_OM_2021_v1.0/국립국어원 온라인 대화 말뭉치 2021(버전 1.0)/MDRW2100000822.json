      "utterance": [
        {
          "id": "MDRW2100000822.1.1",
          "form": "무슨 게임 좋아하세요?",
          "original_form": "무슨 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000822.1.2",
          "form": "전 휴대폰으로 게임하는데",
          "original_form": "전 휴대폰으로 게임하는데",
          "speaker_id": "2",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000822.1.3",
          "form": "위베어베어스랑 캔디크러쉬 같은 거 해요!",
          "original_form": "위베어베어스랑 캔디크러쉬 같은 거 해요!",
          "speaker_id": "2",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000822.1.4",
          "form": "홧팅이 님은요?",
          "original_form": "홧팅이 님은요?",
          "speaker_id": "2",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000822.1.5",
          "form": "저는 퍼즐게임이나 틀린그림 찾기 같은 게임을 좋아해요!",
          "original_form": "저는 퍼즐게임이나 틀린그림 찾기 같은 게임을 좋아해요!",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000822.1.6",
          "form": "저도 휴대폰으로 하는데",
          "original_form": "저도 휴대폰으로 하는데",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000822.1.7",
          "form": "요즘은 무슨 게임이 유행인가요?",
          "original_form": "요즘은 무슨 게임이 유행인가요?",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000822.1.8",
          "form": "위베어베어스 추천이요!",
          "original_form": "위베어베어스 추천이요!",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000822.1.9",
          "form": "너무 귀엽고 중독성있어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 귀엽고 중독성있어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000822.1.10",
          "form": "퍼즐 게임인가요? 어떤식으로 진행되나요?",
          "original_form": "퍼즐 게임인가요? 어떤식으로 진행되나요?",
          "speaker_id": "1",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000822.1.11",
          "form": "저도 정말 좋아하는 캐릭터에요",
          "original_form": "저도 정말 좋아하는 캐릭터에요",
          "speaker_id": "1",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000822.1.12",
          "form": "캔디크러쉬랑 똑같아요",
          "original_form": "캔디크러쉬랑 똑같아요",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000822.1.13",
          "form": "같은 색 맞추면 사라지는!",
          "original_form": "같은 색 맞추면 사라지는!",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000822.1.14",
          "form": "아~ 캐릭터가 귀여워서 더 재밌겠어요",
          "original_form": "아~ 캐릭터가 귀여워서 더 재밌겠어요",
          "speaker_id": "1",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000822.1.15",
          "form": "레벨 어디까지 가셨어요?",
          "original_form": "레벨 어디까지 가셨어요?",
          "speaker_id": "1",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000822.1.16",
          "form": "지금 보니까 2136이네요!",
          "original_form": "지금 보니까 2136이네요!",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000822.1.17",
          "form": "이거 꿀잼 완전 추천이엥",
          "original_form": "이거 꿀잼 완전 추천이엥",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000822.1.18",
          "form": "이에요",
          "original_form": "이에요",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000822.1.19",
          "form": "우와 2136이라니 멋져요",
          "original_form": "우와 2136이라니 멋져요",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000822.1.20",
          "form": "기간이 어느정도 소요되셨어요?",
          "original_form": "기간이 어느정도 소요되셨어요?",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000822.1.21",
          "form": "아유 얼마나 된거지...",
          "original_form": "아유 얼마나 된거지...",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000822.1.22",
          "form": "기억도 안 나요",
          "original_form": "기억도 안 나요",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000822.1.23",
          "form": "그냥 짬 날 때마다 하고 있어서",
          "original_form": "그냥 짬 날 때마다 하고 있어서",
          "speaker_id": "2",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000822.1.24",
          "form": "저도 피쉬돔이라고 전혀 유명하지 않은 게임을",
          "original_form": "저도 피쉬돔이라고 전혀 유명하지 않은 게임을",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000822.1.25",
          "form": "광고로 영업당해서",
          "original_form": "광고로 영업당해서",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000822.1.26",
          "form": "530탄? 까지 하다가",
          "original_form": "530탄? 까지 하다가",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000822.1.27",
          "form": "일상생활불가라 지우고 후회했어요",
          "original_form": "일상생활불가라 지우고 후회했어요",
          "speaker_id": "1",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000822.1.28",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000822.1.29",
          "form": "물고기 잡는 게임이에요?",
          "original_form": "물고기 잡는 게임이에요?",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000822.1.30",
          "form": "캔디크러시 같은건데 열심히 돈벌어서",
          "original_form": "캔디크러시 같은건데 열심히 돈벌어서",
          "speaker_id": "1",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.31",
          "form": "어항에 수초 꽂아주고",
          "original_form": "어항에 수초 꽂아주고",
          "speaker_id": "1",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.32",
          "form": "물고기 사서 넣고",
          "original_form": "물고기 사서 넣고",
          "speaker_id": "1",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.33",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.34",
          "form": "그런 게임이에요ㅋㅋㅋㅠㅠ",
          "original_form": "그런 게임이에요ㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.35",
          "form": "아니 너무 재밌겠는데요?",
          "original_form": "아니 너무 재밌겠는데요?",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.36",
          "form": "저 위베어베어스에 빠진 것도",
          "original_form": "저 위베어베어스에 빠진 것도",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.37",
          "form": "레벨 깰 때마다 막 집도 꾸미고",
          "original_form": "레벨 깰 때마다 막 집도 꾸미고",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000822.1.38",
          "form": "마을도 만들고 그럴 수 있거든요",
          "original_form": "마을도 만들고 그럴 수 있거든요",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000822.1.39",
          "form": "저 진짜 영업당했어요ㅋㅋ",
          "original_form": "저 진짜 영업당했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000822.1.40",
          "form": "그 게임도 길드? 팀? 만들어서 게임하나요?",
          "original_form": "그 게임도 길드? 팀? 만들어서 게임하나요?",
          "speaker_id": "1",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000822.1.41",
          "form": "아니요! 이건 그런 건 없어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요! 이건 그런 건 없어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000822.1.42",
          "form": "완전 개인플레이!",
          "original_form": "완전 개인플레이!",
          "speaker_id": "2",
          "time": "20210819 20:16"
        }
      ]
    }
