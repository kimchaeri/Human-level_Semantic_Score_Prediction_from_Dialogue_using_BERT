      "utterance": [
        {
          "id": "MDRW2100007033.1.1",
          "form": "시드니에 걔 입안에 다넣고 먹는애 생각나노",
          "original_form": "시드니에 걔 입안에 다넣고 먹는애 생각나노",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007033.1.2",
          "form": "파도가 엄청난곳",
          "original_form": "파도가 엄청난곳",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007033.1.3",
          "form": "그럴수있지 ㅋ",
          "original_form": "그럴수있지 ㅋ",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007033.1.4",
          "form": "본다이비치",
          "original_form": "본다이비치",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007033.1.5",
          "form": "어린나이였으니 이해해줘야함 먹성은 ㅋㅋ",
          "original_form": "어린나이였으니 이해해줘야함 먹성은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007033.1.6",
          "form": "난 근데 그거못봄 열심히 뜯어주느라",
          "original_form": "난 근데 그거못봄 열심히 뜯어주느라",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007033.1.7",
          "form": "그냥 먹는거 잘먹은정돈데",
          "original_form": "그냥 먹는거 잘먹은정돈데",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007033.1.8",
          "form": "본다이비치 해파리나온거생각나노",
          "original_form": "본다이비치 해파리나온거생각나노",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007033.1.9",
          "form": "해파리가 있었음?",
          "original_form": "해파리가 있었음?",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007033.1.10",
          "form": "성격 괜찮았음 착함",
          "original_form": "성격 괜찮았음 착함",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007033.1.11",
          "form": "ㅋㅋ그래 같이 간 사람들 다 좋았다",
          "original_form": "ㅋㅋ그래 같이 간 사람들 다 좋았다",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007033.1.12",
          "form": "해파리 나와서 튀엇음 전기해파리",
          "original_form": "해파리 나와서 튀엇음 전기해파리",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007033.1.13",
          "form": "헐 무섭다",
          "original_form": "헐 무섭다",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007033.1.14",
          "form": "발에걸려서",
