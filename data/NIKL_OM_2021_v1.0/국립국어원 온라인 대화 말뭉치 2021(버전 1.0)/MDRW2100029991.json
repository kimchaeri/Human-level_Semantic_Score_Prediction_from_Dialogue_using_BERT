        {
          "id": "MDRW2100029991.1.1",
          "form": "흐리죠?",
          "original_form": "흐리죠?",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100029991.1.2",
          "form": "네 조금요 곧 비 올 것 같아요",
          "original_form": "네 조금요 곧 비 올 것 같아요",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029991.1.3",
          "form": "우산은 챙기셨나요?",
          "original_form": "우산은 챙기셨나요?",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029991.1.4",
          "form": "이미 퇴근해서 집이에요",
          "original_form": "이미 퇴근해서 집이에요",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029991.1.5",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100029991.1.6",
          "form": "제 동네도 곧 비올듯",
          "original_form": "제 동네도 곧 비올듯",
          "speaker_id": "1",
          "time": "20210904 08:32"
        },
        {
          "id": "MDRW2100029991.1.7",
          "form": "지역이 어디쯤이세요?",
          "original_form": "지역이 어디쯤이세요?",
          "speaker_id": "2",
          "time": "20210904 08:33"
        },
        {
          "id": "MDRW2100029991.1.8",
          "form": "여긴 간만에 매우 화창하네요",
          "original_form": "여긴 간만에 매우 화창하네요",
          "speaker_id": "2",
          "time": "20210904 08:33"
        },
        {
          "id": "MDRW2100029991.1.9",
          "form": "대구요",
          "original_form": "대구요",
          "speaker_id": "1",
          "time": "20210904 08:42"
        },
        {
          "id": "MDRW2100029991.1.10",
          "form": "대구는 계속 흐린가봐요",
          "original_form": "대구는 계속 흐린가봐요",
          "speaker_id": "2",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100029991.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 08:44"
        },
        {
          "id": "MDRW2100029991.1.12",
          "form": "비피해없길빌어요",
          "original_form": "비피해없길빌어요",
          "speaker_id": "2",
          "time": "20210904 08:44"
        },
        {
          "id": "MDRW2100029991.1.13",
          "form": "아직 비는여?",
          "original_form": "아직 비는여?",
          "speaker_id": "1",
          "time": "20210904 08:44"
        },
