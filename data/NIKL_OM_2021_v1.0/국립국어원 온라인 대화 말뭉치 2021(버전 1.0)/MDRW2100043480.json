      "utterance": [
        {
          "id": "MDRW2100043480.1.1",
          "form": "안녕하세요 여행 좋아하세요?",
          "original_form": "안녕하세요 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043480.1.2",
          "form": "네좋아해요 ㅎㅎ",
          "original_form": "네좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043480.1.3",
          "form": "제일 좋았던 곳이 어디었나요!!",
          "original_form": "제일 좋았던 곳이 어디었나요!!",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043480.1.4",
          "form": "저는 끄라비요!",
          "original_form": "저는 끄라비요!",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043480.1.5",
          "form": "오 저 처음 들어봐요!",
          "original_form": "오 저 처음 들어봐요!",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043480.1.6",
          "form": "태국에 있습니다 ㅋㅋㅋ",
          "original_form": "태국에 있습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043480.1.7",
          "form": "아 태국이구나ㅎㅎ 저도 어릴 때 가봤어요",
          "original_form": "아 태국이구나ㅎㅎ 저도 어릴 때 가봤어요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043480.1.8",
          "form": "저는 산호섬 갔어요!",
          "original_form": "저는 산호섬 갔어요!",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043480.1.9",
          "form": "네네 ㅋㅋ 태국 좋아여 음식도 맛있구",
          "original_form": "네네 ㅋㅋ 태국 좋아여 음식도 맛있구",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043480.1.10",
          "form": "오오그러시구나",
          "original_form": "오오그러시구나",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043480.1.11",
          "form": "저도 그안에 또 다른 섬에 갔는데 좋았어여ㅠㅠ또가구싶네요..",
          "original_form": "저도 그안에 또 다른 섬에 갔는데 좋았어여ㅠㅠ또가구싶네요..",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043480.1.12",
          "form": "여름에 갔는데 망고 엄청 먹고 왔어요",
          "original_form": "여름에 갔는데 망고 엄청 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043480.1.13",
          "form": "저두요ㅠㅠ 바다가 너무 예쁘지 않나요",
          "original_form": "저두요ㅠㅠ 바다가 너무 예쁘지 않나요",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043480.1.14",
          "form": "ㅋㅋㅋㅋ좋으셨겠다 망고좋져 저는 수박주스 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ좋으셨겠다 망고좋져 저는 수박주스 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043480.1.15",
          "form": "바다너무예뻐요 ㅠㅠ",
          "original_form": "바다너무예뻐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043480.1.16",
          "form": "와 수박주스ㅜㅠ 말씀하시니까 기억나요",
          "original_form": "와 수박주스ㅜㅠ 말씀하시니까 기억나요",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043480.1.17",
          "form": "맞아요ㅠㅠ 완전 연한 에메랄드빛",
          "original_form": "맞아요ㅠㅠ 완전 연한 에메랄드빛",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043480.1.18",
          "form": "진짜맛있어여 ㅠㅠㅋ 천국이에여...스노쿨링도 재밌구여",
          "original_form": "진짜맛있어여 ㅠㅠㅋ 천국이에여...스노쿨링도 재밌구여",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043480.1.19",
          "form": "가격도 부담스럽지도 않고 넘 좋아요",
          "original_form": "가격도 부담스럽지도 않고 넘 좋아요",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043480.1.20",
          "form": "스노쿨링 재밌겠다ㅠㅠ 저는 바나나보트 탔어요ㅋㅋㅋ",
          "original_form": "스노쿨링 재밌겠다ㅠㅠ 저는 바나나보트 탔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043480.1.21",
          "form": "와 재밌겠닼ㅋㅋ 전 보트는 못해봤어여 ㅠㅠㅋㅋ",
          "original_form": "와 재밌겠닼ㅋㅋ 전 보트는 못해봤어여 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:21"
        }
      ]
    }
