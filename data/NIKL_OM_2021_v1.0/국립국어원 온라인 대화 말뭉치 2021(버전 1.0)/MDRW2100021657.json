      "utterance": [
        {
          "id": "MDRW2100021657.1.1",
          "form": "진짜 피아노 하나로 그렇게 울림을 주는 게 대단한 것 같아요",
          "original_form": "진짜 피아노 하나로 그렇게 울림을 주는 게 대단한 것 같아요",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021657.1.2",
          "form": "혹시.라흐마니노프 도 보셨어요?",
          "original_form": "혹시.라흐마니노프 도 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100021657.1.3",
          "form": "아뇨 담주 재택때는 그거 들어볼게요!",
          "original_form": "아뇨 담주 재택때는 그거 들어볼게요!",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021657.1.4",
          "form": "추천 감사합니다!!!",
          "original_form": "추천 감사합니다!!!",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021657.1.5",
          "form": "네! 피아노 협주곡 2018년 12월에 한 실황 추천드려요",
          "original_form": "네! 피아노 협주곡 2018년 12월에 한 실황 추천드려요",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021657.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021657.1.7",
          "form": "그리고 악흥의 순간 이것두요",
          "original_form": "그리고 악흥의 순간 이것두요",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021657.1.8",
          "form": "ㅋㅋ 양인모? 우아한유령인가 이것도 아세요?",
          "original_form": "ㅋㅋ 양인모? 우아한유령인가 이것도 아세요?",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021657.1.9",
          "form": "네네!!!!",
          "original_form": "네네!!!!",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021657.1.10",
          "form": "저 좋아해요",
          "original_form": "저 좋아해요",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021657.1.11",
          "form": "아 너무 좋아요!",
          "original_form": "아 너무 좋아요!",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021657.1.12",
          "form": "피아졸라도!",
          "original_form": "피아졸라도!",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021657.1.13",
          "form": "저는 손열음 버전을 좀 더 좋아하긴 해요 ㅋㅋㅋ",
          "original_form": "저는 손열음 버전을 좀 더 좋아하긴 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021657.1.14",
          "form": "요즘 요렇게 세개 돌려들어요",
          "original_form": "요즘 요렇게 세개 돌려들어요",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021657.1.15",
          "form": "그럼 피아노버젼인거죠?",
          "original_form": "그럼 피아노버젼인거죠?",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021657.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021657.1.17",
          "form": "클라라주미강 이랑 바이올린 협주한 것두 있어요 ㅋㅋ",
          "original_form": "클라라주미강 이랑 바이올린 협주한 것두 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021657.1.18",
          "form": "다 메모했다가 실황찾아보고 코로나가 끝나면 도전해봐야겠어요!",
          "original_form": "다 메모했다가 실황찾아보고 코로나가 끝나면 도전해봐야겠어요!",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021657.1.19",
          "form": "ㅋㅋㅋㅋ추천드린거 찾아봐주신다니 기쁘네용",
          "original_form": "ㅋㅋㅋㅋ추천드린거 찾아봐주신다니 기쁘네용",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021657.1.20",
          "form": "클래식은 다들 잘 안 들으니까요",
          "original_form": "클래식은 다들 잘 안 들으니까요",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021657.1.21",
          "form": "그래도 매니아가 있으니까요",
          "original_form": "그래도 매니아가 있으니까요",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021657.1.22",
          "form": "고급문화라 생각돼서 부담가지나봐요",
          "original_form": "고급문화라 생각돼서 부담가지나봐요",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021657.1.23",
          "form": "그러니까요!",
          "original_form": "그러니까요!",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021657.1.24",
          "form": "사실은 별거 아닌데요",
          "original_form": "사실은 별거 아닌데요",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021657.1.25",
          "form": "그리고 오히려 시향 정기연주 같은 경우는 티켓값도 되게 싼데",
          "original_form": "그리고 오히려 시향 정기연주 같은 경우는 티켓값도 되게 싼데",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021657.1.26",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021657.1.27",
          "form": "아니 코로나전엔 서울시청 잔디앞에서도 공연하고 진짜 좋았는데요",
          "original_form": "아니 코로나전엔 서울시청 잔디앞에서도 공연하고 진짜 좋았는데요",
          "speaker_id": "2",
          "time": "20210828 21:31"
        }
      ]
    }
