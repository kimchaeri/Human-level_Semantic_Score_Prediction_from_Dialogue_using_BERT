      "utterance": [
        {
          "id": "MDRW2100034060.1.1",
          "form": "지금 일은 만족하세여?",
          "original_form": "지금 일은 만족하세여?",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100034060.1.2",
          "form": "그냥저냥요",
          "original_form": "그냥저냥요",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100034060.1.3",
          "form": "그만두고싶은데 만족 안하는걸까요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그만두고싶은데 만족 안하는걸까요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100034060.1.4",
          "form": "ㅋㅋㅋㅋㅋ퇴사욕구만 있어요 정말ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ퇴사욕구만 있어요 정말ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100034060.1.5",
          "form": "ㅋㅋㅋㅋㅋ 저랑 같네요 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 저랑 같네요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100034060.1.6",
          "form": "저도 퇴사만..",
          "original_form": "저도 퇴사만..",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100034060.1.7",
          "form": "하 퇴근입니당",
          "original_form": "하 퇴근입니당",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100034060.1.8",
          "form": "저는 오늘 일찍 했어요",
          "original_form": "저는 오늘 일찍 했어요",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100034060.1.9",
          "form": "이제 집에서 쉬세요!!!",
          "original_form": "이제 집에서 쉬세요!!!",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034060.1.10",
          "form": "ㅋㅋㅋㅋ불금이니깐 낼출근이여도 술한잔중요",
          "original_form": "ㅋㅋㅋㅋ불금이니깐 낼출근이여도 술한잔중요",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034060.1.11",
          "form": "그때가 가장 행복하죠",
          "original_form": "그때가 가장 행복하죠",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034060.1.12",
          "form": "딱 불금이요ㅋㅋㅋㅋ",
          "original_form": "딱 불금이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034060.1.13",
          "form": "내일도 출근하세요?",
          "original_form": "내일도 출근하세요?",
          "speaker_id": "1",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100034060.1.14",
          "form": "다행히",
          "original_form": "다행히",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100034060.1.15",
          "form": "안합니다 ㅎ",
          "original_form": "안합니다 ㅎ",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100034060.1.16",
          "form": "혹시 출근하세요?",
          "original_form": "혹시 출근하세요?",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100034060.1.17",
          "form": "ㅠㅠ 부럽네욫.. 오늘도 합니당",
          "original_form": "ㅠㅠ 부럽네욫.. 오늘도 합니당",
          "speaker_id": "1",
          "time": "20210828 07:57"
        },
        {
          "id": "MDRW2100034060.1.18",
          "form": "헉 ㅠ 지금 하셨나요?",
          "original_form": "헉 ㅠ 지금 하셨나요?",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034060.1.19",
          "form": "으 주말동안 열심히놀았더니 .. 또츨근 ㅠ",
          "original_form": "으 주말동안 열심히놀았더니 .. 또츨근 ㅠ",
          "speaker_id": "1",
          "time": "20210830 07:55"
        },
        {
          "id": "MDRW2100034060.1.20",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210830 08:24"
        },
        {
          "id": "MDRW2100034060.1.21",
