      "utterance": [
        {
          "id": "MDRW2100010895.1.1",
          "form": "여행하는거 좋아하시나요?",
          "original_form": "여행하는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100010895.1.2",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100010895.1.3",
          "form": "다만 요즘은 못 가죠ㅠ",
          "original_form": "다만 요즘은 못 가죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100010895.1.4",
          "form": "맞아요 ㅠㅠ 코로나끝나면 어디제일 가고싶으신가요!",
          "original_form": "맞아요 ㅠㅠ 코로나끝나면 어디제일 가고싶으신가요!",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100010895.1.5",
          "form": "음 저는 미국이요",
          "original_form": "음 저는 미국이요",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100010895.1.6",
          "form": "한 번도 안 가봐서 가보고 싶어요",
          "original_form": "한 번도 안 가봐서 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100010895.1.7",
          "form": "name2님은 가보고 싶은 곳 있으세요?",
          "original_form": "&name2&님은 가보고 싶은 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100010895.1.8",
          "form": "오 저도 미국가보고싶어요 ..ㅠㅠ",
          "original_form": "오 저도 미국가보고싶어요 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100010895.1.9",
          "form": "저는 예전부터 아이슬란드 가보고싶었어요 오로라 꼭 보고싶어요",
          "original_form": "저는 예전부터 아이슬란드 가보고싶었어요 오로라 꼭 보고싶어요",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100010895.1.10",
          "form": "헐 저듀요!",
          "original_form": "헐 저듀요!",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100010895.1.11",
          "form": "오로라를 진짜 보고 싶어요",
          "original_form": "오로라를 진짜 보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100010895.1.12",
          "form": "아이슬란드는 물가가 비싸서 가보고 싶은데 쫄려요",
          "original_form": "아이슬란드는 물가가 비싸서 가보고 싶은데 쫄려요",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100010895.1.13",
          "form": "그쵸ㅠㅠ 돈 모아서 나중에 한번 꼭 가보고싶어요 ...!",
          "original_form": "그쵸ㅠㅠ 돈 모아서 나중에 한번 꼭 가보고싶어요 ...!",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100010895.1.14",
          "form": "꽃청춘에서 간 거 보고 더 가고 싶어졌어요ㅠ",
          "original_form": "꽃청춘에서 간 거 보고 더 가고 싶어졌어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100010895.1.15",
          "form": "오 맞아요ㅋㅋ 저도 그거보고 더 가보고싶어졌어요 ㅜㅜ",
          "original_form": "오 맞아요ㅋㅋ 저도 그거보고 더 가보고싶어졌어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100010895.1.16",
          "form": "거기는 산소도 판매한다고 해서 놀랬어요 ㅋㅋ",
          "original_form": "거기는 산소도 판매한다고 해서 놀랬어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100010895.1.17",
          "form": "정말요? 그거 신기하네요ㅋㅋ",
          "original_form": "정말요? 그거 신기하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100010895.1.18",
          "form": "공기가 맑아서 그런가? 판매한대요 ㅋㅋㅋ",
          "original_form": "공기가 맑아서 그런가? 판매한대요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100010895.1.19",
          "form": "특이한 기념품이 될 것 같아요",
          "original_form": "특이한 기념품이 될 것 같아요",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100010895.1.20",
          "form": "그렇네요! 거기 공기 진짜 좋을 것 같아요..",
          "original_form": "그렇네요! 거기 공기 진짜 좋을 것 같아요..",
