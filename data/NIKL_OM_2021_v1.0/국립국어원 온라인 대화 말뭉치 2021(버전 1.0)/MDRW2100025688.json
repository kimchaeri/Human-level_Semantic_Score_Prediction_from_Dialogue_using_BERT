      "utterance": [
        {
          "id": "MDRW2100025688.1.1",
          "form": "날이 정말 흐려요",
          "original_form": "날이 정말 흐려요",
          "speaker_id": "1",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100025688.1.2",
          "form": "언제 해를 볼 수 있을까요",
          "original_form": "언제 해를 볼 수 있을까요",
          "speaker_id": "1",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100025688.1.3",
          "form": "창문닫았다열었다",
          "original_form": "창문닫았다열었다",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100025688.1.4",
          "form": "난리다",
          "original_form": "난리다",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100025688.1.5",
          "form": "차 창문 열려있는데~",
          "original_form": "차 창문 열려있는데~",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025688.1.6",
          "form": "집에와서 좀쉬는데 다시내려가야되나ㅜ",
          "original_form": "집에와서 좀쉬는데 다시내려가야되나ㅜ",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025688.1.7",
          "form": "한시간 정도는 괜찮을거에요",
          "original_form": "한시간 정도는 괜찮을거에요",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.8",
          "form": "일기예보는 5시에 비라고 되어있긴한데",
          "original_form": "일기예보는 5시에 비라고 되어있긴한데",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.9",
          "form": "20분만 쉬어야겠네ㅜ",
          "original_form": "20분만 쉬어야겠네ㅜ",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.10",
          "form": "기상청은 언제나 틀렸으니까...",
          "original_form": "기상청은 언제나 틀렸으니까...",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.11",
          "form": "좀전에 확오더니",
          "original_form": "좀전에 확오더니",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.12",
          "form": "아직 안올겁니다",
          "original_form": "아직 안올겁니다",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.13",
          "form": "다시 잠잠 ~~",
          "original_form": "다시 잠잠 ~~",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.14",
          "form": "ㅇ0ㅇ 비왔어요 중간에?",
          "original_form": "ㅇ0ㅇ 비왔어요 중간에?",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025688.1.15",
          "form": "저는 비오는거 못 봤는데",
          "original_form": "저는 비오는거 못 봤는데",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025688.1.16",
          "form": "ㅇㅇ 오더라고",
          "original_form": "ㅇㅇ 오더라고",
          "speaker_id": "2",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025688.1.17",
          "form": "ㅋㅋ 여기만왔나보네",
          "original_form": "ㅋㅋ 여기만왔나보네",
          "speaker_id": "2",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025688.1.18",
          "form": "오메 몰랐네",
          "original_form": "오메 몰랐네",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025688.1.19",
          "form": "그런가봐요 이쪽은 잠잠했어요",
          "original_form": "그런가봐요 이쪽은 잠잠했어요",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025688.1.20",
          "form": "일단 창문은 닫아뒀는데 다행이네요ㅋㅋㅋ",
          "original_form": "일단 창문은 닫아뒀는데 다행이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025688.1.21",
          "form": "습하다",
          "original_form": "습하다",
          "speaker_id": "2",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025688.1.22",
          "form": "아까가디건 샀나?",
          "original_form": "아까가디건 샀나?",
          "speaker_id": "2",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025688.1.23",
          "form": "아뇨 다른 옷 하나 더 골라야 무료배송이라 지금 어떤거 살지 보려구요",
          "original_form": "아뇨 다른 옷 하나 더 골라야 무료배송이라 지금 어떤거 살지 보려구요",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025688.1.24",
          "form": "ㅋㅋ사면 내가함 걸쳐볼께",
          "original_form": "ㅋㅋ사면 내가함 걸쳐볼께",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100025688.1.25",
          "form": "아마 거지같을걸 ㅋ",
          "original_form": "아마 거지같을걸 ㅋ",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100025688.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ여기 8.5부 바지도 팔아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ여기 8.5부 바지도 팔아요",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025688.1.27",
          "form": "오면 길이보고 8.5부 한 번 사봅시다",
          "original_form": "오면 길이보고 8.5부 한 번 사봅시다",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025688.1.28",
          "form": "니껀 몇분데~",
          "original_form": "니껀 몇분데~",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025688.1.29",
          "form": "비가오나?",
          "original_form": "비가오나?",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100025688.1.30",
          "form": "비오나?",
          "original_form": "비오나?",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100025688.1.31",
          "form": "에어컨 켜놨는디 습해서",
          "original_form": "에어컨 켜놨는디 습해서",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100025688.1.32",
          "form": "몇 부인지 안나오던데",
          "original_form": "몇 부인지 안나오던데",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025688.1.33",
          "form": "기장 센치 봐선 제 키에 맞을거 같아요",
          "original_form": "기장 센치 봐선 제 키에 맞을거 같아요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025688.1.34",
          "form": "95센치정도 되더라구요",
          "original_form": "95센치정도 되더라구요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025688.1.35",
          "form": "아직 안와요",
          "original_form": "아직 안와요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025688.1.36",
          "form": "근데 습기 땜에 문은 안여는게 좋을 듯",
          "original_form": "근데 습기 땜에 문은 안여는게 좋을 듯",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025688.1.37",
          "form": "일단 오면 입어보자",
          "original_form": "일단 오면 입어보자",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025688.1.38",
          "form": "ㅋㅋㅋ가을엔 역시 기모바지와 가디건이쥬",
          "original_form": "ㅋㅋㅋ가을엔 역시 기모바지와 가디건이쥬",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025688.1.39",
          "form": "1층잠시갔다왔는데",
          "original_form": "1층잠시갔다왔는데",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025688.1.40",
          "form": "날씨장난아니네~~",
          "original_form": "날씨장난아니네~~",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
