      "utterance": [
        {
          "id": "MDRW2100024142.1.1",
          "form": "누구요",
          "original_form": "누구요",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024142.1.2",
          "form": "최근에 바뀌긴 했는데",
          "original_form": "최근에 바뀌긴 했는데",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024142.1.3",
          "form": "예전에는 박보검씨 좋아했어요",
          "original_form": "예전에는 박보검씨 좋아했어요",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024142.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024142.1.5",
          "form": "박보검도 잘생겼죠",
          "original_form": "박보검도 잘생겼죠",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.6",
          "form": "제가 박보검 배우 처음 본게 아마 차이나타운이라는 영화였거든요?",
          "original_form": "제가 박보검 배우 처음 본게 아마 차이나타운이라는 영화였거든요?",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.7",
          "form": "봤어요?",
          "original_form": "봤어요?",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.8",
          "form": "오 보진.않았어요",
          "original_form": "오 보진.않았어요",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.9",
          "form": "듣기만 듣고 ㅋㅋㅋ",
          "original_form": "듣기만 듣고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.10",
          "form": "그때 석현이라는 이름으로 조연 출연했는데",
          "original_form": "그때 석현이라는 이름으로 조연 출연했는데",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.11",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.12",
          "form": "뭐 빚에 쫓기는 그런 인생을 살고 있는 캐릭터였어요",
          "original_form": "뭐 빚에 쫓기는 그런 인생을 살고 있는 캐릭터였어요",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.13",
          "form": "그래서 사채업자가 막 집 찾아와서 돈갚으라고 협박하러 왔는데",
          "original_form": "그래서 사채업자가 막 집 찾아와서 돈갚으라고 협박하러 왔는데",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.14",
          "form": "의외의 캐릭터네요 ㅋㅋ",
          "original_form": "의외의 캐릭터네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024142.1.15",
          "form": "대뜸 \"식사 하셨어요?\" 하면서 되게 생글생글하게 밥도 챙겨주고 하는게 인상깊었어요",
          "original_form": "대뜸 \"식사 하셨어요?\" 하면서 되게 생글생글하게 밥도 챙겨주고 하는게 인상깊었어요",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.16",
          "form": "ㅋㅋㅋㅋ세상 선한 밋ㅅ",
          "original_form": "ㅋㅋㅋㅋ세상 선한 밋ㅅ",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.17",
          "form": "미소",
          "original_form": "미소",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.18",
          "form": "그래서 되게 잘생겼는데 되게 젠틀하고 매너있고 멋있다 라고 생각했는데 실제로도 그런 성격이라면서요",
          "original_form": "그래서 되게 잘생겼는데 되게 젠틀하고 매너있고 멋있다 라고 생각했는데 실제로도 그런 성격이라면서요",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.19",
          "form": "그러게요 잘생겨서 성격도 좋다니 반칙",
          "original_form": "그러게요 잘생겨서 성격도 좋다니 반칙",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.20",
          "form": "그 이후 나영석 피디님이 하는거 무슨 꽃ㄹ보다 청춘?",
          "original_form": "그 이후 나영석 피디님이 하는거 무슨 꽃ㄹ보다 청춘?",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.22",
          "form": "거기에 박보검 나와서 막 자기 인생이야기 해주는데",
          "original_form": "거기에 박보검 나와서 막 자기 인생이야기 해주는데",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024142.1.23",
          "form": "거기에도 또 감동 받아가지고 저 박보검엉아 완전 좋아요",
          "original_form": "거기에도 또 감동 받아가지고 저 박보검엉아 완전 좋아요",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024142.1.24",
          "form": "ㅋㅋㅋㅋㅋ좋아할만한 연예인인거같아요",
          "original_form": "ㅋㅋㅋㅋㅋ좋아할만한 연예인인거같아요",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024142.1.25",
          "form": "인성이 좋은 배우",
          "original_form": "인성이 좋은 배우",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024142.1.26",
          "form": "잘생기고 성격도 좋고 인성도 좋고",
          "original_form": "잘생기고 성격도 좋고 인성도 좋고",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024142.1.27",
          "form": "진짜 오래 롱런했음 좋겠어요 ㅎㅎ",
          "original_form": "진짜 오래 롱런했음 좋겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:38"
        }
      ]
    }
