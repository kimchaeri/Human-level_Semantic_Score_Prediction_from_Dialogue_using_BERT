      "utterance": [
        {
          "id": "MDRW2100033564.1.1",
          "form": "집안일 자주하시나요",
          "original_form": "집안일 자주하시나요",
          "speaker_id": "1",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100033564.1.2",
          "form": "네 몰아서ㅎㅎ",
          "original_form": "네 몰아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100033564.1.3",
          "form": "제가 다해야되거든요ㅎ",
          "original_form": "제가 다해야되거든요ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100033564.1.4",
          "form": "저는 금방 청소기 돌렸네요",
          "original_form": "저는 금방 청소기 돌렸네요",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100033564.1.5",
          "form": "가장 이신가요?",
          "original_form": "가장 이신가요?",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100033564.1.6",
          "form": "아뇨 혼자 살아요",
          "original_form": "아뇨 혼자 살아요",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100033564.1.7",
          "form": "아직 미혼",
          "original_form": "아직 미혼",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100033564.1.8",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100033564.1.9",
          "form": "아니요 비혼이예요",
          "original_form": "아니요 비혼이예요",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100033564.1.10",
          "form": "가족들이랑 같이 살고요",
          "original_form": "가족들이랑 같이 살고요",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100033564.1.11",
          "form": "아 그럼 같이 가사분담하시겠네요",
          "original_form": "아 그럼 같이 가사분담하시겠네요",
          "speaker_id": "2",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100033564.1.12",
          "form": "전 직장근처에 혼자있어서 집안일 전담이예쇼",
          "original_form": "전 직장근처에 혼자있어서 집안일 전담이예쇼",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033564.1.13",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033564.1.14",
          "form": "네 분담하고 있어요",
          "original_form": "네 분담하고 있어요",
          "speaker_id": "1",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033564.1.15",
          "form": "혼자서 힘드시겠네요",
          "original_form": "혼자서 힘드시겠네요",
          "speaker_id": "1",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033564.1.16",
          "form": "피곤하겠어요",
          "original_form": "피곤하겠어요",
          "speaker_id": "1",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033564.1.17",
          "form": "집이 좁아서 청소기돌리기는 괜찮은데",
          "original_form": "집이 좁아서 청소기돌리기는 괜찮은데",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033564.1.18",
          "form": "화장실 청소 음식물 처리가 젤귀찮네요",
          "original_form": "화장실 청소 음식물 처리가 젤귀찮네요",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100033564.1.19",
          "form": "그죠 1인가구는 음식물 처리 불편할거같아요",
          "original_form": "그죠 1인가구는 음식물 처리 불편할거같아요",
          "speaker_id": "1",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100033564.1.20",
          "form": "은근 분리수거할것도 쌓이더라구요",
          "original_form": "은근 분리수거할것도 쌓이더라구요",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100033564.1.21",
          "form": "플라스틱",
          "original_form": "플라스틱",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100033564.1.22",
          "form": "유리같은거 나오면 곤란하시겠어요",
          "original_form": "유리같은거 나오면 곤란하시겠어요",
          "speaker_id": "1",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100033564.1.23",
          "form": "다행히 유리는 비타오백 ㅋ",
          "original_form": "다행히 유리는 비타오백 ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:37"
        },
        {
          "id": "MDRW2100033564.1.24",
          "form": "박카스이런것만 나와서",
          "original_form": "박카스이런것만 나와서",
          "speaker_id": "2",
          "time": "20210824 14:37"
        },
        {
          "id": "MDRW2100033564.1.25",
          "form": "청소업체 불러보신적있으신가요?",
          "original_form": "청소업체 불러보신적있으신가요?",
          "speaker_id": "1",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100033564.1.26",
          "form": "아니요 부를만한 크기도아니라서",
          "original_form": "아니요 부를만한 크기도아니라서",
          "speaker_id": "2",
          "time": "20210824 14:39"
        }
      ]
    }
