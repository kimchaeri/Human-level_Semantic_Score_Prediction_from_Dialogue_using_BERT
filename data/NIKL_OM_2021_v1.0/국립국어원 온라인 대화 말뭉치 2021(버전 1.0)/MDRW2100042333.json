      "utterance": [
        {
          "id": "MDRW2100042333.1.1",
          "form": "그래서 그 엄마가",
          "original_form": "그래서 그 엄마가",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.2",
          "form": "놀토에서 만났어까지ㅋㅋㅋ",
          "original_form": "놀토에서 만났어까지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.3",
          "form": "놀이터에서 나를 보더니",
          "original_form": "놀이터에서 나를 보더니",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.4",
          "form": "얍얍",
          "original_form": "얍얍",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.5",
          "form": "언니 name3이는 원래 밥을 잘 안 먹어요??",
          "original_form": "언니 &name3&이는 원래 밥을 잘 안 먹어요??",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.6",
          "form": "이러는 거야ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이러는 거야ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.7",
          "form": "그래서 내가",
          "original_form": "그래서 내가",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.8",
          "form": "급식 맛있다고 했는데",
          "original_form": "급식 맛있다고 했는데",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.9",
          "form": "아니 우리 name3학교 밥 맛있다고 잘 먹는다고 하던데??",
          "original_form": "아니 우리 &name3&학교 밥 맛있다고 잘 먹는다고 하던데??",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.10",
          "form": "이랬어",
          "original_form": "이랬어",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042333.1.11",
          "form": "그랬지요",
          "original_form": "그랬지요",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.12",
          "form": "아니 밥을 너무 안 먹어서",
          "original_form": "아니 밥을 너무 안 먹어서",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.13",
          "form": "걔가 밥이랑 반찬 좀 잘 먹으라고 몇 번을 얘기했다대",
          "original_form": "걔가 밥이랑 반찬 좀 잘 먹으라고 몇 번을 얘기했다대",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.14",
          "form": "여태까지 밥 맛있다고 많이 먹는다고 했던 건",
          "original_form": "여태까지 밥 맛있다고 많이 먹는다고 했던 건",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.15",
          "form": "다 거짓말이었던 거지",
          "original_form": "다 거짓말이었던 거지",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.16",
          "form": "유치원에서는 선생님이 밥 안 먹으면 나한테 얘기를 하는데",
          "original_form": "유치원에서는 선생님이 밥 안 먹으면 나한테 얘기를 하는데",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.17",
          "form": "그걸 언닌 찰떡같이 믿었네요",
          "original_form": "그걸 언닌 찰떡같이 믿었네요",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.18",
          "form": "여기서는 잘 먹든 안 먹든 얘기를 안 하잖아",
          "original_form": "여기서는 잘 먹든 안 먹든 얘기를 안 하잖아",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.19",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.20",
          "form": "하기에선 아무도 얘기를 안 하니",
          "original_form": "하기에선 아무도 얘기를 안 하니",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042333.1.21",
          "form": "난 진짜 잘 먹는 줄 알았거든",
          "original_form": "난 진짜 잘 먹는 줄 알았거든",
          "speaker_id": "2",
          "time": "20210906 21:39"
        }
      ]
