      "utterance": [
        {
          "id": "MDRW2100026182.1.1",
          "form": "그랑죠 봤죠 봤죠 ㅋㅋ마법으로 빛나는 그랑죠",
          "original_form": "그랑죠 봤죠 봤죠 ㅋㅋ마법으로 빛나는 그랑죠",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026182.1.2",
          "form": "맞아요 진짜 좋아해요!!!!! 모르는 사람들 있던데 아는사람 보니깐 반갑네요",
          "original_form": "맞아요 진짜 좋아해요!!!!! 모르는 사람들 있던데 아는사람 보니깐 반갑네요",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026182.1.3",
          "form": "그리고 꾸러기수비대도 재밌었어요!",
          "original_form": "그리고 꾸러기수비대도 재밌었어요!",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026182.1.4",
          "form": "헐헐 꾸러기 수비대 재밌죠 ㅜㅜ 그걸로 십이간지 외웠잖아요",
          "original_form": "헐헐 꾸러기 수비대 재밌죠 ㅜㅜ 그걸로 십이간지 외웠잖아요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026182.1.5",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026182.1.6",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026182.1.7",
          "form": "십이간지는 꾸러기수비대죠",
          "original_form": "십이간지는 꾸러기수비대죠",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026182.1.8",
          "form": "요즘도 십이간지 생각안나면 똘기 떵이",
          "original_form": "요즘도 십이간지 생각안나면 똘기 떵이",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026182.1.9",
          "form": "하고있어요",
          "original_form": "하고있어요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026182.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ혹시 메리벨은 아시나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ혹시 메리벨은 아시나요",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026182.1.11",
          "form": "기억안나서 검색해보고 왔는데 보니깐 알겠어요ㅋㅋ",
          "original_form": "기억안나서 검색해보고 왔는데 보니깐 알겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026182.1.12",
          "form": "정확히는 기억 안나는데 본거같아여",
          "original_form": "정확히는 기억 안나는데 본거같아여",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026182.1.13",
          "form": "메리벨 알정도면 고수신데요 ㅋㅋㅋㅋ",
          "original_form": "메리벨 알정도면 고수신데요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026182.1.14",
          "form": "이건정말 모를거 같다 이런거 있으신가요",
          "original_form": "이건정말 모를거 같다 이런거 있으신가요",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026182.1.15",
          "form": "음...저는 대부분 알것같아요",
          "original_form": "음...저는 대부분 알것같아요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.16",
          "form": "저는 공중파만 나와서ㅋㅋ",
          "original_form": "저는 공중파만 나와서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.17",
          "form": "그땐 공중파에서 나온건 거의 다 봤거든요",
          "original_form": "그땐 공중파에서 나온건 거의 다 봤거든요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.18",
          "form": "ㅋㅋㅋㅋ 전... 약간 어릴때부터 오타쿠의 피가있었나봐요 ...",
          "original_form": "ㅋㅋㅋㅋ 전... 약간 어릴때부터 오타쿠의 피가있었나봐요 ...",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.19",
          "form": "저도 공중파만봤습니다!!",
          "original_form": "저도 공중파만봤습니다!!",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.20",
          "form": "쥬라기월드컵 아시나요ㅋ",
          "original_form": "쥬라기월드컵 아시나요ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.21",
          "form": "투니버스 이런건 사치죠",
          "original_form": "투니버스 이런건 사치죠",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.22",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.23",
          "form": "그거 좋아했는데ㅋㅋㅋ",
          "original_form": "그거 좋아했는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026182.1.24",
          "form": "그거노래부르다가 아빠가 저 공룡천재인줄알았어요",
          "original_form": "그거노래부르다가 아빠가 저 공룡천재인줄알았어요",
          "speaker_id": "2",
