      "utterance": [
        {
          "id": "MDRW2100024828.1.1",
          "form": "건강관리는 하고 계신가요?",
          "original_form": "건강관리는 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100024828.1.2",
          "form": "그냥 운동 시작하고 영양제 쫌쫌따리 먹고 그러는거죠",
          "original_form": "그냥 운동 시작하고 영양제 쫌쫌따리 먹고 그러는거죠",
          "speaker_id": "2",
          "time": "20210831 19:20"
        },
        {
          "id": "MDRW2100024828.1.3",
          "form": "요새같을때는 건강한게 최고더라구요",
          "original_form": "요새같을때는 건강한게 최고더라구요",
          "speaker_id": "1",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100024828.1.4",
          "form": "챙겨드시는 약은 있으신가요?",
          "original_form": "챙겨드시는 약은 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100024828.1.5",
          "form": "그쵸.",
          "original_form": "그쵸.",
          "speaker_id": "2",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100024828.1.6",
          "form": "종합비타민이랑 프로폴리스요",
          "original_form": "종합비타민이랑 프로폴리스요",
          "speaker_id": "2",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100024828.1.7",
          "form": "비타민은 정말 중요한거 같아요ㅎ",
          "original_form": "비타민은 정말 중요한거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024828.1.8",
          "form": "프로폴리스는 효과가 있던가요?",
          "original_form": "프로폴리스는 효과가 있던가요?",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024828.1.9",
          "form": "제 기분으로는 있었어요",
          "original_form": "제 기분으로는 있었어요",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024828.1.10",
          "form": "잔기침이 있는편인데",
          "original_form": "잔기침이 있는편인데",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024828.1.11",
          "form": "줄더라고요",
          "original_form": "줄더라고요",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024828.1.12",
          "form": "정말 효과가 있군요",
          "original_form": "정말 효과가 있군요",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024828.1.13",
          "form": "스프레이형으로 된거 몇번 사용하다가 중단했었는데",
          "original_form": "스프레이형으로 된거 몇번 사용하다가 중단했었는데",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024828.1.14",
          "form": "다시 복용해봐야겠네요",
          "original_form": "다시 복용해봐야겠네요",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024828.1.15",
          "form": "맛이 참 없지만 효과는 있는것 같아서 계속 먹게돼요",
          "original_form": "맛이 참 없지만 효과는 있는것 같아서 계속 먹게돼요",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024828.1.16",
          "form": "맛이 좀 이상하죠?ㅎㅎㅎ",
          "original_form": "맛이 좀 이상하죠?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024828.1.17",
          "form": "쓰고 좀 소독약 먹는 기분이고",
          "original_form": "쓰고 좀 소독약 먹는 기분이고",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024828.1.18",
          "form": "술이랑 다른 느낌으로 목구멍을 지지는 것 같아요",
          "original_form": "술이랑 다른 느낌으로 목구멍을 지지는 것 같아요",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024828.1.19",
          "form": "그래서 기침이 줄었나..?",
          "original_form": "그래서 기침이 줄었나..?",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024828.1.20",
          "form": "..음? 소독약ㅋㅋㅋ",
          "original_form": "..음? 소독약ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024828.1.21",
          "form": "딱이네요ㅋㅋ비유가",
          "original_form": "딱이네요ㅋㅋ비유가",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024828.1.22",
          "form": "사실 목구멍 지지는 느낌때문에 계속 쓰는거긴 해요",
          "original_form": "사실 목구멍 지지는 느낌때문에 계속 쓰는거긴 해요",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024828.1.23",
          "form": "효과가 진짜 있는거같아서요",
          "original_form": "효과가 진짜 있는거같아서요",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024828.1.24",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024828.1.25",
          "form": "꼭 다시 써봐야겠네요!",
          "original_form": "꼭 다시 써봐야겠네요!",
          "speaker_id": "1",
          "time": "20210831 22:09"
        }
      ]
    }
  ]
