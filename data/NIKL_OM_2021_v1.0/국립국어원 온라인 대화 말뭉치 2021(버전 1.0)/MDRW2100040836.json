      "utterance": [
        {
          "id": "MDRW2100040836.1.1",
          "form": "무슨 나라에서 돈을 엄청 뿌려서",
          "original_form": "무슨 나라에서 돈을 엄청 뿌려서",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100040836.1.2",
          "form": "쓸데없는 일을 많이 하는 거 같더라고요",
          "original_form": "쓸데없는 일을 많이 하는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100040836.1.3",
          "form": "오 어떤일이 쓸데없어 보이셨나요ㅋㅋㅋㅋ?",
          "original_form": "오 어떤일이 쓸데없어 보이셨나요ㅋㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040836.1.4",
          "form": "축제 만들고",
          "original_form": "축제 만들고",
          "speaker_id": "1",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040836.1.5",
          "form": "지방 자치 단체에서 하는 자잘한 교육",
          "original_form": "지방 자치 단체에서 하는 자잘한 교육",
          "speaker_id": "1",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040836.1.6",
          "form": "맨날 자격증 발급하는데",
          "original_form": "맨날 자격증 발급하는데",
          "speaker_id": "1",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040836.1.7",
          "form": "실제로 쓸 때는 없는 그런 자격증과 수료증 남발 하고요",
          "original_form": "실제로 쓸 때는 없는 그런 자격증과 수료증 남발 하고요",
          "speaker_id": "1",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040836.1.8",
          "form": "아 민간자격증이나 교육프로그램",
          "original_form": "아 민간자격증이나 교육프로그램",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040836.1.9",
          "form": "정말 쓸데없죸ㅋㅋㅋㅋ",
          "original_form": "정말 쓸데없죸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040836.1.10",
          "form": "그 강사들 직업 만들어 주는 프로그램 같아요",
          "original_form": "그 강사들 직업 만들어 주는 프로그램 같아요",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040836.1.11",
          "form": "커리어와 이력서에 한 줄 남기기 위해 돈과 시간을 쓰지먀",
          "original_form": "커리어와 이력서에 한 줄 남기기 위해 돈과 시간을 쓰지먀",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040836.1.12",
          "form": "맞아요 강사에겐 좋은 경력이 되겠죠",
          "original_form": "맞아요 강사에겐 좋은 경력이 되겠죠",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040836.1.13",
          "form": "요즘은 또 줌 교육 많이해서 영양가도 없고",
          "original_form": "요즘은 또 줌 교육 많이해서 영양가도 없고",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040836.1.14",
          "form": "생애사 쓰는 글쓰기 강의 이런 거 들어 보셨어요",
          "original_form": "생애사 쓰는 글쓰기 강의 이런 거 들어 보셨어요",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040836.1.15",
          "form": "정말요 공감합니다",
          "original_form": "정말요 공감합니다",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040836.1.16",
          "form": "완전 날림",
          "original_form": "완전 날림",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040836.1.17",
          "form": "글쓰기 강의도 자격증? 수료증같은게 나오나요?",
          "original_form": "글쓰기 강의도 자격증? 수료증같은게 나오나요?",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100040836.1.18",
          "form": "그건 잘 모르겠어요",
          "original_form": "그건 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100040836.1.19",
          "form": "제가 들은 건 번쩍번쩍한 수료증을 보내주긴 하더라구요",
          "original_form": "제가 들은 건 번쩍번쩍한 수료증을 보내주긴 하더라구요",
          "speaker_id": "2",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040836.1.20",
          "form": "그런 걸 또 받아 놔야 다른데 말할 때",
          "original_form": "그런 걸 또 받아 놔야 다른데 말할 때",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040836.1.21",
          "form": "덕분에 경력에 한 줄을 남길 순 있겠지만",
          "original_form": "덕분에 경력에 한 줄을 남길 순 있겠지만",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040836.1.22",
          "form": "쓰이더라고요 요지경이에요",
          "original_form": "쓰이더라고요 요지경이에요",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040836.1.23",
          "form": "맞아요ㅋㅋㅋㅋ 사실 보관도 어려워서 애물단지인데",
          "original_form": "맞아요ㅋㅋㅋㅋ 사실 보관도 어려워서 애물단지인데",
          "speaker_id": "2",
          "time": "20210905 09:46"
        }
      ]
    }
  ]
