      "utterance": [
        {
          "id": "MDRW2100012401.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012401.1.2",
          "form": "예전에는 물고기 새우 키웠는데 지금은 없어요",
          "original_form": "예전에는 물고기 새우 키웠는데 지금은 없어요",
          "speaker_id": "2",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100012401.1.3",
          "form": "와 새우를 키워요?",
          "original_form": "와 새우를 키워요?",
          "speaker_id": "1",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100012401.1.4",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100012401.1.5",
          "form": "체리새우요~ 진짜 작은거",
          "original_form": "체리새우요~ 진짜 작은거",
          "speaker_id": "2",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100012401.1.6",
          "form": "저는 초딩때 닭 키웠어요",
          "original_form": "저는 초딩때 닭 키웠어요",
          "speaker_id": "1",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100012401.1.7",
          "form": "와~~~~ 주택사셨나요?",
          "original_form": "와~~~~ 주택사셨나요?",
          "speaker_id": "2",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012401.1.8",
          "form": "병아리를 키워 닭으로?",
          "original_form": "병아리를 키워 닭으로?",
          "speaker_id": "2",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012401.1.9",
          "form": "빌라 살았어요ㅋㅋ",
          "original_form": "빌라 살았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012401.1.10",
          "form": "애지중지 키웠습니다",
          "original_form": "애지중지 키웠습니다",
          "speaker_id": "1",
          "time": "20210824 21:52"
        },
        {
          "id": "MDRW2100012401.1.11",
          "form": "소리 괜찮으셨나요?ㅋㅋ",
          "original_form": "소리 괜찮으셨나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012401.1.12",
          "form": "다른 주민은 개를 키워서",
          "original_form": "다른 주민은 개를 키워서",
          "speaker_id": "1",
          "time": "20210824 21:54"
        },
        {
          "id": "MDRW2100012401.1.13",
          "form": "이해해줬습니다",
          "original_form": "이해해줬습니다",
          "speaker_id": "1",
          "time": "20210824 21:54"
        },
        {
          "id": "MDRW2100012401.1.14",
          "form": "ㅋㅋㅋㅋ. 빌라에서 아침마다 꼬끼오~ 하면 색다를것같네요",
          "original_form": "ㅋㅋㅋㅋ. 빌라에서 아침마다 꼬끼오~ 하면 색다를것같네요",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012401.1.15",
          "form": "알람이 필요 없었습니다",
          "original_form": "알람이 필요 없었습니다",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012401.1.16",
          "form": "ㅋㅋ절로 부지런해지는ㅎㅎ 시간도 칼같이 맞추나요",
          "original_form": "ㅋㅋ절로 부지런해지는ㅎㅎ 시간도 칼같이 맞추나요",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012401.1.17",
          "form": "생각보다 자주안해요ㅋㅋ",
          "original_form": "생각보다 자주안해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:57"
        }
      ]
