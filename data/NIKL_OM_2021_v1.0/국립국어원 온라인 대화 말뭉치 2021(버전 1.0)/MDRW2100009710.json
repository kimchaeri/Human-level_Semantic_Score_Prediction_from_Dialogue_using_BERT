        {
          "id": "MDRW2100009710.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210823 07:33"
        },
        {
          "id": "MDRW2100009710.1.2",
          "form": "네 비 엄청 오네요",
          "original_form": "네 비 엄청 오네요",
          "speaker_id": "2",
          "time": "20210823 07:59"
        },
        {
          "id": "MDRW2100009710.1.3",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210823 08:04"
        },
        {
          "id": "MDRW2100009710.1.4",
          "form": "이번주 계속 비온다던데요 ㅠ",
          "original_form": "이번주 계속 비온다던데요 ㅠ",
          "speaker_id": "2",
          "time": "20210823 08:18"
        },
        {
          "id": "MDRW2100009710.1.5",
          "form": "걱정이에요",
          "original_form": "걱정이에요",
          "speaker_id": "1",
          "time": "20210823 08:34"
        },
        {
          "id": "MDRW2100009710.1.6",
          "form": "저도요. 여행 계획 잡았는데 비 예보 계속이라 취소해야 하나 싶어요",
          "original_form": "저도요. 여행 계획 잡았는데 비 예보 계속이라 취소해야 하나 싶어요",
          "speaker_id": "2",
          "time": "20210823 09:10"
        },
        {
          "id": "MDRW2100009710.1.7",
          "form": "안전이 최고죠",
          "original_form": "안전이 최고죠",
          "speaker_id": "1",
          "time": "20210823 09:49"
        },
        {
          "id": "MDRW2100009710.1.8",
          "form": "그쵸~ 오늘은 해도 안보이고 종일 우중충 하려나봐요",
          "original_form": "그쵸~ 오늘은 해도 안보이고 종일 우중충 하려나봐요",
          "speaker_id": "2",
          "time": "20210823 11:23"
        },
        {
          "id": "MDRW2100009710.1.9",
          "form": "태풍피해없으시길",
          "original_form": "태풍피해없으시길",
          "speaker_id": "1",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009710.1.10",
          "form": "여기는 그나마 아직 바람은 없고 비만 온종일 내리네요",
          "original_form": "여기는 그나마 아직 바람은 없고 비만 온종일 내리네요",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009710.1.11",
          "form": "여기도",
          "original_form": "여기도",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009710.1.12",
