      "utterance": [
        {
          "id": "MDRW2100006951.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006951.1.2",
          "form": "여행 가고 싶은 밤이네요....",
          "original_form": "여행 가고 싶은 밤이네요....",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006951.1.3",
          "form": "코로나 끝나면 어디 제일 가고 싶으세요?",
          "original_form": "코로나 끝나면 어디 제일 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006951.1.4",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006951.1.5",
          "form": "저 제주도가 가고싶네요",
          "original_form": "저 제주도가 가고싶네요",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006951.1.6",
          "form": "해외야행도 가보고싶고 ㅎㅎ",
          "original_form": "해외야행도 가보고싶고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006951.1.7",
          "form": "name1 님은 어떠신가요",
          "original_form": "&name1& 님은 어떠신가요",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006951.1.8",
          "form": "제주에 오면 뭐 하고 싶으신가요?",
          "original_form": "제주에 오면 뭐 하고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006951.1.9",
          "form": "저는 지금 제주인데",
          "original_form": "저는 지금 제주인데",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006951.1.10",
          "form": "몇 년 사니까 하고픈 것도 별로 없어요 ㅠㅠ",
          "original_form": "몇 년 사니까 하고픈 것도 별로 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006951.1.11",
          "form": "우와 제주도 사세요?",
          "original_form": "우와 제주도 사세요?",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006951.1.12",
          "form": "저는 바다보고 배타고 그러고싶어요~",
          "original_form": "저는 바다보고 배타고 그러고싶어요~",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006951.1.13",
          "form": "지금 스누피 전시회도 하는것같던데",
          "original_form": "지금 스누피 전시회도 하는것같던데",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006951.1.14",
          "form": "너무가보고싶고 ㅠㅠ",
          "original_form": "너무가보고싶고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006951.1.15",
          "form": "맞아요 예쁜 바닷가 많아요",
          "original_form": "맞아요 예쁜 바닷가 많아요",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006951.1.16",
          "form": "해수욕장마다 매력도 다 다르고",
          "original_form": "해수욕장마다 매력도 다 다르고",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006951.1.17",
          "form": "스누피 전시회는 아마 테마파크 같은 거라 상설 전시일 거예요",
          "original_form": "스누피 전시회는 아마 테마파크 같은 거라 상설 전시일 거예요",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006951.1.18",
          "form": "나중에 오셔도 충분히 보실 수 있을 거예요! ㅎㅎ",
          "original_form": "나중에 오셔도 충분히 보실 수 있을 거예요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006951.1.19",
          "form": "저는 스노쿨링이나 서핑 해보고 싶어요",
          "original_form": "저는 스노쿨링이나 서핑 해보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006951.1.20",
          "form": "와 정말요? 그럼 나중에라도 가서 봐야겠네요",
          "original_form": "와 정말요? 그럼 나중에라도 가서 봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006951.1.21",
          "form": "개인정으로 개인 예뻣던 바다 추찬해줄래요 ㅎㅎ",
          "original_form": "개인정으로 개인 예뻣던 바다 추찬해줄래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006951.1.22",
          "form": "꼭 가랴고여!",
          "original_form": "꼭 가랴고여!",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006951.1.23",
          "form": "외 스노쿨링은 뭐죠? 서핑은 저도 해보고싶어요 ㅠㅠ",
          "original_form": "외 스노쿨링은 뭐죠? 서핑은 저도 해보고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006951.1.24",
          "form": "시내 근처에선 함덕은 바다가 얕고 예쁘고, 이호테우에는 그 유명한 말 등대가 있고 서핑 많이 해요",
          "original_form": "시내 근처에선 함덕은 바다가 얕고 예쁘고, 이호테우에는 그 유명한 말 등대가 있고 서핑 많이 해요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006951.1.25",
          "form": "서쪽은 애월쬭 한담해안산책로 걸어보세요 넘 예뻐요",
          "original_form": "서쪽은 애월쬭 한담해안산책로 걸어보세요 넘 예뻐요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006951.1.26",
          "form": "스노쿨링 그거요 입에 길쭉한 거 물고 숨쉬면서 뮬속 구경하는 거요 ㅋㅋㅋ",
          "original_form": "스노쿨링 그거요 입에 길쭉한 거 물고 숨쉬면서 뮬속 구경하는 거요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006951.1.27",
          "form": "추천 감사해요!",
          "original_form": "추천 감사해요!",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100006951.1.28",
          "form": "기록 남겨두고 다음에 여행루트 짤때 꼭 고려할게요 ㅎㅎ",
          "original_form": "기록 남겨두고 다음에 여행루트 짤때 꼭 고려할게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100006951.1.29",
          "form": "아아 그거구나",
          "original_form": "아아 그거구나",
          "speaker_id": "2",
          "time": "20210822 10:46"
        },
        {
          "id": "MDRW2100006951.1.30",
          "form": "친구가 하는거 본적은 있는데 되게 좋을것같더라고요",
          "original_form": "친구가 하는거 본적은 있는데 되게 좋을것같더라고요",
          "speaker_id": "2",
          "time": "20210822 10:46"
        },
        {
          "id": "MDRW2100006951.1.31",
          "form": "바다풍경이 훤이 보이겠죠? 샹각만해도 떨리는 ㅠㅠ",
          "original_form": "바다풍경이 훤이 보이겠죠? 샹각만해도 떨리는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:46"
        },
        {
          "id": "MDRW2100006951.1.32",
          "form": "그러게요 저도 그래서 해보고 싶어요",
          "original_form": "그러게요 저도 그래서 해보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100006951.1.33",
          "form": "해외는 어디 가보고 싶으세요?",
          "original_form": "해외는 어디 가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100006951.1.34",
          "form": "저는 프랑스에 가보고 싶어요",
          "original_form": "저는 프랑스에 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100006951.1.35",
          "form": "도대체 언제가 될지....",
          "original_form": "도대체 언제가 될지....",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100006951.1.36",
          "form": "저는 일본 가보고싶네요",
          "original_form": "저는 일본 가보고싶네요",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100006951.1.37",
          "form": "가깝기도 하고 친구들도 많이가고",
          "original_form": "가깝기도 하고 친구들도 많이가고",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100006951.1.38",
          "form": "코로나 전에 일본 여행 진짜 많이들 갔었던 것 같아요",
          "original_form": "코로나 전에 일본 여행 진짜 많이들 갔었던 것 같아요",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100006951.1.39",
          "form": "제주보다 일본 가는 게 더 싸기도 했잖아요 심지어 ㅋㅋ",
          "original_form": "제주보다 일본 가는 게 더 싸기도 했잖아요 심지어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100006951.1.40",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100006951.1.41",
          "form": "제주가.. 진짜 금값이죠",
          "original_form": "제주가.. 진짜 금값이죠",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100006951.1.42",
          "form": "한번 다녀오면 돈이 ㄷㄷ",
          "original_form": "한번 다녀오면 돈이 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100006951.1.43",
          "form": "그래서 제주에 계신다는게 되게 부럽게 느껴져요",
          "original_form": "그래서 제주에 계신다는게 되게 부럽게 느껴져요",
          "speaker_id": "2",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100006951.1.44",
          "form": "바다가 너무 보거싶네요 ㅠㅠ",
          "original_form": "바다가 너무 보거싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100006951.1.45",
          "form": "어서 코로나 좀 끝나서",
          "original_form": "어서 코로나 좀 끝나서",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100006951.1.46",
          "form": "바다 보이는 예쁜 카페에서 힐링 하시는 날이 오길",
          "original_form": "바다 보이는 예쁜 카페에서 힐링 하시는 날이 오길",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100006951.1.47",
          "form": "바랄게요 ㅎㅎㅎ",
          "original_form": "바랄게요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100006951.1.48",
          "form": "한담해안산책로 꼭 걸어보시고요!!! ^^",
          "original_form": "한담해안산책로 꼭 걸어보시고요!!! ^^",
