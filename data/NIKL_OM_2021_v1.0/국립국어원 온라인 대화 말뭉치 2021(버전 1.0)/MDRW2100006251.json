      "utterance": [
        {
          "id": "MDRW2100006251.1.1",
          "form": "학창시절 추억 있으신가요",
          "original_form": "학창시절 추억 있으신가요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006251.1.2",
          "form": "네 놀러다녓어요",
          "original_form": "네 놀러다녓어요",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006251.1.3",
          "form": "술먹고",
          "original_form": "술먹고",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006251.1.4",
          "form": "님은용",
          "original_form": "님은용",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006251.1.5",
          "form": "저는 학창시절 맞았던 기억이..",
          "original_form": "저는 학창시절 맞았던 기억이..",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006251.1.6",
          "form": "헐 누구한테요??",
          "original_form": "헐 누구한테요??",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006251.1.7",
          "form": "선생님들한테요...체벌 없었나요?",
          "original_form": "선생님들한테요...체벌 없었나요?",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006251.1.8",
          "form": "넹... 어디때려요",
          "original_form": "넹... 어디때려요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006251.1.9",
          "form": "손.발.엉덩이..고루..맞았습니다",
          "original_form": "손.발.엉덩이..고루..맞았습니다",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006251.1.10",
          "form": "ㅜㅜㅜ 아이고ㅜㅜㅜ 뭘로요?",
          "original_form": "ㅜㅜㅜ 아이고ㅜㅜㅜ 뭘로요?",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006251.1.11",
          "form": "들고다니는..지시봉이라고 해야할까요",
          "original_form": "들고다니는..지시봉이라고 해야할까요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006251.1.12",
          "form": "ㅜㅜ 아프셨겟군요 ㅠㅠㅠ",
          "original_form": "ㅜㅜ 아프셨겟군요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006251.1.13",
          "form": "네 맞다가 뛰쳐나간적도...",
          "original_form": "네 맞다가 뛰쳐나간적도...",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006251.1.14",
          "form": "헐... 그기억이 크시군요",
          "original_form": "헐... 그기억이 크시군요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006251.1.15",
          "form": "찬구들관 어땍나요",
          "original_form": "찬구들관 어땍나요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006251.1.16",
          "form": "농구동아리를 재밌게 했어요",
          "original_form": "농구동아리를 재밌게 했어요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006251.1.17",
          "form": "오 ... 혹시 남자세여?",
          "original_form": "오 ... 혹시 남자세여?",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
