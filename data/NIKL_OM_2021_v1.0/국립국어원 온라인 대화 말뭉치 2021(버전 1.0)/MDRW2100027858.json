        {
          "id": "MDRW2100027858.1.1",
          "form": "요리는 하시는 편인가요?",
          "original_form": "요리는 하시는 편인가요?",
          "speaker_id": "1",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027858.1.2",
          "form": "요리 잘은 못해도",
          "original_form": "요리 잘은 못해도",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027858.1.3",
          "form": "자주 합니다 ㅋㅋㅋ",
          "original_form": "자주 합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027858.1.4",
          "form": "최근에 만든 요리는 뭔가요?",
          "original_form": "최근에 만든 요리는 뭔가요?",
          "speaker_id": "1",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100027858.1.5",
          "form": "지금 고기를 구울 예정입니다",
          "original_form": "지금 고기를 구울 예정입니다",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100027858.1.6",
          "form": "최근에 만든거는 라면인데요 ㅋㅋㅋ",
          "original_form": "최근에 만든거는 라면인데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100027858.1.7",
          "form": "어떤 고기 드세요??",
          "original_form": "어떤 고기 드세요??",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100027858.1.8",
          "form": "소고기죠 고기는!!",
          "original_form": "소고기죠 고기는!!",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100027858.1.9",
          "form": "소고기 넘 맛있죠ㅠㅠ 전 좀 덜익혀 먹는거 선호해요",
          "original_form": "소고기 넘 맛있죠ㅠㅠ 전 좀 덜익혀 먹는거 선호해요",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100027858.1.10",
          "form": "ㅋㅋ레어정도로요?",
          "original_form": "ㅋㅋ레어정도로요?",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100027858.1.11",
          "form": "레어도 좋고 육회도 잘먹어요",
          "original_form": "레어도 좋고 육회도 잘먹어요",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100027858.1.12",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
