      "utterance": [
        {
          "id": "MDRW2100006642.1.1",
          "form": "왕십리 사람이랑 같이 가서 먹었는데",
          "original_form": "왕십리 사람이랑 같이 가서 먹었는데",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100006642.1.2",
          "form": "왕십리 곱창은 거품이 심하다 여기가 낫다",
          "original_form": "왕십리 곱창은 거품이 심하다 여기가 낫다",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100006642.1.3",
          "form": "라고 평을 하더라구요ㅋㅋㅋㅋ",
          "original_form": "라고 평을 하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100006642.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100006642.1.5",
          "form": "돼지곱창 + 소금이라니",
          "original_form": "돼지곱창 + 소금이라니",
          "speaker_id": "1",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100006642.1.6",
          "form": "또 사람 미치죠",
          "original_form": "또 사람 미치죠",
          "speaker_id": "1",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100006642.1.7",
          "form": "돼지곱창이 양념 없으면 잡내가 심하기 마련인데",
          "original_form": "돼지곱창이 양념 없으면 잡내가 심하기 마련인데",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100006642.1.8",
          "form": "되게 꼬소하고 부드럽고 맛있어요",
          "original_form": "되게 꼬소하고 부드럽고 맛있어요",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100006642.1.9",
          "form": "혹시 부산 맛집 안필요하신가요?",
          "original_form": "혹시 부산 맛집 안필요하신가요?",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100006642.1.10",
          "form": "부산 맛집 당연히 받죠",
          "original_form": "부산 맛집 당연히 받죠",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100006642.1.11",
          "form": "맛집 추천은 지역을 가리지 않습니다",
          "original_form": "맛집 추천은 지역을 가리지 않습니다",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100006642.1.12",
          "form": "해운대소문난암소갈비",
          "original_form": "해운대소문난암소갈비",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100006642.1.13",
          "form": "여기 한우치고 가격이 나쁘지 않은데 맛있어요....",
          "original_form": "여기 한우치고 가격이 나쁘지 않은데 맛있어요....",
          "speaker_id": "2",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100006642.1.14",
          "form": "와....저 봐버렸습니다",
          "original_form": "와....저 봐버렸습니다",
          "speaker_id": "1",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100006642.1.15",
          "form": "그 영롱한 분홍빛",
          "original_form": "그 영롱한 분홍빛",
          "speaker_id": "1",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006642.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ위험한 길에 발을 들이셨군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ위험한 길에 발을 들이셨군요",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006642.1.17",
          "form": "검색이라는 위험한 선택을 해버렸습니다",
          "original_form": "검색이라는 위험한 선택을 해버렸습니다",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006642.1.18",
          "form": "여름에 좀 더운거 빼고 정말 좋았던 집입니다",
          "original_form": "여름에 좀 더운거 빼고 정말 좋았던 집입니다",
          "speaker_id": "2",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006642.1.19",
          "form": "그리고 부산역 앞에 신발원도 꼭 가주세요",
          "original_form": "그리고 부산역 앞에 신발원도 꼭 가주세요",
          "speaker_id": "2",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006642.1.20",
          "form": "허영만 선생님 픽이라면 믿을 수밖에 없죠",
          "original_form": "허영만 선생님 픽이라면 믿을 수밖에 없죠",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006642.1.21",
          "form": "여긴 웨이팅 꽤 있는데",
          "original_form": "여긴 웨이팅 꽤 있는데",
          "speaker_id": "2",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006642.1.22",
          "form": "제가 절대 웨이팅 안하는 주의인데 여긴 합니다ㅋㅋㅋㅋ",
          "original_form": "제가 절대 웨이팅 안하는 주의인데 여긴 합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006642.1.23",
          "form": "하.....여긴 또 하얀빛이 영롱하네요",
          "original_form": "하.....여긴 또 하얀빛이 영롱하네요",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006642.1.24",
          "form": "육즙팡팡 만두입니다",
          "original_form": "육즙팡팡 만두입니다",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006642.1.25",
          "form": "제가 또 만두에 환장하는걸 어떻게 아시고",
          "original_form": "제가 또 만두에 환장하는걸 어떻게 아시고",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006642.1.26",
          "form": "뜨거울 때 입에 와앙하고 넣어서 팡팡 터지는 육즙 꼭 드셔보세요",
          "original_form": "뜨거울 때 입에 와앙하고 넣어서 팡팡 터지는 육즙 꼭 드셔보세요",
          "speaker_id": "2",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100006642.1.27",
          "form": "그리고 뜨겁게 데인 입안을 단무지로 식혀주세요",
          "original_form": "그리고 뜨겁게 데인 입안을 단무지로 식혀주세요",
          "speaker_id": "2",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100006642.1.28",
          "form": "와....드실 줄 아시는 분이군요?",
          "original_form": "와....드실 줄 아시는 분이군요?",
          "speaker_id": "1",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006642.1.29",
          "form": "만두 먹고 단무지 필승이죠",
          "original_form": "만두 먹고 단무지 필승이죠",
          "speaker_id": "1",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006642.1.30",
          "form": "후훗 매운걸 몸이 못버텨서 못먹어서 그렇지",
          "original_form": "후훗 매운걸 몸이 못버텨서 못먹어서 그렇지",
          "speaker_id": "2",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100006642.1.31",
          "form": "돼지력 상당합니다ㅋㅋㅋㅋ",
          "original_form": "돼지력 상당합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100006642.1.32",
          "form": "여행와서 너무 거한거만 먹기 부담스러우시다 하면",
          "original_form": "여행와서 너무 거한거만 먹기 부담스러우시다 하면",
          "speaker_id": "2",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100006642.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런 분이시라면 진라면 순한맛 인정해드립니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런 분이시라면 진라면 순한맛 인정해드립니다",
          "speaker_id": "1",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100006642.1.34",
          "form": "자갈치역 바로 앞에 고등어 구이 집 모여있거든요",
          "original_form": "자갈치역 바로 앞에 고등어 구이 집 모여있거든요",
          "speaker_id": "2",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100006642.1.35",
          "form": "거기가 또 기가 막힙니다",
          "original_form": "거기가 또 기가 막힙니다",
