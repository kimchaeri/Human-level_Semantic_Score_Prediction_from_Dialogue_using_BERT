      "utterance": [
        {
          "id": "MDRW2100047592.1.1",
          "form": "자기전 ㅎㅎ유튜브",
          "original_form": "자기전 ㅎㅎ유튜브",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047592.1.2",
          "form": "유튜브 뭐 보세요?",
          "original_form": "유튜브 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100047592.1.3",
          "form": "주식요 ㅋㅋ",
          "original_form": "주식요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100047592.1.4",
          "form": "요새 공모주 방송봐요",
          "original_form": "요새 공모주 방송봐요",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100047592.1.5",
          "form": "웃긴게 ㅋㅋ",
          "original_form": "웃긴게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100047592.1.6",
          "form": "어제 방송보고 마지막에 청약했는데",
          "original_form": "어제 방송보고 마지막에 청약했는데",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047592.1.7",
          "form": "결국 결과가 똑같아요",
          "original_form": "결국 결과가 똑같아요",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047592.1.8",
          "form": "뭔짓을 한건지 하루가 다갔어요",
          "original_form": "뭔짓을 한건지 하루가 다갔어요",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047592.1.9",
          "form": "음악도 들어요",
          "original_form": "음악도 들어요",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047592.1.10",
          "form": "아무래도 지난 노래들 유튜브 틀고",
          "original_form": "아무래도 지난 노래들 유튜브 틀고",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047592.1.11",
          "form": "집안일해요",
          "original_form": "집안일해요",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047592.1.12",
          "form": "뭐보세요?",
          "original_form": "뭐보세요?",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100047592.1.13",
          "form": "주식 ㅋㅋㅋㅋㅋ",
          "original_form": "주식 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100047592.1.14",
          "form": "요즘 주식유튜브 정말 많이보더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "요즘 주식유튜브 정말 많이보더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100047592.1.15",
          "form": "저는 운동 유튜브랑 강아지 유튜브 봐요",
          "original_form": "저는 운동 유튜브랑 강아지 유튜브 봐요",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100047592.1.16",
          "form": "아 운동고 한때막 맨날 따라했어요",
          "original_form": "아 운동고 한때막 맨날 따라했어요",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100047592.1.17",
          "form": "강아지 유튜브 키우시는거죠?",
          "original_form": "강아지 유튜브 키우시는거죠?",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100047592.1.18",
          "form": "저는 강아지를 키우지않고 유튜브로 보기만 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 강아지를 키우지않고 유튜브로 보기만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100047592.1.19",
          "form": "잉 저도 사실",
          "original_form": "잉 저도 사실",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100047592.1.20",
          "form": "강아지 나오는거 티비서 맨날 보고 그랬어요",
          "original_form": "강아지 나오는거 티비서 맨날 보고 그랬어요",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100047592.1.21",
          "form": "개는 훌륭하다 인가",
          "original_form": "개는 훌륭하다 인가",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100047592.1.22",
          "form": "키우는건 현재로서는 엄두가 나질 않아서요 ㅋㅋㅋㅋㅋ",
          "original_form": "키우는건 현재로서는 엄두가 나질 않아서요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100047592.1.23",
          "form": "정말 준비가 되엇을때 데려오고 싶어요",
          "original_form": "정말 준비가 되엇을때 데려오고 싶어요",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100047592.1.24",
          "form": "그래야해요",
          "original_form": "그래야해요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047592.1.25",
          "form": "지난노래 유튜브도 좋아요",
          "original_form": "지난노래 유튜브도 좋아요",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047592.1.26",
          "form": "방송보며 맘 달래시고 꼭 나중에 데려오세요",
          "original_form": "방송보며 맘 달래시고 꼭 나중에 데려오세요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047592.1.27",
          "form": "네 유튜브 최고죠",
          "original_form": "네 유튜브 최고죠",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047592.1.28",
          "form": "발라드",
          "original_form": "발라드",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047592.1.29",
          "form": "맞아요 유튜브 보면서 공부도 하고 귀여워도 하고 그래요",
          "original_form": "맞아요 유튜브 보면서 공부도 하고 귀여워도 하고 그래요",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100047592.1.30",
          "form": "ㅋㅋ귀여워요?",
          "original_form": "ㅋㅋ귀여워요?",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100047592.1.31",
          "form": "강아지?",
          "original_form": "강아지?",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100047592.1.32",
          "form": "공부 맞아요 요샌 영상보고해요",
          "original_form": "공부 맞아요 요샌 영상보고해요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047592.1.33",
          "form": "네 사모예드 너무 귀여워욬ㅋㅋㅋㅋㅋ",
          "original_form": "네 사모예드 너무 귀여워욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047592.1.34",
          "form": "아 저희딸 키우고 싶데요",
          "original_form": "아 저희딸 키우고 싶데요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047592.1.35",
          "form": "전 좀커서 무서워요",
          "original_form": "전 좀커서 무서워요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047592.1.36",
          "form": "절대 안돼요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "절대 안돼요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047592.1.37",
          "form": "사모예드... 장난아니에요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사모예드... 장난아니에요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047592.1.38",
          "form": "저도 사모예드 너무 예쁘지만 나중에 강아지 키우게 되더라도 사모예드는 안키울거예요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 사모예드 너무 예쁘지만 나중에 강아지 키우게 되더라도 사모예드는 안키울거예요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047592.1.39",
          "form": "그럼요 집에서 케어하긴 털도 크기도 ㅋㅋ",
          "original_form": "그럼요 집에서 케어하긴 털도 크기도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100047592.1.40",
          "form": "엄두 안나요",
          "original_form": "엄두 안나요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100047592.1.41",
          "form": "작은 녀석으로",
          "original_form": "작은 녀석으로",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100047592.1.42",
          "form": "털빠짐 크기뿐만아니라 활동량이 미쳣더라구요",
          "original_form": "털빠짐 크기뿐만아니라 활동량이 미쳣더라구요",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100047592.1.43",
          "form": "거기에 여름을 심하게 타서 하루종일 에어컨틀어줘야하고ㅠㅠ",
          "original_form": "거기에 여름을 심하게 타서 하루종일 에어컨틀어줘야하고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100047592.1.44",
          "form": "아 그럼 키우기 곤란해요",
          "original_form": "아 그럼 키우기 곤란해요",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100047592.1.45",
          "form": "덩치도 커서 사모예드가 작정하고 힘주면 주인들이 종이인형처럼 나풀나풀 끌려가욬ㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "덩치도 커서 사모예드가 작정하고 힘주면 주인들이 종이인형처럼 나풀나풀 끌려가욬ㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047592.1.46",
          "form": "운동 시키기도 힘들고",
          "original_form": "운동 시키기도 힘들고",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047592.1.47",
          "form": "맞아요 끌려가요",
          "original_form": "맞아요 끌려가요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047592.1.48",
          "form": "따님에게",
          "original_form": "따님에게",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047592.1.49",
          "form": "극한견주 라는 만화책 추천해요",
          "original_form": "극한견주 라는 만화책 추천해요",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047592.1.50",
          "form": "사모예드 키우는 여성작가분이 쓰신 책인데",
          "original_form": "사모예드 키우는 여성작가분이 쓰신 책인데",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047592.1.51",
          "form": "아... 내가 감당할 수 잇는 개가 아니구나... 라는 깨달음을 얻을 수 잇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아... 내가 감당할 수 잇는 개가 아니구나... 라는 깨달음을 얻을 수 잇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047592.1.52",
          "form": "오 봐볼께요 ㅎㅎ",
          "original_form": "오 봐볼께요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047592.1.53",
          "form": "저희딸 좋아할듯요",
          "original_form": "저희딸 좋아할듯요",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047592.1.54",
          "form": "아니면 영상도 괜찮아요",
          "original_form": "아니면 영상도 괜찮아요",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047592.1.55",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047592.1.56",
          "form": "강형욱 동영상중에",
          "original_form": "강형욱 동영상중에",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100047592.1.57",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100047592.1.58",
          "form": "견종백과 사모예드 편이 잇거든요",
          "original_form": "견종백과 사모예드 편이 잇거든요",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100047592.1.59",
          "form": "미리 이렇게 봐야 잘키워요",
          "original_form": "미리 이렇게 봐야 잘키워요",
          "speaker_id": "1",
          "time": "20210826 22:01"
