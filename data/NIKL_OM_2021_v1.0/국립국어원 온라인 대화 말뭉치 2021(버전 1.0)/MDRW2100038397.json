      "utterance": [
        {
          "id": "MDRW2100038397.1.1",
          "form": "학교에서 포도 나오면",
          "original_form": "학교에서 포도 나오면",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038397.1.2",
          "form": "씨 있는 포도 나오지 않나요",
          "original_form": "씨 있는 포도 나오지 않나요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038397.1.3",
          "form": "네 ㅋㅋㅋ 씨랑 껍질 바르기 귀찮아서 다 삼켯어요",
          "original_form": "네 ㅋㅋㅋ 씨랑 껍질 바르기 귀찮아서 다 삼켯어요",
          "speaker_id": "1",
          "time": "20210902 12:49"
        },
        {
          "id": "MDRW2100038397.1.4",
          "form": "헉 그러면 뱃속에서 나무 자란대요",
          "original_form": "헉 그러면 뱃속에서 나무 자란대요",
          "speaker_id": "2",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100038397.1.5",
          "form": "헉 제 뱃속에 포도밭이..",
          "original_form": "헉 제 뱃속에 포도밭이..",
          "speaker_id": "1",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100038397.1.6",
          "form": "포도주 사업해야겟어요 ㅜㅜ",
          "original_form": "포도주 사업해야겟어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100038397.1.7",
          "form": "앜ㅋㅋㅋ 이걸 다시 개그로 받아주시다니",
          "original_form": "앜ㅋㅋㅋ 이걸 다시 개그로 받아주시다니",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100038397.1.8",
          "form": "저도 어릴때 수박",
          "original_form": "저도 어릴때 수박",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038397.1.9",
          "form": "씨앗 삼키면 배에서 자란대서",
          "original_form": "씨앗 삼키면 배에서 자란대서",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038397.1.10",
          "form": "일부러 다 뱉엇는데",
          "original_form": "일부러 다 뱉엇는데",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038397.1.11",
          "form": "그 기억이나네요 ㅋㅋㅋㅋ",
          "original_form": "그 기억이나네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038397.1.12",
          "form": "저도 그래서 씨 다 뱉었어요",
          "original_form": "저도 그래서 씨 다 뱉었어요",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038397.1.13",
          "form": "그래놓고 참외는 쿨하게 다 먹었죠",
          "original_form": "그래놓고 참외는 쿨하게 다 먹었죠",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038397.1.14",
          "form": "아 맞아요 ㅋㅋㅋㅋ 참외는 뱉을 생각을 안햇네요",
          "original_form": "아 맞아요 ㅋㅋㅋㅋ 참외는 뱉을 생각을 안햇네요",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038397.1.15",
          "form": "ㅋㅋㅋㅋㅋ 그때는 왜 그생각을 못했는지",
          "original_form": "ㅋㅋㅋㅋㅋ 그때는 왜 그생각을 못했는지",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038397.1.16",
          "form": "참외는 그부분이 맛잇어서 ㅜ",
          "original_form": "참외는 그부분이 맛잇어서 ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038397.1.17",
          "form": "포도랑 수박만 꼬박꼬박 걸러 먹는 인간이 되었습니다",
          "original_form": "포도랑 수박만 꼬박꼬박 걸러 먹는 인간이 되었습니다",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038397.1.18",
          "form": "ㅋㅋㅋㅋ저도욬ㅋㅋㅋ 사람사는게 다 똑같네요",
          "original_form": "ㅋㅋㅋㅋ저도욬ㅋㅋㅋ 사람사는게 다 똑같네요",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100038397.1.19",
          "form": "학교에서 참외 나오면 씨앗이 다 발라져서 나와요",
          "original_form": "학교에서 참외 나오면 씨앗이 다 발라져서 나와요",
