      "utterance": [
        {
          "id": "MDRW2100036169.1.1",
          "form": "얼룬 가울 옷 입고픈데 넘 더워요",
          "original_form": "얼룬 가울 옷 입고픈데 넘 더워요",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036169.1.2",
          "form": "전 옷정리가 싫어서",
          "original_form": "전 옷정리가 싫어서",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036169.1.3",
          "form": "따뜻하면 좋겠어욬",
          "original_form": "따뜻하면 좋겠어욬",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036169.1.4",
          "form": "아 그러시구나 ㅋㅋㅋ",
          "original_form": "아 그러시구나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036169.1.5",
          "form": "저는 얼른 추워졌음 싶어요",
          "original_form": "저는 얼른 추워졌음 싶어요",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100036169.1.6",
          "form": "전,, 청소가 귀찮아서 여름을 잡고 싶은 사람 ㅋㅋ",
          "original_form": "전,, 청소가 귀찮아서 여름을 잡고 싶은 사람 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100036169.1.7",
          "form": "지금 딱 좋지 않나요?",
          "original_form": "지금 딱 좋지 않나요?",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100036169.1.8",
          "form": "아 요즘도 여긴 너무 습하고 더워서용",
          "original_form": "아 요즘도 여긴 너무 습하고 더워서용",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100036169.1.9",
          "form": "추우면 청소 더 잘되지않나용ㅋㅋㅋ",
          "original_form": "추우면 청소 더 잘되지않나용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036169.1.10",
          "form": "날이 추운거랑 제가 직접 쓸고 닦는건 다른 문제니가욧",
          "original_form": "날이 추운거랑 제가 직접 쓸고 닦는건 다른 문제니가욧",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036169.1.11",
          "form": "그러게요 ㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036169.1.12",
          "form": "지금을 즐길줄 알아야하능데!",
          "original_form": "지금을 즐길줄 알아야하능데!",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036169.1.13",
          "form": "저는 청소라는 고통을 뒤로 지연시키고싶은 거 ㅅ뿐이에요 ㅋㅋㅋ",
          "original_form": "저는 청소라는 고통을 뒤로 지연시키고싶은 거 ㅅ뿐이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100036169.1.14",
          "form": "귀찮귀찮",
          "original_form": "귀찮귀찮",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036169.1.15",
          "form": "옷도 또 세탁소 맡겨야하구 ㅋㅋ",
          "original_form": "옷도 또 세탁소 맡겨야하구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036169.1.16",
          "form": "네 그걸 지연하기위래 좀더 늦게 가을이오길..",
          "original_form": "네 그걸 지연하기위래 좀더 늦게 가을이오길..",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100036169.1.17",
          "form": "낼부터 일주일 비오면 이제 1도씩 떨어지네용",
          "original_form": "낼부터 일주일 비오면 이제 1도씩 떨어지네용",
          "speaker_id": "1",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100036169.1.18",
          "form": "가을옷을 안 꺼낸 저,, 얼어죽지 않으려면 창문을 닫고 자야겠어요",
          "original_form": "가을옷을 안 꺼낸 저,, 얼어죽지 않으려면 창문을 닫고 자야겠어요",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036169.1.19",
          "form": "앜ㅋㅋㅋ저도요 아직 나시에요",
          "original_form": "앜ㅋㅋㅋ저도요 아직 나시에요",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036169.1.20",
          "form": "가디건이 절 살리고 있어요",
          "original_form": "가디건이 절 살리고 있어요",
          "speaker_id": "2",
          "time": "20210830 21:08"
        }
      ]
