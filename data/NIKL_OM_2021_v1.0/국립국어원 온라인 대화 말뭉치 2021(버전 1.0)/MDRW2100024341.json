      "utterance": [
        {
          "id": "MDRW2100024341.1.1",
          "form": "name2님 육쪽마늘빵 드셔보셧어요?ㅎㅎ",
          "original_form": "&name2&님 육쪽마늘빵 드셔보셧어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024341.1.2",
          "form": "헐 네 그거도 완전 미쳤죠",
          "original_form": "헐 네 그거도 완전 미쳤죠",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024341.1.3",
          "form": "ㅠㅠ빵 좋아하세요?",
          "original_form": "ㅠㅠ빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024341.1.4",
          "form": "와.. 몇개 드셔보셧길래 ㅋㅋㅋ",
          "original_form": "와.. 몇개 드셔보셧길래 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024341.1.5",
          "form": "그러는거죠?ㅋㅋㅋ",
          "original_form": "그러는거죠?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024341.1.6",
          "form": "그러는 거라는 게 무슨 말이죠?",
          "original_form": "그러는 거라는 게 무슨 말이죠?",
          "speaker_id": "2",
          "time": "20210831 01:16"
        },
        {
          "id": "MDRW2100024341.1.7",
          "form": "아 너무 좋아하셔서",
          "original_form": "아 너무 좋아하셔서",
          "speaker_id": "1",
          "time": "20210831 01:17"
        },
        {
          "id": "MDRW2100024341.1.8",
          "form": "ㅋㅋㅋㅋ너무 좋아하시냐는 말투엿어여..ㅜ\r\n오해의 소재가 잇엇네요.\r\n저도 좋아하거든요",
          "original_form": "ㅋㅋㅋㅋ너무 좋아하시냐는 말투엿어여..ㅜ\r\n오해의 소재가 잇엇네요.\r\n저도 좋아하거든요",
          "speaker_id": "1",
          "time": "20210831 01:17"
        },
        {
          "id": "MDRW2100024341.1.9",
          "form": "(술마셔서 약간 기분이 좀 업되어잇어요)",
          "original_form": "(술마셔서 약간 기분이 좀 업되어잇어요)",
          "speaker_id": "1",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024341.1.10",
          "form": "ㅋㅋㅋ 아 술드셨군요..",
          "original_form": "ㅋㅋㅋ 아 술드셨군요..",
          "speaker_id": "2",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024341.1.11",
          "form": "얼마 먹진 못했어요 매장에서 바로 먹었으면",
          "original_form": "얼마 먹진 못했어요 매장에서 바로 먹었으면",
          "speaker_id": "2",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024341.1.12",
          "form": "더 사왓을텐데...",
          "original_form": "더 사왓을텐데...",
          "speaker_id": "2",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024341.1.13",
          "form": "ㅋㅋㅋ 죄송해요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 죄송해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024341.1.14",
          "form": "아하 그거 막 갯수한정도ㅜ잇더러고요",
          "original_form": "아하 그거 막 갯수한정도ㅜ잇더러고요",
          "speaker_id": "1",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024341.1.15",
          "form": "ㅋㅋㅋ괜찮아요",
          "original_form": "ㅋㅋㅋ괜찮아요",
          "speaker_id": "2",
          "time": "20210831 01:20"
        },
        {
          "id": "MDRW2100024341.1.16",
          "form": "맞아요ㅠ 그 속초봉브레드도",
          "original_form": "맞아요ㅠ 그 속초봉브레드도",
          "speaker_id": "2",
          "time": "20210831 01:20"
        },
        {
          "id": "MDRW2100024341.1.17",
          "form": "제친구네 언니분도 장사 조금 덜되는곳에서 \r\n인당 갯수 많은곳에서 사셧더라고요 ㅠ",
          "original_form": "제친구네 언니분도 장사 조금 덜되는곳에서 \r\n인당 갯수 많은곳에서 사셧더라고요 ㅠ",
          "speaker_id": "1",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100024341.1.18",
          "form": "맛있다던데",
          "original_form": "맛있다던데",
          "speaker_id": "2",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100024341.1.19",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "2",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100024341.1.20",
          "form": "저도 그거 먹것어요!!",
          "original_form": "저도 그거 먹것어요!!",
          "speaker_id": "1",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100024341.1.21",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100024341.1.22",
          "form": "달달해요 ㅋㅋㅋ 속초가서 사먹엇어요",
          "original_form": "달달해요 ㅋㅋㅋ 속초가서 사먹엇어요",
