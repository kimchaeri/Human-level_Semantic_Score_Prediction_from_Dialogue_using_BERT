      "utterance": [
        {
          "id": "MDRW2100026320.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026320.1.2",
          "form": "연어 들어간 음식 좋아해요",
          "original_form": "연어 들어간 음식 좋아해요",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026320.1.3",
          "form": "오 저도 연어 좋아해요",
          "original_form": "오 저도 연어 좋아해요",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026320.1.4",
          "form": "부페가면 연어부터 ㅋㅋ",
          "original_form": "부페가면 연어부터 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026320.1.5",
          "form": "연어 싫어하는 사람 있나요? ㅋㅋ",
          "original_form": "연어 싫어하는 사람 있나요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026320.1.6",
          "form": "은근 있더라고요 ㅋㅋ 기름기 있다보니..",
          "original_form": "은근 있더라고요 ㅋㅋ 기름기 있다보니..",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026320.1.7",
          "form": "아 기름이 있긴 하죠 그래도 맛 좋고 몸에도 좋은데요",
          "original_form": "아 기름이 있긴 하죠 그래도 맛 좋고 몸에도 좋은데요",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026320.1.8",
          "form": "네 맞아요 ㅎㅎ 케이퍼랑 같이 먹으면 너무 맛있어요!",
          "original_form": "네 맞아요 ㅎㅎ 케이퍼랑 같이 먹으면 너무 맛있어요!",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026320.1.9",
          "form": "케이퍼가 콩같이 생긴 거 맞나요? 이름을 잘 몰라서요",
          "original_form": "케이퍼가 콩같이 생긴 거 맞나요? 이름을 잘 몰라서요",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100026320.1.10",
          "form": "네네 맞아요 연어요리에 같이 나오는거요 연두색 ㅎㅎ",
          "original_form": "네네 맞아요 연어요리에 같이 나오는거요 연두색 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100026320.1.11",
          "form": "오 덕분에 새롭게 배우네요",
          "original_form": "오 덕분에 새롭게 배우네요",
          "speaker_id": "2",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100026320.1.12",
          "form": "ㅎㅎ 그럼 못 드시는 음식은요?",
          "original_form": "ㅎㅎ 그럼 못 드시는 음식은요?",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100026320.1.13",
          "form": "많아요 그 중에 닭발은 도전할 엄두가 안 나더라구요",
          "original_form": "많아요 그 중에 닭발은 도전할 엄두가 안 나더라구요",
          "speaker_id": "2",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100026320.1.14",
          "form": "엇 저도 닭발 못 먹어요 ㅋㅋㅋㅋ 도저히 안 끌리는 ㅋㅋㅋ",
          "original_form": "엇 저도 닭발 못 먹어요 ㅋㅋㅋㅋ 도저히 안 끌리는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100026320.1.15",
          "form": "비주얼이 좀 그렇지 않나요? 근데 잘 먹는 사람은 너무 맛있게 먹더라구요",
          "original_form": "비주얼이 좀 그렇지 않나요? 근데 잘 먹는 사람은 너무 맛있게 먹더라구요",
          "speaker_id": "2",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100026320.1.16",
          "form": "그러니까요 ㅋㅋ 생긴 게 너무 노골적이라",
          "original_form": "그러니까요 ㅋㅋ 생긴 게 너무 노골적이라",
          "speaker_id": "1",
          "time": "20210902 19:38"
        }
      ]
