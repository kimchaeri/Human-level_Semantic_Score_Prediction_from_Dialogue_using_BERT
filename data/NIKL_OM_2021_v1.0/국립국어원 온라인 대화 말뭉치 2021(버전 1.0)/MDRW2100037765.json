      "utterance": [
        {
          "id": "MDRW2100037765.1.1",
          "form": "나 육사시미 먹고시퍼",
          "original_form": "나 육사시미 먹고시퍼",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037765.1.2",
          "form": "오늘 저녁은 뭐 먹을 예정이야?",
          "original_form": "오늘 저녁은 뭐 먹을 예정이야?",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037765.1.3",
          "form": "양념게장도 먹고시퍼",
          "original_form": "양념게장도 먹고시퍼",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037765.1.4",
          "form": "오늘 그러게...",
          "original_form": "오늘 그러게...",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037765.1.5",
          "form": "그녕 집에있는거 먹자 해야지",
          "original_form": "그녕 집에있는거 먹자 해야지",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037765.1.6",
          "form": "빵도 있구",
          "original_form": "빵도 있구",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037765.1.7",
          "form": "엄마가 샌드위치도 사온다고 하셨구",
          "original_form": "엄마가 샌드위치도 사온다고 하셨구",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037765.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100037765.1.9",
          "form": "육사시미 나도 먹어보고 싶어",
          "original_form": "육사시미 나도 먹어보고 싶어",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100037765.1.10",
          "form": "육사시미 존맛...",
          "original_form": "육사시미 존맛...{emoji:♡}",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100037765.1.11",
          "form": "나의 솨랑",
          "original_form": "나의 솨랑",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100037765.1.12",
          "form": "육사시미에",
          "original_form": "육사시미에",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100037765.1.13",
          "form": "배 채썰어진거 넣고",
          "original_form": "배 채썰어진거 넣고",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100037765.1.14",
          "form": "기름장 살짝 바르고",
          "original_form": "기름장 살짝 바르고",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100037765.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.16",
          "form": "돌돌 말아서",
          "original_form": "돌돌 말아서",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.17",
          "form": "한입에 쏙!",
          "original_form": "한입에 쏙!",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.18",
          "form": "존 맛",
          "original_form": "존 맛",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.19",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.20",
          "form": "초고추장에",
          "original_form": "초고추장에",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.21",
          "form": "찍어먹는것도 맛있어",
          "original_form": "찍어먹는것도 맛있어",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.22",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.23",
          "form": "육회랑은 다른 맛이야?",
          "original_form": "육회랑은 다른 맛이야?",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.24",
          "form": "음...양념이 없지?",
          "original_form": "음...양념이 없지?",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.25",
          "form": "그것만 달라",
          "original_form": "그것만 달라",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.26",
          "form": "육회는 달달한 양념으로 버무려져 있잖아",
          "original_form": "육회는 달달한 양념으로 버무려져 있잖아",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.27",
          "form": "그게 없는 겨?",
          "original_form": "그게 없는 겨?",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.28",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037765.1.29",
          "form": "약간 회처럼",
          "original_form": "약간 회처럼",
          "speaker_id": "2",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100037765.1.30",
          "form": "그냥 썰어저만 있어",
          "original_form": "그냥 썰어저만 있어",
          "speaker_id": "2",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100037765.1.31",
          "form": "맛있을 거 같어",
          "original_form": "맛있을 거 같어",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100037765.1.32",
          "form": "얍",
          "original_form": "얍",
          "speaker_id": "1",
          "time": "20210902 22:07"
        }
      ]
    }
  ]
