      "utterance": [
        {
          "id": "MDRW2100042603.1.1",
          "form": "가급적 식사권 사서 가려고 하는데",
          "original_form": "가급적 식사권 사서 가려고 하는데",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042603.1.2",
          "form": "매물이 안보이네...",
          "original_form": "매물이 안보이네...",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042603.1.3",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042603.1.4",
          "form": "매물.. 여긴 십만원도 보이는데",
          "original_form": "매물.. 여긴 십만원도 보이는데",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042603.1.5",
          "form": "옛낭 옛적이여",
          "original_form": "옛낭 옛적이여",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042603.1.6",
          "form": "그러게...",
          "original_form": "그러게...",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042603.1.7",
          "form": "데리",
          "original_form": "데리",
          "speaker_id": "1",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042603.1.8",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042603.1.9",
          "form": "부모님이 할인카드를 갖고계시진 않겟지 ㅋㅋㅋㅋㅋ",
          "original_form": "부모님이 할인카드를 갖고계시진 않겟지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042603.1.10",
          "form": "엄빠가? 음...",
          "original_form": "엄빠가? 음...",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042603.1.11",
          "form": "어떤 카드인지에 따라 다를거 같은데",
          "original_form": "어떤 카드인지에 따라 다를거 같은데",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042603.1.12",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042603.1.13",
          "form": "보통 잘 갖고있는 카드는 아니지..",
          "original_form": "보통 잘 갖고있는 카드는 아니지..",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042603.1.14",
          "form": "신라 멤버십이나",
          "original_form": "신라 멤버십이나",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042603.1.15",
          "form": "프리미엄라인? 카드인듯...",
          "original_form": "프리미엄라인? 카드인듯...",
          "speaker_id": "1",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042603.1.16",
          "form": "음...없을듯...",
          "original_form": "음...없을듯...",
          "speaker_id": "2",
          "time": "20210907 00:07"
        },
        {
          "id": "MDRW2100042603.1.17",
          "form": "가난한 공무원집안..",
          "original_form": "가난한 공무원집안..",
          "speaker_id": "2",
          "time": "20210907 00:07"
        },
        {
          "id": "MDRW2100042603.1.18",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042603.1.19",
          "form": "우리도 없어..ㅎ. ..",
          "original_form": "우리도 없어..ㅎ. ..",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042603.1.20",
          "form": "ㅋㅋ큐ㅠㅠ 글렀어.... (절래절래",
          "original_form": "ㅋㅋ큐ㅠㅠ 글렀어.... (절래절래",
          "speaker_id": "2",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042603.1.21",
          "form": "그냥 정가내고 먹어야하나",
          "original_form": "그냥 정가내고 먹어야하나",
          "speaker_id": "2",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042603.1.22",
          "form": "ㅎ ㅏ 할인 좀 받고 살기가 이렇게 힘들다니",
          "original_form": "ㅎ ㅏ 할인 좀 받고 살기가 이렇게 힘들다니",
          "speaker_id": "1",
          "time": "20210907 07:39"
        },
        {
          "id": "MDRW2100042603.1.23",
          "form": "ㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:11"
        },
        {
          "id": "MDRW2100042603.1.24",
          "form": "있는 놈만 할인받는 세상",
          "original_form": "있는 놈만 할인받는 세상",
          "speaker_id": "2",
          "time": "20210907 08:11"
        },
        {
          "id": "MDRW2100042603.1.25",
          "form": "하지믄 있는 놈은 할인 굳이 안챙기고 살겠지 흑흑",
          "original_form": "하지믄 있는 놈은 할인 굳이 안챙기고 살겠지 흑흑",
          "speaker_id": "2",
          "time": "20210907 08:11"
        },
        {
          "id": "MDRW2100042603.1.26",
          "form": "그니까말야 ㅎ",
          "original_form": "그니까말야 ㅎ",
          "speaker_id": "1",
          "time": "20210907 08:11"
        },
        {
          "id": "MDRW2100042603.1.27",
          "form": "불공평하다....(급기야맨",
          "original_form": "불공평하다....(급기야맨",
