      "utterance": [
        {
          "id": "MDRW2100025464.1.1",
          "form": "동물키워요ㅋ?",
          "original_form": "동물키워요ㅋ?",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025464.1.2",
          "form": "구피 물고기 키워요",
          "original_form": "구피 물고기 키워요",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025464.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025464.1.4",
          "form": "저도 구피",
          "original_form": "저도 구피",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025464.1.5",
          "form": "오~ 반가워요",
          "original_form": "오~ 반가워요",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025464.1.6",
          "form": "개체수 많나요? 저는 거의 100마리 정도?",
          "original_form": "개체수 많나요? 저는 거의 100마리 정도?",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025464.1.7",
          "form": "40마리 낳아서 10마리 남아요",
          "original_form": "40마리 낳아서 10마리 남아요",
          "speaker_id": "1",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025464.1.8",
          "form": "그쵸 구피는.. 많이 나아도 남는게 별로 없죠..ㅎㅎ",
          "original_form": "그쵸 구피는.. 많이 나아도 남는게 별로 없죠..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100025464.1.9",
          "form": "죽드라구요",
          "original_form": "죽드라구요",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025464.1.10",
          "form": "잡아 먹히죠..",
          "original_form": "잡아 먹히죠..",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025464.1.11",
          "form": "그른가여",
          "original_form": "그른가여",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025464.1.12",
          "form": "바닥에 죽어있단데",
          "original_form": "바닥에 죽어있단데",
          "speaker_id": "1",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025464.1.13",
          "form": "아.. 수명이 일년 반쯤이라 나이차면 죽어요..ㅎㅎ",
          "original_form": "아.. 수명이 일년 반쯤이라 나이차면 죽어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100025464.1.14",
          "form": "태어나자마자 죽아요",
          "original_form": "태어나자마자 죽아요",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025464.1.15",
          "form": "태어나자마자는 잡아먹히는 거에요",
          "original_form": "태어나자마자는 잡아먹히는 거에요",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100025464.1.16",
          "form": "구피는 아기들 먹더라구요",
          "original_form": "구피는 아기들 먹더라구요",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100025464.1.17",
          "form": "그래서 따로 분리해줘야해요",
          "original_form": "그래서 따로 분리해줘야해요",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100025464.1.18",
          "form": "분리해도 죽더라구요",
          "original_form": "분리해도 죽더라구요",
          "speaker_id": "1",
          "time": "20210901 12:08"
        }
      ]
