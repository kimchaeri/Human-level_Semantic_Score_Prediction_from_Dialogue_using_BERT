      "utterance": [
        {
          "id": "MDRW2100020611.1.1",
          "form": "대만에 닮은 곳이 있었구나",
          "original_form": "대만에 닮은 곳이 있었구나",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020611.1.2",
          "form": "센과 치히로는 명작이져ㅋㅋㅋ",
          "original_form": "센과 치히로는 명작이져ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020611.1.3",
          "form": "님은 어디가 마지막이었어요",
          "original_form": "님은 어디가 마지막이었어요",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020611.1.4",
          "form": "전 중동...",
          "original_form": "전 중동...",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020611.1.5",
          "form": "중동 여행이 마지막이었어요 ㅎㅎ",
          "original_form": "중동 여행이 마지막이었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020611.1.6",
          "form": "와 중동! 이면 아랍에미리트 쪽인가요?",
          "original_form": "와 중동! 이면 아랍에미리트 쪽인가요?",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020611.1.7",
          "form": "네내 두바이쪽",
          "original_form": "네내 두바이쪽",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020611.1.8",
          "form": "문화가 완전히 달라서 재밌었어요",
          "original_form": "문화가 완전히 달라서 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020611.1.9",
          "form": "오호 두바이 금시장 제 버킷리스트에요",
          "original_form": "오호 두바이 금시장 제 버킷리스트에요",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.10",
          "form": "알라딘 느낌 ㅎㅎ",
          "original_form": "알라딘 느낌 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.12",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.13",
          "form": "전 사실 두바이는 아니었고",
          "original_form": "전 사실 두바이는 아니었고",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.14",
          "form": "옆에 카타르라는 다른 중동 나라였어요",
          "original_form": "옆에 카타르라는 다른 중동 나라였어요",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.15",
          "form": "중동은 진짜 너무 달라서 재밌을 거 같네요",
          "original_form": "중동은 진짜 너무 달라서 재밌을 거 같네요",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.16",
          "form": "아 카타르!",
          "original_form": "아 카타르!",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.17",
          "form": "근데 거기도 금시장 있기는 해요 ㅎㅎ",
          "original_form": "근데 거기도 금시장 있기는 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.18",
          "form": "카타르 아세요?",
          "original_form": "카타르 아세요?",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.19",
          "form": "거길 일부러 찾아가신거에요?",
          "original_form": "거길 일부러 찾아가신거에요?",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020611.1.20",
          "form": "도하 월드컵인가 뭐 하지않았어요?",
          "original_form": "도하 월드컵인가 뭐 하지않았어요?",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020611.1.21",
          "form": "제가 사실 지리학 전공이라 웬만한 나라는 알아요",
          "original_form": "제가 사실 지리학 전공이라 웬만한 나라는 알아요",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020611.1.22",
          "form": "우와~~~~~",
          "original_form": "우와~~~~~",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020611.1.23",
          "form": "거기서 직장을 다녀서",
          "original_form": "거기서 직장을 다녀서",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020611.1.24",
          "form": "휴일마다 틈틈히 여행했었어요",
          "original_form": "휴일마다 틈틈히 여행했었어요",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020611.1.25",
          "form": "오오 지리학 전공이 여기에!",
          "original_form": "오오 지리학 전공이 여기에!",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020611.1.26",
          "form": "영광입니다",
          "original_form": "영광입니다",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020611.1.27",
          "form": "아 역시 직장이 그곳에 ㅋㅋㅋ 여행지라는 생각이 안들어서 거주하시나 했네요 ㅋㅋㅋ",
          "original_form": "아 역시 직장이 그곳에 ㅋㅋㅋ 여행지라는 생각이 안들어서 거주하시나 했네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020611.1.28",
          "form": "네? 지리학 전공이 뭐라고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네? 지리학 전공이 뭐라고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020611.1.29",
          "form": "가보지 않아도 다 가보았다는 ...",
          "original_form": "가보지 않아도 다 가보았다는 ...",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020611.1.30",
          "form": "머리속에서 세계여행을 한다는...!",
          "original_form": "머리속에서 세계여행을 한다는...!",
          "speaker_id": "1",
          "time": "20210828 10:36"
        }
      ]
    }
