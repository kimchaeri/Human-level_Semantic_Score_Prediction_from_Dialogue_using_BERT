      "utterance": [
        {
          "id": "MDRW2100044458.1.1",
          "form": "치킨 말고 먹고 싶은 거 없어",
          "original_form": "치킨 말고 먹고 싶은 거 없어",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044458.1.2",
          "form": "고기가 땡기는데",
          "original_form": "고기가 땡기는데",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044458.1.3",
          "form": "고기 어떤 고기",
          "original_form": "고기 어떤 고기",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044458.1.4",
          "form": "소고기 빼고 다",
          "original_form": "소고기 빼고 다",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044458.1.5",
          "form": "엄마는 소고기 좋아하는데",
          "original_form": "엄마는 소고기 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044458.1.6",
          "form": "이런 거 보면 엄마랑 나랑 진짜 취향 달라",
          "original_form": "이런 거 보면 엄마랑 나랑 진짜 취향 달라",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.7",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.8",
          "form": "왜요 치킨 별로예요",
          "original_form": "왜요 치킨 별로예요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.9",
          "form": "되도록이면 튀김 안 먹는게 좋을 거 같애서",
          "original_form": "되도록이면 튀김 안 먹는게 좋을 거 같애서",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.10",
          "form": "그러면 족발 응",
          "original_form": "그러면 족발 응",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.11",
          "form": "족발도 괜찮네",
          "original_form": "족발도 괜찮네",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.12",
          "form": "족발은 어떤 집이 맛있지",
          "original_form": "족발은 어떤 집이 맛있지",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.13",
          "form": "족발은 시장 족발이 맛있지",
          "original_form": "족발은 시장 족발이 맛있지",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.14",
          "form": "거기도 배달 안 되지 않아요",
          "original_form": "거기도 배달 안 되지 않아요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.15",
          "form": "그럴걸 거기에도 사와야 될 거야",
          "original_form": "그럴걸 거기에도 사와야 될 거야",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.16",
          "form": "족발은 시켜 먹으면 좀 비싸던데",
          "original_form": "족발은 시켜 먹으면 좀 비싸던데",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.17",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044458.1.18",
          "form": "엄마 내일 아침에 시장 가니까 그때 사오까",
          "original_form": "엄마 내일 아침에 시장 가니까 그때 사오까",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044458.1.19",
          "form": "오 그러면 좋지",
          "original_form": "오 그러면 좋지",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044458.1.20",
          "form": "그럼 엄마가 점심시간에 맞춰서 사올게",
          "original_form": "그럼 엄마가 점심시간에 맞춰서 사올게",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
