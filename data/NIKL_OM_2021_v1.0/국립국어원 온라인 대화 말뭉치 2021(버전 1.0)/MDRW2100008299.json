      "utterance": [
        {
          "id": "MDRW2100008299.1.1",
          "form": "시장에 가는 것을 좋아하시나요?",
          "original_form": "시장에 가는 것을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008299.1.2",
          "form": "음 저는 사실 시장보단 마트 좋아하는데",
          "original_form": "음 저는 사실 시장보단 마트 좋아하는데",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008299.1.3",
          "form": "부모님이 좋아하셔서 가요ㅋㅋㅋ",
          "original_form": "부모님이 좋아하셔서 가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008299.1.4",
          "form": "싫어하는것 까진 아니라서",
          "original_form": "싫어하는것 까진 아니라서",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008299.1.5",
          "form": "약간 시장좀 부담스러워요ㅋㅋㅋ",
          "original_form": "약간 시장좀 부담스러워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008299.1.6",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008299.1.7",
          "form": "그럼 마트에 가면 주로 뭘 사시나요",
          "original_form": "그럼 마트에 가면 주로 뭘 사시나요",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008299.1.8",
          "form": "저는 냉동식품ㅋㅋㅋ삽니다",
          "original_form": "저는 냉동식품ㅋㅋㅋ삽니다",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008299.1.9",
          "form": "에어프라이어가 너무 잘되어있어서 말이죠",
          "original_form": "에어프라이어가 너무 잘되어있어서 말이죠",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008299.1.10",
          "form": "추천해주실 냉동식품 있으신가요",
          "original_form": "추천해주실 냉동식품 있으신가요",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008299.1.11",
          "form": "요즘 맛있는거 너무 많더라구요",
          "original_form": "요즘 맛있는거 너무 많더라구요",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008299.1.12",
          "form": "음 저는 곱창이나 막창 많이 먹어요",
          "original_form": "음 저는 곱창이나 막창 많이 먹어요",
          "speaker_id": "2",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008299.1.13",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008299.1.14",
          "form": "안주야.. 좋아하세요?",
          "original_form": "안주야.. 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008299.1.15",
          "form": "그 팩으로 나온거 먹습니다",
          "original_form": "그 팩으로 나온거 먹습니다",
          "speaker_id": "2",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008299.1.16",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008299.1.17",
          "form": "엇 넵 거기도 먹어요",
          "original_form": "엇 넵 거기도 먹어요",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008299.1.18",
          "form": "팩으로 나온건 어디서팔아요? 이마트? 롯데마트?",
          "original_form": "팩으로 나온건 어디서팔아요? 이마트? 롯데마트?",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008299.1.19",
          "form": "???안주야도 팩..?맞지않아요",
          "original_form": "???안주야도 팩..?맞지않아요",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008299.1.20",
          "form": "팩이라구 안하나요ㅋㅋㅋ",
          "original_form": "팩이라구 안하나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008299.1.21",
          "form": "저는 팩이라고ᄏᄏᄏ생각했어요",
          "original_form": "저는 팩이라고ᄏᄏᄏ생각했어요",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008299.1.22",
          "form": "아 에프쓰신다그래서",
          "original_form": "아 에프쓰신다그래서",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008299.1.23",
          "form": "그 양념된거말고",
          "original_form": "그 양념된거말고",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008299.1.24",
          "form": "그 있잖아요",
          "original_form": "그 있잖아요",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008299.1.25",
          "form": "그 뭐지 그 구워먹는거 생각했어요ㅋㅋ",
          "original_form": "그 뭐지 그 구워먹는거 생각했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008299.1.26",
          "form": "아아 그 양념 안된것도 팔걸요? 안주야가 아닌가,,",
          "original_form": "아아 그 양념 안된것도 팔걸요? 안주야가 아닌가,,",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008299.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ모르겠어욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ모르겠어욬ㅋㅋㅋ",
