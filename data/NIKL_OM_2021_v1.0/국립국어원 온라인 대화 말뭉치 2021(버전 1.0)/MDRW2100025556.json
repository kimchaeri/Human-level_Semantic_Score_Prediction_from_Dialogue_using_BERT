      "utterance": [
        {
          "id": "MDRW2100025556.1.1",
          "form": "요즘 성형 쫌 하고 싶어요",
          "original_form": "요즘 성형 쫌 하고 싶어요",
          "speaker_id": "1",
          "time": "20210901 13:17"
        },
        {
          "id": "MDRW2100025556.1.2",
          "form": "어디요?",
          "original_form": "어디요?",
          "speaker_id": "2",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100025556.1.3",
          "form": "저는 코가 매부리라 쫌 하고 싶어요",
          "original_form": "저는 코가 매부리라 쫌 하고 싶어요",
          "speaker_id": "1",
          "time": "20210901 13:25"
        },
        {
          "id": "MDRW2100025556.1.4",
          "form": "성형 관심 있으세요 하고싶은 부위",
          "original_form": "성형 관심 있으세요 하고싶은 부위",
          "speaker_id": "1",
          "time": "20210901 13:25"
        },
        {
          "id": "MDRW2100025556.1.5",
          "form": "전 다하고싶은데요 콧대낮은거나 턱도 깎고싶고요 눈도 더키우고싶고 다이어트도하고싶은데 쉽지않네요",
          "original_form": "전 다하고싶은데요 콧대낮은거나 턱도 깎고싶고요 눈도 더키우고싶고 다이어트도하고싶은데 쉽지않네요",
          "speaker_id": "2",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100025556.1.6",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100025556.1.7",
          "form": "역시 성형해서 확바뀌니 하고싶어저요",
          "original_form": "역시 성형해서 확바뀌니 하고싶어저요",
          "speaker_id": "1",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025556.1.8",
          "form": "최고의 성형은 다이어트라는데",
          "original_form": "최고의 성형은 다이어트라는데",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025556.1.9",
          "form": "살빼기 쉽지않네요",
          "original_form": "살빼기 쉽지않네요",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025556.1.10",
          "form": "그러니까요 피티도받고싶고 필라테스가 다이어트에 좋다던데 비용이 만만치않은것같아요",
          "original_form": "그러니까요 피티도받고싶고 필라테스가 다이어트에 좋다던데 비용이 만만치않은것같아요",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025556.1.11",
          "form": "피티는 엄청 힘들것같어요",
          "original_form": "피티는 엄청 힘들것같어요",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025556.1.12",
          "form": "필라테스는 엄청 몸 좋은 분들만 배우는 느낌...",
          "original_form": "필라테스는 엄청 몸 좋은 분들만 배우는 느낌...",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025556.1.13",
          "form": "그러게요 어떤 운동 좋아하세요",
          "original_form": "그러게요 어떤 운동 좋아하세요",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025556.1.14",
          "form": "걷기가 무난한거 같아요",
          "original_form": "걷기가 무난한거 같아요",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025556.1.15",
          "form": "방금 홈플러스 걷기운동 하고 왔어요",
          "original_form": "방금 홈플러스 걷기운동 하고 왔어요",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025556.1.16",
          "form": "잘하셨네요 걷기가제일무난하죠 그러고보니 런닝머신있으면좋겠네요",
          "original_form": "잘하셨네요 걷기가제일무난하죠 그러고보니 런닝머신있으면좋겠네요",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025556.1.17",
          "form": "집 런닝머신있으면 최고일듯",
          "original_form": "집 런닝머신있으면 최고일듯",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025556.1.18",
          "form": "넘 비싸요",
          "original_form": "넘 비싸요",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025556.1.19",
          "form": "심박수을 올리래요",
          "original_form": "심박수을 올리래요",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025556.1.20",
          "form": "운동시",
          "original_form": "운동시",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025556.1.21",
          "form": "그래요?그럼 뛰어야겠네요",
          "original_form": "그래요?그럼 뛰어야겠네요",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025556.1.22",
          "form": "그런가봐요 마라톤이 좋아보이던데 엄청 힘들듯해요",
          "original_form": "그런가봐요 마라톤이 좋아보이던데 엄청 힘들듯해요",
