      "utterance": [
        {
          "id": "MDRW2100002894.1.1",
          "form": "9월첫주 토요일에",
          "original_form": "9월첫주 토요일에",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002894.1.2",
          "form": "예약했어요",
          "original_form": "예약했어요",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002894.1.3",
          "form": "저도 9월초에 맞아요~",
          "original_form": "저도 9월초에 맞아요~",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002894.1.4",
          "form": "괜찮겠죠ㅠㅠ무서워서ㅋㅋ",
          "original_form": "괜찮겠죠ㅠㅠ무서워서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002894.1.5",
          "form": "저도 걱정되긴하지만..  그래도 맞는게 낫겠죠?ㅜ",
          "original_form": "저도 걱정되긴하지만..  그래도 맞는게 낫겠죠?ㅜ",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002894.1.6",
          "form": "그렇겠죠ㅠㅠ흑흑",
          "original_form": "그렇겠죠ㅠㅠ흑흑",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002894.1.7",
          "form": "안좋은 뉴스들보니 겁나서 사실",
          "original_form": "안좋은 뉴스들보니 겁나서 사실",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002894.1.8",
          "form": "부작용이 심하더라구요..",
          "original_form": "부작용이 심하더라구요..",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002894.1.9",
          "form": "물론 소수지만..",
          "original_form": "물론 소수지만..",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002894.1.10",
          "form": "그렇죠ㅠㅠ그소수가 나?이런생각도 무시못하죠ㅠ",
          "original_form": "그렇죠ㅠㅠ그소수가 나?이런생각도 무시못하죠ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002894.1.11",
          "form": "그러니꺄요..",
          "original_form": "그러니꺄요..",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002894.1.12",
          "form": "혹시내가되면.ㅜㅜ",
          "original_form": "혹시내가되면.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002894.1.13",
          "form": "으헝ㅠㅠ무섭다 그쵸ㅠㅜ",
          "original_form": "으헝ㅠㅠ무섭다 그쵸ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002894.1.14",
          "form": "괜찮을꺼예요ㅜ",
          "original_form": "괜찮을꺼예요ㅜ",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002894.1.15",
          "form": "전 알러지도 있어서ㅠ걱정이에요ㅠ",
          "original_form": "전 알러지도 있어서ㅠ걱정이에요ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002894.1.16",
          "form": "알러지있는분들 맞고나서 잘보셔야하드라구요ㅜ",
          "original_form": "알러지있는분들 맞고나서 잘보셔야하드라구요ㅜ",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002894.1.17",
          "form": "그래서 더 걱정되더라구요.ㅠㅠ",
          "original_form": "그래서 더 걱정되더라구요.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002894.1.18",
          "form": "맞기전까지 고민 할것같아요ㅠ",
          "original_form": "맞기전까지 고민 할것같아요ㅠ",
