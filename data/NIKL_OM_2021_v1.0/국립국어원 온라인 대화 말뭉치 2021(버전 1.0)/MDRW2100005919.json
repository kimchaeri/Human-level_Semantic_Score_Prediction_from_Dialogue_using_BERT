      "utterance": [
        {
          "id": "MDRW2100005919.1.1",
          "form": "영화 장르 뭐 좋아해요?",
          "original_form": "영화 장르 뭐 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100005919.1.2",
          "form": "영화는 장르 거의 안 가리고 보는 거 같아요",
          "original_form": "영화는 장르 거의 안 가리고 보는 거 같아요",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100005919.1.3",
          "form": "공포영화 좋아해요?",
          "original_form": "공포영화 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100005919.1.4",
          "form": "아 공포영화는",
          "original_form": "아 공포영화는",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005919.1.5",
          "form": "제가 안 좋아하는 거의 유일한 장르...",
          "original_form": "제가 안 좋아하는 거의 유일한 장르...",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005919.1.6",
          "form": "쫄보라서요 ㅋㅋㅋ",
          "original_form": "쫄보라서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005919.1.7",
          "form": "아.. ㅋㅋ 전 좋아하는데 로맨스영화 좋아해요?",
          "original_form": "아.. ㅋㅋ 전 좋아하는데 로맨스영화 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005919.1.8",
          "form": "로맨스영화는 좋아해요!",
          "original_form": "로맨스영화는 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005919.1.9",
          "form": "혹시... 팜스프링스라고 아시나요...?",
          "original_form": "혹시... 팜스프링스라고 아시나요...?",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100005919.1.10",
          "form": "로맨스영화에요? 사실 전 그장르 안좋아해서 모르겠어욤 ㅋㅋㅋ",
          "original_form": "로맨스영화에요? 사실 전 그장르 안좋아해서 모르겠어욤 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:56"
        },
        {
          "id": "MDRW2100005919.1.11",
          "form": "사실 유명한 영화는 아니었어요 ㅋㅋㅋ",
          "original_form": "사실 유명한 영화는 아니었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100005919.1.12",
          "form": "판타지 로코 영환데 제가 좋아하는 배우가 나와서 재밌게 봤거든요...",
          "original_form": "판타지 로코 영환데 제가 좋아하는 배우가 나와서 재밌게 봤거든요...",
          "speaker_id": "2",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100005919.1.13",
          "form": "좋아하는 영화예요?",
          "original_form": "좋아하는 영화예요?",
          "speaker_id": "1",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100005919.1.14",
          "form": "전 완전 재밌게 봤어요",
          "original_form": "전 완전 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100005919.1.15",
          "form": "무슨 내용이에요",
          "original_form": "무슨 내용이에요",
          "speaker_id": "1",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100005919.1.16",
          "form": "타임루프에 갇인 두 남녀가 탈출을 시도하면서 서로 가까워지는",
          "original_form": "타임루프에 갇인 두 남녀가 탈출을 시도하면서 서로 가까워지는",
          "speaker_id": "2",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100005919.1.17",
          "form": "로맨틱 코미디요",
          "original_form": "로맨틱 코미디요",
          "speaker_id": "2",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100005919.1.18",
          "form": "오 대박 재밌어보인다 나중에 볼게요",
          "original_form": "오 대박 재밌어보인다 나중에 볼게요",
          "speaker_id": "1",
          "time": "20210907 17:44"
        }
      ]
