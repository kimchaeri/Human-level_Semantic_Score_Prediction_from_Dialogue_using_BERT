        },
        {
          "id": "MDRW2100020676.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020676.1.3",
          "form": "오늘아침드셧나여??",
          "original_form": "오늘아침드셧나여??",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020676.1.4",
          "form": "네! 아침 먹었습니다",
          "original_form": "네! 아침 먹었습니다",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020676.1.5",
          "form": "웽디님은 아침 드셨나요?",
          "original_form": "웽디님은 아침 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020676.1.6",
          "form": "네~ 전 아침으로 간단히 베이글먹엇어용",
          "original_form": "네~ 전 아침으로 간단히 베이글먹엇어용",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100020676.1.7",
          "form": "베이글 맛있갰네요",
          "original_form": "베이글 맛있갰네요",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020676.1.8",
          "form": "전 오늘 식빵 구워서 먹었어요 ㅎㅎ",
          "original_form": "전 오늘 식빵 구워서 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020676.1.9",
          "form": "아하~ 식빵만 단독으로드셨나요?",
          "original_form": "아하~ 식빵만 단독으로드셨나요?",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020676.1.10",
          "form": "네 ㅋㅋㅋㅋ 식빵이랑 우유요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 식빵이랑 우유요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020676.1.11",
          "form": "베이글엔 뭐 발라 드셨나요?",
          "original_form": "베이글엔 뭐 발라 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020676.1.12",
          "form": "네! 전 크림치즈랑 바질페스토 발라먹었습죠 ㅎㅎ",
          "original_form": "네! 전 크림치즈랑 바질페스토 발라먹었습죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020676.1.13",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100020676.1.14",
          "form": "바질페스토 진짜 맛있잖아요",
          "original_form": "바질페스토 진짜 맛있잖아요",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100020676.1.15",
          "form": "맛있어서 만들어먹으려다 바질이 다 죽는 엔딩을 맛봐서 사먹기만 해요 ㅋㅋㅋㅌㅋㅋ",
          "original_form": "맛있어서 만들어먹으려다 바질이 다 죽는 엔딩을 맛봐서 사먹기만 해요 ㅋㅋㅋㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100020676.1.16",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ 그냥 번거롭기도하고 사먹는게나아요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ 그냥 번거롭기도하고 사먹는게나아요",
          "speaker_id": "1",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100020676.1.17",
          "form": "식물은 진짜 키우기 힘들더라구요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "식물은 진짜 키우기 힘들더라구요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100020676.1.18",
          "form": "마쟈요 ㅜㅜ 물주기도 여간 쉬운일이아니더라구요",
          "original_form": "마쟈요 ㅜㅜ 물주기도 여간 쉬운일이아니더라구요",
          "speaker_id": "1",
          "time": "20210829 06:44"
        }
      ]
