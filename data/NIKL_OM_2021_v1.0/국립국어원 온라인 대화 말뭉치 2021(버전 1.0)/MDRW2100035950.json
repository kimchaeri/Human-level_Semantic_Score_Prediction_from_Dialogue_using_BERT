      "utterance": [
        {
          "id": "MDRW2100035950.1.1",
          "form": "저도 아이들 반찬해야 하는데",
          "original_form": "저도 아이들 반찬해야 하는데",
          "speaker_id": "1",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100035950.1.2",
          "form": "메추리알 조림이랑",
          "original_form": "메추리알 조림이랑",
          "speaker_id": "1",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100035950.1.3",
          "form": "닭다리살간장볶음...",
          "original_form": "닭다리살간장볶음...",
          "speaker_id": "1",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100035950.1.4",
          "form": "그리고 뭘해줘야 할까요 ㅜㅜㅋ",
          "original_form": "그리고 뭘해줘야 할까요 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210830 15:56"
        },
        {
          "id": "MDRW2100035950.1.5",
          "form": "우왕 맛있겠어요",
          "original_form": "우왕 맛있겠어요",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100035950.1.6",
          "form": "전 애호박새우젓볶음하고 일본식계란찜?? 뭐그게 엄청 애들이잘먹는대서 했는데",
          "original_form": "전 애호박새우젓볶음하고 일본식계란찜?? 뭐그게 엄청 애들이잘먹는대서 했는데",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100035950.1.7",
          "form": "남편은 별로라하고 애들이 잘먹긴했네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "남편은 별로라하고 애들이 잘먹긴했네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100035950.1.8",
          "form": "일본식 계란찜요? \n저도 검색 들어가봐야겠게요ㅋ",
          "original_form": "일본식 계란찜요? \n저도 검색 들어가봐야겠게요ㅋ",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100035950.1.9",
          "form": "아 별건없고 체에3번거르고",
          "original_form": "아 별건없고 체에3번거르고",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100035950.1.10",
          "form": "거품걷어내고",
          "original_form": "거품걷어내고",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100035950.1.11",
          "form": "약불에 오~~래 익히는거였어요",
          "original_form": "약불에 오~~래 익히는거였어요",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100035950.1.12",
          "form": "엄청 부드럽겠네요ㅋㅋ",
          "original_form": "엄청 부드럽겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100035950.1.13",
          "form": "그래서 애들이 잘먹더라구요 ^^",
          "original_form": "그래서 애들이 잘먹더라구요 ^^",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100035950.1.14",
          "form": "저도 일본식 계란찜도\n도전해 볼게요!",
          "original_form": "저도 일본식 계란찜도\n도전해 볼게요!",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100035950.1.15",
          "form": "또 어떤 반찬 잘 먹나요?",
          "original_form": "또 어떤 반찬 잘 먹나요?",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100035950.1.16",
          "form": "저희애들은 아보카도 비빔밥 엄청 잘먹어요!^^",
          "original_form": "저희애들은 아보카도 비빔밥 엄청 잘먹어요!^^",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100035950.1.17",
          "form": "애호박,당근,양파 각각 볶구요",
          "original_form": "애호박,당근,양파 각각 볶구요",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100035950.1.18",
          "form": "계란 스크램블하고  잘익은 아보카도를 잘게썰어서 넣어요 밥위에",
          "original_form": "계란 스크램블하고  잘익은 아보카도를 잘게썰어서 넣어요 밥위에",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100035950.1.19",
          "form": "그리고 간장 참기름 매실청 조금해서",
          "original_form": "그리고 간장 참기름 매실청 조금해서",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100035950.1.20",
          "form": "김가루 뿌려 비벼주면 엄청 잘먹네요~~^^",
          "original_form": "김가루 뿌려 비벼주면 엄청 잘먹네요~~^^",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100035950.1.21",
          "form": "잘익은 아보카도가 밥에 잘 비벼져서 밥색깔이변해용ㅎ",
          "original_form": "잘익은 아보카도가 밥에 잘 비벼져서 밥색깔이변해용ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100035950.1.22",
          "form": "오 아보카도비빔밥",
          "original_form": "오 아보카도비빔밥",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100035950.1.23",
          "form": "저희 아이는 편식이 심해서\n먹으려나 모르겠네요 ㅜㅜㅋ",
          "original_form": "저희 아이는 편식이 심해서\n먹으려나 모르겠네요 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100035950.1.24",
          "form": "우선 도전해 볼게요",
          "original_form": "우선 도전해 볼게요",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100035950.1.25",
          "form": "네 맛있게 잘먹어주면 좋겠네요 ^^",
          "original_form": "네 맛있게 잘먹어주면 좋겠네요 ^^",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100035950.1.26",
          "form": "오늘 아침에 늦잠자서 굶겨 보내서 마음쓰이네요ㅠㅠ",
          "original_form": "오늘 아침에 늦잠자서 굶겨 보내서 마음쓰이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100035950.1.27",
          "form": "지난주부터  급 늦잠을 자서ㅠㅠ",
          "original_form": "지난주부터  급 늦잠을 자서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100035950.1.28",
          "form": "에고ㅠㅠ 그래도 아이들\n원에 가면 오전에도\n간식 주니까요ㅜㅜ\n넘 속상해 마세용",
          "original_form": "에고ㅠㅠ 그래도 아이들\n원에 가면 오전에도\n간식 주니까요ㅜㅜ\n넘 속상해 마세용",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100035950.1.29",
          "form": "ㅎㅎㅎ 네 부디 원에서도 잘먹었길바라며 ㅋㅋㅋ",
          "original_form": "ㅎㅎㅎ 네 부디 원에서도 잘먹었길바라며 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100035950.1.30",
          "form": "오늘은 사무실에서 떡볶이를 시켜먹었어요",
          "original_form": "오늘은 사무실에서 떡볶이를 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100035950.1.31",
          "form": "근데 전처음먹는데 맛집이었나봐요",
          "original_form": "근데 전처음먹는데 맛집이었나봐요",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100035950.1.32",
          "form": "맛있더라구요 ㅋㅋ",
          "original_form": "맛있더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100035950.1.33",
          "form": "오? 어디 떡볶이 드셨어요?",
