      "utterance": [
        {
          "id": "MDRW2100030240.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030240.1.2",
          "form": "빠꾸도 없어",
          "original_form": "빠꾸도 없어",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030240.1.3",
          "form": "게임.... 세포신곡 하고싶다",
          "original_form": "게임.... 세포신곡 하고싶다",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.4",
          "form": "제출하시겠습니까?도 없네..",
          "original_form": "제출하시겠습니까?도 없네..",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.5",
          "form": "아 나 세포신곡",
          "original_form": "아 나 세포신곡",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.6",
          "form": "영업좀 해줘봐",
          "original_form": "영업좀 해줘봐",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.7",
          "form": "나 상당히 흥미로워요.",
          "original_form": "나 상당히 흥미로워요.",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.8",
          "form": "ㅋㅋㅋㅋㅋ 세포신곡 진짜 재밌어",
          "original_form": "ㅋㅋㅋㅋㅋ 세포신곡 진짜 재밌어",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.9",
          "form": "아무것도 모르고 네 트윗몇개랑 애들 얼굴 정도만 봐서",
          "original_form": "아무것도 모르고 네 트윗몇개랑 애들 얼굴 정도만 봐서",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.10",
          "form": "내가 직접 한 첫 공포게임...",
          "original_form": "내가 직접 한 첫 공포게임...",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.11",
          "form": "무슨 게임인지도 몰라",
          "original_form": "무슨 게임인지도 몰라",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.12",
          "form": "와...두번째 공포게임 리틀 나이트메어로 만들어드리겠습니다.",
          "original_form": "와...두번째 공포게임 리틀 나이트메어로 만들어드리겠습니다.",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030240.1.13",
          "form": "아 ㅋ ㅋ ㅋ ㅋ ㅋ ㅋㅋㅋㅋ",
          "original_form": "아 ㅋ ㅋ ㅋ ㅋ ㅋ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030240.1.14",
          "form": "이게 15년도쯤인가에 나온 공포쯔꾸르 게임인데",
          "original_form": "이게 15년도쯤인가에 나온 공포쯔꾸르 게임인데",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030240.1.15",
          "form": "공포는 역시 쯔꾸르지",
          "original_form": "공포는 역시 쯔꾸르지",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030240.1.16",
          "form": "탐정인 주인공이 자기 동료이자 후배인 애가 실종되서 추적하다가 엄청난 사건에 휘말리게 돼",
          "original_form": "탐정인 주인공이 자기 동료이자 후배인 애가 실종되서 추적하다가 엄청난 사건에 휘말리게 돼",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030240.1.17",
          "form": "와 벌써 흥미로워",
          "original_form": "와 벌써 흥미로워",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030240.1.18",
          "form": "주 키워드가... 사이비 종교... 비윤리적 실험...",
          "original_form": "주 키워드가... 사이비 종교... 비윤리적 실험...",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.19",
          "form": "아 너무 좋아",
          "original_form": "아 너무 좋아",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.20",
          "form": "쬬상 그거 추천드리고싶다.",
          "original_form": "쬬상 그거 추천드리고싶다.",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.21",
          "form": "엔딩 루트가 좀 많고",
          "original_form": "엔딩 루트가 좀 많고",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.22",
          "form": "키미가시네",
          "original_form": "키미가시네",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.23",
          "form": "와 엔딩루트 많은거 너무 좋아",
          "original_form": "와 엔딩루트 많은거 너무 좋아",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.24",
          "form": "진행할때도 무슨 힌트를 갖고있냐에 따라 스크립트 출력이 달라져서",
          "original_form": "진행할때도 무슨 힌트를 갖고있냐에 따라 스크립트 출력이 달라져서",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.25",
          "form": "되게 쯔꾸르다운 숨은 요소가 많은????",
          "original_form": "되게 쯔꾸르다운 숨은 요소가 많은????",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030240.1.26",
          "form": "오오 이브랑 시스템이 비슷하네. 이런게 제일 좋지...",
          "original_form": "오오 이브랑 시스템이 비슷하네. 이런게 제일 좋지...",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030240.1.27",
          "form": "명작쯔꾸르의 묘미",
          "original_form": "명작쯔꾸르의 묘미",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030240.1.28",
          "form": "근데 진짜 무서워 나 디코하다 비명질렀잖아",
          "original_form": "근데 진짜 무서워 나 디코하다 비명질렀잖아",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030240.1.29",
          "form": "깜놀 갑툭튀가 좀 있더라고...",
          "original_form": "깜놀 갑툭튀가 좀 있더라고...",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030240.1.30",
          "form": "무서울것같은데 재밌겠다.. 그냥 다운 받을 수 있나요?",
          "original_form": "무서울것같은데 재밌겠다.. 그냥 다운 받을 수 있나요?",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030240.1.31",
          "form": "아놔 갑툭튀~~",
          "original_form": "아놔 갑툭튀~~",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030240.1.32",
          "form": "응!!!! 검색하면 공식(인데 비공식이라고 하는) 번역판 다운 나와",
          "original_form": "응!!!! 검색하면 공식(인데 비공식이라고 하는) 번역판 다운 나와",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.33",
          "form": "괄호부분이 이해가 안가지만 일단 오케이 ㅋㅋ",
          "original_form": "괄호부분이 이해가 안가지만 일단 오케이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.34",
          "form": "좀... 다운 절차가 옹삭시럽긴 한데 겜 자체는 재밌다",
          "original_form": "좀... 다운 절차가 옹삭시럽긴 한데 겜 자체는 재밌다",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.35",
          "form": "여기 제작사가 이런저런 제약을 많이 걸어놨도라고",
          "original_form": "여기 제작사가 이런저런 제약을 많이 걸어놨도라고",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.36",
          "form": "이브는 해봣어?",
          "original_form": "이브는 해봣어?",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.37",
          "form": "이브??? 아직!!",
          "original_form": "이브??? 아직!!",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.38",
          "form": "아하.....뭐 다운 받을 수 있으면 됐지",
          "original_form": "아하.....뭐 다운 받을 수 있으면 됐지",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.39",
          "form": "이브..뭐 알아?",
          "original_form": "이브..뭐 알아?",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030240.1.40",
          "form": "스포라던가..들으셧나요?",
          "original_form": "스포라던가..들으셧나요?",
          "speaker_id": "1",
