      "utterance": [
        {
          "id": "MDRW2100015413.1.1",
          "form": "나중에 무슨 일 하고 싶으신가요?",
          "original_form": "나중에 무슨 일 하고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015413.1.2",
          "form": "아직 고민중이에요",
          "original_form": "아직 고민중이에요",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015413.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015413.1.4",
          "form": "전 우주비행사요",
          "original_form": "전 우주비행사요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015413.1.5",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015413.1.6",
          "form": "어딜 가고 싶으신가요?",
          "original_form": "어딜 가고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015413.1.7",
          "form": "우주요",
          "original_form": "우주요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015413.1.8",
          "form": "구체적으로 말씀해주시겠어요?",
          "original_form": "구체적으로 말씀해주시겠어요?",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015413.1.9",
          "form": "목성과 토성 사이를 뱅글뱅글 돌고 싶어요",
          "original_form": "목성과 토성 사이를 뱅글뱅글 돌고 싶어요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015413.1.10",
          "form": "name2님은 어떤 쪽에서 고민하세요?",
          "original_form": "&name2&님은 어떤 쪽에서 고민하세요?",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015413.1.11",
          "form": "뱅글뱅글 돌다 안드로메다로 빠지면 어떡하죠?",
          "original_form": "뱅글뱅글 돌다 안드로메다로 빠지면 어떡하죠?",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015413.1.12",
          "form": "저는 아나운서랑 성우 중에 고민이에요",
          "original_form": "저는 아나운서랑 성우 중에 고민이에요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015413.1.13",
          "form": "전 실력있는 비행사가 될거라",
          "original_form": "전 실력있는 비행사가 될거라",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015413.1.14",
          "form": "그럴일은 없어요",
          "original_form": "그럴일은 없어요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015413.1.15",
          "form": "제 감으로 골라드릴게요",
          "original_form": "제 감으로 골라드릴게요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015413.1.16",
          "form": "확고하시네요",
          "original_form": "확고하시네요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015413.1.17",
          "form": "당신은 두구두구",
          "original_form": "당신은 두구두구",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015413.1.18",
          "form": "아나성우 입니다 !",
          "original_form": "아나성우 입니다 !",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015413.1.19",
          "form": "직업을 누가 감으로 고르냐구요",
          "original_form": "직업을 누가 감으로 고르냐구요",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015413.1.20",
          "form": "이상한 심심이네요",
          "original_form": "이상한 심심이네요",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015413.1.21",
          "form": "저희 부장님보다 재미없으세요",
          "original_form": "저희 부장님보다 재미없으세요",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015413.1.22",
          "form": "직업 없으시다면서요",
          "original_form": "직업 없으시다면서요",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015413.1.23",
          "form": "저희 집에 부장님이 있으시죠",
          "original_form": "저희 집에 부장님이 있으시죠",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015413.1.24",
          "form": "세상엔 참 다양한 직업이 있는 것 같아요",
          "original_form": "세상엔 참 다양한 직업이 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015413.1.25",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015413.1.26",
          "form": "독특한 직업 아는 거 있으세요?",
          "original_form": "독특한 직업 아는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015413.1.27",
          "form": "저는 타이어 디자이너가 정말 인상깊었어요",
          "original_form": "저는 타이어 디자이너가 정말 인상깊었어요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015413.1.28",
          "form": "오 첨 들어봐요",
          "original_form": "오 첨 들어봐요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        }
      ]
    }
  ]
