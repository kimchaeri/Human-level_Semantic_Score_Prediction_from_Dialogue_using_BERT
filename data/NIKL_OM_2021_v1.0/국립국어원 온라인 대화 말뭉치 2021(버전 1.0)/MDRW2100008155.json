      "utterance": [
        {
          "id": "MDRW2100008155.1.1",
          "form": "어디로 여행 가고 싶은가?",
          "original_form": "어디로 여행 가고 싶은가?",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008155.1.2",
          "form": "나 글쎄",
          "original_form": "나 글쎄",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008155.1.3",
          "form": "어디로든문!",
          "original_form": "어디로든문!",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008155.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ그냥 어디든?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그냥 어디든?",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008155.1.5",
          "form": "바다도 좋고 꽃도 좋다",
          "original_form": "바다도 좋고 꽃도 좋다",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008155.1.6",
          "form": "엄마는? 오디?",
          "original_form": "엄마는? 오디?",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008155.1.7",
          "form": "난 시원한 계곡도 좋아 ?",
          "original_form": "난 시원한 계곡도 좋아 ?",
          "speaker_id": "1",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008155.1.8",
          "form": "계곡하면?! 백숙인데",
          "original_form": "계곡하면?! 백숙인데",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008155.1.9",
          "form": "화양계곡? 거기 갔었지 예전에?",
          "original_form": "화양계곡? 거기 갔었지 예전에?",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008155.1.10",
          "form": "시원한 계곡물보면 평상에서 백숙 좋은데 ...? ?",
          "original_form": "시원한 계곡물보면 평상에서 백숙 좋은데 ...? ?",
          "speaker_id": "1",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008155.1.11",
          "form": "ㅋㅋㅋ그치",
          "original_form": "ㅋㅋㅋ그치",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008155.1.12",
          "form": "여름엔 백숙",
          "original_form": "여름엔 백숙",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008155.1.13",
          "form": "맛집 여행도 좋아",
          "original_form": "맛집 여행도 좋아",
          "speaker_id": "1",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008155.1.14",
          "form": "하긴 요즘 식당이 잘되어 있어서?!",
          "original_form": "하긴 요즘 식당이 잘되어 있어서?!",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008155.1.15",
          "form": "맛난것먹고 여기저기 다니는것도 좋아",
          "original_form": "맛난것먹고 여기저기 다니는것도 좋아",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008155.1.16",
          "form": "내년에 이시국 끝나면 어디 갈까?",
          "original_form": "내년에 이시국 끝나면 어디 갈까?",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008155.1.17",
          "form": "그땐 가족이 다같이 갈수 있겠지",
          "original_form": "그땐 가족이 다같이 갈수 있겠지",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008155.1.18",
          "form": "엄마 어디 가고싶냐구",
          "original_form": "엄마 어디 가고싶냐구",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008155.1.19",
          "form": "글쎄 지금 딱히 어디를 갈까 고민안해봣어",
          "original_form": "글쎄 지금 딱히 어디를 갈까 고민안해봣어",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008155.1.20",
          "form": "ㅋㅋㅋ고민해봐 여행 ㅠㅠ!!! 내년에 가자구",
          "original_form": "ㅋㅋㅋ고민해봐 여행 ㅠㅠ!!! 내년에 가자구",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008155.1.21",
          "form": "어디로 가고 십니",
          "original_form": "어디로 가고 십니",
