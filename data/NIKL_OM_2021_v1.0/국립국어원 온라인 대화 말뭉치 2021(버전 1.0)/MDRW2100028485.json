      "utterance": [
        {
          "id": "MDRW2100028485.1.1",
          "form": "결혼 준비 잘되고 있니?",
          "original_form": "결혼 준비 잘되고 있니?",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028485.1.2",
          "form": "집이랑 신혼여행만 알아보면 돼",
          "original_form": "집이랑 신혼여행만 알아보면 돼",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028485.1.3",
          "form": "혼수도 잊지 마",
          "original_form": "혼수도 잊지 마",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028485.1.4",
          "form": "혼수 안하고 싶어 대충 살래",
          "original_form": "혼수 안하고 싶어 대충 살래",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028485.1.5",
          "form": "좋은 생각이야 몸만 있으면 되지",
          "original_form": "좋은 생각이야 몸만 있으면 되지",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028485.1.6",
          "form": "넌 결혼 언제할거야?",
          "original_form": "넌 결혼 언제할거야?",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028485.1.7",
          "form": "마흔 전에는 갈 거야",
          "original_form": "마흔 전에는 갈 거야",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028485.1.8",
          "form": "니 애 초등학교 들어가면 나 초산",
          "original_form": "니 애 초등학교 들어가면 나 초산",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028485.1.9",
          "form": "히히 애기 데리고 식장가야지 그 꽃뿌려주는거 해줄게",
          "original_form": "히히 애기 데리고 식장가야지 그 꽃뿌려주는거 해줄게",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028485.1.10",
          "form": "너희 애들이 나 결혼식 때 화동하면 되겠다",
          "original_form": "너희 애들이 나 결혼식 때 화동하면 되겠다",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028485.1.11",
          "form": "맞아 한명씩",
          "original_form": "맞아 한명씩",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028485.1.12",
          "form": "주말에 데이트 어디로가",
          "original_form": "주말에 데이트 어디로가",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028485.1.13",
          "form": "월요일에 떡볶이 먹었는데",
          "original_form": "월요일에 떡볶이 먹었는데",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028485.1.14",
          "form": "떡볶이를 안 좋아하나 봐 남자친구가 먹는내내 죽상이었어",
          "original_form": "떡볶이를 안 좋아하나 봐 남자친구가 먹는내내 죽상이었어",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028485.1.15",
          "form": "화나서 이번 주말에는 데이트 안 할 거야",
          "original_form": "화나서 이번 주말에는 데이트 안 할 거야",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028485.1.16",
          "form": "어떻게 떡볶이를 안좋아할수가 있어 화날만해",
          "original_form": "어떻게 떡볶이를 안좋아할수가 있어 화날만해",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028485.1.17",
          "form": "그지? 너는 주말에 데이트 할 거니?",
          "original_form": "그지? 너는 주말에 데이트 할 거니?",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028485.1.18",
          "form": "난 평창 계곡 가 얼어죽을듯",
          "original_form": "난 평창 계곡 가 얼어죽을듯",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028485.1.19",
          "form": "추울 거 같은데? 데이트 재밌겠다",
          "original_form": "추울 거 같은데? 데이트 재밌겠다",
          "speaker_id": "1",
          "time": "20210902 20:35"
        }
      ]
