      "utterance": [
        {
          "id": "MDRW2100026673.1.1",
          "form": "하루에 몇 번 밥을 차리시나요",
          "original_form": "하루에 몇 번 밥을 차리시나요",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026673.1.2",
          "form": "저는 한 번 차려요",
          "original_form": "저는 한 번 차려요",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026673.1.3",
          "form": "name2 님은요???",
          "original_form": "&name2& 님은요???",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026673.1.4",
          "form": "어 저는 안  차리고 와이프가 차립니다",
          "original_form": "어 저는 안  차리고 와이프가 차립니다",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026673.1.5",
          "form": "한 번이면 그래도 괜찮으시네요",
          "original_form": "한 번이면 그래도 괜찮으시네요",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026673.1.6",
          "form": "저 월 화 수 삼식이었습니다",
          "original_form": "저 월 화 수 삼식이었습니다",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026673.1.7",
          "form": "월화수 삼식이면....",
          "original_form": "월화수 삼식이면....",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026673.1.8",
          "form": "뭐죠??? 제가 요즘 언어에 약해서요...",
          "original_form": "뭐죠??? 제가 요즘 언어에 약해서요...",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026673.1.9",
          "form": "재택근무라서 어쩔 수 없었습니다",
          "original_form": "재택근무라서 어쩔 수 없었습니다",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026673.1.10",
          "form": "월요일 화요일 수요일 삼시 세 끼 다 먹음",
          "original_form": "월요일 화요일 수요일 삼시 세 끼 다 먹음",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026673.1.11",
          "form": "아 재택근무 하시는군요",
          "original_form": "아 재택근무 하시는군요",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026673.1.12",
          "form": "와이프 입장에서는 짜증나는 상황입니다",
          "original_form": "와이프 입장에서는 짜증나는 상황입니다",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026673.1.13",
          "form": "목금은 출근하시나요??",
          "original_form": "목금은 출근하시나요??",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026673.1.14",
          "form": "아 그쵸 ㅋㅋ쿠ㅜㅜ",
          "original_form": "아 그쵸 ㅋㅋ쿠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026673.1.15",
          "form": "작년부터 계속 재택근무 하셨을테니 식사 차리는 분은 힘드실만 하죠ㅠㅠㅠ",
          "original_form": "작년부터 계속 재택근무 하셨을테니 식사 차리는 분은 힘드실만 하죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026673.1.16",
          "form": "네 맞습니다 ㅎㅎ",
          "original_form": "네 맞습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.17",
          "form": "월화수에 외식은 안하시나요?",
          "original_form": "월화수에 외식은 안하시나요?",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.18",
          "form": "이번 주 월 화 수 온라인 교육이 있어서 제되겠어요",
          "original_form": "이번 주 월 화 수 온라인 교육이 있어서 제되겠어요",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.19",
          "form": "요즘 배달음식이 인기더라구오",
          "original_form": "요즘 배달음식이 인기더라구오",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.20",
          "form": "제 탓했습니다",
          "original_form": "제 탓했습니다",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.21",
          "form": "아이고 ㅋㅋㅋㅋㅋ",
          "original_form": "아이고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.22",
          "form": "재택했습니다",
          "original_form": "재택했습니다",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.23",
          "form": "배민",
          "original_form": "배민",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.24",
          "form": "엄청시켜먹어요",
          "original_form": "엄청시켜먹어요",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.25",
          "form": "사실 와이프가 밥을 잘 안했었습니다",
          "original_form": "사실 와이프가 밥을 잘 안했었습니다",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026673.1.26",
          "form": "지금 7년차인데",
          "original_form": "지금 7년차인데",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.27",
          "form": "아내분도 맞벌이 하시는걸까요?",
          "original_form": "아내분도 맞벌이 하시는걸까요?",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.28",
          "form": "밥차린 횟수가 100번이 안될거예요",
          "original_form": "밥차린 횟수가 100번이 안될거예요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.29",
          "form": "애고 낳고 지금은 쉽니당",
          "original_form": "애고 낳고 지금은 쉽니당",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.30",
          "form": "애기가 15개월이에용",
          "original_form": "애기가 15개월이에용",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.31",
          "form": "그럼 1년에 14번 정도 차리신거네요",
          "original_form": "그럼 1년에 14번 정도 차리신거네요",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.32",
          "form": "완전 애기네요",
          "original_form": "완전 애기네요",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.33",
          "form": "네 ㅎㅎㅎ",
          "original_form": "네 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.34",
          "form": "이제 엄마 아빠해요",
          "original_form": "이제 엄마 아빠해요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026673.1.35",
          "form": "아내분께서 힘드실만 하네요 애기보랴 밥 준비하랴",
          "original_form": "아내분께서 힘드실만 하네요 애기보랴 밥 준비하랴",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026673.1.36",
          "form": "자재분 있나요?",
          "original_form": "자재분 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:52"
        }
      ]
