        {
          "id": "MDRW2100007325.1.1",
          "form": "ㅎㅎ기회가 있을거에요",
          "original_form": "ㅎㅎ기회가 있을거에요",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007325.1.2",
          "form": "직업은 있으신가요?",
          "original_form": "직업은 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007325.1.3",
          "form": "네 평범한 회사원이에요 ㅎㅎ name1님은요?",
          "original_form": "네 평범한 회사원이에요 ㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007325.1.4",
          "form": "저는 지방 공기업 다녀요",
          "original_form": "저는 지방 공기업 다녀요",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007325.1.5",
          "form": "그럼 출퇴근일정하세요?",
          "original_form": "그럼 출퇴근일정하세요?",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007325.1.6",
          "form": "아니요 교대근무입니다ㅜㅠ",
          "original_form": "아니요 교대근무입니다ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007325.1.7",
          "form": "에고 고단하시겠어요ㅠㅠ 몇교대로 근무하세요?",
          "original_form": "에고 고단하시겠어요ㅠㅠ 몇교대로 근무하세요?",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007325.1.8",
          "form": "4조2교대인데 자꾸 회사에서",
          "original_form": "4조2교대인데 자꾸 회사에서",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007325.1.9",
          "form": "직원 짜내려고 하네요ㅠ",
          "original_form": "직원 짜내려고 하네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007325.1.10",
          "form": "회사는 다 그렇죠ㅠㅠㅠ 적게주고 많이 부려먹으려고ㅠㅠ",
          "original_form": "회사는 다 그렇죠ㅠㅠㅠ 적게주고 많이 부려먹으려고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007325.1.11",
          "form": "그래도 너무해요ㅋㅋㅋㅋ",
          "original_form": "그래도 너무해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007325.1.12",
          "form": "본사가 2조2교대를 제시했어요ㅎㅎ",
          "original_form": "본사가 2조2교대를 제시했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007325.1.13",
          "form": "에이...2조 2교대가 가능한 부분이에요???",
          "original_form": "에이...2조 2교대가 가능한 부분이에요???",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007325.1.14",
          "form": "ㅋㅋ그러니까요",
          "original_form": "ㅋㅋ그러니까요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007325.1.15",
          "form": "2교대를 하는 곳이 공기업중에 있어요?",
          "original_form": "2교대를 하는 곳이 공기업중에 있어요?",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007325.1.16",
          "form": "정도껏 해야지...",
          "original_form": "정도껏 해야지...",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007325.1.17",
          "form": "없죠ㅋㅋㅋㅋ",
          "original_form": "없죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007325.1.18",
          "form": "와 요즘 공기업도 근무환경 점점 말도 안되게 ㅠㅠㅠㅠ",
          "original_form": "와 요즘 공기업도 근무환경 점점 말도 안되게 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007325.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무 어이가 없는 제안이라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무 어이가 없는 제안이라",
