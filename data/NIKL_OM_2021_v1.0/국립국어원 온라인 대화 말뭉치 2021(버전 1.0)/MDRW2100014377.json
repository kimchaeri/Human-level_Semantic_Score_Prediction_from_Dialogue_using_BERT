      "utterance": [
        {
          "id": "MDRW2100014377.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100014377.1.2",
          "form": "아니요.. 근데 다 조아해요..",
          "original_form": "아니요.. 근데 다 조아해요..",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100014377.1.3",
          "form": "멍멍이 야옹이.. 공룡.. 독수리? ㅋㅋㅋ",
          "original_form": "멍멍이 야옹이.. 공룡.. 독수리? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100014377.1.4",
          "form": "뭐 조아하세요ㅡ",
          "original_form": "뭐 조아하세요ㅡ",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100014377.1.5",
          "form": "저도 동물 다 좋아해요 벌레 빼고ㅋㅋ",
          "original_form": "저도 동물 다 좋아해요 벌레 빼고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100014377.1.6",
          "form": "맞아요.. 벌레는... 윽..",
          "original_form": "맞아요.. 벌레는... 윽..",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100014377.1.7",
          "form": "그래도 파충류 사랑하시죠? ㅋㅋㅋㅋ",
          "original_form": "그래도 파충류 사랑하시죠? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100014377.1.8",
          "form": "네 파충류까진 괜찮네요ㅋㅋ",
          "original_form": "네 파충류까진 괜찮네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 07:45"
        },
        {
          "id": "MDRW2100014377.1.9",
          "form": "윾... 전 파충류 징그러워서 싫은데.. ㅋㅋ \r\n뭐키워보신거 있나요ㅏ",
          "original_form": "윾... 전 파충류 징그러워서 싫은데.. ㅋㅋ \r\n뭐키워보신거 있나요ㅏ",
          "speaker_id": "1",
          "time": "20210826 08:46"
        },
        {
          "id": "MDRW2100014377.1.10",
          "form": "저 강아지랑 햄스터요ㅋㅋ 호랑이님은요?",
          "original_form": "저 강아지랑 햄스터요ㅋㅋ 호랑이님은요?",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100014377.1.11",
          "form": "햄스터 키우기 어렵나요?",
          "original_form": "햄스터 키우기 어렵나요?",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100014377.1.12",
          "form": "어릴때라 오래 키운게 아니라 잘 모르겠어요",
          "original_form": "어릴때라 오래 키운게 아니라 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100014377.1.13",
          "form": "햄찌도 너무 귀여운데.. \r\n그것도 생명이라 시도하기 어렵네요..",
          "original_form": "햄찌도 너무 귀여운데.. \r\n그것도 생명이라 시도하기 어렵네요..",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100014377.1.14",
          "form": "네 병원 데려가긴 오히려 힘들거 같더라고요",
          "original_form": "네 병원 데려가긴 오히려 힘들거 같더라고요",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100014377.1.15",
          "form": "전 고양이 너무 키우고 싶어요",
          "original_form": "전 고양이 너무 키우고 싶어요",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100014377.1.16",
          "form": "고양이 귀엽죠",
          "original_form": "고양이 귀엽죠",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100014377.1.17",
          "form": "요즘 길고양이들 많이 보이더라구요",
          "original_form": "요즘 길고양이들 많이 보이더라구요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100014377.1.18",
          "form": "길고양이도 귀여운데.. 캣맘문제도 심각하더라구요",
          "original_form": "길고양이도 귀여운데.. 캣맘문제도 심각하더라구요",
