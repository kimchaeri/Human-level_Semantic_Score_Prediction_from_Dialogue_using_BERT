      "utterance": [
        {
          "id": "MDRW2100024244.1.1",
          "form": "배달시켜서?",
          "original_form": "배달시켜서?",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024244.1.2",
          "form": "집 근처라 포장하러 갈랬는데 ㅋㅋㅋ",
          "original_form": "집 근처라 포장하러 갈랬는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024244.1.3",
          "form": "아하 집 근처에 횟집이 있구나ㅎㅎ",
          "original_form": "아하 집 근처에 횟집이 있구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024244.1.4",
          "form": "전어구이 먹고싶네",
          "original_form": "전어구이 먹고싶네",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024244.1.5",
          "form": "낼 먹어야지 오랜만에 먹을 의지가 생겼는데 말여 ㅋㅋㅋ",
          "original_form": "낼 먹어야지 오랜만에 먹을 의지가 생겼는데 말여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024244.1.6",
          "form": "생선 구이도 맛있겠",
          "original_form": "생선 구이도 맛있겠",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024244.1.7",
          "form": "응 난 원래 구이를 더 좋아함ㅋㅋ",
          "original_form": "응 난 원래 구이를 더 좋아함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024244.1.8",
          "form": "생선 구이집에서 먹고싶네 ㅋㅋ 고갈비..술 안주...",
          "original_form": "생선 구이집에서 먹고싶네 ㅋㅋ 고갈비..술 안주...",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024244.1.9",
          "form": "고갈비 좋다 캬",
          "original_form": "고갈비 좋다 캬",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024244.1.10",
          "form": "핫플에 술마시러 가곱네 ㅋㅋㅋ",
          "original_form": "핫플에 술마시러 가곱네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024244.1.11",
          "form": "횟집갔다가 공짜회 얻어먹었다 ㅋㅋ",
          "original_form": "횟집갔다가 공짜회 얻어먹었다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024244.1.12",
          "form": "오 전어회 샀구나!",
          "original_form": "오 전어회 샀구나!",
          "speaker_id": "2",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024244.1.13",
          "form": "아니 못 샀어...",
          "original_form": "아니 못 샀어...",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024244.1.14",
          "form": "...공짜 회만 먹은거?",
          "original_form": "...공짜 회만 먹은거?",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024244.1.15",
          "form": "영업은 8시까지라 6시 반에 갔는데 회 다 팔려서 없다해서 못 사고 몇명 먹냐해서 두명이라니까 그럼 1인분 남은 회 있다고 갖고 가서 맛보라고 주심 ㅋㅋㅋㅋ",
          "original_form": "영업은 8시까지라 6시 반에 갔는데 회 다 팔려서 없다해서 못 사고 몇명 먹냐해서 두명이라니까 그럼 1인분 남은 회 있다고 갖고 가서 맛보라고 주심 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024244.1.16",
          "form": "첨 간 가게인데 머선일이고 ㅋㅋㅋ",
          "original_form": "첨 간 가게인데 머선일이고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024244.1.17",
          "form": "오오 장사 잘되나보네ㅎㅎ",
          "original_form": "오오 장사 잘되나보네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024244.1.18",
          "form": "낼 올게요 하고 왔다 낼은 5시에 도전 ㅋㅋㅋ 월요일에 먹을랬는데 휴무라 못 먹었는데 ㅋㅋㅋ",
          "original_form": "낼 올게요 하고 왔다 낼은 5시에 도전 ㅋㅋㅋ 월요일에 먹을랬는데 휴무라 못 먹었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100024244.1.19",
          "form": "낼 저녁은 전어회구먼ㅋㅋㅋ",
          "original_form": "낼 저녁은 전어회구먼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024244.1.20",
          "form": "사장님 너무 친절쓰",
          "original_form": "사장님 너무 친절쓰",
          "speaker_id": "1",
