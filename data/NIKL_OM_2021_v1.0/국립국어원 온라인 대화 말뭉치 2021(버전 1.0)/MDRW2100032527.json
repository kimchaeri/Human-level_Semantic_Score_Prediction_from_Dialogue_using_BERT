      "utterance": [
        {
          "id": "MDRW2100032527.1.1",
          "form": "기혼이신가요?",
          "original_form": "기혼이신가요?",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100032527.1.2",
          "form": "아뇨 미혼입니다 ㅎㅎ",
          "original_form": "아뇨 미혼입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100032527.1.3",
          "form": "그러시군요..저는 결혼 8 년 차 입니다",
          "original_form": "그러시군요..저는 결혼 8 년 차 입니다",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032527.1.4",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032527.1.5",
          "form": "아이도 있으신가여?",
          "original_form": "아이도 있으신가여?",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032527.1.6",
          "form": "네 아이 둘이요",
          "original_form": "네 아이 둘이요",
          "speaker_id": "1",
          "time": "20210906 05:46"
        },
        {
          "id": "MDRW2100032527.1.7",
          "form": "일찍 일어나셨네요",
          "original_form": "일찍 일어나셨네요",
          "speaker_id": "2",
          "time": "20210906 05:49"
        },
        {
          "id": "MDRW2100032527.1.8",
          "form": "네 아이들 잘때 같이 일찍 잠들어요",
          "original_form": "네 아이들 잘때 같이 일찍 잠들어요",
          "speaker_id": "1",
          "time": "20210906 05:58"
        },
        {
          "id": "MDRW2100032527.1.9",
          "form": "이상형 있나요",
          "original_form": "이상형 있나요",
          "speaker_id": "1",
          "time": "20210906 05:58"
        },
        {
          "id": "MDRW2100032527.1.10",
          "form": "이상형이라기 보다는",
          "original_form": "이상형이라기 보다는",
          "speaker_id": "2",
          "time": "20210906 06:00"
        },
        {
          "id": "MDRW2100032527.1.11",
          "form": "잘 통하는사람?",
          "original_form": "잘 통하는사람?",
          "speaker_id": "2",
          "time": "20210906 06:01"
        },
        {
          "id": "MDRW2100032527.1.12",
          "form": "느낌오는사람?ㅋㅋ",
          "original_form": "느낌오는사람?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 06:01"
        },
        {
          "id": "MDRW2100032527.1.13",
          "form": "그렇죠 잘 통하는게 중요할 듯 해요",
          "original_form": "그렇죠 잘 통하는게 중요할 듯 해요",
          "speaker_id": "1",
          "time": "20210906 06:30"
        },
        {
          "id": "MDRW2100032527.1.14",
          "form": "출근길에 이상형을 생각하니까",
          "original_form": "출근길에 이상형을 생각하니까",
          "speaker_id": "2",
          "time": "20210906 07:56"
        },
        {
          "id": "MDRW2100032527.1.15",
          "form": "잘 안떠오르긴하네요",
          "original_form": "잘 안떠오르긴하네요",
          "speaker_id": "2",
          "time": "20210906 07:56"
        },
        {
          "id": "MDRW2100032527.1.16",
          "form": "그렇죠 소개팅은 많이 해보셨나요",
          "original_form": "그렇죠 소개팅은 많이 해보셨나요",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100032527.1.17",
          "form": "소개팅 질리도록 하다가 지금 여친을 만났죠 ㅎㅎ",
          "original_form": "소개팅 질리도록 하다가 지금 여친을 만났죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100032527.1.18",
          "form": "아 그러셨군요 지금 여친분은 첫인상부터 달랐나요",
          "original_form": "아 그러셨군요 지금 여친분은 첫인상부터 달랐나요",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100032527.1.19",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100032527.1.20",
          "form": "사진보고 기대안하고 나갔는데",
          "original_form": "사진보고 기대안하고 나갔는데",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100032527.1.21",
          "form": "사진보다 실물이 낫더라구요",
          "original_form": "사진보다 실물이 낫더라구요",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100032527.1.22",
          "form": "오 더 좋으셨겠어요",
          "original_form": "오 더 좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100032527.1.23",
          "form": "소개팅만 10번 넘게 하다가",
          "original_form": "소개팅만 10번 넘게 하다가",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100032527.1.24",
          "form": "현타와서 그만하려다가",
          "original_form": "현타와서 그만하려다가",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100032527.1.25",
          "form": "만난거였어요 ㅋㅋ",
          "original_form": "만난거였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100032527.1.26",
          "form": "아주 잘하셨네요",
          "original_form": "아주 잘하셨네요",
          "speaker_id": "1",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100032527.1.27",
          "form": "어떻게 만나셨어요?",
          "original_form": "어떻게 만나셨어요?",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100032527.1.28",
          "form": "남편분이여",
          "original_form": "남편분이여",
          "speaker_id": "2",
          "time": "20210906 20:12"
        }
      ]
    }
