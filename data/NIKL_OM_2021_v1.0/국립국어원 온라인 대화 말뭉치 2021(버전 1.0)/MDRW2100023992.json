      "utterance": [
        {
          "id": "MDRW2100023992.1.1",
          "form": "근데 대부분의 끼니를 말씀하신 3분컷으로 먹는 거 같긴 해요.",
          "original_form": "근데 대부분의 끼니를 말씀하신 3분컷으로 먹는 거 같긴 해요.",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023992.1.2",
          "form": "가스불 거의 안 쓰고 전자렌지 5분컷이 가능한 음식을 주로 먹는 듯...",
          "original_form": "가스불 거의 안 쓰고 전자렌지 5분컷이 가능한 음식을 주로 먹는 듯...",
          "speaker_id": "1",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023992.1.3",
          "form": "네 특식이 아닌 이상 어차피 밥은 루틴이라",
          "original_form": "네 특식이 아닌 이상 어차피 밥은 루틴이라",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023992.1.4",
          "form": "부모세대는 맨날 된장찌개 밥과반찬",
          "original_form": "부모세대는 맨날 된장찌개 밥과반찬",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023992.1.5",
          "form": "이게 루틴이었구요",
          "original_form": "이게 루틴이었구요",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023992.1.6",
          "form": "마저요 국/찌개, 밥, 반찬",
          "original_form": "마저요 국/찌개, 밥, 반찬",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023992.1.7",
          "form": "그거랑 비교하면 서양식 간편식이 더 편하죠",
          "original_form": "그거랑 비교하면 서양식 간편식이 더 편하죠",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023992.1.8",
          "form": "네 글서 파스타 같은 건 가끔 만들어 먹어요ㅋㅋ",
          "original_form": "네 글서 파스타 같은 건 가끔 만들어 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023992.1.9",
          "form": "소스는 전부 시판 소스 사서...",
          "original_form": "소스는 전부 시판 소스 사서...",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023992.1.10",
          "form": "편하긴해요",
          "original_form": "편하긴해요",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023992.1.11",
          "form": "된장찌갣8 시판소스사면",
          "original_form": "된장찌갣8 시판소스사면",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023992.1.12",
          "form": "두부감자양파애호박",
          "original_form": "두부감자양파애호박",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023992.1.13",
          "form": "에다가 파우지 된장국 소스 팍",
          "original_form": "에다가 파우지 된장국 소스 팍",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023992.1.14",
          "form": "ㅋㅋㅋㅋㅋ 전 주로 비비고 사서 야채 추가해서 끓여요",
          "original_form": "ㅋㅋㅋㅋㅋ 전 주로 비비고 사서 야채 추가해서 끓여요",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023992.1.15",
          "form": "김치도 비비고 찌개도 비비고",
          "original_form": "김치도 비비고 찌개도 비비고",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023992.1.16",
          "form": "만두도 비비고",
          "original_form": "만두도 비비고",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023992.1.17",
          "form": "대기업의 입맛!",
          "original_form": "대기업의 입맛!",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023992.1.18",
          "form": "오뚜기파는 아니시네요",
          "original_form": "오뚜기파는 아니시네요",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023992.1.19",
          "form": "오뚜기는 좀 맛없긴하죠",
          "original_form": "오뚜기는 좀 맛없긴하죠",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023992.1.20",
          "form": "넹...",
          "original_form": "넹...",
          "speaker_id": "1",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023992.1.21",
          "form": "오뚜기는 햇반도 맛없어요",
          "original_form": "오뚜기는 햇반도 맛없어요",
          "speaker_id": "1",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023992.1.22",
          "form": "갓뚜기잍 이유는",
          "original_form": "갓뚜기잍 이유는",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023992.1.23",
          "form": "저렴해서 ^^ㅋㅋㅋ딱 그거",
          "original_form": "저렴해서 ^^ㅋㅋㅋ딱 그거",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023992.1.24",
          "form": "ㅋㅋㅋㅋ 편의점 컵밥이나 죽 같은 건 딱히 싸지도 않아요.",
          "original_form": "ㅋㅋㅋㅋ 편의점 컵밥이나 죽 같은 건 딱히 싸지도 않아요.",
          "speaker_id": "1",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023992.1.25",
          "form": "양반죽하고 오뚜기죽 가격 똑같고",
          "original_form": "양반죽하고 오뚜기죽 가격 똑같고",
          "speaker_id": "1",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023992.1.26",
          "form": "아 근데 오뚜기 라면은 꽤 맛있어졌어요",
          "original_form": "아 근데 오뚜기 라면은 꽤 맛있어졌어요",
          "speaker_id": "1",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023992.1.27",
          "form": "저야 뭐 식자재 가격을 주로 보니깐",
          "original_form": "저야 뭐 식자재 가격을 주로 보니깐",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023992.1.28",
          "form": "아하ㅋㅋㅋ 이렇게 또 요리하는 자와 사먹는 자의 차이가ㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋ 이렇게 또 요리하는 자와 사먹는 자의 차이가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023992.1.29",
          "form": "20키로 소기름 가격으로 보면",
          "original_form": "20키로 소기름 가격으로 보면",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023992.1.30",
          "form": "오뚜기가 큐원보다 저렴했어요",
          "original_form": "오뚜기가 큐원보다 저렴했어요",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023992.1.31",
          "form": "오뚜기에서 소기름도 파나요ㅋㅋㅋㅋ",
          "original_form": "오뚜기에서 소기름도 파나요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023992.1.32",
          "form": "식자재로 나오는 기름은 조 ㅁ 더 여러가지에요",
