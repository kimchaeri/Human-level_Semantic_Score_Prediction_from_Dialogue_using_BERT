      "utterance": [
        {
          "id": "MDRW2100023121.1.1",
          "form": "글죠글죠ㅎ",
          "original_form": "글죠글죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023121.1.2",
          "form": "아 근데 전 또 한번도 키워본적이 없어서요",
          "original_form": "아 근데 전 또 한번도 키워본적이 없어서요",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023121.1.3",
          "form": "치와와?  동영상보고",
          "original_form": "치와와?  동영상보고",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023121.1.4",
          "form": "바로 로망접음 ㅋㅋ",
          "original_form": "바로 로망접음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023121.1.5",
          "form": "왜요?ㄹㅎ",
          "original_form": "왜요?ㄹㅎ",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023121.1.6",
          "form": "뭐않좋은점이라도?ㅎㅎ",
          "original_form": "뭐않좋은점이라도?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023121.1.7",
          "form": "성질이 성질이 ㅋㅋ",
          "original_form": "성질이 성질이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.8",
          "form": "아아앙ㅎㅎㅎ",
          "original_form": "아아앙ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.9",
          "form": "성질은 개마다 달라요ㅎㅎ",
          "original_form": "성질은 개마다 달라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.10",
          "form": "쟤를 키우면 내가 쟤를 머리에이고 상전모시듯 해야겠구나 싶더라고요 ㅋㅋㅋ",
          "original_form": "쟤를 키우면 내가 쟤를 머리에이고 상전모시듯 해야겠구나 싶더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.11",
          "form": "개들도 다 성격이있더라구요ㅎ",
          "original_form": "개들도 다 성격이있더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.12",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.13",
          "form": "근데 그렇게되요ㅎㅎ",
          "original_form": "근데 그렇게되요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.14",
          "form": "전 그 어떤 외국사람이 인터뷰하는거 봤는데",
          "original_form": "전 그 어떤 외국사람이 인터뷰하는거 봤는데",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.15",
          "form": "생각해보세요ㅎㅎ 목욕시키려면 모셔야해요ㅎㅎ",
          "original_form": "생각해보세요ㅎㅎ 목욕시키려면 모셔야해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.16",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023121.1.17",
          "form": "그  쪼꼬만 치와와가 진짜 끈임없이 성질부리면서",
          "original_form": "그  쪼꼬만 치와와가 진짜 끈임없이 성질부리면서",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023121.1.18",
          "form": "영상내내",
          "original_form": "영상내내",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023121.1.19",
          "form": "성질부리면서",
          "original_form": "성질부리면서",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023121.1.20",
          "form": "주인손을 깨물어 ㄷㄷㄷ",
          "original_form": "주인손을 깨물어 ㄷㄷㄷ",
