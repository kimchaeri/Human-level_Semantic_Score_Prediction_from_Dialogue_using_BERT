      "utterance": [
        {
          "id": "MDRW2100007220.1.1",
          "form": "먹는거 최고!",
          "original_form": "먹는거 최고!",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007220.1.2",
          "form": "탕수육 부먹 최고!",
          "original_form": "탕수육 부먹 최고!",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007220.1.3",
          "form": "탕수육 먹고 싶다",
          "original_form": "탕수육 먹고 싶다",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007220.1.4",
          "form": "진심",
          "original_form": "진심",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007220.1.5",
          "form": "월요일날 중국집 먹을까",
          "original_form": "월요일날 중국집 먹을까",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007220.1.6",
          "form": "회사에서 시켜먹어",
          "original_form": "회사에서 시켜먹어",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007220.1.7",
          "form": "그러면 그 옛날 탕수육 먹을 수 있음",
          "original_form": "그러면 그 옛날 탕수육 먹을 수 있음",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007220.1.8",
          "form": "나 강남에서 유명한 짜장면 시켜먹어봄",
          "original_form": "나 강남에서 유명한 짜장면 시켜먹어봄",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007220.1.9",
          "form": "꿔바로우 이런거 노노 옛날 그 가는 탕수육",
          "original_form": "꿔바로우 이런거 노노 옛날 그 가는 탕수육",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.10",
          "form": "곱창튀긴거랑 파채가 올라가는 짜장면",
          "original_form": "곱창튀긴거랑 파채가 올라가는 짜장면",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.11",
          "form": "그거는 짜장면이 아니야",
          "original_form": "그거는 짜장면이 아니야",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.12",
          "form": "변절자",
          "original_form": "변절자",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.13",
          "form": "첫맛은 좋은데 먹을수록 별로여",
          "original_form": "첫맛은 좋은데 먹을수록 별로여",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.14",
          "form": "창화루",
          "original_form": "창화루",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.15",
          "form": "기억해둬",
          "original_form": "기억해둬",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.16",
          "form": "봐봐 오리지널 최고!",
          "original_form": "봐봐 오리지널 최고!",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.17",
          "form": "먹지마 ㅋㅋㅋㅋㅋ",
          "original_form": "먹지마 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.18",
          "form": "요즘 중국집 꿔바로유 너무 많아",
          "original_form": "요즘 중국집 꿔바로유 너무 많아",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007220.1.19",
          "form": "옛날 탕수육이 좋은데",
          "original_form": "옛날 탕수육이 좋은데",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.20",
          "form": "탕수육만 시켜서 혼자 먹을까 ㅋㅋㅋㅋㅋ",
          "original_form": "탕수육만 시켜서 혼자 먹을까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.21",
          "form": "맞아 중국에서 꿔바로우는 물에 찍어먹어여",
          "original_form": "맞아 중국에서 꿔바로우는 물에 찍어먹어여",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.22",
          "form": "아니가 맛탕인가?",
          "original_form": "아니가 맛탕인가?",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.23",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.24",
          "form": "물?????",
          "original_form": "물?????",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.25",
          "form": "맛탕같기두...",
          "original_form": "맛탕같기두...",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.26",
          "form": "너무 끈적해서",
          "original_form": "너무 끈적해서",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.27",
          "form": "맹물에?????",
          "original_form": "맹물에?????",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007220.1.28",
          "form": "왜??,,,",
          "original_form": "왜??,,,",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.29",
          "form": "물에 한번 찍어먹음",
          "original_form": "물에 한번 찍어먹음",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.30",
          "form": "너무 끈적하니까!",
          "original_form": "너무 끈적하니까!",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.31",
          "form": "진심 이해를 못하겠네",
          "original_form": "진심 이해를 못하겠네",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.32",
          "form": "먹기 편해짐",
          "original_form": "먹기 편해짐",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.33",
          "form": "입에 덜 달라붙고",
          "original_form": "입에 덜 달라붙고",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.34",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.35",
          "form": "이상한 동네네",
          "original_form": "이상한 동네네",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007220.1.36",
          "form": "아닌데 먹기 편하고 좋은디",
          "original_form": "아닌데 먹기 편하고 좋은디",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007220.1.37",
          "form": "나 스벅 쿠폰 5개 있어..",
          "original_form": "나 스벅 쿠폰 5개 있어..",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007220.1.38",
          "form": "포인트로 더 살 슈 있어... 써쥬ㅓㅇ....",
          "original_form": "포인트로 더 살 슈 있어... 써쥬ㅓㅇ....",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007220.1.39",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007220.1.40",
          "form": "부자네부자",
          "original_form": "부자네부자",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007220.1.41",
          "form": "그러면 나 그레이라떼 먹고 싶어",
          "original_form": "그러면 나 그레이라떼 먹고 싶어",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007220.1.42",
          "form": "근데 제스트가 맛나다고 함",
          "original_form": "근데 제스트가 맛나다고 함",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007220.1.43",
          "form": "먹을 수 있으면 먹어봐",
          "original_form": "먹을 수 있으면 먹어봐",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007220.1.44",
          "form": "제스트...?",
          "original_form": "제스트...?",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.45",
          "form": "그게 뭔가여",
          "original_form": "그게 뭔가여",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.46",
          "form": "제스트로 시작하는 이름임",
          "original_form": "제스트로 시작하는 이름임",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.47",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.48",
          "form": "사과맛인가 그렇다고 하는데",
          "original_form": "사과맛인가 그렇다고 하는데",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.49",
          "form": "마시는거야?",
          "original_form": "마시는거야?",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.50",
          "form": "스벅에서 나온 신메뉴",
          "original_form": "스벅에서 나온 신메뉴",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.51",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.52",
          "form": "처음들어보ㅓ",
          "original_form": "처음들어보ㅓ",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.53",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.54",
          "form": "처음들어봐",
          "original_form": "처음들어봐",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.55",
          "form": "먹기 힘들거든",
          "original_form": "먹기 힘들거든",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.56",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.57",
          "form": "안팔아?",
          "original_form": "안팔아?",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.58",
          "form": "잘 안팔아....",
          "original_form": "잘 안팔아....",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007220.1.59",
          "form": "신제품은 여기저기 팔어여지...",
          "original_form": "신제품은 여기저기 팔어여지...",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007220.1.60",
          "form": "아침에 내가 사이렌 오더로 오푼하자마자 봐도 재고뷰족임",
          "original_form": "아침에 내가 사이렌 오더로 오푼하자마자 봐도 재고뷰족임",
