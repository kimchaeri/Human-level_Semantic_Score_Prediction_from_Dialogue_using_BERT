      "utterance": [
        {
          "id": "MDRW2100044601.1.1",
          "form": "과일이 비싸서 싼것들위주로만  일단사여ㅠ",
          "original_form": "과일이 비싸서 싼것들위주로만  일단사여ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044601.1.2",
          "form": "좀내리면 사먹으려구요ㅠ",
          "original_form": "좀내리면 사먹으려구요ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044601.1.3",
          "form": "맞아여 ㅠㅠ 샤인머스캣도 참 좋아하는데..가격이 ..ㅜㅠ",
          "original_form": "맞아여 ㅠㅠ 샤인머스캣도 참 좋아하는데..가격이 ..ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044601.1.4",
          "form": "너무 비싸서 ㅠㅠ",
          "original_form": "너무 비싸서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044601.1.5",
          "form": "그쵸 너무 비싸요 ㅠㅜ",
          "original_form": "그쵸 너무 비싸요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044601.1.6",
          "form": "샤인 처음에 후숙 안하고 먹으니 맛 하나도 없어서 뭐지 했는데 시간 지나면 달아지더라구요",
          "original_form": "샤인 처음에 후숙 안하고 먹으니 맛 하나도 없어서 뭐지 했는데 시간 지나면 달아지더라구요",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044601.1.7",
          "form": "맞아여 그리고 그것도 잘사야되더라구요..",
          "original_form": "맞아여 그리고 그것도 잘사야되더라구요..",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044601.1.8",
          "form": "저번에 샀는데 그냥 청포도맛이어서 진짜.. 사기당힌 기분이었네요",
          "original_form": "저번에 샀는데 그냥 청포도맛이어서 진짜.. 사기당힌 기분이었네요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044601.1.9",
          "form": "가격도 안싼데 ㅠ 그렇게까지 랜덤이면 정말 짜증날듯,,ㅠㅠ",
          "original_form": "가격도 안싼데 ㅠ 그렇게까지 랜덤이면 정말 짜증날듯,,ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044601.1.10",
          "form": "그러니까요 ㅠㅠ",
          "original_form": "그러니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044601.1.11",
          "form": "얼른 귤 제철 왔음 좋겠네요",
          "original_form": "얼른 귤 제철 왔음 좋겠네요",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044601.1.12",
          "form": "귤도 엄청...비싸더라구요 ㅠㅠㅠㅠㅠ 빨리 내리길",
          "original_form": "귤도 엄청...비싸더라구요 ㅠㅠㅠㅠㅠ 빨리 내리길",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044601.1.13",
          "form": "작은박스막 2,3만원하니까",
          "original_form": "작은박스막 2,3만원하니까",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044601.1.14",
          "form": "귤 귀신있는집은 엄두가 안나요",
          "original_form": "귤 귀신있는집은 엄두가 안나요",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044601.1.15",
          "form": "집에 귤귀신이 사시는군요...",
          "original_form": "집에 귤귀신이 사시는군요...",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044601.1.16",
          "form": "겨울되면 저렴해질것같아요 ㅎㅎ",
          "original_form": "겨울되면 저렴해질것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044601.1.17",
          "form": "네 그래서 좀더 있다가 ㅋㅋ 지금은 사과나 캠밸로 ㅎㅎ",
          "original_form": "네 그래서 좀더 있다가 ㅋㅋ 지금은 사과나 캠밸로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044601.1.18",
          "form": "맞아요 사과도 엄청 비쌌었는제 지금은 괜찮죠..!",
          "original_form": "맞아요 사과도 엄청 비쌌었는제 지금은 괜찮죠..!",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044601.1.19",
          "form": "맞아여 홍로가 싸더라구요",
          "original_form": "맞아여 홍로가 싸더라구요",
          "speaker_id": "1",
