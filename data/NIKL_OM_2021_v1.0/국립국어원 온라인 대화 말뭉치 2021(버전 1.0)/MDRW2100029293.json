      "utterance": [
        {
          "id": "MDRW2100029293.1.1",
          "form": "나는 결혼하지 않을 거야",
          "original_form": "나는 결혼하지 않을 거야",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029293.1.2",
          "form": "물론 연애 생각도 없어",
          "original_form": "물론 연애 생각도 없어",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029293.1.3",
          "form": "ㅋㅋㅋㅋㅋ 나도 결혼 생각 없어",
          "original_form": "ㅋㅋㅋㅋㅋ 나도 결혼 생각 없어",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100029293.1.4",
          "form": "연애도 앵간하면 안 하지 않을까",
          "original_form": "연애도 앵간하면 안 하지 않을까",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100029293.1.5",
          "form": "연애는... 솔직히 모르겠다 마음 맞는 사람 있으면 또 생각이 다를 지도",
          "original_form": "연애는... 솔직히 모르겠다 마음 맞는 사람 있으면 또 생각이 다를 지도",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100029293.1.6",
          "form": "이렇게 된 김에 이상형이나 한 번 들어보자",
          "original_form": "이렇게 된 김에 이상형이나 한 번 들어보자",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100029293.1.7",
          "form": "역시 캐릭터랑 결혼하는게 아니니까 현실 사람을 들어야겠지",
          "original_form": "역시 캐릭터랑 결혼하는게 아니니까 현실 사람을 들어야겠지",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100029293.1.8",
          "form": "당연하지ㅋㅋㅋ 어떤 사람이 좋니",
          "original_form": "당연하지ㅋㅋㅋ 어떤 사람이 좋니",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100029293.1.9",
          "form": "엄,,, 사실 아직 잘 모르겠어",
          "original_form": "엄,,, 사실 아직 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100029293.1.10",
          "form": "근데 좀 내가 맞춰주고 따라가는 성격이니까 상대는 반대였음 좋겠어",
          "original_form": "근데 좀 내가 맞춰주고 따라가는 성격이니까 상대는 반대였음 좋겠어",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100029293.1.11",
          "form": "오오 좀 리드해주는 사람?",
          "original_form": "오오 좀 리드해주는 사람?",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100029293.1.12",
          "form": "엉 상대도 좀 맞춰주고 그러는 사람 만나면",
          "original_form": "엉 상대도 좀 맞춰주고 그러는 사람 만나면",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100029293.1.13",
          "form": "우리는 무한한 선택의 굴레에 빠지지 않을까",
          "original_form": "우리는 무한한 선택의 굴레에 빠지지 않을까",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100029293.1.14",
          "form": "좀 자기 주장이 확실하고 리드해주는 사람 만나면 괜찮을 거 같어 내가 좀 좋아하는 범위도 넓고 그러니까?",
          "original_form": "좀 자기 주장이 확실하고 리드해주는 사람 만나면 괜찮을 거 같어 내가 좀 좋아하는 범위도 넓고 그러니까?",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100029293.1.15",
          "form": "ㅋㅋㅋㅋㅋ 하긴 너 성격상 그럴 것 같다",
          "original_form": "ㅋㅋㅋㅋㅋ 하긴 너 성격상 그럴 것 같다",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100029293.1.16",
          "form": "밥 먹는 거나 데이트도 결정하는 데 오래 걸릴 것 같어",
          "original_form": "밥 먹는 거나 데이트도 결정하는 데 오래 걸릴 것 같어",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100029293.1.17",
          "form": "어 완전",
          "original_form": "어 완전",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100029293.1.18",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㄱㅋㄱㅋ난 정말 아무거나 괜찮은데 상대가 좀 지랑 안 놀고싶어하는 걸로 생각할까봐 걱정됨",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㄱㅋㄱㅋ난 정말 아무거나 괜찮은데 상대가 좀 지랑 안 놀고싶어하는 걸로 생각할까봐 걱정됨",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100029293.1.19",
          "form": "아니 뭐가 나쁜말인데",
          "original_form": "아니 뭐가 나쁜말인데",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100029293.1.20",
          "form": "걍 상대쪽에서 먼저 뭐를 하자고 얘기해줬음 좋겠어",
          "original_form": "걍 상대쪽에서 먼저 뭐를 하자고 얘기해줬음 좋겠어",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100029293.1.21",
          "form": "진짜 이건 아니다 싶은 거 아니곤 다 맞춰줄 수 있을 거 같으니까?",
          "original_form": "진짜 이건 아니다 싶은 거 아니곤 다 맞춰줄 수 있을 거 같으니까?",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100029293.1.22",
          "form": "맞춰준다기보단.,, 같이 하는 거지",
          "original_form": "맞춰준다기보단.,, 같이 하는 거지",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100029293.1.23",
          "form": "정말 이끌어주는 사람을 원하는구나",
          "original_form": "정말 이끌어주는 사람을 원하는구나",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100029293.1.24",
          "form": "적극적인 사람!",
          "original_form": "적극적인 사람!",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100029293.1.25",
          "form": "내가 이끄는 거 보다... 이끌리는게 차라리 맘 편하다",
          "original_form": "내가 이끄는 거 보다... 이끌리는게 차라리 맘 편하다",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100029293.1.26",
          "form": "ㅋㅋㅋㅋㅋ 그럴 수 있지",
          "original_form": "ㅋㅋㅋㅋㅋ 그럴 수 있지",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100029293.1.27",
          "form": "상당히 수동적인 인간이라서..ㅋ.ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "상당히 수동적인 인간이라서..ㅋ.ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100029293.1.28",
          "form": "그것도 뭐 성향인 거니까",
          "original_form": "그것도 뭐 성향인 거니까",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100029293.1.29",
          "form": "생각해보면 더 있을 거 같긴 하지만",
          "original_form": "생각해보면 더 있을 거 같긴 하지만",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100029293.1.30",
          "form": "내 얘기만 하면 재미없젆어",
          "original_form": "내 얘기만 하면 재미없젆어",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100029293.1.31",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ 넌 어때",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ 넌 어때",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100029293.1.32",
          "form": "난...",
          "original_form": "난...",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100029293.1.33",
          "form": "외모가 뛰어난 사람...",
          "original_form": "외모가 뛰어난 사람...",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100029293.1.34",
          "form": "보기 좋아야 그나마 오래 가더라고",
          "original_form": "보기 좋아야 그나마 오래 가더라고",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100029293.1.35",
          "form": "아닠ㅋㅋㄱㅋㄱㅋㅋㅋㅋㄱㅋㄱㅋㅋ 그럴 수 있지",
          "original_form": "아닠ㅋㅋㄱㅋㄱㅋㅋㅋㅋㄱㅋㄱㅋㅋ 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100029293.1.36",
          "form": "넘 속물적이니?",
          "original_form": "넘 속물적이니?",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100029293.1.37",
          "form": "외모가 뛰어난 거 중요하니 전혀 속물적이지 않아",
          "original_form": "외모가 뛰어난 거 중요하니 전혀 속물적이지 않아",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100029293.1.38",
          "form": "근데 성격이 완벽하게 맞기는 힘들고",
          "original_form": "근데 성격이 완벽하게 맞기는 힘들고",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100029293.1.39",
          "form": "언젠간 싸우게 되는데",
          "original_form": "언젠간 싸우게 되는데",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100029293.1.40",
          "form": "외모가 잘 나면 화가 덜 나",
          "original_form": "외모가 잘 나면 화가 덜 나",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100029293.1.41",
          "form": "ㅋㄱㅋㄱㅋㄱㅋㅋㅋㅋㄱㅋ아 맞는 말이다",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋㅋㅋㅋㄱㅋ아 맞는 말이다",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100029293.1.42",
          "form": "완전 내 이상형의 사람이 잘못한 거면 솔직히 얼굴만 봐도 반정도는 화가 풀릴듯",
          "original_form": "완전 내 이상형의 사람이 잘못한 거면 솔직히 얼굴만 봐도 반정도는 화가 풀릴듯",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100029293.1.43",
          "form": "그치 그렇다니까",
          "original_form": "그치 그렇다니까",
          "speaker_id": "1",
