      "utterance": [
        {
          "id": "MDRW2100003065.1.1",
          "form": "혿시 요즘 드라마 보세요?",
          "original_form": "혿시 요즘 드라마 보세요?",
          "speaker_id": "1",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003065.1.2",
          "form": "아니요 ㅠㅠ 집에 티비를 없애버렸어요",
          "original_form": "아니요 ㅠㅠ 집에 티비를 없애버렸어요",
          "speaker_id": "2",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003065.1.3",
          "form": "요즘 뭐가 재밌나요?",
          "original_form": "요즘 뭐가 재밌나요?",
          "speaker_id": "2",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003065.1.4",
          "form": "흑... 저는 최근에 슬의보고...",
          "original_form": "흑... 저는 최근에 슬의보고...",
          "speaker_id": "1",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003065.1.5",
          "form": "결혼작사 이혼작곡 봤어요!!",
          "original_form": "결혼작사 이혼작곡 봤어요!!",
          "speaker_id": "1",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003065.1.6",
          "form": "그럼 영상 뭐 보고싶은거 생기시면 어떻게 보세요?",
          "original_form": "그럼 영상 뭐 보고싶은거 생기시면 어떻게 보세요?",
          "speaker_id": "1",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003065.1.7",
          "form": "아! 많이 들어봤어요!",
          "original_form": "아! 많이 들어봤어요!",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003065.1.8",
          "form": "전 넷플릭스 결제했어요 ㅋㅋ",
          "original_form": "전 넷플릭스 결제했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003065.1.9",
          "form": "웬만한건 다 있더라구요",
          "original_form": "웬만한건 다 있더라구요",
          "speaker_id": "2",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003065.1.10",
          "form": "ㅎㅎ 넷플릭스도 결제되어있죠",
          "original_form": "ㅎㅎ 넷플릭스도 결제되어있죠",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003065.1.11",
          "form": "4명까지 등록되어서",
          "original_form": "4명까지 등록되어서",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003065.1.12",
          "form": "가족끼리 다 같이 봐요.ㅋㅋㅋㅋ",
          "original_form": "가족끼리 다 같이 봐요.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003065.1.13",
          "form": "넷플에서는 뭐보세요??",
          "original_form": "넷플에서는 뭐보세요??",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003065.1.14",
          "form": "전 최근에 브리저튼 다봤어요!",
          "original_form": "전 최근에 브리저튼 다봤어요!",
          "speaker_id": "2",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003065.1.15",
          "form": "새콤달콤이라고 한국 영화? 같은것도 있는데 재밌더라구요",
          "original_form": "새콤달콤이라고 한국 영화? 같은것도 있는데 재밌더라구요",
          "speaker_id": "2",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003065.1.16",
          "form": "ㅋㅋㅋㅋ 브리저튼 보셨구나아~~~~",
          "original_form": "ㅋㅋㅋㅋ 브리저튼 보셨구나아~~~~",
          "speaker_id": "1",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003065.1.17",
          "form": "브리저튼 다본친구가 다른 드라마도 추천해 줬는데",
          "original_form": "브리저튼 다본친구가 다른 드라마도 추천해 줬는데",
          "speaker_id": "1",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003065.1.18",
          "form": "나쁜말이라고 심심이가 걸러주네요.ㅠㅠ 나쁜말 아닌데..",
          "original_form": "나쁜말이라고 심심이가 걸러주네요.ㅠㅠ 나쁜말 아닌데..",
          "speaker_id": "1",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003065.1.19",
          "form": "슬의는 넷플릭스에도 있어요..",
          "original_form": "슬의는 넷플릭스에도 있어요..",
          "speaker_id": "1",
          "time": "20210820 18:23"
        },
        {
          "id": "MDRW2100003065.1.20",
          "form": "넷플릭스에 괜찮았던 국내드라마가... 저는.. 무브투헤븐...",
          "original_form": "넷플릭스에 괜찮았던 국내드라마가... 저는.. 무브투헤븐...",
          "speaker_id": "1",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100003065.1.21",
          "form": "헉 뭘까요 너무 궁금해요 ㅠㅠ ㅋㅋ",
          "original_form": "헉 뭘까요 너무 궁금해요 ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003065.1.22",
          "form": "오호 무브투헤븐 기록해둘게요",
          "original_form": "오호 무브투헤븐 기록해둘게요",
          "speaker_id": "2",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003065.1.23",
          "form": "어 고인이 돌아가시고 나면 유품정리사 분들 얘기에요.",
          "original_form": "어 고인이 돌아가시고 나면 유품정리사 분들 얘기에요.",
          "speaker_id": "1",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003065.1.24",
          "form": "여기 나오는 주인공이 좋아서 보게 되더라고요",
          "original_form": "여기 나오는 주인공이 좋아서 보게 되더라고요",
          "speaker_id": "1",
          "time": "20210821 09:31"
        },
        {
          "id": "MDRW2100003065.1.25",
          "form": "오 진짜 재밌을것같아요!",
          "original_form": "오 진짜 재밌을것같아요!",
          "speaker_id": "2",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100003065.1.26",
          "form": "이제훈이 나오네요 이 배우분 좋아해용 ㅎㅎ",
          "original_form": "이제훈이 나오네요 이 배우분 좋아해용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100003065.1.27",
          "form": "맨날 자극적인 치정사만 보다가 보니 좀 힐링 되더라고요",
          "original_form": "맨날 자극적인 치정사만 보다가 보니 좀 힐링 되더라고요",
          "speaker_id": "1",
          "time": "20210821 10:04"
        },
        {
          "id": "MDRW2100003065.1.28",
          "form": "최근에 펜트하우스? 가 유명했잖아요 막장으로 ㅋㅋ",
          "original_form": "최근에 펜트하우스? 가 유명했잖아요 막장으로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:06"
        },
        {
          "id": "MDRW2100003065.1.29",
          "form": "와... 시즌1인가 보다가 하차했어요..",
          "original_form": "와... 시즌1인가 보다가 하차했어요..",
          "speaker_id": "1",
          "time": "20210821 10:11"
        },
        {
          "id": "MDRW2100003065.1.30",
          "form": "개연성도 없이 그냥 막장...",
          "original_form": "개연성도 없이 그냥 막장...",
          "speaker_id": "1",
          "time": "20210821 10:11"
        },
        {
          "id": "MDRW2100003065.1.31",
          "form": "개연성있는 막장 결혼작사이혼작곡도 있었어요.ㅋㅋㅋ",
          "original_form": "개연성있는 막장 결혼작사이혼작곡도 있었어요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:12"
        },
        {
          "id": "MDRW2100003065.1.32",
          "form": "ㅋㅋㅋㅋ 펜트하우스 아직 하는거보고 넘 충격받았어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 펜트하우스 아직 하는거보고 넘 충격받았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:31"
        },
        {
          "id": "MDRW2100003065.1.33",
          "form": "결혼작사이혼작곡도 반전으로 시즌1 끝났다는 기사를 본것같아요",
          "original_form": "결혼작사이혼작곡도 반전으로 시즌1 끝났다는 기사를 본것같아요",
          "speaker_id": "2",
          "time": "20210821 11:31"
        },
        {
          "id": "MDRW2100003065.1.34",
          "form": "ㅎㅎ시즌2까지 끝났어요",
          "original_form": "ㅎㅎ시즌2까지 끝났어요",
          "speaker_id": "1",
          "time": "20210821 11:55"
        }
      ]
    }
