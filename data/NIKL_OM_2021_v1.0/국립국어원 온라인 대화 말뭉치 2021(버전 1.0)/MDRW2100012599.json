      "utterance": [
        {
          "id": "MDRW2100012599.1.1",
          "form": "혹시 동물키우세요??",
          "original_form": "혹시 동물키우세요??",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100012599.1.2",
          "form": "키우지는 않지만 좋아해요",
          "original_form": "키우지는 않지만 좋아해요",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100012599.1.3",
          "form": "어릴때 고슴도치 키웠었어요",
          "original_form": "어릴때 고슴도치 키웠었어요",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012599.1.4",
          "form": "키우시는 동물 있으세요?",
          "original_form": "키우시는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012599.1.5",
          "form": "오 고슴도치",
          "original_form": "오 고슴도치",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012599.1.6",
          "form": "저는 열대어요",
          "original_form": "저는 열대어요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012599.1.7",
          "form": "금붕어! 키워봤어요",
          "original_form": "금붕어! 키워봤어요",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012599.1.8",
          "form": "구피는 좀 흔하죠?",
          "original_form": "구피는 좀 흔하죠?",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012599.1.9",
          "form": "흔하죠 저도 왁플래티 흔해요",
          "original_form": "흔하죠 저도 왁플래티 흔해요",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012599.1.10",
          "form": "가만 보고있으면 넘 귀여워요~",
          "original_form": "가만 보고있으면 넘 귀여워요~",
          "speaker_id": "2",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012599.1.11",
          "form": "물갈기 가 구찮긴하지만요",
          "original_form": "물갈기 가 구찮긴하지만요",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012599.1.12",
          "form": "맞아요 제일 귀찮음ㅠㅠ근데 밥주면 뽀로록 올라와서 먹는거 보는게 젤 귀엽더라구요",
          "original_form": "맞아요 제일 귀찮음ㅠㅠ근데 밥주면 뽀로록 올라와서 먹는거 보는게 젤 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100012599.1.13",
          "form": "ㅋㅋ 밥때는 귀신이죠ㅋ",
          "original_form": "ㅋㅋ 밥때는 귀신이죠ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012599.1.14",
          "form": "ㅋㅋㅋㅋ또 키워보신 동물 있어요?",
          "original_form": "ㅋㅋㅋㅋ또 키워보신 동물 있어요?",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012599.1.15",
          "form": "아뇨., 상황이 안되네요 키우고싶은데",
          "original_form": "아뇨., 상황이 안되네요 키우고싶은데",
          "speaker_id": "1",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100012599.1.16",
          "form": "어떤동물 키워보고싶어요??",
          "original_form": "어떤동물 키워보고싶어요??",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100012599.1.17",
          "form": "새요^^!!",
