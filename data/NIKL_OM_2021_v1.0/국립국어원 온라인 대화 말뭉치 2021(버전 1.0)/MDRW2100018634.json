      "utterance": [
        {
          "id": "MDRW2100018634.1.1",
          "form": "연예인이라..",
          "original_form": "연예인이라..",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018634.1.2",
          "form": "저 두루두루 좋아하는 편이라 딱히 콕 찝을 사람은 .. 없는거같은데",
          "original_form": "저 두루두루 좋아하는 편이라 딱히 콕 찝을 사람은 .. 없는거같은데",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018634.1.3",
          "form": "좋아하는 연예인있으신가요",
          "original_form": "좋아하는 연예인있으신가요",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018634.1.4",
          "form": "저도 좀 두루두루 좋아하는데 최근에 한소희가 너무 좋더라구요",
          "original_form": "저도 좀 두루두루 좋아하는데 최근에 한소희가 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100018634.1.5",
          "form": "한소희씨 너무 매력적이죠",
          "original_form": "한소희씨 너무 매력적이죠",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018634.1.6",
          "form": "어떤점이 좋던가요",
          "original_form": "어떤점이 좋던가요",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018634.1.7",
          "form": "외모도 좋고 성격도 재밌고 ㅋㅋㅋㅋ",
          "original_form": "외모도 좋고 성격도 재밌고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100018634.1.8",
          "form": "클립 뜬거 보면 연기도 잘하더라고요",
          "original_form": "클립 뜬거 보면 연기도 잘하더라고요",
          "speaker_id": "2",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100018634.1.9",
          "form": "그 부부에 세계 거기서도 연기 진짜 잘하시더라구요",
          "original_form": "그 부부에 세계 거기서도 연기 진짜 잘하시더라구요",
          "speaker_id": "1",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100018634.1.10",
          "form": "전 그 분을 거기서 처음봐서",
          "original_form": "전 그 분을 거기서 처음봐서",
          "speaker_id": "1",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100018634.1.11",
          "form": "맞아요! 거기서 진짜 잘하더라구요",
          "original_form": "맞아요! 거기서 진짜 잘하더라구요",
          "speaker_id": "2",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100018634.1.12",
          "form": "또 다른 분 좋아하시는 분은 없으신가요",
          "original_form": "또 다른 분 좋아하시는 분은 없으신가요",
          "speaker_id": "1",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100018634.1.13",
          "form": "음 ,,,,,,,, itzy?!",
          "original_form": "음 ,,,,,,,, itzy?!",
          "speaker_id": "2",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018634.1.14",
          "form": "마.피.아",
          "original_form": "마.피.아",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018634.1.15",
          "form": "그분들이요?",
          "original_form": "그분들이요?",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018634.1.16",
          "form": "아 제발 마.피.아 라고 하지말아주세요 ㅋ ㅋ ㅋ ㅋ ㅠㅠ",
          "original_form": "아 제발 마.피.아 라고 하지말아주세요 ㅋ ㅋ ㅋ ㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 09:49"
        },
        {
          "id": "MDRW2100018634.1.17",
          "form": "진짜 제왑삐 감떨어졌다 진짜로..",
          "original_form": "진짜 제왑삐 감떨어졌다 진짜로..",
          "speaker_id": "2",
          "time": "20210827 09:49"
        },
        {
          "id": "MDRW2100018634.1.18",
          "form": "그노래",
          "original_form": "그노래",
          "speaker_id": "1",
          "time": "20210827 09:49"
        },
        {
          "id": "MDRW2100018634.1.19",
          "form": "처음 들을 때 와 진짜 마이너하다 했거든요",
          "original_form": "처음 들을 때 와 진짜 마이너하다 했거든요",
          "speaker_id": "1",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.20",
          "form": "대중성을 아예 버린건가..했는데",
          "original_form": "대중성을 아예 버린건가..했는데",
          "speaker_id": "1",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.21",
          "form": "근데 솔직히 중독되긴해요 짜증나게",
          "original_form": "근데 솔직히 중독되긴해요 짜증나게",
          "speaker_id": "2",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.22",
          "form": "어느새 따라하게되더라궁ㆍ느",
          "original_form": "어느새 따라하게되더라궁ㆍ느",
          "speaker_id": "1",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.23",
          "form": "가사 너무 구린데..",
          "original_form": "가사 너무 구린데..",
          "speaker_id": "2",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.24",
          "form": "배우 보다 더 배우...",
          "original_form": "배우 보다 더 배우...",
          "speaker_id": "2",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.25",
          "form": "그건 그래요..",
          "original_form": "그건 그래요..",
          "speaker_id": "1",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.26",
          "form": "늑대 가지고 노는 여우..",
          "original_form": "늑대 가지고 노는 여우..",
          "speaker_id": "1",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018634.1.27",
          "form": "그게 뭐야.. 너무 시대착오적",
          "original_form": "그게 뭐야.. 너무 시대착오적",
          "speaker_id": "1",
          "time": "20210827 09:51"
        },
        {
          "id": "MDRW2100018634.1.28",
          "form": "아악",
