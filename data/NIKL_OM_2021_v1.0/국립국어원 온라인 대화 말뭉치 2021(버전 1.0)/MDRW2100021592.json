      "utterance": [
        {
          "id": "MDRW2100021592.1.1",
          "form": "지금 혹시 학생이세용?",
          "original_form": "지금 혹시 학생이세용?",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021592.1.2",
          "form": "앗 저는 회사다니다가 퇴사했어용ㅠㅠㅠㅠ",
          "original_form": "앗 저는 회사다니다가 퇴사했어용ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021592.1.3",
          "form": "name2음요?",
          "original_form": "&name2&음요?",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021592.1.4",
          "form": "지금은 그럼 쉬시는 중이세요??",
          "original_form": "지금은 그럼 쉬시는 중이세요??",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021592.1.5",
          "form": "저는 회사 다니구 있어요ㅠ",
          "original_form": "저는 회사 다니구 있어요ㅠ",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021592.1.6",
          "form": "넹넹ㅠㅠㅠ 아 힘드시겠어요ㅠㅠ",
          "original_form": "넹넹ㅠㅠㅠ 아 힘드시겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.7",
          "form": "맨날 출장가고 근무지 옮기고",
          "original_form": "맨날 출장가고 근무지 옮기고",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.8",
          "form": "와 최고부럽다...",
          "original_form": "와 최고부럽다...",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.9",
          "form": "너무 힘들어서 퇴사했어요ㅠ",
          "original_form": "너무 힘들어서 퇴사했어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.11",
          "form": "저는 완전 사무직 내근직이거든요",
          "original_form": "저는 완전 사무직 내근직이거든요",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.12",
          "form": "저도 사무직 내근직이었는데",
          "original_form": "저도 사무직 내근직이었는데",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.13",
          "form": "출장갈 일이 없는 직종이라 너무 신기?해요",
          "original_form": "출장갈 일이 없는 직종이라 너무 신기?해요",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.14",
          "form": "팀 출장이나",
          "original_form": "팀 출장이나",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.15",
          "form": "부서 옮겨야하면",
          "original_form": "부서 옮겨야하면",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021592.1.16",
          "form": "저부터 보내더라구요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저부터 보내더라구요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.17",
          "form": "출장 어떤 기분일까...",
          "original_form": "출장 어떤 기분일까...",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.18",
          "form": "동기들이랑",
          "original_form": "동기들이랑",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.19",
          "form": "이거 취업사기 아니냐고하면서",
          "original_form": "이거 취업사기 아니냐고하면서",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.20",
          "form": "ㅋㅋㅋㅋㅋ 그러다 퇴사했어요",
          "original_form": "ㅋㅋㅋㅋㅋ 그러다 퇴사했어요",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 힘드셨겠어요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 힘드셨겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.22",
          "form": "다른 분들은 사무실에만 계시고^^",
          "original_form": "다른 분들은 사무실에만 계시고^^",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.23",
          "form": "그래도 동기분들이랑은 잘 지내셨나봐요",
          "original_form": "그래도 동기분들이랑은 잘 지내셨나봐요",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.24",
          "form": "네네ㅠㅜㅋㅋㅋㅋ",
          "original_form": "네네ㅠㅜㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.25",
          "form": "근데 동기들도",
          "original_form": "근데 동기들도",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.26",
          "form": "다 퇴사해서ㅋㅋㅋㅋㅋ",
          "original_form": "다 퇴사해서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.27",
          "form": "저는 동기가 없어가지구...",
          "original_form": "저는 동기가 없어가지구...",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.28",
          "form": "16명있었는데",
          "original_form": "16명있었는데",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021592.1.29",
          "form": "ㅋㅋㅋㅋ지금두 연락하고 지내세려?",
          "original_form": "ㅋㅋㅋㅋ지금두 연락하고 지내세려?",
          "speaker_id": "1",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021592.1.30",
          "form": "지금 한 세명 남았다하더라구요",
          "original_form": "지금 한 세명 남았다하더라구요",
          "speaker_id": "2",
          "time": "20210828 22:42"
        },
