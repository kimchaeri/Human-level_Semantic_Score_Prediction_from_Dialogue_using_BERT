      "utterance": [
        {
          "id": "MDRW2100045156.1.1",
          "form": "요즘 하는 게임있나요",
          "original_form": "요즘 하는 게임있나요",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045156.1.2",
          "form": "저는 주로 생방 봐요",
          "original_form": "저는 주로 생방 봐요",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045156.1.3",
          "form": "오 유투브??",
          "original_form": "오 유투브??",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045156.1.4",
          "form": "저는 트위치가 편하더라구요",
          "original_form": "저는 트위치가 편하더라구요",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045156.1.5",
          "form": "자주보는 사람 있나요",
          "original_form": "자주보는 사람 있나요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045156.1.6",
          "form": "저는 풍월량 많이봐요",
          "original_form": "저는 풍월량 많이봐요",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045156.1.7",
          "form": "저는 대도서관이요",
          "original_form": "저는 대도서관이요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045156.1.8",
          "form": "유투브에서는 그분 쫌 보긴해썽요",
          "original_form": "유투브에서는 그분 쫌 보긴해썽요",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045156.1.9",
          "form": "풍월량은 뭐가 잼죠?",
          "original_form": "풍월량은 뭐가 잼죠?",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045156.1.10",
          "form": "요즘은 좀보이드? 이거 하더르구오",
          "original_form": "요즘은 좀보이드? 이거 하더르구오",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045156.1.11",
          "form": "그게 대세인가요 유투브에 많이보이던데",
          "original_form": "그게 대세인가요 유투브에 많이보이던데",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045156.1.12",
          "form": "그런가바요",
          "original_form": "그런가바요",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045156.1.13",
          "form": "좀보이드 먼가요?",
          "original_form": "좀보이드 먼가요?",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045156.1.14",
          "form": "좀비 생존겜이더라구요",
          "original_form": "좀비 생존겜이더라구요",
