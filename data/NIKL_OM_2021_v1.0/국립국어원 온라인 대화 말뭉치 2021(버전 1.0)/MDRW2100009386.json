      "utterance": [
        {
          "id": "MDRW2100009386.1.1",
          "form": "오늘 날씨가 너무 좋아서 걷기운동 하고왔어요",
          "original_form": "오늘 날씨가 너무 좋아서 걷기운동 하고왔어요",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009386.1.2",
          "form": "요즘 하시는 운동 있으신가요?",
          "original_form": "요즘 하시는 운동 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009386.1.3",
          "form": "저는 필라테스 하고 있어요",
          "original_form": "저는 필라테스 하고 있어요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009386.1.4",
          "form": "필라테스 괜찮나용?",
          "original_form": "필라테스 괜찮나용?",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009386.1.5",
          "form": "죽을 거같다던데...",
          "original_form": "죽을 거같다던데...",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009386.1.6",
          "form": "ㅋㅋㅋㅋ 첨엔 좀 힘들긴 해요",
          "original_form": "ㅋㅋㅋㅋ 첨엔 좀 힘들긴 해요",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009386.1.7",
          "form": "생각보다 근력을 많이 써요",
          "original_form": "생각보다 근력을 많이 써요",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009386.1.8",
          "form": "근력에 필테만한 게 없다고 하시더라구요 다들",
          "original_form": "근력에 필테만한 게 없다고 하시더라구요 다들",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009386.1.9",
          "form": "근육을 여러군데 많이 쓰는 것 같아요",
          "original_form": "근육을 여러군데 많이 쓰는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009386.1.10",
          "form": "강사님이 안될 것 같은 곳까지 늘려 주세요",
          "original_form": "강사님이 안될 것 같은 곳까지 늘려 주세요",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009386.1.11",
          "form": "저도 한번 알아봐야겠어용ㅎㅎ",
          "original_form": "저도 한번 알아봐야겠어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:35"
        },
        {
          "id": "MDRW2100009386.1.12",
          "form": "비타민은 챙겨드시나요",
          "original_form": "비타민은 챙겨드시나요",
          "speaker_id": "2",
          "time": "20210823 09:50"
        },
        {
          "id": "MDRW2100009386.1.13",
          "form": "영양제 엄청 잘 먹고 있어요!",
          "original_form": "영양제 엄청 잘 먹고 있어요!",
          "speaker_id": "1",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009386.1.14",
          "form": "영양제 드시는 거 있으세요?",
          "original_form": "영양제 드시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009386.1.15",
          "form": "비타민 d랑 오메가쓰리 먹고 있어요!",
          "original_form": "비타민 d랑 오메가쓰리 먹고 있어요!",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009386.1.16",
          "form": "저는 유산균이랑 시스테인 루테인 먹고있어요",
          "original_form": "저는 유산균이랑 시스테인 루테인 먹고있어요",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100009386.1.17",
          "form": "유산균 꼭 드세용!",
          "original_form": "유산균 꼭 드세용!",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100009386.1.18",
          "form": "유산균은 어디에 좋나요",
          "original_form": "유산균은 어디에 좋나요",
          "speaker_id": "2",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100009386.1.19",
          "form": "장활동에 좋아요!",
          "original_form": "장활동에 좋아요!",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100009386.1.20",
          "form": "시스테인 루테인은 처음 들어보네요...",
          "original_form": "시스테인 루테인은 처음 들어보네요...",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100009386.1.21",
          "form": "루테인은 눈에 좋고 시스테인은 활력과 모발 단백질 등에 도움을준대서 꾸준히 먹고 있습니당",
          "original_form": "루테인은 눈에 좋고 시스테인은 활력과 모발 단백질 등에 도움을준대서 꾸준히 먹고 있습니당",
          "speaker_id": "1",
          "time": "20210823 17:53"
        }
      ]
