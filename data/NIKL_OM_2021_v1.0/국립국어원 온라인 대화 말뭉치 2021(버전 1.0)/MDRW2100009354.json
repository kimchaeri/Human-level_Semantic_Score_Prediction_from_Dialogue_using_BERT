      "utterance": [
        {
          "id": "MDRW2100009354.1.1",
          "form": "이번주에 뭐 구입하신 것 있나요?",
          "original_form": "이번주에 뭐 구입하신 것 있나요?",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009354.1.2",
          "form": "가을옷도 사고ㅎㅎ 런닝화 보구있어요",
          "original_form": "가을옷도 사고ㅎㅎ 런닝화 보구있어요",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009354.1.3",
          "form": "옷 같은거는 어디서 주로 사세요??",
          "original_form": "옷 같은거는 어디서 주로 사세요??",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009354.1.4",
          "form": "아울렛에서도 사고 인터넷에서도 사는거같아요 어디서 구매하세요?",
          "original_form": "아울렛에서도 사고 인터넷에서도 사는거같아요 어디서 구매하세요?",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009354.1.5",
          "form": "저는 인터넷에서 많이 사는것 같아요 실패와 성공의 반반 확률이지만..",
          "original_form": "저는 인터넷에서 많이 사는것 같아요 실패와 성공의 반반 확률이지만..",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009354.1.6",
          "form": "그래서 맘에 들면 색깔별로 다 사서 입어요",
          "original_form": "그래서 맘에 들면 색깔별로 다 사서 입어요",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009354.1.7",
          "form": "혹시 당근마켓 해보셨나요?",
          "original_form": "혹시 당근마켓 해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009354.1.8",
          "form": "네 당근 해봤는데 팔기만 해봤어요ㅎㅎ",
          "original_form": "네 당근 해봤는데 팔기만 해봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009354.1.9",
          "form": "저는 완전 당근 마켓 애용자거든요 ㅋㅋ",
          "original_form": "저는 완전 당근 마켓 애용자거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009354.1.10",
          "form": "혹시 지금 장바구니에 넣어두신거 있으신가요?",
          "original_form": "혹시 지금 장바구니에 넣어두신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009354.1.11",
          "form": "런닝화랑 다른거?",
          "original_form": "런닝화랑 다른거?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009354.1.12",
          "form": "갤럭시 워치 보구있어요ㅎㅎ",
          "original_form": "갤럭시 워치 보구있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009354.1.13",
          "form": "아 이번애 나온 워치죠??새로운 기능이 추가된게 있나요??",
          "original_form": "아 이번애 나온 워치죠??새로운 기능이 추가된게 있나요??",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009354.1.14",
          "form": "그 인바디기능이 있다고 해서 혹하더라구요",
          "original_form": "그 인바디기능이 있다고 해서 혹하더라구요",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009354.1.15",
          "form": "와 대박 워치에 인바디가 있다구요...?",
          "original_form": "와 대박 워치에 인바디가 있다구요...?",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009354.1.16",
          "form": "대체 무슨 원리일까요 대박이다",
          "original_form": "대체 무슨 원리일까요 대박이다",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009354.1.17",
          "form": "네 저도 신기해서 사보구 싶더라구요",
          "original_form": "네 저도 신기해서 사보구 싶더라구요",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009354.1.18",
          "form": "저도 사보고 싶네요!",
          "original_form": "저도 사보고 싶네요!",
          "speaker_id": "1",
          "time": "20210822 23:12"
        }
