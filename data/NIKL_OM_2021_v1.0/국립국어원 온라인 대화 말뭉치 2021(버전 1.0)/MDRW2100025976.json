      "utterance": [
        {
          "id": "MDRW2100025976.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025976.1.2",
          "form": "일하시는거 있으세요?",
          "original_form": "일하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025976.1.3",
          "form": "안녕하세요.퇴근하고 이제 집에서 저녁먹어요",
          "original_form": "안녕하세요.퇴근하고 이제 집에서 저녁먹어요",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025976.1.4",
          "form": "name1은요",
          "original_form": "&name1&은요",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025976.1.5",
          "form": "아 그러시구나.",
          "original_form": "아 그러시구나.",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025976.1.6",
          "form": "저는 방금 저녁먹었어요ㅋㅋ",
          "original_form": "저는 방금 저녁먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025976.1.7",
          "form": "무슨일하세요? 저녁드시고 톡주셔도되세요ㅋ",
          "original_form": "무슨일하세요? 저녁드시고 톡주셔도되세요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025976.1.8",
          "form": "회계팀에서 일해요",
          "original_form": "회계팀에서 일해요",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025976.1.9",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025976.1.10",
          "form": "아니요.ㅋㅋ 30대 주부요ㅋㅋ",
          "original_form": "아니요.ㅋㅋ 30대 주부요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025976.1.11",
          "form": "아 회계팀ㅋㅋ",
          "original_form": "아 회계팀ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025976.1.12",
          "form": "저 옛날에 잠시 회계업무했었는데",
          "original_form": "저 옛날에 잠시 회계업무했었는데",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025976.1.13",
          "form": "멋진일하시네요ㅋ",
          "original_form": "멋진일하시네요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025976.1.14",
          "form": "저도 나이 적지는않아요ㅋㅋ",
          "original_form": "저도 나이 적지는않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025976.1.15",
          "form": "괜히 반갑네요",
          "original_form": "괜히 반갑네요",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025976.1.16",
          "form": "머리아픈일이죠ㅠ",
          "original_form": "머리아픈일이죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025976.1.17",
          "form": "네네 마리아픈일이죠ㅜ",
          "original_form": "네네 마리아픈일이죠ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025976.1.18",
          "form": "고생하시네요",
          "original_form": "고생하시네요",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025976.1.19",
          "form": "저도 3년하다가ㅋㅋㅋ",
          "original_form": "저도 3년하다가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025976.1.20",
          "form": "힘들어서 다른쪽으로 갔어요ㅋ",
          "original_form": "힘들어서 다른쪽으로 갔어요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025976.1.21",
          "form": "오래하셨네요~워킹맘이세요?",
          "original_form": "오래하셨네요~워킹맘이세요?",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025976.1.22",
          "form": "저도 다른일하고도싶은데",
          "original_form": "저도 다른일하고도싶은데",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025976.1.23",
          "form": "그러면 초봉이 최저임금이에요ㅠ",
          "original_form": "그러면 초봉이 최저임금이에요ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025976.1.24",
          "form": "아 워킹맘이라고 해야되겠죠?ㅋ알바하고있어요.ㅋ",
          "original_form": "아 워킹맘이라고 해야되겠죠?ㅋ알바하고있어요.ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100025976.1.25",
          "form": "맞아요ㅜ초봉이 최저죠ㅜ",
          "original_form": "맞아요ㅜ초봉이 최저죠ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025976.1.26",
          "form": "넹 회계쪽도 엄청 짜게 시작해서ㅠㅠ",
          "original_form": "넹 회계쪽도 엄청 짜게 시작해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025976.1.27",
          "form": "선택권이없었어요",
          "original_form": "선택권이없었어요",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025976.1.28",
          "form": "네네ㅜㅜ그러시군요.ㅜㅜ",
          "original_form": "네네ㅜㅜ그러시군요.ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025976.1.29",
          "form": "저는 하다가 저랑 안맞는것같아서ㅜㅜ과감하게",
          "original_form": "저는 하다가 저랑 안맞는것같아서ㅜㅜ과감하게",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025976.1.30",
          "form": "다른쪽갔어요.ㅜ",
          "original_form": "다른쪽갔어요.ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025976.1.31",
          "form": "용기있으시네요! 한편으로 부럽네요",
          "original_form": "용기있으시네요! 한편으로 부럽네요",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100025976.1.32",
          "form": "지금일은 어때요",
          "original_form": "지금일은 어때요",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100025976.1.33",
          "form": "지금일은 미술보조강사인데",
          "original_form": "지금일은 미술보조강사인데",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100025976.1.34",
          "form": "시간도 좋고 페이도좋고ㅋㅋ",
          "original_form": "시간도 좋고 페이도좋고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100025976.1.35",
          "form": "다 맘에들어요.ㅋㄱ",
          "original_form": "다 맘에들어요.ㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100025976.1.36",
          "form": "힘든건없어요.",
          "original_form": "힘든건없어요.",
          "speaker_id": "1",
          "time": "20210901 18:59"
        }
      ]
    }
