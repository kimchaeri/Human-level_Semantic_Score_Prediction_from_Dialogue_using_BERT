      "utterance": [
        {
          "id": "MDRW2100034207.1.1",
          "form": "카페가면 보통 뭐드새요?",
          "original_form": "카페가면 보통 뭐드새요?",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034207.1.2",
          "form": "저는 커피는 못마셔서 음료는 밀크티나 티종류 많이 마셔요!",
          "original_form": "저는 커피는 못마셔서 음료는 밀크티나 티종류 많이 마셔요!",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034207.1.3",
          "form": "name2님은 주로 뭐 드시나요?",
          "original_form": "&name2&님은 주로 뭐 드시나요?",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034207.1.4",
          "form": "저는 커피 엄청 좋아해요 ㅎㅎ",
          "original_form": "저는 커피 엄청 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100034207.1.5",
          "form": "어느순간부터 커피 향이 그렇게 좋더라고요",
          "original_form": "어느순간부터 커피 향이 그렇게 좋더라고요",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100034207.1.6",
          "form": "커피가 잘 안맞으시나요?",
          "original_form": "커피가 잘 안맞으시나요?",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100034207.1.7",
          "form": "쓴맛이 잘 안맞고 커피 먹으면 잠이 안오더라구요 ..ㅠ",
          "original_form": "쓴맛이 잘 안맞고 커피 먹으면 잠이 안오더라구요 ..ㅠ",
          "speaker_id": "1",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100034207.1.8",
          "form": "다른 카페인 음료는 괜찮은데 유독 커피만 그래서 .. 못먹고 있어요 향은 좋은데..!",
          "original_form": "다른 카페인 음료는 괜찮은데 유독 커피만 그래서 .. 못먹고 있어요 향은 좋은데..!",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034207.1.9",
          "form": "이런.. 커피가 안맞나보네요 ㅠ",
          "original_form": "이런.. 커피가 안맞나보네요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034207.1.10",
          "form": "그래도 맛읶는게 널렸으니까요",
          "original_form": "그래도 맛읶는게 널렸으니까요",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034207.1.11",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034207.1.12",
          "form": "아직이요! 벌써 드셨나요?!",
          "original_form": "아직이요! 벌써 드셨나요?!",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034207.1.13",
          "form": "점심을 일찍 먹었거든요 ㅎㅎ",
          "original_form": "점심을 일찍 먹었거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034207.1.14",
          "form": "그리고 오늘 치팅데이 ㅎㅎ",
          "original_form": "그리고 오늘 치팅데이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034207.1.15",
          "form": "와우 많이 드셔야죠 시간이 금입니다... 오늘 하루 알차게 드세요!",
          "original_form": "와우 많이 드셔야죠 시간이 금입니다... 오늘 하루 알차게 드세요!",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034207.1.16",
          "form": "뭐 드셨나요!",
          "original_form": "뭐 드셨나요!",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034207.1.17",
          "form": "저 마라탕이요!",
          "original_form": "저 마라탕이요!",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034207.1.18",
          "form": "지금 배 터질거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "지금 배 터질거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034207.1.19",
          "form": "사실 이렇개 먹으면 안되지만. .ㅎ",
          "original_form": "사실 이렇개 먹으면 안되지만. .ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034207.1.20",
          "form": "아이스크림도 달리러갑니다",
          "original_form": "아이스크림도 달리러갑니다",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034207.1.21",
          "form": "하루먹어도 내일 조절하면 살 안찌더라구요 맞죠..ㅎㅎ",
          "original_form": "하루먹어도 내일 조절하면 살 안찌더라구요 맞죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034207.1.22",
          "form": "아이스크림 넘 맛있죠 다이어트때 먹음 한 백배는 더 맛있어요..",
          "original_form": "아이스크림 넘 맛있죠 다이어트때 먹음 한 백배는 더 맛있어요..",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034207.1.23",
          "form": "ㅎㅎ ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅎㅎ ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034207.1.24",
          "form": "살..모르겠어요",
          "original_form": "살..모르겠어요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034207.1.25",
          "form": "오늘 잠깐 행복해질래요",
          "original_form": "오늘 잠깐 행복해질래요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034207.1.26",
          "form": "몸무게는 잠시 잊고",
          "original_form": "몸무게는 잠시 잊고",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034207.1.27",
          "form": "맞죠맞죠...!",
          "original_form": "맞죠맞죠...!",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034207.1.28",
          "form": "맛있게먹음 0칼로라자나요 저녁 뭐드시나요?",
          "original_form": "맛있게먹음 0칼로라자나요 저녁 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034207.1.29",
          "form": "지금이게 저녁이에요 ㅋㅋㅋㅋ",
          "original_form": "지금이게 저녁이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034207.1.30",
          "form": "삭삭 긁어먹었내요",
          "original_form": "삭삭 긁어먹었내요",
          "speaker_id": "2",
          "time": "20210828 17:27"
        }
      ]
