        },
        {
          "id": "MDRW2100019573.1.5",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019573.1.6",
          "form": "말씀 편하게 하세요 누나!!!!@",
          "original_form": "말씀 편하게 하세요 누나!!!!@",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019573.1.7",
          "form": "저는 안키워요!!!!",
          "original_form": "저는 안키워요!!!!",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019573.1.8",
          "form": "누나는요??!!",
          "original_form": "누나는요??!!",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019573.1.9",
          "form": "아 괜찮아요 이게 편해서 ㅋㅋㅋㅋ",
          "original_form": "아 괜찮아요 이게 편해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019573.1.10",
          "form": "저도여 ㅠㅠ 한번도 안 키워보셨나요??",
          "original_form": "저도여 ㅠㅠ 한번도 안 키워보셨나요??",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019573.1.11",
