        },
        {
          "id": "MDRW2100038280.1.2",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210902 07:50"
        },
        {
          "id": "MDRW2100038280.1.3",
          "form": "전 가을이요",
          "original_form": "전 가을이요",
          "speaker_id": "2",
