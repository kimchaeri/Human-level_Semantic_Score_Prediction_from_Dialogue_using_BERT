      "utterance": [
        {
          "id": "MDRW2100023157.1.1",
          "form": "톡으로 사진?",
          "original_form": "톡으로 사진?",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023157.1.2",
          "form": "다른 브랜드는 맛이 어떤가 하고",
          "original_form": "다른 브랜드는 맛이 어떤가 하고",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023157.1.3",
          "form": "뜯어보려고 햇는대",
          "original_form": "뜯어보려고 햇는대",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023157.1.4",
          "form": "하몽이 원래 하얗게 기름층이 있고",
          "original_form": "하몽이 원래 하얗게 기름층이 있고",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.6",
          "form": "그 아래 빨갛게 고기층이 있거든?",
          "original_form": "그 아래 빨갛게 고기층이 있거든?",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.7",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.8",
          "form": "그거는 알고잇어",
          "original_form": "그거는 알고잇어",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.9",
          "form": "근데 하얀 층이 녹았는지",
          "original_form": "근데 하얀 층이 녹았는지",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.10",
          "form": "사라진거야 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사라진거야 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.11",
          "form": "그래서 첨엔",
          "original_form": "그래서 첨엔",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.12",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.13",
          "form": "녹았나??",
          "original_form": "녹았나??",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.14",
          "form": "싶었는데",
          "original_form": "싶었는데",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023157.1.15",
          "form": "가만 생각해보니까",
          "original_form": "가만 생각해보니까",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023157.1.16",
          "form": "내가 얠 방금 냉장고에서 꺼냈는데??",
          "original_form": "내가 얠 방금 냉장고에서 꺼냈는데??",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023157.1.17",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023157.1.18",
          "form": "진공포장되어있는데",
          "original_form": "진공포장되어있는데",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023157.1.19",
          "form": "혹시 소금같은건",
          "original_form": "혹시 소금같은건",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.20",
          "form": "아니지?",
          "original_form": "아니지?",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.21",
          "form": "안에 약간 물?같는게 잇고",
          "original_form": "안에 약간 물?같는게 잇고",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.22",
          "form": "나도 첨엔 소금이라 생각햇는데",
          "original_form": "나도 첨엔 소금이라 생각햇는데",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.23",
          "form": "지방층이 다 녹은게",
          "original_form": "지방층이 다 녹은게",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.24",
          "form": "매우 찝찝함......",
          "original_form": "매우 찝찝함......",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.25",
          "form": "찝찝하네;;;",
          "original_form": "찝찝하네;;;",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.26",
          "form": "근데 구분할 자신 없으면",
          "original_form": "근데 구분할 자신 없으면",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.27",
          "form": "그티",
          "original_form": "그티",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.28",
          "form": "치",
          "original_form": "치",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.29",
          "form": "버리는게 나을듯.....",
          "original_form": "버리는게 나을듯.....",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.30",
          "form": "ㅇㅇ 버려야지 ㅠ",
          "original_form": "ㅇㅇ 버려야지 ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.31",
          "form": "잘못먹으면 탈 크게 나...",
          "original_form": "잘못먹으면 탈 크게 나...",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023157.1.32",
          "form": "ㅇㅇ 고기류라서",
          "original_form": "ㅇㅇ 고기류라서",
          "speaker_id": "1",
          "time": "20210829 22:20"
        }
      ]
    }
