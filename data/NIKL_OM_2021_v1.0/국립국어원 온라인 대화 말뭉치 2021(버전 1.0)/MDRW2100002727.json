      "utterance": [
        {
          "id": "MDRW2100002727.1.1",
          "form": "나 방금 사과 공짜로 구매함 ㅎㅎ",
          "original_form": "나 방금 사과 공짜로 구매함 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002727.1.2",
          "form": "엥 어떻게?",
          "original_form": "엥 어떻게?",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002727.1.3",
          "form": "쇼핑앱 가입하면 그 금액으로 구매할 수 있어서",
          "original_form": "쇼핑앱 가입하면 그 금액으로 구매할 수 있어서",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002727.1.4",
          "form": "오 득템",
          "original_form": "오 득템",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002727.1.5",
          "form": "근데 썩은 사과 오는 거 아니겠지? ㅋㅋㅋ",
          "original_form": "근데 썩은 사과 오는 거 아니겠지? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002727.1.6",
          "form": "에이 설마~ 이미지가 있는데",
          "original_form": "에이 설마~ 이미지가 있는데",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002727.1.7",
          "form": "새로 생긴 쇼핑몰이니 너무 나쁜 상품은 안보내겠지?",
          "original_form": "새로 생긴 쇼핑몰이니 너무 나쁜 상품은 안보내겠지?",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002727.1.8",
          "form": "믿어보겠어 ㅎㅎ",
          "original_form": "믿어보겠어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002727.1.9",
          "form": "어찌 그런 정보를 알고 사과를 득템했디야 ㅋㅋ",
          "original_form": "어찌 그런 정보를 알고 사과를 득템했디야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002727.1.10",
          "form": "안그래도 과일 다 떨어져 가는데 잘됐당 ㅎㅎ",
          "original_form": "안그래도 과일 다 떨어져 가는데 잘됐당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002727.1.11",
          "form": "아주 알뜰해 굿이얌",
          "original_form": "아주 알뜰해 굿이얌",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002727.1.12",
          "form": "ㅎㅎ 쌩유",
          "original_form": "ㅎㅎ 쌩유",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002727.1.13",
          "form": "사과 맛있으면 신랑도 가입해",
          "original_form": "사과 맛있으면 신랑도 가입해",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002727.1.14",
          "form": "ㅇㅇ 그래야겠다",
          "original_form": "ㅇㅇ 그래야겠다",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002727.1.15",
          "form": "두배로 득템 ㅋㅋ",
          "original_form": "두배로 득템 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002727.1.16",
          "form": "그렇쥐 ㅎㅎ 두배",
          "original_form": "그렇쥐 ㅎㅎ 두배",
