      "utterance": [
        {
          "id": "MDRW2100007844.1.1",
          "form": "오늘 비가 올까요",
          "original_form": "오늘 비가 올까요",
          "speaker_id": "1",
          "time": "20210822 11:16"
        },
        {
          "id": "MDRW2100007844.1.2",
          "form": "오늘은 안올것 같은데",
          "original_form": "오늘은 안올것 같은데",
          "speaker_id": "2",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007844.1.3",
          "form": "뭔가 공기에 습기가 있는것 같아",
          "original_form": "뭔가 공기에 습기가 있는것 같아",
          "speaker_id": "2",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007844.1.4",
          "form": "비만 안오면 되여...",
          "original_form": "비만 안오면 되여...",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007844.1.5",
          "form": "비오면 address 역으로 가야하니깤ㅋㅋ",
          "original_form": "비오면 &address& 역으로 가야하니깤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007844.1.6",
          "form": "물어본거",
          "original_form": "물어본거",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007844.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007844.1.8",
          "form": "작은 우산 챙겨가",
          "original_form": "작은 우산 챙겨가",
          "speaker_id": "2",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007844.1.9",
          "form": "비 안온다면서 우산 필요?",
          "original_form": "비 안온다면서 우산 필요?",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007844.1.10",
          "form": "어.....",
          "original_form": "어.....",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007844.1.11",
          "form": "기상예보가 바뀜",
          "original_form": "기상예보가 바뀜",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007844.1.12",
          "form": "2-3시에 비온다고 되어있는뎅",
          "original_form": "2-3시에 비온다고 되어있는뎅",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007844.1.13",
          "form": "어... ? 기상예보는 왜 만날 바껴 ㅠㅠㅠ",
          "original_form": "어... ? 기상예보는 왜 만날 바껴 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007844.1.14",
          "form": "그 시간에만 비 오면 안가져갈래.. 그때는 어짜피 실내인걸",
          "original_form": "그 시간에만 비 오면 안가져갈래.. 그때는 어짜피 실내인걸",
          "speaker_id": "1",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100007844.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100007844.1.16",
          "form": "기상청은 실시간예보임 ㅋㅋㅋㅋ",
          "original_form": "기상청은 실시간예보임 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100007844.1.17",
          "form": "실시간이면 그게 어떻게 예보임",
          "original_form": "실시간이면 그게 어떻게 예보임",
          "speaker_id": "1",
          "time": "20210822 11:22"
        },
        {
          "id": "MDRW2100007844.1.18",
          "form": "조오오오오금 뒤를 예보해쥬니까 ㅋㅋㅋ",
          "original_form": "조오오오오금 뒤를 예보해쥬니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007844.1.19",
          "form": "ㅋㅋㅋㅋㅋ어이없내..",
          "original_form": "ㅋㅋㅋㅋㅋ어이없내..",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007844.1.20",
          "form": "태풍도 온데",
          "original_form": "태풍도 온데",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007844.1.21",
          "form": "아니 지금 장마야?",
          "original_form": "아니 지금 장마야?",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007844.1.22",
          "form": "언제???",
          "original_form": "언제???",
          "speaker_id": "2",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007844.1.23",
          "form": "장마는 끝났지",
          "original_form": "장마는 끝났지",
          "speaker_id": "2",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007844.1.24",
          "form": "장마는 6-7월에 오는게 장마지",
          "original_form": "장마는 6-7월에 오는게 장마지",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.25",
          "form": "지금 다가오는 중이라는데?",
          "original_form": "지금 다가오는 중이라는데?",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.26",
          "form": "올해는 마른장마.....",
          "original_form": "올해는 마른장마.....",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.27",
          "form": "서울쪽으로 온다 했는뎅...",
          "original_form": "서울쪽으로 온다 했는뎅...",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.28",
          "form": "가을장마인가",
          "original_form": "가을장마인가",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.29",
          "form": "어디 들어본것 같기도 하고",
          "original_form": "어디 들어본것 같기도 하고",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.30",
          "form": "비오는거 귀찮",
          "original_form": "비오는거 귀찮",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.31",
          "form": "어제 뉴스에 나옴",
          "original_form": "어제 뉴스에 나옴",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.32",
          "form": "비오는거 너무 싫어",
          "original_form": "비오는거 너무 싫어",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.33",
          "form": "요즘 날씨 안되겠네",
          "original_form": "요즘 날씨 안되겠네",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.34",
          "form": "너무 자기들 맘이네",
          "original_form": "너무 자기들 맘이네",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007844.1.35",
          "form": "출근하는 날에는 비오먄 안됨",
          "original_form": "출근하는 날에는 비오먄 안됨",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.36",
          "form": "어제도 나갔다 집 오니까 비 그치고...",
          "original_form": "어제도 나갔다 집 오니까 비 그치고...",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.37",
          "form": "화가난다",
          "original_form": "화가난다",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.38",
          "form": "어제 너무....",
          "original_form": "어제 너무....",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.39",
          "form": "나갈때 비가 엄청 오고....",
          "original_form": "나갈때 비가 엄청 오고....",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.40",
          "form": "들어오니까 해가 쨍쨍이고....",
          "original_form": "들어오니까 해가 쨍쨍이고....",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.41",
          "form": "그러니까..비 많이와서 우산의미없었는데",
          "original_form": "그러니까..비 많이와서 우산의미없었는데",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.42",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007844.1.43",
          "form": "이제 추워지나? 긴팔입는건 아직 이른가?",
          "original_form": "이제 추워지나? 긴팔입는건 아직 이른가?",
          "speaker_id": "1",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007844.1.44",
          "form": "어제 머리만 빼고 많이 맞았지",
          "original_form": "어제 머리만 빼고 많이 맞았지",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007844.1.45",
          "form": "가디건 어떠심?",
          "original_form": "가디건 어떠심?",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007844.1.46",
          "form": "안입으면 들고다니기 귀찬ㄹ고",
          "original_form": "안입으면 들고다니기 귀찬ㄹ고",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007844.1.47",
          "form": "얇은 가디건이 없어",
          "original_form": "얇은 가디건이 없어",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007844.1.48",
          "form": "그럴수가",
          "original_form": "그럴수가",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007844.1.49",
          "form": "나는 있는데",
          "original_form": "나는 있는데",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007844.1.50",
          "form": "근데 어디있지",
          "original_form": "근데 어디있지",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007844.1.51",
          "form": "가디건 있다고 자랑하는거요?",
          "original_form": "가디건 있다고 자랑하는거요?",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007844.1.52",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007844.1.53",
          "form": "그거 하나 있는데요",
          "original_form": "그거 하나 있는데요",
          "speaker_id": "2",
          "time": "20210822 11:30"
        }
      ]
    }
