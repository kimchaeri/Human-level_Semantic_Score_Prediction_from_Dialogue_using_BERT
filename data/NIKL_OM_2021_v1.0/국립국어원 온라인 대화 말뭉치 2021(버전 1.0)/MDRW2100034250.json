      "utterance": [
        {
          "id": "MDRW2100034250.1.1",
          "form": "좋아하는 음식 마음껏 드시나요?",
          "original_form": "좋아하는 음식 마음껏 드시나요?",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034250.1.2",
          "form": "먹는건 좋아하는데 체중때문에 스트레스는 있죠 ㅠ",
          "original_form": "먹는건 좋아하는데 체중때문에 스트레스는 있죠 ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034250.1.3",
          "form": "저는 맛있는거 먹고싶은데",
          "original_form": "저는 맛있는거 먹고싶은데",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034250.1.4",
          "form": "돈을 못버니까 제맘대로 못먹어서요",
          "original_form": "돈을 못버니까 제맘대로 못먹어서요",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034250.1.5",
          "form": "아 슬프네요 ㅠ 비싼 음식을 좋아하시나요?",
          "original_form": "아 슬프네요 ㅠ 비싼 음식을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100034250.1.6",
          "form": "엄청 비싼거도 아니고 그냥",
          "original_form": "엄청 비싼거도 아니고 그냥",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034250.1.7",
          "form": "좀 좋은 초밥같은거요",
          "original_form": "좀 좋은 초밥같은거요",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034250.1.8",
          "form": "아 그렇죠 초밥은 저렴하진 않죠 ㅠㅠ",
          "original_form": "아 그렇죠 초밥은 저렴하진 않죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100034250.1.9",
          "form": "먹고싶은거 맘대로 못먹는게",
          "original_form": "먹고싶은거 맘대로 못먹는게",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034250.1.10",
          "form": "뭔 인생인가 싶어서 우울해요",
          "original_form": "뭔 인생인가 싶어서 우울해요",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034250.1.11",
          "form": "그러게요 전 과자를 좋아해서..돈에 크게 구애받진 않은데 살이 문제네요",
          "original_form": "그러게요 전 과자를 좋아해서..돈에 크게 구애받진 않은데 살이 문제네요",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100034250.1.12",
          "form": "어떤과자 좋아하세요?",
          "original_form": "어떤과자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100034250.1.13",
          "form": "초코과자요 ㅎㅎ",
          "original_form": "초코과자요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100034250.1.14",
          "form": "맛있겠어요 ㅎㅎ",
          "original_form": "맛있겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100034250.1.15",
          "form": "네 지금도 홈런볼 먹네요...",
          "original_form": "네 지금도 홈런볼 먹네요...",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100034250.1.16",
          "form": "맛있게 드세요 ㅎㅎ",
          "original_form": "맛있게 드세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100034250.1.17",
          "form": "어제 칙촉 티라미수맛 첨으로 먹었는데 맛있었어요",
          "original_form": "어제 칙촉 티라미수맛 첨으로 먹었는데 맛있었어요",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100034250.1.18",
          "form": "앍..맛있겠오요 못먹어봤어요",
          "original_form": "앍..맛있겠오요 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        }
