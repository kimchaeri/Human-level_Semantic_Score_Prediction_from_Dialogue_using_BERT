      "utterance": [
        {
          "id": "MDRW2100023144.1.1",
          "form": "보통?가자 하면 가고 없으면 안가고",
          "original_form": "보통?가자 하면 가고 없으면 안가고",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100023144.1.2",
          "form": "싫어하진 않고?",
          "original_form": "싫어하진 않고?",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100023144.1.3",
          "form": "응 싫진 않아",
          "original_form": "응 싫진 않아",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100023144.1.4",
          "form": "산에 한번 가도 좋을 거 같아서",
          "original_form": "산에 한번 가도 좋을 거 같아서",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023144.1.5",
          "form": "가면 좋지",
          "original_form": "가면 좋지",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023144.1.6",
          "form": "인왕산도 가봤었는데 좋더라",
          "original_form": "인왕산도 가봤었는데 좋더라",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023144.1.7",
          "form": "인왕산이 어디에 있어?",
          "original_form": "인왕산이 어디에 있어?",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023144.1.8",
          "form": "서울에 있어",
          "original_form": "서울에 있어",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023144.1.9",
          "form": "나중에 한번 가봐야겠다",
          "original_form": "나중에 한번 가봐야겠다",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023144.1.10",
          "form": "응 낮아서 좋더라",
          "original_form": "응 낮아서 좋더라",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023144.1.11",
          "form": "별로 안힘들어?",
          "original_form": "별로 안힘들어?",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023144.1.12",
          "form": "힘들긴 한데 엄청 빡세게 힘들지는 않어",
          "original_form": "힘들긴 한데 엄청 빡세게 힘들지는 않어",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023144.1.13",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023144.1.14",
