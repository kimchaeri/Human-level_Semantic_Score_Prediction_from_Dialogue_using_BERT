      "utterance": [
        {
          "id": "MDRW2100024910.1.1",
          "form": "로제떡볶이 좋아ㅋㅋㅋ",
          "original_form": "로제떡볶이 좋아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100024910.1.2",
          "form": "칼로리 보고 기절할뻔ㅠㅠ",
          "original_form": "칼로리 보고 기절할뻔ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100024910.1.3",
          "form": "맛있는건칼로리가높지 ㅠㅠㅠ",
          "original_form": "맛있는건칼로리가높지 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100024910.1.4",
          "form": "중국당면칼로리 ㅎㄷㄷ",
          "original_form": "중국당면칼로리 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100024910.1.5",
          "form": "ㅋㅋㅋㄱ당면꼭추가하는데,칼로리높아?ㅋㅋ",
          "original_form": "ㅋㅋㅋㄱ당면꼭추가하는데,칼로리높아?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100024910.1.6",
          "form": "장난아니래ㅋㅋ",
          "original_form": "장난아니래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100024910.1.7",
          "form": "인터넷검색해봐ㅋㅋㅋ",
          "original_form": "인터넷검색해봐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100024910.1.8",
          "form": "모르고있을래ㅋㅋ",
          "original_form": "모르고있을래ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100024910.1.9",
          "form": "모르면0칸로리ㅋㅋㅋ",
          "original_form": "모르면0칸로리ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100024910.1.10",
          "form": "맛있게 먹어도",
