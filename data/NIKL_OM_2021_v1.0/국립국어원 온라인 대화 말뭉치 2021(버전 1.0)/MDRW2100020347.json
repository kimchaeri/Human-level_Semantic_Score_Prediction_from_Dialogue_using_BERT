      "utterance": [
        {
          "id": "MDRW2100020347.1.1",
          "form": "네 공홈전용으로 알고잇어요 그리고 좀 늦게나온대요",
          "original_form": "네 공홈전용으로 알고잇어요 그리고 좀 늦게나온대요",
          "speaker_id": "2",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100020347.1.2",
          "form": "그런데 힌지부분이 까만색이더라구요",
          "original_form": "그런데 힌지부분이 까만색이더라구요",
          "speaker_id": "2",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020347.1.3",
          "form": "저는 그게 별로라서 기존 네가지 색상중에 골랏어요",
          "original_form": "저는 그게 별로라서 기존 네가지 색상중에 골랏어요",
          "speaker_id": "2",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020347.1.4",
          "form": "어 맞아요!!! 아그걸 왜 그렇게 만들었나몰라",
          "original_form": "어 맞아요!!! 아그걸 왜 그렇게 만들었나몰라",
          "speaker_id": "1",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020347.1.5",
          "form": "그거만 색깔 잘 맞췄어도 유입 더 많았을듯해요",
          "original_form": "그거만 색깔 잘 맞췄어도 유입 더 많았을듯해요",
          "speaker_id": "1",
          "time": "20210828 01:51"
        },
        {
          "id": "MDRW2100020347.1.6",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:51"
        },
        {
          "id": "MDRW2100020347.1.7",
          "form": "힌지만 까만색이니까 너무 튀더라구요",
          "original_form": "힌지만 까만색이니까 너무 튀더라구요",
          "speaker_id": "2",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020347.1.8",
          "form": "그 좀 냉장고스럽죠?",
          "original_form": "그 좀 냉장고스럽죠?",
          "speaker_id": "1",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020347.1.9",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020347.1.10",
          "form": "참 매번 안타까운......",
          "original_form": "참 매번 안타까운......",
          "speaker_id": "1",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020347.1.11",
          "form": "기본 색상 잘 뽑아내는데 모든힘을 다썼나봐요 ㅋㅋㅋㅋ",
          "original_form": "기본 색상 잘 뽑아내는데 모든힘을 다썼나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:53"
        },
        {
          "id": "MDRW2100020347.1.12",
          "form": "윗뚜껑에 움짤나오게 만든거도",
          "original_form": "윗뚜껑에 움짤나오게 만든거도",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020347.1.13",
          "form": "대단하다고 느꼈어요",
          "original_form": "대단하다고 느꼈어요",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020347.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020347.1.15",
          "form": "아직 플립 오지도 않앗는데",
          "original_form": "아직 플립 오지도 않앗는데",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020347.1.16",
          "form": "움짤 뭘로 할지 고민중 ㅋㅋㅋㅋㅋ",
          "original_form": "움짤 뭘로 할지 고민중 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020347.1.17",
          "form": "아ㅠㅠ 재밌겠다",
          "original_form": "아ㅠㅠ 재밌겠다",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020347.1.18",
          "form": "저는 전자기기 살때가 젤 행복해요",
          "original_form": "저는 전자기기 살때가 젤 행복해요",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020347.1.19",
          "form": "아 전자기기 원래 좋아하시는구나",
          "original_form": "아 전자기기 원래 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020347.1.20",
          "form": "저는 자주 사는 편은 아니에요 ㅋㅋㅋㅋ",
          "original_form": "저는 자주 사는 편은 아니에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020347.1.21",
          "form": "넵 그래서 이거저거 관심이 많아요ㅋㅋㅋ",
          "original_form": "넵 그래서 이거저거 관심이 많아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100020347.1.22",
          "form": "이전 폰은 뭐쓰셨어요?",
          "original_form": "이전 폰은 뭐쓰셨어요?",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100020347.1.23",
          "form": "노트8이요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "노트8이요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100020347.1.24",
          "form": "와 진짜 오래쓰셨네요! 바꿀때가 되셔서 바꾸신거구나ㅋㅋㅋㅋㅋ",
          "original_form": "와 진짜 오래쓰셨네요! 바꿀때가 되셔서 바꾸신거구나ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100020347.1.25",
          "form": "한번 바꾸면 오래쓰시나봐요!",
          "original_form": "한번 바꾸면 오래쓰시나봐요!",
          "speaker_id": "1",
