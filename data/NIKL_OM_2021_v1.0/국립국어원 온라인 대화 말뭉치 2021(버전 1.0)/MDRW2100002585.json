        },
        {
          "id": "MDRW2100002585.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002585.1.3",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002585.1.4",
          "form": "구매하신거있으신가요?",
          "original_form": "구매하신거있으신가요?",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002585.1.5",
          "form": "여름 투피스샀어요 어제 택배로 ㅎㅎ받았는데 괜찮더라고요",
          "original_form": "여름 투피스샀어요 어제 택배로 ㅎㅎ받았는데 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002585.1.6",
          "form": "오 좋네요ㅋㅋㅋ저는",
          "original_form": "오 좋네요ㅋㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002585.1.7",
          "form": "가을옷사고싶더라구요ㅠ",
          "original_form": "가을옷사고싶더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002585.1.8",
          "form": "벌써 겨울도  나오던데 빨리 사야할꺼예요",
          "original_form": "벌써 겨울도  나오던데 빨리 사야할꺼예요",
          "speaker_id": "2",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002585.1.9",
          "form": "참시간이 빠른거같아요ㅋㅋㅋ",
          "original_form": "참시간이 빠른거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002585.1.10",
          "form": "저는요즘 플립사고싶어요ㅠ",
          "original_form": "저는요즘 플립사고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002585.1.11",
          "form": "플립요 좋지요 전 월동준비하고 싶네요 온수매트 고상나서",
          "original_form": "플립요 좋지요 전 월동준비하고 싶네요 온수매트 고상나서",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002585.1.12",
          "form": "월동준비좋네요ㅋㅋㅋ",
          "original_form": "월동준비좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002585.1.13",
          "form": "롱패딩 갑자기 뜨신거사고싶어지네요",
          "original_form": "롱패딩 갑자기 뜨신거사고싶어지네요",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002585.1.14",
          "form": "양털조끼요",
          "original_form": "양털조끼요",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002585.1.15",
          "form": "오 이름만들어도포근하네요ㅎㅎㅎ",
          "original_form": "오 이름만들어도포근하네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002585.1.16",
          "form": "원피스에 걸쳐 입고 싶어요",
          "original_form": "원피스에 걸쳐 입고 싶어요",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002585.1.17",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002585.1.18",
          "form": "옷 구경가고싶네요",
          "original_form": "옷 구경가고싶네요",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002585.1.19",
          "form": "옷사러간지 2년은 된거 같아요",
          "original_form": "옷사러간지 2년은 된거 같아요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        }
