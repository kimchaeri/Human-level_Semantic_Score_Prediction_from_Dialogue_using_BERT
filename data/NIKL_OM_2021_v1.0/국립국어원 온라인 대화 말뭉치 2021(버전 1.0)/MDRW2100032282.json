      "utterance": [
        {
          "id": "MDRW2100032282.1.1",
          "form": "7위 아니면 8위 예상합니다ㅋㅋ",
          "original_form": "7위 아니면 8위 예상합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032282.1.2",
          "form": "선발 몇 명 빼고는 투수진이 다 붕괴됐어요",
          "original_form": "선발 몇 명 빼고는 투수진이 다 붕괴됐어요",
          "speaker_id": "2",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032282.1.3",
          "form": "기아 롯데 한화는 못 올라올거 같고",
          "original_form": "기아 롯데 한화는 못 올라올거 같고",
          "speaker_id": "1",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032282.1.4",
          "form": "8위는 어려울듯요 ㅋㅋ",
          "original_form": "8위는 어려울듯요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032282.1.5",
          "form": "키움도 선발진 아작 나서",
          "original_form": "키움도 선발진 아작 나서",
          "speaker_id": "1",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032282.1.6",
          "form": "브리검 나쁜 놈ㅠ",
          "original_form": "브리검 나쁜 놈ㅠ",
          "speaker_id": "1",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032282.1.7",
          "form": "어제경기 끝나고 롯데랑 두산 공동7위였어서..",
          "original_form": "어제경기 끝나고 롯데랑 두산 공동7위였어서..",
          "speaker_id": "2",
          "time": "20210905 14:57"
        },
        {
          "id": "MDRW2100032282.1.8",
          "form": "브리검 사정은 알겠는데 미리 연락해줬음 좋았을텐데요. 돈만 더 받아가고",
          "original_form": "브리검 사정은 알겠는데 미리 연락해줬음 좋았을텐데요. 돈만 더 받아가고",
          "speaker_id": "2",
          "time": "20210905 14:57"
        },
        {
          "id": "MDRW2100032282.1.9",
          "form": "헐 7위에요?ㅠ",
          "original_form": "헐 7위에요?ㅠ",
          "speaker_id": "1",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032282.1.10",
          "form": "그니까요 못오면 못온다 말을 해줘야지ㅠㅠ",
          "original_form": "그니까요 못오면 못온다 말을 해줘야지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032282.1.11",
          "form": "젊은 선수들 경험치나 쌓았으면 싶네요ㅎㅎ",
          "original_form": "젊은 선수들 경험치나 쌓았으면 싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032282.1.12",
          "form": "그쵸 애매하게 6위하느니",
          "original_form": "그쵸 애매하게 6위하느니",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032282.1.13",
          "form": "어린애들 키우는게",
          "original_form": "어린애들 키우는게",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032282.1.14",
          "form": "드래프트 순번도 좋구요ㅋㅋ",
          "original_form": "드래프트 순번도 좋구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100032282.1.15",
          "form": "1픽 잘뽑아서 키우는것도 ㅎㅎ",
          "original_form": "1픽 잘뽑아서 키우는것도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100032282.1.16",
          "form": "신인드래프트 얼마 안 남았는데 잘 뽑았으면 좋겠네요 ㅎㅎ",
          "original_form": "신인드래프트 얼마 안 남았는데 잘 뽑았으면 좋겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100032282.1.17",
          "form": "이미 서울권은",
          "original_form": "이미 서울권은",
          "speaker_id": "1",
          "time": "20210906 08:24"
        },
        {
          "id": "MDRW2100032282.1.18",
          "form": "뽑은거 아닌가용",
          "original_form": "뽑은거 아닌가용",
          "speaker_id": "1",
          "time": "20210906 08:24"
        },
        {
          "id": "MDRW2100032282.1.19",
          "form": "1차는 이미 다들 뽑았고 2차는 다음주 예정이에요 ㅎㅎ",
          "original_form": "1차는 이미 다들 뽑았고 2차는 다음주 예정이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032282.1.20",
          "form": "7산 화이팅입니다",
          "original_form": "7산 화이팅입니다",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100032282.1.21",
          "form": "이제 2차군요",
          "original_form": "이제 2차군요",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100032282.1.22",
          "form": "어제 하루 이겼다고 좋네요ㅎㅎ",
          "original_form": "어제 하루 이겼다고 좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:46"
        }
      ]
