      "utterance": [
        {
          "id": "MDRW2100028064.1.1",
          "form": "저지아이즈? 그 게임도 재밌어보여요",
          "original_form": "저지아이즈? 그 게임도 재밌어보여요",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028064.1.2",
          "form": "법보다 주먹을 사용하는 주인공의 리걸 서스펜스 게임이라는 소갠데",
          "original_form": "법보다 주먹을 사용하는 주인공의 리걸 서스펜스 게임이라는 소갠데",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028064.1.3",
          "form": "엄청 신선한데요ㅋㅋ",
          "original_form": "엄청 신선한데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028064.1.4",
          "form": "맞아요 싸우는 겜이라 그런가바요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 싸우는 겜이라 그런가바요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028064.1.5",
          "form": "근데 기무타쿠 모델이라는 ㄷㄷ",
          "original_form": "근데 기무타쿠 모델이라는 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028064.1.6",
          "form": "변호사인데 싸움으로 해결하나요ㅋㅋ",
          "original_form": "변호사인데 싸움으로 해결하나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028064.1.7",
          "form": "변호사하면 역전재판같은게 생각나는데 진짜 신선하네요ㅋㅋ",
          "original_form": "변호사하면 역전재판같은게 생각나는데 진짜 신선하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028064.1.8",
          "form": "오오 역재 재밌는데",
          "original_form": "오오 역재 재밌는데",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028064.1.9",
          "form": "변호사출신",
          "original_form": "변호사출신",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028064.1.10",
          "form": "깡패라던가 ㅋㅋㅋㅋ",
          "original_form": "깡패라던가 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028064.1.11",
          "form": "그랬던듯",
          "original_form": "그랬던듯",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028064.1.12",
          "form": "깡패출신 변호사도 아니고 변호사출신 깡패요? ㅋㅋㅋ",
          "original_form": "깡패출신 변호사도 아니고 변호사출신 깡패요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028064.1.13",
          "form": "그런가바요 ㅋㅋㅋ 보다 말았는데 박탈됐던가",
          "original_form": "그런가바요 ㅋㅋㅋ 보다 말았는데 박탈됐던가",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028064.1.14",
          "form": "설정이 재미있네요 진짜ㅋㅋㅋ",
          "original_form": "설정이 재미있네요 진짜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028064.1.15",
          "form": "검색하니 플스만 나오는데 다른 플랫폼에도 제공이 되나용",
          "original_form": "검색하니 플스만 나오는데 다른 플랫폼에도 제공이 되나용",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028064.1.16",
          "form": "일본 유명배우를 모델로 썻데요 ㅎㅎ",
          "original_form": "일본 유명배우를 모델로 썻데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028064.1.17",
          "form": "그럼 플스만 될것같아요",
          "original_form": "그럼 플스만 될것같아요",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028064.1.18",
          "form": "그나저나 게임 과금관련 구글페이 바뀌는 뉴스 보셨나요",
          "original_form": "그나저나 게임 과금관련 구글페이 바뀌는 뉴스 보셨나요",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028064.1.19",
          "form": "헉 그건 또 어케 바뀐데요?",
          "original_form": "헉 그건 또 어케 바뀐데요?",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028064.1.20",
          "form": "사용자나 겜회사입장에서는 나쁠거 없어요!",
          "original_form": "사용자나 겜회사입장에서는 나쁠거 없어요!",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028064.1.21",
          "form": "이전에는 무조건 구글 플레이 통해서 결제 이루어졌고",
          "original_form": "이전에는 무조건 구글 플레이 통해서 결제 이루어졌고",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028064.1.22",
          "form": "구글이 수수료를 30프로까지 요구했다고 알고있고요",
          "original_form": "구글이 수수료를 30프로까지 요구했다고 알고있고요",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028064.1.23",
          "form": "우리나라에서 구글 갑질 방지법이라고 이번에 새로 법 만들어서 구글 통한 결제 강제 못하도록 했대요",
          "original_form": "우리나라에서 구글 갑질 방지법이라고 이번에 새로 법 만들어서 구글 통한 결제 강제 못하도록 했대요",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028064.1.24",
          "form": "이게 다른나라에서 추진을 못한 일이라 우리나라가 최초래요",
          "original_form": "이게 다른나라에서 추진을 못한 일이라 우리나라가 최초래요",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028064.1.25",
          "form": "그래서 어디 외국 게임사 ceo가 자기 한국인이라고ㅋㅋ 그런 트윗도 올리고 했다던데요",
          "original_form": "그래서 어디 외국 게임사 ceo가 자기 한국인이라고ㅋㅋ 그런 트윗도 올리고 했다던데요",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028064.1.26",
          "form": "아 그거봤어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그거봤어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028064.1.27",
          "form": "한국인이라고 ㅋㅋㅋ",
          "original_form": "한국인이라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028064.1.28",
          "form": "요즘 구글 자동결제 당하는사람 많더라구요 ㄷㄷ",
          "original_form": "요즘 구글 자동결제 당하는사람 많더라구요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100028064.1.29",
          "form": "오잉 당하다니요",
          "original_form": "오잉 당하다니요",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100028064.1.30",
          "form": "무슨 리니지 결제되고 그랫데요",
          "original_form": "무슨 리니지 결제되고 그랫데요",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028064.1.31",
          "form": "2차비번 필수라고 하네요 구글",
          "original_form": "2차비번 필수라고 하네요 구글",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028064.1.32",
          "form": "카드가 등록되었나봐요",
          "original_form": "카드가 등록되었나봐요",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028064.1.33",
          "form": "결제 내역 확인해봐야겠네요",
          "original_form": "결제 내역 확인해봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100028064.1.34",
          "form": "저처럼 과금 자주하고 내역 안살펴보는 사람은 모르고 지나가겠어요",
          "original_form": "저처럼 과금 자주하고 내역 안살펴보는 사람은 모르고 지나가겠어요",
          "speaker_id": "1",
          "time": "20210902 18:05"
        }
      ]
