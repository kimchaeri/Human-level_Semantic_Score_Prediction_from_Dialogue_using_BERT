      "utterance": [
        {
          "id": "MDRW2100011859.1.1",
          "form": "진짜 다들 본능적으로 아는 것 같아요",
          "original_form": "진짜 다들 본능적으로 아는 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.2",
          "form": "그 신부아빠가 신부 신랑한테 전해주는거가",
          "original_form": "그 신부아빠가 신부 신랑한테 전해주는거가",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.3",
          "form": "소유권 이양을 극명하게 드러내주지않나요",
          "original_form": "소유권 이양을 극명하게 드러내주지않나요",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.5",
          "form": "꼭 잘부탁한다",
          "original_form": "꼭 잘부탁한다",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.6",
          "form": "이런말 하잖아요",
          "original_form": "이런말 하잖아요",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.7",
          "form": "마자요!!!",
          "original_form": "마자요!!!",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.8",
          "form": "그럼 신랑은 대답하고",
          "original_form": "그럼 신랑은 대답하고",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011859.1.9",
          "form": "예전부터 그런 의미였지만",
          "original_form": "예전부터 그런 의미였지만",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011859.1.10",
          "form": "지금도 달라지지 않았다는 거겠죠",
          "original_form": "지금도 달라지지 않았다는 거겠죠",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011859.1.11",
          "form": "이렇게보니 결혼식 진짜 기괴하네요",
          "original_form": "이렇게보니 결혼식 진짜 기괴하네요",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011859.1.12",
          "form": "결혼식 문화 진짜 이상한데",
          "original_form": "결혼식 문화 진짜 이상한데",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011859.1.13",
          "form": "왜 다들 눈치못채는지 모르겟어요",
          "original_form": "왜 다들 눈치못채는지 모르겟어요",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011859.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011859.1.15",
          "form": "로맨틱하다고 하고",
          "original_form": "로맨틱하다고 하고",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011859.1.16",
          "form": "예쁘게 꾸미고",
          "original_form": "예쁘게 꾸미고",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011859.1.17",
          "form": "예로부터 잔칫날인건데",
          "original_form": "예로부터 잔칫날인건데",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011859.1.18",
          "form": "저는 제가 상위 0.0001퍼 테라피스트남을만나 결혼한다고 말도안되는 가정을 해도",
          "original_form": "저는 제가 상위 0.0001퍼 테라피스트남을만나 결혼한다고 말도안되는 가정을 해도",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011859.1.19",
          "form": "그런식의 결혼식은 자존심상해서 못할거같거든요",
          "original_form": "그런식의 결혼식은 자존심상해서 못할거같거든요",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011859.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011859.1.21",
          "form": "하나부터 열까지",
          "original_form": "하나부터 열까지",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011859.1.22",
          "form": "이상한데",
          "original_form": "이상한데",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011859.1.23",
          "form": "신부,신랑은 그 날이 최고로 행복하다고",
          "original_form": "신부,신랑은 그 날이 최고로 행복하다고",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011859.1.24",
          "form": "그게 이상해요",
          "original_form": "그게 이상해요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011859.1.25",
          "form": "돈도 많이들고",
          "original_form": "돈도 많이들고",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011859.1.26",
          "form": "신부는 행복하다면서 울고",
