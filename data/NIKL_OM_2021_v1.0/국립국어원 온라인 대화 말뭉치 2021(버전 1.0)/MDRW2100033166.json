      "utterance": [
        {
          "id": "MDRW2100033166.1.1",
          "form": "안녕하세요 name2씨",
          "original_form": "안녕하세요 &name2&씨",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100033166.1.2",
          "form": "저희 반려동물에 대해 대화해보도록해요",
          "original_form": "저희 반려동물에 대해 대화해보도록해요",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100033166.1.3",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100033166.1.4",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100033166.1.5",
          "form": "어떤 동물 좋아하시나요",
          "original_form": "어떤 동물 좋아하시나요",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033166.1.6",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033166.1.7",
          "form": "저희집 강아지 name3이는 간식 다 잘먹는답니다",
          "original_form": "저희집 강아지 &name3&이는 간식 다 잘먹는답니다",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033166.1.8",
          "form": "쩔죠?",
          "original_form": "쩔죠?",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033166.1.9",
          "form": "기특하죠?",
          "original_form": "기특하죠?",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033166.1.10",
          "form": "와 정말 기특하네요",
          "original_form": "와 정말 기특하네요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033166.1.11",
          "form": "동물 집에 들이게 된다면 어떤 동물을 들이고싶으세요",
          "original_form": "동물 집에 들이게 된다면 어떤 동물을 들이고싶으세요",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100033166.1.12",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100033166.1.13",
          "form": "내가 관리 잘 못할거같아서 안들이고싶긴한데",
          "original_form": "내가 관리 잘 못할거같아서 안들이고싶긴한데",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033166.1.14",
          "form": "굳이 키운다면 좀 작은 애들이 좋을듯",
          "original_form": "굳이 키운다면 좀 작은 애들이 좋을듯",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033166.1.15",
          "form": "새나 햄스터같은",
          "original_form": "새나 햄스터같은",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033166.1.16",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033166.1.17",
          "form": "앵무새 키우는 친구가 있었었는데",
          "original_form": "앵무새 키우는 친구가 있었었는데",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033166.1.18",
          "form": "왕관앵무새였나? 그랬었음",
          "original_form": "왕관앵무새였나? 그랬었음",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.19",
          "form": "막 묘기부리는데",
          "original_form": "막 묘기부리는데",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.20",
          "form": "안녕하세요! 이러고",
          "original_form": "안녕하세요! 이러고",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.21",
          "form": "산토끼 노래부르고",
          "original_form": "산토끼 노래부르고",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.22",
          "form": "그러더라",
          "original_form": "그러더라",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.23",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.24",
          "form": "그래서 새가 키우고 싶었었는데",
          "original_form": "그래서 새가 키우고 싶었었는데",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.25",
          "form": "어쩌다보니 강아지가 집에 있네요",
          "original_form": "어쩌다보니 강아지가 집에 있네요",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033166.1.26",
          "form": "지금 무릎 위에서 헥헥거리고 잇숨",
          "original_form": "지금 무릎 위에서 헥헥거리고 잇숨",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033166.1.27",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033166.1.28",
          "form": "강아지는 별로 안좋아해?",
          "original_form": "강아지는 별로 안좋아해?",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100033166.1.29",
          "form": "중간인듯",
          "original_form": "중간인듯",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100033166.1.30",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100033166.1.31",
          "form": "고양이는?",
          "original_form": "고양이는?",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100033166.1.32",
          "form": "고양이도 보통",
          "original_form": "고양이도 보통",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100033166.1.33",
          "form": "근데 오히려 작은 친구들이 관리하기 더 어렵더라구",
          "original_form": "근데 오히려 작은 친구들이 관리하기 더 어렵더라구",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033166.1.34",
          "form": "고슴도치도 키워봤었는데",
          "original_form": "고슴도치도 키워봤었는데",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033166.1.35",
          "form": "고슴도치 목욕시키기 너무 어려웠어",
          "original_form": "고슴도치 목욕시키기 너무 어려웠어",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033166.1.36",
          "form": "ㅋㅋㅋ그렇겠네",
          "original_form": "ㅋㅋㅋ그렇겠네",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033166.1.37",
          "form": "고슴도치 아픈것도 몰랐어서",
          "original_form": "고슴도치 아픈것도 몰랐어서",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100033166.1.38",
          "form": "쬐끄만하니까....",
          "original_form": "쬐끄만하니까....",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100033166.1.39",
          "form": "그래서 어느날 갑자기 죽었었어...........",
          "original_form": "그래서 어느날 갑자기 죽었었어...........",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100033166.1.40",
          "form": "오 저런",
          "original_form": "오 저런",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033166.1.41",
          "form": "반려동물이랑 이별하는게 제일 가슴 찢어지는 일인것같아",
          "original_form": "반려동물이랑 이별하는게 제일 가슴 찢어지는 일인것같아",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033166.1.42",
          "form": "강아지같이 큰 친구들도 아픈거 잘 티가 안나는데...",
          "original_form": "강아지같이 큰 친구들도 아픈거 잘 티가 안나는데...",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033166.1.43",
          "form": "또르륵....",
          "original_form": "또르륵....",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033166.1.44",
          "form": "수명도 짧으니까",
          "original_form": "수명도 짧으니까",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033166.1.45",
          "form": "크흐흑",
          "original_form": "크흐흑",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033166.1.46",
          "form": "귀엽고 쬐끄만 털복숭이 친구들아",
          "original_form": "귀엽고 쬐끄만 털복숭이 친구들아",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033166.1.47",
          "form": "제발 평생 행복하게 오래오래 살아주라",
          "original_form": "제발 평생 행복하게 오래오래 살아주라",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033166.1.48",
          "form": "흐흑",
          "original_form": "흐흑",
          "speaker_id": "1",
          "time": "20210906 18:09"
        }
      ]
    }
