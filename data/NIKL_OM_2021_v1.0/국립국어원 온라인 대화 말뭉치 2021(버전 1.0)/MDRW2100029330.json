      "utterance": [
        {
          "id": "MDRW2100029330.1.1",
          "form": "전 반숙과 완숙 사이요",
          "original_form": "전 반숙과 완숙 사이요",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029330.1.2",
          "form": "노른자가 덜 익으면 싫어요 ?",
          "original_form": "노른자가 덜 익으면 싫어요 ?",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029330.1.3",
          "form": "네 노른자가 반이상은 익어야 좋아해요",
          "original_form": "네 노른자가 반이상은 익어야 좋아해요",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029330.1.4",
          "form": "반투명하고 젤리같고 그럴때 ?",
          "original_form": "반투명하고 젤리같고 그럴때 ?",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029330.1.5",
          "form": "노른자가 주르륵 흐르는 느낌은 별로고 \n반숙에서 살짝 더 익혀진 느낌이 좋아요",
          "original_form": "노른자가 주르륵 흐르는 느낌은 별로고 \n반숙에서 살짝 더 익혀진 느낌이 좋아요",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029330.1.6",
          "form": "감동란 먹어봤어요?",
          "original_form": "감동란 먹어봤어요?",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029330.1.7",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100029330.1.8",
          "form": "그거 같은건가?",
          "original_form": "그거 같은건가?",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100029330.1.9",
          "form": "감동란을 먹어보지 않았으니 알수없네요",
          "original_form": "감동란을 먹어보지 않았으니 알수없네요",
          "speaker_id": "2",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100029330.1.10",
          "form": "저도 취향이 비슷헤서 그래요",
          "original_form": "저도 취향이 비슷헤서 그래요",
          "speaker_id": "1",
          "time": "20210903 01:42"
        },
        {
          "id": "MDRW2100029330.1.11",
          "form": "엇 비슷하다니 신기하네요",
          "original_form": "엇 비슷하다니 신기하네요",
          "speaker_id": "2",
          "time": "20210903 01:42"
        },
        {
          "id": "MDRW2100029330.1.12",
          "form": "요즘엔 집에 계란후라이 장인이 있어서\n잘 구슬려서 부쳐달라해야해요 ㅎ",
          "original_form": "요즘엔 집에 계란후라이 장인이 있어서\n잘 구슬려서 부쳐달라해야해요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:44"
        },
        {
          "id": "MDRW2100029330.1.13",
          "form": "계란후라이를 정말 잘 하나보네요",
          "original_form": "계란후라이를 정말 잘 하나보네요",
          "speaker_id": "2",
