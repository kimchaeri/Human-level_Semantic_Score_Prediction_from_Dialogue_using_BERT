      "utterance": [
        {
          "id": "MDRW2100046848.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211107 21:25"
        },
        {
          "id": "MDRW2100046848.1.2",
          "form": "안녕하세요! 즐거운 주말 보내셨나요",
          "original_form": "안녕하세요! 즐거운 주말 보내셨나요",
          "speaker_id": "2",
          "time": "20211107 21:26"
        },
        {
          "id": "MDRW2100046848.1.3",
          "form": "네 평범한 주말보냇어요",
          "original_form": "네 평범한 주말보냇어요",
          "speaker_id": "1",
          "time": "20211107 21:26"
        },
        {
          "id": "MDRW2100046848.1.4",
          "form": "저는 밀린 드라마를 봤어요 ㅎㅎ",
          "original_form": "저는 밀린 드라마를 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211107 21:29"
        },
        {
          "id": "MDRW2100046848.1.5",
          "form": "전 집에서 하루종일 롤햇어요 ㅎ",
          "original_form": "전 집에서 하루종일 롤햇어요 ㅎ",
          "speaker_id": "1",
          "time": "20211107 21:30"
        },
        {
          "id": "MDRW2100046848.1.6",
          "form": "와 하루종일 게임을!!",
          "original_form": "와 하루종일 게임을!!",
          "speaker_id": "2",
          "time": "20211107 21:30"
        },
        {
          "id": "MDRW2100046848.1.7",
          "form": "대단하시네요.. 게임 잘 하시나요",
          "original_form": "대단하시네요.. 게임 잘 하시나요",
          "speaker_id": "2",
          "time": "20211107 21:31"
        },
        {
          "id": "MDRW2100046848.1.8",
          "form": "그냥 중간정도 해요",
          "original_form": "그냥 중간정도 해요",
          "speaker_id": "1",
          "time": "20211107 21:31"
        },
        {
          "id": "MDRW2100046848.1.9",
          "form": "이겨야재밋죠",
          "original_form": "이겨야재밋죠",
          "speaker_id": "1",
          "time": "20211107 21:31"
        },
        {
          "id": "MDRW2100046848.1.10",
          "form": "ㅋㅋ아 맞아요 게임은 이겨야 재밌죠",
          "original_form": "ㅋㅋ아 맞아요 게임은 이겨야 재밌죠",
          "speaker_id": "2",
          "time": "20211107 21:32"
        },
        {
          "id": "MDRW2100046848.1.11",
          "form": "저는 게임을 잘 못해서.. 안하게 되더라구요",
          "original_form": "저는 게임을 잘 못해서.. 안하게 되더라구요",
          "speaker_id": "2",
          "time": "20211107 21:32"
        },
        {
          "id": "MDRW2100046848.1.12",
          "form": "자기랑 맞는 게임하다보면 늘어요",
          "original_form": "자기랑 맞는 게임하다보면 늘어요",
          "speaker_id": "1",
          "time": "20211107 21:32"
        },
        {
          "id": "MDRW2100046848.1.13",
          "form": "그런가요.. 제가 기껏해야 이길 수 있는 게임은",
          "original_form": "그런가요.. 제가 기껏해야 이길 수 있는 게임은",
          "speaker_id": "2",
          "time": "20211107 21:34"
        },
        {
          "id": "MDRW2100046848.1.14",
          "form": "카트라이더 뿐이네요...",
          "original_form": "카트라이더 뿐이네요...",
          "speaker_id": "2",
          "time": "20211107 21:34"
        },
        {
          "id": "MDRW2100046848.1.15",
          "form": "그런가요 할게없어서 다시게임하러가야겟네요",
          "original_form": "그런가요 할게없어서 다시게임하러가야겟네요",
          "speaker_id": "1",
          "time": "20211107 21:38"
        },
        {
          "id": "MDRW2100046848.1.16",
          "form": "앗 오늘 밤까지 달리시겠네요 ",
          "original_form": "앗 오늘 밤까지 달리시겠네요 ",
