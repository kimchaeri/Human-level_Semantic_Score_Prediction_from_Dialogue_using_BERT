      "utterance": [
        {
          "id": "MDRW2100045044.1.1",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045044.1.2",
          "form": "흑흑 고영...",
          "original_form": "흑흑 고영...",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045044.1.3",
          "form": "행복해야돼 다들",
          "original_form": "행복해야돼 다들",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045044.1.4",
          "form": "특히 하하하ㅔ서 야통이 삼색이 큰일나면 너무슬플거야...",
          "original_form": "특히 하하하ㅔ서 야통이 삼색이 큰일나면 너무슬플거야...",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045044.1.5",
          "form": "그럼 한국 커뮤 다뒤집어진다....",
          "original_form": "그럼 한국 커뮤 다뒤집어진다....",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045044.1.6",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045044.1.7",
          "form": "행복하게 살자 얘들아...",
          "original_form": "행복하게 살자 얘들아...",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.8",
          "form": "무 아팠을때만해도",
          "original_form": "무 아팠을때만해도",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.9",
          "form": "중성화 수술은 꼭 받고...",
          "original_form": "중성화 수술은 꼭 받고...",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.10",
          "form": "난리였는데",
          "original_form": "난리였는데",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.11",
          "form": "무야 아프지마....",
          "original_form": "무야 아프지마....",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.12",
          "form": "아 그러고보니 고양ㅇ 복막염 걸리는 애들은",
          "original_form": "아 그러고보니 고양ㅇ 복막염 걸리는 애들은",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.14",
          "form": "약한개체래",
          "original_form": "약한개체래",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.15",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.16",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.17",
          "form": "나아도 재발가능성있으니",
          "original_form": "나아도 재발가능성있으니",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.18",
          "form": "면역력같은게?",
          "original_form": "면역력같은게?",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.19",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.20",
          "form": "으악...",
          "original_form": "으악...",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045044.1.21",
          "form": "수두바이러스가",
          "original_form": "수두바이러스가",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.22",
          "form": "한번몸에들어오면",
          "original_form": "한번몸에들어오면",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.23",
          "form": "대상포진으로 계속 남아있듯이",
          "original_form": "대상포진으로 계속 남아있듯이",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.24",
          "form": "겁나무섭가",
          "original_form": "겁나무섭가",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.25",
          "form": "다",
          "original_form": "다",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.26",
          "form": "복막염도 그렇다네여",
          "original_form": "복막염도 그렇다네여",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.27",
          "form": "평생관리대상이네...",
          "original_form": "평생관리대상이네...",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.28",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.29",
          "form": "그래서 잠깐 산책은 해도",
          "original_form": "그래서 잠깐 산책은 해도",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.30",
          "form": "완전히 마당냥이는 안될고야",
          "original_form": "완전히 마당냥이는 안될고야",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045044.1.31",
          "form": "지금처럼키워야 할것...",
          "original_form": "지금처럼키워야 할것...",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045044.1.32",
          "form": "래기도 아프지마",
          "original_form": "래기도 아프지마",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045044.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ무는 완전히 건강해진다 하더라도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ무는 완전히 건강해진다 하더라도",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045044.1.34",
          "form": "집밖으로 안나갈거같지만...",
          "original_form": "집밖으로 안나갈거같지만...",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045044.1.35",
          "form": "얘들아 건강하자",
          "original_form": "얘들아 건강하자",
          "speaker_id": "1",
          "time": "20210908 00:42"
        }
      ]
    }
