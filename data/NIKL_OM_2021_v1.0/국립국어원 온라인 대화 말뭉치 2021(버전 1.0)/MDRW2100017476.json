      "utterance": [
        {
          "id": "MDRW2100017476.1.1",
          "form": "운동 즐겨하세요?",
          "original_form": "운동 즐겨하세요?",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017476.1.2",
          "form": "하는 건 좋은데 헬스는 좀 어렵더라구요.",
          "original_form": "하는 건 좋은데 헬스는 좀 어렵더라구요.",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017476.1.3",
          "form": "피티 받는 건 너무 돈이 부담되고 ㅠㅠ",
          "original_form": "피티 받는 건 너무 돈이 부담되고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017476.1.4",
          "form": "아 맞아요 입문자가 하기는 장벽이 좀 있는 거 같아요",
          "original_form": "아 맞아요 입문자가 하기는 장벽이 좀 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100017476.1.5",
          "form": "부상 위험도 있고ㅠㅠ",
          "original_form": "부상 위험도 있고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100017476.1.6",
          "form": "근데 또 기구를 안 쓰면 가서 할게 없어요 ㅋㅋ",
          "original_form": "근데 또 기구를 안 쓰면 가서 할게 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100017476.1.7",
          "form": "그쵸ㅠㅠ 헬스는 기구 쓰러 가니까요ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅠㅠ 헬스는 기구 쓰러 가니까요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100017476.1.8",
          "form": "ㅋㅋㅋㅋㅋ근데 피티는 비싸서.... 차라리 필라테스 이런걸 찾아보게 되더라고요",
          "original_form": "ㅋㅋㅋㅋㅋ근데 피티는 비싸서.... 차라리 필라테스 이런걸 찾아보게 되더라고요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100017476.1.9",
          "form": "현실은 집에서 땅끄부부..",
          "original_form": "현실은 집에서 땅끄부부..",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100017476.1.10",
          "form": "맞아요 저도 필라테스 다녔었어요",
          "original_form": "맞아요 저도 필라테스 다녔었어요",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100017476.1.11",
          "form": "잘 찾아보면 할인하는 곳도 있고",
          "original_form": "잘 찾아보면 할인하는 곳도 있고",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100017476.1.12",
          "form": "저두 지금은 홈트해요ㅋㅋㅋㅋ",
          "original_form": "저두 지금은 홈트해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100017476.1.13",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 요즘은 어디 가서 운동하기도 어렵죠",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 요즘은 어디 가서 운동하기도 어렵죠",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100017476.1.14",
          "form": "나중에 여유되면 헬스 꼭 해보려구요",
          "original_form": "나중에 여유되면 헬스 꼭 해보려구요",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017476.1.15",
          "form": "아니면 동생한테 가르쳐달라고 하려구요ㅋㅋㅋ",
          "original_form": "아니면 동생한테 가르쳐달라고 하려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017476.1.16",
          "form": "와 동생이 운동하나요?",
          "original_form": "와 동생이 운동하나요?",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100017476.1.17",
          "form": "네 동생 운동 꾸준히 하더라구요! 전문적으로 하는 건 아닌데 헬스장 계속 다녀요ㅋㅋㅋ",
          "original_form": "네 동생 운동 꾸준히 하더라구요! 전문적으로 하는 건 아닌데 헬스장 계속 다녀요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100017476.1.18",
          "form": "한 번 운동하면 그 맛이 있긴해요 ㅋㅋㅋㅋ",
          "original_form": "한 번 운동하면 그 맛이 있긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100017476.1.19",
          "form": "저도 복근 만들고 싶어요..",
          "original_form": "저도 복근 만들고 싶어요..",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100017476.1.20",
          "form": "와 복근.. 저두요..",
          "original_form": "와 복근.. 저두요..",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100017476.1.21",
          "form": "아직 살면서 복근 만들어 본 적이 없네요ㅠㅠ",
          "original_form": "아직 살면서 복근 만들어 본 적이 없네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100017476.1.22",
          "form": "저도요 ㅋㅋㅋ 근데 복근은.. 식단 해야할 것 같고 ㅠ",
          "original_form": "저도요 ㅋㅋㅋ 근데 복근은.. 식단 해야할 것 같고 ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100017476.1.23",
          "form": "맞아요.. 배는 진짜 식단이래요..",
          "original_form": "맞아요.. 배는 진짜 식단이래요..",
          "speaker_id": "1",
