        {
          "id": "MDRW2100042881.1.1",
          "form": "위내시경은 그냥 수면으로하니까\r\n꿀잠자고일어난기분 좋더라구욧",
          "original_form": "위내시경은 그냥 수면으로하니까\r\n꿀잠자고일어난기분 좋더라구욧",
          "speaker_id": "1",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042881.1.2",
          "form": "대장내시경 ..약부터진짜..양이양이",
          "original_form": "대장내시경 ..약부터진짜..양이양이",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042881.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042881.1.4",
          "form": "대장내시경",
          "original_form": "대장내시경",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042881.1.5",
          "form": "해보셧어용?",
          "original_form": "해보셧어용?",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042881.1.6",
          "form": "*.*",
          "original_form": "*.*",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042881.1.7",
          "form": "아니요ㅋㅋㅋㅋㄱ저는 위도무서워하는싸람..",
          "original_form": "아니요ㅋㅋㅋㅋㄱ저는 위도무서워하는싸람..{emoji:☆}",
          "speaker_id": "2",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100042881.1.8",
          "form": "어머니하는거옆에서봤는데",
          "original_form": "어머니하는거옆에서봤는데",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100042881.1.9",
          "form": "진짜괴롭겟더라구요..",
          "original_form": "진짜괴롭겟더라구요..",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100042881.1.10",
          "form": "크",
          "original_form": "크",
          "speaker_id": "1",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100042881.1.11",
          "form": "근데 위내시경은",
          "original_form": "근데 위내시경은",
          "speaker_id": "1",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042881.1.12",
          "form": "꿀잠자고일어나는거라서",
          "original_form": "꿀잠자고일어나는거라서",
          "speaker_id": "1",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042881.1.13",
          "form": "음청조아요",
          "original_form": "음청조아요",
          "speaker_id": "1",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042881.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042881.1.15",
          "form": "ㅋㄲㅋㅋㅋㅋ오 엄청좋다니ㅋㅋㅋ",
          "original_form": "ㅋㄲㅋㅋㅋㅋ오 엄청좋다니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042881.1.16",
          "form": "강추하시네요ㅋㅋㅋㅋ",
          "original_form": "강추하시네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042881.1.17",
          "form": "ㅋㅋ진짜 1초만에잠들엇다가",
          "original_form": "ㅋㅋ진짜 1초만에잠들엇다가",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042881.1.18",
          "form": "개운하게 깬 느낌",
          "original_form": "개운하게 깬 느낌",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042881.1.19",
          "form": "그 우유주사 연옌들 문제 많자나유",
          "original_form": "그 우유주사 연옌들 문제 많자나유",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042881.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042881.1.21",
          "form": "근데 그 발목다치게햇던",
          "original_form": "근데 그 발목다치게햇던",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042881.1.22",
          "form": "배드민턴",
          "original_form": "배드민턴",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042881.1.23",
          "form": "재밋나용?",
          "original_form": "재밋나용?",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042881.1.24",
          "form": "안그래도 남친이",
          "original_form": "안그래도 남친이",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042881.1.25",
          "form": "날도시원해지니까",
          "original_form": "날도시원해지니까",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042881.1.26",
          "form": "배드민턴 같이 치자고하던뎅",
          "original_form": "배드민턴 같이 치자고하던뎅",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042881.1.27",
          "form": "제가 한번도 안쳐밧어욤",
          "original_form": "제가 한번도 안쳐밧어욤",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042881.1.28",
          "form": "저는 스피드민턴이라구 공도더가볍구빨리나가고 그런거였는데\r\n배드민턴데이트좋죠ㅋㅋㅋ",
          "original_form": "저는 스피드민턴이라구 공도더가볍구빨리나가고 그런거였는데\r\n배드민턴데이트좋죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042881.1.29",
          "form": "저도예전에남친이랑 내기걸고 배드민턴치구 그랬슴돶ㅋㅋㅋㄱ",
          "original_form": "저도예전에남친이랑 내기걸고 배드민턴치구 그랬슴돶ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042881.1.30",
          "form": "재밌어요 할만해요 쉽구!",
          "original_form": "재밌어요 할만해요 쉽구!",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042881.1.31",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042881.1.32",
          "form": "이제 시원해지니까",
          "original_form": "이제 시원해지니까",
          "speaker_id": "1",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042881.1.33",
          "form": "몸뚱이를 슬 움직여봐야겟어욤",
          "original_form": "몸뚱이를 슬 움직여봐야겟어욤",
          "speaker_id": "1",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042881.1.34",
          "form": "네네 근데 배드민턴 좀 만 쳐도 땀쟁이가 될거예요..",
          "original_form": "네네 근데 배드민턴 좀 만 쳐도 땀쟁이가 될거예요..",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042881.1.35",
          "form": "ㅋㅋㅋㅈㅋ",
          "original_form": "ㅋㅋㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042881.1.36",
          "form": "어후",
          "original_form": "어후",
          "speaker_id": "1",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042881.1.37",
          "form": "당연하져ㅋㅋㅋㅋ",
          "original_form": "당연하져ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042881.1.38",
          "form": "저는 아직도 여름옷차림인 땀쟁이에요",
          "original_form": "저는 아직도 여름옷차림인 땀쟁이에요",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042881.1.39",
          "form": "ㅋㅋㅋ저도..ㅋㅋㅋㅋㅋ좀시원한밤에치면 그나마낫겠지만..그래두..;-;",
          "original_form": "ㅋㅋㅋ저도..ㅋㅋㅋㅋㅋ좀시원한밤에치면 그나마낫겠지만..그래두..;-;",
          "speaker_id": "2",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100042881.1.40",
          "form": "ㅋㅋㅋ배드민턴 생각해바야겟어유",
