      "utterance": [
        {
          "id": "MDRW2100016590.1.1",
          "form": "해외여행 엄청 가고싶네요ㅠ",
          "original_form": "해외여행 엄청 가고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016590.1.2",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016590.1.3",
          "form": "원래 못가게 하거나 못가면 더가고 싶어요",
          "original_form": "원래 못가게 하거나 못가면 더가고 싶어요",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016590.1.4",
          "form": "진짜 너무 가고싶슴다",
          "original_form": "진짜 너무 가고싶슴다",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016590.1.5",
          "form": "신행도 대충 가서ㅠㅠ",
          "original_form": "신행도 대충 가서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016590.1.6",
          "form": "아 그러셨어요?그럼 더 가고싶죠",
          "original_form": "아 그러셨어요?그럼 더 가고싶죠",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016590.1.7",
          "form": "대부분 신행을 좋음곳 가려고 하잖아요 언제 가냐며",
          "original_form": "대부분 신행을 좋음곳 가려고 하잖아요 언제 가냐며",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016590.1.8",
          "form": "아쉬우시겠어요",
          "original_form": "아쉬우시겠어요",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016590.1.9",
          "form": "코로나만 끝나면 바로 달립니다",
          "original_form": "코로나만 끝나면 바로 달립니다",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016590.1.10",
          "form": "미리 돈을 부어 놀까봐요",
          "original_form": "미리 돈을 부어 놀까봐요",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016590.1.11",
          "form": "여행적금 많던데",
          "original_form": "여행적금 많던데",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016590.1.12",
          "form": "그럼 그가보며 위로 되지 않을까요 ㅜㅜ",
          "original_form": "그럼 그가보며 위로 되지 않을까요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016590.1.13",
          "form": "어디 가고 싶으새요?",
          "original_form": "어디 가고 싶으새요?",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016590.1.14",
          "form": "해외 어디든지요ㅠㅠ",
          "original_form": "해외 어디든지요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016590.1.15",
          "form": "네 어디든가면 장땡이죠 ㅜㅜ",
          "original_form": "네 어디든가면 장땡이죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016590.1.16",
          "form": "근데 이제 애 가질거라서 가능하려나",
          "original_form": "근데 이제 애 가질거라서 가능하려나",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016590.1.17",
          "form": "모르겠어요ㅠㅠ",
          "original_form": "모르겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016590.1.18",
          "form": "음 애낳고 잠시 맡기고 다녀오세요",
          "original_form": "음 애낳고 잠시 맡기고 다녀오세요",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100016590.1.19",
          "form": "제2의 신혼여행으로 많이 가던데요",
          "original_form": "제2의 신혼여행으로 많이 가던데요",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100016590.1.20",
          "form": "일찍 코로나 사라지진 않을것 같아요",
          "original_form": "일찍 코로나 사라지진 않을것 같아요",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100016590.1.21",
          "form": "충분히 슬프지만 아기 좀 걸을때 갈수 있지 싶어요",
          "original_form": "충분히 슬프지만 아기 좀 걸을때 갈수 있지 싶어요",
          "speaker_id": "1",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016590.1.22",
          "form": "그때까지 왕창 돈을모아서 한방에 좋은곳 가세요",
          "original_form": "그때까지 왕창 돈을모아서 한방에 좋은곳 가세요",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100016590.1.23",
          "form": "네 그래야겠습니다. . .",
          "original_form": "네 그래야겠습니다. . .",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100016590.1.24",
          "form": "해외 가고 싶으신가여",
          "original_form": "해외 가고 싶으신가여",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100016590.1.25",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016590.1.26",
          "form": "대만이요",
          "original_form": "대만이요",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016590.1.27",
          "form": "대만 좋았는데",
          "original_form": "대만 좋았는데",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016590.1.28",
          "form": "제가 비행기 타고 뭔곳은 자신 없어서요",
          "original_form": "제가 비행기 타고 뭔곳은 자신 없어서요",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016590.1.29",
          "form": "가오슝 갔었어요ㅋㅋ",
          "original_form": "가오슝 갔었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016590.1.30",
          "form": "코로나 전에 자유여행갔었는데",
          "original_form": "코로나 전에 자유여행갔었는데",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100016590.1.31",
          "form": "또 가고싶어요",
          "original_form": "또 가고싶어요",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100016590.1.32",
          "form": "오 가셨군요 ㅎㅎ",
          "original_form": "오 가셨군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100016590.1.33",
          "form": "특가떠서 ㅋㅋ급하게 갔었어요",
          "original_form": "특가떠서 ㅋㅋ급하게 갔었어요",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100016590.1.34",
          "form": "저희는 보면 별계획없이 후딱 가요",
          "original_form": "저희는 보면 별계획없이 후딱 가요",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100016590.1.35",
          "form": "그때 예스진지 버스 투어 하루 만 얼마였는데 그거",
          "original_form": "그때 예스진지 버스 투어 하루 만 얼마였는데 그거",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100016590.1.36",
          "form": "급하게 예약해서 하루",
          "original_form": "급하게 예약해서 하루",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100016590.1.37",
          "form": "버스투어도 했어요",
          "original_form": "버스투어도 했어요",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100016590.1.38",
          "form": "저도 특가 뜨면 슝 주의자에요 ㅋㅋ",
          "original_form": "저도 특가 뜨면 슝 주의자에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100016590.1.39",
          "form": "어 싸다 휴가 고고",
          "original_form": "어 싸다 휴가 고고",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100016590.1.40",
          "form": "네 맞아요 어치피 그렇게 안가면 못가요 망설여서",
          "original_form": "네 맞아요 어치피 그렇게 안가면 못가요 망설여서",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100016590.1.41",
          "form": "ㅋㅋ가족끼리 가셨어요?",
          "original_form": "ㅋㅋ가족끼리 가셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100016590.1.42",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016590.1.43",
          "form": "가족끼리",
          "original_form": "가족끼리",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016590.1.44",
          "form": "같어요",
          "original_form": "같어요",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016590.1.45",
          "form": "누구랑 가셨어요?",
          "original_form": "누구랑 가셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016590.1.46",
          "form": "저 최근 4번은 와이프랑요 ㅎㅎ",
          "original_form": "저 최근 4번은 와이프랑요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100016590.1.47",
          "form": "아 그렇죠 ㅎㅎ",
          "original_form": "아 그렇죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100016590.1.48",
          "form": "4번이나 가셨어요",
          "original_form": "4번이나 가셨어요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100016590.1.49",
          "form": "많이 가셨네요",
          "original_form": "많이 가셨네요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100016590.1.50",
          "form": "그러니 더 가고싶으시겠어요",
          "original_form": "그러니 더 가고싶으시겠어요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        }
      ]
