      "utterance": [
        {
          "id": "MDRW2100001710.1.1",
          "form": "그 앵무새..",
          "original_form": "그 앵무새..",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001710.1.2",
          "form": "김루이 나오는 채널 아나?",
          "original_form": "김루이 나오는 채널 아나?",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001710.1.3",
          "form": "아아 근데 name3아닌가?",
          "original_form": "아아 근데 &name3&아닌가?",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001710.1.4",
          "form": "name3는..우리집샌데",
          "original_form": "&name3&는..우리집샌데",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001710.1.5",
          "form": "김루이다.",
          "original_form": "김루이다.",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001710.1.6",
          "form": "아ㅠㅠ아니 첨들어본다",
          "original_form": "아ㅠㅠ아니 첨들어본다",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001710.1.7",
          "form": "역시 새집사라고",
          "original_form": "역시 새집사라고",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.8",
          "form": "루몽다로 채널이지싶다",
          "original_form": "루몽다로 채널이지싶다",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.9",
          "form": "새 유투버 보는가봐",
          "original_form": "새 유투버 보는가봐",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.10",
          "form": "새 한번보면 계속 새만 나오는 거 알제",
          "original_form": "새 한번보면 계속 새만 나오는 거 알제",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ알지알지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ알지알지",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.12",
          "form": "나 근데 원래 동물영상 많이 봐서",
          "original_form": "나 근데 원래 동물영상 많이 봐서",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.13",
          "form": "그래도 봐도봐도 귀엽잖아",
          "original_form": "그래도 봐도봐도 귀엽잖아",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.14",
          "form": "동물 구조하는 영상도 많이 뜬다",
          "original_form": "동물 구조하는 영상도 많이 뜬다",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001710.1.15",
          "form": "봐도봐도 귀엽지",
          "original_form": "봐도봐도 귀엽지",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001710.1.16",
          "form": "지금 내 옆에 우리 앵무새있는데",
          "original_form": "지금 내 옆에 우리 앵무새있는데",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001710.1.17",
          "form": "계속 자기도 폰 만지려고한다",
          "original_form": "계속 자기도 폰 만지려고한다",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001710.1.18",
          "form": "너네 앵무새는 몇살이야",
          "original_form": "너네 앵무새는 몇살이야",
          "speaker_id": "1",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001710.1.19",
          "form": "이제 5개월 넘었다",
          "original_form": "이제 5개월 넘었다",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001710.1.20",
          "form": "7개월부터 성조래",
          "original_form": "7개월부터 성조래",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001710.1.21",
          "form": "오늘 나 정말 귀여운 동물을 봤어",
          "original_form": "오늘 나 정말 귀여운 동물을 봤어",
