      "utterance": [
        {
          "id": "MDRW2100043276.1.1",
          "form": "디피봐야겠다 그러면",
          "original_form": "디피봐야겠다 그러면",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100043276.1.2",
          "form": "디피재밌어",
          "original_form": "디피재밌어",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.3",
          "form": "나와같이 구교환에 빠지자",
          "original_form": "나와같이 구교환에 빠지자",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.4",
          "form": "어 나안그래도 걔 좋아해",
          "original_form": "어 나안그래도 걔 좋아해",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.5",
          "form": "반도에서도 매력있어어",
          "original_form": "반도에서도 매력있어어",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무웃겨",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.7",
          "form": "마자 매력있어",
          "original_form": "마자 매력있어",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.8",
          "form": "모가디슈봄? 거기도 나와",
          "original_form": "모가디슈봄? 거기도 나와",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.9",
          "form": "말투 거기서도그런가",
          "original_form": "말투 거기서도그런가",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.10",
          "form": "아니안봤어",
          "original_form": "아니안봤어",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.11",
          "form": "말투 독특하긴해",
          "original_form": "말투 독특하긴해",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043276.1.12",
          "form": "쪼가있나보네",
          "original_form": "쪼가있나보네",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.13",
          "form": "근데 진짜 20대같아 깜놀했다굿...",
          "original_form": "근데 진짜 20대같아 깜놀했다굿...",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.14",
          "form": "40살이라고?",
          "original_form": "40살이라고?",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.15",
          "form": "목소리가 특이해서그런거같아",
          "original_form": "목소리가 특이해서그런거같아",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.16",
          "form": "네..ㅎㅎ",
          "original_form": "네..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.17",
          "form": "8년사귄여친까지 힙해...",
          "original_form": "8년사귄여친까지 힙해...",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.18",
          "form": "그래좋아오늘디피본다",
          "original_form": "그래좋아오늘디피본다",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.19",
          "form": "조았어",
          "original_form": "조았어",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.20",
          "form": "안돼헤어져",
          "original_form": "안돼헤어져",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043276.1.21",
          "form": "한명영업했따..희희...",
          "original_form": "한명영업했따..희희...",
