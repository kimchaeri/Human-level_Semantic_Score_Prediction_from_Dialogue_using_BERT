      "utterance": [
        {
          "id": "MDRW2100034942.1.1",
          "form": "저녁 맛있게 드셨나요?",
          "original_form": "저녁 맛있게 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100034942.1.2",
          "form": "네 집밥먹었어요",
          "original_form": "네 집밥먹었어요",
          "speaker_id": "2",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100034942.1.3",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100034942.1.4",
          "form": "부대찌개 해먹었어요",
          "original_form": "부대찌개 해먹었어요",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100034942.1.5",
          "form": "배달음식 자주 드시나요?",
          "original_form": "배달음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100034942.1.6",
          "form": "네. 아무래도 편해서요ㅎㅎ",
          "original_form": "네. 아무래도 편해서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100034942.1.7",
          "form": "얼마나 자주 드세요?",
          "original_form": "얼마나 자주 드세요?",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100034942.1.8",
          "form": "주말엔 꼭 시켜먹는 것 같아요",
          "original_form": "주말엔 꼭 시켜먹는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100034942.1.9",
          "form": "어떤 메뉴 즐겨드세요?",
          "original_form": "어떤 메뉴 즐겨드세요?",
          "speaker_id": "1",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100034942.1.10",
          "form": "보통 치킨을 가장 많이 사먹어요",
          "original_form": "보통 치킨을 가장 많이 사먹어요",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100034942.1.11",
          "form": "어디 브랜드 좋아하세요?",
          "original_form": "어디 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034942.1.12",
          "form": "노랑통닭이요",
          "original_form": "노랑통닭이요",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034942.1.13",
          "form": "요새는 여기서 자주 먹어요",
          "original_form": "요새는 여기서 자주 먹어요",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034942.1.14",
          "form": "노랑통닭은 깐풍치킨이 맛있었는데 요즘은 뭐가 맛있어요?",
          "original_form": "노랑통닭은 깐풍치킨이 맛있었는데 요즘은 뭐가 맛있어요?",
          "speaker_id": "1",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100034942.1.15",
          "form": "전 후라이드가 맛있어서요ㅋㅋ 양념이랑 깐풍이랑 같이 오는 세트도 좋더라구요",
          "original_form": "전 후라이드가 맛있어서요ㅋㅋ 양념이랑 깐풍이랑 같이 오는 세트도 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100034942.1.16",
          "form": "후라이드는 카레향이 나는것이 매력인거같아요",
