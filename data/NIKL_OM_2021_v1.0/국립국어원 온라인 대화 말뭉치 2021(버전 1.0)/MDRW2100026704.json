      "utterance": [
        {
          "id": "MDRW2100026704.1.1",
          "form": "혹시 동물을 키우시나요?",
          "original_form": "혹시 동물을 키우시나요?",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026704.1.2",
          "form": "사슴벌레 키워요 ㅋ",
          "original_form": "사슴벌레 키워요 ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026704.1.3",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026704.1.4",
          "form": "저는 고양이와 열대어를 키웁니다",
          "original_form": "저는 고양이와 열대어를 키웁니다",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026704.1.5",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026704.1.6",
          "form": "전에는 토끼.강아지를 키워보았습니다.",
          "original_form": "전에는 토끼.강아지를 키워보았습니다.",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026704.1.7",
          "form": "고양이는 깨끗한 동물이라던데",
          "original_form": "고양이는 깨끗한 동물이라던데",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026704.1.8",
          "form": "토끼가 대소변을 화장실에 가리게 만들어서\n나름 토끼카페에서 핫했어요",
          "original_form": "토끼가 대소변을 화장실에 가리게 만들어서\n나름 토끼카페에서 핫했어요",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026704.1.9",
          "form": "강아지는 산책가고 혼자를 싫어하는데",
          "original_form": "강아지는 산책가고 혼자를 싫어하는데",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026704.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026704.1.11",
          "form": "그게 가능한가요??",
          "original_form": "그게 가능한가요??",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026704.1.12",
          "form": "강아지만 가능한줄요ㅎ",
          "original_form": "강아지만 가능한줄요ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026704.1.13",
          "form": "고양이는 정말 화장실 모래만 자주 갈아주면\n애교많고 좋습니다",
          "original_form": "고양이는 정말 화장실 모래만 자주 갈아주면\n애교많고 좋습니다",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026704.1.14",
          "form": "고양이는 알아서 다 해요",
          "original_form": "고양이는 알아서 다 해요",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026704.1.15",
          "form": "그냥 전..사슴벌레만 ㅎㅎㅎ",
          "original_form": "그냥 전..사슴벌레만 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026704.1.16",
          "form": "챙겨줄 자신이 없네요",
          "original_form": "챙겨줄 자신이 없네요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026704.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026704.1.18",
          "form": "정들면 떠나보낼때..힘듭니다",
          "original_form": "정들면 떠나보낼때..힘듭니다",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026704.1.19",
          "form": "말만들어도 슬프네요",
          "original_form": "말만들어도 슬프네요",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026704.1.20",
          "form": "반려견 반려묘",
          "original_form": "반려견 반려묘",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026704.1.21",
          "form": "정이 많이 들텐데 ㅠ",
          "original_form": "정이 많이 들텐데 ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026704.1.22",
          "form": "ㅠㅠ이미 7년넘게 지낸 고양이라\n할머니입니다..",
          "original_form": "ㅠㅠ이미 7년넘게 지낸 고양이라\n할머니입니다..",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026704.1.23",
          "form": "아이고..질병때문에 걱정이 많으시겠어요",
          "original_form": "아이고..질병때문에 걱정이 많으시겠어요",
          "speaker_id": "2",
          "time": "20210901 23:52"
        }
      ]
