      "utterance": [
        {
          "id": "MDRW2100016020.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 07:55"
        },
        {
          "id": "MDRW2100016020.1.2",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210826 07:57"
        },
        {
          "id": "MDRW2100016020.1.3",
          "form": "가지고겨신 종교가있으신가요",
          "original_form": "가지고겨신 종교가있으신가요",
          "speaker_id": "1",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100016020.1.4",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "2",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100016020.1.5",
          "form": "ㅋㄲㄱㄱ구",
          "original_form": "ㅋㄲㄱㄱ구",
          "speaker_id": "2",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100016020.1.6",
          "form": "저도 없어요ㅎㅎ",
          "original_form": "저도 없어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100016020.1.7",
          "form": "으헉..",
          "original_form": "으헉..",
          "speaker_id": "2",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100016020.1.8",
          "form": "종교를 가지지않는 이유 있나요?",
          "original_form": "종교를 가지지않는 이유 있나요?",
          "speaker_id": "1",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.9",
          "form": "음..끌려갔어요",
          "original_form": "음..끌려갔어요",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.10",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.11",
          "form": "도를믿습니까가서",
          "original_form": "도를믿습니까가서",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.12",
          "form": "한복입고절하고",
          "original_form": "한복입고절하고",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.13",
          "form": "20만원뜯기고",
          "original_form": "20만원뜯기고",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.14",
          "form": "세상에..",
          "original_form": "세상에..",
          "speaker_id": "1",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.15",
          "form": "와 지금생각해도빡치네요",
          "original_form": "와 지금생각해도빡치네요",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.16",
          "form": "안믿을만 하네요",
          "original_form": "안믿을만 하네요",
          "speaker_id": "1",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.17",
          "form": "ㅋㄱㅋㄱㅋㄱㄲ",
          "original_form": "ㅋㄱㅋㄱㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.18",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.19",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 07:59"
        },
        {
          "id": "MDRW2100016020.1.20",
          "form": "저는.. 보이지았는 존재를 안믿어요",
          "original_form": "저는.. 보이지았는 존재를 안믿어요",
          "speaker_id": "1",
          "time": "20210826 08:00"
        },
        {
          "id": "MDRW2100016020.1.21",
          "form": "약간 못믿는거에 가깝죠",
          "original_form": "약간 못믿는거에 가깝죠",
          "speaker_id": "1",
          "time": "20210826 08:00"
        },
        {
          "id": "MDRW2100016020.1.22",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210826 08:00"
        },
        {
          "id": "MDRW2100016020.1.23",
          "form": "그럼 sf안좋아하시겠다",
          "original_form": "그럼 sf안좋아하시겠다",
          "speaker_id": "2",
          "time": "20210826 08:00"
        },
        {
          "id": "MDRW2100016020.1.24",
          "form": "그쵸",
