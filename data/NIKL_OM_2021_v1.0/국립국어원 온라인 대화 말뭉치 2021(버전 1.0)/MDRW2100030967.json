        {
          "id": "MDRW2100030967.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030967.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030967.1.3",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030967.1.4",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030967.1.5",
          "form": "네 저는 찜닭 먹었습니다",
          "original_form": "네 저는 찜닭 먹었습니다",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030967.1.6",
          "form": "254님은요?",
          "original_form": "254님은요?",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030967.1.7",
          "form": "맛있었겠어요 전 빵",
          "original_form": "맛있었겠어요 전 빵",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030967.1.8",
          "form": "배달 시키셨어요?",
          "original_form": "배달 시키셨어요?",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030967.1.9",
          "form": "엊그제 시킨 게 남아서 그거 데워 먹었어요",
          "original_form": "엊그제 시킨 게 남아서 그거 데워 먹었어요",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030967.1.10",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030967.1.11",
          "form": "짱 좋아해요",
          "original_form": "짱 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030967.1.12",
          "form": "매일 먹어도 안 질릴듯",
          "original_form": "매일 먹어도 안 질릴듯",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030967.1.13",
          "form": "오 어떤 빵 좋아하세요?!",
          "original_form": "오 어떤 빵 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030967.1.14",
          "form": "저는 종류불문입니다",
          "original_form": "저는 종류불문입니다",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030967.1.15",
          "form": "캄파뉴 치아바타처럼 담백이도 좋고",
          "original_form": "캄파뉴 치아바타처럼 담백이도 좋고",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030967.1.16",
          "form": "치즈가득도 좋고",
          "original_form": "치즈가득도 좋고",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030967.1.17",
          "form": "생크림듬뿍도 좋고",
          "original_form": "생크림듬뿍도 좋고",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030967.1.18",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100030967.1.19",
          "form": "와 빵 종류가 그리 많나요?!",
          "original_form": "와 빵 종류가 그리 많나요?!",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100030967.1.20",
          "form": "앗 제가 폭주했네요ㅋㄱㅋ",
          "original_form": "앗 제가 폭주했네요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100030967.1.21",
          "form": "캄파뉴 치아바타가 뭔지 궁금해서",
          "original_form": "캄파뉴 치아바타가 뭔지 궁금해서",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030967.1.22",
          "form": "검색해봤어요 ㅎㅎ",
          "original_form": "검색해봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030967.1.23",
          "form": "유럽사람들한테 밥같은거죵",
          "original_form": "유럽사람들한테 밥같은거죵",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030967.1.24",
          "form": "피자빵 같은 걸줄 알았는데 정말 빵이어서",
          "original_form": "피자빵 같은 걸줄 알았는데 정말 빵이어서",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030967.1.25",
          "form": "놀랐어요",
          "original_form": "놀랐어요",
          "speaker_id": "1",
