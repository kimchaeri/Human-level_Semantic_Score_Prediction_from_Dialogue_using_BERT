      "utterance": [
        {
          "id": "MDRW2100008515.1.1",
          "form": "커뮤니티 얘기 했어요~",
          "original_form": "커뮤니티 얘기 했어요~",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008515.1.2",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008515.1.3",
          "form": "즐겨읽으시는 장르 있으세요?",
          "original_form": "즐겨읽으시는 장르 있으세요?",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008515.1.4",
          "form": "종이 책은 여러 분야 읽고 웹으로는 현대 판타지나 로판 작품 많이 봐요",
          "original_form": "종이 책은 여러 분야 읽고 웹으로는 현대 판타지나 로판 작품 많이 봐요",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008515.1.5",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008515.1.6",
          "form": "저는 소설이면 다 좋아하는 것같아요",
          "original_form": "저는 소설이면 다 좋아하는 것같아요",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008515.1.7",
          "form": "자기계발 도서도 좋아하고",
          "original_form": "자기계발 도서도 좋아하고",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008515.1.8",
          "form": "웹소설는 안본지 꽤 됐는데 재밌는거 있나요?",
          "original_form": "웹소설는 안본지 꽤 됐는데 재밌는거 있나요?",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008515.1.9",
          "form": "유명한 작품들 위주로 봤어요",
          "original_form": "유명한 작품들 위주로 봤어요",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008515.1.10",
          "form": "로판은 주인공이 머리 싸움하고 정치하는 거 위주로 봐서",
          "original_form": "로판은 주인공이 머리 싸움하고 정치하는 거 위주로 봐서",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008515.1.11",
          "form": "좋아하는 분위기 있으세요?",
          "original_form": "좋아하는 분위기 있으세요?",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008515.1.12",
          "form": "음.. 저는 너무 고통스러운건 안좋아해요",
          "original_form": "음.. 저는 너무 고통스러운건 안좋아해요",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008515.1.13",
          "form": "행복한걸 좋아하는 편이라 ㅎㅎ",
          "original_form": "행복한걸 좋아하는 편이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008515.1.14",
          "form": "주인공이 멋있는 작품 너뮤 좋죠!",
          "original_form": "주인공이 멋있는 작품 너뮤 좋죠!",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008515.1.15",
          "form": "힐링되는 작품들 좋아하시나봐요!",
          "original_form": "힐링되는 작품들 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008515.1.16",
          "form": "따뜻한 분위기도 좋죠",
          "original_form": "따뜻한 분위기도 좋죠",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008515.1.17",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008515.1.18",
          "form": "점점 힘든건 못보겠더라고요",
          "original_form": "점점 힘든건 못보겠더라고요",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008515.1.19",
          "form": "드라마나 영화도. ..",
          "original_form": "드라마나 영화도. ..",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008515.1.20",
          "form": "저도 요즘 피폐한 건 중도 하차할 때도 있어요",
          "original_form": "저도 요즘 피폐한 건 중도 하차할 때도 있어요",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008515.1.21",
          "form": "힘들더라고요",
          "original_form": "힘들더라고요",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008515.1.22",
          "form": "제일 재밌게 본 작품은 뭔가요?",
          "original_form": "제일 재밌게 본 작품은 뭔가요?",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008515.1.23",
          "form": "제일 재밌게 본 건 <오직교는 싫습니다> 요거 생각나네요",
          "original_form": "제일 재밌게 본 건 <오직교는 싫습니다> 요거 생각나네요",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008515.1.24",
          "form": "오작교는 싫습니다",
          "original_form": "오작교는 싫습니다",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008515.1.25",
          "form": "고통스럽 않은 건 <악녀의 남주님>이라고 카카페 작인데 피폐요소나 고구마 없고 주인공들 이야기 편하게 진행돼서 재밌게 봤어요",
          "original_form": "고통스럽 않은 건 <악녀의 남주님>이라고 카카페 작인데 피폐요소나 고구마 없고 주인공들 이야기 편하게 진행돼서 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008515.1.26",
          "form": "와 오작교는 싫숩니다",
          "original_form": "와 오작교는 싫숩니다",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008515.1.27",
          "form": "전에 봤었어요!",
          "original_form": "전에 봤었어요!",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008515.1.28",
          "form": "조아라에서 연제할때 ㅎㅎ",
          "original_form": "조아라에서 연제할때 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008515.1.29",
          "form": "아래 추천해쥬신것도 읽어볼개요 ㅎㅎ 쟈밌어보여뇨",
          "original_form": "아래 추천해쥬신것도 읽어볼개요 ㅎㅎ 쟈밌어보여뇨",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008515.1.30",
          "form": "시간 나실 때 한 번 보세요~!",
          "original_form": "시간 나실 때 한 번 보세요~!",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008515.1.31",
          "form": "취향에 맞으면 좋겠네요",
          "original_form": "취향에 맞으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210822 16:40"
        }
      ]
