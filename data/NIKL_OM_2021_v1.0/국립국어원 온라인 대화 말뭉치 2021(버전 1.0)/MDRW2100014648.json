      "utterance": [
        {
          "id": "MDRW2100014648.1.1",
          "form": "배달음식좋아하세요?",
          "original_form": "배달음식좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014648.1.2",
          "form": "좋아해요~ 브리또 자주시켜먹어요 ㅎㅎ",
          "original_form": "좋아해요~ 브리또 자주시켜먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014648.1.3",
          "form": "브리또 말고 또 어떤거 잘시켜먹나요",
          "original_form": "브리또 말고 또 어떤거 잘시켜먹나요",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014648.1.4",
          "form": "피자요!! 최애는 파파존스입니당",
          "original_form": "피자요!! 최애는 파파존스입니당",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014648.1.5",
          "form": "오 파파존스 저는 도미노가좋아요",
          "original_form": "오 파파존스 저는 도미노가좋아요",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014648.1.6",
          "form": "아 도미노좋져! 오늘 티멤버쉽 50퍼 하던데!!",
          "original_form": "아 도미노좋져! 오늘 티멤버쉽 50퍼 하던데!!",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014648.1.7",
          "form": "아그래요? 오대박 좋은정보감사",
          "original_form": "아그래요? 오대박 좋은정보감사",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014648.1.8",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014648.1.9",
          "form": "맛집가면 주로 어떤곳으로가나요 한식양식?",
          "original_form": "맛집가면 주로 어떤곳으로가나요 한식양식?",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014648.1.10",
          "form": "전 양식이요~ 파스타좋아해서요!",
          "original_form": "전 양식이요~ 파스타좋아해서요!",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014648.1.11",
          "form": "파스타 맛있죠? 오일파스타?토마토?",
          "original_form": "파스타 맛있죠? 오일파스타?토마토?",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014648.1.12",
          "form": "전 로제나 바질페스토 주로먹어요~ㅎㅎ",
          "original_form": "전 로제나 바질페스토 주로먹어요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014648.1.13",
          "form": "바질페스토 아시는구나",
          "original_form": "바질페스토 아시는구나",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014648.1.14",
          "form": "먹을줄아시네요!!",
          "original_form": "먹을줄아시네요!!",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014648.1.15",
          "form": "앜ㅋㅋㅋㅋ 네 좋아해요!!치아바타랑 곁들여먹으면 얼마나맛있게요",
          "original_form": "앜ㅋㅋㅋㅋ 네 좋아해요!!치아바타랑 곁들여먹으면 얼마나맛있게요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014648.1.16",
          "form": "치아바타ㅠㅠ 진자 최애빵이에요 전 치아바타 앙버터 좋아해요 ㅋㅋ",
          "original_form": "치아바타ㅠㅠ 진자 최애빵이에요 전 치아바타 앙버터 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014648.1.17",
          "form": "빵순이니까",
          "original_form": "빵순이니까",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014648.1.18",
          "form": "오옷!!! 대박 저도 빵순이에요~ 사워도우 너무좋아해요",
