      "utterance": [
        {
          "id": "MDRW2100018289.1.1",
          "form": "택배가 너무 자주 오는데",
          "original_form": "택배가 너무 자주 오는데",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018289.1.2",
          "form": "오늘 돈까스랑 낙곱새 왔을꺼야",
          "original_form": "오늘 돈까스랑 낙곱새 왔을꺼야",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018289.1.3",
          "form": "그거 와이드 영상찍은거 적립금으오 산거",
          "original_form": "그거 와이드 영상찍은거 적립금으오 산거",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018289.1.4",
          "form": "어 그거든데",
          "original_form": "어 그거든데",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018289.1.5",
          "form": "아직 오고있는것도 있고",
          "original_form": "아직 오고있는것도 있고",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018289.1.6",
          "form": "더온다고?",
          "original_form": "더온다고?",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018289.1.7",
          "form": "어 아직 다온게 아냐",
          "original_form": "어 아직 다온게 아냐",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018289.1.8",
          "form": "그리고 아직 쇼핑 들햇어",
          "original_form": "그리고 아직 쇼핑 들햇어",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018289.1.9",
          "form": "얼마나 시키는 건지",
          "original_form": "얼마나 시키는 건지",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018289.1.10",
          "form": "분리수거 할게 많아",
          "original_form": "분리수거 할게 많아",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018289.1.11",
          "form": "다양하게 먹어서 좋지 않아?",
          "original_form": "다양하게 먹어서 좋지 않아?",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018289.1.12",
          "form": "글쎄",
          "original_form": "글쎄",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018289.1.13",
          "form": "방금 육포랑 도시락도 시켰는대",
          "original_form": "방금 육포랑 도시락도 시켰는대",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018289.1.14",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018289.1.15",
          "form": "국종류도 시켜",
          "original_form": "국종류도 시켜",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018289.1.16",
          "form": "국? 알앗어 뭐있는지 볼게",
          "original_form": "국? 알앗어 뭐있는지 볼게",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018289.1.17",
          "form": "전에 산거 다먹어가 2팩남앗어",
          "original_form": "전에 산거 다먹어가 2팩남앗어",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018289.1.18",
          "form": "이번주 먹음 끝이겟네 ㅋㅋㅋ 구경해야 겠다",
          "original_form": "이번주 먹음 끝이겟네 ㅋㅋㅋ 구경해야 겠다",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018289.1.19",
          "form": "나무 많이 시키지 말고",
          "original_form": "나무 많이 시키지 말고",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018289.1.20",
          "form": "응 알았어",
          "original_form": "응 알았어",
          "speaker_id": "1",
          "time": "20210826 23:45"
        }
      ]
