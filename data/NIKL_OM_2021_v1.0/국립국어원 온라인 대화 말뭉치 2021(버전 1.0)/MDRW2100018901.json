      "utterance": [
        {
          "id": "MDRW2100018901.1.1",
          "form": "킥복싱",
          "original_form": "킥복싱",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018901.1.2",
          "form": "킥복싱일걸?",
          "original_form": "킥복싱일걸?",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018901.1.3",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018901.1.4",
          "form": "킥봉싱",
          "original_form": "킥봉싱",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018901.1.5",
          "form": "복싱",
          "original_form": "복싱",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018901.1.6",
          "form": "그거 할 때 맞을까봐 겁나 무서웠는데",
          "original_form": "그거 할 때 맞을까봐 겁나 무서웠는데",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018901.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ너한테",
          "original_form": "ㅋㅋㅋㅋㅋㅋ너한테",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018901.1.8",
          "form": "기술써본다고",
          "original_form": "기술써본다고",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018901.1.9",
          "form": "하지않음?",
          "original_form": "하지않음?",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018901.1.10",
          "form": "그러시진 않으셨을 걸 아마",
          "original_form": "그러시진 않으셨을 걸 아마",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018901.1.11",
          "form": "대신 아빠한테 했을 걸",
          "original_form": "대신 아빠한테 했을 걸",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018901.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018901.1.13",
          "form": "ㅋㅋㅋㅋㅋ너는따라서",
          "original_form": "ㅋㅋㅋㅋㅋ너는따라서",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018901.1.14",
          "form": "안배움?",
          "original_form": "안배움?",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018901.1.15",
          "form": "나는 그때 고등학생이었어서 안배움",
          "original_form": "나는 그때 고등학생이었어서 안배움",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018901.1.16",
          "form": "그때 배웠어야 했는데",
          "original_form": "그때 배웠어야 했는데",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018901.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ따로학원다신거야?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ따로학원다신거야?",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018901.1.18",
          "form": "엉엉 도장 다니셨댜",
          "original_form": "엉엉 도장 다니셨댜",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018901.1.19",
          "form": "오 진짜 대단하신듯",
          "original_form": "오 진짜 대단하신듯",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018901.1.20",
          "form": "아직도 새로운 운동을 배우신다는게",
          "original_form": "아직도 새로운 운동을 배우신다는게",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018901.1.21",
          "form": "그니까 지금은 안다니시는데",
          "original_form": "그니까 지금은 안다니시는데",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018901.1.22",
          "form": "요즘도운동하셔?",
          "original_form": "요즘도운동하셔?",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018901.1.23",
          "form": "나중에 또 배우실 건가봐",
          "original_form": "나중에 또 배우실 건가봐",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018901.1.24",
          "form": "오 진짜 멋있으시네",
          "original_form": "오 진짜 멋있으시네",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018901.1.25",
          "form": "너도그때같이배워",
          "original_form": "너도그때같이배워",
          "speaker_id": "1",
          "time": "20210827 13:33"
        }
      ]
