      "utterance": [
        {
          "id": "MDRW2100039561.1.1",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039561.1.2",
          "form": "네 그래서 자꾸",
          "original_form": "네 그래서 자꾸",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039561.1.3",
          "form": "아내 허리띠로 때리고..",
          "original_form": "아내 허리띠로 때리고..",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039561.1.4",
          "form": "헉 네",
          "original_form": "헉 네",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039561.1.5",
          "form": "발로 차고 밟고..",
          "original_form": "발로 차고 밟고..",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039561.1.6",
          "form": "헉 심함데요",
          "original_form": "헉 심함데요",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039561.1.7",
          "form": "네 사람들 철창에 가둬두고",
          "original_form": "네 사람들 철창에 가둬두고",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.8",
          "form": "나쁜 드라마네요",
          "original_form": "나쁜 드라마네요",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.9",
          "form": "네 엄마도 아들 허리띠로 때리고..",
          "original_form": "네 엄마도 아들 허리띠로 때리고..",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.10",
          "form": "안 봐야겠어요",
          "original_form": "안 봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.11",
          "form": "네 너무 자극적이에요",
          "original_form": "네 너무 자극적이에요",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.12",
          "form": "불편하겠어요",
          "original_form": "불편하겠어요",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.13",
          "form": "네 형수님이랑 불륜하고",
          "original_form": "네 형수님이랑 불륜하고",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.14",
          "form": "헉 그게 심의가 돼요?",
          "original_form": "헉 그게 심의가 돼요?",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039561.1.15",
          "form": "그니까요..",
          "original_form": "그니까요..",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039561.1.16",
          "form": "드라마 바벨 ㅋㅋㅋ",
          "original_form": "드라마 바벨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039561.1.17",
          "form": "불법 장기이식도 나오고 ㅋㅋ",
          "original_form": "불법 장기이식도 나오고 ㅋㅋ",
