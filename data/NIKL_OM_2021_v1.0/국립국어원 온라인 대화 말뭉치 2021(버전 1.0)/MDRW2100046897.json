      "utterance": [
        {
          "id": "MDRW2100046897.1.1",
          "form": "애플 13 그냥 그렇다며",
          "original_form": "애플 13 그냥 그렇다며",
          "speaker_id": "2",
          "time": "20211001 18:55"
        },
        {
          "id": "MDRW2100046897.1.2",
          "form": "이번에 애플에서 아이폰 13출시햇다는데",
          "original_form": "이번에 애플에서 아이폰 13출시햇다는데",
          "speaker_id": "1",
          "time": "20211001 18:55"
        },
        {
          "id": "MDRW2100046897.1.3",
          "form": "생각보단 12보다 별로 달라진게 앖는것 같앙",
          "original_form": "생각보단 12보다 별로 달라진게 앖는것 같앙",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.4",
          "form": "애플 제품 좋아해?",
          "original_form": "애플 제품 좋아해?",
          "speaker_id": "2",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.5",
          "form": "아니 한번도 안써봣어",
          "original_form": "아니 한번도 안써봣어",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.6",
          "form": "언니가 아이폰 7쓰는데",
          "original_form": "언니가 아이폰 7쓰는데",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.7",
          "form": "별로 안좋아보여ㅡㅡ",
          "original_form": "별로 안좋아보여ㅡㅡ",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.8",
          "form": "난 13살려고 기다렷는데..",
          "original_form": "난 13살려고 기다렷는데..",
          "speaker_id": "2",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.9",
          "form": "비싸잖아 아이폰",
          "original_form": "비싸잖아 아이폰",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.10",
          "form": "별로라 삼성살까봐",
          "original_form": "별로라 삼성살까봐",
          "speaker_id": "2",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.11",
          "form": "요금제고 비싸고",
          "original_form": "요금제고 비싸고",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.12",
          "form": "그렇긴해..",
          "original_form": "그렇긴해..",
          "speaker_id": "2",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.13",
          "form": "삼성페이가 짱이지",
          "original_form": "삼성페이가 짱이지",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.14",
          "form": "ㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "2",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.15",
          "form": "그치..",
          "original_form": "그치..",
          "speaker_id": "1",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.16",
          "form": "그거때매 지갑 막 잃어버려",
          "original_form": "그거때매 지갑 막 잃어버려",
          "speaker_id": "2",
          "time": "20211001 18:56"
        },
        {
          "id": "MDRW2100046897.1.17",
          "form": "나 지금 삼성폰 쓰는중..삼성페이 짜임..",
          "original_form": "나 지금 삼성폰 쓰는중..삼성페이 짜임..",
