      "utterance": [
        {
          "id": "MDRW2100009825.1.1",
          "form": "저도 해리포터 좋아해요!",
          "original_form": "저도 해리포터 좋아해요!",
          "speaker_id": "1",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009825.1.2",
          "form": "어떤 기숙사 들어가고 싶으세요?",
          "original_form": "어떤 기숙사 들어가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210823 10:31"
        },
        {
          "id": "MDRW2100009825.1.3",
          "form": "전 후프플루나  레번클로요",
          "original_form": "전 후프플루나  레번클로요",
          "speaker_id": "2",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009825.1.4",
          "form": "독수리 기숙사는 공부를 근데 잘해야 하는뎁 ㅎㅎ",
          "original_form": "독수리 기숙사는 공부를 근데 잘해야 하는뎁 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009825.1.5",
          "form": "ㅋㅋㅋㅋㅋ 아 뭐 모자가 결정해주지 않을까요?",
          "original_form": "ㅋㅋㅋㅋㅋ 아 뭐 모자가 결정해주지 않을까요?",
          "speaker_id": "1",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009825.1.6",
          "form": "저도 후플프푸 가고싶어요 과잉경쟁이 싫어서 ㅋㅋㅋ",
          "original_form": "저도 후플프푸 가고싶어요 과잉경쟁이 싫어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009825.1.7",
          "form": "아 생각해보니 그러네요 ㅋㅋ",
          "original_form": "아 생각해보니 그러네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009825.1.8",
          "form": "굳이 고민하지말지어다",
          "original_form": "굳이 고민하지말지어다",
          "speaker_id": "2",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009825.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 뭐 맘에 둔 기숙사 잇으면 좋져",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 뭐 맘에 둔 기숙사 잇으면 좋져",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009825.1.10",
          "form": "모자는 너의 맘을 알고있답 ㅋ",
          "original_form": "모자는 너의 맘을 알고있답 ㅋ",
          "speaker_id": "2",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009825.1.11",
          "form": "그거말고 음 인생영화? 같은건 있으세요?",
          "original_form": "그거말고 음 인생영화? 같은건 있으세요?",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009825.1.12",
          "form": "저 왕가위 영화 좋아해요 ㅎㅎ 아시나요",
          "original_form": "저 왕가위 영화 좋아해요 ㅎㅎ 아시나요",
          "speaker_id": "1",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009825.1.13",
          "form": "음 아니요 영화제목에 어떤것들이 있나요",
          "original_form": "음 아니요 영화제목에 어떤것들이 있나요",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009825.1.14",
          "form": "중경삼림 화양연화 이런건데",
          "original_form": "중경삼림 화양연화 이런건데",
          "speaker_id": "1",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009825.1.15",
          "form": "음 ㅎㅎ 지브리 만화도 재밌게 봤어요!",
          "original_form": "음 ㅎㅎ 지브리 만화도 재밌게 봤어요!",
          "speaker_id": "1",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009825.1.16",
          "form": "하울이랑 센과 치히로는",
          "original_form": "하울이랑 센과 치히로는",
          "speaker_id": "2",
          "time": "20210823 11:46"
        },
        {
          "id": "MDRW2100009825.1.17",
          "form": "정말 길이길이 명작 같아요",
          "original_form": "정말 길이길이 명작 같아요",
          "speaker_id": "2",
          "time": "20210823 11:46"
        },
        {
          "id": "MDRW2100009825.1.18",
          "form": "아 맞아요 ㅋㅋㅋ 센과 치히로는 백번은 본거같아요",
          "original_form": "아 맞아요 ㅋㅋㅋ 센과 치히로는 백번은 본거같아요",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009825.1.19",
          "form": "볼때마다 뭉클한거같아요",
          "original_form": "볼때마다 뭉클한거같아요",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009825.1.20",
          "form": "너무 감동적이고 슬프고 아름답고 ㅜㅜ 다시 보고싶네요",
          "original_form": "너무 감동적이고 슬프고 아름답고 ㅜㅜ 다시 보고싶네요",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009825.1.21",
          "form": "그리고 마루밑 아리에티 아세요",
          "original_form": "그리고 마루밑 아리에티 아세요",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009825.1.22",
          "form": "엄청 예쁜 인형의 집 기억나요",
          "original_form": "엄청 예쁜 인형의 집 기억나요",
          "speaker_id": "1",
          "time": "20210823 12:27"
        }
      ]
    }
