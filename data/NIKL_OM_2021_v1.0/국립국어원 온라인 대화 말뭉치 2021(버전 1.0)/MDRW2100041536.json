      "utterance": [
        {
          "id": "MDRW2100041536.1.1",
          "form": "신전 로제도 먹을만하네",
          "original_form": "신전 로제도 먹을만하네",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041536.1.2",
          "form": "부드럽고 약간 칼칼한게 맛났어",
          "original_form": "부드럽고 약간 칼칼한게 맛났어",
          "speaker_id": "1",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041536.1.3",
          "form": "그래도 태리로제가 젤 나은듯해",
          "original_form": "그래도 태리로제가 젤 나은듯해",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100041536.1.4",
          "form": "엄마가 한번ㅈ만들어줘봐\n잘만드는 연예인 있다면서?",
          "original_form": "엄마가 한번ㅈ만들어줘봐\n잘만드는 연예인 있다면서?",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100041536.1.5",
          "form": "류수영씨 ㅎ\n그 레시피로 만들어봐야겠다\n그사람 요리 잘하더라구",
          "original_form": "류수영씨 ㅎ\n그 레시피로 만들어봐야겠다\n그사람 요리 잘하더라구",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100041536.1.6",
          "form": "조리법을 개발하렴 무지 여러번 해봤겠네",
          "original_form": "조리법을 개발하렴 무지 여러번 해봤겠네",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100041536.1.7",
          "form": "그치 요것 조것 넣고 빼고 함서 맛난 조리법을 개발했겠지",
          "original_form": "그치 요것 조것 넣고 빼고 함서 맛난 조리법을 개발했겠지",
          "speaker_id": "2",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100041536.1.8",
          "form": "대단한것같아 그리고 그 레시필 알려주잖아\n돈도 안받고",
          "original_form": "대단한것같아 그리고 그 레시필 알려주잖아\n돈도 안받고",
          "speaker_id": "1",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100041536.1.9",
          "form": "그래야 유튜브 구독자 많아지니 그게 돈받고 가르쳐주는것과 매한가지겠지",
          "original_form": "그래야 유튜브 구독자 많아지니 그게 돈받고 가르쳐주는것과 매한가지겠지",
          "speaker_id": "2",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100041536.1.10",
          "form": "그러네 ㅎ\n옿 추석엔 맛집서 시켜먹자\n손도 아프니",
          "original_form": "그러네 ㅎ\n옿 추석엔 맛집서 시켜먹자\n손도 아프니",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100041536.1.11",
          "form": "할머니가 계시니 맘대로 못하고 그래도 편하게 하긴하지만",
          "original_form": "할머니가 계시니 맘대로 못하고 그래도 편하게 하긴하지만",
          "speaker_id": "2",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100041536.1.12",
          "form": "맛난것만 먹자 ㅎ",
          "original_form": "맛난것만 먹자 ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100041536.1.13",
          "form": "고구마튀김 갈비 잡채하자",
          "original_form": "고구마튀김 갈비 잡채하자",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100041536.1.14",
          "form": "잡채 한번에 하는법있던데 그것 찾아서 해야지 ㅎ",
          "original_form": "잡채 한번에 하는법있던데 그것 찾아서 해야지 ㅎ",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100041536.1.15",
          "form": "캐슈넛마요도 정말 맛난데",
          "original_form": "캐슈넛마요도 정말 맛난데",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100041536.1.16",
          "form": "그치 건강에좋은 마요네즈지",
          "original_form": "그치 건강에좋은 마요네즈지",
          "speaker_id": "2",
          "time": "20210906 20:09"
        }
      ]
