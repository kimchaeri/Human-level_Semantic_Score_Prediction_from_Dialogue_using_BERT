      "utterance": [
        {
          "id": "MDRW2100043469.1.1",
          "form": "피터팬일걸",
          "original_form": "피터팬일걸",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043469.1.2",
          "form": "어 나 기억이 안나는데",
          "original_form": "어 나 기억이 안나는데",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043469.1.3",
          "form": "20년 전이라 그래 ㅋㅋ",
          "original_form": "20년 전이라 그래 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043469.1.4",
          "form": "아 잠만 나 기억났어",
          "original_form": "아 잠만 나 기억났어",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043469.1.5",
          "form": "그래 거기서 티셔츠도 팔았었어",
          "original_form": "그래 거기서 티셔츠도 팔았었어",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043469.1.6",
          "form": "맞아ㅋㅋ 진짜 작은 뮤지컬이었는데",
          "original_form": "맞아ㅋㅋ 진짜 작은 뮤지컬이었는데",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043469.1.7",
          "form": "어린이용이라 그래",
          "original_form": "어린이용이라 그래",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043469.1.8",
          "form": "근데 피터팬이 여자였어",
          "original_form": "근데 피터팬이 여자였어",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043469.1.9",
          "form": "피터팬이 어린애라서 성인남자가 하면 이상할걸ㅋㅋ",
          "original_form": "피터팬이 어린애라서 성인남자가 하면 이상할걸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043469.1.10",
          "form": "좀 그렇긴하겠다 근데 후크선장도 여자아니었나?",
          "original_form": "좀 그렇긴하겠다 근데 후크선장도 여자아니었나?",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043469.1.11",
          "form": "어맞아 후크도 여자였다",
          "original_form": "어맞아 후크도 여자였다",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043469.1.12",
          "form": "모든 배우가 여자였지ㅋㅋㅋ",
          "original_form": "모든 배우가 여자였지ㅋㅋㅋ",
