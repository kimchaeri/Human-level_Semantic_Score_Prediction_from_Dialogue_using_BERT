      "utterance": [
        {
          "id": "MDRW2100002141.1.1",
          "form": "지난 도쿄 올림픽 보셨나요?",
          "original_form": "지난 도쿄 올림픽 보셨나요?",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002141.1.2",
          "form": "네 업무시간이라 잘 챙겨보지는 않았지만 양궁은 다 본거같아요",
          "original_form": "네 업무시간이라 잘 챙겨보지는 않았지만 양궁은 다 본거같아요",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002141.1.3",
          "form": "양궁 보셨어요?",
          "original_form": "양궁 보셨어요?",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002141.1.4",
          "form": "네 양궁 봤어요!!",
          "original_form": "네 양궁 봤어요!!",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002141.1.5",
          "form": "엄청 먼데 잘맞추는게 너무 신기하더라구요",
          "original_form": "엄청 먼데 잘맞추는게 너무 신기하더라구요",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002141.1.6",
          "form": "저는 심박수가 너무 신기하더라구요",
          "original_form": "저는 심박수가 너무 신기하더라구요",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002141.1.7",
          "form": "아 맞아요!!!",
          "original_form": "아 맞아요!!!",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002141.1.8",
          "form": "또 다른 관전 포인트네요!!",
          "original_form": "또 다른 관전 포인트네요!!",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002141.1.9",
          "form": "양궁 한번도 안해봤는데",
          "original_form": "양궁 한번도 안해봤는데",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002141.1.10",
          "form": "올림픽 보고 한 번 해보고싶어졌어요ㅎㅎ",
          "original_form": "올림픽 보고 한 번 해보고싶어졌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002141.1.11",
          "form": "저두요 시내지나가다보니 양궁장이 있긴하더라구요 실내라 그런맛은 안날것같지만 들어가보고싶더라구요",
          "original_form": "저두요 시내지나가다보니 양궁장이 있긴하더라구요 실내라 그런맛은 안날것같지만 들어가보고싶더라구요",
          "speaker_id": "1",
          "time": "20210820 07:04"
        },
        {
          "id": "MDRW2100002141.1.12",
          "form": "안산 너무 대단한거같아요",
          "original_form": "안산 너무 대단한거같아요",
          "speaker_id": "1",
          "time": "20210820 07:05"
        },
        {
          "id": "MDRW2100002141.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210820 10:42"
        },
        {
          "id": "MDRW2100002141.1.14",
          "form": "금메달 1개도 대단한데 3개라니",
          "original_form": "금메달 1개도 대단한데 3개라니",
          "speaker_id": "2",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002141.1.15",
          "form": "얼마나 열심히 연습했을지...",
          "original_form": "얼마나 열심히 연습했을지...",
          "speaker_id": "2",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002141.1.16",
          "form": "심박수도 진짜 떨림이 없더라구요",
          "original_form": "심박수도 진짜 떨림이 없더라구요",
          "speaker_id": "1",
          "time": "20210820 10:44"
        },
        {
          "id": "MDRW2100002141.1.17",
          "form": "표정도 너무 침착하고",
          "original_form": "표정도 너무 침착하고",
          "speaker_id": "1",
          "time": "20210820 10:45"
        },
        {
          "id": "MDRW2100002141.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 10:47"
        },
        {
          "id": "MDRW2100002141.1.19",
          "form": "너무 멋있고 대단해요",
          "original_form": "너무 멋있고 대단해요",
          "speaker_id": "2",
          "time": "20210820 10:47"
        },
        {
          "id": "MDRW2100002141.1.20",
          "form": "배구 경기도 보셨나요?",
          "original_form": "배구 경기도 보셨나요?",
          "speaker_id": "2",
          "time": "20210820 10:48"
        },
        {
          "id": "MDRW2100002141.1.21",
          "form": "배구는 한번봤는데 너무 편파판정도 심하고\r\n속답답해서 보다 말았어요",
          "original_form": "배구는 한번봤는데 너무 편파판정도 심하고\r\n속답답해서 보다 말았어요",
          "speaker_id": "1",
          "time": "20210820 10:48"
        },
        {
          "id": "MDRW2100002141.1.22",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 10:50"
        },
        {
          "id": "MDRW2100002141.1.23",
          "form": "외국선수들은 남자선수들처럼 힘이 느껴지더라구요",
          "original_form": "외국선수들은 남자선수들처럼 힘이 느껴지더라구요",
          "speaker_id": "1",
          "time": "20210820 10:50"
        },
        {
          "id": "MDRW2100002141.1.24",
          "form": "보다가 판정 때문에 화난적이 많아요",
          "original_form": "보다가 판정 때문에 화난적이 많아요",
          "speaker_id": "2",
          "time": "20210820 10:50"
        },
        {
          "id": "MDRW2100002141.1.25",
          "form": "진짜 브라질 경기 보면서 너무 놀랐어요...",
          "original_form": "진짜 브라질 경기 보면서 너무 놀랐어요...",
          "speaker_id": "2",
          "time": "20210820 10:51"
        },
        {
          "id": "MDRW2100002141.1.26",
          "form": "보는사람도 답답한데 선수들은 오죽 답답할까싶어 너무 안타깝더라구요",
          "original_form": "보는사람도 답답한데 선수들은 오죽 답답할까싶어 너무 안타깝더라구요",
          "speaker_id": "1",
          "time": "20210820 10:51"
        }
      ]
