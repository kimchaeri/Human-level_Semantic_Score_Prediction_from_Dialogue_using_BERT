      "utterance": [
        {
          "id": "MDRW2100016235.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016235.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100016235.1.3",
          "form": "왜이렇게 답장이 늦었나요 기다렸잖아요",
          "original_form": "왜이렇게 답장이 늦었나요 기다렸잖아요",
          "speaker_id": "1",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100016235.1.4",
          "form": "ㅋㅋㅋㅋㅋ죄송해요 다른거하고있어서",
          "original_form": "ㅋㅋㅋㅋㅋ죄송해요 다른거하고있어서",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016235.1.5",
          "form": "딴거 하지말고 제게 집중하세요",
          "original_form": "딴거 하지말고 제게 집중하세요",
          "speaker_id": "1",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016235.1.6",
          "form": "집착이 심하네요",
          "original_form": "집착이 심하네요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016235.1.7",
          "form": "연애관이 그러신가봐여",
          "original_form": "연애관이 그러신가봐여",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016235.1.8",
          "form": "잘 아시는군요",
          "original_form": "잘 아시는군요",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016235.1.9",
          "form": "이런 연애를 할 겁니다",
          "original_form": "이런 연애를 할 겁니다",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016235.1.10",
          "form": "별로 맞추고 싶지 않았네요",
          "original_form": "별로 맞추고 싶지 않았네요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016235.1.11",
          "form": "하지만 맞추셨네요",
          "original_form": "하지만 맞추셨네요",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016235.1.12",
          "form": "마음이 잘 맞아요",
          "original_form": "마음이 잘 맞아요",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016235.1.13",
          "form": "뜬금없지만",
          "original_form": "뜬금없지만",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016235.1.14",
          "form": "결혼은 하고싶으세요",
          "original_form": "결혼은 하고싶으세요",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016235.1.15",
          "form": "지금은 딱히 생각이 없고 나중에 햄스터를 키우면 귀엽지 않을까요",
          "original_form": "지금은 딱히 생각이 없고 나중에 햄스터를 키우면 귀엽지 않을까요",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016235.1.16",
          "form": "네 키우세요 귀엽겠네요",
          "original_form": "네 키우세요 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016235.1.17",
          "form": "왜 이렇게 제 햄스터에 성의가 없죠",
          "original_form": "왜 이렇게 제 햄스터에 성의가 없죠",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016235.1.18",
          "form": "name2님은 결혼 생각 있나요",
          "original_form": "&name2&님은 결혼 생각 있나요",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016235.1.19",
          "form": "별로 없어요 혼자 돈 많이벌고싶네요",
          "original_form": "별로 없어요 혼자 돈 많이벌고싶네요",
