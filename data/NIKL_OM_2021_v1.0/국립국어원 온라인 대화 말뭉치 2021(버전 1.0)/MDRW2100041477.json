      "utterance": [
        {
          "id": "MDRW2100041477.1.1",
          "form": "일단 처음에는 빵 종류를 고르는 거야",
          "original_form": "일단 처음에는 빵 종류를 고르는 거야",
          "speaker_id": "2",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.2",
          "form": "어떤 거 와 어떤 거",
          "original_form": "어떤 거 와 어떤 거",
          "speaker_id": "1",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.3",
          "form": "크로플과 식빵",
          "original_form": "크로플과 식빵",
          "speaker_id": "2",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.4",
          "form": "나는 무조건 크로플",
          "original_form": "나는 무조건 크로플",
          "speaker_id": "1",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.5",
          "form": "잉 식빵이 당첨되었지",
          "original_form": "잉 식빵이 당첨되었지",
          "speaker_id": "2",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.6",
          "form": "그다음엔",
          "original_form": "그다음엔",
          "speaker_id": "1",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.7",
          "form": "소세지와 햄 중에서 고르면 돼",
          "original_form": "소세지와 햄 중에서 고르면 돼",
          "speaker_id": "2",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.8",
          "form": "그건 좀 막상막 한데",
          "original_form": "그건 좀 막상막 한데",
          "speaker_id": "1",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.9",
          "form": "이땐 아마 햄이 됐을 거야",
          "original_form": "이땐 아마 햄이 됐을 거야",
          "speaker_id": "2",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041477.1.10",
          "form": "몸은 어떤 것과 어떤 거 가 투표야",
          "original_form": "몸은 어떤 것과 어떤 거 가 투표야",
          "speaker_id": "1",
          "time": "20210906 01:05"
        },
        {
          "id": "MDRW2100041477.1.11",
          "form": "매운맛과 마늘 맛",
          "original_form": "매운맛과 마늘 맛",
          "speaker_id": "2",
          "time": "20210906 01:06"
        },
        {
          "id": "MDRW2100041477.1.12",
          "form": "왠지 마늘이 이길 것 같은데",
          "original_form": "왠지 마늘이 이길 것 같은데",
          "speaker_id": "1",
          "time": "20210906 01:06"
        },
        {
          "id": "MDRW2100041477.1.13",
          "form": "또 그랬으면 좋겠어",
          "original_form": "또 그랬으면 좋겠어",
          "speaker_id": "2",
          "time": "20210906 01:06"
        },
        {
          "id": "MDRW2100041477.1.14",
          "form": "결과 나오면 알려줘",
          "original_form": "결과 나오면 알려줘",
          "speaker_id": "1",
          "time": "20210906 01:06"
        },
        {
          "id": "MDRW2100041477.1.15",
          "form": "사먹으러 가 보자",
          "original_form": "사먹으러 가 보자",
          "speaker_id": "2",
          "time": "20210906 01:06"
