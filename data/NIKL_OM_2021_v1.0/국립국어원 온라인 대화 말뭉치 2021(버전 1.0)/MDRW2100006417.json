          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006417.1.5",
          "form": "반려동물 키우고계세요?",
          "original_form": "반려동물 키우고계세요?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006417.1.6",
          "form": "헉 저는 거북이 키우고있어요",
          "original_form": "헉 저는 거북이 키우고있어요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006417.1.7",
          "form": "어떤 동물 키우세요?",
          "original_form": "어떤 동물 키우세요?",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006417.1.8",
          "form": "와 거북이 귀엽겠다",
          "original_form": "와 거북이 귀엽겠다",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006417.1.9",
          "form": "전 고양이 키우고있어요!",
          "original_form": "전 고양이 키우고있어요!",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006417.1.10",
          "form": "헐 고양이",
          "original_form": "헐 고양이",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006417.1.11",
          "form": "전 고양이 좋아하는데",
          "original_form": "전 고양이 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006417.1.12",
          "form": "고양이들이 절 싫어하는지",
          "original_form": "고양이들이 절 싫어하는지",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006417.1.13",
          "form": "절 피하더라구요ㅠㅜ",
          "original_form": "절 피하더라구요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006417.1.14",
          "form": "아이구",
          "original_form": "아이구",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006417.1.15",
          "form": "원래 고양이들이",
          "original_form": "원래 고양이들이",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006417.1.16",
          "form": "사람들을 잘 피해요ㅎㅎ..",
          "original_form": "사람들을 잘 피해요ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006417.1.17",
          "form": "저희집 고양이도 엄청 까칠한걸요",
          "original_form": "저희집 고양이도 엄청 까칠한걸요",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006417.1.18",
          "form": "ㅠㅜ 키우시는 고양이도 집사님 피하세요?",
          "original_form": "ㅠㅜ 키우시는 고양이도 집사님 피하세요?",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006417.1.19",
          "form": "전 키운지 오래돼서 이제 많이 친해졌어요!",
          "original_form": "전 키운지 오래돼서 이제 많이 친해졌어요!",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006417.1.20",
          "form": "근데 까칠할 때도 많아요..ㅎㅎ",
          "original_form": "근데 까칠할 때도 많아요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006417.1.21",
          "form": "키우시는 거북이 이름은 뭐예요?",
          "original_form": "키우시는 거북이 이름은 뭐예요?",
          "speaker_id": "1",
