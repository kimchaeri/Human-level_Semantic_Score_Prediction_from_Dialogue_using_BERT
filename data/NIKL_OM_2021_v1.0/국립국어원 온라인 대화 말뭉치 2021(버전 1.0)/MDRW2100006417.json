        },
        {
          "id": "MDRW2100006417.1.27",
          "form": "넘 귀엽던데ㅠㅜ",
          "original_form": "넘 귀엽던데ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006417.1.28",
          "form": "그쵸ㅠㅠ 넘 부러워요 그런 애옹이들..",
