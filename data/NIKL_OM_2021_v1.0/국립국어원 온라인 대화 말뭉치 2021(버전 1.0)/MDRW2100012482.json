      "utterance": [
        {
          "id": "MDRW2100012482.1.1",
          "form": "해야되는데 이거 예약하기가ㅠ",
          "original_form": "해야되는데 이거 예약하기가ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012482.1.2",
          "form": "예약 할라카믄 또 스케줄 꼬이고~",
          "original_form": "예약 할라카믄 또 스케줄 꼬이고~",
          "speaker_id": "2",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012482.1.3",
          "form": "앞에 신데렐라 가면 되는데 그것마져도 힘드네여 ㅋㅋ",
          "original_form": "앞에 신데렐라 가면 되는데 그것마져도 힘드네여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012482.1.4",
          "form": "신데렐라에 아는 사람 있나??",
          "original_form": "신데렐라에 아는 사람 있나??",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012482.1.5",
          "form": "난 눈썹은 민래쉬가 낫더라",
          "original_form": "난 눈썹은 민래쉬가 낫더라",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012482.1.6",
          "form": "아니여 ㅋㅋㅋㅋ가까워서요 ㅋㅋㅋㅋ",
          "original_form": "아니여 ㅋㅋㅋㅋ가까워서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.7",
          "form": "송포유 도 나쁘지 않음",
          "original_form": "송포유 도 나쁘지 않음",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.9",
          "form": "그래 가까운데 가야지",
          "original_form": "그래 가까운데 가야지",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.10",
          "form": "지는 뭐든 가까운곳 ㅋㅋㅋㅋ그래서 병원도 행복한 다녔잖아요 ㅋㅋㅋ",
          "original_form": "지는 뭐든 가까운곳 ㅋㅋㅋㅋ그래서 병원도 행복한 다녔잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.11",
          "form": "발톱은 좀 했나??",
          "original_form": "발톱은 좀 했나??",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그래\n가까운곳",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그래\n가까운곳",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.13",
          "form": "이런사람 있어서 그런데도 돈번다며 ㅋㅋㅋ",
          "original_form": "이런사람 있어서 그런데도 돈번다며 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.14",
          "form": "그래 거기까지 누가 가겠노",
          "original_form": "그래 거기까지 누가 가겠노",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.15",
          "form": "발톱도 얼여름 데싱디바로 버팀요 ㅋㅋㅋ",
          "original_form": "발톱도 얼여름 데싱디바로 버팀요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012482.1.16",
          "form": "올 데싱디바 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "올 데싱디바 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.17",
          "form": "와따 데싱디바 짱짱함요~",
          "original_form": "와따 데싱디바 짱짱함요~",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.18",
          "form": "잘했네",
          "original_form": "잘했네",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.19",
          "form": "그 이쁜 거 엄청 많더라",
          "original_form": "그 이쁜 거 엄청 많더라",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.20",
          "form": "떼고나니 발톱 껍질 다 벗겨져요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "떼고나니 발톱 껍질 다 벗겨져요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.21",
          "form": "얼마나 접착력 좋은지~",
          "original_form": "얼마나 접착력 좋은지~",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.22",
          "form": "좋은 거 가 안 좋은 거 가",
          "original_form": "좋은 거 가 안 좋은 거 가",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.23",
          "form": "반반요 ㅋㅋㅋㅋㅋ",
          "original_form": "반반요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.24",
          "form": "해석하기 나름 ㅋㅋㅋㅋ",
          "original_form": "해석하기 나름 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.25",
          "form": "그래도 여름 잘 버텼네",
          "original_form": "그래도 여름 잘 버텼네",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012482.1.26",
          "form": "난 거기 언니네서 두 번 했다",
          "original_form": "난 거기 언니네서 두 번 했다",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012482.1.27",
          "form": "ㅋㅋㅋㅋㅋ올여름 늠 빨리끝나는듯요~",
          "original_form": "ㅋㅋㅋㅋㅋ올여름 늠 빨리끝나는듯요~",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012482.1.28",
          "form": "그언니네 한번 가야는데",
          "original_form": "그언니네 한번 가야는데",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012482.1.29",
          "form": "넘멀어요 ㅋㅋㅋㅋ",
          "original_form": "넘멀어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012482.1.30",
          "form": "그렇네 비오니까 이제 여름 끝이네",
          "original_form": "그렇네 비오니까 이제 여름 끝이네",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012482.1.31",
          "form": "아부지한테 갈 때 해라",
          "original_form": "아부지한테 갈 때 해라",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012482.1.32",
          "form": "나도 맨날 엄마한테 맡기고 감",
          "original_form": "나도 맨날 엄마한테 맡기고 감",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012482.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ아빠 둘은 못봐가..내년 여름에 시도해보께여 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아빠 둘은 못봐가..내년 여름에 시도해보께여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100012482.1.34",
          "form": "내년까지.잘 버티고 계시길 ㅋㅋㅋ",
          "original_form": "내년까지.잘 버티고 계시길 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100012482.1.35",
          "form": "아 그래  둘째는 빡시지",
          "original_form": "아 그래  둘째는 빡시지",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012482.1.36",
          "form": "그래도 거기 나름 손님 많더라",
          "original_form": "그래도 거기 나름 손님 많더라",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012482.1.37",
          "form": "얼마 전에 카페 명품녀77",
          "original_form": "얼마 전에 카페 명품녀77",
          "speaker_id": "1",
          "time": "20210825 12:51"
        },
        {
          "id": "MDRW2100012482.1.38",
          "form": "보톡스 맞았다고 올라온 거 봤나",
          "original_form": "보톡스 맞았다고 올라온 거 봤나",
          "speaker_id": "1",
          "time": "20210825 12:52"
        },
        {
          "id": "MDRW2100012482.1.39",
          "form": "입술이 안젤리나졸린 줄",
          "original_form": "입술이 안젤리나졸린 줄",
