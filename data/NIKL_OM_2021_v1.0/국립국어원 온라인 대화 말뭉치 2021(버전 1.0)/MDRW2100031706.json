      "utterance": [
        {
          "id": "MDRW2100031706.1.1",
          "form": "친구들 모여라~",
          "original_form": "친구들 모여라~",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031706.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031706.1.3",
          "form": "나는 그",
          "original_form": "나는 그",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031706.1.4",
          "form": "여행갈때",
          "original_form": "여행갈때",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031706.1.5",
          "form": "내 현생이랑 동떨어져서",
          "original_form": "내 현생이랑 동떨어져서",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031706.1.6",
          "form": "아 ㅇㅈ...",
          "original_form": "아 ㅇㅈ...",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031706.1.7",
          "form": "새로운거 보고 그런부분이 좋은데",
          "original_form": "새로운거 보고 그런부분이 좋은데",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031706.1.8",
          "form": "그래서 돌아올때",
          "original_form": "그래서 돌아올때",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031706.1.9",
          "form": "공항에서 내랴서 집으로 돌아가는 그때가",
          "original_form": "공항에서 내랴서 집으로 돌아가는 그때가",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031706.1.10",
          "form": "좀 현타오기더하고 체력도 없으니까 너무힘듦ㅌㅋㅋㅋㅋㅋ",
          "original_form": "좀 현타오기더하고 체력도 없으니까 너무힘듦ㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031706.1.11",
          "form": "아 진짜 ㅇㅈ",
          "original_form": "아 진짜 ㅇㅈ",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031706.1.12",
          "form": "그 현실로 돌아와야 하는 그 시간...",
          "original_form": "그 현실로 돌아와야 하는 그 시간...",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031706.1.13",
          "form": "너무 싫다",
          "original_form": "너무 싫다",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031706.1.14",
          "form": "와서 옷 다 빨래하고",
          "original_form": "와서 옷 다 빨래하고",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.15",
          "form": "정리하고",
          "original_form": "정리하고",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.17",
          "form": "그런것들..",
          "original_form": "그런것들..",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.18",
          "form": "안그래도 공항에서 집까지 몇시간 걸리는데",
          "original_form": "안그래도 공항에서 집까지 몇시간 걸리는데",
          "speaker_id": "1",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.19",
          "form": "집가서 또 뒷정리하고",
          "original_form": "집가서 또 뒷정리하고",
          "speaker_id": "1",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.20",
          "form": "진이 쫙빠짐",
          "original_form": "진이 쫙빠짐",
          "speaker_id": "1",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.21",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031706.1.22",
          "form": "그래도 사온 믈건들 보면 좋더라",
          "original_form": "그래도 사온 믈건들 보면 좋더라",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031706.1.23",
          "form": "그.래.도~",
          "original_form": "그.래.도~",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031706.1.24",
          "form": "유럽은 가보고싶다 ㅎ",
          "original_form": "유럽은 가보고싶다 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031706.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031706.1.26",
          "form": "나 벨기에 꼭 갈꺼야",
          "original_form": "나 벨기에 꼭 갈꺼야",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031706.1.27",
          "form": "가서 틴틴 굿즈",
          "original_form": "가서 틴틴 굿즈",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031706.1.28",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031706.1.29",
          "form": "다 쓸어온다",
          "original_form": "다 쓸어온다",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031706.1.30",
          "form": "근데 인종차별 심하니까",
          "original_form": "근데 인종차별 심하니까",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031706.1.31",
          "form": "하루만 있어야지",
          "original_form": "하루만 있어야지",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031706.1.32",
          "form": "맞아 나도 유럽 인종차별이 젤걱정됨",
          "original_form": "맞아 나도 유럽 인종차별이 젤걱정됨",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031706.1.33",
          "form": "나나",
          "original_form": "나나",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100031706.1.34",
          "form": "유튜브에서 신기한거 봣는데",
          "original_form": "유튜브에서 신기한거 봣는데",
          "speaker_id": "1",
          "time": "20210905 19:33"
        }
      ]
    }
