      "utterance": [
        {
          "id": "MDRW2100031831.1.1",
          "form": "그래도 별로같은데 ㅋㅋ",
          "original_form": "그래도 별로같은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:53"
        },
        {
          "id": "MDRW2100031831.1.2",
          "form": "포스틱을 쉐이크에 찍어먹는다 생각하면 조금 납득가는 조합인더같은데요 ㅋㅋㅋ",
          "original_form": "포스틱을 쉐이크에 찍어먹는다 생각하면 조금 납득가는 조합인더같은데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:54"
        },
        {
          "id": "MDRW2100031831.1.3",
          "form": "음 먹어보시고",
          "original_form": "음 먹어보시고",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031831.1.4",
          "form": "알려주세요 ㅎㅎ",
          "original_form": "알려주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031831.1.5",
          "form": "아침드셨나요?!",
          "original_form": "아침드셨나요?!",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100031831.1.6",
          "form": "아니요 귀찮아서 ㅋㅋ",
          "original_form": "아니요 귀찮아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031831.1.7",
          "form": "우유 한모금 먹고 누워있어요",
          "original_form": "우유 한모금 먹고 누워있어요",
          "speaker_id": "2",
          "time": "20210905 10:04"
        },
        {
          "id": "MDRW2100031831.1.8",
          "form": "우유좋아하세요?!",
          "original_form": "우유좋아하세요?!",
          "speaker_id": "1",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100031831.1.9",
          "form": "음 좋아하는 편인거 같아요",
          "original_form": "음 좋아하는 편인거 같아요",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100031831.1.10",
          "form": "우유 맛있어요 ㅋㅋ",
          "original_form": "우유 맛있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100031831.1.11",
          "form": "전약한유당불내증이있어서 ㅋㅋㅋㅋ 집에서만먹어요 ㅋㅋㅋ",
          "original_form": "전약한유당불내증이있어서 ㅋㅋㅋㅋ 집에서만먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031831.1.12",
          "form": "밀크쉐이크이런것도 맛있지만 무조건 테이크아웃 후 집에서 먹습니다 ㅋㅋㅋ",
          "original_form": "밀크쉐이크이런것도 맛있지만 무조건 테이크아웃 후 집에서 먹습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031831.1.13",
          "form": "오 유당불내증",
          "original_form": "오 유당불내증",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031831.1.14",
          "form": "락토프리 뭐 이런거 있지 싶은데",
          "original_form": "락토프리 뭐 이런거 있지 싶은데",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031831.1.15",
          "form": "근데 왜 집에서 먹어요?",
          "original_form": "근데 왜 집에서 먹어요?",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031831.1.16",
          "form": "배아프니까요 ㅋㅋㅋㅋ 아이스크림같은것도그래요 ㅋㅋ",
          "original_form": "배아프니까요 ㅋㅋㅋㅋ 아이스크림같은것도그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031831.1.17",
          "form": "근데 또 뒤에잘보면 우유가 별로 안들어있는친구들이 많더라구요 ㅋㅋㅋ",
          "original_form": "근데 또 뒤에잘보면 우유가 별로 안들어있는친구들이 많더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031831.1.18",
          "form": "우유가 좀 스친 친구들.. 그런애들은 괜찮습니다 ㅋㅋㅋ",
          "original_form": "우유가 좀 스친 친구들.. 그런애들은 괜찮습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031831.1.19",
          "form": "아 화장실 가야되서요?",
          "original_form": "아 화장실 가야되서요?",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031831.1.20",
          "form": "네네 ㅋㅋ 그래서 밖에선 안먹어요 모든 유당불내증있으신분들은 공감하실걸요?",
          "original_form": "네네 ㅋㅋ 그래서 밖에선 안먹어요 모든 유당불내증있으신분들은 공감하실걸요?",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031831.1.21",
          "form": "못드시는음식있으세요?",
          "original_form": "못드시는음식있으세요?",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031831.1.22",
          "form": "저는 딱히 못먹지는 않는데",
          "original_form": "저는 딱히 못먹지는 않는데",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031831.1.23",
          "form": "고수 싫어해요 ㅋㅋ",
          "original_form": "고수 싫어해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031831.1.24",
          "form": "아 맞다 향 센거 못드신다했죠 ㅋㅋㅋㅋ 그럼 동남아음식은 드세요?",
          "original_form": "아 맞다 향 센거 못드신다했죠 ㅋㅋㅋㅋ 그럼 동남아음식은 드세요?",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031831.1.25",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031831.1.26",
          "form": "저 동남아 가면",
          "original_form": "저 동남아 가면",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031831.1.27",
          "form": "막 로컬 음식 먹으러",
          "original_form": "막 로컬 음식 먹으러",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031831.1.28",
          "form": "골목 돌아다니고 하는데 ㅎㅎ",
          "original_form": "골목 돌아다니고 하는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:31"
        }
      ]
