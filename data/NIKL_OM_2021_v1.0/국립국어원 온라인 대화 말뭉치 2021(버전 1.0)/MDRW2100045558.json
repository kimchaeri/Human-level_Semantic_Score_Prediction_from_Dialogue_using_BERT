        {
          "id": "MDRW2100045558.1.1",
          "form": "무기가 약해 보여요",
          "original_form": "무기가 약해 보여요",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100045558.1.2",
          "form": "곧 물리는 거 아닌지",
          "original_form": "곧 물리는 거 아닌지",
          "speaker_id": "1",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100045558.1.3",
          "form": "인기 긴 인기 네요",
          "original_form": "인기 긴 인기 네요",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045558.1.4",
          "form": "이시간인데 많이 보긴 하네요",
          "original_form": "이시간인데 많이 보긴 하네요",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045558.1.5",
          "form": "변기 물을 먹다니",
          "original_form": "변기 물을 먹다니",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045558.1.6",
          "form": "다 먹어요ㅜㅜㅋㅋ",
          "original_form": "다 먹어요ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045558.1.7",
          "form": "티비도 나오나 봐요",
          "original_form": "티비도 나오나 봐요",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045558.1.8",
          "form": "편성 표도 있어요",
          "original_form": "편성 표도 있어요",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100045558.1.9",
          "form": "먹는 거 보니 배 고프네요",
          "original_form": "먹는 거 보니 배 고프네요",
          "speaker_id": "1",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100045558.1.10",
          "form": "과자가 먹고 싶은데요",
          "original_form": "과자가 먹고 싶은데요",
          "speaker_id": "2",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045558.1.11",
          "form": "이분은 집에 숨어 계시네",
          "original_form": "이분은 집에 숨어 계시네",
          "speaker_id": "1",
          "time": "20210908 03:34"
