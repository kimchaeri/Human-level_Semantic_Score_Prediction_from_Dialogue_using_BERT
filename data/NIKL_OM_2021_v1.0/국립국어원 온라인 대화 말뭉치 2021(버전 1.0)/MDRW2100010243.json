      "utterance": [
        {
          "id": "MDRW2100010243.1.1",
          "form": "둘찌데리러가나",
          "original_form": "둘찌데리러가나",
          "speaker_id": "2",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010243.1.2",
          "form": "네 둘째 데리러가려고요.",
          "original_form": "네 둘째 데리러가려고요.",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010243.1.3",
          "form": "언니는요?",
          "original_form": "언니는요?",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010243.1.4",
          "form": "집이지ㅋㅋ",
          "original_form": "집이지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010243.1.5",
          "form": "애들학원나가ㅁ",
          "original_form": "애들학원나가ㅁ",
          "speaker_id": "2",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010243.1.6",
          "form": "몇시에데리러가는고",
          "original_form": "몇시에데리러가는고",
          "speaker_id": "2",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010243.1.7",
          "form": "좋네요ㅋ",
          "original_form": "좋네요ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010243.1.8",
          "form": "둘째 10분뒤에 와요, ㅋ",
          "original_form": "둘째 10분뒤에 와요, ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010243.1.9",
          "form": "첫째가 갑자기 아이공방 하고싶다해서",
          "original_form": "첫째가 갑자기 아이공방 하고싶다해서",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010243.1.10",
          "form": "아하ㅋㅋ",
          "original_form": "아하ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010243.1.11",
          "form": "지금 거기 가 있어요.",
          "original_form": "지금 거기 가 있어요.",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010243.1.12",
          "form": "헉 진짜?",
          "original_form": "헉 진짜?",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010243.1.13",
          "form": "둘째데리고 같이 가야됨",
          "original_form": "둘째데리고 같이 가야됨",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010243.1.14",
          "form": "한달끊었나?",
          "original_form": "한달끊었나?",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010243.1.15",
          "form": "아니요. 일단 체험수업ㅋ",
          "original_form": "아니요. 일단 체험수업ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010243.1.16",
          "form": "근데 재미있다할것같아서 한달 끊을까 싶어요.",
          "original_form": "근데 재미있다할것같아서 한달 끊을까 싶어요.",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010243.1.17",
          "form": "그래 좋아하지ㅋㅋ",
          "original_form": "그래 좋아하지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010243.1.18",
          "form": "나도 첫째는 했었음",
          "original_form": "나도 첫째는 했었음",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010243.1.19",
          "form": "ㅋㅋㅋ 그러니깐요",
          "original_form": "ㅋㅋㅋ 그러니깐요",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010243.1.20",
          "form": "언니 그때 했잖아요ㅋ",
          "original_form": "언니 그때 했잖아요ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010243.1.21",
          "form": "좋아하는데 ㅋㅋ",
          "original_form": "좋아하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010243.1.22",
          "form": "아이들은ㅋ",
          "original_form": "아이들은ㅋ",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010243.1.23",
          "form": "너무 좋아할것같아요.",
          "original_form": "너무 좋아할것같아요.",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010243.1.24",
          "form": "그래서 긴장되네요 ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "그래서 긴장되네요 ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010243.1.25",
          "form": "ㅋㅋ 하게되겠군ㅋㅋ",
          "original_form": "ㅋㅋ 하게되겠군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010243.1.26",
          "form": "난 지금 청소중",
          "original_form": "난 지금 청소중",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010243.1.27",
          "form": "ㅋㅋ언니 청소 열심히해요,  이제 둘째 데리러갔다올게요, ㅋ",
          "original_form": "ㅋㅋ언니 청소 열심히해요,  이제 둘째 데리러갔다올게요, ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:05"
        }
      ]
    }
  ]
