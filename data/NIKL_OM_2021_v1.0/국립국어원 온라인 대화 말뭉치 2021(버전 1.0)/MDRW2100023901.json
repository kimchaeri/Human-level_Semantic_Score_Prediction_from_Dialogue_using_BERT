      "utterance": [
        {
          "id": "MDRW2100023901.1.1",
          "form": "필라테스는 얼마나 하신거에요, 그럼",
          "original_form": "필라테스는 얼마나 하신거에요, 그럼",
          "speaker_id": "1",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100023901.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100023901.1.3",
          "form": "저 이제 두달? 좀 넘었어요 ㅋㅋㅋ",
          "original_form": "저 이제 두달? 좀 넘었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100023901.1.4",
          "form": "아직 왕초보예요...",
          "original_form": "아직 왕초보예요...",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100023901.1.5",
          "form": "글쿤요.",
          "original_form": "글쿤요.",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023901.1.6",
          "form": "제 친구한테 들었는데",
          "original_form": "제 친구한테 들었는데",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023901.1.7",
          "form": "초보는 진짜 초보팔찌 차고 해요?",
          "original_form": "초보는 진짜 초보팔찌 차고 해요?",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023901.1.8",
          "form": "네??",
          "original_form": "네??",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023901.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023901.1.10",
          "form": "배우는 곳마다 다른거 아닐까요 ㅋㅋㅋㅋ",
          "original_form": "배우는 곳마다 다른거 아닐까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023901.1.11",
          "form": "아예 반을 나눠서 초보들만 단체로 하기도 하고",
          "original_form": "아예 반을 나눠서 초보들만 단체로 하기도 하고",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023901.1.12",
          "form": "아님 그냥 섞여서 하기도 해요 ㅋㅋㅋ",
          "original_form": "아님 그냥 섞여서 하기도 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023901.1.13",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023901.1.14",
          "form": "제 친구는 초보는 초보팔찌 찬다고",
          "original_form": "제 친구는 초보는 초보팔찌 찬다고",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023901.1.15",
          "form": "이거 안차면 무슨 일 일어났을지 모른다고",
          "original_form": "이거 안차면 무슨 일 일어났을지 모른다고",
          "speaker_id": "1",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023901.1.16",
          "form": "호달달 떨더라구요",
          "original_form": "호달달 떨더라구요",
          "speaker_id": "1",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023901.1.17",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023901.1.18",
          "form": "강사님이 돌아다니면서 자세 교정해주시는데",
          "original_form": "강사님이 돌아다니면서 자세 교정해주시는데",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023901.1.19",
          "form": "초보인거 모르면 가차없어서 그런가봐요 ㅋㅋ",
          "original_form": "초보인거 모르면 가차없어서 그런가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023901.1.20",
          "form": "으악ㅋㅋㅋㅋ",
          "original_form": "으악ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023901.1.21",
          "form": "무섭네요ㅋㅋㅋ",
          "original_form": "무섭네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023901.1.22",
          "form": "반복운동이랑 버티기? 시켜서 가끔은 좀 회의감이 들어요 ㅋㅋㅋㅋ",
          "original_form": "반복운동이랑 버티기? 시켜서 가끔은 좀 회의감이 들어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023901.1.23",
          "form": "이 고통을 견디면서 운동을 해야 할 가치가 있을지 ㅋㅋㅋ",
          "original_form": "이 고통을 견디면서 운동을 해야 할 가치가 있을지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023901.1.24",
          "form": "근데 운동 제대로  한다는 증거가 고통이라잖아요",
          "original_form": "근데 운동 제대로  한다는 증거가 고통이라잖아요",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023901.1.25",
          "form": "근육통이 있어야 근육이 생기는거고...",
          "original_form": "근육통이 있어야 근육이 생기는거고...",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023901.1.26",
          "form": "정말 비효율적인 시스템인거같아요 ㅋㅋㅋㅋㅋ ㅠ",
          "original_form": "정말 비효율적인 시스템인거같아요 ㅋㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023901.1.27",
          "form": "어렸을때 성장기에 좀 더 신나게 뛰어놀걸...",
          "original_form": "어렸을때 성장기에 좀 더 신나게 뛰어놀걸...",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023901.1.28",
          "form": "그쵸, 그때 근육이 평생간다던데",
          "original_form": "그쵸, 그때 근육이 평생간다던데",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100023901.1.29",
          "form": "이렇게 일찍 재미도 아니고 건강을 위해서 운동하게 될 중은 몰랐어요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "이렇게 일찍 재미도 아니고 건강을 위해서 운동하게 될 중은 몰랐어요 ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100023901.1.30",
          "form": "다들 서른즈음에 영양제 먹고 운동하게 된다고 하더라구요ㅠㅠ",
          "original_form": "다들 서른즈음에 영양제 먹고 운동하게 된다고 하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100023901.1.31",
          "form": "저도 슬슬 영양제 챙기는 중이에요 ...ㅠ",
          "original_form": "저도 슬슬 영양제 챙기는 중이에요 ...ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100023901.1.32",
          "form": "저도 종합비타민이랑 프로폴리스 먹어요",
          "original_form": "저도 종합비타민이랑 프로폴리스 먹어요",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100023901.1.33",
          "form": "프로폴리스 먹으면 효과 있나요?",
          "original_form": "프로폴리스 먹으면 효과 있나요?",
