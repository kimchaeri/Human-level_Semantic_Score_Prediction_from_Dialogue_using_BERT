      "utterance": [
        {
          "id": "MDRW2100013906.1.1",
          "form": "무슨 일을 해?",
          "original_form": "무슨 일을 해?",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013906.1.2",
          "form": "나는 사무직이라서 ㅋㅋ",
          "original_form": "나는 사무직이라서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013906.1.3",
          "form": "빨리 퇴근하고 싶겠다",
          "original_form": "빨리 퇴근하고 싶겠다",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013906.1.4",
          "form": "ㅋㅋㅋ 그건 언제나 하는 생각이지",
          "original_form": "ㅋㅋㅋ 그건 언제나 하는 생각이지",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013906.1.5",
          "form": "나는 구직활동을하고있어",
          "original_form": "나는 구직활동을하고있어",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013906.1.6",
          "form": "요즘 취직 준비하느라 바쁘겠다",
          "original_form": "요즘 취직 준비하느라 바쁘겠다",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013906.1.7",
          "form": "맞아 코로나 이후로 취업이 더 어렵대",
          "original_form": "맞아 코로나 이후로 취업이 더 어렵대",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013906.1.8",
          "form": "확실히 취직 자리가 적겠다",
          "original_form": "확실히 취직 자리가 적겠다",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013906.1.9",
          "form": "나는 배터리쪽을희망해",
          "original_form": "나는 배터리쪽을희망해",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013906.1.10",
          "form": "와우 전망이 좋아보이네",
          "original_form": "와우 전망이 좋아보이네",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013906.1.11",
          "form": "2차전지 분야는 괜찮을거같아  대신인기도많아",
          "original_form": "2차전지 분야는 괜찮을거같아  대신인기도많아",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013906.1.12",
          "form": "취직이 잘되면 좋겠다",
          "original_form": "취직이 잘되면 좋겠다",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013906.1.13",
          "form": "응원많이해줘 ㅎㅎ",
          "original_form": "응원많이해줘 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013906.1.14",
          "form": "좋은 회사 취직하길!",
          "original_form": "좋은 회사 취직하길!",
          "speaker_id": "2",
          "time": "20210825 16:29"
        }
      ]
