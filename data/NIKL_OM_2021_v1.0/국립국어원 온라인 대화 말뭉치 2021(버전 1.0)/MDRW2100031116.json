      "utterance": [
        {
          "id": "MDRW2100031116.1.1",
          "form": "드라마 많이 봤어요!",
          "original_form": "드라마 많이 봤어요!",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031116.1.2",
          "form": "님도 드라마 보세요?",
          "original_form": "님도 드라마 보세요?",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031116.1.3",
          "form": "한국드라마는 안보고",
          "original_form": "한국드라마는 안보고",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031116.1.4",
          "form": "해외거는 몇개 봤어요!",
          "original_form": "해외거는 몇개 봤어요!",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031116.1.5",
          "form": "뭐 보셨어요??",
          "original_form": "뭐 보셨어요??",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031116.1.6",
          "form": "유명한 것 중에선 굿플레이스 원헌드레드 어둠속으로 봤어요",
          "original_form": "유명한 것 중에선 굿플레이스 원헌드레드 어둠속으로 봤어요",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100031116.1.7",
          "form": "굿플레이스 천국 잘못가는 드라마 맞죠?ㅋㅋㅋㅋㅋ",
          "original_form": "굿플레이스 천국 잘못가는 드라마 맞죠?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031116.1.8",
          "form": "네 맞아요 ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 맞아요 ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031116.1.9",
          "form": "밥먹으면서 킬링타임용으로 보기 딱 좋더라구요",
          "original_form": "밥먹으면서 킬링타임용으로 보기 딱 좋더라구요",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031116.1.10",
          "form": "결말까지 넘 좋았어요",
          "original_form": "결말까지 넘 좋았어요",
          "speaker_id": "1",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031116.1.11",
          "form": "그거 시즌도 여러개 나오지 않았나요??",
          "original_form": "그거 시즌도 여러개 나오지 않았나요??",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031116.1.12",
          "form": "맞아요 시즌 4까지 있어요",
          "original_form": "맞아요 시즌 4까지 있어요",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.13",
          "form": "외국은 드라마 잘되면 시즌 오래오래 뽑아먹는데",
          "original_form": "외국은 드라마 잘되면 시즌 오래오래 뽑아먹는데",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.14",
          "form": "와 길게 나왔네요?!",
          "original_form": "와 길게 나왔네요?!",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.15",
          "form": "그러다가 꼭 산을타죠...",
          "original_form": "그러다가 꼭 산을타죠...",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.16",
          "form": "굿플은 딱 보여주고 싶은 거 보여주고 엔딩 낸 것 같아서 넘 좋아요",
          "original_form": "굿플은 딱 보여주고 싶은 거 보여주고 엔딩 낸 것 같아서 넘 좋아요",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.17",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.18",
          "form": "내용이 점점 스케일이 커지는..",
          "original_form": "내용이 점점 스케일이 커지는..",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.19",
          "form": "외국드라마는 끝까지 잘만든 드라마 찾기가 힘들더라고요",
          "original_form": "외국드라마는 끝까지 잘만든 드라마 찾기가 힘들더라고요",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031116.1.20",
          "form": "아니 막 다들 친구 전남친이",
          "original_form": "아니 막 다들 친구 전남친이",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100031116.1.21",
          "form": "현남친되있고...",
          "original_form": "현남친되있고...",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100031116.1.22",
          "form": "쟤네 둘이 사겼는데 지금은 얘네둘이 사귀고 정말ㅋㅋㅋㅋ",
          "original_form": "쟤네 둘이 사겼는데 지금은 얘네둘이 사귀고 정말ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100031116.1.23",
          "form": "아놔 ㅋㅋㅋㅋ 왜그러는걸까요 증말..",
          "original_form": "아놔 ㅋㅋㅋㅋ 왜그러는걸까요 증말..",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100031116.1.24",
          "form": "글고 외국은 드라마가 시즌제라",
          "original_form": "글고 외국은 드라마가 시즌제라",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031116.1.25",
          "form": "뭔가.. 적응이 안된다고 해야하나",
          "original_form": "뭔가.. 적응이 안된다고 해야하나",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031116.1.26",
          "form": "캐릭터가 좋으면",
          "original_form": "캐릭터가 좋으면",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031116.1.27",
          "form": "시즌 막화에서 다음 내용 여지 막 주면서 그대로 종영해버리고 그러면 너무 짜증나요 ㅋㅋㅋㅋ ㅜ 찝찝하고",
          "original_form": "시즌 막화에서 다음 내용 여지 막 주면서 그대로 종영해버리고 그러면 너무 짜증나요 ㅋㅋㅋㅋ ㅜ 찝찝하고",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031116.1.28",
          "form": "캐릭터를 계속 볼 수있다는게 장점인데",
          "original_form": "캐릭터를 계속 볼 수있다는게 장점인데",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031116.1.29",
          "form": "이제 그 캐릭터가 내가 알던 그 캐릭터가 아닌...ㅋㅋㅋㅋ",
          "original_form": "이제 그 캐릭터가 내가 알던 그 캐릭터가 아닌...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100031116.1.30",
          "form": "ㅋㅋㅋㅋㅋ맞아요.. 처음의 주인공은 어디 가고.. 넘 달라져있죠",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요.. 처음의 주인공은 어디 가고.. 넘 달라져있죠",
          "speaker_id": "1",
          "time": "20210904 17:15"
        }
      ]
