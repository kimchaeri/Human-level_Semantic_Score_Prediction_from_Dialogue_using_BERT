      "utterance": [
        {
          "id": "MDRW2100026581.1.1",
          "form": "엽떡 잘 드세요?",
          "original_form": "엽떡 잘 드세요?",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026581.1.2",
          "form": "매운거 잘먹는편??",
          "original_form": "매운거 잘먹는편??",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026581.1.3",
          "form": "매운거 좋아해요!!",
          "original_form": "매운거 좋아해요!!",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026581.1.4",
          "form": "입은 버티는데 장이못버틴답니다",
          "original_form": "입은 버티는데 장이못버틴답니다",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026581.1.5",
          "form": "그럼에도 계속 먹어요ㅋㅋ",
          "original_form": "그럼에도 계속 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026581.1.6",
          "form": "ㅋㅋㅋㅋ 매운거 먹으면 스트레스 풀리죠",
          "original_form": "ㅋㅋㅋㅋ 매운거 먹으면 스트레스 풀리죠",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026581.1.7",
          "form": "중독적이고 ㅎ",
          "original_form": "중독적이고 ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026581.1.8",
          "form": "맞아요 중독성이 너무 강해요",
          "original_form": "맞아요 중독성이 너무 강해요",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026581.1.9",
          "form": "불닭볶음면도 좋아하세요?",
          "original_form": "불닭볶음면도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026581.1.10",
          "form": "몸에 안좋은거 알면서 계속 찾아요",
          "original_form": "몸에 안좋은거 알면서 계속 찾아요",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026581.1.11",
          "form": "네네 오늘 저녁에도먹었어요",
          "original_form": "네네 오늘 저녁에도먹었어요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026581.1.12",
          "form": "어제엽떡먹고?!",
          "original_form": "어제엽떡먹고?!",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026581.1.13",
          "form": "아 잠시만 어제 엽떡 드신분 맞나요 지금",
          "original_form": "아 잠시만 어제 엽떡 드신분 맞나요 지금",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026581.1.14",
          "form": "엽떡 얘기를 여러군데에서 해섴ㅋㅋㅋ",
          "original_form": "엽떡 얘기를 여러군데에서 해섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026581.1.15",
          "form": "네네 어제 엽떡먹었어욬ㅋㅋㅋㅋ",
          "original_form": "네네 어제 엽떡먹었어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026581.1.16",
          "form": "역시 국민떡볶이",
          "original_form": "역시 국민떡볶이",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026581.1.17",
          "form": "전 중당추가했던 사람이에요",
          "original_form": "전 중당추가했던 사람이에요",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026581.1.18",
          "form": "중당추가..?",
          "original_form": "중당추가..?",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026581.1.19",
          "form": "중국당면?",
          "original_form": "중국당면?",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026581.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026581.1.21",
          "form": "중국당면 엽떡에 추가하니까 맛있더라고요",
          "original_form": "중국당면 엽떡에 추가하니까 맛있더라고요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026581.1.22",
          "form": "우와...진짜 매울거 같은뎈ㅋㅋㅋㅋㅋ",
          "original_form": "우와...진짜 매울거 같은뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026581.1.23",
          "form": "떡보다 맵지않아요?",
          "original_form": "떡보다 맵지않아요?",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026581.1.24",
          "form": "어제 착한맛먹었어요!!",
          "original_form": "어제 착한맛먹었어요!!",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026581.1.25",
          "form": "떡볶이 얘기하니까 먹고싶네요.. 이밤중에",
