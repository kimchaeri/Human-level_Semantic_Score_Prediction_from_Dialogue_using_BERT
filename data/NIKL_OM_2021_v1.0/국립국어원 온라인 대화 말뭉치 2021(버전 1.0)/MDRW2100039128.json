      "utterance": [
        {
          "id": "MDRW2100039128.1.1",
          "form": "투잡...회사서 허용해주나",
          "original_form": "투잡...회사서 허용해주나",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100039128.1.2",
          "form": "그게 회사마다 다른것 같아요 근데 보통 저 정도는 허락해줄거에요",
          "original_form": "그게 회사마다 다른것 같아요 근데 보통 저 정도는 허락해줄거에요",
          "speaker_id": "1",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100039128.1.3",
          "form": "오...신기하다진짜   .....",
          "original_form": "오...신기하다진짜   .....",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100039128.1.4",
          "form": "되는구나 그게..",
          "original_form": "되는구나 그게..",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100039128.1.5",
          "form": "안되는줄 ...",
          "original_form": "안되는줄 ...",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100039128.1.6",
          "form": "술 집 경영 같이 회사 품위에",
          "original_form": "술 집 경영 같이 회사 품위에",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039128.1.7",
          "form": "영향을 주는 것이나",
          "original_form": "영향을 주는 것이나",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039128.1.8",
          "form": "쇼핑몰 경영 처럼 회사 근무시간에 몰래 투잡하는거",
          "original_form": "쇼핑몰 경영 처럼 회사 근무시간에 몰래 투잡하는거",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039128.1.9",
          "form": "는 금지이고",
          "original_form": "는 금지이고",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039128.1.10",
          "form": "회사 업무에 영향을 주지 않는 선에서는 괜찮을 거에요",
          "original_form": "회사 업무에 영향을 주지 않는 선에서는 괜찮을 거에요",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039128.1.11",
          "form": "앗글쿠나..",
          "original_form": "앗글쿠나..",
          "speaker_id": "2",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100039128.1.12",
          "form": "그럼 막 보험..이런기중복아닌가요",
          "original_form": "그럼 막 보험..이런기중복아닌가요",
