      "utterance": [
        {
          "id": "MDRW2100019209.1.1",
          "form": "자매나 형제 있으세요?",
          "original_form": "자매나 형제 있으세요?",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019209.1.2",
          "form": "외동이에요",
          "original_form": "외동이에요",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019209.1.3",
          "form": "형제있으세요?",
          "original_form": "형제있으세요?",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019209.1.4",
          "form": "전 언니 있어요!",
          "original_form": "전 언니 있어요!",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019209.1.5",
          "form": "외동이면 안외로우신가요 ?",
          "original_form": "외동이면 안외로우신가요 ?",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019209.1.6",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019209.1.7",
          "form": "어렸을땐 좋았는데 나이먹으니 외롭더라구요 ㅜㅜ",
          "original_form": "어렸을땐 좋았는데 나이먹으니 외롭더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019209.1.8",
          "form": "또 있으면 맨날 싸워서 ㅋㅋ 전 외동이 부러웠어요 !",
          "original_form": "또 있으면 맨날 싸워서 ㅋㅋ 전 외동이 부러웠어요 !",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019209.1.9",
          "form": "서로 그러더라구여 반대로 ㅋㅋㅋ",
          "original_form": "서로 그러더라구여 반대로 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019209.1.10",
          "form": "제친구는 자매끼리 놀러다니고 그래서 너무 보기좋더라구요",
          "original_form": "제친구는 자매끼리 놀러다니고 그래서 너무 보기좋더라구요",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019209.1.11",
          "form": "아 같이 놀러가고 그런건 좋아요 ㅋㅋ",
          "original_form": "아 같이 놀러가고 그런건 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019209.1.12",
          "form": "신기해여 어렸을땐 다 싸우더니 ㅋㅋㅋ크니까 사이가 너무 좋아요",
          "original_form": "신기해여 어렸을땐 다 싸우더니 ㅋㅋㅋ크니까 사이가 너무 좋아요",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019209.1.13",
          "form": "뭔가 어릴때는 뭣모르고 싸우다가",
          "original_form": "뭔가 어릴때는 뭣모르고 싸우다가",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019209.1.14",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019209.1.15",
          "form": "크면 사소한거는 그냥 넘기게 돼요 ㅋㅋ",
          "original_form": "크면 사소한거는 그냥 넘기게 돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019209.1.16",
          "form": "아 그냥 내가 참자~ 이런 느낌ㅋㅋ",
          "original_form": "아 그냥 내가 참자~ 이런 느낌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019209.1.17",
          "form": "같이 나이들어가면서 인생의 동반자느낌일거같아요 ㅋㅋㅋ",
          "original_form": "같이 나이들어가면서 인생의 동반자느낌일거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019209.1.18",
          "form": "맞아요 ㅋㅋㅋ 약간 나이들면",
          "original_form": "맞아요 ㅋㅋㅋ 약간 나이들면",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019209.1.19",
          "form": "친구도 다 멀어지고 .. 그러니까 ㅠㅠ",
          "original_form": "친구도 다 멀어지고 .. 그러니까 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019209.1.20",
          "form": "맞아요 슬퍼요 ㅠㅠ 어렸을땐 친구뿐이었는데",
          "original_form": "맞아요 슬퍼요 ㅠㅠ 어렸을땐 친구뿐이었는데",
