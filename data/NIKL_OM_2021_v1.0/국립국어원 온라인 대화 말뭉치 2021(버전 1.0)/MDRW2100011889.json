      "utterance": [
        {
          "id": "MDRW2100011889.1.1",
          "form": "유안 님은 반려동물 키우세요?",
          "original_form": "유안 님은 반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011889.1.2",
          "form": "지금은 안 키워요",
          "original_form": "지금은 안 키워요",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011889.1.3",
          "form": "어떤 동물 키우셨어요?",
          "original_form": "어떤 동물 키우셨어요?",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011889.1.4",
          "form": "햄스터 어릴 때 키웠어요",
          "original_form": "햄스터 어릴 때 키웠어요",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011889.1.5",
          "form": "저도 햄스터 키웠었어요",
          "original_form": "저도 햄스터 키웠었어요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011889.1.6",
          "form": "어떤 종이었어요?",
          "original_form": "어떤 종이었어요?",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011889.1.7",
          "form": "골든이었어요!",
          "original_form": "골든이었어요!",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011889.1.8",
          "form": "오 저도 골든 키웠어요",
          "original_form": "오 저도 골든 키웠어요",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011889.1.9",
          "form": "골든이 뭔가 제가 키웠던 다른 종에 비해서 토실토실했던 것 같아요",
          "original_form": "골든이 뭔가 제가 키웠던 다른 종에 비해서 토실토실했던 것 같아요",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011889.1.10",
          "form": "맞아요ㅋㅋㅋ 아이가 좀 더 큰 사이즈죠",
          "original_form": "맞아요ㅋㅋㅋ 아이가 좀 더 큰 사이즈죠",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100011889.1.11",
          "form": "햄스터는 수명이 짧아서 아쉬워요 키우다보면 금방 늙는 것 같아요",
          "original_form": "햄스터는 수명이 짧아서 아쉬워요 키우다보면 금방 늙는 것 같아요",
          "speaker_id": "2",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100011889.1.12",
          "form": "아이 보낼 때 많이 울었던 기억이 있네요ㅠㅠ 정들었는데 너무 금방 가죠ㅠ",
          "original_form": "아이 보낼 때 많이 울었던 기억이 있네요ㅠㅠ 정들었는데 너무 금방 가죠ㅠ",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100011889.1.13",
          "form": "맞아요 정 들었는데 떠나면 마음이 허해요",
          "original_form": "맞아요 정 들었는데 떠나면 마음이 허해요",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100011889.1.14",
          "form": "그래서 커서는 반려동물을 키운다는 생각을 안 하게 되더라구요ㅠ",
          "original_form": "그래서 커서는 반려동물을 키운다는 생각을 안 하게 되더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100011889.1.15",
          "form": "그쵸 아무래도 책임감이 엄청 필요하니까 저도 못 키울 것 같아요",
          "original_form": "그쵸 아무래도 책임감이 엄청 필요하니까 저도 못 키울 것 같아요",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100011889.1.16",
          "form": "아무래도 생명이니 책임감이 필요하죠ㅠ",
          "original_form": "아무래도 생명이니 책임감이 필요하죠ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:20"
        }
      ]
