      "utterance": [
        {
          "id": "MDRW2100013594.1.1",
          "form": "야식 땡기고 왔어요",
          "original_form": "야식 땡기고 왔어요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100013594.1.2",
          "form": "ㅋㅋㅋㅋㅋ 야식 뭐드셨어요 맛있었겠다",
          "original_form": "ㅋㅋㅋㅋㅋ 야식 뭐드셨어요 맛있었겠다",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100013594.1.3",
          "form": "식빵 버터에 구워먹었어요ㅋㅋ",
          "original_form": "식빵 버터에 구워먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100013594.1.4",
          "form": "야식 먹는 시간에 먹으면 뭐든 존맛이에요",
          "original_form": "야식 먹는 시간에 먹으면 뭐든 존맛이에요",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100013594.1.5",
          "form": "개강 전 마지막 주말인데 오늘도 야식 땡길까요",
          "original_form": "개강 전 마지막 주말인데 오늘도 야식 땡길까요",
          "speaker_id": "2",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100013594.1.6",
          "form": "악 개강 안했으면 좋겟어요",
          "original_form": "악 개강 안했으면 좋겟어요",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100013594.1.7",
          "form": "개강하면 또 스트레스받아서 단거 엄청먹을듯요",
          "original_form": "개강하면 또 스트레스받아서 단거 엄청먹을듯요",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100013594.1.8",
          "form": "저는 스트레스 받으면 매운거 찾는데ㅋㅋ",
          "original_form": "저는 스트레스 받으면 매운거 찾는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100013594.1.9",
          "form": "개강 전 스트레스로 짬뽕 먹었어요",
          "original_form": "개강 전 스트레스로 짬뽕 먹었어요",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100013594.1.10",
          "form": "맞아요 ㅋㅋㅋㅋ 저는 술 엄청먹고 위염걸렷엇어요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 저는 술 엄청먹고 위염걸렷엇어요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100013594.1.11",
          "form": "헉ㅠ 지금은 괜찮으세요?",
          "original_form": "헉ㅠ 지금은 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100013594.1.12",
          "form": "ㅋㅋㅋㅋㅋ네 이제는 나앗는데",
          "original_form": "ㅋㅋㅋㅋㅋ네 이제는 나앗는데",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100013594.1.13",
          "form": "먹을걸로 개강스트레스 푸는거는 조심해야할거같아요",
          "original_form": "먹을걸로 개강스트레스 푸는거는 조심해야할거같아요",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100013594.1.14",
          "form": "ㅠ 슬프네요",
          "original_form": "ㅠ 슬프네요",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100013594.1.15",
          "form": "건강 생각하면서 먹는건 적성에 안맞는데",
          "original_form": "건강 생각하면서 먹는건 적성에 안맞는데",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100013594.1.16",
          "form": "ㅋㅋㅋㅋ근데 이제 나이가... 무섭습니닼ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ근데 이제 나이가... 무섭습니닼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100013594.1.17",
          "form": "나이 얘기 슬퍼요 앞자리 바뀐지 얼마 안돼서",
          "original_form": "나이 얘기 슬퍼요 앞자리 바뀐지 얼마 안돼서",
          "speaker_id": "2",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100013594.1.18",
          "form": "ㅋㅋㅋㅋ ㅜㅜ 저는 이제 슬슬 몸이 못버티고잇어요",
          "original_form": "ㅋㅋㅋㅋ ㅜㅜ 저는 이제 슬슬 몸이 못버티고잇어요",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100013594.1.19",
          "form": "ㅠ 건강 관리 잘하세요",
          "original_form": "ㅠ 건강 관리 잘하세요",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100013594.1.20",
          "form": "야채를 많이 먹고 있어요 ㅋㅋㅋㅋ",
          "original_form": "야채를 많이 먹고 있어요 ㅋㅋㅋㅋ",
