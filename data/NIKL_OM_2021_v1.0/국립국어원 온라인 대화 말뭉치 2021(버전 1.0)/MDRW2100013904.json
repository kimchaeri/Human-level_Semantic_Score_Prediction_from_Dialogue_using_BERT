      "utterance": [
        {
          "id": "MDRW2100013904.1.1",
          "form": "혹시 지금 하시는 일이 어떻게 되세용?",
          "original_form": "혹시 지금 하시는 일이 어떻게 되세용?",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013904.1.2",
          "form": "저는 대학생이에요! name2은용?",
          "original_form": "저는 대학생이에요! &name2&은용?",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013904.1.3",
          "form": "저는 이제 졸업반이에요 ㅠ0ㅜ",
          "original_form": "저는 이제 졸업반이에요 ㅠ0ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013904.1.4",
          "form": "저도 학교 더 다닐래요..",
          "original_form": "저도 학교 더 다닐래요..",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013904.1.5",
          "form": "으아아아아아 ㅠㅠㅠㅠ고생많으셨어용,,,",
          "original_form": "으아아아아아 ㅠㅠㅠㅠ고생많으셨어용,,,",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013904.1.6",
          "form": "알바해보셨나용?",
          "original_form": "알바해보셨나용?",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013904.1.7",
          "form": "넵넵 알바몬 그 자체죠",
          "original_form": "넵넵 알바몬 그 자체죠",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013904.1.8",
          "form": "name1도 알바해보셜나용?",
          "original_form": "&name1&도 알바해보셜나용?",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013904.1.9",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013904.1.10",
          "form": "저는 학원알바 대타로 일주일정도만 한것같아요 ㅎㅎ",
          "original_form": "저는 학원알바 대타로 일주일정도만 한것같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013904.1.11",
          "form": "제일 꿀알바..!! 는 어떤건가용",
          "original_form": "제일 꿀알바..!! 는 어떤건가용",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013904.1.12",
          "form": "저는 근로장학생으로 고등학교 도서관 사서한게 가장 꿀이었어요 ㅋㅋㅋ",
          "original_form": "저는 근로장학생으로 고등학교 도서관 사서한게 가장 꿀이었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013904.1.13",
          "form": "어떤 과목 가르치셨어요??",
          "original_form": "어떤 과목 가르치셨어요??",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013904.1.14",
          "form": "저는 영어 했어요! 중딩이라 쉬웟던것같아용 ㅎㅎ",
          "original_form": "저는 영어 했어요! 중딩이라 쉬웟던것같아용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013904.1.15",
          "form": "오옹 근장 부러워용 ㅠㅠㅠㅠ!!!",
          "original_form": "오옹 근장 부러워용 ㅠㅠㅠㅠ!!!",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013904.1.16",
          "form": "오오 영어 잘하시나부다!",
          "original_form": "오오 영어 잘하시나부다!",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013904.1.17",
          "form": "근장 되게 꿀인데 모르는 친규들이 많아서 안타까워용",
          "original_form": "근장 되게 꿀인데 모르는 친규들이 많아서 안타까워용",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013904.1.18",
          "form": "ㅠㅠ 저희학교는 경쟁률장난아니더라구오",
          "original_form": "ㅠㅠ 저희학교는 경쟁률장난아니더라구오",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013904.1.19",
          "form": "내부는 되게 치열한데 외부는 좀 널널하더라구용",
          "original_form": "내부는 되게 치열한데 외부는 좀 널널하더라구용",
