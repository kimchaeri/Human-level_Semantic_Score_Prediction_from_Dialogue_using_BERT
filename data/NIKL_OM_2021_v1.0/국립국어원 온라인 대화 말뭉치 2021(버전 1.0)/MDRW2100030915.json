      "utterance": [
        {
          "id": "MDRW2100030915.1.1",
          "form": "커뮤니티하시나요?",
          "original_form": "커뮤니티하시나요?",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030915.1.2",
          "form": "아니요..ㅋㅋ",
          "original_form": "아니요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030915.1.3",
          "form": "그럼 보통 인터넷하시면 인스타하시나요??",
          "original_form": "그럼 보통 인터넷하시면 인스타하시나요??",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030915.1.4",
          "form": "뭔가 정보가 많아져서 피곤하더라구요ㅋㅋ 그래서 그냥 요즘은 뭘 안 보려고 노력해요",
          "original_form": "뭔가 정보가 많아져서 피곤하더라구요ㅋㅋ 그래서 그냥 요즘은 뭘 안 보려고 노력해요",
          "speaker_id": "2",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100030915.1.5",
          "form": "그렇긴해요 저는 하긴 하지만 댓글 많으면 제목만 보고 넘겨요",
          "original_form": "그렇긴해요 저는 하긴 하지만 댓글 많으면 제목만 보고 넘겨요",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100030915.1.6",
          "form": "내 주관도 없어지는거같고요",
          "original_form": "내 주관도 없어지는거같고요",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030915.1.7",
          "form": "마자요 너무 잘 휘둘리게 되는 거 같아서",
          "original_form": "마자요 너무 잘 휘둘리게 되는 거 같아서",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030915.1.8",
          "form": "댓글보면 이것도 맞는거 같고 저것도 맞는거 같고요..",
          "original_form": "댓글보면 이것도 맞는거 같고 저것도 맞는거 같고요..",
          "speaker_id": "1",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100030915.1.9",
          "form": "넹 은근 오류도 많아서 ㅋㅋ",
          "original_form": "넹 은근 오류도 많아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:49"
        },
        {
          "id": "MDRW2100030915.1.10",
          "form": "그래서 제 주관 키우기 연습중에요 ㅋㅋ",
          "original_form": "그래서 제 주관 키우기 연습중에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:49"
        },
        {
          "id": "MDRW2100030915.1.11",
          "form": "글쓰기가 좋대요 종이책이랑..",
          "original_form": "글쓰기가 좋대요 종이책이랑..",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100030915.1.12",
          "form": "어떤거하세요?",
          "original_form": "어떤거하세요?",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100030915.1.13",
          "form": "저두 책 읽고 글쓰고",
          "original_form": "저두 책 읽고 글쓰고",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030915.1.14",
          "form": "좋은분들이랑 대화많이 하고",
          "original_form": "좋은분들이랑 대화많이 하고",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030915.1.15",
          "form": "오프라인으로요?",
          "original_form": "오프라인으로요?",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030915.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030915.1.17",
          "form": "오프가 좋긴해요",
          "original_form": "오프가 좋긴해요",
