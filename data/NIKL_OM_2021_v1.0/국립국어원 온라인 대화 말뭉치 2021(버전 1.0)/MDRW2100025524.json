          "speaker_id": "1",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025524.1.3",
          "form": "수업다끝났음?",
          "original_form": "수업다끝났음?",
          "speaker_id": "1",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025524.1.4",
          "form": "끝내긴 했는데 사강 하나 남음",
          "original_form": "끝내긴 했는데 사강 하나 남음",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025524.1.5",
          "form": "녹강하나",
          "original_form": "녹강하나",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025524.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100025524.1.7",
          "form": "다음주면",
          "original_form": "다음주면",
          "speaker_id": "1",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100025524.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100025524.1.9",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "1",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100025524.1.10",
          "form": "그나마녹강있어서",
          "original_form": "그나마녹강있어서",
          "speaker_id": "1",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100025524.1.11",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100025524.1.12",
          "form": "지금 안들어도 되긴하는데",
          "original_form": "지금 안들어도 되긴하는데",
          "speaker_id": "2",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100025524.1.13",
          "form": "시간이 너무 길어서 지금 들을 거임... 듣기만 할래",
          "original_form": "시간이 너무 길어서 지금 들을 거임... 듣기만 할래",
          "speaker_id": "2",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025524.1.14",
          "form": "이번주듣는거임?",
          "original_form": "이번주듣는거임?",
          "speaker_id": "1",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025524.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025524.1.16",
          "form": "그냥틀어놓기만하는더임?",
          "original_form": "그냥틀어놓기만하는더임?",
          "speaker_id": "1",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025524.1.17",
          "form": "교양이라서 괜첞을 듯",
          "original_form": "교양이라서 괜첞을 듯",
          "speaker_id": "2",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025524.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025524.1.19",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025524.1.20",
          "form": "원래녹강은",
          "original_form": "원래녹강은",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025524.1.21",
          "form": "시험기간에몰아듣는거님",
          "original_form": "시험기간에몰아듣는거님",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025524.1.22",
          "form": "아 그러고 싶다ㅡ....",
          "original_form": "아 그러고 싶다ㅡ....",
          "speaker_id": "2",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025524.1.23",
          "form": "교수님 바꿔주세요.......",
          "original_form": "교수님 바꿔주세요.......",
          "speaker_id": "2",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025524.1.24",
          "form": "교수님별로임?",
          "original_form": "교수님별로임?",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025524.1.25",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025524.1.26",
          "form": "아니 좋은데 좋긴 한데",
          "original_form": "아니 좋은데 좋긴 한데",
          "speaker_id": "2",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100025524.1.27",
          "form": "과제를 너무 많이 낼 각이고",
          "original_form": "과제를 너무 많이 낼 각이고",
          "speaker_id": "2",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100025524.1.28",
          "form": "질문도 많을 예정임",
          "original_form": "질문도 많을 예정임",
          "speaker_id": "2",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100025524.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ허긴",
          "original_form": "ㅋㅋㅋㅋㅋㅋ허긴",
          "speaker_id": "1",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100025524.1.30",
          "form": "첫날부터과제면",
          "original_form": "첫날부터과제면",
          "speaker_id": "1",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100025524.1.31",
          "form": "질문은",
          "original_form": "질문은",
          "speaker_id": "1",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100025524.1.32",
          "form": "그니까 너네는 고ㅓ제 아직 안받았냐?",
          "original_form": "그니까 너네는 고ㅓ제 아직 안받았냐?",
          "speaker_id": "2",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100025524.1.33",
          "form": "나아직한과목밖에안들음",
          "original_form": "나아직한과목밖에안들음",
          "speaker_id": "1",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025524.1.34",
          "form": "정정해서",
          "original_form": "정정해서",
          "speaker_id": "1",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025524.1.35",
          "form": "부럽네 과제 안주심? ㄴ왤까",
          "original_form": "부럽네 과제 안주심? ㄴ왤까",
          "speaker_id": "2",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025524.1.36",
          "form": "받아라 과제....",
          "original_form": "받아라 과제....",
          "speaker_id": "2",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025524.1.37",
          "form": "ㅇㅇ원래과제없는과모깅ㅁ",
          "original_form": "ㅇㅇ원래과제없는과모깅ㅁ",
          "speaker_id": "1",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025524.1.38",
          "form": "첫날부터과제읶는",
          "original_form": "첫날부터과제읶는",
          "speaker_id": "1",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025524.1.39",
          "form": "니학과가",
          "original_form": "니학과가",
          "speaker_id": "1",
          "time": "20210901 12:44"
        },
