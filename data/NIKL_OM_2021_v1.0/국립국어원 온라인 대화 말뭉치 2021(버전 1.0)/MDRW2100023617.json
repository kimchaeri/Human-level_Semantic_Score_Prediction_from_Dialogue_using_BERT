      "utterance": [
        {
          "id": "MDRW2100023617.1.1",
          "form": "아ㅋㅋㅋㅋ 아니에용",
          "original_form": "아ㅋㅋㅋㅋ 아니에용",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100023617.1.2",
          "form": "할미견ㅠㅠㅠ",
          "original_form": "할미견ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100023617.1.3",
          "form": "지금 생각해보면 저 닮은거 같아요",
          "original_form": "지금 생각해보면 저 닮은거 같아요",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100023617.1.4",
          "form": "저도 어렸을때부터 애늙은이 소리 많이 들어서 ㅎㅎ",
          "original_form": "저도 어렸을때부터 애늙은이 소리 많이 들어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100023617.1.5",
          "form": "앗 성숙하시니 좋죠! ㅎㅎ",
          "original_form": "앗 성숙하시니 좋죠! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023617.1.6",
          "form": "이젠 진짜 반늙은이ㅋㅋㅋ 가 되서 건강도 신경쓰게 되더라구요",
          "original_form": "이젠 진짜 반늙은이ㅋㅋㅋ 가 되서 건강도 신경쓰게 되더라구요",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100023617.1.7",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100023617.1.8",
          "form": "영양제같은거 챙겨드세요?",
          "original_form": "영양제같은거 챙겨드세요?",
          "speaker_id": "1",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100023617.1.9",
          "form": "종합비타민이랑 프로폴리스요.",
          "original_form": "종합비타민이랑 프로폴리스요.",
          "speaker_id": "2",
          "time": "20210830 13:10"
        },
        {
          "id": "MDRW2100023617.1.10",
          "form": "오오 저는 비타민D 챙겨먹는데",
          "original_form": "오오 저는 비타민D 챙겨먹는데",
          "speaker_id": "1",
          "time": "20210830 13:10"
        },
        {
          "id": "MDRW2100023617.1.11",
          "form": "프로폴리스는 어디에 좋은가용?",
          "original_form": "프로폴리스는 어디에 좋은가용?",
          "speaker_id": "1",
          "time": "20210830 13:10"
        },
        {
          "id": "MDRW2100023617.1.12",
          "form": "면역력에 좋대요",
          "original_form": "면역력에 좋대요",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100023617.1.13",
          "form": "제가 기관지가 좀 안좋은데",
          "original_form": "제가 기관지가 좀 안좋은데",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100023617.1.14",
          "form": "아아 저도 하나 챙겨먹을까 솔깃하네요ㅋㅋㅋ",
          "original_form": "아아 저도 하나 챙겨먹을까 솔깃하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100023617.1.15",
          "form": "액상형 프로폴리스 먹으면 뭔가 목구멍이 소독되는 기분이라 좋아해요",
          "original_form": "액상형 프로폴리스 먹으면 뭔가 목구멍이 소독되는 기분이라 좋아해요",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100023617.1.16",
          "form": "맛은 없어요.... ㅎ",
          "original_form": "맛은 없어요.... ㅎ",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100023617.1.17",
          "form": "아아 액상형으로 나오는구나",
          "original_form": "아아 액상형으로 나오는구나",
          "speaker_id": "1",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023617.1.18",
          "form": "아 맛이 중요한데요ㅠㅠㅠㅋㅋㅋ",
          "original_form": "아 맛이 중요한데요ㅠㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023617.1.19",
          "form": "써요. 좀 많이",
          "original_form": "써요. 좀 많이",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023617.1.20",
          "form": "그래서 소독 기분 드는거죠 ㅋㅋ",
          "original_form": "그래서 소독 기분 드는거죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023617.1.21",
          "form": "아하 그래서 소독이라고 말씀하셨구나ㅋㅋㅋㅋ",
          "original_form": "아하 그래서 소독이라고 말씀하셨구나ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:15"
        }
      ]
