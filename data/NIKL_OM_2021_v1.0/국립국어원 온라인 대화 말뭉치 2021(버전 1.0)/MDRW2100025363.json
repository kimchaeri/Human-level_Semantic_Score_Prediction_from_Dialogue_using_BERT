      "utterance": [
        {
          "id": "MDRW2100025363.1.1",
          "form": "아까 밥 드셨다고 했나요?!",
          "original_form": "아까 밥 드셨다고 했나요?!",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025363.1.2",
          "form": "네 전 이미 밥을 먹었고요",
          "original_form": "네 전 이미 밥을 먹었고요",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025363.1.3",
          "form": "옆에서 제 강아지가 밥 먹고 있네요ㅋㅋㅋㅋ",
          "original_form": "옆에서 제 강아지가 밥 먹고 있네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025363.1.4",
          "form": "오 반려견 키우시나봐요??",
          "original_form": "오 반려견 키우시나봐요??",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025363.1.5",
          "form": "네 키운지 2년 됐어요",
          "original_form": "네 키운지 2년 됐어요",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025363.1.6",
          "form": "오오 부럽네요 저도 강아지 키우고 싶은데 가족들 있어서ㅠㅠ",
          "original_form": "오오 부럽네요 저도 강아지 키우고 싶은데 가족들 있어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025363.1.7",
          "form": "견종은 어떻게 되나요?",
          "original_form": "견종은 어떻게 되나요?",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025363.1.8",
          "form": "아아 그렇구나ㅠㅠ 아무래도 가족들이랑 고민해보는게 중요하죠ㅠㅠ",
          "original_form": "아아 그렇구나ㅠㅠ 아무래도 가족들이랑 고민해보는게 중요하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025363.1.9",
          "form": "저는 비숑 키워요",
          "original_form": "저는 비숑 키워요",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025363.1.10",
          "form": "비숑 귀엽죠ㅠㅠ 털이 복슬거리고",
          "original_form": "비숑 귀엽죠ㅠㅠ 털이 복슬거리고",
          "speaker_id": "1",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025363.1.11",
          "form": "털 안 깎은 비숑 보고 막 털쪘다라고 하더라구요ㅋㅋㅋ",
          "original_form": "털 안 깎은 비숑 보고 막 털쪘다라고 하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025363.1.12",
          "form": "아 맞아요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025363.1.13",
          "form": "살찐게 아니라 털쪘다고ㅋㅋㅋㅋ 너무 웃겼어요",
          "original_form": "살찐게 아니라 털쪘다고ㅋㅋㅋㅋ 너무 웃겼어요",
          "speaker_id": "2",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025363.1.14",
          "form": "그러니까요 ㅎㅎ 동물들도 과하게 살찌지 않게 밥 양조절 해줘야 하나 봐요",
          "original_form": "그러니까요 ㅎㅎ 동물들도 과하게 살찌지 않게 밥 양조절 해줘야 하나 봐요",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025363.1.15",
          "form": "네네 맞아요 사료의 양도 잘 조절해서 급여해야 해요",
          "original_form": "네네 맞아요 사료의 양도 잘 조절해서 급여해야 해요",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025363.1.16",
          "form": "애들도 성인병 걸린대요ㅋㅋㅋㅋㅋ",
          "original_form": "애들도 성인병 걸린대요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025363.1.17",
          "form": "아아ㅋㅋㅋㅋ 되게 유익한 정보였네요 동물들도 살찌면 건강이 안좋아지는구나",
          "original_form": "아아ㅋㅋㅋㅋ 되게 유익한 정보였네요 동물들도 살찌면 건강이 안좋아지는구나",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025363.1.18",
          "form": "넵 맞아요 저희도 건강관리 반려동물도 건강관리 해야 합니다 ㅎㅎ",
          "original_form": "넵 맞아요 저희도 건강관리 반려동물도 건강관리 해야 합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025363.1.19",
          "form": "모두 병에 걸리지 않도록 사람도 강아지도 다이어트~!ㅎ",
          "original_form": "모두 병에 걸리지 않도록 사람도 강아지도 다이어트~!ㅎ",
          "speaker_id": "1",
          "time": "20210901 10:20"
        }
      ]
