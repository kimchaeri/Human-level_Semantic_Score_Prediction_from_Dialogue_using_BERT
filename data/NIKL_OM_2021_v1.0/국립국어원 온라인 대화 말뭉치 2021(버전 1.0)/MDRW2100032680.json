      "utterance": [
        {
          "id": "MDRW2100032680.1.1",
          "form": "결혼 언제 하실 건가요",
          "original_form": "결혼 언제 하실 건가요",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.2",
          "form": "남자친구 있죠",
          "original_form": "남자친구 있죠",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.3",
          "form": "남친 있으시죠?",
          "original_form": "남친 있으시죠?",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.4",
          "form": "님도 있죠?",
          "original_form": "님도 있죠?",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.5",
          "form": "넷플 보자곤 안 하나요?",
          "original_form": "넷플 보자곤 안 하나요?",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.6",
          "form": "요즘 트렌드인데",
          "original_form": "요즘 트렌드인데",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.7",
          "form": "아 가끔해요",
          "original_form": "아 가끔해요",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.8",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.9",
          "form": "제가 쉬운 여자가 아니라서요",
          "original_form": "제가 쉬운 여자가 아니라서요",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.10",
          "form": "안했죠",
          "original_form": "안했죠",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100032680.1.11",
          "form": "본다고",
          "original_form": "본다고",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.12",
          "form": "남친분께",
          "original_form": "남친분께",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.13",
          "form": "재가 잘 아는",
          "original_form": "재가 잘 아는",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.14",
          "form": "티비 브랜드가 있다고",
          "original_form": "티비 브랜드가 있다고",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.15",
          "form": "넷플 보기 좋은",
          "original_form": "넷플 보기 좋은",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.16",
          "form": "스마트 티비",
          "original_form": "스마트 티비",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.17",
          "form": "무슨 소리시죠?",
          "original_form": "무슨 소리시죠?",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.18",
          "form": "전해주시겠어요?",
          "original_form": "전해주시겠어요?",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.19",
          "form": "왜 그래야 하죠?",
          "original_form": "왜 그래야 하죠?",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100032680.1.20",
          "form": "연인끼리 사이 돈독해지라고요",
          "original_form": "연인끼리 사이 돈독해지라고요",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100032680.1.21",
          "form": "저기요?",
          "original_form": "저기요?",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100032680.1.22",
          "form": "아 죄송해요",
          "original_form": "아 죄송해요",
          "speaker_id": "2",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100032680.1.23",
          "form": "넷플 보세요?",
          "original_form": "넷플 보세요?",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032680.1.24",
          "form": "넷플 보라고 격려해 주시네요",
          "original_form": "넷플 보라고 격려해 주시네요",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032680.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 22:17"
        }
