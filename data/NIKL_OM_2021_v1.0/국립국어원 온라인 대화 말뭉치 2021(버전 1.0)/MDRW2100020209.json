      "utterance": [
        {
          "id": "MDRW2100020209.1.1",
          "form": "코코미 모션 봤어?",
          "original_form": "코코미 모션 봤어?",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020209.1.2",
          "form": "아니 안봤.. 다른 친구들 얼굴 찾아보느라.. 감우도 에ㅖ쁘네야",
          "original_form": "아니 안봤.. 다른 친구들 얼굴 찾아보느라.. 감우도 에ㅖ쁘네야",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020209.1.3",
          "form": "모션..",
          "original_form": "모션..",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020209.1.4",
          "form": "코코미 모션이 진짜 예뻐",
          "original_form": "코코미 모션이 진짜 예뻐",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020209.1.5",
          "form": "이 게임 다음버전에서 업데이트 되는데",
          "original_form": "이 게임 다음버전에서 업데이트 되는데",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020209.1.6",
          "form": "여태까지 게임 캐릭터들이 단순히 무기 휘두르는거였다면",
          "original_form": "여태까지 게임 캐릭터들이 단순히 무기 휘두르는거였다면",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020209.1.7",
          "form": "코코미는 효과음도 있고 좀 더 마법같은 분위기임",
          "original_form": "코코미는 효과음도 있고 좀 더 마법같은 분위기임",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020209.1.8",
          "form": "ㅇㅎ 기대해보겠어요..는 뽑아야 하는 거잖어..ㅠ 됐다 인생",
          "original_form": "ㅇㅎ 기대해보겠어요..는 뽑아야 하는 거잖어..ㅠ 됐다 인생",
          "speaker_id": "2",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020209.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020209.1.10",
          "form": "나는 뽑기까지 감안해서 말한거야",
          "original_form": "나는 뽑기까지 감안해서 말한거야",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020209.1.11",
          "form": "이것도 럽프듀처럼 재화 많이 뿌려서",
          "original_form": "이것도 럽프듀처럼 재화 많이 뿌려서",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020209.1.12",
          "form": "무과금으로도 충분히 플레이 할 수 있어",
          "original_form": "무과금으로도 충분히 플레이 할 수 있어",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020209.1.13",
          "form": "코코미는 9월 20일쯤부터 뽑기 시작이라",
          "original_form": "코코미는 9월 20일쯤부터 뽑기 시작이라",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020209.1.14",
          "form": "지금부터 하면 충분해",
          "original_form": "지금부터 하면 충분해",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020209.1.15",
          "form": "ㅇㅎ.. 9월 20일부터면...이나즈미를 그때까지 열어야 한다는 그런건가요 아니면 통합인가..",
          "original_form": "ㅇㅎ.. 9월 20일부터면...이나즈미를 그때까지 열어야 한다는 그런건가요 아니면 통합인가..",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020209.1.16",
          "form": "이나즈마 오픈이랑은 상관없이",
          "original_form": "이나즈마 오픈이랑은 상관없이",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020209.1.17",
          "form": "뽑기 열리기만 하면 가능한데",
          "original_form": "뽑기 열리기만 하면 가능한데",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020209.1.18",
          "form": "과금할거 아니면 무료원석 모아야하니까",
          "original_form": "과금할거 아니면 무료원석 모아야하니까",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020209.1.19",
          "form": "천장이 90연이라 그거만큼 모으려면",
          "original_form": "천장이 90연이라 그거만큼 모으려면",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020209.1.20",
          "form": "상자랑 업적 좀 깨야함",
          "original_form": "상자랑 업적 좀 깨야함",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020209.1.21",
          "form": "아하... 그렇구나 잘 모으고 있어야 한다는 말이네 ㅇㅇ...알겠어 뭔말인지.....(꾸닥",
          "original_form": "아하... 그렇구나 잘 모으고 있어야 한다는 말이네 ㅇㅇ...알겠어 뭔말인지.....(꾸닥",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020209.1.22",
          "form": "일단 처음만 돌려보고 5성 기원하고...",
          "original_form": "일단 처음만 돌려보고 5성 기원하고...",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020209.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020209.1.24",
          "form": "지금 몇레벨이야?",
          "original_form": "지금 몇레벨이야?",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020209.1.25",
          "form": "기원이 연차야?",
          "original_form": "기원이 연차야?",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100020209.1.26",
          "form": "모험이 5랩인거 기사단 들어가서 했는데",
          "original_form": "모험이 5랩인거 기사단 들어가서 했는데",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100020209.1.27",
          "form": "연차래 가챠야?",
          "original_form": "연차래 가챠야?",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100020209.1.28",
          "form": "ㅇㅇ 그거맞아!",
          "original_form": "ㅇㅇ 그거맞아!",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100020209.1.29",
          "form": "초보자 기원이라고 하는 것도",
          "original_form": "초보자 기원이라고 하는 것도",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100020209.1.30",
          "form": "5성 나올 수 있으니까",
          "original_form": "5성 나올 수 있으니까",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100020209.1.31",
          "form": "걔부터 돌려보면 돼",
          "original_form": "걔부터 돌려보면 돼",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100020209.1.32",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100020209.1.33",
          "form": "ㄱ.럴게",
          "original_form": "ㄱ.럴게",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100020209.1.34",
          "form": "진 나왔당",
          "original_form": "진 나왔당",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100020209.1.35",
          "form": "오 축하해",
          "original_form": "오 축하해",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100020209.1.36",
          "form": "5성 확률이 낮은데 한번에 나왔네",
          "original_form": "5성 확률이 낮은데 한번에 나왔네",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100020209.1.37",
          "form": "리세마라 안해도 돼서 다행이야 ㅋㅋㅋㅋ",
          "original_form": "리세마라 안해도 돼서 다행이야 ㅋㅋㅋㅋ",
