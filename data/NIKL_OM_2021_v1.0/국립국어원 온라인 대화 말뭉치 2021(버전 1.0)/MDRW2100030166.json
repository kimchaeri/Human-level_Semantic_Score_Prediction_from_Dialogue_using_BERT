      "utterance": [
        {
          "id": "MDRW2100030166.1.1",
          "form": "귀여운 반려동물 주제로 이야기를 합시다",
          "original_form": "귀여운 반려동물 주제로 이야기를 합시다",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030166.1.2",
          "form": "음~~~~ 굿 초이스",
          "original_form": "음~~~~ 굿 초이스",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030166.1.3",
          "form": "나는... 햄스터가 너무너무 조아",
          "original_form": "나는... 햄스터가 너무너무 조아",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030166.1.4",
          "form": "말랑말랑... 멍청한 햄스터",
          "original_form": "말랑말랑... 멍청한 햄스터",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030166.1.5",
          "form": "...하...나도 어릴때 햄져 키웟었는데",
          "original_form": "...하...나도 어릴때 햄져 키웟었는데",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030166.1.6",
          "form": "멍청한 햄스터보다",
          "original_form": "멍청한 햄스터보다",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030166.1.7",
          "form": "어린 내가 더 바보였다...",
          "original_form": "어린 내가 더 바보였다...",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030166.1.8",
          "form": "그때 그시절에는 햄스터에 대한 상식이 전혀 없었워..",
          "original_form": "그때 그시절에는 햄스터에 대한 상식이 전혀 없었워..",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030166.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠ 어쩔수없지 어렷응께",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠ 어쩔수없지 어렷응께",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030166.1.10",
          "form": "햄스터 다시한번 키우고싶긴해..",
          "original_form": "햄스터 다시한번 키우고싶긴해..",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030166.1.11",
          "form": "응 ㅠㅠ어릴때 유희로 반려동물을 키우면 안됬었는데 말야",
          "original_form": "응 ㅠㅠ어릴때 유희로 반려동물을 키우면 안됬었는데 말야",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030166.1.12",
          "form": "ㅠㅠ 골든 햄스터로... 키우고싶어",
          "original_form": "ㅠㅠ 골든 햄스터로... 키우고싶어",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030166.1.13",
          "form": "아 골든 좋지...",
          "original_form": "아 골든 좋지...",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.14",
          "form": "만약 키운다면 내 방 절반을 내줄거야..",
          "original_form": "만약 키운다면 내 방 절반을 내줄거야..",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.15",
          "form": "멋져.",
          "original_form": "멋져.",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.16",
          "form": "아직까지는 생각없어?",
          "original_form": "아직까지는 생각없어?",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.17",
          "form": "아마 독립해야 키울것같애",
          "original_form": "아마 독립해야 키울것같애",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.18",
          "form": "아아~~ 그렇지",
          "original_form": "아아~~ 그렇지",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.19",
          "form": "아직은 내 방도 좁기때문에....",
          "original_form": "아직은 내 방도 좁기때문에....",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.20",
          "form": "자취벙도 안된다..적어도 투룸에 전세집은 구해야",
          "original_form": "자취벙도 안된다..적어도 투룸에 전세집은 구해야",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.21",
          "form": "반려동물 기를 만 할듯..",
          "original_form": "반려동물 기를 만 할듯..",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030166.1.22",
          "form": "나도 이번에 내 손에 돌아가신 식물들 보면서 깨달은거지만",
          "original_form": "나도 이번에 내 손에 돌아가신 식물들 보면서 깨달은거지만",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030166.1.23",
          "form": "정말...생물을 기른다는건 어려운 일이더라",
          "original_form": "정말...생물을 기른다는건 어려운 일이더라",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030166.1.24",
          "form": "1년넘게 멀쩡하게 있는 마리모가 신기할 지경입니다.",
          "original_form": "1년넘게 멀쩡하게 있는 마리모가 신기할 지경입니다.",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030166.1.25",
          "form": "내 마리모. name3...",
          "original_form": "내 마리모. &name3&...",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030166.1.26",
          "form": "그런데 마리모는 너무 조금씩 자라서 솔직히 크고있는건지 잘 모르겠지만 말입니다.",
          "original_form": "그런데 마리모는 너무 조금씩 자라서 솔직히 크고있는건지 잘 모르겠지만 말입니다.",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030166.1.27",
          "form": "아 ㅋㅋㅠㅠ name3쿤... !!!",
          "original_form": "아 ㅋㅋㅠㅠ &name3&쿤... !!!",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030166.1.28",
          "form": "확실히.. 나 하나 살피기도 힘든데",
          "original_form": "확실히.. 나 하나 살피기도 힘든데",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030166.1.29",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030166.1.30",
          "form": "말도 안통하는 동물 키우는건 더",
          "original_form": "말도 안통하는 동물 키우는건 더",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030166.1.31",
          "form": "어려울것갘다",
          "original_form": "어려울것갘다",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030166.1.32",
          "form": "일하면서 크게 느낀거지만",
          "original_form": "일하면서 크게 느낀거지만",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030166.1.33",
          "form": "정말로 회사 갔다오면 내 시간도 없는데",
          "original_form": "정말로 회사 갔다오면 내 시간도 없는데",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030166.1.34",
          "form": "반려동물.. 특히 손 많이가는 강아지,고영애들 키우는거",
          "original_form": "반려동물.. 특히 손 많이가는 강아지,고영애들 키우는거",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030166.1.35",
          "form": "정말로 불가능. 아닙니까..",
          "original_form": "정말로 불가능. 아닙니까..",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030166.1.36",
          "form": "다들 어떻게 키우시는지 너무 대단해",
          "original_form": "다들 어떻게 키우시는지 너무 대단해",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030166.1.37",
          "form": "ㅠㅠ맞아 백수동거인 없으면 불가능할듯",
          "original_form": "ㅠㅠ맞아 백수동거인 없으면 불가능할듯",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030166.1.38",
          "form": "백수동거인이 필수라니 믿을 수 없군",
          "original_form": "백수동거인이 필수라니 믿을 수 없군",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030166.1.39",
          "form": "그 백수. 정말 희망사항이긴하다.",
          "original_form": "그 백수. 정말 희망사항이긴하다.",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030166.1.40",
          "form": "ㅋ ㅋ ㅋㅋㅋㅋ 본인이 백수가 되고싶으신가요",
          "original_form": "ㅋ ㅋ ㅋㅋㅋㅋ 본인이 백수가 되고싶으신가요",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030166.1.41",
          "form": "ㅋㅋㅋㅋㅋ예!!!",
          "original_form": "ㅋㅋㅋㅋㅋ예!!!",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030166.1.42",
          "form": "고양이는 특히",
          "original_form": "고양이는 특히",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030166.1.43",
          "form": "미야옹철 유투브 맨날 보면서 아는거지만",
          "original_form": "미야옹철 유투브 맨날 보면서 아는거지만",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030166.1.44",
          "form": "운동도 자주 시켜줘야하고...아니 이건 강아지도 똑같네",
          "original_form": "운동도 자주 시켜줘야하고...아니 이건 강아지도 똑같네",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030166.1.45",
          "form": "산책에 고영은 사냥놀이 해줘야하고..",
          "original_form": "산책에 고영은 사냥놀이 해줘야하고..",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030166.1.46",
          "form": "이런 현실적인 어려움때문에 강아지나 고양이는 정말 힘든 것 같아",
          "original_form": "이런 현실적인 어려움때문에 강아지나 고양이는 정말 힘든 것 같아",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030166.1.47",
          "form": "그런 의미에서 케이지에 사는 반려동물 그래도 한번쯤..같이 살아볼만 하다 싶어",
          "original_form": "그런 의미에서 케이지에 사는 반려동물 그래도 한번쯤..같이 살아볼만 하다 싶어",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030166.1.48",
          "form": "ㅠㅠ마자 진짜 꿈의 반려둉물",
          "original_form": "ㅠㅠ마자 진짜 꿈의 반려둉물",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030166.1.49",
          "form": "크...마자",
          "original_form": "크...마자",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030166.1.50",
          "form": "하..어떻게 기르냐",
          "original_form": "하..어떻게 기르냐",
