      "utterance": [
        {
          "id": "MDRW2100029280.1.1",
          "form": "엄마가 내줄테니까 그냥 등록해",
          "original_form": "엄마가 내줄테니까 그냥 등록해",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029280.1.2",
          "form": "11월에 법구공 시험도 있어서 10월까지 다녀보고 더 등록할게",
          "original_form": "11월에 법구공 시험도 있어서 10월까지 다녀보고 더 등록할게",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029280.1.3",
          "form": "그건 시험 뭐뭐보는데",
          "original_form": "그건 시험 뭐뭐보는데",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029280.1.4",
          "form": "과목은 비슷해",
          "original_form": "과목은 비슷해",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029280.1.5",
          "form": "1차가 자소서일뿐",
          "original_form": "1차가 자소서일뿐",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029280.1.6",
          "form": "작년에 서류붙고 시험보러 갔던거??",
          "original_form": "작년에 서류붙고 시험보러 갔던거??",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029280.1.7",
          "form": "응 그거 올해 또 보러가려구",
          "original_form": "응 그거 올해 또 보러가려구",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029280.1.8",
          "form": "힘들면 얘기해",
          "original_form": "힘들면 얘기해",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029280.1.9",
          "form": "혼자서 그러지말고",
          "original_form": "혼자서 그러지말고",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029280.1.10",
          "form": "갠차나...",
          "original_form": "갠차나...",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029280.1.11",
          "form": "이제 별 생각없어",
          "original_form": "이제 별 생각없어",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029280.1.12",
          "form": "어떻게든 되겠지",
          "original_form": "어떻게든 되겠지",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029280.1.13",
          "form": "그래 뭐든 되겠지",
          "original_form": "그래 뭐든 되겠지",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029280.1.14",
          "form": "부담스러우니까 너무 기대는 하지말고...",
          "original_form": "부담스러우니까 너무 기대는 하지말고...",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029280.1.15",
          "form": "알겠어 관심 안 가질게",
          "original_form": "알겠어 관심 안 가질게",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029280.1.16",
          "form": "나중에 다 끝나면 얘기할게",
          "original_form": "나중에 다 끝나면 얘기할게",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029280.1.17",
          "form": "기다릴게",
          "original_form": "기다릴게",
          "speaker_id": "2",
          "time": "20210903 01:05"
        }
      ]
