      "utterance": [
        {
          "id": "MDRW2100016227.1.1",
          "form": "용산도 온라인 처럼 싸면좋겠어요",
          "original_form": "용산도 온라인 처럼 싸면좋겠어요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016227.1.2",
          "form": "예전에 맨날 현금내라",
          "original_form": "예전에 맨날 현금내라",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016227.1.3",
          "form": "안그럼 비싸다",
          "original_form": "안그럼 비싸다",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016227.1.4",
          "form": "그러면서 강매했는데",
          "original_form": "그러면서 강매했는데",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016227.1.5",
          "form": "요샌 안그러겠죠 ㅋㅋ",
          "original_form": "요샌 안그러겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016227.1.6",
          "form": "저 그럼 시대 가보고 올만이네여",
          "original_form": "저 그럼 시대 가보고 올만이네여",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016227.1.7",
          "form": "맞아요! 요즘은 시대가다르니 안그러겠죠? 저도 애들 컴퓨터사줄때 매번 용산가서 사오고그랬었어요^^",
          "original_form": "맞아요! 요즘은 시대가다르니 안그러겠죠? 저도 애들 컴퓨터사줄때 매번 용산가서 사오고그랬었어요^^",
          "speaker_id": "1",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016227.1.8",
          "form": "요새 사고싶은거 없으세요?",
          "original_form": "요새 사고싶은거 없으세요?",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016227.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016227.1.10",
          "form": "전저 살때요 ㅎㅎ",
          "original_form": "전저 살때요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016227.1.11",
          "form": "큰애 살때랑",
          "original_form": "큰애 살때랑",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016227.1.12",
          "form": "아 진짜 오랜만이예요 ㅋㅋ",
          "original_form": "아 진짜 오랜만이예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016227.1.13",
          "form": "요새 저는 건조기? 그거사고싶어요!!",
          "original_form": "요새 저는 건조기? 그거사고싶어요!!",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016227.1.14",
          "form": "요새도 현금 요구할것 같긴한데",
          "original_form": "요새도 현금 요구할것 같긴한데",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016227.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016227.1.16",
          "form": "저 구식이라 ㅜㅜ",
          "original_form": "저 구식이라 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016227.1.17",
          "form": "요즘완전좋아졌던데요?",
          "original_form": "요즘완전좋아졌던데요?",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016227.1.18",
          "form": "냄새나고 옷이 누렇게 돼요",
          "original_form": "냄새나고 옷이 누렇게 돼요",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016227.1.19",
          "form": "그렇데요",
          "original_form": "그렇데요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016227.1.20",
          "form": "이런알 딱 사야하는데",
          "original_form": "이런알 딱 사야하는데",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016227.1.21",
          "form": "요즘 날씨이래서 보송보송 마르지도 않고",
          "original_form": "요즘 날씨이래서 보송보송 마르지도 않고",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016227.1.22",
          "form": "냄새도 나고요",
          "original_form": "냄새도 나고요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016227.1.23",
          "form": "잘못말리면 냄새나고 건조기가 이래서필요하구나 싶었어요",
          "original_form": "잘못말리면 냄새나고 건조기가 이래서필요하구나 싶었어요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016227.1.24",
          "form": "말라도 축축해요",
          "original_form": "말라도 축축해요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016227.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016227.1.26",
          "form": "아 사고싶네요 더더",
          "original_form": "아 사고싶네요 더더",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016227.1.27",
          "form": "가격대가 상당해요",
          "original_form": "가격대가 상당해요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016227.1.28",
          "form": "저희 애아빠 친구는",
          "original_form": "저희 애아빠 친구는",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016227.1.29",
          "form": "그죠 그리고 집에둘곳이없어서 지금은 참고있어요",
          "original_form": "그죠 그리고 집에둘곳이없어서 지금은 참고있어요",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016227.1.30",
          "form": "부인이 옷을 반팔로 만들었대요",
          "original_form": "부인이 옷을 반팔로 만들었대요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016227.1.31",
          "form": "긴팔을 ㅋㅋ",
          "original_form": "긴팔을 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016227.1.32",
          "form": "등산복 같은 재질인데 막돌려서",
          "original_form": "등산복 같은 재질인데 막돌려서",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016227.1.33",
          "form": "저도 둘곳도 없어요",
          "original_form": "저도 둘곳도 없어요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016227.1.34",
          "form": "고건 조심해야 한대요",
          "original_form": "고건 조심해야 한대요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        }
      ]
    }
