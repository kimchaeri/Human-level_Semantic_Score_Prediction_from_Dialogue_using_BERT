      "utterance": [
        {
          "id": "MDRW2100034801.1.1",
          "form": "안녕하세요,  게임 좋아하시나요?",
          "original_form": "안녕하세요,  게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034801.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034801.1.3",
          "form": "모바게임해요",
          "original_form": "모바게임해요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034801.1.4",
          "form": "어떤 게임 주로 하시나요?",
          "original_form": "어떤 게임 주로 하시나요?",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034801.1.5",
          "form": "포켓몬고요 ㅎㅎ",
          "original_form": "포켓몬고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034801.1.6",
          "form": "저 올 초까지 했었어요!",
          "original_form": "저 올 초까지 했었어요!",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034801.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034801.1.8",
          "form": "꽤하셨네요",
          "original_form": "꽤하셨네요",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034801.1.9",
          "form": "뮤츠잡으셧나요",
          "original_form": "뮤츠잡으셧나요",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034801.1.10",
          "form": "그만두기 전에 잡았어요ㅎㅎ 기술은 사이코브레이크, 염동력",
          "original_form": "그만두기 전에 잡았어요ㅎㅎ 기술은 사이코브레이크, 염동력",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034801.1.11",
          "form": "이요! 이로치도 잡았어요ㅎㅎ",
          "original_form": "이요! 이로치도 잡았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034801.1.12",
          "form": "오오 저도요",
          "original_form": "오오 저도요",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034801.1.13",
          "form": "메론배 뮤츠",
          "original_form": "메론배 뮤츠",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034801.1.14",
          "form": "다크라이도 잡으셨나요?",
          "original_form": "다크라이도 잡으셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034801.1.15",
          "form": "아니요ㅜㅜㅜ",
          "original_form": "아니요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034801.1.16",
          "form": "그건언제떳었죠",
          "original_form": "그건언제떳었죠",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034801.1.17",
          "form": "작년에 떴던 것 같아요, 이로치도 풀려서 엄청 잡으러 다녔어요ㅋㅋㅋ",
          "original_form": "작년에 떴던 것 같아요, 이로치도 풀려서 엄청 잡으러 다녔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034801.1.18",
          "form": "ㅜㅜ다크라이 다시와줘ㅜ",
          "original_form": "ㅜㅜ다크라이 다시와줘ㅜ",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034801.1.19",
          "form": "이후에는 그만드셔서 요샌 안하시겠군요",
          "original_form": "이후에는 그만드셔서 요샌 안하시겠군요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034801.1.20",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:56"
        }
      ]
