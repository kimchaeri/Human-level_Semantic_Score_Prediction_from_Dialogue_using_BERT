      "utterance": [
        {
          "id": "MDRW2100034160.1.1",
          "form": "해보고는싶은디",
          "original_form": "해보고는싶은디",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034160.1.2",
          "form": "오 주제가 왔다갔다한다 ㅋㅋㄱ",
          "original_form": "오 주제가 왔다갔다한다 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034160.1.3",
          "form": "아까봉사활동이었는데",
          "original_form": "아까봉사활동이었는데",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034160.1.4",
          "form": "쌀국수 맛있었음 ㅋㅋ",
          "original_form": "쌀국수 맛있었음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100034160.1.5",
          "form": "반미도",
          "original_form": "반미도",
          "speaker_id": "1",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100034160.1.6",
          "form": "쌀국수",
          "original_form": "쌀국수",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100034160.1.7",
          "form": "먹었남?",
          "original_form": "먹었남?",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100034160.1.8",
          "form": "ㅇㅇ미스사이공",
          "original_form": "ㅇㅇ미스사이공",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100034160.1.9",
          "form": "싸고 딱조아",
          "original_form": "싸고 딱조아",
          "speaker_id": "2",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100034160.1.10",
          "form": "신기해 다들 계좌이체한다고하면 어디선가 계좌를 줌 ㅋㅋ",
          "original_form": "신기해 다들 계좌이체한다고하면 어디선가 계좌를 줌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100034160.1.11",
          "form": "반미는,",
          "original_form": "반미는,",
          "speaker_id": "2",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100034160.1.12",
          "form": "난ㅜㅜ또 절제못함",
          "original_form": "난ㅜㅜ또 절제못함",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100034160.1.13",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100034160.1.14",
          "form": "ㅋ사장님들은 그게은근좋은가봐",
          "original_form": "ㅋ사장님들은 그게은근좋은가봐",
          "speaker_id": "2",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100034160.1.15",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100034160.1.16",
          "form": "반미랑 볶음면 둘중하나만 먹을걸",
          "original_form": "반미랑 볶음면 둘중하나만 먹을걸",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100034160.1.17",
          "form": "궁금함을 풀었음 댓지머",
          "original_form": "궁금함을 풀었음 댓지머",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100034160.1.18",
          "form": "반미는 흠 그냥 그래쓰",
          "original_form": "반미는 흠 그냥 그래쓰",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100034160.1.19",
          "form": "구래?",
          "original_form": "구래?",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100034160.1.20",
          "form": "쌀국수가나은가",
          "original_form": "쌀국수가나은가",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100034160.1.21",
          "form": "아니 볶음면 맛이 강해서 묻힘ㅜ",
          "original_form": "아니 볶음면 맛이 강해서 묻힘ㅜ",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100034160.1.22",
          "form": "볶음면을시켜드셨군용",
          "original_form": "볶음면을시켜드셨군용",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100034160.1.23",
          "form": "아님  반미안에 볶음면이들가나?",
          "original_form": "아님  반미안에 볶음면이들가나?",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100034160.1.24",
          "form": "반미랑 볶음면 두개먹음ㅜ",
          "original_form": "반미랑 볶음면 두개먹음ㅜ",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100034160.1.25",
          "form": "하나만 먹을걸",
          "original_form": "하나만 먹을걸",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034160.1.26",
          "form": "ㅋㅋㅋ 삼인분처럼 들리는데..",
          "original_form": "ㅋㅋㅋ 삼인분처럼 들리는데..",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034160.1.27",
          "form": "나는 왜이렇게 먹고 싶은게 많을까ㅜ",
          "original_form": "나는 왜이렇게 먹고 싶은게 많을까ㅜ",
          "speaker_id": "1",
