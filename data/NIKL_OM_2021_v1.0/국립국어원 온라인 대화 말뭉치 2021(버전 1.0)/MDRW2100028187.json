      "utterance": [
        {
          "id": "MDRW2100028187.1.1",
          "form": "정세랑 작가님 영업할래",
          "original_form": "정세랑 작가님 영업할래",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.2",
          "form": "보건교사 안은영 작가님인데 이후로 책 다 읽고 있어",
          "original_form": "보건교사 안은영 작가님인데 이후로 책 다 읽고 있어",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.3",
          "form": "아~ 그분이구나",
          "original_form": "아~ 그분이구나",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.4",
          "form": "이북으로?",
          "original_form": "이북으로?",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.5",
          "form": "샀어...",
          "original_form": "샀어...",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.6",
          "form": "도서관에서 읽다가",
          "original_form": "도서관에서 읽다가",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.7",
          "form": "완전 책으로 산거구나",
          "original_form": "완전 책으로 산거구나",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.8",
          "form": "참지 못하고 몇권 사버렸어",
          "original_form": "참지 못하고 몇권 사버렸어",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028187.1.9",
          "form": "하지만 그만큼 잘쓴 소설이면 소장가치가 있지",
          "original_form": "하지만 그만큼 잘쓴 소설이면 소장가치가 있지",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.10",
          "form": "개인적으로 피프티피플 좋았어",
          "original_form": "개인적으로 피프티피플 좋았어",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.11",
          "form": "난 보건교사 안은영도 본적 없어",
          "original_form": "난 보건교사 안은영도 본적 없어",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.12",
          "form": "왜냐면 나 약간 그런거 좋아하거든",
          "original_form": "왜냐면 나 약간 그런거 좋아하거든",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.13",
          "form": "등장인물이 다른책에서도 등장하거나",
          "original_form": "등장인물이 다른책에서도 등장하거나",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.14",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.15",
          "form": "외전에 모인다거나 그런거",
          "original_form": "외전에 모인다거나 그런거",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.16",
          "form": "약간 메타픽션 좋아하는군",
          "original_form": "약간 메타픽션 좋아하는군",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.17",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028187.1.18",
          "form": "계속 이야기 속에서 살아가는 느낌이라서 좋아",
          "original_form": "계속 이야기 속에서 살아가는 느낌이라서 좋아",
