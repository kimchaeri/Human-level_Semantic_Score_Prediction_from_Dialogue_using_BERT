      "utterance": [
        {
          "id": "MDRW2100042371.1.1",
          "form": "name3는 좀 나아졌나??",
          "original_form": "&name3&는 좀 나아졌나??",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042371.1.2",
          "form": "name3는 뭐 여자네요",
          "original_form": "&name3&는 뭐 여자네요",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042371.1.3",
          "form": "여전해요",
          "original_form": "여전해요",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042371.1.4",
          "form": "근데 이제 name4가 적응을 했는지",
          "original_form": "근데 이제 &name4&가 적응을 했는지",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042371.1.5",
          "form": "재밌다 하대요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "재밌다 하대요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042371.1.6",
          "form": "다행이네 그래도",
          "original_form": "다행이네 그래도",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042371.1.7",
          "form": "재밌다고 하니",
          "original_form": "재밌다고 하니",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.8",
          "form": "우리 name5도 어린이집 보다는",
          "original_form": "우리 &name5&도 어린이집 보다는",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.9",
          "form": "유치원이 더 재밌다고 하더라",
          "original_form": "유치원이 더 재밌다고 하더라",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.10",
          "form": "갈 때 울지 않고 가는 것만으로도",
          "original_form": "갈 때 울지 않고 가는 것만으로도",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.11",
          "form": "감사하다",
          "original_form": "감사하다",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.12",
          "form": "재밌다고 하는데 지금 한 학기가 흘렀네요ㅋㅋㅋㅋ",
          "original_form": "재밌다고 하는데 지금 한 학기가 흘렀네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.13",
          "form": "맞아요 언니 진짜 아침마다 전쟁이었는데",
          "original_form": "맞아요 언니 진짜 아침마다 전쟁이었는데",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.15",
          "form": "요즘에 그냥 군말 없이 나가는 것만 해도 너무 좋아요",
          "original_form": "요즘에 그냥 군말 없이 나가는 것만 해도 너무 좋아요",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.16",
          "form": "그래 name5도 어린이집 갈 때 매일 울었고",
          "original_form": "그래 &name5&도 어린이집 갈 때 매일 울었고",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.17",
          "form": "애들 마다 맞는 곳이 있는가 봄",
          "original_form": "애들 마다 맞는 곳이 있는가 봄",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.18",
          "form": "name6은 2년동안 울었잖아",
          "original_form": "&name6&은 2년동안 울었잖아",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.19",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042371.1.21",
          "form": "레전드",
          "original_form": "레전드",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.22",
          "form": "안 간다고 하는 것만 아니면 다 괜찮음",
          "original_form": "안 간다고 하는 것만 아니면 다 괜찮음",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.23",
          "form": "난 우리 name6이",
          "original_form": "난 우리 &name6&이",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.24",
          "form": "밥을 먹던지 안 먹던지",
          "original_form": "밥을 먹던지 안 먹던지",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.25",
          "form": "학교 가기 싫다는 소리는 안 하니까",
          "original_form": "학교 가기 싫다는 소리는 안 하니까",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.26",
          "form": "정말 다행이다",
          "original_form": "정말 다행이다",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.27",
          "form": "맞아요 학교는 그래도 봐야 한다는 걸 아는 건가",
          "original_form": "맞아요 학교는 그래도 봐야 한다는 걸 아는 건가",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.28",
          "form": "우리 name6은 가기 싫어하는데",
          "original_form": "우리 &name6&은 가기 싫어하는데",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.29",
          "form": "주말만 기다려요",
          "original_form": "주말만 기다려요",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.30",
          "form": "학교가 가기 싫어서 주말만 기다리는 거가",
          "original_form": "학교가 가기 싫어서 주말만 기다리는 거가",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.31",
          "form": "음 주말에 다른 걸 하고 놀고 싶어서 기다리는 거가",
          "original_form": "음 주말에 다른 걸 하고 놀고 싶어서 기다리는 거가",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042371.1.32",
          "form": "학교가 가기 싫어서 주말을 기다리는 거",
          "original_form": "학교가 가기 싫어서 주말을 기다리는 거",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042371.1.33",
          "form": "제일 좋아하는 시간 점심 시간",
          "original_form": "제일 좋아하는 시간 점심 시간",
          "speaker_id": "1",
          "time": "20210906 21:56"
        }
      ]
