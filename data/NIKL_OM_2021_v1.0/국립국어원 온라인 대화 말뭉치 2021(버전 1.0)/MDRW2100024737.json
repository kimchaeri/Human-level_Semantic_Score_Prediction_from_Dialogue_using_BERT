      "utterance": [
        {
          "id": "MDRW2100024737.1.1",
          "form": "주인공 굴린다음에",
          "original_form": "주인공 굴린다음에",
          "speaker_id": "2",
          "time": "20210831 17:55"
        },
        {
          "id": "MDRW2100024737.1.2",
          "form": "와 오메데토! 열심히 하셨군요. 하지만 운명은 바뀌지 않았고 세상은 멸망할거에요",
          "original_form": "와 오메데토! 열심히 하셨군요. 하지만 운명은 바뀌지 않았고 세상은 멸망할거에요",
          "speaker_id": "2",
          "time": "20210831 17:55"
        },
        {
          "id": "MDRW2100024737.1.3",
          "form": "당신의 노력은 세상에 1도 영향을 주지 않앟지만",
          "original_form": "당신의 노력은 세상에 1도 영향을 주지 않앟지만",
          "speaker_id": "2",
          "time": "20210831 17:55"
        },
        {
          "id": "MDRW2100024737.1.4",
          "form": "여튼 노력이 감탄스럽네요",
          "original_form": "여튼 노력이 감탄스럽네요",
          "speaker_id": "2",
          "time": "20210831 17:55"
        },
        {
          "id": "MDRW2100024737.1.5",
          "form": "이따위짓좀",
          "original_form": "이따위짓좀",
          "speaker_id": "2",
          "time": "20210831 17:56"
        },
        {
          "id": "MDRW2100024737.1.6",
          "form": "안했으면",
          "original_form": "안했으면",
          "speaker_id": "2",
          "time": "20210831 17:56"
        },
        {
          "id": "MDRW2100024737.1.7",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ 누구 놀리냐고",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ 누구 놀리냐고",
          "speaker_id": "1",
          "time": "20210831 17:56"
        },
        {
          "id": "MDRW2100024737.1.8",
          "form": "ㄹㅇ....",
          "original_form": "ㄹㅇ....",
          "speaker_id": "2",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024737.1.9",
          "form": "난 운명을 바꾸려고 내 캐릭터를 키운거지",
          "original_form": "난 운명을 바꾸려고 내 캐릭터를 키운거지",
          "speaker_id": "1",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024737.1.10",
          "form": "어차피 결말은 정해져있어",
          "original_form": "어차피 결말은 정해져있어",
          "speaker_id": "1",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024737.1.11",
          "form": "이딴소릴 들으려고",
          "original_form": "이딴소릴 들으려고",
          "speaker_id": "1",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024737.1.12",
          "form": "노가다 뛴게 아니라고;;",
          "original_form": "노가다 뛴게 아니라고;;",
          "speaker_id": "1",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024737.1.13",
          "form": "ㅇㅇ.....캐릭터 개인의 성장이 있었다고?",
          "original_form": "ㅇㅇ.....캐릭터 개인의 성장이 있었다고?",
          "speaker_id": "2",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024737.1.14",
          "form": "그럼 뭐해",
          "original_form": "그럼 뭐해",
          "speaker_id": "2",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024737.1.15",
          "form": "세상은 멸망하고",
          "original_form": "세상은 멸망하고",
          "speaker_id": "2",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024737.1.16",
          "form": "걔네 다 죽는데",
          "original_form": "걔네 다 죽는데",
          "speaker_id": "2",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024737.1.17",
          "form": "ㅇㅇ 젤 무쓸모...",
          "original_form": "ㅇㅇ 젤 무쓸모...",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024737.1.18",
          "form": "아니 애초에 왤케",
          "original_form": "아니 애초에 왤케",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024737.1.19",
          "form": "멸망을 좋어하는거야",
          "original_form": "멸망을 좋어하는거야",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024737.1.20",
          "form": "그만 세상을 멸망시켜 좀",
          "original_form": "그만 세상을 멸망시켜 좀",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024737.1.21",
          "form": "그니까 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024737.1.22",
          "form": "다른 나라 게임이라는걸",
          "original_form": "다른 나라 게임이라는걸",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024737.1.23",
          "form": "이렇게 알게 될 줄은...",
          "original_form": "이렇게 알게 될 줄은...",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024737.1.24",
          "form": "아니 진짜 이상해",
          "original_form": "아니 진짜 이상해",
          "speaker_id": "2",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024737.1.25",
          "form": "아무리 그래도 바로 옆나란데",
          "original_form": "아무리 그래도 바로 옆나란데",
          "speaker_id": "2",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024737.1.26",
          "form": "왜 이렇게까지 감성이 다른거야",
          "original_form": "왜 이렇게까지 감성이 다른거야",
          "speaker_id": "2",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024737.1.27",
          "form": "멸망 좀 그만 좋아해",
          "original_form": "멸망 좀 그만 좋아해",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024737.1.28",
          "form": "당장 신야근을 봐",
          "original_form": "당장 신야근을 봐",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024737.1.29",
          "form": "여주가 신 안한다고 하면",
          "original_form": "여주가 신 안한다고 하면",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024737.1.30",
          "form": "설화계야 망하겠지만",
          "original_form": "설화계야 망하겠지만",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024737.1.31",
          "form": "인간들은 아무 이상 없어",
          "original_form": "인간들은 아무 이상 없어",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024737.1.32",
          "form": "신야근도",
          "original_form": "신야근도",
          "speaker_id": "1",
          "time": "20210831 18:04"
        },
        {
          "id": "MDRW2100024737.1.33",
          "form": "후반부는 아포칼립스이긴 해 ㅋㅋㅋㅋ",
          "original_form": "후반부는 아포칼립스이긴 해 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:04"
        },
        {
          "id": "MDRW2100024737.1.34",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024737.1.35",
          "form": "여튼 난 한국 게임이",
          "original_form": "여튼 난 한국 게임이",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024737.1.36",
          "form": "작정하고 세드로 가겠다고 마음먹은게 아닌 이상",
          "original_form": "작정하고 세드로 가겠다고 마음먹은게 아닌 이상",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024737.1.37",
          "form": "세상이 멸망하는 실은",
          "original_form": "세상이 멸망하는 실은",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024737.1.38",
          "form": "일은",
          "original_form": "일은",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024737.1.39",
          "form": "일어나지 않을거라고 생각하거든",
          "original_form": "일어나지 않을거라고 생각하거든",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024737.1.40",
          "form": "근데 중겜은 세상이 멸망하지만",
          "original_form": "근데 중겜은 세상이 멸망하지만",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.41",
          "form": "우리는 우리의 일상을 살것이고",
          "original_form": "우리는 우리의 일상을 살것이고",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.42",
          "form": "그게 행복이고",
          "original_form": "그게 행복이고",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.43",
          "form": "어쩌고저쩌고",
          "original_form": "어쩌고저쩌고",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.45",
          "form": "여튼 맘에 안들어.....",
          "original_form": "여튼 맘에 안들어.....",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.46",
          "form": "딱히 멸망을 수습할 생각도",
          "original_form": "딱히 멸망을 수습할 생각도",
          "speaker_id": "1",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.47",
          "form": "없어보이지 ㅋㅋㅋㅋ",
          "original_form": "없어보이지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.48",
          "form": "솔직히 저것도 크게 정이 안가",
          "original_form": "솔직히 저것도 크게 정이 안가",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.49",
          "form": "정발한다면",
          "original_form": "정발한다면",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024737.1.50",
          "form": "찍먹은 해보겠지만",
          "original_form": "찍먹은 해보겠지만",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.51",
          "form": "ㄹㅇ 찍어만 먹어볼듯",
          "original_form": "ㄹㅇ 찍어만 먹어볼듯",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.52",
          "form": "솔직히 원신도",
          "original_form": "솔직히 원신도",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.53",
          "form": "이 부분에선",
          "original_form": "이 부분에선",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.54",
          "form": "큰 믿음이 없어",
          "original_form": "큰 믿음이 없어",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.55",
          "form": "당장 세상은 가짜라서",
          "original_form": "당장 세상은 가짜라서",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.56",
          "form": "그 세상을 멸망시키려는쪽이",
          "original_form": "그 세상을 멸망시키려는쪽이",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.57",
          "form": "선인데",
          "original_form": "선인데",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024737.1.58",
          "form": "뭘 기대하겠어",
          "original_form": "뭘 기대하겠어",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024737.1.59",
          "form": "그렇긴 해 ㅋㅋㅋㅋ",
          "original_form": "그렇긴 해 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.60",
          "form": "그래도 수습만 잘 해줘도 평타는 치는데",
          "original_form": "그래도 수습만 잘 해줘도 평타는 치는데",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.61",
          "form": "럽프듀는 스토리 자체가 산으로 가서",
          "original_form": "럽프듀는 스토리 자체가 산으로 가서",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.62",
          "form": "이게 뭔데? 대체 뭔데? 아니 그래서 뭐냐고;;;",
          "original_form": "이게 뭔데? 대체 뭔데? 아니 그래서 뭐냐고;;;",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.63",
          "form": "ㅇㅇ.......",
          "original_form": "ㅇㅇ.......",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.64",
          "form": "만 하다가 끝나니까 문제...",
          "original_form": "만 하다가 끝나니까 문제...",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.65",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.66",
          "form": "걍 한 14챕쯤에서",
          "original_form": "걍 한 14챕쯤에서",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024737.1.67",
          "form": "유연이가 세상을 위해 죽고",
