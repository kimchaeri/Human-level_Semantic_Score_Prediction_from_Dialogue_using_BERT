      "utterance": [
        {
          "id": "MDRW2100032799.1.1",
          "form": "전...제가 사이버가수 덕질하는줄 알았어요.. 보기전엔...",
          "original_form": "전...제가 사이버가수 덕질하는줄 알았어요.. 보기전엔...",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.2",
          "form": "뮤지컬이라니... 찐이시다",
          "original_form": "뮤지컬이라니... 찐이시다",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.3",
          "form": "무슨 아담 덕질하는 것도 아니고..",
          "original_form": "무슨 아담 덕질하는 것도 아니고..",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.4",
          "form": "앜ㅋㅋㅋ 너무 웃겨여",
          "original_form": "앜ㅋㅋㅋ 너무 웃겨여",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.5",
          "form": "투디 덕질하는 것도 아니고..",
          "original_form": "투디 덕질하는 것도 아니고..",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.6",
          "form": "ㅇㅈ이요 쓸디는 봐줘야 제맛",
          "original_form": "ㅇㅈ이요 쓸디는 봐줘야 제맛",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.7",
          "form": "눈물이 주루구구국ㄱ 났었어요 가서..",
          "original_form": "눈물이 주루구구국ㄱ 났었어요 가서..",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.8",
          "form": "아아아악 너무 공감돼여",
          "original_form": "아아아악 너무 공감돼여",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.9",
          "form": "ㅜ 진짜 이 외모를",
          "original_form": "ㅜ 진짜 이 외모를",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032799.1.10",
          "form": "제 눈에 직접 담지 못하고 시간을 낭비하고 이ㅛ는 기",
          "original_form": "제 눈에 직접 담지 못하고 시간을 낭비하고 이ㅛ는 기",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.11",
          "form": "있는 게 한이됩니다ㅠㅠ",
          "original_form": "있는 게 한이됩니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.12",
          "form": "진짜 알죠..",
          "original_form": "진짜 알죠..",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.13",
          "form": "덕후는 ... 울어요..",
          "original_form": "덕후는 ... 울어요..",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.14",
          "form": "코로나 얼른 가버려라...",
          "original_form": "코로나 얼른 가버려라...",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.15",
          "form": "눈치챙겨... 제발",
          "original_form": "눈치챙겨... 제발",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.16",
          "form": "케이팝 돌려내라,,,",
          "original_form": "케이팝 돌려내라,,,",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.17",
          "form": "얼른 다시 덕질해야한다고...",
          "original_form": "얼른 다시 덕질해야한다고...",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.18",
          "form": "진짜... 돌려내...",
          "original_form": "진짜... 돌려내...",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.19",
          "form": "진짜 저 입덕전엔 사실 간잽 정말로 많이했었거든요 엔시티..",
          "original_form": "진짜 저 입덕전엔 사실 간잽 정말로 많이했었거든요 엔시티..",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.20",
          "form": "우리 작소 케이팝...",
          "original_form": "우리 작소 케이팝...",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.21",
          "form": "그때 왜 안했을까요..",
          "original_form": "그때 왜 안했을까요..",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.22",
          "form": "개때리고싶어요 그때의저를..",
          "original_form": "개때리고싶어요 그때의저를..",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032799.1.23",
          "form": "부정기 오래 겪으셧나보내요",
          "original_form": "부정기 오래 겪으셧나보내요",
          "speaker_id": "1",
          "time": "20210905 23:52"
        }
