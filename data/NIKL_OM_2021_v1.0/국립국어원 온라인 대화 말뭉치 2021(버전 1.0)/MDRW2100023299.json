        },
        {
          "id": "MDRW2100023299.1.2",
          "form": "안녕하세요ㅋㅋ",
          "original_form": "안녕하세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100023299.1.3",
          "form": "음악 들으시나요?",
          "original_form": "음악 들으시나요?",
          "speaker_id": "1",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100023299.1.4",
          "form": "네 자주듣죠",
          "original_form": "네 자주듣죠",
          "speaker_id": "2",
          "time": "20210830 12:05"
        },
        {
          "id": "MDRW2100023299.1.5",
          "form": "어디서 들으세요?",
          "original_form": "어디서 들으세요?",
          "speaker_id": "1",
          "time": "20210830 12:09"
        },
        {
          "id": "MDRW2100023299.1.6",
          "form": "멜론이요 거기서 실시간스트리밍듣고있어요",
          "original_form": "멜론이요 거기서 실시간스트리밍듣고있어요",
          "speaker_id": "2",
          "time": "20210830 12:15"
        },
        {
          "id": "MDRW2100023299.1.7",
          "form": "음악 좋아하세요?",
          "original_form": "음악 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 12:15"
        },
        {
          "id": "MDRW2100023299.1.8",
          "form": "오 저도요!",
          "original_form": "오 저도요!",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100023299.1.9",
          "form": "음 요즘에는 운동할때만 듣고있어요",
          "original_form": "음 요즘에는 운동할때만 듣고있어요",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100023299.1.10",
          "form": "오? 뭐들으세요?",
          "original_form": "오? 뭐들으세요?",
          "speaker_id": "2",
          "time": "20210830 12:38"
        },
        {
          "id": "MDRW2100023299.1.11",
          "form": "장르는?",
          "original_form": "장르는?",
          "speaker_id": "2",
          "time": "20210830 12:38"
        },
        {
          "id": "MDRW2100023299.1.12",
          "form": "요즘 일본노래듣고 있어요",
          "original_form": "요즘 일본노래듣고 있어요",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023299.1.13",
          "form": "아라시 아세요?",
          "original_form": "아라시 아세요?",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023299.1.14",
          "form": "알아요! 가끔 듣습니다 노래",
          "original_form": "알아요! 가끔 듣습니다 노래",
          "speaker_id": "2",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100023299.1.15",
          "form": "오 신기해용",
          "original_form": "오 신기해용",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023299.1.16",
          "form": "최근에 알았는데 좋더라구요",
          "original_form": "최근에 알았는데 좋더라구요",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023299.1.17",
          "form": "오? 그렇군요",
          "original_form": "오? 그렇군요",
          "speaker_id": "2",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100023299.1.18",
          "form": "아라시말고는 없으세요?",
          "original_form": "아라시말고는 없으세요?",
          "speaker_id": "2",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100023299.1.19",
          "form": "음 애니 노래도 듣고 있어요ㅋㅋ",
          "original_form": "음 애니 노래도 듣고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023299.1.20",
          "form": "애니노래도 좋은거많죠",
          "original_form": "애니노래도 좋은거많죠",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100023299.1.21",
          "form": "노래 어떤거 들으세요?",
          "original_form": "노래 어떤거 들으세요?",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023299.1.22",
          "form": "다양하게 기분에 맞게듣는거같아요",
          "original_form": "다양하게 기분에 맞게듣는거같아요",
          "speaker_id": "2",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100023299.1.23",
          "form": "꽂히면 그것만듣거든요",
          "original_form": "꽂히면 그것만듣거든요",
          "speaker_id": "2",
