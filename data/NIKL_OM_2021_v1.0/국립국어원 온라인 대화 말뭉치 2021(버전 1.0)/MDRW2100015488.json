      "utterance": [
        {
          "id": "MDRW2100015488.1.1",
          "form": "하나님을 믿습니까",
          "original_form": "하나님을 믿습니까",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015488.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015488.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015488.1.4",
          "form": "무교입니다.",
          "original_form": "무교입니다.",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015488.1.5",
          "form": "초등학교 때는 기독교였는데..",
          "original_form": "초등학교 때는 기독교였는데..",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015488.1.6",
          "form": "아니 저도 무신론자예요",
          "original_form": "아니 저도 무신론자예요",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015488.1.7",
          "form": "아 집안이 기독교에요?",
          "original_form": "아 집안이 기독교에요?",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015488.1.8",
          "form": "그저 초등학교 후문에서 포섭당했을 뿐이에요 초코파이에..",
          "original_form": "그저 초등학교 후문에서 포섭당했을 뿐이에요 초코파이에..",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015488.1.9",
          "form": "친척들은 기독교 많이 있긴 해요!",
          "original_form": "친척들은 기독교 많이 있긴 해요!",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015488.1.10",
          "form": "아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015488.1.11",
          "form": "전 만약한다면 불교?",
          "original_form": "전 만약한다면 불교?",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015488.1.12",
          "form": "산분위기좋은거같아요",
          "original_form": "산분위기좋은거같아요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015488.1.13",
          "form": "오 불교~ 좋죠",
          "original_form": "오 불교~ 좋죠",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015488.1.14",
          "form": "사실 뭔가를 잘못믿겠어요 ㅋㅋㅋ",
          "original_form": "사실 뭔가를 잘못믿겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.15",
          "form": "근데 스님 논란 있었잖아요..",
          "original_form": "근데 스님 논란 있었잖아요..",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.16",
          "form": "신의존재를 잘모르겠는 ㅋㅋㅋ",
          "original_form": "신의존재를 잘모르겠는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.17",
          "form": "혜민 스님이었던가요..",
          "original_form": "혜민 스님이었던가요..",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.18",
          "form": "맞아요 ㅋㄱㅋ그분은",
          "original_form": "맞아요 ㅋㄱㅋ그분은",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.19",
          "form": "패션스님",
          "original_form": "패션스님",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.20",
          "form": "ㅋㅋㅋㅋ종교는 믿진 않은데 종교행사는 잘챙기는 편이에요",
          "original_form": "ㅋㅋㅋㅋ종교는 믿진 않은데 종교행사는 잘챙기는 편이에요",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.21",
          "form": "이를테면 부처님 오신날, 크리스마스랄까..",
          "original_form": "이를테면 부처님 오신날, 크리스마스랄까..",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.22",
          "form": "오 정말요?",
          "original_form": "오 정말요?",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015488.1.23",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.24",
          "form": "저노 그런건 챙기죠",
          "original_form": "저노 그런건 챙기죠",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.26",
          "form": "쉬니까",
          "original_form": "쉬니까",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.28",
          "form": "길거리에 그 포교활동 하는 분들 만나본 적 있어요?",
          "original_form": "길거리에 그 포교활동 하는 분들 만나본 적 있어요?",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.29",
          "form": "전 종교가 어렵다해야하나 공감이 잘안가서",
          "original_form": "전 종교가 어렵다해야하나 공감이 잘안가서",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.30",
          "form": "일명 신천지..",
          "original_form": "일명 신천지..",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.31",
          "form": "너무 싫어해요",
          "original_form": "너무 싫어해요",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.32",
          "form": "도망가요 ㅋㅋㅋㅋ",
          "original_form": "도망가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.33",
          "form": "도망가도 붙잡고 막 끝까지 따라오잖아요",
          "original_form": "도망가도 붙잡고 막 끝까지 따라오잖아요",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.34",
          "form": "말 너무 거시더라",
          "original_form": "말 너무 거시더라",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015488.1.35",
          "form": "저 그럴 때 중국어 해요..",
          "original_form": "저 그럴 때 중국어 해요..",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015488.1.36",
          "form": "ㄹㅇ... 근데 신천지 엄청 많대요 그수가",
          "original_form": "ㄹㅇ... 근데 신천지 엄청 많대요 그수가",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015488.1.37",
          "form": "ㅋㅋㅋㅋ아진짜요?ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아진짜요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015488.1.38",
          "form": "너무 웃기다",
          "original_form": "너무 웃기다",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015488.1.39",
          "form": "네네 그럼 한국말 안통하는 거 알고 바로 휙 가던데요 ㅋㅋㅋ",
          "original_form": "네네 그럼 한국말 안통하는 거 알고 바로 휙 가던데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015488.1.40",
          "form": "아 ㅋㅋㅋㅋㅋ니하오 하나요?",
          "original_form": "아 ㅋㅋㅋㅋㅋ니하오 하나요?",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015488.1.41",
          "form": "아뇨 그냥 워 쓰 중궈런 워 부야오 슈에시 한구어",
          "original_form": "아뇨 그냥 워 쓰 중궈런 워 부야오 슈에시 한구어",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015488.1.42",
          "form": "이런식으로요....ㅋㅋㅋ",
          "original_form": "이런식으로요....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015488.1.43",
          "form": "앜ㅋㅋㅋㅋ저도 써먹어야겠네요 이만희 추종자가 너무 많은거같애요 진짜 ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ저도 써먹어야겠네요 이만희 추종자가 너무 많은거같애요 진짜 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015488.1.44",
          "form": "이만희 너무 무섭게 생기지않았나요?",
          "original_form": "이만희 너무 무섭게 생기지않았나요?",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015488.1.45",
          "form": "얼마 전에 저희 집에까지 찾아와서 종교 포교활동 하더라구요..",
          "original_form": "얼마 전에 저희 집에까지 찾아와서 종교 포교활동 하더라구요..",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015488.1.46",
          "form": "와 집까지는 심하네요",
          "original_form": "와 집까지는 심하네요",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015488.1.47",
          "form": "재택근무 하다가 늦은 점심 먹던 찰나여서 화났어요 ㅜㅜ",
          "original_form": "재택근무 하다가 늦은 점심 먹던 찰나여서 화났어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015488.1.48",
          "form": "그 사람들 툭하면 베풀으래요. 그래서 먼저 저한테 베풀으라고 하면 절대 안베품 ㅠㅠ 이중..",
          "original_form": "그 사람들 툭하면 베풀으래요. 그래서 먼저 저한테 베풀으라고 하면 절대 안베품 ㅠㅠ 이중..",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015488.1.49",
          "form": "화날만도 해요 진짜 ㅋㅋㅋ 너무 심함 그분들",
          "original_form": "화날만도 해요 진짜 ㅋㅋㅋ 너무 심함 그분들",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015488.1.50",
          "form": "꼭 2인 1조로 다니잖아요 이상한 백팩 메고 ㅋㅋ",
          "original_form": "꼭 2인 1조로 다니잖아요 이상한 백팩 메고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015488.1.51",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
