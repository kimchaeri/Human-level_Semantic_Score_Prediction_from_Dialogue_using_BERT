      "utterance": [
        {
          "id": "MDRW2100014927.1.1",
          "form": "해외여행은 어디가보셨어요?",
          "original_form": "해외여행은 어디가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014927.1.2",
          "form": "일본은 오키나와가 엄청좋아요",
          "original_form": "일본은 오키나와가 엄청좋아요",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014927.1.3",
          "form": "저 일본은 안가봤어요!",
          "original_form": "저 일본은 안가봤어요!",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014927.1.4",
          "form": "오키나와는 뭐가 유명해요?",
          "original_form": "오키나와는 뭐가 유명해요?",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014927.1.5",
          "form": "날도좋고 건물도 다낮아서",
          "original_form": "날도좋고 건물도 다낮아서",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014927.1.6",
          "form": "음식도 너무맛있구요\r\n바다...그분위기가 너무좋았어요",
          "original_form": "음식도 너무맛있구요\r\n바다...그분위기가 너무좋았어요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014927.1.7",
          "form": "아하ㅠ저 바다 되게 좋아하는데",
          "original_form": "아하ㅠ저 바다 되게 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014927.1.8",
          "form": "일본바다는 어떤 느낌일지 궁금하네요",
          "original_form": "일본바다는 어떤 느낌일지 궁금하네요",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014927.1.9",
          "form": "잔잔하고...너무좋았어요\r\n그림같이 \r\n색도 .에머랄드빛이요",
          "original_form": "잔잔하고...너무좋았어요\r\n그림같이 \r\n색도 .에머랄드빛이요",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014927.1.10",
          "form": "호오 너무 예쁘겠네요ㅠㅠㅠ",
          "original_form": "호오 너무 예쁘겠네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014927.1.11",
          "form": "음식은 어떤거 드셨어요?",
          "original_form": "음식은 어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014927.1.12",
          "form": "스테이크...와규",
          "original_form": "스테이크...와규",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014927.1.13",
          "form": "싸고 맛났어요",
          "original_form": "싸고 맛났어요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014927.1.14",
          "form": "전통음식도 많이먹었는데 이름을 모르겠네요 ㅠ",
          "original_form": "전통음식도 많이먹었는데 이름을 모르겠네요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014927.1.15",
          "form": "와규ㅠㅠㅠ진짜좋죠",
          "original_form": "와규ㅠㅠㅠ진짜좋죠",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014927.1.16",
          "form": "외국음식은 이름이 낯설어서",
          "original_form": "외국음식은 이름이 낯설어서",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014927.1.17",
          "form": "나중되면 기억잘안나더라고요ㅋㅋㅋ",
          "original_form": "나중되면 기억잘안나더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014927.1.18",
          "form": "네 맞아요 ㅋ",
          "original_form": "네 맞아요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014927.1.19",
          "form": "렌트해서 다녔는데 길거리맥주도 좋았고",
          "original_form": "렌트해서 다녔는데 길거리맥주도 좋았고",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014927.1.20",
          "form": "일본맥주 유명하죠!알차게 즐기셨네요ㅎㅎ",
          "original_form": "일본맥주 유명하죠!알차게 즐기셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014927.1.21",
          "form": "네네 너무맛있었어요",
          "original_form": "네네 너무맛있었어요",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014927.1.22",
          "form": "100엔샵도 가구요 ㅎ",
          "original_form": "100엔샵도 가구요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014927.1.23",
          "form": "오ㅋㅋㅋ구경할거 엄청 많았을 것 같네요",
          "original_form": "오ㅋㅋㅋ구경할거 엄청 많았을 것 같네요",
          "speaker_id": "2",
          "time": "20210825 21:38"
        }
      ]
