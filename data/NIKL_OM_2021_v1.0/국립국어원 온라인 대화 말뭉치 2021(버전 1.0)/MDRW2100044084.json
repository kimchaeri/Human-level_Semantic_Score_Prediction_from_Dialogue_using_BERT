      "utterance": [
        {
          "id": "MDRW2100044084.1.1",
          "form": "할인 정보이런건 어디서 주로 얻으세요?",
          "original_form": "할인 정보이런건 어디서 주로 얻으세요?",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044084.1.2",
          "form": "전 네이버쇼핑에서 최저가로 자주봐여",
          "original_form": "전 네이버쇼핑에서 최저가로 자주봐여",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044084.1.3",
          "form": "name1님은어디서보세요",
          "original_form": "&name1&님은어디서보세요",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044084.1.4",
          "form": "아 근데 그거요즘 좀 이상하지않나요",
          "original_form": "아 근데 그거요즘 좀 이상하지않나요",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044084.1.5",
          "form": "관련없는물건자꾸뜨고",
          "original_form": "관련없는물건자꾸뜨고",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044084.1.6",
          "form": "사실 글케최저가인지모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "사실 글케최저가인지모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044084.1.7",
          "form": "맞아여 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044084.1.8",
          "form": "알리익스프레스 이딴거떠서",
          "original_form": "알리익스프레스 이딴거떠서",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044084.1.9",
          "form": "잘사용했었는데 빡치더라구앋",
          "original_form": "잘사용했었는데 빡치더라구앋",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044084.1.10",
          "form": "아 맞아여 해외구매사이트뜨고 ㅡㅡ",
          "original_form": "아 맞아여 해외구매사이트뜨고 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044084.1.11",
          "form": "어디 중국에서 협찬받았나 왜 쓸데없는 중국사이트를 보여주는지",
          "original_form": "어디 중국에서 협찬받았나 왜 쓸데없는 중국사이트를 보여주는지",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044084.1.12",
          "form": "맞아여ㅜ양만쎄리많이 뜨면 단지",
          "original_form": "맞아여ㅜ양만쎄리많이 뜨면 단지",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044084.1.13",
          "form": "약간 최저가 잘못찾게해서",
          "original_form": "약간 최저가 잘못찾게해서",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044084.1.14",
          "form": "자기들 추천상품으로 유도하려는가싶기도하구요",
          "original_form": "자기들 추천상품으로 유도하려는가싶기도하구요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044084.1.15",
          "form": "찾기만더어려워진ㅜ",
          "original_form": "찾기만더어려워진ㅜ",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044084.1.16",
          "form": "네이버가 편하긴한데 다른 가격비교사이트 찾기도 어렵더라고요",
          "original_form": "네이버가 편하긴한데 다른 가격비교사이트 찾기도 어렵더라고요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044084.1.17",
          "form": "맞아여 그래서 좀불편해도 걍쓰네여",
          "original_form": "맞아여 그래서 좀불편해도 걍쓰네여",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044084.1.18",
          "form": "약간 가격비교사이트의 독점느낌ㅋㅋ",
          "original_form": "약간 가격비교사이트의 독점느낌ㅋㅋ",
