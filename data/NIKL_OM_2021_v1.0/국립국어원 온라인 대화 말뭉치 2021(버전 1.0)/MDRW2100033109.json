        },
        {
          "id": "MDRW2100033109.1.5",
          "form": "어서오십시오...",
          "original_form": "어서오십시오...",
          "speaker_id": "2",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100033109.1.6",
          "form": "최선을 다해 모시겠습니다",
          "original_form": "최선을 다해 모시겠습니다",
          "speaker_id": "2",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100033109.1.7",
          "form": "ㅋㅎㅋㅎㅋㅎㅋㅎㅋㅎㅋㅅㅋㅎㅋㅎㅋㅎㅋ",
          "original_form": "ㅋㅎㅋㅎㅋㅎㅋㅎㅋㅎㅋㅅㅋㅎㅋㅎㅋㅎㅋ",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100033109.1.8",
          "form": "학교생활에 대해",
          "original_form": "학교생활에 대해",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100033109.1.9",
          "form": "말해보래",
          "original_form": "말해보래",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100033109.1.10",
          "form": "등교하는거 너무 힘들어",
          "original_form": "등교하는거 너무 힘들어",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100033109.1.11",
          "form": "수업도 너무 지루해",
          "original_form": "수업도 너무 지루해",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100033109.1.12",
          "form": "그냥 자고싶다",
          "original_form": "그냥 자고싶다",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100033109.1.13",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100033109.1.14",
          "form": "재미없고",
          "original_form": "재미없고",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100033109.1.15",
          "form": "쌤말은 너무 자장가같애",
          "original_form": "쌤말은 너무 자장가같애",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100033109.1.16",
          "form": "자도 아무말 안 하니까 그냥 ....",
          "original_form": "자도 아무말 안 하니까 그냥 ....",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100033109.1.17",
          "form": "드르렁",
          "original_form": "드르렁",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100033109.1.18",
          "form": "맞아.......",
          "original_form": "맞아.......",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100033109.1.19",
          "form": "특히 국어시간",
          "original_form": "특히 국어시간",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100033109.1.20",
          "form": "너무졸려",
          "original_form": "너무졸려",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100033109.1.21",
          "form": "우리 12월달에 동아리 발표회하는데",
          "original_form": "우리 12월달에 동아리 발표회하는데",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100033109.1.22",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033109.1.23",
          "form": "연습하기가",
          "original_form": "연습하기가",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033109.1.24",
          "form": "너무귀찮습니다",
          "original_form": "너무귀찮습니다",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033109.1.25",
          "form": "뭘 발표하는데??",
          "original_form": "뭘 발표하는데??",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033109.1.26",
          "form": "나는 밴드부라서",
          "original_form": "나는 밴드부라서",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033109.1.27",
          "form": "공연해",
          "original_form": "공연해",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033109.1.28",
          "form": "후훗 나는나비 하는데",
          "original_form": "후훗 나는나비 하는데",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
