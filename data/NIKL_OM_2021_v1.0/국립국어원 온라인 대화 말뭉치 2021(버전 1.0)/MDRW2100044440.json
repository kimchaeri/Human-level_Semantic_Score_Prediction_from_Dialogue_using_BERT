      "utterance": [
        {
          "id": "MDRW2100044440.1.1",
          "form": "어 딸이 알려준 거 보는데 재밌더라구",
          "original_form": "어 딸이 알려준 거 보는데 재밌더라구",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.2",
          "form": "어떤가",
          "original_form": "어떤가",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.3",
          "form": "딱 엄마가 좋아하는 취향이야",
          "original_form": "딱 엄마가 좋아하는 취향이야",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.4",
          "form": "별할 거 같았어",
          "original_form": "별할 거 같았어",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.5",
          "form": "다 봤어요",
          "original_form": "다 봤어요",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.6",
          "form": "어 다 봤어",
          "original_form": "어 다 봤어",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.7",
          "form": "쭉 정주행했나보네",
          "original_form": "쭉 정주행했나보네",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.8",
          "form": "그렇게 재미있었어요",
          "original_form": "그렇게 재미있었어요",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.9",
          "form": "응 잘 만들었더라구",
          "original_form": "응 잘 만들었더라구",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.10",
          "form": "엄마는 그런 사극 물이 좋아요",
          "original_form": "엄마는 그런 사극 물이 좋아요",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044440.1.11",
          "form": "엄마는 그런 옛날 게 더 좋더라",
          "original_form": "엄마는 그런 옛날 게 더 좋더라",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044440.1.12",
          "form": "난 사극 물은 재미없어",
          "original_form": "난 사극 물은 재미없어",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044440.1.13",
          "form": "왜 왜 재미없어",
          "original_form": "왜 왜 재미없어",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044440.1.14",
          "form": "나는 그냥 현대물이나 미래 물이 좋더라고",
          "original_form": "나는 그냥 현대물이나 미래 물이 좋더라고",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044440.1.15",
          "form": "옛날 이야기도 재밌지",
          "original_form": "옛날 이야기도 재밌지",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044440.1.16",
          "form": "신기하게 흥미가 가지 않아요",
          "original_form": "신기하게 흥미가 가지 않아요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044440.1.17",
          "form": "엄마랑 딸이랑 취향이 다르네",
          "original_form": "엄마랑 딸이랑 취향이 다르네",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044440.1.18",
          "form": "그런가보네",
          "original_form": "그런가보네",
          "speaker_id": "1",
          "time": "20210907 22:44"
        }
      ]
