      "utterance": [
        {
          "id": "MDRW2100035632.1.1",
          "form": "ㅋㅋㅋ그래서 색깔보고 막 과자사구요",
          "original_form": "ㅋㅋㅋ그래서 색깔보고 막 과자사구요",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.2",
          "form": "일본은",
          "original_form": "일본은",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.3",
          "form": "와사비 간장 에그 김",
          "original_form": "와사비 간장 에그 김",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.4",
          "form": "편의점에서 내려주는 백엔짜리 커피 매일마시고",
          "original_form": "편의점에서 내려주는 백엔짜리 커피 매일마시고",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.5",
          "form": "이런거 진짜 좋아하더라구요ㅋㅋㅋ",
          "original_form": "이런거 진짜 좋아하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.6",
          "form": "아아 맛있겠다!",
          "original_form": "아아 맛있겠다!",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.7",
          "form": "로손 편의점 자주갔는뎅ㅋㅋㅋ",
          "original_form": "로손 편의점 자주갔는뎅ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.8",
          "form": "ㅋㅋ그니까요 약간 밥반찬을 과자로 먹는거 좋아하더라고요",
          "original_form": "ㅋㅋ그니까요 약간 밥반찬을 과자로 먹는거 좋아하더라고요",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.9",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035632.1.10",
          "form": "그리구 일본 편의점에서",
          "original_form": "그리구 일본 편의점에서",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.11",
          "form": "술을 샀는데",
          "original_form": "술을 샀는데",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.13",
          "form": "성인 확인하는게 한국이랑 달라서",
          "original_form": "성인 확인하는게 한국이랑 달라서",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.14",
          "form": "신기했어요",
          "original_form": "신기했어요",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.15",
          "form": "아 그거 일본은",
          "original_form": "아 그거 일본은",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.16",
          "form": "뭐 화면이 뜨더라구요",
          "original_form": "뭐 화면이 뜨더라구요",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.17",
          "form": "사는 사람이 성인이 아닐경우 책임을 묻는 시스템이죠?",
          "original_form": "사는 사람이 성인이 아닐경우 책임을 묻는 시스템이죠?",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.18",
          "form": "네네!ㅋㅋㅋ 그런것같았어요",
          "original_form": "네네!ㅋㅋㅋ 그런것같았어요",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.19",
          "form": "일본어는 못읽지만ㅋㅋㅋ",
          "original_form": "일본어는 못읽지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035632.1.20",
          "form": "ㅋㅋ저도ㅋㅋ",
          "original_form": "ㅋㅋ저도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035632.1.21",
          "form": "커피 하나 주세요 이거 하나만 외우고 무작정 가서 커피달라고 하고 그랬어요ㅋㅋ",
          "original_form": "커피 하나 주세요 이거 하나만 외우고 무작정 가서 커피달라고 하고 그랬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035632.1.22",
          "form": "앜ㅋㅋㅋㅋ 너무 귀여우시네요ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ 너무 귀여우시네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035632.1.23",
          "form": "생활 일본어!ㄴㅋㅋㅋㅋ",
          "original_form": "생활 일본어!ㄴㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:39"
        }
      ]
    }
