        {
          "id": "MDRW2100047436.1.1",
          "form": "맛집 좋아하세요?",
          "original_form": "맛집 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100047436.1.2",
          "form": "아니요ㅎㅎ",
          "original_form": "아니요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100047436.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그럼 주로 요리해서 드세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그럼 주로 요리해서 드세요?",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100047436.1.4",
          "form": "매일 요리해요ㅋㅋ",
          "original_form": "매일 요리해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100047436.1.5",
          "form": "요리고수셧구나",
          "original_form": "요리고수셧구나",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100047436.1.6",
          "form": "어떤 요리가 특기세요? 비법이 궁금합니다",
          "original_form": "어떤 요리가 특기세요? 비법이 궁금합니다",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100047436.1.7",
          "form": "고수는 아니고요ㅋㅋㅋ",
          "original_form": "고수는 아니고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100047436.1.8",
          "form": "저는 냄비밥 잘해요",
          "original_form": "저는 냄비밥 잘해요",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100047436.1.9",
          "form": "냄비밥이요??",
          "original_form": "냄비밥이요??",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100047436.1.10",
          "form": "고수신데요??",
          "original_form": "고수신데요??",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100047436.1.11",
          "form": "해드셔보세요",
          "original_form": "해드셔보세요",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100047436.1.12",
          "form": "밥솥이랑 다른매력이 있어요",
          "original_form": "밥솥이랑 다른매력이 있어요",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100047436.1.13",
          "form": "삼단으로 설익고 타고 그럴까봐 여태 못해봣어요 ㅋㅋㅋㅋ",
          "original_form": "삼단으로 설익고 타고 그럴까봐 여태 못해봣어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100047436.1.14",
          "form": "어떤 요리 잘하세요,ㅈ",
          "original_form": "어떤 요리 잘하세요,ㅈ",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100047436.1.15",
          "form": "저는 간단간단한거밖에 못해요 ㅋㅋㅋ",
          "original_form": "저는 간단간단한거밖에 못해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100047436.1.16",
          "form": "볶음밥 제육볶음 미역국 같은",
          "original_form": "볶음밥 제육볶음 미역국 같은",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100047436.1.17",
          "form": "고기 좋아하세요?",
          "original_form": "고기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100047436.1.18",
          "form": "고기 너무 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "고기 너무 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
