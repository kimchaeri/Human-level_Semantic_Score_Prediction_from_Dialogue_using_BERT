      "utterance": [
        {
          "id": "MDRW2100033350.1.1",
          "form": "요즘 들으시는ㅋㄱㄱㅋㄱㅋㄱㅋㅋ",
          "original_form": "요즘 들으시는ㅋㄱㄱㅋㄱㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100033350.1.2",
          "form": "곡어떤거있으실까요?",
          "original_form": "곡어떤거있으실까요?",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.3",
          "form": "저는~",
          "original_form": "저는~",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.4",
          "form": "남들처럼",
          "original_form": "남들처럼",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.5",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋ인트로가 있으시네요 아버님,,",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋ인트로가 있으시네요 아버님,,",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.6",
          "form": "임영웅 이런거 안듣습니다..",
          "original_form": "임영웅 이런거 안듣습니다..",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.7",
          "form": "저는 요즘 젊은이들이 듣는거",
          "original_form": "저는 요즘 젊은이들이 듣는거",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.8",
          "form": "무족권...",
          "original_form": "무족권...",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.9",
          "form": "젊은 감성 좋아합니다...",
          "original_form": "젊은 감성 좋아합니다...",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033350.1.10",
          "form": "name2님은요~?",
          "original_form": "&name2&님은요~?",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033350.1.11",
          "form": "ㅋㅋㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋ역시,,, 도태되지않으시는,,,",
          "original_form": "ㅋㅋㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋ역시,,, 도태되지않으시는,,,",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033350.1.12",
          "form": "저는 새소년 난춘이라는 곡 아실까요?",
          "original_form": "저는 새소년 난춘이라는 곡 아실까요?",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.13",
          "form": "물론이죠...",
          "original_form": "물론이죠...",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.14",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.15",
          "form": "아휴 젊은날을",
          "original_form": "아휴 젊은날을",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.16",
          "form": "아주 좋아합니다",
          "original_form": "아주 좋아합니다",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.17",
          "form": "떠올리시기에",
          "original_form": "떠올리시기에",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.18",
          "form": "딱좋은 명곡일까요",
          "original_form": "딱좋은 명곡일까요",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.19",
          "form": "그렇습니다 동의합니다",
          "original_form": "그렇습니다 동의합니다",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.20",
          "form": "근데요",
          "original_form": "근데요",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.21",
          "form": "새소년의 자유",
          "original_form": "새소년의 자유",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.22",
          "form": "거기에 유아인을",
          "original_form": "거기에 유아인을",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.24",
          "form": "아이고 도대체 누가 유아인을 뿌렸을까요",
          "original_form": "아이고 도대체 누가 유아인을 뿌렸을까요",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.25",
          "form": "최악으로봅니다",
          "original_form": "최악으로봅니다",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033350.1.26",
          "form": "정말,.",
          "original_form": "정말,.",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.27",
          "form": "그래서 제가 자유를 여태 안듣습니다",
          "original_form": "그래서 제가 자유를 여태 안듣습니다",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.28",
          "form": "대체왜",
          "original_form": "대체왜",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.29",
          "form": "이정도 뚝심은 있어야",
          "original_form": "이정도 뚝심은 있어야",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.30",
          "form": "도태되지 않습니다",
          "original_form": "도태되지 않습니다",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.31",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋ역시,,,올곧은이시대의 어른상이십니다,,,",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋ역시,,,올곧은이시대의 어른상이십니다,,,",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.32",
          "form": "물론입니다",
          "original_form": "물론입니다",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.33",
          "form": "name2님 좋아하시는 황소윤 처자",
          "original_form": "&name2&님 좋아하시는 황소윤 처자",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.34",
          "form": "저도 아주 존경합니다",
          "original_form": "저도 아주 존경합니다",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100033350.1.35",
          "form": "혹시... 처자 워딩도 빻은건가요?",
          "original_form": "혹시... 처자 워딩도 빻은건가요?",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033350.1.36",
          "form": "ㅋㄱㄱㄱㅋㄱㅋㄱㄱ황소윤처자가 중학생들앞에서",
          "original_form": "ㅋㄱㄱㄱㅋㄱㅋㄱㄱ황소윤처자가 중학생들앞에서",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033350.1.37",
          "form": "그렇다면 머리를 조아리며..",
          "original_form": "그렇다면 머리를 조아리며..",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033350.1.38",
          "form": "ㅋㄱㄱㅋㅋㅋㅋ아 그정도는 용인가능합니다",
          "original_form": "ㅋㄱㄱㅋㅋㅋㅋ아 그정도는 용인가능합니다",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033350.1.39",
          "form": "아버님의 열정,,",
          "original_form": "아버님의 열정,,",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033350.1.40",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
