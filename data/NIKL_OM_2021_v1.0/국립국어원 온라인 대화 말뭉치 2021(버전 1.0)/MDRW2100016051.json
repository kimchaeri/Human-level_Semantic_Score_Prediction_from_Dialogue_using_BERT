      "utterance": [
        {
          "id": "MDRW2100016051.1.1",
          "form": "맞다\r\n부모를 닮는거같다 자식은",
          "original_form": "맞다\r\n부모를 닮는거같다 자식은",
          "speaker_id": "2",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016051.1.2",
          "form": "같이 사는세월 무시못한다",
          "original_form": "같이 사는세월 무시못한다",
          "speaker_id": "2",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016051.1.3",
          "form": "그렇지 자식 잘 되라고 우리가 이러는거 아니겠나?",
          "original_form": "그렇지 자식 잘 되라고 우리가 이러는거 아니겠나?",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016051.1.4",
          "form": "맞다 \r\n큰 name3이는 요즘 학원잘다니나",
          "original_form": "맞다 \r\n큰 &name3&이는 요즘 학원잘다니나",
          "speaker_id": "2",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016051.1.5",
          "form": "학원 잘가지 할머니 주말에라도 수술하라고 난리다~ㅋㅋ",
          "original_form": "학원 잘가지 할머니 주말에라도 수술하라고 난리다~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100016051.1.6",
          "form": "공부는 열심히하나",
          "original_form": "공부는 열심히하나",
          "speaker_id": "2",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100016051.1.7",
          "form": "뭐를 잘하는거같아?",
          "original_form": "뭐를 잘하는거같아?",
          "speaker_id": "2",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100016051.1.8",
          "form": "몰라ㅜ",
          "original_form": "몰라ㅜ",
          "speaker_id": "1",
          "time": "20210826 10:04"
        },
        {
          "id": "MDRW2100016051.1.9",
          "form": "맞다 잘하는거 아는것도 큰복인듯해",
          "original_form": "맞다 잘하는거 아는것도 큰복인듯해",
          "speaker_id": "2",
          "time": "20210826 10:05"
        },
        {
          "id": "MDRW2100016051.1.10",
          "form": "그치ㅜ",
          "original_form": "그치ㅜ",
          "speaker_id": "1",
          "time": "20210826 10:08"
        },
        {
          "id": "MDRW2100016051.1.11",
          "form": "빨리 재능을 보이는것도 중요",
          "original_form": "빨리 재능을 보이는것도 중요",
          "speaker_id": "1",
          "time": "20210826 10:08"
        },
        {
          "id": "MDRW2100016051.1.12",
          "form": "너희 아드님은?",
          "original_form": "너희 아드님은?",
          "speaker_id": "1",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016051.1.13",
          "form": "나도 잘모르겠다",
          "original_form": "나도 잘모르겠다",
          "speaker_id": "2",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016051.1.14",
          "form": "수학이 가장 싫단다",
          "original_form": "수학이 가장 싫단다",
          "speaker_id": "2",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016051.1.15",
          "form": "유투브로 요즘...역사 본다 세계사",
          "original_form": "유투브로 요즘...역사 본다 세계사",
          "speaker_id": "2",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016051.1.16",
          "form": "왜?잘하는데....",
          "original_form": "왜?잘하는데....",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016051.1.17",
          "form": "역사랑 어울리긴하다",
          "original_form": "역사랑 어울리긴하다",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016051.1.18",
          "form": "수학은 머리를 너무많이써야한단다",
          "original_form": "수학은 머리를 너무많이써야한단다",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016051.1.19",
          "form": "ㅋㅋ영어는 머리를 그래안써도되고",
          "original_form": "ㅋㅋ영어는 머리를 그래안써도되고",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016051.1.20",
          "form": "질렸나보다ㅜ수학ㆍ",
          "original_form": "질렸나보다ㅜ수학ㆍ",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016051.1.21",
          "form": "그런갑다 ㅋ수학은 혼이많이나서 그런거같애",
          "original_form": "그런갑다 ㅋ수학은 혼이많이나서 그런거같애",
          "speaker_id": "2",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016051.1.22",
          "form": "세계사가 너무잼있는갑드라 유투브로 자주보데 이집트하고..",
          "original_form": "세계사가 너무잼있는갑드라 유투브로 자주보데 이집트하고..",
          "speaker_id": "2",
          "time": "20210826 11:49"
        }
      ]
