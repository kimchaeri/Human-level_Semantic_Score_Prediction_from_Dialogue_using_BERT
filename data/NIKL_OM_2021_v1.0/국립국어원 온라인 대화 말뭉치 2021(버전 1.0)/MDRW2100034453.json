      "utterance": [
        {
          "id": "MDRW2100034453.1.1",
          "form": "아 이제 한다 기다리다 지친다",
          "original_form": "아 이제 한다 기다리다 지친다",
          "speaker_id": "2",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100034453.1.2",
          "form": "그러게 몇위부터 해?",
          "original_form": "그러게 몇위부터 해?",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100034453.1.3",
          "form": "아직 이제 주제 이야기 한다",
          "original_form": "아직 이제 주제 이야기 한다",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100034453.1.4",
          "form": "몇명 그려지네",
          "original_form": "몇명 그려지네",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100034453.1.5",
          "form": "이십세기 힛트쏭",
          "original_form": "이십세기 힛트쏭",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100034453.1.6",
          "form": "10위부터 하네",
          "original_form": "10위부터 하네",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100034453.1.7",
          "form": "아 옛날 가수지",
          "original_form": "아 옛날 가수지",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100034453.1.8",
          "form": "김원준이다 10위가",
          "original_form": "김원준이다 10위가",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100034453.1.9",
          "form": "그럼 예상하기 힘들어지네ㅋ",
          "original_form": "그럼 예상하기 힘들어지네ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100034453.1.10",
          "form": "그러게 10위부터 이러니",
          "original_form": "그러게 10위부터 이러니",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100034453.1.11",
          "form": "아 서태지 신해철 있겠다",
          "original_form": "아 서태지 신해철 있겠다",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100034453.1.12",
          "form": "그럴수도 있겠다 김원준은 춤인가",
          "original_form": "그럴수도 있겠다 김원준은 춤인가",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100034453.1.13",
          "form": "근데 김원준이 작곡을 했나",
          "original_form": "근데 김원준이 작곡을 했나",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100034453.1.14",
          "form": "작사 작곡한 곳이 200곡이상이란다",
          "original_form": "작사 작곡한 곳이 200곡이상이란다",
