      "utterance": [
        {
          "id": "MDRW2100016803.1.1",
          "form": "내 인생 살기도 바쁜데",
          "original_form": "내 인생 살기도 바쁜데",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016803.1.2",
          "form": "쇼핑 하려고",
          "original_form": "쇼핑 하려고",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016803.1.3",
          "form": "돈을 모으는데 쉽지 않네요",
          "original_form": "돈을 모으는데 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016803.1.4",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016803.1.5",
          "form": "일단 집가서 다시 할게",
          "original_form": "일단 집가서 다시 할게",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016803.1.6",
          "form": "그랭",
          "original_form": "그랭",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016803.1.7",
          "form": "야 대박 갤럭시워치 다음모델에",
          "original_form": "야 대박 갤럭시워치 다음모델에",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100016803.1.8",
          "form": "비침습형으로 혈당재는거 들어갈수도있대",
          "original_form": "비침습형으로 혈당재는거 들어갈수도있대",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100016803.1.9",
          "form": "우리 그거 사자",
          "original_form": "우리 그거 사자",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100016803.1.10",
          "form": "얼마쯤 하려나",
          "original_form": "얼마쯤 하려나",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100016803.1.11",
          "form": "근데 나는 갤럭시 워치 이전모델이",
          "original_form": "근데 나는 갤럭시 워치 이전모델이",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100016803.1.12",
          "form": "폰하고 색이 맞아서",
          "original_form": "폰하고 색이 맞아서",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100016803.1.13",
          "form": "버즈랑 다 맞추고 싶었는데",
          "original_form": "버즈랑 다 맞추고 싶었는데",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100016803.1.14",
          "form": "포기했다..",
          "original_form": "포기했다..",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100016803.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100016803.1.16",
          "form": "근데 혈당 재는거 있으면",
          "original_form": "근데 혈당 재는거 있으면",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.17",
          "form": "진짜 편할것 같다",
          "original_form": "진짜 편할것 같다",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.18",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.19",
          "form": "무채혈 혈당측정",
          "original_form": "무채혈 혈당측정",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.20",
          "form": "단 음료 많이 먹어서",
          "original_form": "단 음료 많이 먹어서",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.21",
          "form": "ㄹㅇ혁신인듯",
          "original_form": "ㄹㅇ혁신인듯",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.22",
          "form": "진짜 삼전.. 믿는다",
          "original_form": "진짜 삼전.. 믿는다",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.23",
          "form": "갤럭시 워치랑 또 뭐사지",
          "original_form": "갤럭시 워치랑 또 뭐사지",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.24",
          "form": "응?머가 나븐말이지",
          "original_form": "응?머가 나븐말이지",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.25",
          "form": "애플보다. 먼저내줘 제발",
          "original_form": "애플보다. 먼저내줘 제발",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100016803.1.26",
          "form": "뭐 썼딜래?",
          "original_form": "뭐 썼딜래?",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100016803.1.27",
          "form": "ㅈㅂ",
          "original_form": "ㅈㅂ",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100016803.1.28",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100016803.1.29",
          "form": "아니네?",
          "original_form": "아니네?",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100016803.1.30",
          "form": "모르겟ㄱ",
          "original_form": "모르겟ㄱ",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100016803.1.31",
          "form": "나 카메라도 사고 싶다",
          "original_form": "나 카메라도 사고 싶다",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100016803.1.32",
          "form": "맞아 옛날부터 사고싶다며",
          "original_form": "맞아 옛날부터 사고싶다며",
          "speaker_id": "1",
          "time": "20210826 17:48"
        }
      ]
    }
