      "utterance": [
        {
          "id": "MDRW2100023644.1.1",
          "form": "접때 말한적있는데 괜찮다하더라고?",
          "original_form": "접때 말한적있는데 괜찮다하더라고?",
          "speaker_id": "2",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100023644.1.2",
          "form": "그러고 나 할머니네갔을때 포장해서 먹었어ㅋㅋㅋ",
          "original_form": "그러고 나 할머니네갔을때 포장해서 먹었어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100023644.1.3",
          "form": "아 진짜?ㅋㅋㅋㅋ혼자서?",
          "original_form": "아 진짜?ㅋㅋㅋㅋ혼자서?",
          "speaker_id": "1",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100023644.1.4",
          "form": "지하철타기 괜찮다고?",
          "original_form": "지하철타기 괜찮다고?",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023644.1.5",
          "form": "응ㅋㅋ 자기 올때쯤엔 쟈철 탈만하다해서",
          "original_form": "응ㅋㅋ 자기 올때쯤엔 쟈철 탈만하다해서",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023644.1.6",
          "form": "오 진짜?\n그럼 나중에 한번 부탁해봐야겠넼ㅋㅋ",
          "original_form": "오 진짜?\n그럼 나중에 한번 부탁해봐야겠넼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023644.1.7",
          "form": "응 나도 먹고싶어ㅋㅋ",
          "original_form": "응 나도 먹고싶어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023644.1.8",
          "form": "너가 부탁 좀 해줘봨ㅋㅋㅋㅋ",
          "original_form": "너가 부탁 좀 해줘봨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023644.1.9",
          "form": "그래야겠어ㅋㅋ",
          "original_form": "그래야겠어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023644.1.10",
          "form": "4명모이면 뽕족하나 여기꺼하나 시켜먹을까?",
          "original_form": "4명모이면 뽕족하나 여기꺼하나 시켜먹을까?",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023644.1.11",
          "form": "오 좋아좋아! 최고다",
          "original_form": "오 좋아좋아! 최고다",
          "speaker_id": "1",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023644.1.12",
          "form": "두개다 먹어볼수있겠군",
          "original_form": "두개다 먹어볼수있겠군",
          "speaker_id": "1",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023644.1.13",
          "form": "맞아 비교하면서ㅋㅋ 여기도 막국수없는거같더라",
          "original_form": "맞아 비교하면서ㅋㅋ 여기도 막국수없는거같더라",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023644.1.14",
          "form": "맛집은 막국수가 없는것인가",
          "original_form": "맛집은 막국수가 없는것인가",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.15",
          "form": "아 이런..",
          "original_form": "아 이런..",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.16",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.17",
          "form": "난 막국수 필수인데",
          "original_form": "난 막국수 필수인데",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.18",
          "form": "막국수도 은근 별미인데",
          "original_form": "막국수도 은근 별미인데",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.19",
          "form": "나도 그랬는데 족발이 맛있으면 용서되긴 하더라",
          "original_form": "나도 그랬는데 족발이 맛있으면 용서되긴 하더라",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.20",
          "form": "역시 맛집은 아쉬운게 없겠지ㅠ",
          "original_form": "역시 맛집은 아쉬운게 없겠지ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.21",
          "form": "마자ㅋㅋㅋㅋㄱ",
          "original_form": "마자ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.22",
          "form": "그니까ㅋㅋ 족발만해도 바쁘니",
          "original_form": "그니까ㅋㅋ 족발만해도 바쁘니",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.23",
          "form": "재료마감도 일찍 하자너ㅠㅠ",
          "original_form": "재료마감도 일찍 하자너ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023644.1.24",
          "form": "막국수에 쏟을정성 족발에 쏟아야지",
          "original_form": "막국수에 쏟을정성 족발에 쏟아야지",
          "speaker_id": "1",
          "time": "20210830 13:43"
        }
      ]
    }
