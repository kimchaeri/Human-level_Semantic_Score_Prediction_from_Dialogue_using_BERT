      "utterance": [
        {
          "id": "MDRW2100007095.1.1",
          "form": "돌고래가",
          "original_form": "돌고래가",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007095.1.2",
          "form": "네네",
