      "utterance": [
        {
          "id": "MDRW2100037438.1.1",
          "form": "영화 자주 보시나요?",
          "original_form": "영화 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210831 23:52"
        },
        {
          "id": "MDRW2100037438.1.2",
          "form": "아니요 영화값이 너무 올라서 잘 안보게돼요",
          "original_form": "아니요 영화값이 너무 올라서 잘 안보게돼요",
          "speaker_id": "2",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100037438.1.3",
          "form": "자주 보셔요?",
          "original_form": "자주 보셔요?",
          "speaker_id": "2",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100037438.1.4",
          "form": "저도 자주는 아닌데 넷플이나 웨이브로 종종 보고있어요!",
          "original_form": "저도 자주는 아닌데 넷플이나 웨이브로 종종 보고있어요!",
          "speaker_id": "1",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100037438.1.5",
          "form": "최근엔 어떤거 보셨나요?",
          "original_form": "최근엔 어떤거 보셨나요?",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100037438.1.6",
          "form": "키씽부스요!",
          "original_form": "키씽부스요!",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100037438.1.7",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100037438.1.8",
          "form": "로맨틱코미디 영화 아닌가요?",
          "original_form": "로맨틱코미디 영화 아닌가요?",
          "speaker_id": "2",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100037438.1.9",
          "form": "기억이 가물가물해요 들어봤는데...",
          "original_form": "기억이 가물가물해요 들어봤는데...",
          "speaker_id": "2",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100037438.1.10",
          "form": "맞아요! 3가 나왔더라구요",
          "original_form": "맞아요! 3가 나왔더라구요",
          "speaker_id": "1",
          "time": "20210901 07:22"
        },
        {
          "id": "MDRW2100037438.1.11",
          "form": "로코 좋아하시나보디",
          "original_form": "로코 좋아하시나보디",
          "speaker_id": "2",
          "time": "20210901 08:54"
        },
        {
          "id": "MDRW2100037438.1.12",
          "form": "넹~",
          "original_form": "넹~",
          "speaker_id": "1",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100037438.1.13",
          "form": "name2님은 어떤장르 좋아하세요?",
          "original_form": "&name2&님은 어떤장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100037438.1.14",
          "form": "저는 드라마...? 아련하게 엔딩난 영화들 좋아해요",
          "original_form": "저는 드라마...? 아련하게 엔딩난 영화들 좋아해요",
          "speaker_id": "2",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100037438.1.15",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "1",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100037438.1.16",
          "form": "어떤거 제일 좋아하세요?",
          "original_form": "어떤거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100037438.1.17",
          "form": "왕의남자, 센치로, 이정도만 생각나요",
          "original_form": "왕의남자, 센치로, 이정도만 생각나요",
          "speaker_id": "2",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100037438.1.18",
          "form": "센치로 저도 좋아해요",
          "original_form": "센치로 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 09:48"
        },
        {
          "id": "MDRW2100037438.1.19",
          "form": "지브리",
          "original_form": "지브리",
          "speaker_id": "1",
          "time": "20210901 09:48"
        },
        {
          "id": "MDRW2100037438.1.20",
          "form": "지브리좋죠 특유의 분위기가 좋아요",
          "original_form": "지브리좋죠 특유의 분위기가 좋아요",
          "speaker_id": "2",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100037438.1.21",
          "form": "주기적으로 찾아보게 돼요",
          "original_form": "주기적으로 찾아보게 돼요",
          "speaker_id": "2",
          "time": "20210901 09:56"
        }
