        },
        {
          "id": "MDRW2100038767.1.2",
          "form": "저는 다리 윙만 주문하는 것으로",
          "original_form": "저는 다리 윙만 주문하는 것으로",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100038767.1.3",
          "form": "타협했죠 그래서",
          "original_form": "타협했죠 그래서",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100038767.1.4",
          "form": "저희가족 다 다리윙만 좋아해서",
          "original_form": "저희가족 다 다리윙만 좋아해서",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100038767.1.5",
          "form": "오~~~윙이랑 다리없으믄 치킨 맛이 없죠",
          "original_form": "오~~~윙이랑 다리없으믄 치킨 맛이 없죠",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038767.1.6",
          "form": "마자요!!! 퍽퍽살 타도",
          "original_form": "마자요!!! 퍽퍽살 타도",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100038767.1.7",
          "form": "퍽퍽살은 왜 있는겨",
          "original_form": "퍽퍽살은 왜 있는겨",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038767.1.8",
          "form": "치킨중에서 이해안가는맛",
          "original_form": "치킨중에서 이해안가는맛",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038767.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038767.1.10",
          "form": "닭에게도 가슴은 있어야해요...",
          "original_form": "닭에게도 가슴은 있어야해요...",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038767.1.11",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038767.1.12",
