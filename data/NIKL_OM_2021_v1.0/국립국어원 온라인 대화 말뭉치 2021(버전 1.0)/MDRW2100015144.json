        {
          "id": "MDRW2100015144.1.1",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015144.1.2",
          "form": "매운게 좋지않고 성가시게 느껴졌어요ㅋㅋㅋㅋㅋ",
          "original_form": "매운게 좋지않고 성가시게 느껴졌어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015144.1.3",
          "form": "카레가 매운가요?",
          "original_form": "카레가 매운가요?",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015144.1.4",
          "form": "거기",
          "original_form": "거기",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015144.1.5",
          "form": "단계조절할수있어요",
          "original_form": "단계조절할수있어요",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015144.1.6",
          "form": "그머지 카레매운맛은 또 다르잖아요 그져",
          "original_form": "그머지 카레매운맛은 또 다르잖아요 그져",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015144.1.7",
          "form": "ㅋㅋ순하게 먹을껄! 폭풍후회",
          "original_form": "ㅋㅋ순하게 먹을껄! 폭풍후회",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015144.1.8",
          "form": "ㅋㅋㅋㅋ나중에 가게된다면 참고할게요",
          "original_form": "ㅋㅋㅋㅋ나중에 가게된다면 참고할게요",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015144.1.9",
          "form": "매운건 떡볶이가 진리,,",
          "original_form": "매운건 떡볶이가 진리,,",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015144.1.10",
          "form": "ㅋㅋㅋ 엽떡 이런거요?",
          "original_form": "ㅋㅋㅋ 엽떡 이런거요?",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015144.1.11",
          "form": "그렇죠오",
          "original_form": "그렇죠오",
          "speaker_id": "2",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015144.1.12",
          "form": "저는 청년다방이 젤 맛있는거 같아요",
          "original_form": "저는 청년다방이 젤 맛있는거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015144.1.13",
          "form": "그래요 ? 안먹어봤는데",
          "original_form": "그래요 ? 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015144.1.14",
          "form": "맛있게맵고 그런가요",
          "original_form": "맛있게맵고 그런가요",
          "speaker_id": "2",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015144.1.15",
          "form": "앗 그래요? 싼건아닌데",
          "original_form": "앗 그래요? 싼건아닌데",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015144.1.16",
          "form": "오징어랑 차돌박이 떡볶이 맛있습니다",
          "original_form": "오징어랑 차돌박이 떡볶이 맛있습니다",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015144.1.17",
          "form": "휴,,듣기만해도 맛있을거같아요",
          "original_form": "휴,,듣기만해도 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015144.1.18",
          "form": "체인점이니까 어디 있을겁니다 아마도",
