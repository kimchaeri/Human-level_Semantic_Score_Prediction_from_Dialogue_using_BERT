        },
        {
          "id": "MDRW2100017054.1.2",
          "form": "배가 고프진 않은데",
          "original_form": "배가 고프진 않은데",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.3",
          "form": "나도 배 안 고프다",
          "original_form": "나도 배 안 고프다",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.4",
          "form": "그래도 뭘 먹어야지용",
          "original_form": "그래도 뭘 먹어야지용",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.5",
          "form": "그래 딸내미가 요리해 봐라",
          "original_form": "그래 딸내미가 요리해 봐라",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.6",
          "form": "그를까?",
          "original_form": "그를까?",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.7",
          "form": "그래 안 딴 걸로",
          "original_form": "그래 안 딴 걸로",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.8",
          "form": "음 한번 생각해보고 ㅋㅋ",
          "original_form": "음 한번 생각해보고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.9",
          "form": "아니면 두부 먹든지",
          "original_form": "아니면 두부 먹든지",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.10",
          "form": "두부? 두부 요리를 해야겠네",
          "original_form": "두부? 두부 요리를 해야겠네",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017054.1.11",
          "form": "찌개를 만들던지",
