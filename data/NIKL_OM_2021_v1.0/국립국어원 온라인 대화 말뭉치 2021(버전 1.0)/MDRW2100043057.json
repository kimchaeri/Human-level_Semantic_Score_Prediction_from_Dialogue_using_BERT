      "utterance": [
        {
          "id": "MDRW2100043057.1.1",
          "form": "재난 지원금은 신청하셨나요?",
          "original_form": "재난 지원금은 신청하셨나요?",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043057.1.2",
          "form": "앗 아니요! 신청하셨나요?",
          "original_form": "앗 아니요! 신청하셨나요?",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043057.1.3",
          "form": "아뇨 이것도 대상이 아닌 것 같더라구요ㅋㅋ 얼른 신청해보세요!!",
          "original_form": "아뇨 이것도 대상이 아닌 것 같더라구요ㅋㅋ 얼른 신청해보세요!!",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043057.1.4",
          "form": "ㅎㅎ앗..사실저두요",
          "original_form": "ㅎㅎ앗..사실저두요",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043057.1.5",
          "form": "앗 그렇군요ㅋ큐ㅠ 부모님이 정말 열심히 일하시는 ㅠ",
          "original_form": "앗 그렇군요ㅋ큐ㅠ 부모님이 정말 열심히 일하시는 ㅠ",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043057.1.6",
          "form": "이게 가구당이어서...",
          "original_form": "이게 가구당이어서...",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043057.1.7",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043057.1.8",
          "form": "아쉽지만 더 필요한 분들께 가야죠ㅠㅜ",
          "original_form": "아쉽지만 더 필요한 분들께 가야죠ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043057.1.9",
          "form": "맞아요! 그래도 전 경기도민이어서",
          "original_form": "맞아요! 그래도 전 경기도민이어서",
          "speaker_id": "2",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043057.1.10",
          "form": "곧 신청할수있지 않을까 싶습니당",
          "original_form": "곧 신청할수있지 않을까 싶습니당",
          "speaker_id": "2",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043057.1.11",
          "form": "혹시 부모님과 같이 사시나요?",
          "original_form": "혹시 부모님과 같이 사시나요?",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043057.1.12",
          "form": "네! 같이 거주 중이에요!",
          "original_form": "네! 같이 거주 중이에요!",
          "speaker_id": "1",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100043057.1.13",
          "form": "아 그러시군요! 혹시 맡고 계신 집안일이 있으실까요?:)",
          "original_form": "아 그러시군요! 혹시 맡고 계신 집안일이 있으실까요?:)",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100043057.1.14",
          "form": "저는 분리수거를 담당하고 있습니당",
          "original_form": "저는 분리수거를 담당하고 있습니당",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100043057.1.15",
          "form": "헉 딱히 담당은 없고 이것저것 해요",
          "original_form": "헉 딱히 담당은 없고 이것저것 해요",
          "speaker_id": "1",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043057.1.16",
          "form": "ㅎㅎ아 저도 다른일도 하긴하는데..분리수거는 제가 다 하고 있어요",
          "original_form": "ㅎㅎ아 저도 다른일도 하긴하는데..분리수거는 제가 다 하고 있어요",
          "speaker_id": "2",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043057.1.17",
          "form": "혹시 가사 중에 좋아하시는 일이 있으신가요? 요리라던지",
          "original_form": "혹시 가사 중에 좋아하시는 일이 있으신가요? 요리라던지",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043057.1.18",
          "form": "우와 멋지셔요... 좋아하는거.. 저는 움직이는 걸 너무 귀찮아해서... 맨날 잔소리 듣고 하는 편입니다... 불효...",
          "original_form": "우와 멋지셔요... 좋아하는거.. 저는 움직이는 걸 너무 귀찮아해서... 맨날 잔소리 듣고 하는 편입니다... 불효...",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043057.1.19",
          "form": "앗...아니에욥 ㅎㅎ 저도 잔소리 많이 듣고 있습니당..",
          "original_form": "앗...아니에욥 ㅎㅎ 저도 잔소리 많이 듣고 있습니당..",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043057.1.20",
          "form": "아 이제 곧 추석인데",
          "original_form": "아 이제 곧 추석인데",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043057.1.21",
          "form": "가족끼리 모이시나요?:)",
          "original_form": "가족끼리 모이시나요?:)",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043057.1.22",
          "form": "코로나 때문에 어떻게 될 지 모르겠어요ㅜㅜ 모일 거 같기도 하구..",
          "original_form": "코로나 때문에 어떻게 될 지 모르겠어요ㅜㅜ 모일 거 같기도 하구..",
          "speaker_id": "1",
          "time": "20210907 13:51"
        }
      ]
    }
  ]
