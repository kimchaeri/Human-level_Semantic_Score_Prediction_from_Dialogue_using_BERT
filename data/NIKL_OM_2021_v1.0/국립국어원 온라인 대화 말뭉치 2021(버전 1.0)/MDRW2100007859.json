      "utterance": [
        {
          "id": "MDRW2100007859.1.1",
          "form": "아 팔꿈치가 계속 쑤시네",
          "original_form": "아 팔꿈치가 계속 쑤시네",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007859.1.2",
          "form": "내일은 병원 가봐",
          "original_form": "내일은 병원 가봐",
          "speaker_id": "2",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007859.1.3",
          "form": "건강을 챙깁시다!",
          "original_form": "건강을 챙깁시다!",
          "speaker_id": "2",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007859.1.4",
          "form": "그러게 내일은 병원에 가서 약을 타오든가",
          "original_form": "그러게 내일은 병원에 가서 약을 타오든가",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007859.1.5",
          "form": "테니스엘보 관련 운동치료 영상 봐야겠어",
          "original_form": "테니스엘보 관련 운동치료 영상 봐야겠어",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007859.1.6",
          "form": "넌 어디 아픈데 없어?",
          "original_form": "넌 어디 아픈데 없어?",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007859.1.7",
          "form": "그냥 피곤한 것  빼고는",
          "original_form": "그냥 피곤한 것  빼고는",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007859.1.8",
          "form": "특별히 아픈 건 없음",
          "original_form": "특별히 아픈 건 없음",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007859.1.9",
          "form": "다행이구먼",
          "original_form": "다행이구먼",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007859.1.10",
          "form": "타자치고 늦게까지 핸드폰 들고 그래서 팔에 무리가 왔나벼",
          "original_form": "타자치고 늦게까지 핸드폰 들고 그래서 팔에 무리가 왔나벼",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007859.1.11",
          "form": "날씨 좀 선선해지면 운동을 좀 시작해봐~",
          "original_form": "날씨 좀 선선해지면 운동을 좀 시작해봐~",
          "speaker_id": "2",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007859.1.12",
          "form": "스트레칭이라도 조금씩",
          "original_form": "스트레칭이라도 조금씩",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007859.1.13",
          "form": "몸이 재산이잖아",
          "original_form": "몸이 재산이잖아",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007859.1.14",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007859.1.15",
          "form": "지금 보니까 스트레칭 영상이 많네",
          "original_form": "지금 보니까 스트레칭 영상이 많네",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007859.1.16",
          "form": "응 요즘 홈트 영상 진짜 다양함",
          "original_form": "응 요즘 홈트 영상 진짜 다양함",
          "speaker_id": "2",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007859.1.17",
          "form": "아침 저녁은 선선해졌으니 잠깐씩 홈트하는 것도 좋을듯",
          "original_form": "아침 저녁은 선선해졌으니 잠깐씩 홈트하는 것도 좋을듯",
          "speaker_id": "2",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007859.1.18",
          "form": "아 홈트영상말고",
          "original_form": "아 홈트영상말고",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007859.1.19",
          "form": "재활스트레칭 ㅋㅋ",
          "original_form": "재활스트레칭 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007859.1.20",
          "form": "홈트 많은건 알지",
          "original_form": "홈트 많은건 알지",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007859.1.21",
          "form": "아 재활스트레칭은 또다른 분야인가",
          "original_form": "아 재활스트레칭은 또다른 분야인가",
          "speaker_id": "2",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007859.1.22",
          "form": "집에 밴드도 있으니 잘 활용해보시오~",
          "original_form": "집에 밴드도 있으니 잘 활용해보시오~",
          "speaker_id": "2",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007859.1.23",
          "form": "좀 달라",
          "original_form": "좀 달라",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007859.1.24",
          "form": "응 ㅋㅋ세라밴드같은거 말하는거지?",
          "original_form": "응 ㅋㅋ세라밴드같은거 말하는거지?",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007859.1.25",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007859.1.26",
          "form": "일단은 뉴질랜드 파스인지 크림인지 발라뒀어",
          "original_form": "일단은 뉴질랜드 파스인지 크림인지 발라뒀어",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007859.1.27",
          "form": "시원하고 좋구먼",
          "original_form": "시원하고 좋구먼",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007859.1.28",
          "form": "손목을 덜 써야한다는데 그걸 어케할 수 있는지..",
          "original_form": "손목을 덜 써야한다는데 그걸 어케할 수 있는지..",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007859.1.29",
          "form": "ㅋㅋㅋ 타이핑을 멈추고",
          "original_form": "ㅋㅋㅋ 타이핑을 멈추고",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007859.1.30",
          "form": "누워서 쉽니다",
          "original_form": "누워서 쉽니다",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007859.1.31",
          "form": "손목을 늘리지 않고 손목과 팔이 일직선이 되게 하고 타이핑중",
          "original_form": "손목을 늘리지 않고 손목과 팔이 일직선이 되게 하고 타이핑중",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007859.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007859.1.33",
          "form": "누워서 휴대폰 잡고 있는건 확실히 줄여야겠어",
          "original_form": "누워서 휴대폰 잡고 있는건 확실히 줄여야겠어",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007859.1.34",
          "form": "오늘 퇴근하고 운동하고 간다~",
          "original_form": "오늘 퇴근하고 운동하고 간다~",
          "speaker_id": "2",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100007859.1.35",
          "form": "비가 오니 가기 귀찮아지지만, 건강을 챙겨야지...",
          "original_form": "비가 오니 가기 귀찮아지지만, 건강을 챙겨야지...",
          "speaker_id": "2",
          "time": "20210823 16:16"
        }
      ]
