      "utterance": [
        {
          "id": "MDRW2100007028.1.1",
          "form": "시드니잼지",
          "original_form": "시드니잼지",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.2",
          "form": "재밌었지 추억이다",
          "original_form": "재밌었지 추억이다",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.3",
          "form": "캥거루 고기또먹자",
          "original_form": "캥거루 고기또먹자",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.4",
          "form": "ㅋㅋ 캥거루피자 맛있었다",
          "original_form": "ㅋㅋ 캥거루피자 맛있었다",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.5",
          "form": "티본승테이크 먹든동",
          "original_form": "티본승테이크 먹든동",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.6",
          "form": "나는 거기 오페라하우스 근처 걷는게 좋았다",
          "original_form": "나는 거기 오페라하우스 근처 걷는게 좋았다",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.7",
          "form": "만얼마에 티본 맨날꾸어먹음",
          "original_form": "만얼마에 티본 맨날꾸어먹음",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.8",
          "form": "스테이크가 참 맛있었지",
          "original_form": "스테이크가 참 맛있었지",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007028.1.9",
          "form": "오페라근처 걷는거 별기억없음",
          "original_form": "오페라근처 걷는거 별기억없음",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007028.1.10",
          "form": "달링하버인지 먼지",
          "original_form": "달링하버인지 먼지",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007028.1.11",
          "form": "나는 그냥 티본굽는게 젤기억에남음",
          "original_form": "나는 그냥 티본굽는게 젤기억에남음",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007028.1.12",
          "form": "기억 안나나?",
          "original_form": "기억 안나나?",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007028.1.13",
          "form": "달링하버 내 버리고 갓잖아",
          "original_form": "달링하버 내 버리고 갓잖아",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007028.1.14",
          "form": "기억력이 안좋네",
          "original_form": "기억력이 안좋네",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007028.1.15",
          "form": "시드니 거기 해변 이름이 기억이 안나",
          "original_form": "시드니 거기 해변 이름이 기억이 안나",
          "speaker_id": "1",
          "time": "20210822 00:18"
