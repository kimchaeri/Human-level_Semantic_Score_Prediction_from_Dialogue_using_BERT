      "utterance": [
        {
          "id": "MDRW2100029698.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029698.1.2",
          "form": "아니요 미혼이에ㅛ",
          "original_form": "아니요 미혼이에ㅛ",
          "speaker_id": "2",
          "time": "20210903 12:23"
        },
        {
          "id": "MDRW2100029698.1.3",
          "form": "언제쯤?",
          "original_form": "언제쯤?",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100029698.1.4",
          "form": "흐음 아직생각없어요ㅠ",
          "original_form": "흐음 아직생각없어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100029698.1.5",
          "form": "결혼 하면 좋은가요?",
          "original_form": "결혼 하면 좋은가요?",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100029698.1.6",
          "form": "네 행복한 하루를 보내고 있네요",
          "original_form": "네 행복한 하루를 보내고 있네요",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029698.1.7",
          "form": "아내분은 어떻게 만나셨어요?",
          "original_form": "아내분은 어떻게 만나셨어요?",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029698.1.8",
          "form": "친구소개요",
          "original_form": "친구소개요",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029698.1.9",
          "form": "우와 얼마나 만나고 결혼하셨어요?",
          "original_form": "우와 얼마나 만나고 결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029698.1.10",
          "form": "1년이요",
          "original_form": "1년이요",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100029698.1.11",
          "form": "결혼해야겠다는 결심은 언제 드셨나요?",
          "original_form": "결혼해야겠다는 결심은 언제 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100029698.1.12",
          "form": "1년후요",
          "original_form": "1년후요",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029698.1.13",
          "form": "아 결혼을 결심하게 해준 어떤 모습? 이나 어떤 계기가 있으셨나요?",
          "original_form": "아 결혼을 결심하게 해준 어떤 모습? 이나 어떤 계기가 있으셨나요?",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029698.1.14",
          "form": "말이 잘 통했어요",
          "original_form": "말이 잘 통했어요",
