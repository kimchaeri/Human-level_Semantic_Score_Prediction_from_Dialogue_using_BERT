      "utterance": [
        {
          "id": "MDRW2100039278.1.1",
          "form": "망고먹었어어제",
          "original_form": "망고먹었어어제",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039278.1.2",
          "form": "ㅎㅎ..망고맛있지",
          "original_form": "ㅎㅎ..망고맛있지",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039278.1.3",
          "form": "갑자기 태국망고먹고싶다",
          "original_form": "갑자기 태국망고먹고싶다",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039278.1.4",
          "form": "나는 땡모반",
          "original_form": "나는 땡모반",
          "speaker_id": "1",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039278.1.5",
          "form": "땡모반조아",
          "original_form": "땡모반조아",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039278.1.6",
          "form": "유일하게 태국에서 원없이먹은듯..",
          "original_form": "유일하게 태국에서 원없이먹은듯..",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039278.1.7",
          "form": "셩지옌먹고싶어",
          "original_form": "셩지옌먹고싶어",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039278.1.8",
          "form": "츠케멘은..",
          "original_form": "츠케멘은..",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039278.1.9",
          "form": "땡모반은 우리나라에허 그 맛이안나더라",
          "original_form": "땡모반은 우리나라에허 그 맛이안나더라",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039278.1.10",
          "form": "미치지 츠케멘",
          "original_form": "미치지 츠케멘",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039278.1.11",
          "form": "맞아 시럽인가...",
          "original_form": "맞아 시럽인가...",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039278.1.12",
          "form": "인천에 셩지엔 팔더라",
          "original_form": "인천에 셩지엔 팔더라",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039278.1.13",
          "form": "그거 차이나타운?",
          "original_form": "그거 차이나타운?",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039278.1.14",
          "form": "맛없대",
          "original_form": "맛없대",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039278.1.15",
          "form": "모야...지금도맛없어?",
          "original_form": "모야...지금도맛없어?",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039278.1.16",
          "form": "아 왜 그맛이안날까 슬퍼",
          "original_form": "아 왜 그맛이안날까 슬퍼",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039278.1.17",
          "form": "그러면 그 체인점이라도 들어오던가했으묜..",
          "original_form": "그러면 그 체인점이라도 들어오던가했으묜..",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.18",
          "form": "그니까 하나 들어와주지",
          "original_form": "그니까 하나 들어와주지",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.19",
          "form": "딘타이펑도있는데",
          "original_form": "딘타이펑도있는데",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.20",
          "form": "그건 타이완꺼잖아",
          "original_form": "그건 타이완꺼잖아",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.21",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.22",
          "form": "아닌가???",
          "original_form": "아닌가???",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.23",
          "form": "몰랐네",
          "original_form": "몰랐네",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.24",
          "form": "맞을거야",
          "original_form": "맞을거야",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039278.1.25",
          "form": "버블티먹고싶어",
          "original_form": "버블티먹고싶어",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
