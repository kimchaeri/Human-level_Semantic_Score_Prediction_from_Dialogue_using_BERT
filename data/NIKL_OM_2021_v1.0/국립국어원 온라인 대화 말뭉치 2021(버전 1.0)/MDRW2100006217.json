        {
          "id": "MDRW2100006217.1.1",
          "form": "결혼은 하셨나요",
          "original_form": "결혼은 하셨나요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006217.1.2",
          "form": "네 8년됐습니다",
          "original_form": "네 8년됐습니다",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006217.1.3",
          "form": "전 20년도 넘었네요",
          "original_form": "전 20년도 넘었네요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006217.1.4",
          "form": "아이도 있나요",
          "original_form": "아이도 있나요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006217.1.5",
          "form": "네 유아 둘이요",
          "original_form": "네 유아 둘이요",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100006217.1.6",
          "form": "아직 어린가요",
          "original_form": "아직 어린가요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100006217.1.7",
          "form": "네 3.6살이요",
          "original_form": "네 3.6살이요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006217.1.8",
          "form": "아 손이 만히 필요한 나이네요",
          "original_form": "아 손이 만히 필요한 나이네요",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006217.1.9",
          "form": "한참 이쁠 나이네요",
          "original_form": "한참 이쁠 나이네요",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006217.1.10",
          "form": "네 name1님은요??",
          "original_form": "네 &name1&님은요??",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006217.1.11",
          "form": "전 애들이 다 성인 이예요",
          "original_form": "전 애들이 다 성인 이예요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006217.1.12",
          "form": "대학생 둘요",
          "original_form": "대학생 둘요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006217.1.13",
          "form": "다 키우셨군요 부럽습니다",
          "original_form": "다 키우셨군요 부럽습니다",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006217.1.14",
          "form": "지금 어릴때도 즐기세요",
          "original_form": "지금 어릴때도 즐기세요",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006217.1.15",
          "form": "다키우고 나니 그때가 그리워요",
          "original_form": "다키우고 나니 그때가 그리워요",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006217.1.16",
          "form": "그러신가요 아이들이 어떤 짝을 만나길 바라시나요",
          "original_form": "그러신가요 아이들이 어떤 짝을 만나길 바라시나요",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006217.1.17",
          "form": "글쎄요 배려심 있고 서로 대화가 통하는 사람요",
          "original_form": "글쎄요 배려심 있고 서로 대화가 통하는 사람요",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006217.1.18",
          "form": "어떤짝을 만날지 아님 안 만날수도 있죠",
          "original_form": "어떤짝을 만날지 아님 안 만날수도 있죠",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100006217.1.19",
          "form": "그렇죠 아이들이.성인이라니 짝꿍에 대한 생각도 종종 하게.될 것 같네요",
