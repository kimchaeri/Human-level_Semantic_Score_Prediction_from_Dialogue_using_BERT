      "utterance": [
        {
          "id": "MDRW2100035330.1.1",
          "form": "점심식사하셨나요~",
          "original_form": "점심식사하셨나요~",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100035330.1.2",
          "form": "아이고 벌써 저녁 시간이네요! 아까 느지막히 일어나서 점심 챙겨 먹었어요~",
          "original_form": "아이고 벌써 저녁 시간이네요! 아까 느지막히 일어나서 점심 챙겨 먹었어요~",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035330.1.3",
          "form": "식사 잘챙기는 것도 은근 힘들더라고요 ㅎㅎ",
          "original_form": "식사 잘챙기는 것도 은근 힘들더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100035330.1.4",
          "form": "저녁은 또 뭘먹나~",
          "original_form": "저녁은 또 뭘먹나~",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100035330.1.5",
          "form": "저는 방금 카레랑 고등어랑 같이 먹었어요!",
          "original_form": "저는 방금 카레랑 고등어랑 같이 먹었어요!",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100035330.1.6",
          "form": "맛난거 드셨내요!!",
          "original_form": "맛난거 드셨내요!!",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100035330.1.7",
          "form": "둘다 너무 좋아해요",
          "original_form": "둘다 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100035330.1.8",
          "form": "카레 감자 큼게 좋아요 작은게 좋아요?ㅎㅎ",
          "original_form": "카레 감자 큼게 좋아요 작은게 좋아요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100035330.1.9",
          "form": "각설탕 정도 작은 크기요!",
          "original_form": "각설탕 정도 작은 크기요!",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035330.1.10",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035330.1.11",
          "form": "저도 그정도가 좋더라고뇨",
          "original_form": "저도 그정도가 좋더라고뇨",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100035330.1.12",
          "form": "너무 작으면 식감이 별로얘요",
          "original_form": "너무 작으면 식감이 별로얘요",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100035330.1.13",
          "form": "카레 자주 드시나요?",
          "original_form": "카레 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035330.1.14",
          "form": "지금은 못먹고있는데",
          "original_form": "지금은 못먹고있는데",
          "speaker_id": "2",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100035330.1.15",
          "form": "예전에는 틈만나면 먹었어요",
          "original_form": "예전에는 틈만나면 먹었어요",
          "speaker_id": "2",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100035330.1.16",
          "form": "카레에 뭐뭐 넣어서 만드시나요?",
          "original_form": "카레에 뭐뭐 넣어서 만드시나요?",
          "speaker_id": "1",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100035330.1.17",
          "form": "저는 감자랑 당근만 있으면 잘먹는데",
          "original_form": "저는 감자랑 당근만 있으면 잘먹는데",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100035330.1.18",
          "form": "어머니는 꼭 햄을 넣으셔요",
          "original_form": "어머니는 꼭 햄을 넣으셔요",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100035330.1.19",
          "form": "햄은 한 번도 안 넣어 본 거 같아요",
          "original_form": "햄은 한 번도 안 넣어 본 거 같아요",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100035330.1.20",
          "form": "잘 어울리나요?",
          "original_form": "잘 어울리나요?",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100035330.1.21",
          "form": "아이들이 좋아할것같은맛?",
          "original_form": "아이들이 좋아할것같은맛?",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100035330.1.22",
          "form": "햄 특유의 맛이나서 뭔가 느낌이 달라요",
          "original_form": "햄 특유의 맛이나서 뭔가 느낌이 달라요",
          "speaker_id": "2",
          "time": "20210830 22:33"
