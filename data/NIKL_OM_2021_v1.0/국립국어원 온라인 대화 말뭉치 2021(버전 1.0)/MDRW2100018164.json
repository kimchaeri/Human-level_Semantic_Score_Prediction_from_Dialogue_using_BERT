      "utterance": [
        {
          "id": "MDRW2100018164.1.1",
          "form": "무슨일이 있었나요?",
          "original_form": "무슨일이 있었나요?",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018164.1.2",
          "form": "아 밥먹으면서 보다가 넘 쇼킹해서",
          "original_form": "아 밥먹으면서 보다가 넘 쇼킹해서",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018164.1.3",
          "form": "끄고 유튜브봤거든요",
          "original_form": "끄고 유튜브봤거든요",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018164.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018164.1.5",
          "form": "앜ㅋㅋㅋㅋ끄실정도 였구나",
          "original_form": "앜ㅋㅋㅋㅋ끄실정도 였구나",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018164.1.6",
          "form": "설정자체가 좀...쇼킹하죠",
          "original_form": "설정자체가 좀...쇼킹하죠",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018164.1.7",
          "form": "저는 그런거 보다보니까 자극적인게 좋아서",
          "original_form": "저는 그런거 보다보니까 자극적인게 좋아서",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018164.1.8",
          "form": "쏘핫도 봤어요....",
          "original_form": "쏘핫도 봤어요....",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018164.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ쏘핫도 마라맛인가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ쏘핫도 마라맛인가요",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018164.1.10",
          "form": "네 엄청요ㅋㅋㅋㅋㄱㅋ",
          "original_form": "네 엄청요ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018164.1.11",
          "form": "스킨쉽 하면 상금깍이는 섬에서",
          "original_form": "스킨쉽 하면 상금깍이는 섬에서",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018164.1.12",
          "form": "버티는 설정이예요ㅋㅋㅋㄱㅋ",
          "original_form": "버티는 설정이예요ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018164.1.13",
          "form": "아니세상에",
          "original_form": "아니세상에",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018164.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018164.1.15",
          "form": "완전 핵불닭마라맛",
          "original_form": "완전 핵불닭마라맛",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018164.1.16",
          "form": "유교걸 기겁중",
          "original_form": "유교걸 기겁중",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018164.1.17",
          "form": "우리나라버전은 상금잘지켜나갈거 같아요 그래서 안만들걸요ㅋㅋㅋㅋㅋ",
          "original_form": "우리나라버전은 상금잘지켜나갈거 같아요 그래서 안만들걸요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018164.1.18",
          "form": "외국은 유교걸이 없나봐요...",
          "original_form": "외국은 유교걸이 없나봐요...",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018164.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ완전 다들 눈에 불켜고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ완전 다들 눈에 불켜고",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018164.1.20",
          "form": "돈이최고지 하믄서..",
          "original_form": "돈이최고지 하믄서..",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018164.1.21",
          "form": "누구 뽀뽀라도하면 눈에 불키고",
          "original_form": "누구 뽀뽀라도하면 눈에 불키고",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018164.1.22",
          "form": "비난할거 같아요",
          "original_form": "비난할거 같아요",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018164.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그려져요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그려져요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018164.1.24",
          "form": "넷플 근데ㅜ은근 보다보면",
          "original_form": "넷플 근데ㅜ은근 보다보면",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018164.1.25",
          "form": "볼 게 없어요!",
          "original_form": "볼 게 없어요!",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018164.1.26",
          "form": "추천영화 뽀개다보면 더 이상 취향을 찾지못하는..",
          "original_form": "추천영화 뽀개다보면 더 이상 취향을 찾지못하는..",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018164.1.27",
          "form": "오리지널 몇개 보고나면 딱히 볼게없어요...ㅈ",
          "original_form": "오리지널 몇개 보고나면 딱히 볼게없어요...ㅈ",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018164.1.28",
          "form": "스트리밍 서비스들 좀 합쳐줬으면 좋겠어요",
          "original_form": "스트리밍 서비스들 좀 합쳐줬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018164.1.29",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018164.1.30",
          "form": "쿠팡도 쿠팡ㅇㅎ리지널 낸 거 아세요?",
          "original_form": "쿠팡도 쿠팡ㅇㅎ리지널 낸 거 아세요?",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018164.1.31",
