          "id": "MDRW2100027306.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027306.1.2",
          "form": "전집순이라",
          "original_form": "전집순이라",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027306.1.3",
          "form": "자주안가지만",
          "original_form": "자주안가지만",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027306.1.4",
          "form": "가는건 좋아요",
          "original_form": "가는건 좋아요",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027306.1.5",
          "form": "집순이시구나",
          "original_form": "집순이시구나",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.6",
          "form": "맛집여행",
          "original_form": "맛집여행",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.7",
          "form": "어디가 가장좋았나요?",
          "original_form": "어디가 가장좋았나요?",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.8",
          "form": "가고싶어요",
          "original_form": "가고싶어요",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.9",
          "form": "맛집여행 최고죠",
          "original_form": "맛집여행 최고죠",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.10",
          "form": "전 인천이요",
          "original_form": "전 인천이요",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.11",
          "form": "지하에 쇼핑몰",
          "original_form": "지하에 쇼핑몰",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.12",
          "form": "남친맛집말고요",
          "original_form": "남친맛집말고요",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.13",
          "form": "엄청길어요",
          "original_form": "엄청길어요",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.14",
          "form": "아 쇼핑하기좋구나",
          "original_form": "아 쇼핑하기좋구나",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.15",
          "form": "쇼핑맛집이에요",
          "original_form": "쇼핑맛집이에요",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.16",
          "form": "부천에있어요",
          "original_form": "부천에있어요",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027306.1.17",
          "form": "가을이 다가오니",
          "original_form": "가을이 다가오니",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027306.1.18",
          "form": "너무머네요",
          "original_form": "너무머네요",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027306.1.19",
          "form": "산으로 가고싶네요",
          "original_form": "산으로 가고싶네요",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027306.1.20",
          "form": "단풍보러요",
          "original_form": "단풍보러요",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027306.1.21",
          "form": "단풍은 경주죠",
          "original_form": "단풍은 경주죠",
