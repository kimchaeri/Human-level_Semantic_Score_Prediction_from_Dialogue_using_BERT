      "utterance": [
        {
          "id": "MDRW2100027787.1.1",
          "form": "nc주식이 슥슥 내려간단 소식입니다",
          "original_form": "nc주식이 슥슥 내려간단 소식입니다",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027787.1.2",
          "form": "아 이건 게임인듯 아닌듯",
          "original_form": "아 이건 게임인듯 아닌듯",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027787.1.3",
          "form": "블소2가 망해서?",
          "original_form": "블소2가 망해서?",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027787.1.4",
          "form": "어제오늘 메이플 공모전으로 시끄럽습니다",
          "original_form": "어제오늘 메이플 공모전으로 시끄럽습니다",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027787.1.5",
          "form": "겜얘기네요",
          "original_form": "겜얘기네요",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027787.1.6",
          "form": "블소 과금 구조가 리니지 복사판일 뿐이라",
          "original_form": "블소 과금 구조가 리니지 복사판일 뿐이라",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027787.1.7",
          "form": "게임성이 특출난 것도 아니고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "게임성이 특출난 것도 아니고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027787.1.8",
          "form": "근데 왜 망해",
          "original_form": "근데 왜 망해",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027787.1.9",
          "form": "하는 사람도 신선하고 재미있어야 붙어있는 건데",
          "original_form": "하는 사람도 신선하고 재미있어야 붙어있는 건데",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027787.1.10",
          "form": "뭘해도 똑같으니 정이 안가는 덧",
          "original_form": "뭘해도 똑같으니 정이 안가는 덧",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027787.1.11",
          "form": "새 게임같지않다 이건가",
          "original_form": "새 게임같지않다 이건가",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027787.1.12",
          "form": "메이플은 먼데요",
          "original_form": "메이플은 먼데요",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027787.1.13",
          "form": "걍 가망이 없다는듯",
          "original_form": "걍 가망이 없다는듯",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027787.1.14",
          "form": "코스튬 공모전 했는데",
          "original_form": "코스튬 공모전 했는데",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027787.1.15",
          "form": "디자인 공모전 ㅇㅇ",
          "original_form": "디자인 공모전 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027787.1.16",
          "form": "성의 있는 건 은상이고",
          "original_form": "성의 있는 건 은상이고",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027787.1.17",
          "form": "성의 없는게 1등이라는 듯",
          "original_form": "성의 없는게 1등이라는 듯",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027787.1.18",
          "form": "제가 하는 로오히는...",
          "original_form": "제가 하는 로오히는...",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027787.1.19",
          "form": "진짜 너무 어중간하다는 단점이",
          "original_form": "진짜 너무 어중간하다는 단점이",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027787.1.20",
          "form": "로오히도 공모전이 있었나요?",
          "original_form": "로오히도 공모전이 있었나요?",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027787.1.21",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027787.1.22",
          "form": "겜에 지친 사람들한테 장점이 되어서",
          "original_form": "겜에 지친 사람들한테 장점이 되어서",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.23",
          "form": "그냥 게임얘기 다다다닥?",
          "original_form": "그냥 게임얘기 다다다닥?",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.24",
          "form": "없뎃인데 살아있는 게 웃심",
          "original_form": "없뎃인데 살아있는 게 웃심",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.27",
          "form": "쿠킹덤도 엄청 뜨더니",
          "original_form": "쿠킹덤도 엄청 뜨더니",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.28",
          "form": "이제 좀 잠잠한듯",
          "original_form": "이제 좀 잠잠한듯",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.29",
          "form": "근데 고정층이 박힌듯",
          "original_form": "근데 고정층이 박힌듯",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.30",
          "form": "일본 성우 뜨던데",
          "original_form": "일본 성우 뜨던데",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.31",
          "form": "장난 아니던데",
          "original_form": "장난 아니던데",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027787.1.32",
          "form": "성우에 돈 쓰면 뭐해....",
          "original_form": "성우에 돈 쓰면 뭐해....",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027787.1.33",
          "form": "성우겜이 아닌걸 ㅠ",
          "original_form": "성우겜이 아닌걸 ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027787.1.34",
          "form": "우유 성우 누구냐고 ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "우유 성우 누구냐고 ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027787.1.35",
          "form": "왜 아몬드보다 늦게뜨냐 ㅠㅠㅠ지가뭔데ㅠㅠ",
          "original_form": "왜 아몬드보다 늦게뜨냐 ㅠㅠㅠ지가뭔데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027787.1.36",
          "form": "이시카와 카이토래",
          "original_form": "이시카와 카이토래",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
