      "utterance": [
        {
          "id": "MDRW2100032136.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100032136.1.2",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100032136.1.3",
          "form": "얼마 전에 이직했는데",
          "original_form": "얼마 전에 이직했는데",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100032136.1.4",
          "form": "고민이 있어",
          "original_form": "고민이 있어",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100032136.1.5",
          "form": "이직해봤어?",
          "original_form": "이직해봤어?",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100032136.1.6",
          "form": "이직했지",
          "original_form": "이직했지",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032136.1.7",
          "form": "몇번",
          "original_form": "몇번",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032136.1.8",
          "form": "무슨고민?",
          "original_form": "무슨고민?",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032136.1.9",
          "form": "난 이직이 두번째인데",
          "original_form": "난 이직이 두번째인데",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032136.1.10",
          "form": "다른데서는 나름 적응을 빨리했어",
          "original_form": "다른데서는 나름 적응을 빨리했어",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032136.1.11",
          "form": "근데 이번에 옮긴데는 적응이 안되서 힘드네",
          "original_form": "근데 이번에 옮긴데는 적응이 안되서 힘드네",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032136.1.12",
          "form": "이런 고민 해봤어?",
          "original_form": "이런 고민 해봤어?",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032136.1.13",
          "form": "적응이 왜 잘 안되는데?",
          "original_form": "적응이 왜 잘 안되는데?",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032136.1.14",
          "form": "이번에 이직한 곳은 사람들이 오래 같이 일해서",
          "original_form": "이번에 이직한 곳은 사람들이 오래 같이 일해서",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032136.1.15",
          "form": "너무 친한 느낌? 은근한 텃세가 있는 느낌?",
          "original_form": "너무 친한 느낌? 은근한 텃세가 있는 느낌?",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032136.1.16",
          "form": "내 느낌인지는 몰라도 그런 기분이 들어서 어렵네..",
          "original_form": "내 느낌인지는 몰라도 그런 기분이 들어서 어렵네..",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032136.1.17",
          "form": "근데 그런건 다들 조금씩 있는거 같은데",
          "original_form": "근데 그런건 다들 조금씩 있는거 같은데",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032136.1.18",
          "form": "맞아 늘 있기는 했는데",
          "original_form": "맞아 늘 있기는 했는데",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032136.1.19",
          "form": "이번에 다니는 곳은 이직이 거의 없어서",
          "original_form": "이번에 다니는 곳은 이직이 거의 없어서",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032136.1.20",
          "form": "좋은회산가보네",
          "original_form": "좋은회산가보네",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032136.1.21",
          "form": "이직이 거의 없으면",
          "original_form": "이직이 거의 없으면",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032136.1.22",
          "form": "음 바꿔서 생각하면 그럴수도 있겠다",
          "original_form": "음 바꿔서 생각하면 그럴수도 있겠다",
          "speaker_id": "1",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032136.1.23",
          "form": "그래도 간만에 인간관계가 좀 어렵게 느껴지는거야",
          "original_form": "그래도 간만에 인간관계가 좀 어렵게 느껴지는거야",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032136.1.24",
          "form": "금밤 지나가는 느낌이겠지?",
          "original_form": "금밤 지나가는 느낌이겠지?",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032136.1.25",
          "form": "텃새 이겨내려면 그냥 철판깔고 숙이고 들이대야지",
          "original_form": "텃새 이겨내려면 그냥 철판깔고 숙이고 들이대야지",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032136.1.26",
          "form": "자연스럽게 스며들려면 시간좀 걸리고 ㅎㅎ",
          "original_form": "자연스럽게 스며들려면 시간좀 걸리고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032136.1.27",
          "form": "역시 철판깔고 들이대는게 최고겠지?",
          "original_form": "역시 철판깔고 들이대는게 최고겠지?",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032136.1.28",
          "form": "사실 금요일부터는",
          "original_form": "사실 금요일부터는",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032136.1.29",
          "form": "그렇게 해봤는데",
          "original_form": "그렇게 해봤는데",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032136.1.30",
          "form": "효과가 좀 있더라",
          "original_form": "효과가 좀 있더라",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032136.1.31",
          "form": "여자가 많은곳인가?",
          "original_form": "여자가 많은곳인가?",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032136.1.32",
          "form": "다 여자야",
          "original_form": "다 여자야",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032136.1.33",
          "form": "그렇군....",
          "original_form": "그렇군....",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032136.1.34",
          "form": "남자들보다 더 심한거 같긴해",
          "original_form": "남자들보다 더 심한거 같긴해",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032136.1.35",
          "form": "솔직히 좀 그렇긴해",
          "original_form": "솔직히 좀 그렇긴해",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032136.1.36",
          "form": "다른 사람이랑 이야기 중인데",
          "original_form": "다른 사람이랑 이야기 중인데",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032136.1.37",
          "form": "내가 시선을 눈치 챌때까지 쳐다보더라고...",
          "original_form": "내가 시선을 눈치 챌때까지 쳐다보더라고...",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032136.1.38",
          "form": "탐색하는 눈빛...?",
          "original_form": "탐색하는 눈빛...?",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032136.1.39",
          "form": "첨에는 모른척했고",
          "original_form": "첨에는 모른척했고",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032136.1.40",
          "form": "모른척하기 힘들때까지 봐서",
          "original_form": "모른척하기 힘들때까지 봐서",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032136.1.41",
          "form": "근데 다 그렇진 않을텐데",
          "original_form": "근데 다 그렇진 않을텐데",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032136.1.42",
          "form": "그냥 보고 웃었는데 그런경우 첨이라 당황스럽더라구;;",
          "original_form": "그냥 보고 웃었는데 그런경우 첨이라 당황스럽더라구;;",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032136.1.43",
          "form": "그중에 친해지기 쉬운사람부터 공략",
          "original_form": "그중에 친해지기 쉬운사람부터 공략",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032136.1.44",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032136.1.45",
          "form": "좋은 사람들도",
          "original_form": "좋은 사람들도",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032136.1.46",
          "form": "있는거 같아",
          "original_form": "있는거 같아",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032136.1.47",
          "form": "그런사람부터 친해져서",
          "original_form": "그런사람부터 친해져서",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032136.1.48",
          "form": "상태파악해야지 ㅎㅎ",
          "original_form": "상태파악해야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032136.1.49",
          "form": "근데 또 불편하게 구는 사람이 나이가 좀 있는 사람이라 그게 좀 맘에 걸려서",
          "original_form": "근데 또 불편하게 구는 사람이 나이가 좀 있는 사람이라 그게 좀 맘에 걸려서",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032136.1.50",
          "form": "고민이었어",
          "original_form": "고민이었어",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032136.1.51",
          "form": "그래도 금요일부터는",
          "original_form": "그래도 금요일부터는",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032136.1.52",
          "form": "그냥 기분 나쁘건 말건",
          "original_form": "그냥 기분 나쁘건 말건",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032136.1.53",
          "form": "웃는 얼굴에 침 못뱉겠지 하고",
          "original_form": "웃는 얼굴에 침 못뱉겠지 하고",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032136.1.54",
          "form": "나이좀 있으면 원래그럼",
          "original_form": "나이좀 있으면 원래그럼",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032136.1.55",
          "form": "들이댔는데 효과가 좀 있는거 같아서",
          "original_form": "들이댔는데 효과가 좀 있는거 같아서",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032136.1.56",
          "form": "낯가리나",
          "original_form": "낯가리나",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032136.1.57",
          "form": "다행이야",
          "original_form": "다행이야",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032136.1.58",
          "form": "전에 다니던곳보단 좋아?",
          "original_form": "전에 다니던곳보단 좋아?",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032136.1.59",
          "form": "일이 더 많아 책임도 더 크고 사람도 많고",
          "original_form": "일이 더 많아 책임도 더 크고 사람도 많고",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032136.1.60",
          "form": "경력 쌓기는 좋을거 같은데",
          "original_form": "경력 쌓기는 좋을거 같은데",
          "speaker_id": "1",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032136.1.61",
          "form": "몸과 정신은 더 힘든거 같아",
          "original_form": "몸과 정신은 더 힘든거 같아",
          "speaker_id": "1",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032136.1.62",
          "form": "음...  좋은건가",
          "original_form": "음...  좋은건가",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032136.1.63",
          "form": "나쁜건가",
          "original_form": "나쁜건가",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032136.1.64",
          "form": "솔직히 나쁜거 같긴한데",
          "original_form": "솔직히 나쁜거 같긴한데",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032136.1.65",
          "form": "걍 좋게 생각하려구",
          "original_form": "걍 좋게 생각하려구",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
