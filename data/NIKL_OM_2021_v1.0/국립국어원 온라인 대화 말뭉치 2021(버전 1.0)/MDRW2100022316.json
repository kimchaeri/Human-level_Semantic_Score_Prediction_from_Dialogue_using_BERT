        {
          "id": "MDRW2100022316.1.1",
          "form": "오늘 아몬드 사야 돼",
          "original_form": "오늘 아몬드 사야 돼",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022316.1.2",
          "form": "이제 다 먹었어?",
          "original_form": "이제 다 먹었어?",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022316.1.3",
          "form": "ㅇㅇ 아침마다 샐러드에 넣어서 먹으니 금방 먹네 ㅎㅎ",
          "original_form": "ㅇㅇ 아침마다 샐러드에 넣어서 먹으니 금방 먹네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022316.1.4",
          "form": "우리 먹는 아몬드 어디꺼지?",
          "original_form": "우리 먹는 아몬드 어디꺼지?",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022316.1.5",
          "form": "산과들에 아몬드",
          "original_form": "산과들에 아몬드",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022316.1.6",
          "form": "볶음 아몬드라 고소하더라",
          "original_form": "볶음 아몬드라 고소하더라",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022316.1.7",
          "form": "두봉지에 만원 정도라 이번엔 4봉지 주문해야겠다",
          "original_form": "두봉지에 만원 정도라 이번엔 4봉지 주문해야겠다",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022316.1.8",
          "form": "ㅇㅇ 그랴~ 냉동실에 두면 한참 먹으니까",
          "original_form": "ㅇㅇ 그랴~ 냉동실에 두면 한참 먹으니까",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022316.1.9",
          "form": "애들도 하루에 한줌씩 먹이니 건강에 좋구",
          "original_form": "애들도 하루에 한줌씩 먹이니 건강에 좋구",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022316.1.10",
          "form": "아몬드 먹으면 뇌발달에 좋아 ㅎㅎ",
          "original_form": "아몬드 먹으면 뇌발달에 좋아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022316.1.11",
          "form": "오 꾸준히 먹여야겠네",
          "original_form": "오 꾸준히 먹여야겠네",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022316.1.12",
          "form": "ㅇㅇ 우리도 건강 위해서 아몬드 계속 챙겨먹자",
          "original_form": "ㅇㅇ 우리도 건강 위해서 아몬드 계속 챙겨먹자",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022316.1.13",
          "form": "가격비교 해봐야겠네 ㅎㅎ 오늘 주문할께",
          "original_form": "가격비교 해봐야겠네 ㅎㅎ 오늘 주문할께",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022316.1.14",
          "form": "마스크팩은 찾아봤더니 차앤박이 좀 괜찮나봐",
          "original_form": "마스크팩은 찾아봤더니 차앤박이 좀 괜찮나봐",
