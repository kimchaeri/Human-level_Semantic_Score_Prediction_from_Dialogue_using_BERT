      "utterance": [
        {
          "id": "MDRW2100017757.1.1",
          "form": "수학쓰는건 어떤 직업이라요?",
          "original_form": "수학쓰는건 어떤 직업이라요?",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017757.1.2",
          "form": "ㅎㅎ 집에서도 수학해요 매일",
          "original_form": "ㅎㅎ 집에서도 수학해요 매일",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017757.1.3",
          "form": "와.. 그냥 취미가... 존경 반.. 놀라움 반.. 이에요..",
          "original_form": "와.. 그냥 취미가... 존경 반.. 놀라움 반.. 이에요..",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017757.1.4",
          "form": "어떤 직업이신데여?  선생님?",
          "original_form": "어떤 직업이신데여?  선생님?",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017757.1.5",
          "form": "일상은 수학이죠 ㅋㅋ 아이랑 하는 부루마블도 수학 ㅋㅋㅋ",
          "original_form": "일상은 수학이죠 ㅋㅋ 아이랑 하는 부루마블도 수학 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017757.1.6",
          "form": "부르마블 수학이? 게임 같은거에요?",
          "original_form": "부르마블 수학이? 게임 같은거에요?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017757.1.7",
          "form": "부루마블 모르세요?",
          "original_form": "부루마블 모르세요?",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017757.1.8",
          "form": "그것도 집안 일 ㅋㅋ ㅠㅠ",
          "original_form": "그것도 집안 일 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017757.1.9",
          "form": "아하... 땅값 계산이요? ㅋㅋㅋㅋ",
          "original_form": "아하... 땅값 계산이요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017757.1.10",
          "form": "아이랑 노는것도 일이긴 하죠..",
          "original_form": "아이랑 노는것도 일이긴 하죠..",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017757.1.11",
          "form": "그럼요 ㅋㅋ 삶은 수학 ㅋㅋ 업무 에요 ㅠ 매일 한판",
          "original_form": "그럼요 ㅋㅋ 삶은 수학 ㅋㅋ 업무 에요 ㅠ 매일 한판",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017757.1.12",
          "form": "일은 주로 정시퇴근이신가여?",
          "original_form": "일은 주로 정시퇴근이신가여?",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017757.1.13",
          "form": "야근도 많아요",
          "original_form": "야근도 많아요",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017757.1.14",
          "form": "야근이랑 부르마블이랑 어떤게 더 좋으세여? ㅋㅋㅋ",
          "original_form": "야근이랑 부르마블이랑 어떤게 더 좋으세여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017757.1.15",
          "form": "솔직히 야근 ㅋㅋ",
          "original_form": "솔직히 야근 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017757.1.16",
          "form": "ㅋㅋㅋ 다 그럴꺼 같아요..",
