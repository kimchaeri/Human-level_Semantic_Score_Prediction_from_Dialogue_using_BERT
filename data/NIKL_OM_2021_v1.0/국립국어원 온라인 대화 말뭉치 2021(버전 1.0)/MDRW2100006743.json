      "utterance": [
        {
          "id": "MDRW2100006743.1.1",
          "form": "뉴욕에 버거맛집많죠...",
          "original_form": "뉴욕에 버거맛집많죠...",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100006743.1.2",
          "form": "대도시라 그런기 휘양찬란하더라고요",
          "original_form": "대도시라 그런기 휘양찬란하더라고요",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100006743.1.3",
          "form": "커피 드시나요",
          "original_form": "커피 드시나요",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100006743.1.4",
          "form": "앗 커피 얘기가 왜 지금 보이짘ㅋㅋㅋㅋㅋ",
          "original_form": "앗 커피 얘기가 왜 지금 보이짘ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100006743.1.5",
          "form": "커피는 잘 안 마셔요",
          "original_form": "커피는 잘 안 마셔요",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100006743.1.6",
          "form": "아 뉴욕 맛집이야기했던가요?",
          "original_form": "아 뉴욕 맛집이야기했던가요?",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006743.1.7",
          "form": "네네 괜찮아요 저도 거기 잘 몰라욬ㅋㅋㅋㅋ",
          "original_form": "네네 괜찮아요 저도 거기 잘 몰라욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006743.1.8",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006743.1.9",
          "form": "저도 안마셔요..ㅎㅎ 그럼 카페 안가시나요?",
          "original_form": "저도 안마셔요..ㅎㅎ 그럼 카페 안가시나요?",
          "speaker_id": "1",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100006743.1.10",
          "form": "친구랑 만나면 카페느 자주 가요",
          "original_form": "친구랑 만나면 카페느 자주 가요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100006743.1.11",
          "form": "그럼 뭐드셔요?ㅎㅎ",
          "original_form": "그럼 뭐드셔요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006743.1.12",
          "form": "요거트나 에이드 마셔요",
          "original_form": "요거트나 에이드 마셔요",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006743.1.13",
          "form": "아 그러시군요 ㅎㅎ 저도 커피를 안먹으니 카페가면 난감해요",
          "original_form": "아 그러시군요 ㅎㅎ 저도 커피를 안먹으니 카페가면 난감해요",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006743.1.14",
          "form": "보통 뭐 드세요?",
          "original_form": "보통 뭐 드세요?",
