      "utterance": [
        {
          "id": "MDRW2100010536.1.1",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010536.1.2",
          "form": "거긴 커서 돈도많아서",
          "original_form": "거긴 커서 돈도많아서",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010536.1.3",
          "form": "영화 공짜로 볼수있는것도",
          "original_form": "영화 공짜로 볼수있는것도",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010536.1.4",
          "form": "당연히 주는데",
          "original_form": "당연히 주는데",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010536.1.5",
          "form": "10년간 몰랐어요",
          "original_form": "10년간 몰랐어요",
          "speaker_id": "1",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010536.1.6",
          "form": "왜 고객에게 안알려주나요",
          "original_form": "왜 고객에게 안알려주나요",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010536.1.7",
          "form": "이것도 부들부들",
          "original_form": "이것도 부들부들",
          "speaker_id": "1",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010536.1.8",
          "form": "진짜 저 앞으로 할인 꼭 물어볼께요",
          "original_form": "진짜 저 앞으로 할인 꼭 물어볼께요",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010536.1.9",
          "form": "저도요 몰라서 결합할인을 못했어요",
          "original_form": "저도요 몰라서 결합할인을 못했어요",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010536.1.10",
          "form": "이제라도 아니 다행이죠",
          "original_form": "이제라도 아니 다행이죠",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010536.1.11",
          "form": "저는 기본요금제라",
          "original_form": "저는 기본요금제라",
          "speaker_id": "1",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.12",
          "form": "근데 엄마명의로 결합이라서",
          "original_form": "근데 엄마명의로 결합이라서",
          "speaker_id": "1",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.13",
          "form": "장기고객 쿠폰 선물도 본사에서 신청 선착순이 있더라고요",
          "original_form": "장기고객 쿠폰 선물도 본사에서 신청 선착순이 있더라고요",
          "speaker_id": "2",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.14",
          "form": "엄마는 vip였던거에요",
          "original_form": "엄마는 vip였던거에요",
          "speaker_id": "1",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.15",
          "form": "그것도 말안해서 2틀전에 우연히 들러갔다",
          "original_form": "그것도 말안해서 2틀전에 우연히 들러갔다",
          "speaker_id": "2",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.16",
          "form": "신청했어요",
          "original_form": "신청했어요",
          "speaker_id": "2",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.17",
          "form": "왜진작안알아봐줬는지",
          "original_form": "왜진작안알아봐줬는지",
          "speaker_id": "1",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.18",
          "form": "도넛 공짜랑 할인요",
          "original_form": "도넛 공짜랑 할인요",
          "speaker_id": "2",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010536.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010536.1.20",
          "form": "세상에 영화가 몇편이예요",
          "original_form": "세상에 영화가 몇편이예요",
          "speaker_id": "2",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010536.1.21",
          "form": "영화 할인 진짜 아깝네요",
          "original_form": "영화 할인 진짜 아깝네요",
          "speaker_id": "2",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010536.1.22",
          "form": "저도 저번달 영화권 까먹고 못썼어요 ㅜㅜ잘안가지만 아쉬워요",
          "original_form": "저도 저번달 영화권 까먹고 못썼어요 ㅜㅜ잘안가지만 아쉬워요",
          "speaker_id": "2",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010536.1.23",
          "form": "그래서 다른 할인으로 바꿨어요",
          "original_form": "그래서 다른 할인으로 바꿨어요",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010536.1.24",
          "form": "꼭 월초에 챙겨야해요",
          "original_form": "꼭 월초에 챙겨야해요",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010536.1.25",
          "form": "지금은 일년에 세번공짜영화인데",
          "original_form": "지금은 일년에 세번공짜영화인데",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010536.1.26",
          "form": "작년인가 재작년까지는",
          "original_form": "작년인가 재작년까지는",
          "speaker_id": "1",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010536.1.27",
          "form": "6번이었거든요",
          "original_form": "6번이었거든요",
          "speaker_id": "1",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010536.1.28",
          "form": "세상에 그게 다 얼만지",
          "original_form": "세상에 그게 다 얼만지",
          "speaker_id": "1",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010536.1.29",
          "form": "진짜 돈이 어마어마 하네요",
          "original_form": "진짜 돈이 어마어마 하네요",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010536.1.30",
          "form": "세상에 할인 못받음 바보라는데 전 완전 바보됐어요ㅜㅜ저번에 폰 하면서 결합을 안해서요",
          "original_form": "세상에 할인 못받음 바보라는데 전 완전 바보됐어요ㅜㅜ저번에 폰 하면서 결합을 안해서요",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010536.1.31",
          "form": "앞으로도 여러군데 알아봐야겠어요",
          "original_form": "앞으로도 여러군데 알아봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010536.1.32",
          "form": "안쓰면 포인트라도 주던가",
          "original_form": "안쓰면 포인트라도 주던가",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010536.1.33",
          "form": "알림이라도 주지",
          "original_form": "알림이라도 주지",
          "speaker_id": "2",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010536.1.34",
          "form": "할인이 이렇게 많은데",
          "original_form": "할인이 이렇게 많은데",
          "speaker_id": "2",
          "time": "20210823 19:45"
        }
      ]
    }
