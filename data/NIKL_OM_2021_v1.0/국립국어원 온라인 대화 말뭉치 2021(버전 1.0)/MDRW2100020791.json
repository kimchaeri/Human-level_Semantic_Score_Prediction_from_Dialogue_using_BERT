      "utterance": [
        {
          "id": "MDRW2100020791.1.1",
          "form": "여행가고싶어",
          "original_form": "여행가고싶어",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020791.1.2",
          "form": "어디가고싶어",
          "original_form": "어디가고싶어",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020791.1.3",
          "form": "코로나로 해외는 못나가니깐 국내라면 제주도나 부산이나 전주",
          "original_form": "코로나로 해외는 못나가니깐 국내라면 제주도나 부산이나 전주",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020791.1.4",
          "form": "오로지 먹방...",
          "original_form": "오로지 먹방...",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020791.1.5",
          "form": "먹고죽어!",
          "original_form": "먹고죽어!",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020791.1.6",
          "form": "전주노잼이야",
          "original_form": "전주노잼이야",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020791.1.7",
          "form": "먹을건많더라",
          "original_form": "먹을건많더라",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020791.1.8",
          "form": "전주 노잼이야?",
          "original_form": "전주 노잼이야?",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020791.1.9",
          "form": "전주는 그럼 안되겠다",
          "original_form": "전주는 그럼 안되겠다",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020791.1.10",
          "form": "아 경주도 봄에 가면 이쁘데",
          "original_form": "아 경주도 봄에 가면 이쁘데",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020791.1.11",
          "form": "봄애?",
          "original_form": "봄애?",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.12",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.13",
          "form": "가을에가서구랫나바ㅋㅋㅋ",
          "original_form": "가을에가서구랫나바ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.14",
          "form": "그 뭐지",
          "original_form": "그 뭐지",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.15",
          "form": "곁벚꽃?",
          "original_form": "곁벚꽃?",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.16",
          "form": "아 경주ㅋㅋㅋ",
          "original_form": "아 경주ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.17",
          "form": "아 그거 우리집꽃",
          "original_form": "아 그거 우리집꽃",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.18",
          "form": "벚꽃 다 진뒤 피는거 ㅋㅋㅋ 그게 이쁘데",
          "original_form": "벚꽃 다 진뒤 피는거 ㅋㅋㅋ 그게 이쁘데",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020791.1.19",
          "form": "그그안압지인가? 거기두 이쁘구 ㅋㅋㅋㅋㅋ",
          "original_form": "그그안압지인가? 거기두 이쁘구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020791.1.20",
          "form": "마자늦게펴",
          "original_form": "마자늦게펴",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020791.1.21",
          "form": "내년에꼭간다!",
          "original_form": "내년에꼭간다!",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020791.1.22",
          "form": "내년에는 잠잠해질껴 ㅋㅋㅋㅋ",
          "original_form": "내년에는 잠잠해질껴 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020791.1.23",
          "form": "난 11월 초 부산이나 갈라규",
          "original_form": "난 11월 초 부산이나 갈라규",
          "speaker_id": "2",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020791.1.24",
          "form": "내대신 야구보러가줘",
          "original_form": "내대신 야구보러가줘",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020791.1.25",
          "form": "야구... ㅋㅋㅋㅋㅋㅋㅋ 아 롯데 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "야구... ㅋㅋㅋㅋㅋㅋㅋ 아 롯데 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020791.1.26",
          "form": "빵집털어줘",
          "original_form": "빵집털어줘",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020791.1.27",
          "form": "예쁜숙소많더라",
          "original_form": "예쁜숙소많더라",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020791.1.28",
          "form": "친구가 있으니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "친구가 있으니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020791.1.29",
          "form": "돼지국밥이랑 막창 맛집은 갈거 같은데 ㅋㅋㅋㅋ",
          "original_form": "돼지국밥이랑 막창 맛집은 갈거 같은데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020791.1.30",
          "form": "숙소는 저렴히 가지 않을 가 싶규",
          "original_form": "숙소는 저렴히 가지 않을 가 싶규",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020791.1.31",
          "form": "아친구있어?",
          "original_form": "아친구있어?",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020791.1.32",
          "form": "인싸네",
          "original_form": "인싸네",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020791.1.33",
          "form": "부산 로컬 친구 있쥐",
          "original_form": "부산 로컬 친구 있쥐",
          "speaker_id": "2",
          "time": "20210828 12:45"
        }
      ]
