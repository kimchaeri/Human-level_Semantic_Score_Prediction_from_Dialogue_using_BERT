      "utterance": [
        {
          "id": "MDRW2100024607.1.1",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024607.1.2",
          "form": "계단오르기 조깅만 가끔해요!",
          "original_form": "계단오르기 조깅만 가끔해요!",
          "speaker_id": "1",
          "time": "20210831 17:55"
        },
        {
          "id": "MDRW2100024607.1.3",
          "form": "오 계단오르기 좋네요",
          "original_form": "오 계단오르기 좋네요",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024607.1.4",
          "form": "아파트에서 하세요?",
          "original_form": "아파트에서 하세요?",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024607.1.5",
          "form": "네네! 마땅히 할곳이 없더라고요",
          "original_form": "네네! 마땅히 할곳이 없더라고요",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100024607.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024607.1.7",
          "form": "뭔가 좀.. 바깥을 보면서 즐거운 계단 오르기를 하고 싶은데",
          "original_form": "뭔가 좀.. 바깥을 보면서 즐거운 계단 오르기를 하고 싶은데",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024607.1.8",
          "form": "그럴 공간이 없어서ㅠ 항상 비상계단이나 오르게 돼요..",
          "original_form": "그럴 공간이 없어서ㅠ 항상 비상계단이나 오르게 돼요..",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024607.1.9",
          "form": "맞아요ㅠ 맘편히 운동할공간이 있으면 좋겠어요",
          "original_form": "맞아요ㅠ 맘편히 운동할공간이 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100024607.1.10",
          "form": "육삼빌딩 올라보고싶어요 ㅋㅋㅋㅋ",
          "original_form": "육삼빌딩 올라보고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100024607.1.11",
          "form": "죽겠죠..? 숨차서..",
          "original_form": "죽겠죠..? 숨차서..",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100024607.1.12",
          "form": "한번엔 거의 힘들죠ㅋㅋㅋㅋㅋ 전 10층도 숨차요..!",
          "original_form": "한번엔 거의 힘들죠ㅋㅋㅋㅋㅋ 전 10층도 숨차요..!",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100024607.1.13",
          "form": "한참 운동 할때는 15층까지는 그냥 올랐는데",
          "original_form": "한참 운동 할때는 15층까지는 그냥 올랐는데",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100024607.1.14",
          "form": "또 쉬니까 바로 안되더라고요",
          "original_form": "또 쉬니까 바로 안되더라고요",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100024607.1.15",
          "form": "이야 대단해요",
          "original_form": "이야 대단해요",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100024607.1.16",
          "form": "이제 안돼요 ㅋㅋㅋㅋ",
          "original_form": "이제 안돼요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100024607.1.17",
          "form": "그래두요ㅋㅋㅋㅋㅋ",
          "original_form": "그래두요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100024607.1.18",
          "form": "저는 분발해야겠어용",
          "original_form": "저는 분발해야겠어용",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100024607.1.19",
          "form": "8층만 올라도 숨차여 저는 이제..",
          "original_form": "8층만 올라도 숨차여 저는 이제..",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100024607.1.20",
          "form": "저는 5층만 올라도 숨차요ㅋㅋㅋㅋ",
          "original_form": "저는 5층만 올라도 숨차요ㅋㅋㅋㅋ",
