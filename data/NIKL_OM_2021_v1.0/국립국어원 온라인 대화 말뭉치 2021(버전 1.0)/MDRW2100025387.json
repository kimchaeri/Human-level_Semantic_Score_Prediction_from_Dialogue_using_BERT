      "utterance": [
        {
          "id": "MDRW2100025387.1.1",
          "form": "반갑읍니다",
          "original_form": "반갑읍니다",
          "speaker_id": "1",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025387.1.2",
          "form": "오 안녕안녕",
          "original_form": "오 안녕안녕",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025387.1.3",
          "form": "요새 무슨 게임하시나여???,",
          "original_form": "요새 무슨 게임하시나여???,",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025387.1.4",
          "form": "옵ㅊ...",
          "original_form": "옵ㅊ...",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.5",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.6",
          "form": "...ㅜ",
          "original_form": "...ㅜ",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.7",
          "form": "요새 블리자드 망했다던데....",
          "original_form": "요새 블리자드 망했다던데....",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.8",
          "form": "누가 망겜하누",
          "original_form": "누가 망겜하누",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.9",
          "form": "저요저요",
          "original_form": "저요저요",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.10",
          "form": "아직도 옵치를 하시는군여...",
          "original_form": "아직도 옵치를 하시는군여...",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.11",
          "form": "이제 일상이죠",
          "original_form": "이제 일상이죠",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.12",
          "form": "요새 다 롤하지않나",
          "original_form": "요새 다 롤하지않나",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.13",
          "form": "밥먹고 옵치하고",
          "original_form": "밥먹고 옵치하고",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.14",
          "form": "배그나...",
          "original_form": "배그나...",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.15",
          "form": "롤은 안해봤어요",
          "original_form": "롤은 안해봤어요",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.16",
          "form": "아 그러고보니 옵치였나",
          "original_form": "아 그러고보니 옵치였나",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.17",
          "form": "배그두... 한길 인생오져따리",
          "original_form": "배그두... 한길 인생오져따리",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025387.1.18",
          "form": "블리자드직원이름따왔는데",
          "original_form": "블리자드직원이름따왔는데",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025387.1.19",
          "form": "블리자드 내부비리연관되서",
          "original_form": "블리자드 내부비리연관되서",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025387.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025387.1.21",
          "form": "블리자드 제시 맥크리",
          "original_form": "블리자드 제시 맥크리",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025387.1.22",
          "form": "캐이름이 바뀐다고",
          "original_form": "캐이름이 바뀐다고",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025387.1.23",
          "form": "걔 이제 이름 못쓴다고",
          "original_form": "걔 이제 이름 못쓴다고",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025387.1.24",
          "form": "이게 무슨 일이람",
          "original_form": "이게 무슨 일이람",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025387.1.25",
          "form": "ㅋㅋㄱㄱㄱㅋㅋ 아니...실존인물에서",
          "original_form": "ㅋㅋㄱㄱㄱㅋㅋ 아니...실존인물에서",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025387.1.26",
          "form": "이름좀따오지말지",
          "original_form": "이름좀따오지말지",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025387.1.27",
          "form": "이제 안한대요",
          "original_form": "이제 안한대요",
          "speaker_id": "1",
          "time": "20210901 10:41"
        }
      ]
