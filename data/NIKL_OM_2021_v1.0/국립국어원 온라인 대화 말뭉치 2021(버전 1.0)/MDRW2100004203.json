      "utterance": [
        {
          "id": "MDRW2100004203.1.1",
          "form": "혹시 여행 가는거 좋아해?",
          "original_form": "혹시 여행 가는거 좋아해?",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004203.1.2",
          "form": "완전 좋아하지!",
          "original_form": "완전 좋아하지!",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004203.1.3",
          "form": "여행가는거좋아해??",
          "original_form": "여행가는거좋아해??",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004203.1.4",
          "form": "나도 좋아해!",
          "original_form": "나도 좋아해!",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004203.1.5",
          "form": "너는 제일 좋았던 여행지가 있다면 어디야?",
          "original_form": "너는 제일 좋았던 여행지가 있다면 어디야?",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004203.1.6",
          "form": "나는 부산! 또 가고싶어ㅠㅠ",
          "original_form": "나는 부산! 또 가고싶어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004203.1.7",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004203.1.8",
          "form": "부산 좋지ㅠㅠ",
          "original_form": "부산 좋지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004203.1.9",
          "form": "나는 홍콩에 갔던게 좋았던거 같아! 국내는 제주도!",
          "original_form": "나는 홍콩에 갔던게 좋았던거 같아! 국내는 제주도!",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004203.1.10",
          "form": "부산에서는 뭐가 제일 기억에 남았어?",
          "original_form": "부산에서는 뭐가 제일 기억에 남았어?",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004203.1.11",
          "form": "투명케이블카 탔던거!",
          "original_form": "투명케이블카 탔던거!",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004203.1.12",
          "form": "우와 홍콩 재밌었겠다",
          "original_form": "우와 홍콩 재밌었겠다",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004203.1.13",
          "form": "넌 뭐가 제일 좋았어?",
          "original_form": "넌 뭐가 제일 좋았어?",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004203.1.14",
          "form": "나는 먹는게 좋았어ㅎㅎㅎㅎ",
          "original_form": "나는 먹는게 좋았어ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004203.1.15",
          "form": "딤섬이 너무 맛있더라ㅠ",
          "original_form": "딤섬이 너무 맛있더라ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004203.1.16",
          "form": "오 딤섬ㅠㅠ맛있겠다",
          "original_form": "오 딤섬ㅠㅠ맛있겠다",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004203.1.17",
          "form": "얼마동안 다녀온거야??",
          "original_form": "얼마동안 다녀온거야??",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004203.1.18",
          "form": "그땐 일주일 정도 다녀왔어!",
          "original_form": "그땐 일주일 정도 다녀왔어!",
          "speaker_id": "1",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004203.1.19",
          "form": "부산여행은 어느정도 다녀온거야?",
          "original_form": "부산여행은 어느정도 다녀온거야?",
          "speaker_id": "1",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004203.1.20",
          "form": "난 2박3일! 짧게 다녀왔는데",
          "original_form": "난 2박3일! 짧게 다녀왔는데",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004203.1.21",
          "form": "그래도 잼썼어ㅎㅎ",
          "original_form": "그래도 잼썼어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004203.1.22",
          "form": "좋겠다~ 혹시 언제 다녀온 여행이야?",
          "original_form": "좋겠다~ 혹시 언제 다녀온 여행이야?",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004203.1.23",
          "form": "나도 부산 가보고 싶은데 어느 계절이 제일 좋은지 모르겠어",
          "original_form": "나도 부산 가보고 싶은데 어느 계절이 제일 좋은지 모르겠어",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004203.1.24",
          "form": "최근엔 여행 못간지 한참 됐구 2,3년 전에!",
          "original_form": "최근엔 여행 못간지 한참 됐구 2,3년 전에!",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004203.1.25",
          "form": "난 여름에 갔었는데도",
          "original_form": "난 여름에 갔었는데도",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004203.1.26",
          "form": "좋더라구 바다보고!",
          "original_form": "좋더라구 바다보고!",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004203.1.27",
          "form": "역시 부산은 여름이 좋은가? 다음에 꼭 참고할게!",
          "original_form": "역시 부산은 여름이 좋은가? 다음에 꼭 참고할게!",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004203.1.28",
          "form": "코로나 때문에 언제 다시 여행 갈 수 있을지는 모르겠지만ㅠㅠ",
          "original_form": "코로나 때문에 언제 다시 여행 갈 수 있을지는 모르겠지만ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:49"
        }
      ]
    }
  ]
