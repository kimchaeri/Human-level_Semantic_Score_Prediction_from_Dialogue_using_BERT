      "utterance": [
        {
          "id": "MDRW2100017153.1.1",
          "form": "요리 하시나요??",
          "original_form": "요리 하시나요??",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017153.1.2",
          "form": "잘 못해요 ㅠㅠ 잘 하세요??",
          "original_form": "잘 못해요 ㅠㅠ 잘 하세요??",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017153.1.3",
          "form": "아뇨 저도 간편식 정도?",
          "original_form": "아뇨 저도 간편식 정도?",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017153.1.4",
          "form": "배달은 많이 시켜드시나요?",
          "original_form": "배달은 많이 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017153.1.5",
          "form": "엄청 시켜먹어요 ㅋㅋㅋ",
          "original_form": "엄청 시켜먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017153.1.6",
          "form": "ㅋㅋ 뭐 제일 자주 시켜드세요??",
          "original_form": "ㅋㅋ 뭐 제일 자주 시켜드세요??",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.7",
          "form": "로제떡볶이?",
          "original_form": "로제떡볶이?",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.8",
          "form": "요샌 유가네닭갈비가 맛있더라구요",
          "original_form": "요샌 유가네닭갈비가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.9",
          "form": "저는 유가네닭갈비 닭갈비보다",
          "original_form": "저는 유가네닭갈비 닭갈비보다",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.10",
          "form": "ㅋㅋㅋ 로제떡볶이도 자주 먹어요 또 먹고 싶네요",
          "original_form": "ㅋㅋㅋ 로제떡볶이도 자주 먹어요 또 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.11",
          "form": "닭갈비볶음밥이",
          "original_form": "닭갈비볶음밥이",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.12",
          "form": "그렇게맛있더라구요",
          "original_form": "그렇게맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.13",
          "form": "맞아요 그 양념에다가..",
          "original_form": "맞아요 그 양념에다가..",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017153.1.14",
          "form": "그래서 배달시킬 때 볶음밥만 시켜먹어요 ㅋㅋ",
          "original_form": "그래서 배달시킬 때 볶음밥만 시켜먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017153.1.15",
          "form": "어디 로제 제일 좋아하세요?",
          "original_form": "어디 로제 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017153.1.16",
          "form": "저는 신전 좋아해요 양도 적당하고 꾸덕해서 맛있어요",
          "original_form": "저는 신전 좋아해요 양도 적당하고 꾸덕해서 맛있어요",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017153.1.17",
          "form": "오 아직 신전 로제는 못먹어봤어요",
          "original_form": "오 아직 신전 로제는 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017153.1.18",
          "form": "배떡 삼첩 명랑 엽떡 까지는 먹어봤는데",
          "original_form": "배떡 삼첩 명랑 엽떡 까지는 먹어봤는데",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017153.1.19",
          "form": "와 많이 드셔보셨네요 명랑까지",
          "original_form": "와 많이 드셔보셨네요 명랑까지",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017153.1.20",
          "form": "저는 배떡 삼첩 신전이요 ㅋㅋㅋ",
          "original_form": "저는 배떡 삼첩 신전이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017153.1.21",
          "form": "명랑 그제 먹어봤어요! 맛있더라구요",
          "original_form": "명랑 그제 먹어봤어요! 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017153.1.22",
          "form": "명랑에 로제파는줄은 몰랐어요",
          "original_form": "명랑에 로제파는줄은 몰랐어요",
          "speaker_id": "1",
          "time": "20210826 17:45"
        }
      ]
    }
