        {
          "id": "MDRW2100021191.1.1",
          "form": "네 매일봐요 ㅋㅋㅋ",
          "original_form": "네 매일봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021191.1.2",
          "form": "보세요?",
          "original_form": "보세요?",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021191.1.3",
          "form": "자주봅니다ㅎㅎ",
          "original_form": "자주봅니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021191.1.4",
          "form": "어떤거보시나요?",
          "original_form": "어떤거보시나요?",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021191.1.5",
          "form": "여행유튭이요",
          "original_form": "여행유튭이요",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021191.1.6",
          "form": "아 누구보세요?",
          "original_form": "아 누구보세요?",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021191.1.7",
          "form": "빠니보틀이요!",
          "original_form": "빠니보틀이요!",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021191.1.8",
          "form": "오 매일보시나요??",
          "original_form": "오 매일보시나요??",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021191.1.9",
          "form": "올라오는거 다 보는편이에요ㅎㅎ",
          "original_form": "올라오는거 다 보는편이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021191.1.10",
          "form": "아 애청자시네요 ㅋㅋㅋ",
          "original_form": "아 애청자시네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021191.1.11",
          "form": "저는 먹방위주로 봐요",
          "original_form": "저는 먹방위주로 봐요",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021191.1.12",
          "form": "히밥! 엄청잘먹어요",
          "original_form": "히밥! 엄청잘먹어요",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021191.1.13",
          "form": "오 들어본거같아요",
          "original_form": "오 들어본거같아요",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021191.1.14",
          "form": "저는 나도님 봐요 햄지님이랑요",
          "original_form": "저는 나도님 봐요 햄지님이랑요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021191.1.15",
          "form": "햄지님 그 중국참교육 맞나요",
          "original_form": "햄지님 그 중국참교육 맞나요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021191.1.16",
          "form": "언뜻 들은거같은데 잘 모르겠어요 뭔 일 있었다곤 하더라고요",
          "original_form": "언뜻 들은거같은데 잘 모르겠어요 뭔 일 있었다곤 하더라고요",
