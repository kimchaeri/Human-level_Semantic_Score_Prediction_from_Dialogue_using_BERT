        },
        {
          "id": "MDRW2100021407.1.2",
          "form": "카카오가루 두스푼에다가 알룰로스 살짝 넣어서",
          "original_form": "카카오가루 두스푼에다가 알룰로스 살짝 넣어서",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.3",
          "form": "갈아먹으면",
          "original_form": "갈아먹으면",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.4",
          "form": "완전 초코아이스크림임",
          "original_form": "완전 초코아이스크림임",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.5",
          "form": "개맛있어",
          "original_form": "개맛있어",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.6",
          "form": "알룰로스는 뭐야?",
          "original_form": "알룰로스는 뭐야?",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.7",
          "form": "설탕대체품인데",
          "original_form": "설탕대체품인데",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.8",
          "form": "꿀같은거임",
          "original_form": "꿀같은거임",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.9",
          "form": "근데 칼로리가 진짜낮아",
          "original_form": "근데 칼로리가 진짜낮아",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021407.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100021407.1.11",
          "form": "교수님한테 들은것 같다",
          "original_form": "교수님한테 들은것 같다",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021407.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021407.1.13",
          "form": "나도 작년 생물학시간에",
          "original_form": "나도 작년 생물학시간에",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021407.1.14",
          "form": "설탕대체감미료 보고서쓰면서 알앗음",
          "original_form": "설탕대체감미료 보고서쓰면서 알앗음",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021407.1.15",
          "form": "다음에 너네집 놀러가면",
          "original_form": "다음에 너네집 놀러가면",
          "speaker_id": "2",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021407.1.16",
          "form": "만들어줘",
          "original_form": "만들어줘",
          "speaker_id": "2",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021407.1.17",
          "form": "zㅋㅋㅋㅋㅋㅋ그래",
          "original_form": "zㅋㅋㅋㅋㅋㅋ그래",
          "speaker_id": "1",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021407.1.18",
          "form": "이거 갈아서 몇시간 얼려먹어야하는데",
          "original_form": "이거 갈아서 몇시간 얼려먹어야하는데",
          "speaker_id": "1",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021407.1.19",
          "form": "나 걍 귀찮아서 믹서기째로 퍼먹음",
          "original_form": "나 걍 귀찮아서 믹서기째로 퍼먹음",
          "speaker_id": "1",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021407.1.20",
          "form": "내취향이야",
          "original_form": "내취향이야",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021407.1.21",
          "form": "한번에 다 퍼먹기",
          "original_form": "한번에 다 퍼먹기",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021407.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ고마워><",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ고마워><",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100021407.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100021407.1.24",
          "form": "내일은 또 뭐먹지",
          "original_form": "내일은 또 뭐먹지",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100021407.1.25",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100021407.1.26",
          "form": "부지런하게 챙겨먹던 박수정돌아와...",
          "original_form": "부지런하게 챙겨먹던 박수정돌아와...",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100021407.1.27",
          "form": "9월부터 다시 다이어트 하자",
          "original_form": "9월부터 다시 다이어트 하자",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100021407.1.28",
          "form": "맞아 너 만들어 먹는거 보면",
          "original_form": "맞아 너 만들어 먹는거 보면",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100021407.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그래",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그래",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100021407.1.30",
          "form": "진짜 대단했는데",
          "original_form": "진짜 대단했는데",
