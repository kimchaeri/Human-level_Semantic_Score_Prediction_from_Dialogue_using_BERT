      "utterance": [
        {
          "id": "MDRW2100014802.1.1",
          "form": "음... 이렇게 생각하시면되요ㅎ 내 얼굴을똑 닮은 아이가",
          "original_form": "음... 이렇게 생각하시면되요ㅎ 내 얼굴을똑 닮은 아이가",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014802.1.2",
          "form": "태어나서 나만 사랑해주는거?",
          "original_form": "태어나서 나만 사랑해주는거?",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014802.1.3",
          "form": "누구 많이 닮았나요?",
          "original_form": "누구 많이 닮았나요?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014802.1.4",
          "form": "저를 많이 닮아서ㅎㅎ",
          "original_form": "저를 많이 닮아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014802.1.5",
          "form": "결혼은 아이에요ㅎㅎㅎ",
          "original_form": "결혼은 아이에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014802.1.6",
          "form": "정말 많이 싸우니깐ㅍㅍ",
          "original_form": "정말 많이 싸우니깐ㅍㅍ",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014802.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014802.1.8",
          "form": "다들 그소리...",
          "original_form": "다들 그소리...",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014802.1.9",
          "form": "결혼 준비하면서도 겁나 싸운다고...",
          "original_form": "결혼 준비하면서도 겁나 싸운다고...",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014802.1.10",
          "form": "네 맞아요ㅎ",
          "original_form": "네 맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014802.1.11",
          "form": "결혼하고나서도요ㅎㅎ",
          "original_form": "결혼하고나서도요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014802.1.12",
          "form": "그걸 남자가 다 참아야해요 ㅎㅎ",
          "original_form": "그걸 남자가 다 참아야해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014802.1.13",
          "form": "아이땜에 참는건가요?",
          "original_form": "아이땜에 참는건가요?",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014802.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014802.1.15",
          "form": "암요ㅎㅎ",
          "original_form": "암요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014802.1.16",
          "form": "육아를해도",
          "original_form": "육아를해도",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014802.1.17",
          "form": "아이때문에참죠ㅎ",
          "original_form": "아이때문에참죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014802.1.18",
          "form": "제친구가 얼마전에 전화해서",
          "original_form": "제친구가 얼마전에 전화해서",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014802.1.19",
          "form": "와이프랑 싸우고",
          "original_form": "와이프랑 싸우고",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014802.1.20",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014802.1.21",
          "form": "애땜에 산다고",
          "original_form": "애땜에 산다고",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014802.1.22",
          "form": "아니었음 진작에,...",
          "original_form": "아니었음 진작에,...",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014802.1.23",
          "form": "끝....",
          "original_form": "끝....",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014802.1.24",
          "form": "그게정답이라서ㅎㅎ",
          "original_form": "그게정답이라서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014802.1.25",
          "form": "저도 이제 끝이요ㅎㅎㅎ",
          "original_form": "저도 이제 끝이요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014802.1.26",
          "form": "이혼곧해요ㅎㅎ",
          "original_form": "이혼곧해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014802.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014802.1.28",
          "form": "모에요",
          "original_form": "모에요",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014802.1.29",
          "form": "아이때문에 제 삶을 낭비할수없다는생각에 서로 각자 행복하게 살기로했어요ㅎ",
          "original_form": "아이때문에 제 삶을 낭비할수없다는생각에 서로 각자 행복하게 살기로했어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014802.1.30",
          "form": "육아는 진짜 힘들죠?",
          "original_form": "육아는 진짜 힘들죠?",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014802.1.31",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014802.1.32",
          "form": "힘들죠ㄹㄹ",
          "original_form": "힘들죠ㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014802.1.33",
          "form": "항상 힘들어요ㅎ 아이가 내마음대로 안되서ㅎㅎ",
          "original_form": "항상 힘들어요ㅎ 아이가 내마음대로 안되서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014802.1.34",
          "form": "아직 어리니까....",
          "original_form": "아직 어리니까....",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014802.1.35",
          "form": "아들인가요 딸인가요?",
          "original_form": "아들인가요 딸인가요?",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014802.1.36",
          "form": "딸이요ㅎㅎ",
          "original_form": "딸이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014802.1.37",
          "form": "너무이뻐요ㅎㅎㅎ",
          "original_form": "너무이뻐요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014802.1.38",
          "form": "꼭 한명을 낳으실꺼면 딸을 낳으세요ㅎㄹ",
          "original_form": "꼭 한명을 낳으실꺼면 딸을 낳으세요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014802.1.39",
          "form": "저도 딸 원하거든요 근데 그게 제맘대로 되나요....ㅜ",
          "original_form": "저도 딸 원하거든요 근데 그게 제맘대로 되나요....ㅜ",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014802.1.40",
          "form": "글죠ㅠ",
          "original_form": "글죠ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014802.1.41",
          "form": "그래도 딸이건 아들이건ㅎㅎ 내자식이면 뭐든 이뻐요ㅎㅎ",
          "original_form": "그래도 딸이건 아들이건ㅎㅎ 내자식이면 뭐든 이뻐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014802.1.42",
          "form": "그렇죠 ㅋㅋ",
          "original_form": "그렇죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014802.1.43",
          "form": "키우긴 힘들어도",
          "original_form": "키우긴 힘들어도",
          "speaker_id": "1",
          "time": "20210825 21:06"
        }
      ]
