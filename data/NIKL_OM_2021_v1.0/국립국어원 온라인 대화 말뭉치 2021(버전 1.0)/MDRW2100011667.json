      "utterance": [
        {
          "id": "MDRW2100011667.1.1",
          "form": "현재 대학 재학중이신가요?",
          "original_form": "현재 대학 재학중이신가요?",
          "speaker_id": "1",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100011667.1.2",
          "form": "네 대학생이에요!",
          "original_form": "네 대학생이에요!",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011667.1.3",
          "form": "무슨 분야 전공중이신지 여쭤봐도 될까요?",
          "original_form": "무슨 분야 전공중이신지 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011667.1.4",
          "form": "교육학 전공하고 있어요",
          "original_form": "교육학 전공하고 있어요",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011667.1.5",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011667.1.6",
          "form": "네 저도 대학생이고 디자인 전공하고 있어요",
          "original_form": "네 저도 대학생이고 디자인 전공하고 있어요",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100011667.1.7",
          "form": "오오 그렇군요",
          "original_form": "오오 그렇군요",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011667.1.8",
          "form": "다음 학기 수강신청은 잘 하셨나요?",
          "original_form": "다음 학기 수강신청은 잘 하셨나요?",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011667.1.9",
          "form": "원하는 시간대로 23학점 꽉채우기 성공했어요",
          "original_form": "원하는 시간대로 23학점 꽉채우기 성공했어요",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011667.1.10",
          "form": "name2님은 잘 하셨나요?",
          "original_form": "&name2&님은 잘 하셨나요?",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011667.1.11",
          "form": "저는 너무 힘들 것 같아서 원하지 않았던 교수님 수업을 두 개나 잡아서..살짝 망했어요ㅎㅎ..",
          "original_form": "저는 너무 힘들 것 같아서 원하지 않았던 교수님 수업을 두 개나 잡아서..살짝 망했어요ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011667.1.12",
          "form": "ㅠ 힘드시겠어요",
          "original_form": "ㅠ 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011667.1.13",
          "form": "열심히 해야죠..",
          "original_form": "열심히 해야죠..",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100011667.1.14",
          "form": "name1님도 다음학기 23학점이라 바쁘시겠어요",
          "original_form": "&name1&님도 다음학기 23학점이라 바쁘시겠어요",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100011667.1.15",
          "form": "한 번 불살라야죠...ㅋㅋ",
          "original_form": "한 번 불살라야죠...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100011667.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100011667.1.17",
          "form": "다음주가 개강이라는 게 실감이 안 나요..",
          "original_form": "다음주가 개강이라는 게 실감이 안 나요..",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100011667.1.18",
          "form": "맞아요... 다음주 수요일이던가요...",
          "original_form": "맞아요... 다음주 수요일이던가요...",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100011667.1.19",
          "form": "오 수요일이시군요..  전 월요일이에요",
          "original_form": "오 수요일이시군요..  전 월요일이에요",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
