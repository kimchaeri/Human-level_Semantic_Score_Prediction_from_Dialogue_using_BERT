        {
          "id": "MDRW2100039243.1.1",
          "form": "그쵸그쵸  그거 흰여울 마을 가는데 거기 진짜 좋던데요",
          "original_form": "그쵸그쵸  그거 흰여울 마을 가는데 거기 진짜 좋던데요",
          "speaker_id": "1",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100039243.1.2",
          "form": "2층버스 타보셨나요??",
          "original_form": "2층버스 타보셨나요??",
          "speaker_id": "2",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100039243.1.3",
          "form": "어떤가요??",
          "original_form": "어떤가요??",
          "speaker_id": "2",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100039243.1.4",
          "form": "오 흰여율",
          "original_form": "오 흰여율",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039243.1.5",
          "form": "저는 개인적으로 영도쪽가는게 좋았어요",
          "original_form": "저는 개인적으로 영도쪽가는게 좋았어요",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039243.1.6",
          "form": "커플들 진짜 많이오더라구요",
          "original_form": "커플들 진짜 많이오더라구요",
          "speaker_id": "1",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039243.1.7",
          "form": "거기 사진찍을곳이 많더라구요",
          "original_form": "거기 사진찍을곳이 많더라구요",
          "speaker_id": "1",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100039243.1.8",
          "form": "커플한테인기쟝이인가바요",
          "original_form": "커플한테인기쟝이인가바요",
          "speaker_id": "2",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100039243.1.9",
          "form": "카페도 많아요",
          "original_form": "카페도 많아요",
          "speaker_id": "1",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100039243.1.10",
          "form": "아진짜요오??그쪽은 안가본거같아요",
          "original_form": "아진짜요오??그쪽은 안가본거같아요",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100039243.1.11",
          "form": "남친 있으시면 놀러가보세요",
          "original_form": "남친 있으시면 놀러가보세요",
          "speaker_id": "1",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100039243.1.12",
          "form": "영도는 뭐가 좋던가요",
          "original_form": "영도는 뭐가 좋던가요",
          "speaker_id": "1",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100039243.1.13",
          "form": "저 영도에서 바다봤어요 ㅋㅋㅋ",
          "original_form": "저 영도에서 바다봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039243.1.14",
          "form": "역시 부산바다가 짱이뇨",
          "original_form": "역시 부산바다가 짱이뇨",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039243.1.15",
          "form": "맞어요 다대포도가보고싶어요",
          "original_form": "맞어요 다대포도가보고싶어요",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039243.1.16",
          "form": "저도 안가본",
          "original_form": "저도 안가본",
          "speaker_id": "1",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100039243.1.17",
          "form": "ㅋㅋㅋㅋ 다음에 가보고 후기알려주세요 ㅋㅋ",
