      "utterance": [
        {
          "id": "MDRW2100004345.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004345.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004345.1.3",
          "form": "혹시 좋아하는 스포츠 있으세요?",
          "original_form": "혹시 좋아하는 스포츠 있으세요?",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004345.1.4",
          "form": "네 저는 여자배구요!",
          "original_form": "네 저는 여자배구요!",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004345.1.5",
          "form": "이번에 올림픽에서 여자배구 보고 팬됐어요",
          "original_form": "이번에 올림픽에서 여자배구 보고 팬됐어요",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004345.1.6",
          "form": "혹시 여자배구 보셨어요?",
          "original_form": "혹시 여자배구 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004345.1.7",
          "form": "완전 재밌게 봤어요!!",
          "original_form": "완전 재밌게 봤어요!!",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004345.1.8",
          "form": "저녁형 인간인데 배구 보러 아침에 일어나고ㅋㅋㅋㅋㅋ",
          "original_form": "저녁형 인간인데 배구 보러 아침에 일어나고ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004345.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ다들 같은 마음이엇군여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ다들 같은 마음이엇군여",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004345.1.10",
          "form": "직관은 가보셨나요??",
          "original_form": "직관은 가보셨나요??",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004345.1.11",
          "form": "이번에 코보컵 이틀뒤에? 한다는데",
          "original_form": "이번에 코보컵 이틀뒤에? 한다는데",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004345.1.12",
          "form": "그건 무관중일것같고 브이리그때는 ...유관중해줬으면..",
          "original_form": "그건 무관중일것같고 브이리그때는 ...유관중해줬으면..",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004345.1.13",
          "form": "그러니까요ㅠ 직접 가서 보고 응원하면 진짜 좋을 것 같은데..",
          "original_form": "그러니까요ㅠ 직접 가서 보고 응원하면 진짜 좋을 것 같은데..",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004345.1.14",
          "form": "이번 올림픽도 직접 봤으면 진짜 몇 배로 재미있었을 것 같더라고요ㅋㅋㅠ",
          "original_form": "이번 올림픽도 직접 봤으면 진짜 몇 배로 재미있었을 것 같더라고요ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004345.1.15",
          "form": "마자요 텅빈 관중석이 좀.. 그랬어요",
          "original_form": "마자요 텅빈 관중석이 좀.. 그랬어요",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004345.1.16",
          "form": "근데 유관중이여도 좀..ㅋㅋㅋㅋ",
          "original_form": "근데 유관중이여도 좀..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004345.1.17",
          "form": "좋아하시는 다른 스포츠있으세요?",
          "original_form": "좋아하시는 다른 스포츠있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004345.1.18",
          "form": "원래 스포츠에 관심이 없었는데 이번에 클라이밍 보니까 좀 재미있어 보이더라고요!",
          "original_form": "원래 스포츠에 관심이 없었는데 이번에 클라이밍 보니까 좀 재미있어 보이더라고요!",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004345.1.19",
          "form": "그리고 양궁이랑 복싱도요!",
          "original_form": "그리고 양궁이랑 복싱도요!",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004345.1.20",
          "form": "오 클라이밍 여자선수 인상깊었어요",
          "original_form": "오 클라이밍 여자선수 인상깊었어요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004345.1.21",
          "form": "양궁도 안산선수 화제성 짱이구요",
          "original_form": "양궁도 안산선수 화제성 짱이구요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004345.1.22",
          "form": "양궁은 경기관람이 될까요?",
          "original_form": "양궁은 경기관람이 될까요?",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004345.1.23",
          "form": "양궁도 리그가 있나..?",
          "original_form": "양궁도 리그가 있나..?",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004345.1.24",
          "form": "아 양궁은 국내리그가 없대요",
          "original_form": "아 양궁은 국내리그가 없대요",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004345.1.25",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004345.1.26",
          "form": "클라이밍은 대한산악연맹에서 관리하나봐요",
          "original_form": "클라이밍은 대한산악연맹에서 관리하나봐요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004345.1.27",
          "form": "서채현 선수!마자요",
          "original_form": "서채현 선수!마자요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004345.1.28",
          "form": "이 분 이름 찾고싶었어요",
          "original_form": "이 분 이름 찾고싶었어요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004345.1.29",
          "form": "아 맞아요 서채현 선수!",
          "original_form": "아 맞아요 서채현 선수!",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004345.1.30",
          "form": "진짜 신기하더라고요",
          "original_form": "진짜 신기하더라고요",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004345.1.31",
          "form": "어떻게 저 벽을 타고 올라가지? 싶었어요ㅋㅋㅋ",
          "original_form": "어떻게 저 벽을 타고 올라가지? 싶었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004345.1.32",
          "form": "발 짚고 그런거보면 머리도 좋아야 할것같고",
          "original_form": "발 짚고 그런거보면 머리도 좋아야 할것같고",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004345.1.33",
          "form": "너무 대단했어요",
          "original_form": "너무 대단했어요",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004345.1.34",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004345.1.35",
          "form": "저 너무 관심생겨서 직접 해보고 싶어서 찾아보기까지 했어요ㅋㅋㅋ",
          "original_form": "저 너무 관심생겨서 직접 해보고 싶어서 찾아보기까지 했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004345.1.36",
          "form": "근데 균형감각이 중요하다더라고요",
          "original_form": "근데 균형감각이 중요하다더라고요",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004345.1.37",
          "form": "전..균형감각이 안 좋아서ㅋㅋㅋㅋ 좀..노력이 필요할 것 같네요",
          "original_form": "전..균형감각이 안 좋아서ㅋㅋㅋㅋ 좀..노력이 필요할 것 같네요",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004345.1.38",
          "form": "와 균형감각.. 전 안될듯ㅋㅋㅋㅋ",
          "original_form": "와 균형감각.. 전 안될듯ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004345.1.39",
          "form": "저도 배구 경기 보고 배구 하고싶어서 동호회 찾아봤는데",
          "original_form": "저도 배구 경기 보고 배구 하고싶어서 동호회 찾아봤는데",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004345.1.40",
          "form": "한달이면 자기가 할수잇는지 없는지 알수있대요",
          "original_form": "한달이면 자기가 할수잇는지 없는지 알수있대요",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004345.1.41",
          "form": "너무 극명하게 소질이 갈린대요",
          "original_form": "너무 극명하게 소질이 갈린대요",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004345.1.42",
          "form": "스포츠 경기보고 자기도 스포츠 하고싶어서 학원이나 그런데 찾는 사람도 은근 많을듯해요",
          "original_form": "스포츠 경기보고 자기도 스포츠 하고싶어서 학원이나 그런데 찾는 사람도 은근 많을듯해요",
          "speaker_id": "1",
