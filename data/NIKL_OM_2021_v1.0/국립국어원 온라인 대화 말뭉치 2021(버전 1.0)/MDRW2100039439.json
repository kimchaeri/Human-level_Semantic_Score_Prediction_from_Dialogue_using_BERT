      "utterance": [
        {
          "id": "MDRW2100039439.1.1",
          "form": "앗 저는 최근에 복숭아 샀는데ㅋㅋㅋㅋㅋ",
          "original_form": "앗 저는 최근에 복숭아 샀는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039439.1.2",
          "form": "요즘 사과 맛있대요",
          "original_form": "요즘 사과 맛있대요",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039439.1.3",
          "form": "진짜 사과가 제철이네요!",
          "original_form": "진짜 사과가 제철이네요!",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039439.1.4",
          "form": "네 곧 추석이라ㅜㅜ",
          "original_form": "네 곧 추석이라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039439.1.5",
          "form": "조금만 늦으면 추석때문에 더 비싸질 것 같은데",
          "original_form": "조금만 늦으면 추석때문에 더 비싸질 것 같은데",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039439.1.6",
          "form": "때 맞춰 잘 사셨네요!",
          "original_form": "때 맞춰 잘 사셨네요!",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.7",
          "form": "그쵸... 얼른 과일 사먹어야해요",
          "original_form": "그쵸... 얼른 과일 사먹어야해요",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.8",
          "form": "저도 추석전에 이것저것 더 사둬야하는데ㅠ",
          "original_form": "저도 추석전에 이것저것 더 사둬야하는데ㅠ",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.9",
          "form": "name1님?은 과일 좋아하세요??",
          "original_form": "&name1&님?은 과일 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.10",
          "form": "배송밀리기 전에 사셔야해요ㅋㅋㅋㅋ",
          "original_form": "배송밀리기 전에 사셔야해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.11",
          "form": "저는 복숭아 좋아하고",
          "original_form": "저는 복숭아 좋아하고",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.12",
          "form": "여름내내 수박도 많이 먹었어요!",
          "original_form": "여름내내 수박도 많이 먹었어요!",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.13",
          "form": "맞아요 추석배송ㅋㅋㄱㅋㅠ",
          "original_form": "맞아요 추석배송ㅋㅋㄱㅋㅠ",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.14",
          "form": "헉 부럽다ㅜㅜ 저는 일정때문에 수박 많이 못 사먹었어요",
          "original_form": "헉 부럽다ㅜㅜ 저는 일정때문에 수박 많이 못 사먹었어요",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039439.1.15",
          "form": "늦은 결정은 배송만 밀리게 할 뿐",
          "original_form": "늦은 결정은 배송만 밀리게 할 뿐",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.16",
          "form": "추석배송 진짜...ㅋㅋㅋ 네..",
          "original_form": "추석배송 진짜...ㅋㅋㅋ 네..",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.17",
          "form": "옷도 샀는데",
          "original_form": "옷도 샀는데",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.18",
          "form": "오 가을옷 사셨나요?",
          "original_form": "오 가을옷 사셨나요?",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.19",
          "form": "좀 늦어지네요ㅜㅜ",
          "original_form": "좀 늦어지네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.20",
          "form": "네~!!",
          "original_form": "네~!!",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.21",
          "form": "날씨가 갑자기 추워져서",
          "original_form": "날씨가 갑자기 추워져서",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.22",
          "form": "요즘에 흰색 바지에 꽂혀서ㅋㅋㅋㅋ",
          "original_form": "요즘에 흰색 바지에 꽂혀서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.24",
          "form": "저도 긴팔 긴바지가 급하더라구요ㅋㅋㅋ",
          "original_form": "저도 긴팔 긴바지가 급하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.25",
          "form": "여름 옷 이제 넣을 때가 됐죠",
          "original_form": "여름 옷 이제 넣을 때가 됐죠",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.26",
          "form": "옷정리도 싹했어요",
          "original_form": "옷정리도 싹했어요",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.27",
          "form": "흰바지 가을에 입기 딱 좋을 것 같아요!",
          "original_form": "흰바지 가을에 입기 딱 좋을 것 같아요!",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039439.1.28",
          "form": "더 추워지기 전에 옷 사세요ㅋㅋㅋ",
          "original_form": "더 추워지기 전에 옷 사세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.29",
          "form": "저는 거의",
          "original_form": "저는 거의",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.30",
          "form": "저도 빨리 옷정리해야 새 옷을 살 수 있을텐데!",
          "original_form": "저도 빨리 옷정리해야 새 옷을 살 수 있을텐데!",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.31",
          "form": "하의가 검정이라",
          "original_form": "하의가 검정이라",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.32",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.33",
          "form": "옷 정리하고나면",
          "original_form": "옷 정리하고나면",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.34",
          "form": "맞아요 지금 빨리 안 사면",
          "original_form": "맞아요 지금 빨리 안 사면",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.35",
          "form": "와 이게 우리집 옷장에??? 가 연신 나옵니다,,,",
          "original_form": "와 이게 우리집 옷장에??? 가 연신 나옵니다,,,",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.36",
          "form": "많이 추운 추석을 보내게 될 것 같아요ㅋㅋㅋㅜㅜ",
          "original_form": "많이 추운 추석을 보내게 될 것 같아요ㅋㅋㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.37",
          "form": "지금 안사면 이제",
          "original_form": "지금 안사면 이제",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.38",
          "form": "와 이거 진짜",
          "original_form": "와 이거 진짜",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.39",
          "form": "추석 이후에 올거에요",
          "original_form": "추석 이후에 올거에요",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039439.1.40",
          "form": "맞아요 옷 정리 안하면 꼭 비슷한 옷 두 번 사게되고",
          "original_form": "맞아요 옷 정리 안하면 꼭 비슷한 옷 두 번 사게되고",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039439.1.41",
          "form": "옷쇼핑도 부지런해야 잘 할 수 있는 것 같아요ㅋㅋㅋ",
          "original_form": "옷쇼핑도 부지런해야 잘 할 수 있는 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:45"
        }
      ]
