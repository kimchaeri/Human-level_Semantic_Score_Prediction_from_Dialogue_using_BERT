      "utterance": [
        {
          "id": "MDRW2100023986.1.1",
          "form": "우리 부모님은 또 나훈아한텐 큰 관심이 없어요",
          "original_form": "우리 부모님은 또 나훈아한텐 큰 관심이 없어요",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023986.1.2",
          "form": "하... 효도 참 힘드네요 ㅋㅋ",
          "original_form": "하... 효도 참 힘드네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023986.1.3",
          "form": "아 그 미스터트롯 멤버들은 안좋아하시나요?",
          "original_form": "아 그 미스터트롯 멤버들은 안좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023986.1.4",
          "form": "아! 임영웅 좋아한다!",
          "original_form": "아! 임영웅 좋아한다!",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023986.1.5",
          "form": "근데 그것도 요즘은 조금 시들해지셨나",
          "original_form": "근데 그것도 요즘은 조금 시들해지셨나",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023986.1.6",
          "form": "사랑의 콜센터 안보시더라구요",
          "original_form": "사랑의 콜센터 안보시더라구요",
          "speaker_id": "1",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100023986.1.7",
          "form": "아,, 흠, 저희엄마도 약간 시들해지신,,ㅎㅎ",
          "original_form": "아,, 흠, 저희엄마도 약간 시들해지신,,ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100023986.1.8",
          "form": "하기사, 덕질엔 떡밥이 중요한데",
          "original_form": "하기사, 덕질엔 떡밥이 중요한데",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023986.1.9",
          "form": "딱히 양질의 떡밥이 많진 않죠",
          "original_form": "딱히 양질의 떡밥이 많진 않죠",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023986.1.10",
          "form": "맞아요 떡밥이중요하져 ㅎㅎ 최근에본영화있으세요??",
          "original_form": "맞아요 떡밥이중요하져 ㅎㅎ 최근에본영화있으세요??",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023986.1.11",
          "form": "사실 영화는 영화관은 안가니까",
          "original_form": "사실 영화는 영화관은 안가니까",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023986.1.12",
          "form": "집구석에서 옛날 영화를 재탕하게 되더라고요",
          "original_form": "집구석에서 옛날 영화를 재탕하게 되더라고요",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023986.1.13",
          "form": "그래서 가장 최근에 본게 금발이 너무해",
          "original_form": "그래서 가장 최근에 본게 금발이 너무해",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023986.1.14",
          "form": "조금 유치하지만 귀엽고 재밌고 행복해지는 영화죠",
          "original_form": "조금 유치하지만 귀엽고 재밌고 행복해지는 영화죠",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023986.1.15",
          "form": "금발이너무해 제목은 많이들었는데 보진않앗어요 ㅎ 저는 대만영화가 재밌더라구요",
          "original_form": "금발이너무해 제목은 많이들었는데 보진않앗어요 ㅎ 저는 대만영화가 재밌더라구요",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023986.1.16",
          "form": "오, 대만드라마는 예전에 하나 봤었는데",
          "original_form": "오, 대만드라마는 예전에 하나 봤었는데",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023986.1.17",
          "form": "어떤 영화 보셨어요?",
          "original_form": "어떤 영화 보셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023986.1.18",
          "form": "젤 좋았던건 나의소녀시대요! 그거보고 왕대륙에 빠졌었어요 ㅎㅎ",
          "original_form": "젤 좋았던건 나의소녀시대요! 그거보고 왕대륙에 빠졌었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023986.1.19",
          "form": "오! 저도 그거 들어봤어요",
          "original_form": "오! 저도 그거 들어봤어요",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100023986.1.20",
          "form": "사람 설레게 만든다고",
          "original_form": "사람 설레게 만든다고",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100023986.1.21",
          "form": "맞아요,,, 진짜,, 영화관에서 봤거든요 그래서 더 뭔가 그랬어요 ㅎㅎㅎㅎ",
          "original_form": "맞아요,,, 진짜,, 영화관에서 봤거든요 그래서 더 뭔가 그랬어요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100023986.1.22",
          "form": "어머어머 영화관에서 보셨구나",
          "original_form": "어머어머 영화관에서 보셨구나",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023986.1.23",
          "form": "네 그래서 더 빠져들었었죠,,,큰 화면에서 봐서긍가 ㅎㅎ",
          "original_form": "네 그래서 더 빠져들었었죠,,,큰 화면에서 봐서긍가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023986.1.24",
          "form": "큰화면 좋죠",
          "original_form": "큰화면 좋죠",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023986.1.25",
          "form": "전 이제 팝콘도 못뜯는거 걍 아이패드로 보는데",
          "original_form": "전 이제 팝콘도 못뜯는거 걍 아이패드로 보는데",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023986.1.26",
          "form": "가끔은 큰 화면과 서라운드가 그립기도 해요",
          "original_form": "가끔은 큰 화면과 서라운드가 그립기도 해요",
          "speaker_id": "1",
          "time": "20210830 21:28"
        }
      ]
