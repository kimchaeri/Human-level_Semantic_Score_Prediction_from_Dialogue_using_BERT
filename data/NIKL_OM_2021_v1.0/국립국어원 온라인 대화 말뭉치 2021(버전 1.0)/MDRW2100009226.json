      "utterance": [
        {
          "id": "MDRW2100009226.1.1",
          "form": "오늘 날씨 어땠어요?ㅎ",
          "original_form": "오늘 날씨 어땠어요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009226.1.2",
          "form": "오늘은 비안온거 같아요",
          "original_form": "오늘은 비안온거 같아요",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009226.1.3",
          "form": "태풍오고있다던데요",
          "original_form": "태풍오고있다던데요",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009226.1.4",
          "form": "태풍이오고야 말았네요ㅠ",
          "original_form": "태풍이오고야 말았네요ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009226.1.5",
          "form": "이번주에 당직이 걸릴확이률이있겟어요ㅠ",
          "original_form": "이번주에 당직이 걸릴확이률이있겟어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009226.1.6",
          "form": "에잇ㅠ",
          "original_form": "에잇ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009226.1.7",
          "form": "업무에 영향좀 있겠어요",
          "original_form": "업무에 영향좀 있겠어요",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009226.1.8",
          "form": "아랫지방 많이오나보던데",
          "original_form": "아랫지방 많이오나보던데",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009226.1.9",
          "form": "날씨에 민감한거라ㅠ",
          "original_form": "날씨에 민감한거라ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009226.1.10",
          "form": "아랫지방이라 더 걱정되네여ㅎ",
          "original_form": "아랫지방이라 더 걱정되네여ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009226.1.11",
          "form": "제주도 영향권인데 400미리까지온대요",
          "original_form": "제주도 영향권인데 400미리까지온대요",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009226.1.12",
          "form": "어쩔수없죠뭐ㅎ 피항보내야지ㅎ",
          "original_form": "어쩔수없죠뭐ㅎ 피항보내야지ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009226.1.13",
          "form": "비바람 몰아친다고",
          "original_form": "비바람 몰아친다고",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009226.1.14",
          "form": "지가 와봐야 태풍이죠ㅎ",
          "original_form": "지가 와봐야 태풍이죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009226.1.15",
          "form": "ㅋㅋ 무섭게 올지도 몰라요",
          "original_form": "ㅋㅋ 무섭게 올지도 몰라요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009226.1.16",
          "form": "비피해 없게 조심하세요",
          "original_form": "비피해 없게 조심하세요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009226.1.17",
          "form": "ㅎㅎ 그래야죠ㅎ 오늘 기상도 보내야하는데",
          "original_form": "ㅎㅎ 그래야죠ㅎ 오늘 기상도 보내야하는데",
          "speaker_id": "2",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009226.1.18",
          "form": "알면서도 주말엔 쉬고싶은ㅎㅎ",
          "original_form": "알면서도 주말엔 쉬고싶은ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009226.1.19",
          "form": "아에 안보냈어요ㅎㅎㅎㅎㅎㅎ",
          "original_form": "아에 안보냈어요ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009226.1.20",
          "form": "낼 하시면 되죠ㅎ",
          "original_form": "낼 하시면 되죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.21",
          "form": "낼아침에는 업무니깐 보내야죠ㅎ",
          "original_form": "낼아침에는 업무니깐 보내야죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.22",
          "form": "비오면 출근하기 귀찮은데",
          "original_form": "비오면 출근하기 귀찮은데",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.23",
          "form": "기상도 보내는것도 일이에요ㅠ",
          "original_form": "기상도 보내는것도 일이에요ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.24",
          "form": "출근길에만 오지말길",
          "original_form": "출근길에만 오지말길",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.25",
          "form": "글죠ㅠ",
          "original_form": "글죠ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.26",
          "form": "버스타세요?",
          "original_form": "버스타세요?",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.27",
          "form": "아님 지하철?",
          "original_form": "아님 지하철?",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009226.1.28",
          "form": "아뇨 회사근처로 이사와서 걸어서 10분",
          "original_form": "아뇨 회사근처로 이사와서 걸어서 10분",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009226.1.29",
          "form": "에고ㅠ 차는 없으시구요?",
          "original_form": "에고ㅠ 차는 없으시구요?",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009226.1.30",
          "form": "걸어서 가면... 비오면 싫겠네여ㅠ",
          "original_form": "걸어서 가면... 비오면 싫겠네여ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009226.1.31",
          "form": "요기 코앞이예요ㅋㅋ",
          "original_form": "요기 코앞이예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009226.1.32",
          "form": "뛰어가면 5분ㅋ",
          "original_form": "뛰어가면 5분ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:27"
        }
      ]
    }
