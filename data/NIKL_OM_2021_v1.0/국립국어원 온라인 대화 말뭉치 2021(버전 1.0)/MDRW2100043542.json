      "utterance": [
        {
          "id": "MDRW2100043542.1.1",
          "form": "영양제 사주던가",
          "original_form": "영양제 사주던가",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043542.1.2",
          "form": "어떤 영양제면 꾸준히 먹을까?",
          "original_form": "어떤 영양제면 꾸준히 먹을까?",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043542.1.3",
          "form": "먹기편한거",
          "original_form": "먹기편한거",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043542.1.4",
          "form": "멋잇는거",
          "original_form": "멋잇는거",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043542.1.5",
          "form": "맛잇는거 아니다",
          "original_form": "맛잇는거 아니다",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043542.1.6",
          "form": "사실 잘 안먹을듯",
          "original_form": "사실 잘 안먹을듯",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043542.1.7",
          "form": "ㅋㅋ 꾸준히 먹는게 영양제도 약 같아서 구찮어",
          "original_form": "ㅋㅋ 꾸준히 먹는게 영양제도 약 같아서 구찮어",
          "speaker_id": "2",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043542.1.8",
          "form": "요즘 많이 힘들어??  심적으로??",
          "original_form": "요즘 많이 힘들어??  심적으로??",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043542.1.9",
          "form": "아니 ㅋㅋㅋㅋ 안힘들어",
          "original_form": "아니 ㅋㅋㅋㅋ 안힘들어",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043542.1.10",
          "form": "심적으로가 아니라 물리적으로야",
          "original_form": "심적으로가 아니라 물리적으로야",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043542.1.11",
          "form": "아하 물리적으로 였구만",
          "original_form": "아하 물리적으로 였구만",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043542.1.12",
          "form": "운동해야하는데",
          "original_form": "운동해야하는데",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043542.1.13",
          "form": "생각만해",
          "original_form": "생각만해",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043542.1.14",
          "form": "운동도 여유가 있어서 하는건  아닌데 참 잘 잘 안돼",
          "original_form": "운동도 여유가 있어서 하는건  아닌데 참 잘 잘 안돼",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043542.1.15",
          "form": "지금 심정으론 재활부터 해야하는거 아닌가 싶기도 해 ㅋㅋㅋ",
          "original_form": "지금 심정으론 재활부터 해야하는거 아닌가 싶기도 해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043542.1.16",
          "form": "재활?  그 정도야??",
          "original_form": "재활?  그 정도야??",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043542.1.17",
          "form": "체력키우기가 먼저인거같은데?",
          "original_form": "체력키우기가 먼저인거같은데?",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043542.1.18",
          "form": "그게 재활이지",
          "original_form": "그게 재활이지",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043542.1.19",
          "form": "회사랑 대학만 아니면 나 완전 체력왕인데",
          "original_form": "회사랑 대학만 아니면 나 완전 체력왕인데",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043542.1.20",
          "form": "그럼 일단 좀 줄일 수 있는걸 줄여볼까??  공부를 좀 천천히? 는 어때?  과목 수 줄여보자",
          "original_form": "그럼 일단 좀 줄일 수 있는걸 줄여볼까??  공부를 좀 천천히? 는 어때?  과목 수 줄여보자",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043542.1.21",
          "form": "과목수를 줄이면",
          "original_form": "과목수를 줄이면",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043542.1.22",
          "form": "대학 졸업이 늦어지잖아",
          "original_form": "대학 졸업이 늦어지잖아",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043542.1.23",
          "form": "안되지....",
          "original_form": "안되지....",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043542.1.24",
          "form": "그런가 몇 년 만 힘내보자",
          "original_form": "그런가 몇 년 만 힘내보자",
          "speaker_id": "2",
          "time": "20210907 19:46"
        }
      ]
