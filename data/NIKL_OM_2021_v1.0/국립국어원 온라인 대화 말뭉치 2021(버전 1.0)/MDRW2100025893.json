      "utterance": [
        {
          "id": "MDRW2100025893.1.1",
          "form": "안녕하세용 여긴 부산인데 비가오네요",
          "original_form": "안녕하세용 여긴 부산인데 비가오네요",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100025893.1.2",
          "form": "안녕하세요. 부산은 아직 비가 오는군요",
          "original_form": "안녕하세요. 부산은 아직 비가 오는군요",
          "speaker_id": "1",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100025893.1.3",
          "form": "서울은 흐리기만해요",
          "original_form": "서울은 흐리기만해요",
          "speaker_id": "1",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100025893.1.4",
          "form": "비가 많이오나요?",
          "original_form": "비가 많이오나요?",
          "speaker_id": "1",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100025893.1.5",
          "form": "출근길에 추적추적와서 살짝 맞고 갔는데",
          "original_form": "출근길에 추적추적와서 살짝 맞고 갔는데",
          "speaker_id": "2",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100025893.1.6",
          "form": "여긴 항상 오다말다하네용 ㅎㅎ",
          "original_form": "여긴 항상 오다말다하네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100025893.1.7",
          "form": "그래도 많이오는건 아니라 다행이네요",
          "original_form": "그래도 많이오는건 아니라 다행이네요",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100025893.1.8",
          "form": "비.너무많이오면 무섭잖아요ㅠ",
          "original_form": "비.너무많이오면 무섭잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100025893.1.9",
          "form": "이제 한국도 뭔가 열대기후같아요",
          "original_form": "이제 한국도 뭔가 열대기후같아요",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100025893.1.10",
          "form": "그래도 이제 좀 선선하네요",
          "original_form": "그래도 이제 좀 선선하네요",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100025893.1.11",
          "form": "맞아요 이 비 지나고나면 훅 쌀쌀해질것 같아요",
          "original_form": "맞아요 이 비 지나고나면 훅 쌀쌀해질것 같아요",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100025893.1.12",
          "form": "가을 짧게 훅 지나갈것같아요",
          "original_form": "가을 짧게 훅 지나갈것같아요",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100025893.1.13",
          "form": "어제 기차역앞에는 물이 차가지구 ㅎㅎ",
          "original_form": "어제 기차역앞에는 물이 차가지구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100025893.1.14",
          "form": "여름이 예전보다 짧긴하네용",
          "original_form": "여름이 예전보다 짧긴하네용",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100025893.1.15",
          "form": "헐헐 물이 차다니ㅜㅠ",
          "original_form": "헐헐 물이 차다니ㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100025893.1.16",
          "form": "맞아요 ㅋㅋ 일주일정도는 정말 너무더워서 숨막혔는데",
          "original_form": "맞아요 ㅋㅋ 일주일정도는 정말 너무더워서 숨막혔는데",
          "speaker_id": "1",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100025893.1.17",
          "form": "그 주 제외하곤 또 괜찮았던 것 같아요",
          "original_form": "그 주 제외하곤 또 괜찮았던 것 같아요",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100025893.1.18",
          "form": "맞아용 ㅎㅎㅎ 별로 안더워서 다행이아도 해야나",
          "original_form": "맞아용 ㅎㅎㅎ 별로 안더워서 다행이아도 해야나",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100025893.1.19",
          "form": "맞아요 ㅋㅋ 근데 이제 점점 해짧아지니까 슬퍼요ㅠ",
          "original_form": "맞아요 ㅋㅋ 근데 이제 점점 해짧아지니까 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100025893.1.20",
          "form": "아 또 그렇네요 ㅜㅜㅠ 하루가 짧아지는..",
          "original_form": "아 또 그렇네요 ㅜㅜㅠ 하루가 짧아지는..",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100025893.1.21",
          "form": "여름이 벌레때매 싫은 점도 있지만 낮이 긴건 좋았는데ㅜ 벌써 해가 많이 짧아졌더라구요",
          "original_form": "여름이 벌레때매 싫은 점도 있지만 낮이 긴건 좋았는데ㅜ 벌써 해가 많이 짧아졌더라구요",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100025893.1.22",
          "form": "맞아요 뭔가 하루가 알차긴했는데 ㅎㅎ",
          "original_form": "맞아요 뭔가 하루가 알차긴했는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:28"
        }
      ]
