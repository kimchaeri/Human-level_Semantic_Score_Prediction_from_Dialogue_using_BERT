      "utterance": [
        {
          "id": "MDRW2100036089.1.1",
          "form": "비빔면 좋아하세요?",
          "original_form": "비빔면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 16:01"
        },
        {
          "id": "MDRW2100036089.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210830 16:03"
        },
        {
          "id": "MDRW2100036089.1.3",
          "form": "더울때 자주먹었네요",
          "original_form": "더울때 자주먹었네요",
          "speaker_id": "2",
          "time": "20210830 16:03"
        },
        {
          "id": "MDRW2100036089.1.4",
          "form": "배홍동 이나 진비빔?",
          "original_form": "배홍동 이나 진비빔?",
          "speaker_id": "2",
          "time": "20210830 16:03"
        },
        {
          "id": "MDRW2100036089.1.5",
          "form": "삼양에서 나온건데",
          "original_form": "삼양에서 나온건데",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100036089.1.6",
          "form": "열무비빔면이라고 있어용",
          "original_form": "열무비빔면이라고 있어용",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100036089.1.7",
          "form": "이거 되게 맛있어요",
          "original_form": "이거 되게 맛있어요",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100036089.1.8",
          "form": "먹어봤나기억이안나네요",
          "original_form": "먹어봤나기억이안나네요",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100036089.1.9",
          "form": "비빔면은 거의다 무난 이상은 한ㅡㄴ거같아요",
          "original_form": "비빔면은 거의다 무난 이상은 한ㅡㄴ거같아요",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036089.1.10",
          "form": "파는 마트가 잘 없긴한데 보이면 드셔보세요ㅎㅎ",
          "original_form": "파는 마트가 잘 없긴한데 보이면 드셔보세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036089.1.11",
          "form": "넵ㅎ 보이면 카트에 넣어볼게요",
          "original_form": "넵ㅎ 보이면 카트에 넣어볼게요",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036089.1.12",
          "form": "라면은 뭐 좋아하세요?",
          "original_form": "라면은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036089.1.13",
          "form": "저 참깨라면 좋아해요",
          "original_form": "저 참깨라면 좋아해요",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036089.1.14",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036089.1.15",
          "form": "저는 너구리요ㅎㅎ",
          "original_form": "저는 너구리요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036089.1.16",
          "form": "너구리 얼큰인가요?",
          "original_form": "너구리 얼큰인가요?",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036089.1.17",
          "form": "넵ㅎㅎ",
          "original_form": "넵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036089.1.18",
          "form": "다시마가 들어있어서 맛있죠ㅋㅋㅋ",
          "original_form": "다시마가 들어있어서 맛있죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036089.1.19",
          "form": "다시마 꼭 먹어요",
          "original_form": "다시마 꼭 먹어요",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036089.1.20",
          "form": "당연하져",
          "original_form": "당연하져",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036089.1.21",
          "form": "별미!",
          "original_form": "별미!",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036089.1.22",
          "form": "전 늘 그거 먹고있었는데",
          "original_form": "전 늘 그거 먹고있었는데",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036089.1.23",
          "form": "원래 먹는거 아니다 먹는거다 논란 생겨서 의아했어요ㅋㅋ",
          "original_form": "원래 먹는거 아니다 먹는거다 논란 생겨서 의아했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100036089.1.24",
          "form": "읭 그거 엄선해서 고른거래요",
          "original_form": "읭 그거 엄선해서 고른거래요",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036089.1.25",
          "form": "버리면 아깝죠",
          "original_form": "버리면 아깝죠",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036089.1.26",
          "form": "당연히 먹는거 아니었어....? 싶었는데 버리는 사람도 많더라구요",
          "original_form": "당연히 먹는거 아니었어....? 싶었는데 버리는 사람도 많더라구요",
          "speaker_id": "1",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036089.1.27",
          "form": "몸에좋은걸 ㅠ",
          "original_form": "몸에좋은걸 ㅠ",
          "speaker_id": "2",
