      "utterance": [
        {
          "id": "MDRW2100019295.1.1",
          "form": "군것질 좋아하시나요?",
          "original_form": "군것질 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019295.1.2",
          "form": "네 저는 군것질 너무 조아해용 과자 젤리.. 초콜렛..",
          "original_form": "네 저는 군것질 너무 조아해용 과자 젤리.. 초콜렛..",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019295.1.3",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019295.1.4",
          "form": "저도 좋죠!!! 근데 너무 단거는 별로 안좋아해요",
          "original_form": "저도 좋죠!!! 근데 너무 단거는 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100019295.1.5",
          "form": "꼬북칩 쵸코 추러스보다 꼬깔콘같은 과자가 더좋아요",
          "original_form": "꼬북칩 쵸코 추러스보다 꼬깔콘같은 과자가 더좋아요",
          "speaker_id": "2",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100019295.1.6",
          "form": "오 저두 꼬깔콘 좋아요. 꼬북칩은 인절미맛이 맛있어요 ㅋㅋㅋ",
          "original_form": "오 저두 꼬깔콘 좋아요. 꼬북칩은 인절미맛이 맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:41"
        },
        {
          "id": "MDRW2100019295.1.7",
          "form": "드셔보셨어요? ㅋㅋㅋ",
          "original_form": "드셔보셨어요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:41"
        },
        {
          "id": "MDRW2100019295.1.8",
          "form": "아뇨!! 먹어볼게요",
          "original_form": "아뇨!! 먹어볼게요",
          "speaker_id": "2",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019295.1.9",
          "form": "저 인절미맛좋아합니당",
          "original_form": "저 인절미맛좋아합니당",
          "speaker_id": "2",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019295.1.10",
          "form": "ㅋㅋ네 인절미맛이 제일 좋아용",
          "original_form": "ㅋㅋ네 인절미맛이 제일 좋아용",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019295.1.11",
          "form": "저 설빙 인절미빙수도 좋아해요",
          "original_form": "저 설빙 인절미빙수도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019295.1.12",
          "form": "전 단맛 과자도 좋아하긴하는데 그런거면 약간 빈츠같이 초코(단거)+비스킷(덜단것) 섞인게좋아요",
          "original_form": "전 단맛 과자도 좋아하긴하는데 그런거면 약간 빈츠같이 초코(단거)+비스킷(덜단것) 섞인게좋아요",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019295.1.13",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019295.1.14",
          "form": "전부다달면 못먹어용",
          "original_form": "전부다달면 못먹어용",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019295.1.15",
          "form": "그래서 도넛같은거 잘 안먹어요",
          "original_form": "그래서 도넛같은거 잘 안먹어요",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019295.1.16",
          "form": "오우 맞아요 인절미빙수 넘 맛있죠",
          "original_form": "오우 맞아요 인절미빙수 넘 맛있죠",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019295.1.17",
          "form": "맞아요 너무 달면 입맛 버리는 느낌 ㅜㅠㅠ",
          "original_form": "맞아요 너무 달면 입맛 버리는 느낌 ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019295.1.18",
          "form": "그쵸 그래서 초코파이나 몽쉘 오예스 이런건 잘 안먹어요..",
          "original_form": "그쵸 그래서 초코파이나 몽쉘 오예스 이런건 잘 안먹어요..",
          "speaker_id": "1",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019295.1.19",
          "form": "빈츠 초코송이 빼빼로 이런게좋아요 ㅋㅋㅋ",
          "original_form": "빈츠 초코송이 빼빼로 이런게좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019295.1.20",
          "form": "와 빼빼로 먹고싶다",
          "original_form": "와 빼빼로 먹고싶다",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019295.1.21",
          "form": "마자여 그런건 진짜 가끔 땡길때",
          "original_form": "마자여 그런건 진짜 가끔 땡길때",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019295.1.22",
          "form": "한번씩 먹고",
          "original_form": "한번씩 먹고",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019295.1.23",
          "form": "그쵸 저는 빼빼로중에선 또 포키좋아해요 ㅋㅋㅋ 그거 얇아서 맛있어요",
          "original_form": "그쵸 저는 빼빼로중에선 또 포키좋아해요 ㅋㅋㅋ 그거 얇아서 맛있어요",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019295.1.24",
          "form": "극세사포키!!",
          "original_form": "극세사포키!!",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019295.1.25",
          "form": "맞아요!! 포키 맛있죠!!! 오도독오도독 먹고싶네여",
          "original_form": "맞아요!! 포키 맛있죠!!! 오도독오도독 먹고싶네여",
          "speaker_id": "2",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019295.1.26",
          "form": "그쵸 오독오독 ㅋㅋ",
          "original_form": "그쵸 오독오독 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019295.1.27",
          "form": "예전에 조청유과도 맛있어서 좋아했었어요 ㅋㅋ 적당히 달콤ㅋㅋㅋ",
          "original_form": "예전에 조청유과도 맛있어서 좋아했었어요 ㅋㅋ 적당히 달콤ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019295.1.28",
          "form": "아 과자얘기하니까 또 먹구싶네용ㅋㅋㅋ",
          "original_form": "아 과자얘기하니까 또 먹구싶네용ㅋㅋㅋ",
