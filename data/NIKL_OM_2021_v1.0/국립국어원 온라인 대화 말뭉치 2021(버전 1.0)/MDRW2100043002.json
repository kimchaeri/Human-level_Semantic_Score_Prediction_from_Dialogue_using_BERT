      "utterance": [
        {
          "id": "MDRW2100043002.1.1",
          "form": "혹시 반려동물 가족 있으세요?",
          "original_form": "혹시 반려동물 가족 있으세요?",
          "speaker_id": "1",
          "time": "20210907 12:52"
        },
        {
          "id": "MDRW2100043002.1.2",
          "form": "넹! 2살 포메라니안키워요",
          "original_form": "넹! 2살 포메라니안키워요",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043002.1.3",
          "form": "이름은 name3이고 갈색인데 귀여움",
          "original_form": "이름은 &name3&이고 갈색인데 귀여움",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043002.1.4",
          "form": "name1님은 키우는아기잇나요?",
          "original_form": "&name1&님은 키우는아기잇나요?",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043002.1.5",
          "form": "갈색name3... 벌써 귀여워요",
          "original_form": "갈색&name3&... 벌써 귀여워요",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043002.1.6",
          "form": "6살 치와와 키워요 이름은 name4에요",
          "original_form": "6살 치와와 키워요 이름은 &name4&에요",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043002.1.7",
          "form": "헐ㅠ 벌써귀여움",
          "original_form": "헐ㅠ 벌써귀여움",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043002.1.8",
          "form": "name4라니..name3이와 name4..",
          "original_form": "&name4&라니..&name3&이와 &name4&..",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043002.1.9",
          "form": "name4는 어떻게 만난친군가용",
          "original_form": "&name4&는 어떻게 만난친군가용",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043002.1.10",
          "form": "둘다... 고소하네요...",
          "original_form": "둘다... 고소하네요...",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043002.1.11",
          "form": "유기견 센터에서 떨고있어서 데려왔어요 12월 31일이었네요",
          "original_form": "유기견 센터에서 떨고있어서 데려왔어요 12월 31일이었네요",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100043002.1.12",
          "form": "name3이는 어떻게 만나셨어요?",
          "original_form": "&name3&이는 어떻게 만나셨어요?",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100043002.1.13",
          "form": "헉ㅠㅠ고소하단말이 넘귀엽고 웃겨요",
          "original_form": "헉ㅠㅠ고소하단말이 넘귀엽고 웃겨요",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.14",
          "form": "저는 임보하다가 제가 정이너무들어서",
          "original_form": "저는 임보하다가 제가 정이너무들어서",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.15",
          "form": "입양하게됐어요",
          "original_form": "입양하게됐어요",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.16",
          "form": "둘다 고소한 강쥐고 귀엽네요",
          "original_form": "둘다 고소한 강쥐고 귀엽네요",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.17",
          "form": "산책은 자주하시나요",
          "original_form": "산책은 자주하시나요",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.18",
          "form": "후후후 아주 고소해버려...",
          "original_form": "후후후 아주 고소해버려...",
          "speaker_id": "1",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.19",
          "form": "저는 2살이라그런지 매일안나가면 화내요ㅠ",
          "original_form": "저는 2살이라그런지 매일안나가면 화내요ㅠ",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.20",
          "form": "앞어깨 관절염이 있어서.. 오래 못걸어요...",
          "original_form": "앞어깨 관절염이 있어서.. 오래 못걸어요...",
          "speaker_id": "1",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100043002.1.21",
          "form": "ㅠㅠ 산책자주못나가서 우울해 있어요.. 부러워요",
          "original_form": "ㅠㅠ 산책자주못나가서 우울해 있어요.. 부러워요",
          "speaker_id": "1",
          "time": "20210907 12:57"
        },
        {
          "id": "MDRW2100043002.1.22",
          "form": "포메도 관절이 안좋은 편이지 않나요?",
          "original_form": "포메도 관절이 안좋은 편이지 않나요?",
          "speaker_id": "1",
          "time": "20210907 12:57"
        },
        {
          "id": "MDRW2100043002.1.23",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043002.1.24",
          "form": "그래서 자제하는데",
          "original_form": "그래서 자제하는데",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043002.1.25",
          "form": "앉나가면 현관중문앞에서",
          "original_form": "앉나가면 현관중문앞에서",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043002.1.26",
          "form": "저 쳐다봐요..",
          "original_form": "저 쳐다봐요..",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043002.1.27",
          "form": "그래도 매일 나가야하니까요ㅎㅎ",
          "original_form": "그래도 매일 나가야하니까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100043002.1.28",
          "form": "너무 귀여워!! 오늘 비도 와서 못나가는데 맘아파서 힘드시겠어요",
          "original_form": "너무 귀여워!! 오늘 비도 와서 못나가는데 맘아파서 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100043002.1.29",
          "form": "ㅠㅠ헉 그쪽은 비가왔나요",
          "original_form": "ㅠㅠ헉 그쪽은 비가왔나요",
          "speaker_id": "2",
          "time": "20210907 13:00"
        },
        {
