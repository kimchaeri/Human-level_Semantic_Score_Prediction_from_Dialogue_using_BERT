      "utterance": [
        {
          "id": "MDRW2100022421.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022421.1.2",
          "form": "ㅎㅎ 네 맛있게 먹고 왔어요",
          "original_form": "ㅎㅎ 네 맛있게 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100022421.1.3",
          "form": "지금 많이 바쁘세요?",
          "original_form": "지금 많이 바쁘세요?",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100022421.1.4",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022421.1.5",
          "form": "아니욥! 별로 안바빠요ㅋㅋ",
          "original_form": "아니욥! 별로 안바빠요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022421.1.6",
          "form": "저는 방금 참치회덮밥 먹고 왔어요 후후",
          "original_form": "저는 방금 참치회덮밥 먹고 왔어요 후후",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100022421.1.7",
          "form": "오오.. 최고로 맛있는거 드셨네요!!",
          "original_form": "오오.. 최고로 맛있는거 드셨네요!!",
          "speaker_id": "2",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100022421.1.8",
          "form": "어머! 집에서 만들어 먹었더니 더 꿀맛이에요 호호",
          "original_form": "어머! 집에서 만들어 먹었더니 더 꿀맛이에요 호호",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100022421.1.9",
          "form": "님은 뭐 드셨나용",
          "original_form": "님은 뭐 드셨나용",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100022421.1.10",
          "form": "대박! 쉽지않은데 대단하시네요",
          "original_form": "대박! 쉽지않은데 대단하시네요",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100022421.1.11",
          "form": "전 피자먹었어요!",
          "original_form": "전 피자먹었어요!",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100022421.1.12",
          "form": "어머 무슨 피자 드신거에요?",
          "original_form": "어머 무슨 피자 드신거에요?",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100022421.1.13",
          "form": "꿀고구마 먹었어요!!",
          "original_form": "꿀고구마 먹었어요!!",
          "speaker_id": "2",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100022421.1.14",
          "form": "꿀고구마 피자요?",
          "original_form": "꿀고구마 피자요?",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100022421.1.15",
          "form": "그런거 파는 브랜드도 이써요?",
          "original_form": "그런거 파는 브랜드도 이써요?",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100022421.1.16",
          "form": "넹 피자마루용ㅋㅋ",
          "original_form": "넹 피자마루용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100022421.1.17",
          "form": "아 제가 피잦마루는 잘 이용 안해봤네요 ㅠ",
          "original_form": "아 제가 피잦마루는 잘 이용 안해봤네요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100022421.1.18",
          "form": "피자마루 맛있어요?",
          "original_form": "피자마루 맛있어요?",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100022421.1.19",
          "form": "넹! 저게 진짜 맛나용! 가격도 저렴하고~~",
          "original_form": "넹! 저게 진짜 맛나용! 가격도 저렴하고~~",
          "speaker_id": "2",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100022421.1.20",
          "form": "동네 근처에 있었는데 담에 꼭 먹어봐야겠어요 ㅎㅎ",
          "original_form": "동네 근처에 있었는데 담에 꼭 먹어봐야겠어요 ㅎㅎ",
