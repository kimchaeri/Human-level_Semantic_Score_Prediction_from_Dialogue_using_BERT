        },
        {
          "id": "MDRW2100011011.1.19",
          "form": "그러니까 준기독교.",
          "original_form": "그러니까 준기독교.",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011011.1.20",
          "form": "하느님인가요 하나님인가요???",
          "original_form": "하느님인가요 하나님인가요???",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011011.1.21",
          "form": "저 궁금했어요",
          "original_form": "저 궁금했어요",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011011.1.22",
          "form": "하나님이에요 그 하느님은 천주교?",
