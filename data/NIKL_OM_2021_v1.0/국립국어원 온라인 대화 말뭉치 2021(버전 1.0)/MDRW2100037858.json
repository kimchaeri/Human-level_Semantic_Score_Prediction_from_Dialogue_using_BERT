      "utterance": [
        {
          "id": "MDRW2100037858.1.1",
          "form": "사실 돈 걱정만 아니면",
          "original_form": "사실 돈 걱정만 아니면",
          "speaker_id": "2",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037858.1.2",
          "form": "피티받아보고싶어요",
          "original_form": "피티받아보고싶어요",
          "speaker_id": "2",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037858.1.3",
          "form": "운동하다가 버거킹직원과 친분을ㅋㅋㅋㅋ",
          "original_form": "운동하다가 버거킹직원과 친분을ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037858.1.4",
          "form": "아님 필라테스나",
          "original_form": "아님 필라테스나",
          "speaker_id": "2",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037858.1.5",
          "form": "아 맞아요 저두 피티!!!",
          "original_form": "아 맞아요 저두 피티!!!",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037858.1.6",
          "form": "근데 너무터무니없이 비싸던",
          "original_form": "근데 너무터무니없이 비싸던",
          "speaker_id": "2",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037858.1.7",
          "form": "저 필라테스 다음주부터 가는데",
          "original_form": "저 필라테스 다음주부터 가는데",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037858.1.8",
          "form": "이게 혼자서는 홈트가 참 좋긴한데ㅜㅜ",
          "original_form": "이게 혼자서는 홈트가 참 좋긴한데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037858.1.9",
          "form": "왜 안할까요ㅜㅜ",
          "original_form": "왜 안할까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037858.1.10",
          "form": "회당 15000원 x 30회로 끊엇어요",
          "original_form": "회당 15000원 x 30회로 끊엇어요",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037858.1.11",
          "form": "홈트 너무 지겹지않나요",
          "original_form": "홈트 너무 지겹지않나요",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037858.1.12",
          "form": "가격 알려주시다니 천사셔ㅋㅋ",
          "original_form": "가격 알려주시다니 천사셔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037858.1.13",
          "form": "약간 가격? 이런거 물으면 예민하게 반응하는사람들있더라고요 ㅋㅋ",
          "original_form": "약간 가격? 이런거 물으면 예민하게 반응하는사람들있더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100037858.1.14",
          "form": "오 제가생각한거보다 싼건가",
          "original_form": "오 제가생각한거보다 싼건가",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100037858.1.15",
          "form": "1회로 계산해서 그렇게느껴지는것인가 ㅋㅋ",
          "original_form": "1회로 계산해서 그렇게느껴지는것인가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100037858.1.16",
          "form": "ㄹㅇ 필라테스는 가격 안알려줘요",
          "original_form": "ㄹㅇ 필라테스는 가격 안알려줘요",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100037858.1.17",
          "form": "등록하려면 값을알아야하는데 왜이리비밀스러운지;;;",
          "original_form": "등록하려면 값을알아야하는데 왜이리비밀스러운지;;;",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100037858.1.18",
          "form": "혹시  사는곳이 대도시세요 중소도시세요?",
          "original_form": "혹시  사는곳이 대도시세요 중소도시세요?",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100037858.1.19",
          "form": "저 대도시에요!",
          "original_form": "저 대도시에요!",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100037858.1.20",
