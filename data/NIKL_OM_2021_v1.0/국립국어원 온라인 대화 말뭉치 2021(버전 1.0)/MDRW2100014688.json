      "utterance": [
        {
          "id": "MDRW2100014688.1.1",
          "form": "너 그거 들었니",
          "original_form": "너 그거 들었니",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014688.1.2",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014688.1.3",
          "form": "연예인 루카스 말이야",
          "original_form": "연예인 루카스 말이야",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014688.1.4",
          "form": "다자간 연애를 했더라",
          "original_form": "다자간 연애를 했더라",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.5",
          "form": "정확히는 양양다리지",
          "original_form": "정확히는 양양다리지",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.6",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.7",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.8",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.9",
          "form": "이게 무슨 일이니",
          "original_form": "이게 무슨 일이니",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.10",
          "form": "누군지는 모르지만",
          "original_form": "누군지는 모르지만",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.11",
          "form": "지금 4명의 전연인이",
          "original_form": "지금 4명의 전연인이",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.12",
          "form": "폭로했어",
          "original_form": "폭로했어",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.13",
          "form": "아주 못된 사람이네",
          "original_form": "아주 못된 사람이네",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.14",
          "form": "찬열에 이은 새로운",
          "original_form": "찬열에 이은 새로운",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014688.1.15",
          "form": "연애 폭로야",
          "original_form": "연애 폭로야",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.16",
          "form": "엄청나네",
          "original_form": "엄청나네",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.17",
          "form": "여자들이 모든 데이트 비용을 부담했대",
          "original_form": "여자들이 모든 데이트 비용을 부담했대",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.18",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.19",
          "form": "식비 간식 숙박",
          "original_form": "식비 간식 숙박",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.20",
          "form": "심지어 담배 비용까지",
          "original_form": "심지어 담배 비용까지",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.21",
          "form": "놀라웠어",
          "original_form": "놀라웠어",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.22",
          "form": "와 너무 놀라운데",
          "original_form": "와 너무 놀라운데",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.23",
          "form": "연애의 참견에 나올법해",
          "original_form": "연애의 참견에 나올법해",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014688.1.24",
          "form": "여자들은 어떤 직업종사자들 일까",
          "original_form": "여자들은 어떤 직업종사자들 일까",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.25",
          "form": "어쩌다가",
          "original_form": "어쩌다가",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.26",
          "form": "여자들은 팬이래",
          "original_form": "여자들은 팬이래",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.27",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.28",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.29",
          "form": "주제에 대해서 얘기하래",
          "original_form": "주제에 대해서 얘기하래",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.30",
          "form": "연애는 즐겁지",
          "original_form": "연애는 즐겁지",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.31",
          "form": "연애하면 즐겁겠지",
          "original_form": "연애하면 즐겁겠지",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.32",
          "form": "그렇겠지 ㅎㅎ",
          "original_form": "그렇겠지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014688.1.33",
          "form": "결혼 생각은 없다고 했지",
          "original_form": "결혼 생각은 없다고 했지",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.34",
          "form": "비혼식은 계획있니",
          "original_form": "비혼식은 계획있니",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.35",
          "form": "요즘은 소개팅같은거 안받았니",
          "original_form": "요즘은 소개팅같은거 안받았니",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.36",
          "form": "비혼식이란것도 있니",
          "original_form": "비혼식이란것도 있니",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.37",
          "form": "4단계에서 소개팅은 무리인 것 같아",
          "original_form": "4단계에서 소개팅은 무리인 것 같아",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.38",
          "form": "그럼 비혼식이라고 있어",
          "original_form": "그럼 비혼식이라고 있어",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.39",
          "form": "그건 그렇네",
          "original_form": "그건 그렇네",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.40",
          "form": "그동안 낸 축의금을 비혼식을 통해 돌려받는 것이지",
          "original_form": "그동안 낸 축의금을 비혼식을 통해 돌려받는 것이지",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.41",
          "form": "비혼식은 처음 들어본다",
          "original_form": "비혼식은 처음 들어본다",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.42",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014688.1.43",
          "form": "대신 비혼식을 하면 결혼식 때 부를 수 없지",
          "original_form": "대신 비혼식을 하면 결혼식 때 부를 수 없지",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014688.1.44",
          "form": "신중히 고민해야겠네",
          "original_form": "신중히 고민해야겠네",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014688.1.45",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210825 20:23"
        }
      ]
    }
