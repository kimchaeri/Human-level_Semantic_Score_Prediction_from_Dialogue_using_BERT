      "utterance": [
        {
          "id": "MDRW2100014058.1.1",
          "form": "네 말해봐쇼",
          "original_form": "네 말해봐쇼",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014058.1.2",
          "form": "엑소 팬사 가느라",
          "original_form": "엑소 팬사 가느라",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014058.1.3",
          "form": "수학여행을 못갓어요",
          "original_form": "수학여행을 못갓어요",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014058.1.4",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014058.1.5",
          "form": "그때 딱 팬사인회라",
          "original_form": "그때 딱 팬사인회라",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014058.1.6",
          "form": "그래도 되는거에요?",
          "original_form": "그래도 되는거에요?",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.7",
          "form": "수학여행 안가고 그거 갔어요",
          "original_form": "수학여행 안가고 그거 갔어요",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.9",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.10",
          "form": "엄청난 팬이셨구나",
          "original_form": "엄청난 팬이셨구나",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.11",
          "form": "애들 흑돼지 사진 보낼때",
          "original_form": "애들 흑돼지 사진 보낼때",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.12",
          "form": "저 혼자 경수 찍어서 보내고",
          "original_form": "저 혼자 경수 찍어서 보내고",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.13",
          "form": "ㅋㅋㅋ대단하다 진짜",
          "original_form": "ㅋㅋㅋ대단하다 진짜",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.14",
          "form": "가서 뭐 물어봤어요",
          "original_form": "가서 뭐 물어봤어요",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014058.1.15",
          "form": "경수야 나 수학여행 째고 여기왔어.ㅈ.",
          "original_form": "경수야 나 수학여행 째고 여기왔어.ㅈ.",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014058.1.16",
          "form": "그러셨나요",
          "original_form": "그러셨나요",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014058.1.17",
          "form": "엇 그랬던 거 같기도 하고",
          "original_form": "엇 그랬던 거 같기도 하고",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014058.1.18",
          "form": "제가 행복하긴 했는데",
          "original_form": "제가 행복하긴 했는데",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014058.1.19",
          "form": "횡설수설 했을거에요 아마",
          "original_form": "횡설수설 했을거에요 아마",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014058.1.20",
          "form": "행복하면 된거죠",
          "original_form": "행복하면 된거죠",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014058.1.21",
          "form": "수능보기 전날에도 가고 그랬어요",
          "original_form": "수능보기 전날에도 가고 그랬어요",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014058.1.22",
          "form": "가장 좋아하는 가수를 눈앞엣 보는..",
          "original_form": "가장 좋아하는 가수를 눈앞엣 보는..",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014058.1.23",
          "form": "네..,",
          "original_form": "네..,",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014058.1.24",
          "form": "그러지 말걸..",
          "original_form": "그러지 말걸..",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014058.1.25",
          "form": "쓰읍..",
          "original_form": "쓰읍..",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014058.1.26",
          "form": "유툽 보시는 거 있으신가요",
          "original_form": "유툽 보시는 거 있으신가요",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014058.1.27",
          "form": "유튜브 예능 클립같은거 봐요",
          "original_form": "유튜브 예능 클립같은거 봐요",
          "speaker_id": "1",
