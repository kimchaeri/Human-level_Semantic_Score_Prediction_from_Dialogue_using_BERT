      "utterance": [
        {
          "id": "MDRW2100002775.1.1",
          "form": "마켓컬리 추천좀 해주세요",
          "original_form": "마켓컬리 추천좀 해주세요",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002775.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002775.1.3",
          "form": "연복 짜장면 맛있다던데",
          "original_form": "연복 짜장면 맛있다던데",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002775.1.4",
          "form": "오 구매해봐야겠네요",
          "original_form": "오 구매해봐야겠네요",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002775.1.5",
          "form": "도제 연어? 그거 맛있다는데",
          "original_form": "도제 연어? 그거 맛있다는데",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002775.1.6",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002775.1.7",
          "form": "근데 최근에 사건이 터져서",
          "original_form": "근데 최근에 사건이 터져서",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002775.1.8",
          "form": "무슨 사건이요?",
          "original_form": "무슨 사건이요?",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002775.1.9",
          "form": "좀 그런가봐요",
          "original_form": "좀 그런가봐요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002775.1.10",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002775.1.11",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002775.1.12",
          "form": "근데 가격이 좀 비싼 거 같아요",
          "original_form": "근데 가격이 좀 비싼 거 같아요",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002775.1.13",
          "form": "저도 그런 거 같아요",
          "original_form": "저도 그런 거 같아요",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002775.1.14",
          "form": "그래도 쿠폰 받으면 괜찮으니까요",
          "original_form": "그래도 쿠폰 받으면 괜찮으니까요",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002775.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002775.1.16",
          "form": "오래 구매 안하면 쿠폰 주더라구요 ㅋㅋ",
          "original_form": "오래 구매 안하면 쿠폰 주더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002775.1.17",
          "form": "금액이 점점 높아진다는 ㅋㅋㅋ",
          "original_form": "금액이 점점 높아진다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002775.1.18",
          "form": "ㅋㅋ 그러게요",
          "original_form": "ㅋㅋ 그러게요",
          "speaker_id": "1",
