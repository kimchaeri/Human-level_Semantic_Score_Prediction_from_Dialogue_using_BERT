          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040540.1.7",
          "form": "그래 싼 데는 싸다",
          "original_form": "그래 싼 데는 싸다",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040540.1.8",
          "form": "좋구만",
          "original_form": "좋구만",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040540.1.9",
