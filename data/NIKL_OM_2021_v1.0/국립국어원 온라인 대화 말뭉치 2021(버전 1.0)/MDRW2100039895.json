      "utterance": [
        {
          "id": "MDRW2100039895.1.1",
          "form": "어중간하게 재료가 남는게 싫어서요 ㅎㅎ",
          "original_form": "어중간하게 재료가 남는게 싫어서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039895.1.2",
          "form": "가족이랑 같이 사세요?",
          "original_form": "가족이랑 같이 사세요?",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039895.1.3",
          "form": "네 부모님과 형제자매같이요",
          "original_form": "네 부모님과 형제자매같이요",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039895.1.4",
          "form": "같이 사시는구나 저도용!!",
          "original_form": "같이 사시는구나 저도용!!",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039895.1.5",
          "form": "독립하고 싶을때 있으세요?",
          "original_form": "독립하고 싶을때 있으세요?",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039895.1.6",
          "form": "많죠ㅋㅋ작년에 한참 많이 싸웠어요ㅜㅠ",
          "original_form": "많죠ㅋㅋ작년에 한참 많이 싸웠어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039895.1.7",
          "form": "ㅋㅋㅋ 가족들이랑 어쩔수 없는거 같아요",
          "original_form": "ㅋㅋㅋ 가족들이랑 어쩔수 없는거 같아요",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039895.1.8",
          "form": "멀리 살면 해결된다는데",
          "original_form": "멀리 살면 해결된다는데",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039895.1.9",
          "form": "그러기엔 나갈수가 없네요 ㅠㅠ",
          "original_form": "그러기엔 나갈수가 없네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039895.1.10",
          "form": "집이 있어야!",
          "original_form": "집이 있어야!",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039895.1.11",
          "form": "맞아요ㅜㅠ 작년에 코로나때매 외출을 잘못하니까 더 심했어요!ㅜ",
          "original_form": "맞아요ㅜㅠ 작년에 코로나때매 외출을 잘못하니까 더 심했어요!ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039895.1.12",
          "form": "독립 진짜 돈이..ㅜ",
          "original_form": "독립 진짜 돈이..ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039895.1.13",
          "form": "독립이 진짜 힘들거 같아요",
          "original_form": "독립이 진짜 힘들거 같아요",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039895.1.14",
          "form": "지금 버는걸로는 절대",
          "original_form": "지금 버는걸로는 절대",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039895.1.15",
          "form": "ㅋㅋㅋ 시도도 하지 않으려고요",
          "original_form": "ㅋㅋㅋ 시도도 하지 않으려고요",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039895.1.16",
          "form": "ㅋㅋ서울이신가요? 그럼 더더욱...",
          "original_form": "ㅋㅋ서울이신가요? 그럼 더더욱...",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039895.1.17",
          "form": "서울안에서 독립이란...ㅜ",
          "original_form": "서울안에서 독립이란...ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039895.1.18",
          "form": "인천인데도 안돼욬ㅋㅋㅋㅋ",
          "original_form": "인천인데도 안돼욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039895.1.19",
          "form": "인천 집값마저 오르는 이 이상한 세상",
          "original_form": "인천 집값마저 오르는 이 이상한 세상",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039895.1.20",
          "form": "ㅋㅋ 차라리 집대신 차를 사라며ㅋㅋ",
          "original_form": "ㅋㅋ 차라리 집대신 차를 사라며ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039895.1.21",
          "form": "답답할때 드라이브하게요ㅜㅎㅎ",
          "original_form": "답답할때 드라이브하게요ㅜㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039895.1.22",
          "form": "ㅋㅋㅋ 악 운전면허도 없는데",
          "original_form": "ㅋㅋㅋ 악 운전면허도 없는데",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039895.1.23",
          "form": "집안일은 좀 하세요?",
          "original_form": "집안일은 좀 하세요?",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039895.1.24",
          "form": "저도 뚜벅이ㅋㅋ",
          "original_form": "저도 뚜벅이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039895.1.25",
          "form": "주말엔 거의 다 하고있어요ㅋㅋ양심상",
          "original_form": "주말엔 거의 다 하고있어요ㅋㅋ양심상",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039895.1.26",
          "form": "부지런하시네요!!",
          "original_form": "부지런하시네요!!",
          "speaker_id": "1",
          "time": "20210903 23:22"
        }
      ]
    }
