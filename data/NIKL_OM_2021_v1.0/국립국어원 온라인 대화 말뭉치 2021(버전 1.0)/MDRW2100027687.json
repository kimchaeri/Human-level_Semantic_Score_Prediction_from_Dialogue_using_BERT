        },
        {
          "id": "MDRW2100027687.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027687.1.46",
