        },
        {
          "id": "MDRW2100027687.1.2",
          "form": "근데 name2이랑 여친이랑 비슷한점도 많은데",
          "original_form": "근데 &name2&이랑 여친이랑 비슷한점도 많은데",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027687.1.3",
          "form": "다른점도 있네요 ㅋㅋㅋㅋ",
          "original_form": "다른점도 있네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027687.1.4",
          "form": "표현을 잘하시네요",
          "original_form": "표현을 잘하시네요",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027687.1.5",
          "form": "저도 원래는 잘 못하는 편이었는데",
          "original_form": "저도 원래는 잘 못하는 편이었는데",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.6",
          "form": "다년간 연애로",
          "original_form": "다년간 연애로",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.7",
          "form": "바뀌신건가여?ㅋㅋ",
          "original_form": "바뀌신건가여?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.8",
          "form": "네네 학습된 ㅋㅋㅋㅋ",
          "original_form": "네네 학습된 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.9",
          "form": "이런 점들은 개선하면 좋겠다 이러면서",
          "original_form": "이런 점들은 개선하면 좋겠다 이러면서",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.11",
          "form": "그래서 경험이 중요한거 같아요",
          "original_form": "그래서 경험이 중요한거 같아요",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027687.1.13",
          "form": "넹ㅋㅋㅋㅋ 그리고 지금 남친이 표현이 좀 덜한편인 거 같아서",
          "original_form": "넹ㅋㅋㅋㅋ 그리고 지금 남친이 표현이 좀 덜한편인 거 같아서",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027687.1.14",
          "form": "약간 시범학습 같은 걸 시키는 중이라고 해야하나",
          "original_form": "약간 시범학습 같은 걸 시키는 중이라고 해야하나",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027687.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027687.1.16",
          "form": "제가 한번 해주면은 알아서 학습이 되더라구요",
          "original_form": "제가 한번 해주면은 알아서 학습이 되더라구요",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027687.1.17",
          "form": "그런 재미가 있어요",
          "original_form": "그런 재미가 있어요",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027687.1.18",
          "form": "ㅋㅋㅋㅋ가르치는?",
          "original_form": "ㅋㅋㅋㅋ가르치는?",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027687.1.19",
          "form": "저는 그걸 제가했다니까요",
          "original_form": "저는 그걸 제가했다니까요",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027687.1.20",
          "form": "지금이야 자기야 사랑한다 하지만",
          "original_form": "지금이야 자기야 사랑한다 하지만",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027687.1.21",
          "form": "예전엔 정말....",
          "original_form": "예전엔 정말....",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027687.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027687.1.23",
          "form": "표현이 서툰 초딩선생님",
          "original_form": "표현이 서툰 초딩선생님",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027687.1.24",
          "form": "중딩선생님은 다르군여",
          "original_form": "중딩선생님은 다르군여",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027687.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027687.1.26",
          "form": "ㅋㅋㅋㅋ초등선생님이면 오히려 그런거 잘할거 같은 느낌인데",
          "original_form": "ㅋㅋㅋㅋ초등선생님이면 오히려 그런거 잘할거 같은 느낌인데",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027687.1.27",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027687.1.28",
          "form": "리액션이 좋을텐데분명",
          "original_form": "리액션이 좋을텐데분명",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027687.1.29",
          "form": "저는 지금은 중등이긴 해도 그전엔 고등이었기땜에",
          "original_form": "저는 지금은 중등이긴 해도 그전엔 고등이었기땜에",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027687.1.30",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027687.1.31",
          "form": "더 그런거 못할 이미지엿는데",
          "original_form": "더 그런거 못할 이미지엿는데",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027687.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027687.1.33",
          "form": "인기많은 여자선생님?",
          "original_form": "인기많은 여자선생님?",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027687.1.34",
          "form": "여자애들한테 인기많은",
          "original_form": "여자애들한테 인기많은",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027687.1.35",
          "form": "그런 이미진데",
          "original_form": "그런 이미진데",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027687.1.36",
          "form": "남자는요?",
          "original_form": "남자는요?",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027687.1.37",
          "form": "애들이 신여성이라고 부르는",
          "original_form": "애들이 신여성이라고 부르는",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027687.1.38",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027687.1.39",
          "form": "남자애들은 더 여성여성한 쌤들 좋아하구요",
          "original_form": "남자애들은 더 여성여성한 쌤들 좋아하구요",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027687.1.40",
          "form": "작년 저희반 애들이  나보고 너무 무뚝뚝하다고",
          "original_form": "작년 저희반 애들이  나보고 너무 무뚝뚝하다고",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027687.1.41",
          "form": "쌤 애교 하나도 없죠 그랬는데",
          "original_form": "쌤 애교 하나도 없죠 그랬는데",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
