        },
        {
          "id": "MDRW2100020508.1.2",
          "form": "노놉",
          "original_form": "노놉",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.3",
          "form": "쫄깃하다던데",
          "original_form": "쫄깃하다던데",
          "speaker_id": "2",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.4",
          "form": "후기에서?",
          "original_form": "후기에서?",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.6",
          "form": "떡같을까?",
          "original_form": "떡같을까?",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.7",
          "form": "아무래두?",
          "original_form": "아무래두?",
          "speaker_id": "2",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.8",
          "form": "그럼 맛잇을듯",
          "original_form": "그럼 맛잇을듯",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.9",
          "form": "난 떡이시러",
          "original_form": "난 떡이시러",
          "speaker_id": "2",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.10",
          "form": "오ㅔ시러?",
          "original_form": "오ㅔ시러?",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.11",
          "form": "너무 쫄깃해서..",
          "original_form": "너무 쫄깃해서..",
          "speaker_id": "2",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.12",
          "form": "그럼 별로겟다",
          "original_form": "그럼 별로겟다",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020508.1.13",
          "form": "마자 나는 떡 별로",
          "original_form": "마자 나는 떡 별로",
