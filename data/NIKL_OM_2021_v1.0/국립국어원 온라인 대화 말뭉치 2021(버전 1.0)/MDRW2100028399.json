      "utterance": [
        {
          "id": "MDRW2100028399.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ여행 가고 싶은데 있니?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ여행 가고 싶은데 있니?",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100028399.1.2",
          "form": "아까 name3이와",
          "original_form": "아까 &name3&이와",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028399.1.3",
          "form": "삿포로가고싶다는 이야기를",
          "original_form": "삿포로가고싶다는 이야기를",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028399.1.4",
          "form": "했었어 눈오는 날의 온천욕...",
          "original_form": "했었어 눈오는 날의 온천욕...",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028399.1.5",
          "form": "와 가취가욥",
          "original_form": "와 가취가욥",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028399.1.6",
          "form": "코로나가 좀 괜찮아져야 할텐데 흑흑",
          "original_form": "코로나가 좀 괜찮아져야 할텐데 흑흑",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028399.1.7",
          "form": "조아요옹",
          "original_form": "조아요옹",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028399.1.8",
          "form": "그리고 다같이 시간도 낼수잇을때에...",
          "original_form": "그리고 다같이 시간도 낼수잇을때에...",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028399.1.9",
          "form": "나취직해서 돈도 벌어놓고... 흑흑",
          "original_form": "나취직해서 돈도 벌어놓고... 흑흑",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028399.1.10",
          "form": "ㅠㅠ젊을땐 돈 없어서 여행 못가고",
          "original_form": "ㅠㅠ젊을땐 돈 없어서 여행 못가고",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028399.1.11",
          "form": "나이 들어선 돈은 있는데 시간이 없어서 못가는..",
          "original_form": "나이 들어선 돈은 있는데 시간이 없어서 못가는..",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028399.1.12",
          "form": "나도 열심히 돈 모아놓을게 흑흑 가취 여행갑시다",
          "original_form": "나도 열심히 돈 모아놓을게 흑흑 가취 여행갑시다",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028399.1.13",
          "form": "그니까~~ ㅜㅜ 코로나 아녓으면",
          "original_form": "그니까~~ ㅜㅜ 코로나 아녓으면",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100028399.1.14",
          "form": "지금 가면 좋은데...  물론 지금도 돈모아놔야하지만",
          "original_form": "지금 가면 좋은데...  물론 지금도 돈모아놔야하지만",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100028399.1.15",
          "form": "나중에 다같이 여행가자 애들모아서",
          "original_form": "나중에 다같이 여행가자 애들모아서",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100028399.1.16",
          "form": "좋아좋아!! 삿포로 말고도 가보고 싶은데 있어?",
          "original_form": "좋아좋아!! 삿포로 말고도 가보고 싶은데 있어?",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028399.1.17",
          "form": "난.... 유럽ㅎ",
          "original_form": "난.... 유럽ㅎ",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028399.1.18",
          "form": "열흘씩 가고싶당",
          "original_form": "열흘씩 가고싶당",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028399.1.19",
          "form": "오 유럽...나도 가보고싶어 인종차별이 살짝 무섭긴하지만ㅋㅋ",
          "original_form": "오 유럽...나도 가보고싶어 인종차별이 살짝 무섭긴하지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028399.1.20",
          "form": "나두... 그치만 죽기전엔함",
          "original_form": "나두... 그치만 죽기전엔함",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028399.1.21",
          "form": "가바야하지않나 생각중",
          "original_form": "가바야하지않나 생각중",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028399.1.22",
          "form": "함태어낫으니 여기저기 다 즐겨봐야지",
          "original_form": "함태어낫으니 여기저기 다 즐겨봐야지",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028399.1.23",
          "form": "맞아 한번 사는 인생 가고싶은곳 다 가보자ㅎㅎ",
          "original_form": "맞아 한번 사는 인생 가고싶은곳 다 가보자ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028399.1.24",
          "form": "좋아~~~ 돈마니모으는걸루 혀ㅎㅎ",
          "original_form": "좋아~~~ 돈마니모으는걸루 혀ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028399.1.25",
          "form": "오케 돈 많이 모아서 일본도 가고 유럽도 가는거야!",
          "original_form": "오케 돈 많이 모아서 일본도 가고 유럽도 가는거야!",
          "speaker_id": "1",
