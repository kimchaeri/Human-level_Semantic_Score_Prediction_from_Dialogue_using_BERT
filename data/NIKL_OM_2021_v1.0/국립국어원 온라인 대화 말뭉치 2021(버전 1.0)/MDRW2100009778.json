      "utterance": [
        {
          "id": "MDRW2100009778.1.1",
          "form": "게임 즐겨하시는거 있으신가요?",
          "original_form": "게임 즐겨하시는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009778.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009778.1.3",
          "form": "어떤 게임 주로 하세요?",
          "original_form": "어떤 게임 주로 하세요?",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009778.1.4",
          "form": "저는 롤 많이 해요",
          "original_form": "저는 롤 많이 해요",
          "speaker_id": "1",
          "time": "20210823 09:49"
        },
        {
          "id": "MDRW2100009778.1.5",
          "form": "아 롤 유명하죠",
          "original_form": "아 롤 유명하죠",
          "speaker_id": "2",
          "time": "20210823 09:50"
        },
        {
          "id": "MDRW2100009778.1.6",
          "form": "하루에 몇시간 정도 하세요?",
          "original_form": "하루에 몇시간 정도 하세요?",
          "speaker_id": "2",
          "time": "20210823 09:50"
        },
        {
          "id": "MDRW2100009778.1.7",
          "form": "2시간 정도 해요 물론 방학이라서 그런 거지만요",
          "original_form": "2시간 정도 해요 물론 방학이라서 그런 거지만요",
          "speaker_id": "1",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009778.1.8",
          "form": "방학인데 2시간이면 아주 괜찮죠",
          "original_form": "방학인데 2시간이면 아주 괜찮죠",
          "speaker_id": "2",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009778.1.9",
          "form": "저는 어렸을때 메이플이나 크아 같은것만 해봤어요",
          "original_form": "저는 어렸을때 메이플이나 크아 같은것만 해봤어요",
          "speaker_id": "2",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009778.1.10",
          "form": "저도 메이플이랑 크아 했었어요",
          "original_form": "저도 메이플이랑 크아 했었어요",
          "speaker_id": "1",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009778.1.11",
          "form": "크아는 나이들면서 끊고 메이플은 사고가 너무 크게 터져서...",
          "original_form": "크아는 나이들면서 끊고 메이플은 사고가 너무 크게 터져서...",
          "speaker_id": "1",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009778.1.12",
          "form": "어떤 사고가 터졌어요??",
          "original_form": "어떤 사고가 터졌어요??",
          "speaker_id": "2",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009778.1.13",
          "form": "아이템에 대한 확률 조작이 있었다고 하더라구요",
          "original_form": "아이템에 대한 확률 조작이 있었다고 하더라구요",
          "speaker_id": "1",
          "time": "20210823 09:55"
        },
        {
          "id": "MDRW2100009778.1.14",
          "form": "아아 뉴스로 본거 같기도 하네요",
          "original_form": "아아 뉴스로 본거 같기도 하네요",
          "speaker_id": "2",
          "time": "20210823 09:55"
        },
        {
          "id": "MDRW2100009778.1.15",
          "form": "그럼 정말 정떨어지겠어요",
          "original_form": "그럼 정말 정떨어지겠어요",
          "speaker_id": "2",
          "time": "20210823 09:55"
        },
        {
          "id": "MDRW2100009778.1.16",
          "form": "맞아요ㅠ 그 일 때문에 저도 끊고 주변에서도 대부분 끊었어요",
          "original_form": "맞아요ㅠ 그 일 때문에 저도 끊고 주변에서도 대부분 끊었어요",
          "speaker_id": "1",
          "time": "20210823 09:55"
        },
        {
          "id": "MDRW2100009778.1.17",
          "form": "그렇구나ㅠ 그래도 한번씩 메이플 배경이 예뻐서 들어가보고 싶긴해요",
          "original_form": "그렇구나ㅠ 그래도 한번씩 메이플 배경이 예뻐서 들어가보고 싶긴해요",
          "speaker_id": "2",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100009778.1.18",
          "form": "요즘같이 그래픽 좋은 게임 나오는 때에 도트 게임이라 많이 실망하실 수도 있어요",
          "original_form": "요즘같이 그래픽 좋은 게임 나오는 때에 도트 게임이라 많이 실망하실 수도 있어요",
          "speaker_id": "1",
          "time": "20210823 09:57"
        }
      ]
    }
