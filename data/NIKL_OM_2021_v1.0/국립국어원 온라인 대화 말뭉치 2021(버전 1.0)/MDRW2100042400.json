      "utterance": [
        {
          "id": "MDRW2100042400.1.1",
          "form": "좋아하시는 음식이 뭔가요??",
          "original_form": "좋아하시는 음식이 뭔가요??",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042400.1.2",
          "form": "떡볶이요! 어떤음식 좋아하시나요?",
          "original_form": "떡볶이요! 어떤음식 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042400.1.3",
          "form": "오 저는 고기요!!!",
          "original_form": "오 저는 고기요!!!",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042400.1.4",
          "form": "어디 떡볶이 좋아하세요",
          "original_form": "어디 떡볶이 좋아하세요",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042400.1.5",
          "form": "엽기떡볶이요!",
          "original_form": "엽기떡볶이요!",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042400.1.6",
          "form": "고기 맛있죠 ㅎㅎ",
          "original_form": "고기 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042400.1.7",
          "form": "우아 매운거 잘드세요?",
          "original_form": "우아 매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042400.1.8",
          "form": "아뇨  못먹어요 그래서 맨날 착한맛으로 시켜먹어요 ㅋㅋ",
          "original_form": "아뇨  못먹어요 그래서 맨날 착한맛으로 시켜먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042400.1.9",
          "form": "착한맛은 그래도 먹을만 한가요??",
          "original_form": "착한맛은 그래도 먹을만 한가요??",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042400.1.10",
          "form": "네 저는 먹을만 했어요!",
          "original_form": "네 저는 먹을만 했어요!",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042400.1.11",
          "form": "토핑은 어떤거 추가 하세요?",
          "original_form": "토핑은 어떤거 추가 하세요?",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042400.1.12",
          "form": "튀김만두랑  같이 먹어요!",
          "original_form": "튀김만두랑  같이 먹어요!",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042400.1.13",
          "form": "오오 튀김 ㅜㅜ 소스에 먹으면 맛잇죠",
          "original_form": "오오 튀김 ㅜㅜ 소스에 먹으면 맛잇죠",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042400.1.14",
          "form": "오늘 점심에 뭐 드실계획이세요?",
          "original_form": "오늘 점심에 뭐 드실계획이세요?",
          "speaker_id": "2",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042400.1.15",
          "form": "저는 오늘 짜장먹을려구요",
          "original_form": "저는 오늘 짜장먹을려구요",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042400.1.16",
          "form": "중식이 땡기네요 ㅎㅎ",
          "original_form": "중식이 땡기네요 ㅎㅎ",
          "speaker_id": "1",
