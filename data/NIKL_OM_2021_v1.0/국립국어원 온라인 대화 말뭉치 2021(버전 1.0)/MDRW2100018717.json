      "utterance": [
        {
          "id": "MDRW2100018717.1.1",
          "form": "휴가내고 캠핑가시나요? 아님 휴일에,",
          "original_form": "휴가내고 캠핑가시나요? 아님 휴일에,",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018717.1.2",
          "form": "금토일 가요",
          "original_form": "금토일 가요",
          "speaker_id": "2",
          "time": "20210827 11:24"
        },
        {
          "id": "MDRW2100018717.1.3",
          "form": "일하고 일찍 퇴근해서 갑니다",
          "original_form": "일하고 일찍 퇴근해서 갑니다",
          "speaker_id": "2",
          "time": "20210827 11:24"
        },
        {
          "id": "MDRW2100018717.1.4",
          "form": "평일에는 잘 못가구요",
          "original_form": "평일에는 잘 못가구요",
          "speaker_id": "2",
          "time": "20210827 11:24"
        },
        {
          "id": "MDRW2100018717.1.5",
          "form": "좋은곳가서 힐링하면 일 스트레스 많이 풀리겠어요",
          "original_form": "좋은곳가서 힐링하면 일 스트레스 많이 풀리겠어요",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018717.1.6",
          "form": "네 산 쳐다보고 있으면 너무 좋더라구요",
          "original_form": "네 산 쳐다보고 있으면 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100018717.1.7",
          "form": "저는 물보다는 산을 좋아해서",
          "original_form": "저는 물보다는 산을 좋아해서",
          "speaker_id": "2",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100018717.1.8",
          "form": "출근할때",
          "original_form": "출근할때",
          "speaker_id": "2",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100018717.1.9",
          "form": "산에 안개끼여있으면 그거 보는게 너무 좋더라구요",
          "original_form": "산에 안개끼여있으면 그거 보는게 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018717.1.10",
          "form": "일 하는건 힘들진 않으세요??",
          "original_form": "일 하는건 힘들진 않으세요??",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018717.1.11",
          "form": "네.. 백수라.. 너무 안힘들어요..ㅋㅋ",
          "original_form": "네.. 백수라.. 너무 안힘들어요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018717.1.12",
          "form": "힘들고 싶어요",
          "original_form": "힘들고 싶어요",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018717.1.13",
          "form": "아하...어느쪽으로 취업하고 싶으세요??",
          "original_form": "아하...어느쪽으로 취업하고 싶으세요??",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018717.1.14",
          "form": "늙고 병들어서 ㅋㅋㅋㅋ 시켜주는거 아무거나요..",
          "original_form": "늙고 병들어서 ㅋㅋㅋㅋ 시켜주는거 아무거나요..",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018717.1.15",
          "form": "전공 재취업은 어렵고... 그냥 생계수단 찾아야할듯해요..",
          "original_form": "전공 재취업은 어렵고... 그냥 생계수단 찾아야할듯해요..",
          "speaker_id": "1",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100018717.1.16",
          "form": "아 그렇군요 ㅠ 힘드시겠어요 ㅠ",
          "original_form": "아 그렇군요 ㅠ 힘드시겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100018717.1.17",
          "form": "하고 싶은일 찾기가 힘들죠",
          "original_form": "하고 싶은일 찾기가 힘들죠",
          "speaker_id": "2",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100018717.1.18",
          "form": "어떤 직종이세요?",
          "original_form": "어떤 직종이세요?",
          "speaker_id": "1",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018717.1.19",
          "form": "서비스업이예요 친절응대 이런거 ㅠ",
          "original_form": "서비스업이예요 친절응대 이런거 ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018717.1.20",
          "form": "아ㅡㅡ 스트레스 엄청나시쥬? \n저도 서비스 경험있는데.. 진상들",
          "original_form": "아ㅡㅡ 스트레스 엄청나시쥬? \n저도 서비스 경험있는데.. 진상들",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018717.1.21",
          "form": "상상초월",
          "original_form": "상상초월",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018717.1.22",
          "form": "네 스트레스 많이 받아요 ㅠㅠ",
          "original_form": "네 스트레스 많이 받아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018717.1.23",
          "form": "피곤합니다",
          "original_form": "피곤합니다",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018717.1.24",
          "form": "심신이 지치죠....",
          "original_form": "심신이 지치죠....",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018717.1.25",
          "form": "위에 관리자 없고 혼자 다 처리하시나여?",
          "original_form": "위에 관리자 없고 혼자 다 처리하시나여?",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100018717.1.26",
          "form": "아니요 관리자는 있어요",
          "original_form": "아니요 관리자는 있어요",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100018717.1.27",
          "form": "커버 쳐주시기는 하지만",
          "original_form": "커버 쳐주시기는 하지만",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100018717.1.28",
          "form": "일단 응대하는 자체가 힘들어서;;;",
          "original_form": "일단 응대하는 자체가 힘들어서;;;",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100018717.1.29",
          "form": "다른 일 찾기도 힘들고;;",
          "original_form": "다른 일 찾기도 힘들고;;",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100018717.1.30",
          "form": "관리자 있음.. 무조건 떠넘기기죠..ㅋㅋ \n네.. 잠시만요하고.. 매니저님~~~~ㅋㅋㅋㅋ",
          "original_form": "관리자 있음.. 무조건 떠넘기기죠..ㅋㅋ \n네.. 잠시만요하고.. 매니저님~~~~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100018717.1.31",
          "form": "정말 그러고 싶어요 ㅎㅎ",
          "original_form": "정말 그러고 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100018717.1.32",
          "form": "제발 나를 찾지마라고 ㅋㅋㅋ",
          "original_form": "제발 나를 찾지마라고 ㅋㅋㅋ",
