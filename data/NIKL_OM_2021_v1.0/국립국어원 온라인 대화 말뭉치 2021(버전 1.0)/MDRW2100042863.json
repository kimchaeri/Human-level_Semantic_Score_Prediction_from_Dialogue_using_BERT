      "utterance": [
        {
          "id": "MDRW2100042863.1.1",
          "form": "가자 취준생인 나를 위한 주제",
          "original_form": "가자 취준생인 나를 위한 주제",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042863.1.2",
          "form": "아니 나 혼다미오밖에 생각안나서",
          "original_form": "아니 나 혼다미오밖에 생각안나서",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042863.1.3",
          "form": "ㅇ ㅔ...? 이랬어",
          "original_form": "ㅇ ㅔ...? 이랬어",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042863.1.4",
          "form": "저도 곧..취준생됩니다^^",
          "original_form": "저도 곧..취준생됩니다^^",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042863.1.5",
          "form": "나 그래서 국민취업지원제도 신청했어",
          "original_form": "나 그래서 국민취업지원제도 신청했어",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042863.1.6",
          "form": "ㅋㅋㅠㅠㅠ헐 그게 뭐야?",
          "original_form": "ㅋㅋㅠㅠㅠ헐 그게 뭐야?",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100042863.1.7",
          "form": "취업성공패키지에서 바뀐거!!!",
          "original_form": "취업성공패키지에서 바뀐거!!!",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042863.1.8",
          "form": "직업훈련이나 인턴 보내주기도하고",
          "original_form": "직업훈련이나 인턴 보내주기도하고",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042863.1.9",
          "form": "다달이 생활비도 주고",
          "original_form": "다달이 생활비도 주고",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042863.1.10",
          "form": "취업하면 또 취업성공지원금도 주고",
          "original_form": "취업하면 또 취업성공지원금도 주고",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042863.1.11",
          "form": "그...뭐지 배움카드로 학원도 다닐수있음!!",
          "original_form": "그...뭐지 배움카드로 학원도 다닐수있음!!",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042863.1.12",
          "form": "우와 대박 취업지원해주는거 좋구나...",
          "original_form": "우와 대박 취업지원해주는거 좋구나...",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042863.1.13",
          "form": "아 친구가 비슷한 거 신청했었던 것 같기도 하다",
          "original_form": "아 친구가 비슷한 거 신청했었던 것 같기도 하다",
          "speaker_id": "1",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100042863.1.14",
          "form": "나 빨리...고시생 생활 끝내고 취업하고싶어 커리어우먼이 된 냐즈",
          "original_form": "나 빨리...고시생 생활 끝내고 취업하고싶어 커리어우먼이 된 냐즈",
          "speaker_id": "1",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100042863.1.15",
          "form": "나도나도~~~!",
          "original_form": "나도나도~~~!",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042863.1.16",
          "form": "커리어우먼돼서",
          "original_form": "커리어우먼돼서",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042863.1.17",
          "form": "매달 게임패스를 끊는",
          "original_form": "매달 게임패스를 끊는",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042863.1.18",
          "form": "훌륭한 커리어오타쿠우먼이 되고싶어요",
          "original_form": "훌륭한 커리어오타쿠우먼이 되고싶어요",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042863.1.19",
          "form": "멋지다~~~~ 짱인것같애 글고 요즘 시급 올라서",
          "original_form": "멋지다~~~~ 짱인것같애 글고 요즘 시급 올라서",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042863.1.20",
          "form": "진짜 못받아도 취업하면 막 블랙 아닌 이상 세후 200은 될거아냐",
          "original_form": "진짜 못받아도 취업하면 막 블랙 아닌 이상 세후 200은 될거아냐",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042863.1.21",
          "form": "마자~!~!~!!",
          "original_form": "마자~!~!~!!",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042863.1.22",
          "form": "요즘 알바도 시급 많이올랐더라",
          "original_form": "요즘 알바도 시급 많이올랐더라",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042863.1.23",
          "form": "근로 기본시급 9천원 짱이죠",
          "original_form": "근로 기본시급 9천원 짱이죠",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042863.1.24",
          "form": "마좌요~~ 아 취업 전에 또 알바하러 돌아가야하는데",
          "original_form": "마좌요~~ 아 취업 전에 또 알바하러 돌아가야하는데",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042863.1.25",
          "form": "빙글빙글 돌아가는 편의점 주말 알바",
          "original_form": "빙글빙글 돌아가는 편의점 주말 알바",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042863.1.26",
          "form": "다시 가는구나~!",
          "original_form": "다시 가는구나~!",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042863.1.27",
          "form": "ㅎ ㅏ 난 근로끝나고",
          "original_form": "ㅎ ㅏ 난 근로끝나고",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042863.1.28",
          "form": "취준하면 알바구해야되는데^-^",
          "original_form": "취준하면 알바구해야되는데^-^",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042863.1.29",
          "form": "그냥 편의점할듯 편의점이 짱",
          "original_form": "그냥 편의점할듯 편의점이 짱",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042863.1.30",
          "form": "마자 편의점이 최고야...특히 난 엄마 가게라 진짜편함",
          "original_form": "마자 편의점이 최고야...특히 난 엄마 가게라 진짜편함",
          "speaker_id": "1",
          "time": "20210907 10:26"
        },
        {
          "id": "MDRW2100042863.1.31",
          "form": "과자 날름 먹기~~~",
          "original_form": "과자 날름 먹기~~~",
          "speaker_id": "1",
          "time": "20210907 10:26"
        },
        {
          "id": "MDRW2100042863.1.32",
          "form": "부럽다~~~~",
          "original_form": "부럽다~~~~",
          "speaker_id": "2",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100042863.1.33",
