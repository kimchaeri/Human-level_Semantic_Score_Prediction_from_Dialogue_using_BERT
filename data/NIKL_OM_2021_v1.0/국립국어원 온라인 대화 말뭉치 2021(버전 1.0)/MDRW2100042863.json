        },
        {
          "id": "MDRW2100042863.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100042863.1.41",
