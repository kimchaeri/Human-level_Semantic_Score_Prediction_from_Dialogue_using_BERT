        {
          "id": "MDRW2100045291.1.1",
          "form": "털이 좀 날 립니다",
          "original_form": "털이 좀 날 립니다",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045291.1.2",
          "form": "청소하기 힘들겠네요",
          "original_form": "청소하기 힘들겠네요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045291.1.3",
          "form": "맞아요 털뭉치가 굴러다녀요",
          "original_form": "맞아요 털뭉치가 굴러다녀요",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045291.1.4",
          "form": "몇 마리인데요",
          "original_form": "몇 마리인데요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045291.1.5",
          "form": "두 마리 요",
          "original_form": "두 마리 요",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045291.1.6",
          "form": "성별은 어떻게 되나요",
          "original_form": "성별은 어떻게 되나요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045291.1.7",
          "form": "둘 다 남자 수컷이에요",
          "original_form": "둘 다 남자 수컷이에요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045291.1.8",
          "form": "나이는요",
          "original_form": "나이는요",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045291.1.9",
          "form": "같이 태어나서 형제에요",
          "original_form": "같이 태어나서 형제에요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045291.1.10",
          "form": "오귀엽겠어요",
          "original_form": "오귀엽겠어요",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045291.1.11",
          "form": "길냥이 엄마가 나았어요",
          "original_form": "길냥이 엄마가 나았어요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045291.1.12",
          "form": "그러면 코숏?",
          "original_form": "그러면 코숏?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045291.1.13",
          "form": "그런 셈이죠",
          "original_form": "그런 셈이죠",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045291.1.14",
          "form": "무늬는 어떤데요",
          "original_form": "무늬는 어떤데요",
