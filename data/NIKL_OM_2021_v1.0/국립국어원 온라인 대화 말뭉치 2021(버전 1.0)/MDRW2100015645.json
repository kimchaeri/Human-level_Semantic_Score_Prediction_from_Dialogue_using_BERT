      "utterance": [
        {
          "id": "MDRW2100015645.1.1",
          "form": "저희 홈트하는 얘기했어요! 운동 꾸준히 해야겠다고ㅎㅎ",
          "original_form": "저희 홈트하는 얘기했어요! 운동 꾸준히 해야겠다고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015645.1.2",
          "form": "ㅋㅋ 가을옷 꺼내셨나요?",
          "original_form": "ㅋㅋ 가을옷 꺼내셨나요?",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015645.1.3",
          "form": "아직은 안 꺼냈어요ㅋㅋㅋ",
          "original_form": "아직은 안 꺼냈어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015645.1.4",
          "form": "이번에 태풍 지나면 진짜 서늘해질 것 같긴 해요",
          "original_form": "이번에 태풍 지나면 진짜 서늘해질 것 같긴 해요",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015645.1.5",
          "form": "지금도 서늘해서 긴팔 긴바지 입고 있어요",
          "original_form": "지금도 서늘해서 긴팔 긴바지 입고 있어요",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015645.1.6",
          "form": "이젠 밤에 창문 열기 그른거 같아요",
          "original_form": "이젠 밤에 창문 열기 그른거 같아요",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015645.1.7",
          "form": "맞아요 이제 밤에는 쌀쌀하더라구요",
          "original_form": "맞아요 이제 밤에는 쌀쌀하더라구요",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015645.1.8",
          "form": "근데 쌀쌀해지고 갑자기 모기가 나와요ㅠㅜ",
          "original_form": "근데 쌀쌀해지고 갑자기 모기가 나와요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015645.1.9",
          "form": "그러게요... 마지막 발악이라도 하는지 안그래도 오늘만 3방 물렸어요",
          "original_form": "그러게요... 마지막 발악이라도 하는지 안그래도 오늘만 3방 물렸어요",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015645.1.10",
          "form": "헉 많이 물렸네요ㅠㅜㅠ",
          "original_form": "헉 많이 물렸네요ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015645.1.11",
          "form": "이번에 가을 모기가 성행한대요ㅜㅠ",
          "original_form": "이번에 가을 모기가 성행한대요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015645.1.12",
          "form": "ㅠ 진짜 안좋은 소식이네요",
          "original_form": "ㅠ 진짜 안좋은 소식이네요",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015645.1.13",
          "form": "분명 추워지면 모기가 사라졌는데",
          "original_form": "분명 추워지면 모기가 사라졌는데",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015645.1.14",
          "form": "그러니까요 올해 이상하게 여름에 안 물려서 좋았는데ㅠㅠ",
          "original_form": "그러니까요 올해 이상하게 여름에 안 물려서 좋았는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015645.1.15",
          "form": "가을 모기는 더 독하대요",
          "original_form": "가을 모기는 더 독하대요",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015645.1.16",
          "form": "진짜 마지막 발악이군요...",
          "original_form": "진짜 마지막 발악이군요...",
          "speaker_id": "2",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100015645.1.17",
          "form": "그러니까요.. 가을이 산란기래요",
          "original_form": "그러니까요.. 가을이 산란기래요",
          "speaker_id": "1",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100015645.1.18",
          "form": "가을이 산란기인줄은 몰랐어요",
          "original_form": "가을이 산란기인줄은 몰랐어요",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100015645.1.19",
          "form": "저도 이번에 알았어요",
          "original_form": "저도 이번에 알았어요",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100015645.1.20",
          "form": "많이 안 물리고 넘기면 좋겠네요ㅠㅠ",
          "original_form": "많이 안 물리고 넘기면 좋겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100015645.1.21",
          "form": "그러게요ㅠ 모기 없는 날 보내시기 바랍니다",
          "original_form": "그러게요ㅠ 모기 없는 날 보내시기 바랍니다",
          "speaker_id": "2",
          "time": "20210826 13:09"
        }
