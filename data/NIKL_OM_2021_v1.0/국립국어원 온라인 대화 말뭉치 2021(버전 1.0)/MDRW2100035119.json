      "utterance": [
        {
          "id": "MDRW2100035119.1.1",
          "form": "최소몇명이",
          "original_form": "최소몇명이",
          "speaker_id": "2",
          "time": "20210829 03:14"
        },
        {
          "id": "MDRW2100035119.1.2",
          "form": "쪼인되야하니ㅜ",
          "original_form": "쪼인되야하니ㅜ",
          "speaker_id": "2",
          "time": "20210829 03:14"
        },
        {
          "id": "MDRW2100035119.1.3",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:14"
        },
        {
          "id": "MDRW2100035119.1.4",
          "form": "패키지 여행은",
          "original_form": "패키지 여행은",
          "speaker_id": "1",
          "time": "20210829 03:14"
        },
        {
          "id": "MDRW2100035119.1.5",
          "form": "계획이 거의 삼십분 단위로 쪼개지다보니까",
          "original_form": "계획이 거의 삼십분 단위로 쪼개지다보니까",
          "speaker_id": "1",
          "time": "20210829 03:15"
        },
        {
          "id": "MDRW2100035119.1.6",
          "form": "엄청 빡빡해요ㅋㅋㅋㅋ",
          "original_form": "엄청 빡빡해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:15"
        },
        {
          "id": "MDRW2100035119.1.7",
          "form": "와 엄청 디테일하네요 ㅋㅋㅋ",
          "original_form": "와 엄청 디테일하네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:15"
        },
        {
          "id": "MDRW2100035119.1.8",
          "form": "저 마지막날에는 그냥 버스에서 안내리고",
          "original_form": "저 마지막날에는 그냥 버스에서 안내리고",
          "speaker_id": "1",
          "time": "20210829 03:15"
        },
        {
          "id": "MDRW2100035119.1.9",
          "form": "잠잤어욬ㅋㅋㅋㅋㅋ",
          "original_form": "잠잤어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:15"
        },
        {
          "id": "MDRW2100035119.1.10",
          "form": "국내는 많이 가보셨어요??",
          "original_form": "국내는 많이 가보셨어요??",
          "speaker_id": "2",
          "time": "20210829 03:15"
        },
        {
          "id": "MDRW2100035119.1.11",
          "form": "국내는 제주도를 제일 자주 갔어용",
          "original_form": "국내는 제주도를 제일 자주 갔어용",
          "speaker_id": "1",
          "time": "20210829 03:15"
        },
        {
          "id": "MDRW2100035119.1.12",
          "form": "와! 좋지요",
          "original_form": "와! 좋지요",
          "speaker_id": "2",
          "time": "20210829 03:16"
        },
        {
          "id": "MDRW2100035119.1.13",
          "form": "전 제주도가외가라 그냥 무슨일있고하면 가서 관광지를 막상 잘몰라요ㅠㅠㅋㅋㅋ",
          "original_form": "전 제주도가외가라 그냥 무슨일있고하면 가서 관광지를 막상 잘몰라요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:16"
        },
        {
          "id": "MDRW2100035119.1.14",
          "form": "제주도에 친척이 살아서 숙식이 해결되니까",
          "original_form": "제주도에 친척이 살아서 숙식이 해결되니까",
          "speaker_id": "1",
          "time": "20210829 03:16"
        },
        {
          "id": "MDRW2100035119.1.15",
          "form": "많이 갔던것같아용",
          "original_form": "많이 갔던것같아용",
          "speaker_id": "1",
          "time": "20210829 03:16"
        },
        {
          "id": "MDRW2100035119.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 03:16"
        },
        {
          "id": "MDRW2100035119.1.17",
          "form": "외가! 너무 부러워요!!",
          "original_form": "외가! 너무 부러워요!!",
          "speaker_id": "1",
          "time": "20210829 03:16"
        },
        {
          "id": "MDRW2100035119.1.18",
          "form": "그쵸 저도 삼촌네 이모네에서 자니깐 ㅋㅋㅋ",
          "original_form": "그쵸 저도 삼촌네 이모네에서 자니깐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:16"
        },
        {
          "id": "MDRW2100035119.1.19",
          "form": "제주도는 관광지랑 주거지역의 경계가 명확한것같더라구요",
          "original_form": "제주도는 관광지랑 주거지역의 경계가 명확한것같더라구요",
          "speaker_id": "1",
          "time": "20210829 03:17"
        },
        {
          "id": "MDRW2100035119.1.20",
          "form": "근데 마지막으로 언제갓더러 ㅋㅋ",
          "original_form": "근데 마지막으로 언제갓더러 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:17"
        },
        {
          "id": "MDRW2100035119.1.21",
          "form": "ㅋㅋㅋㅋ 저도 코로나 터지고 안갔어요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 저도 코로나 터지고 안갔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:17"
        },
        {
          "id": "MDRW2100035119.1.22",
          "form": "많이들가긴하나봐요ㅠㅠ",
          "original_form": "많이들가긴하나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:17"
        },
        {
          "id": "MDRW2100035119.1.23",
          "form": "네네ㅠㅠ",
