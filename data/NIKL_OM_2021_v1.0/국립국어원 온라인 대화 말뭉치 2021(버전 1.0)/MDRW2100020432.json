      "utterance": [
        {
          "id": "MDRW2100020432.1.1",
          "form": "맞아요 ㅠㅠ ㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.2",
          "form": "근데 또 먹다가물려서 마니느 못먹어요",
          "original_form": "근데 또 먹다가물려서 마니느 못먹어요",
          "speaker_id": "2",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.3",
          "form": "뿌링클도 좋아하는데",
          "original_form": "뿌링클도 좋아하는데",
          "speaker_id": "1",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.4",
          "form": "이런 특이한 치킨은 좀 잘 질리더라구요",
          "original_form": "이런 특이한 치킨은 좀 잘 질리더라구요",
          "speaker_id": "1",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.5",
          "form": "레드콤보랑 허니콤보 같이먹어야 좀 ㅋㅋ",
          "original_form": "레드콤보랑 허니콤보 같이먹어야 좀 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.6",
          "form": "마자요 뿌링클도 ㅋㅋㅋ",
          "original_form": "마자요 뿌링클도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.7",
          "form": "달달하면 더 질리는것같아요",
          "original_form": "달달하면 더 질리는것같아요",
          "speaker_id": "1",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.8",
          "form": "매콤하면 괜찮은데",
          "original_form": "매콤하면 괜찮은데",
          "speaker_id": "1",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020432.1.9",
          "form": "저는 어제 짬뽕 먹었어요",
          "original_form": "저는 어제 짬뽕 먹었어요",
          "speaker_id": "1",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.10",
          "form": "달달하면 좀 물려요 ㅋㅋㅋ",
          "original_form": "달달하면 좀 물려요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.11",
          "form": "우와와",
          "original_form": "우와와",
          "speaker_id": "2",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.12",
          "form": "차돌박이 짬뽕ㅋㅋㅋ",
          "original_form": "차돌박이 짬뽕ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.13",
          "form": "배달요?",
          "original_form": "배달요?",
          "speaker_id": "2",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.14",
          "form": "그 중식집 가서 먹었어용",
          "original_form": "그 중식집 가서 먹었어용",
          "speaker_id": "1",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.15",
          "form": "고기들어간 짬뽕 너무 맛있어요ㅠㅠ",
          "original_form": "고기들어간 짬뽕 너무 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.16",
          "form": "차돌박이!!",
          "original_form": "차돌박이!!",
          "speaker_id": "2",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.17",
          "form": "직접가서 먹는 짬뽕",
          "original_form": "직접가서 먹는 짬뽕",
          "speaker_id": "2",
          "time": "20210828 05:29"
        },
        {
          "id": "MDRW2100020432.1.18",
          "form": "진짜 맛있죠 ㅋㅋ",
          "original_form": "진짜 맛있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:30"
        },
        {
          "id": "MDRW2100020432.1.19",
          "form": "해물좋아하세요 차돌좋아하세요?",
          "original_form": "해물좋아하세요 차돌좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 05:30"
        },
        {
          "id": "MDRW2100020432.1.20",
          "form": "저도 엊그제 홍콩반점 갔어요 ㅋㅋㅋ",
          "original_form": "저도 엊그제 홍콩반점 갔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:30"
        },
        {
          "id": "MDRW2100020432.1.21",
          "form": "전 근데 얼큰한 맛에",
          "original_form": "전 근데 얼큰한 맛에",
          "speaker_id": "2",
          "time": "20210828 05:30"
        },
        {
          "id": "MDRW2100020432.1.22",
          "form": "우와앙 거기도 짬뽕 맛있는데!",
          "original_form": "우와앙 거기도 짬뽕 맛있는데!",
          "speaker_id": "1",
          "time": "20210828 05:30"
        },
        {
          "id": "MDRW2100020432.1.23",
          "form": "해물짬뽕이 좀 더 ㅋㅋㅋ",
          "original_form": "해물짬뽕이 좀 더 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:30"
        },
        {
          "id": "MDRW2100020432.1.24",
          "form": "해물이 근본이긴하죠ㅋㅋ",
          "original_form": "해물이 근본이긴하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:30"
        }
      ]
