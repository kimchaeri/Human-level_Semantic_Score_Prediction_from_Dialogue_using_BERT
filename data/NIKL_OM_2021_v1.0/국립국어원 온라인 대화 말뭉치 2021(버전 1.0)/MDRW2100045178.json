      "utterance": [
        {
          "id": "MDRW2100045178.1.1",
          "form": "전 연어를 너무 좋아해서 별명이 연어예요",
          "original_form": "전 연어를 너무 좋아해서 별명이 연어예요",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045178.1.2",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045178.1.3",
          "form": "근데 연어국수 아직 안드셔 보셨어요??!",
          "original_form": "근데 연어국수 아직 안드셔 보셨어요??!",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045178.1.4",
          "form": "네....그게....",
          "original_form": "네....그게....",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045178.1.5",
          "form": "뭐랄까...만들어 먹는 게 낫겠다 하면서",
          "original_form": "뭐랄까...만들어 먹는 게 낫겠다 하면서",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045178.1.6",
          "form": "엄청 미뤄가지고 이렇게 됐네요 ㅋㅋ",
          "original_form": "엄청 미뤄가지고 이렇게 됐네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045178.1.7",
          "form": "확실히 사서 만들어먹는데 양이 든든하죠",
          "original_form": "확실히 사서 만들어먹는데 양이 든든하죠",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045178.1.8",
          "form": "그쵸 가격도 훨 낫고요",
          "original_form": "그쵸 가격도 훨 낫고요",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045178.1.9",
          "form": "갑자기 내일 먹어볼까 하는 생각이 드네요",
          "original_form": "갑자기 내일 먹어볼까 하는 생각이 드네요",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045178.1.10",
          "form": "저도그래서 연어초밥같은거 먹을때 대용량으로 사서 집에서 먹어요",
          "original_form": "저도그래서 연어초밥같은거 먹을때 대용량으로 사서 집에서 먹어요",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045178.1.11",
          "form": "직접 초밥 해드세요?",
          "original_form": "직접 초밥 해드세요?",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045178.1.12",
          "form": "네! 손질된 큰덩이 사서 해먹는데 원없이 먹으니까 너무 좋더라고요",
          "original_form": "네! 손질된 큰덩이 사서 해먹는데 원없이 먹으니까 너무 좋더라고요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045178.1.13",
          "form": "우왕 초밥에 밥 간하기 어렵잖아요",
          "original_form": "우왕 초밥에 밥 간하기 어렵잖아요",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045178.1.14",
          "form": "대단하다...",
          "original_form": "대단하다...",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045178.1.15",
          "form": "연어 숙성하면 더 맛난 거 아세요??",
          "original_form": "연어 숙성하면 더 맛난 거 아세요??",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045178.1.16",
          "form": "앗 그런가요??",
          "original_form": "앗 그런가요??",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045178.1.17",
          "form": "저는 먹기만하느라 잘 몰랏네요 ㅎㅋㅎㅋㅋㅋㅋ",
          "original_form": "저는 먹기만하느라 잘 몰랏네요 ㅎㅋㅎㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045178.1.18",
          "form": "훨 맛있어요",
          "original_form": "훨 맛있어요",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045178.1.19",
          "form": "연어 살도 단단해지고",
          "original_form": "연어 살도 단단해지고",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045178.1.20",
          "form": "24시간을 기다려야하긴 하지만...",
          "original_form": "24시간을 기다려야하긴 하지만...",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045178.1.21",
          "form": "어우 갑자기 확땡기네요",
          "original_form": "어우 갑자기 확땡기네요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045178.1.22",
          "form": "사실 야식생각없었는데 연어얘기하니까 침샘자극되네요",
          "original_form": "사실 야식생각없었는데 연어얘기하니까 침샘자극되네요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045178.1.23",
          "form": "ㅋㅋㅋㅋㅋ그니까요....후",
          "original_form": "ㅋㅋㅋㅋㅋ그니까요....후",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045178.1.24",
          "form": "연어 꼭...숙성시켜 드세요..",
          "original_form": "연어 꼭...숙성시켜 드세요..",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045178.1.25",
          "form": "넵.. 꼭해볼게요!",
          "original_form": "넵.. 꼭해볼게요!",
          "speaker_id": "2",
          "time": "20210908 01:15"
        }
      ]
    }
  ]
