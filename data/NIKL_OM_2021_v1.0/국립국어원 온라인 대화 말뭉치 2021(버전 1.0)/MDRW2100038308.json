      "utterance": [
        {
          "id": "MDRW2100038308.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아 힘드네이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아 힘드네이",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038308.1.2",
          "form": "감자탕가서 1시간 뛰어놀았는데도",
          "original_form": "감자탕가서 1시간 뛰어놀았는데도",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038308.1.3",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038308.1.4",
          "form": "그날또11시넘어서 잤어",
          "original_form": "그날또11시넘어서 잤어",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038308.1.5",
          "form": "트램폴린 살아 그냥",
          "original_form": "트램폴린 살아 그냥",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038308.1.6",
          "form": "오후 열한시라니",
          "original_form": "오후 열한시라니",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038308.1.7",
          "form": "진짜 잠이없네",
          "original_form": "진짜 잠이없네",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038308.1.8",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ집에서도 계속 뛰는데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ집에서도 계속 뛰는데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038308.1.9",
          "form": "어제 저녁에 덜 먹고잤거든",
          "original_form": "어제 저녁에 덜 먹고잤거든",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038308.1.10",
          "form": "평소보다ㅋㅋ",
          "original_form": "평소보다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038308.1.11",
          "form": "일찍일어나서 밥부터",
          "original_form": "일찍일어나서 밥부터",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038308.1.12",
          "form": "먹더라",
          "original_form": "먹더라",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038308.1.13",
          "form": "아 진짜 잘 먹네",
          "original_form": "아 진짜 잘 먹네",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100038308.1.14",
          "form": "우리는 뭐 어제 저녁에 덜 먹고 잤다고",
          "original_form": "우리는 뭐 어제 저녁에 덜 먹고 잤다고",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100038308.1.15",
          "form": "일찍 일어나서 밥 먹고 이런 거 없는데",
          "original_form": "일찍 일어나서 밥 먹고 이런 거 없는데",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100038308.1.16",
          "form": "그래ㅜ두찌는 근데 편식이심하다ㅋㅋ",
          "original_form": "그래ㅜ두찌는 근데 편식이심하다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100038308.1.17",
          "form": "여튼 하..정말 정떨어질뻔했다. 어제는ㅋㅋ",
          "original_form": "여튼 하..정말 정떨어질뻔했다. 어제는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100038308.1.18",
          "form": "오늘 케이스마트가서 뛰어놀게하려고ㅋㅋ",
          "original_form": "오늘 케이스마트가서 뛰어놀게하려고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100038308.1.19",
          "form": "첫째 같이 데리고가서 놀다와야겠음",
          "original_form": "첫째 같이 데리고가서 놀다와야겠음",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100038308.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038308.1.21",
          "form": "아 그래 정도로 지면 안 되니까 응",
          "original_form": "아 그래 정도로 지면 안 되니까 응",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038308.1.22",
          "form": "정 떨어지면 안 되니까",
          "original_form": "정 떨어지면 안 되니까",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038308.1.23",
          "form": "케이스마트 가서 놀다 와라",
          "original_form": "케이스마트 가서 놀다 와라",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038308.1.24",
          "form": "장도 보고",
          "original_form": "장도 보고",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038308.1.25",
          "form": "응응 장도 좀 봐야지 ㅜ와 먹을것도없다ㅋㅋ",
          "original_form": "응응 장도 좀 봐야지 ㅜ와 먹을것도없다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038308.1.26",
          "form": "진짜 징하게놀아라캐야지 후ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 징하게놀아라캐야지 후ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038308.1.27",
          "form": "메일 장보고 메일 노 네",
          "original_form": "메일 장보고 메일 노 네",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038308.1.28",
          "form": "네 살 되면 재우지 마라고 해야겠다 낮잠",
          "original_form": "네 살 되면 재우지 마라고 해야겠다 낮잠",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038308.1.29",
          "form": "둘째가 빠른 개월 생일이라서",
          "original_form": "둘째가 빠른 개월 생일이라서",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038308.1.30",
          "form": "친구들에 비해 더 잠이 없나보다",
          "original_form": "친구들에 비해 더 잠이 없나보다",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038308.1.31",
          "form": "그런가ㅜ모르겠다ㅜ",
          "original_form": "그런가ㅜ모르겠다ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038308.1.32",
          "form": "아휴 디다ㅋㅋ",
          "original_form": "아휴 디다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038308.1.33",
          "form": "그래 디다 ㅠ",
          "original_form": "그래 디다 ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038308.1.34",
          "form": "안자면 디다",
          "original_form": "안자면 디다",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038308.1.35",
          "form": "이제 우리 애들은 둘이 그냥 잘 자는 듯",
          "original_form": "이제 우리 애들은 둘이 그냥 잘 자는 듯",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038308.1.36",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038308.1.37",
