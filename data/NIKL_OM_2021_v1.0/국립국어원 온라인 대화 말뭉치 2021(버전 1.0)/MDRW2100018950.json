      "utterance": [
        {
          "id": "MDRW2100018950.1.1",
          "form": "아직ㅋㅋ 아리따움에 다른 패드도 몇개 있어서 비교좀 할라고ㅋㅋ",
          "original_form": "아직ㅋㅋ 아리따움에 다른 패드도 몇개 있어서 비교좀 할라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018950.1.2",
          "form": "나듀 오늘 주문해야지",
          "original_form": "나듀 오늘 주문해야지",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018950.1.3",
          "form": "난 배송료 내기 싫어서 매장 픽업할라고ㅋ",
          "original_form": "난 배송료 내기 싫어서 매장 픽업할라고ㅋ",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018950.1.4",
          "form": "아 매장 픽업도 되나 그럼 매장서 사는거랑 뭐가 다름",
          "original_form": "아 매장 픽업도 되나 그럼 매장서 사는거랑 뭐가 다름",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018950.1.5",
          "form": "매장에서는 세일상품 뷰포 못씀ㅋㅋ",
          "original_form": "매장에서는 세일상품 뷰포 못씀ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018950.1.6",
          "form": "아 글쿠나 난 걍 무배만큼 채워 살라고 ㅋㅋㅋㅋ",
          "original_form": "아 글쿠나 난 걍 무배만큼 채워 살라고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018950.1.7",
          "form": "2만 이상 무료인가?",
          "original_form": "2만 이상 무료인가?",
          "speaker_id": "2",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018950.1.8",
          "form": "응 ㅋㅋㅋ",
          "original_form": "응 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018950.1.9",
          "form": "난 어차피 조만간 도서관 갈꺼같아서 가는길에 픽업 할라고ㅋㅋ",
          "original_form": "난 어차피 조만간 도서관 갈꺼같아서 가는길에 픽업 할라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018950.1.10",
          "form": "우방에도 아리따움 있지 않나",
          "original_form": "우방에도 아리따움 있지 않나",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100018950.1.11",
          "form": "응 있지 근데 문을 가끔 여는듯ㅋㅋ",
          "original_form": "응 있지 근데 문을 가끔 여는듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018950.1.12",
          "form": "아니 장사를 왜 ㅋㅋㅋㅋ",
          "original_form": "아니 장사를 왜 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:15"
        },
        {
          "id": "MDRW2100018950.1.13",
          "form": "진짜 어쩌다 한번씩 열어ㅋㅋ",
          "original_form": "진짜 어쩌다 한번씩 열어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100018950.1.14",
          "form": "손님들이 문 안열었겠거니 하고 안 가겠다 그 매장",
          "original_form": "손님들이 문 안열었겠거니 하고 안 가겠다 그 매장",
          "speaker_id": "1",
          "time": "20210827 14:21"
        }
