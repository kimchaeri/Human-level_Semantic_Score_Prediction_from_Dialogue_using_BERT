      "utterance": [
        {
          "id": "MDRW2100028436.1.1",
          "form": "연장전 폐지한 인간들 꼭 망해라",
          "original_form": "연장전 폐지한 인간들 꼭 망해라",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100028436.1.2",
          "form": "연장전 아예 폐지된 거임??",
          "original_form": "연장전 아예 폐지된 거임??",
          "speaker_id": "2",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100028436.1.3",
          "form": "이번 시즌엔 없음",
          "original_form": "이번 시즌엔 없음",
          "speaker_id": "1",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028436.1.4",
          "form": "내년에 돌아오겠지...",
          "original_form": "내년에 돌아오겠지...",
          "speaker_id": "1",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028436.1.5",
          "form": "실화냐...",
          "original_form": "실화냐...",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028436.1.6",
          "form": "144경기 다 하겠다고 7n 경기를 노잼으로 만드는 리그가 있다",
          "original_form": "144경기 다 하겠다고 7n 경기를 노잼으로 만드는 리그가 있다",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028436.1.7",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028436.1.8",
          "form": "지대짱나",
          "original_form": "지대짱나",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028436.1.9",
          "form": "이런 야구 싫어요",
          "original_form": "이런 야구 싫어요",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028436.1.10",
          "form": "싫어요... 연장전 돌려줘",
          "original_form": "싫어요... 연장전 돌려줘",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028436.1.11",
          "form": "포시도 제대로 돌려줘",
          "original_form": "포시도 제대로 돌려줘",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028436.1.12",
          "form": "포시가 뭐야",
          "original_form": "포시가 뭐야",
