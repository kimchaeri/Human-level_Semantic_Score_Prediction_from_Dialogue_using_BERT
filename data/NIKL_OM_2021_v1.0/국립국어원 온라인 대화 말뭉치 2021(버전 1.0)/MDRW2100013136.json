      "utterance": [
        {
          "id": "MDRW2100013136.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 12:54"
        },
        {
          "id": "MDRW2100013136.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013136.1.3",
          "form": "밖에 날씨 보셨나요? 비가 엄청 내리더라구요",
          "original_form": "밖에 날씨 보셨나요? 비가 엄청 내리더라구요",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013136.1.4",
          "form": "오 지방이신가요",
          "original_form": "오 지방이신가요",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100013136.1.5",
          "form": "네네 지방인데",
          "original_form": "네네 지방인데",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100013136.1.6",
          "form": "서울은 안왔나요??",
          "original_form": "서울은 안왔나요??",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100013136.1.7",
          "form": "네 거의안왔어요ㅠ",
          "original_form": "네 거의안왔어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100013136.1.8",
          "form": "헋스 제 친구가 부산에 사는데",
          "original_form": "헋스 제 친구가 부산에 사는데",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100013136.1.9",
          "form": "해운대 쪽에 물이 찰랑찰랑...",
          "original_form": "해운대 쪽에 물이 찰랑찰랑...",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100013136.1.10",
          "form": "발이 잠겼더라구요",
          "original_form": "발이 잠겼더라구요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100013136.1.11",
          "form": "와 엄청왔네요..",
          "original_form": "와 엄청왔네요..",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100013136.1.12",
          "form": "그러니까요ㅋㅋㅋㅠ",
          "original_form": "그러니까요ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100013136.1.13",
          "form": "근데 왠지 비 그치면 가을 날씨 확 올 것같지 않나요? ㅎㅎ",
          "original_form": "근데 왠지 비 그치면 가을 날씨 확 올 것같지 않나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100013136.1.14",
          "form": "마자요 이제 여름끝",
          "original_form": "마자요 이제 여름끝",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100013136.1.15",
          "form": "여름 끝나서 너무 좋아요ㅎㅎ",
          "original_form": "여름 끝나서 너무 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100013136.1.16",
          "form": "여름 너무더웠어요..",
          "original_form": "여름 너무더웠어요..",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100013136.1.17",
          "form": "맞아요ㅠ 햇빛이 진짜 타버릴 것같이 뜨거웠어요",
          "original_form": "맞아요ㅠ 햇빛이 진짜 타버릴 것같이 뜨거웠어요",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100013136.1.18",
          "form": "좋아하는 계절이 어떻게 되세요?",
          "original_form": "좋아하는 계절이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100013136.1.19",
          "form": "봄 가을이요~",
          "original_form": "봄 가을이요~",
          "speaker_id": "1",
          "time": "20210826 12:14"
        }
      ]
