      "utterance": [
        {
          "id": "MDRW2100017718.1.1",
          "form": "처음 뵙겠습니다",
          "original_form": "처음 뵙겠습니다",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017718.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017718.1.3",
          "form": "저는 작가는 김진명님을 좋아하고",
          "original_form": "저는 작가는 김진명님을 좋아하고",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017718.1.4",
          "form": "근래 베스트 셀러는 본게 없어서 모르겠네요",
          "original_form": "근래 베스트 셀러는 본게 없어서 모르겠네요",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017718.1.5",
          "form": "저는 웹소설이랑 웹툰을좋아합니다",
          "original_form": "저는 웹소설이랑 웹툰을좋아합니다",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017718.1.6",
          "form": "저도 좋아하는데 어떤거 좋아하세요? 저는 웹소설 근래 재미있게 본거는 재혼황후이고요 웹툰은 동트는 로맨스 재미있게봤어요",
          "original_form": "저도 좋아하는데 어떤거 좋아하세요? 저는 웹소설 근래 재미있게 본거는 재혼황후이고요 웹툰은 동트는 로맨스 재미있게봤어요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017718.1.7",
          "form": "요즘은 경성빵집이라는걸 재미있게보고있습니다",
          "original_form": "요즘은 경성빵집이라는걸 재미있게보고있습니다",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017718.1.8",
          "form": "플랫폼이 어딘가요?",
          "original_form": "플랫폼이 어딘가요?",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017718.1.9",
          "form": "카카오페이지요",
          "original_form": "카카오페이지요",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017718.1.10",
          "form": "기다리면무료형식인데",
          "original_form": "기다리면무료형식인데",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017718.1.11",
          "form": "빵에진심인 남녀라 재미있습니다ㅎ",
          "original_form": "빵에진심인 남녀라 재미있습니다ㅎ",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017718.1.12",
          "form": "재미있을거 같아요",
          "original_form": "재미있을거 같아요",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017718.1.13",
          "form": "저는 카카오페이지에서 재미있게 본거는",
          "original_form": "저는 카카오페이지에서 재미있게 본거는",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017718.1.14",
          "form": "루시아가 좋았어요",
          "original_form": "루시아가 좋았어요",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017718.1.15",
          "form": "수위가 조금 있기는 해도",
          "original_form": "수위가 조금 있기는 해도",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017718.1.16",
          "form": "만족합니다",
          "original_form": "만족합니다",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017718.1.17",
          "form": "그렇구나 루시아 저도 나중에 한번 봐야겠네요",
          "original_form": "그렇구나 루시아 저도 나중에 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017718.1.18",
          "form": "브링더 러브도 재미있어요",
          "original_form": "브링더 러브도 재미있어요",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017718.1.19",
          "form": "악녀도사랑한다? 그것도 재미있습니다",
          "original_form": "악녀도사랑한다? 그것도 재미있습니다",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017718.1.20",
          "form": "음? 카카페꺼인가요?",
          "original_form": "음? 카카페꺼인가요?",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017718.1.21",
          "form": "네 맞습니다",
          "original_form": "네 맞습니다",
          "speaker_id": "2",
          "time": "20210826 21:05"
        }
