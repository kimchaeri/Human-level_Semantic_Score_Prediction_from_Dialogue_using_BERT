        {
          "id": "MDRW2100045987.1.1",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045987.1.2",
          "form": "하잇 밥은?",
          "original_form": "하잇 밥은?",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045987.1.3",
          "form": "난 먹었지",
          "original_form": "난 먹었지",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045987.1.4",
          "form": "난 아직못먹었어",
          "original_form": "난 아직못먹었어",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045987.1.5",
          "form": "두찌먹는중",
          "original_form": "두찌먹는중",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045987.1.6",
          "form": "어여 아침먹어",
          "original_form": "어여 아침먹어",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045987.1.7",
          "form": "응 그냥 빵산거에 커피먹으려고ㅠㅠ",
          "original_form": "응 그냥 빵산거에 커피먹으려고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045987.1.8",
          "form": "맛있겠네ㅋ 나도 대충먹고옴",
          "original_form": "맛있겠네ㅋ 나도 대충먹고옴",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045987.1.9",
          "form": "든든히 무야또 열일하지ㅠㅠ",
          "original_form": "든든히 무야또 열일하지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045987.1.10",
          "form": "파라바게뜨 오늘사러가나",
          "original_form": "파라바게뜨 오늘사러가나",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045987.1.11",
          "form": "응가야지 무슨빵사지",
          "original_form": "응가야지 무슨빵사지",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045987.1.12",
          "form": "유통기한긴걸로 사야지ㅋㅋ",
          "original_form": "유통기한긴걸로 사야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045987.1.13",
          "form": "그래야겠네",
          "original_form": "그래야겠네",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045987.1.14",
          "form": "나는 냉동식품 사게 있대서ㅋㄱ",
          "original_form": "나는 냉동식품 사게 있대서ㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045987.1.15",
          "form": "나도그거는보긴 봤어",
          "original_form": "나도그거는보긴 봤어",
