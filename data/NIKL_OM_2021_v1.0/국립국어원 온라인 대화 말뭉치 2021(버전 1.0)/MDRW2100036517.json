      "utterance": [
        {
          "id": "MDRW2100036517.1.1",
          "form": "닉네임 너무 귀여워요 ㅎㅎ 잘 주무세요? ㅋㅋ",
          "original_form": "닉네임 너무 귀여워요 ㅎㅎ 잘 주무세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036517.1.2",
          "form": "네네 잠은 잘자요ㅋㅋ",
          "original_form": "네네 잠은 잘자요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100036517.1.3",
          "form": "ㅋㅋㅋㅋㅋ 맨날 자요? 또자 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 맨날 자요? 또자 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100036517.1.4",
          "form": "자고 또자고ㅎㅎ",
          "original_form": "자고 또자고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100036517.1.5",
          "form": "육아하시나요?",
          "original_form": "육아하시나요?",
          "speaker_id": "2",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036517.1.6",
          "form": "출근해서 육아는 유치원에서 ㅋㅋㅋㅋ",
          "original_form": "출근해서 육아는 유치원에서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036517.1.7",
          "form": "ㅋㅋㅋ 유치원 고마운곳",
          "original_form": "ㅋㅋㅋ 유치원 고마운곳",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036517.1.8",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 너무 고맙죠 ㅋㅋ 밥에 간식까지 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 너무 고맙죠 ㅋㅋ 밥에 간식까지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100036517.1.9",
          "form": "진짜 그때가 제일 좋았던것같아요",
          "original_form": "진짜 그때가 제일 좋았던것같아요",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036517.1.10",
          "form": "몇살이랬죠?",
          "original_form": "몇살이랬죠?",
          "speaker_id": "1",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100036517.1.11",
          "form": "1살이여",
          "original_form": "1살이여",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100036517.1.12",
          "form": "완전 에너자이저",
          "original_form": "완전 에너자이저",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100036517.1.13",
          "form": "어이쿠 ㅠㅠ 몇개월 안되었나봐요 ㅜㅜ",
          "original_form": "어이쿠 ㅠㅠ 몇개월 안되었나봐요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100036517.1.14",
          "form": "아가예요",
          "original_form": "아가예요",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100036517.1.15",
