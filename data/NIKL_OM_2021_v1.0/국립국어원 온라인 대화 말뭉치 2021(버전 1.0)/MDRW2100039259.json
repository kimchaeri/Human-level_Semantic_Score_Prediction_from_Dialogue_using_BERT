        },
        {
          "id": "MDRW2100039259.1.2",
          "form": "안녕ㅎㅏ세요",
          "original_form": "안녕ㅎㅏ세요",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100039259.1.3",
          "form": "장마가 지나갔네요",
          "original_form": "장마가 지나갔네요",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039259.1.4",
          "form": "그쪽은 괜찮은가요",
          "original_form": "그쪽은 괜찮은가요",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039259.1.5",
          "form": "네ㅋ엄청 더워요ㅠ",
          "original_form": "네ㅋ엄청 더워요ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039259.1.6",
          "form": "갑자기 늦더위가 기승을 부리네요",
          "original_form": "갑자기 늦더위가 기승을 부리네요",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039259.1.7",
          "form": "그러게요ㅠ 날도덥고 졸리기도ㅜ",
          "original_form": "그러게요ㅠ 날도덥고 졸리기도ㅜ",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100039259.1.8",
          "form": "일교차가 심해서 더 피곤한거 같아요",
          "original_form": "일교차가 심해서 더 피곤한거 같아요",
          "speaker_id": "1",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100039259.1.9",
          "form": "ㅋㅋㅋ저녁엔 모기도많고 하던데ㅜ 님동넨ㄷㄴ요?",
          "original_form": "ㅋㅋㅋ저녁엔 모기도많고 하던데ㅜ 님동넨ㄷㄴ요?",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039259.1.10",
          "form": "많아요ㅠ",
          "original_form": "많아요ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039259.1.11",
          "form": "어제도 2방 물렸어요",
          "original_form": "어제도 2방 물렸어요",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039259.1.12",
          "form": "ㅎㅎ방금도 서너군데 물려왔네요ㅜ",
          "original_form": "ㅎㅎ방금도 서너군데 물려왔네요ㅜ",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039259.1.13",
          "form": "아이고야 약은 바르셨나요",
          "original_form": "아이고야 약은 바르셨나요",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039259.1.14",
          "form": "ㅋㅋㅋ뭐 이미 물린거 의미있나요ㅋ",
          "original_form": "ㅋㅋㅋ뭐 이미 물린거 의미있나요ㅋ",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039259.1.15",
          "form": "들 간지럽게 하는데에는 좋죠",
          "original_form": "들 간지럽게 하는데에는 좋죠",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039259.1.16",
          "form": "ㅎㅎ하도 물려서 이제ㅠ",
          "original_form": "ㅎㅎ하도 물려서 이제ㅠ",
