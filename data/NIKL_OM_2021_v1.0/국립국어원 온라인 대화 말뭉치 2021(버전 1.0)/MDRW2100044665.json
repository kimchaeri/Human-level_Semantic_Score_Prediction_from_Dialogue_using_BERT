      "utterance": [
        {
          "id": "MDRW2100044665.1.1",
          "form": "요즘 그룹피티 다녀요 ㅋㅋㅋ",
          "original_form": "요즘 그룹피티 다녀요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044665.1.2",
          "form": "와 진짜 노력중이셨군요!",
          "original_form": "와 진짜 노력중이셨군요!",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044665.1.3",
          "form": "몇명이요?",
          "original_form": "몇명이요?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044665.1.4",
          "form": "그때그때 신청해서 가는 거라서",
          "original_form": "그때그때 신청해서 가는 거라서",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044665.1.5",
          "form": "매번 달라요 ㅋㅋㅋ",
          "original_form": "매번 달라요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044665.1.6",
          "form": "오늘은 7명 정도? 있엇어요",
          "original_form": "오늘은 7명 정도? 있엇어요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044665.1.7",
          "form": "오 신기하네요 어떻게 신청하는거에요?",
          "original_form": "오 신기하네요 어떻게 신청하는거에요?",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044665.1.8",
          "form": "온라인으로 가고 싶은 시간대 신청해요! ㅋㅋ",
          "original_form": "온라인으로 가고 싶은 시간대 신청해요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044665.1.9",
          "form": "오 그럼 가기전에는 몇명이 모이는지 모르고 가는건가요?",
          "original_form": "오 그럼 가기전에는 몇명이 모이는지 모르고 가는건가요?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044665.1.10",
          "form": "아니여 1시간 전까지만 신청이랑 취소돼서",
          "original_form": "아니여 1시간 전까지만 신청이랑 취소돼서",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044665.1.11",
          "form": "가기 전엔 알 수 있어요",
          "original_form": "가기 전엔 알 수 있어요",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044665.1.12",
          "form": "아하 그렇군요~ 오늘 수업은 어떠셨어요?",
          "original_form": "아하 그렇군요~ 오늘 수업은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044665.1.13",
          "form": "ㅎㅎ 늘 그렇듯이 넘 힘들었어요",
          "original_form": "ㅎㅎ 늘 그렇듯이 넘 힘들었어요",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044665.1.14",
          "form": "근데 지금 헬스장에서 샤워 가능해요?",
          "original_form": "근데 지금 헬스장에서 샤워 가능해요?",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044665.1.15",
          "form": "아니요 집 앞이라서 그냥 다냐와요 ㅋㅋ",
          "original_form": "아니요 집 앞이라서 그냥 다냐와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044665.1.16",
          "form": "역시 운동은 가까운게 짱이네요 ㅠ",
          "original_form": "역시 운동은 가까운게 짱이네요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044665.1.17",
          "form": "피티는 어떤 목표로 하시는건가요?",
          "original_form": "피티는 어떤 목표로 하시는건가요?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044665.1.18",
          "form": "체력증진이요 ㅋㅋㅋㅋ",
          "original_form": "체력증진이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044665.1.19",
