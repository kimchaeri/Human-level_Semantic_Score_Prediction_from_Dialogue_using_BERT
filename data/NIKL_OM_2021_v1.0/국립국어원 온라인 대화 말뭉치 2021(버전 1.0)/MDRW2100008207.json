      "utterance": [
        {
          "id": "MDRW2100008207.1.1",
          "form": "포켓몬고는 해보셨어요?",
          "original_form": "포켓몬고는 해보셨어요?",
          "speaker_id": "2",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008207.1.2",
          "form": "아!! 저 옛날에",
          "original_form": "아!! 저 옛날에",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008207.1.3",
          "form": "속초에서 됐을때만 한번 해봤어요 ㅋㅋㅋㅋ",
          "original_form": "속초에서 됐을때만 한번 해봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008207.1.4",
          "form": "제 폰이 구려서 오래 못버티더라구요",
          "original_form": "제 폰이 구려서 오래 못버티더라구요",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008207.1.5",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008207.1.6",
          "form": "저도 한참 인기일땐 폰이 구리더니",
          "original_form": "저도 한참 인기일땐 폰이 구리더니",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008207.1.7",
          "form": "요새는 또 시들한가봐요?",
          "original_form": "요새는 또 시들한가봐요?",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008207.1.8",
          "form": "아 요새는 또 시들해요?",
          "original_form": "아 요새는 또 시들해요?",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008207.1.9",
          "form": "그거 하면 재밌을 거 같아요 야외로 나가고 강제 운동도 하고..",
          "original_form": "그거 하면 재밌을 거 같아요 야외로 나가고 강제 운동도 하고..",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008207.1.10",
          "form": "근데 gps 조작해서 하는 사람들 많더라구요",
          "original_form": "근데 gps 조작해서 하는 사람들 많더라구요",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008207.1.11",
          "form": "아 그런",
          "original_form": "아 그런",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008207.1.12",
          "form": "근데 뭔가 그 심리 이해갈것같기도하고",
          "original_form": "근데 뭔가 그 심리 이해갈것같기도하고",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008207.1.13",
          "form": "하다보면 이게뭐라고 집착하게되더는",
          "original_form": "하다보면 이게뭐라고 집착하게되더는",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008207.1.14",
          "form": "게임이라는게 ㅋㅋ",
          "original_form": "게임이라는게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008207.1.15",
          "form": "맞아욬ㅋㅋㅋㅋㅋ 현생에 도움도 안되는데 집착하게되고",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋ 현생에 도움도 안되는데 집착하게되고",
          "speaker_id": "1",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008207.1.16",
          "form": "현생에서 쪼렙인만큼 게임에서 고렙된다고 하잖아요",
          "original_form": "현생에서 쪼렙인만큼 게임에서 고렙된다고 하잖아요",
          "speaker_id": "1",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008207.1.17",
          "form": "그래서 게임 하고 나면 그렇게 현타가 오나봐요",
          "original_form": "그래서 게임 하고 나면 그렇게 현타가 오나봐요",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008207.1.18",
          "form": "내가 뭘한거지 같은 ㅋㅋ",
          "original_form": "내가 뭘한거지 같은 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008207.1.19",
          "form": "그 솜사탕 씻은 너구리 짤 아세요?",
          "original_form": "그 솜사탕 씻은 너구리 짤 아세요?",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008207.1.20",
          "form": "시간이 날아가서 허무한 그 상태 됩니닼ㅋㅋ",
          "original_form": "시간이 날아가서 허무한 그 상태 됩니닼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008207.1.21",
          "form": "폰겜만해서 다행인거같기도하고 ㅋㅋ",
          "original_form": "폰겜만해서 다행인거같기도하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008207.1.22",
          "form": "저는 그래서 최대한 새로시작하는거라도 안늘이려고요 ㅋㅋ",
          "original_form": "저는 그래서 최대한 새로시작하는거라도 안늘이려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008207.1.23",
          "form": "아 맞아욬ㅋㅋㅋ 새로 시작하면 또 시간 금방 가요",
          "original_form": "아 맞아욬ㅋㅋㅋ 새로 시작하면 또 시간 금방 가요",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008207.1.24",
          "form": "폰겜은 폰 배터리 많이 안 잡아먹나요",
          "original_form": "폰겜은 폰 배터리 많이 안 잡아먹나요",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008207.1.25",
          "form": "최근 시작한것들은 그렇게 용량이 안커서요",
          "original_form": "최근 시작한것들은 그렇게 용량이 안커서요",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008207.1.26",
          "form": "아하 저는 폰도 너무 발열 심해서 게임을 못해요 포켓몬고도 나중에 다시 해보고싶어요 ㅎㅎ",
          "original_form": "아하 저는 폰도 너무 발열 심해서 게임을 못해요 포켓몬고도 나중에 다시 해보고싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008207.1.27",
          "form": "확실히 폰 수명이 줄어드는것같긴 하더라고요",
          "original_form": "확실히 폰 수명이 줄어드는것같긴 하더라고요",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008207.1.28",
          "form": "전 지금하는게임들 끊는게 목표인데ㅋㅋ",
          "original_form": "전 지금하는게임들 끊는게 목표인데ㅋㅋ",
          "speaker_id": "2",
