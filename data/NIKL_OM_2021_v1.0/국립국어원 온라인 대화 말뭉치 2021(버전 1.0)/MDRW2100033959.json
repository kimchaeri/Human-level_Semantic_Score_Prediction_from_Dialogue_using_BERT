      "utterance": [
        {
          "id": "MDRW2100033959.1.1",
          "form": "점심 맛있는거 드셨어요?",
          "original_form": "점심 맛있는거 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100033959.1.2",
          "form": "저는 라떼로 점심을 떼웠어요ㅜㅜ",
          "original_form": "저는 라떼로 점심을 떼웠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100033959.1.3",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100033959.1.4",
          "form": "헉 라떼가 점심이라뇨!!ㅜㅜ\r\n잘챙겨드셔야해요!!",
          "original_form": "헉 라떼가 점심이라뇨!!ㅜㅜ\r\n잘챙겨드셔야해요!!",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100033959.1.5",
          "form": "저는 북촌손만두에서 떡만둣국 배달시켜먹었어요ㅎㅎ",
          "original_form": "저는 북촌손만두에서 떡만둣국 배달시켜먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100033959.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033959.1.7",
          "form": "ㅇㄴ 멋진 점심 드셨다",
          "original_form": "ㅇㄴ 멋진 점심 드셨다",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033959.1.8",
          "form": "만두 어떤거 좋아하세요?",
          "original_form": "만두 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033959.1.9",
          "form": "고기 vs 김치",
          "original_form": "고기 vs 김치",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033959.1.10",
          "form": "와 이질문은 넘 어려운데요?",
          "original_form": "와 이질문은 넘 어려운데요?",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100033959.1.11",
          "form": "일단..오늘은 김치로 할게요ㅋㄱㅋ",
          "original_form": "일단..오늘은 김치로 할게요ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100033959.1.12",
          "form": "웹툰웹소님은요?",
          "original_form": "웹툰웹소님은요?",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100033959.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100033959.1.14",
          "form": "엇 저는 고기.",
          "original_form": "엇 저는 고기.",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100033959.1.15",
          "form": "군침고이네요",
          "original_form": "군침고이네요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100033959.1.16",
          "form": "그러면 라떼 vs 아메리카노",
          "original_form": "그러면 라떼 vs 아메리카노",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100033959.1.17",
          "form": "크크 같이 만두먹으러가면 반반시키면 되겠어요\r\n전 커피는 아메리카노요~~ㅎㅎ",
          "original_form": "크크 같이 만두먹으러가면 반반시키면 되겠어요\r\n전 커피는 아메리카노요~~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033959.1.18",
          "form": "왠지 밥먹고는 아메리카노 마시게 되더라구요!",
          "original_form": "왠지 밥먹고는 아메리카노 마시게 되더라구요!",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033959.1.19",
          "form": "깔끔담백!!",
          "original_form": "깔끔담백!!",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033959.1.20",
          "form": "맞아요 글고 아메리카노가",
          "original_form": "맞아요 글고 아메리카노가",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100033959.1.21",
          "form": "진짜 소화를 돕는다는 연구결과가 있더라고요",
          "original_form": "진짜 소화를 돕는다는 연구결과가 있더라고요",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100033959.1.22",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033959.1.23",
          "form": "합법적으로",
          "original_form": "합법적으로",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033959.1.24",
          "form": "마실 이유가",
          "original_form": "마실 이유가",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033959.1.25",
          "form": "생겼네요",
          "original_form": "생겼네요",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033959.1.26",
          "form": "크크크",
          "original_form": "크크크",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033959.1.27",
          "form": "그리고 디저트류 먹을때 궁합이 너무 좋아요",
          "original_form": "그리고 디저트류 먹을때 궁합이 너무 좋아요",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033959.1.28",
          "form": "맞아요!! 디저트도 좋아하시는구나",
          "original_form": "맞아요!! 디저트도 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100033959.1.29",
          "form": "어떤 디저트 젤 좋아하세여?",
          "original_form": "어떤 디저트 젤 좋아하세여?",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100033959.1.30",
          "form": "요즘은 스콘이나 마들렌같은 구움과자가 맛있더라구요!!!",
          "original_form": "요즘은 스콘이나 마들렌같은 구움과자가 맛있더라구요!!!",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100033959.1.31",
          "form": "무슨 디저트 좋아하세요?",
          "original_form": "무슨 디저트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033959.1.32",
          "form": "저는 마카롱이랑 헉 저도 스콘 좋아해요",
          "original_form": "저는 마카롱이랑 헉 저도 스콘 좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100033959.1.33",
          "form": "말차 스콘이랑 얼그레이스콘이 좋더라고요",
          "original_form": "말차 스콘이랑 얼그레이스콘이 좋더라고요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100033959.1.34",
          "form": "악 마카롱도 너무 맛있죠ㅠㅠ\r\n한창 마카롱에 미쳐서 어디 놀러갈때마다 근처 마카롱맛집 털었는뎈ㅋㅋㅋㅋ",
          "original_form": "악 마카롱도 너무 맛있죠ㅠㅠ\r\n한창 마카롱에 미쳐서 어디 놀러갈때마다 근처 마카롱맛집 털었는뎈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033959.1.35",
          "form": "스콘도 맛이 다양해서 진짜 맛있고ㅜㅠ",
          "original_form": "스콘도 맛이 다양해서 진짜 맛있고ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033959.1.36",
          "form": "딸기잼이랑 먹으면 넘 맛있죠ㅜㅜ",
