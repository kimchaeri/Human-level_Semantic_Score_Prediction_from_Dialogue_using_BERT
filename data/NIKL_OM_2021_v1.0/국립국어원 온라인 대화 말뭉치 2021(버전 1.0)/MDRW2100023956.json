          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023956.1.6",
          "form": "맥도날드",
          "original_form": "맥도날드",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023956.1.7",
          "form": "엇 맥날 뭐 드셨어여",
          "original_form": "엇 맥날 뭐 드셨어여",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100023956.1.8",
          "form": "창녕갈릭버거 드셔보셨어여? ㅋㅋ",
          "original_form": "창녕갈릭버거 드셔보셨어여? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100023956.1.9",
          "form": "그거 맛 괜차나여",
          "original_form": "그거 맛 괜차나여",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100023956.1.10",
          "form": "오늘은 그거 안먹었지먀",
          "original_form": "오늘은 그거 안먹었지먀",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100023956.1.11",
          "form": "만",
          "original_form": "만",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100023956.1.12",
          "form": "저는 주말에 갔더니\r\n판매일시중단이라 못먹었어요",
          "original_form": "저는 주말에 갔더니\r\n판매일시중단이라 못먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023956.1.13",
          "form": "호불호 있던데\r\nname1님한테는 호였군영",
          "original_form": "호불호 있던데\r\n&name1&님한테는 호였군영",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100023956.1.14",
          "form": "네 전 좋았어요",
          "original_form": "네 전 좋았어요",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100023956.1.15",
          "form": "버거 좋아하시는군여",
          "original_form": "버거 좋아하시는군여",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100023956.1.16",
          "form": "ㅋㅋ코시국 이후에\r\n외출하면 항상 맥날\r\n드라이브스루행이라서요ㅠㅋㅋ",
          "original_form": "ㅋㅋ코시국 이후에\r\n외출하면 항상 맥날\r\n드라이브스루행이라서요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023956.1.17",
          "form": "아.....",
          "original_form": "아.....",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100023956.1.18",
          "form": "슬픈이야기였네요",
          "original_form": "슬픈이야기였네요",
          "speaker_id": "1",
          "time": "20210830 21:59"
