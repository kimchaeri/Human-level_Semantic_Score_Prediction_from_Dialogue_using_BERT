      "utterance": [
        {
          "id": "MDRW2100018471.1.1",
          "form": "ㅇㄴ 군대라",
          "original_form": "ㅇㄴ 군대라",
          "speaker_id": "1",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018471.1.2",
          "form": "먼저 군대가시나요 안가셔도 되나요",
          "original_form": "먼저 군대가시나요 안가셔도 되나요",
          "speaker_id": "1",
          "time": "20210827 02:31"
        },
        {
          "id": "MDRW2100018471.1.3",
          "form": "안가도 되지 말입니다...!",
          "original_form": "안가도 되지 말입니다...!",
          "speaker_id": "2",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018471.1.4",
          "form": "님은요?ㅋㅋㅋㅋ",
          "original_form": "님은요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018471.1.5",
          "form": "저도 그렇지 말입니다!!!",
          "original_form": "저도 그렇지 말입니다!!!",
          "speaker_id": "1",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018471.1.6",
          "form": "그러면",
          "original_form": "그러면",
          "speaker_id": "1",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018471.1.7",
          "form": "군대갔다고 치고",
          "original_form": "군대갔다고 치고",
          "speaker_id": "1",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018471.1.8",
          "form": "아니 군대간 남친이 있다 치고",
          "original_form": "아니 군대간 남친이 있다 치고",
          "speaker_id": "1",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018471.1.9",
          "form": "내가 면회가면 나한테 오올~~~~이쁜 여자친구 분 하면서 추파 던지는 남친 후임 vs 여친 요리 잘하냐고 물어보면서 담엔 맛있는거 더 싸오하는 선임",
          "original_form": "내가 면회가면 나한테 오올~~~~이쁜 여자친구 분 하면서 추파 던지는 남친 후임 vs 여친 요리 잘하냐고 물어보면서 담엔 맛있는거 더 싸오하는 선임",
          "speaker_id": "1",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018471.1.10",
          "form": "더 꼴불견은?",
          "original_form": "더 꼴불견은?",
          "speaker_id": "1",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018471.1.11",
          "form": "*싸오라는",
          "original_form": "*싸오라는",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018471.1.12",
          "form": "ㅋㅋㅋㅋㅋㄱㅋ진심이시네요",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋ진심이시네요",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018471.1.13",
          "form": "고민 들어갑니다",
          "original_form": "고민 들어갑니다",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.14",
          "form": "방금 제가 고른거",
          "original_form": "방금 제가 고른거",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.15",
          "form": "훔쳐왔어요",
          "original_form": "훔쳐왔어요",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.17",
          "form": "아앜ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "아앜ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.18",
          "form": "후자다",
          "original_form": "후자다",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.19",
          "form": "이건 후자가 진상 선임 아닙니까",
          "original_form": "이건 후자가 진상 선임 아닙니까",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.20",
          "form": "진심이 담겨있어서 더 싫어",
          "original_form": "진심이 담겨있어서 더 싫어",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.21",
          "form": "후자가 꼴불견이다?",
          "original_form": "후자가 꼴불견이다?",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018471.1.22",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.23",
          "form": "ㅋㅋㅋㅋ네 뭐 고르셨어요",
          "original_form": "ㅋㅋㅋㅋ네 뭐 고르셨어요",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.24",
          "form": "있을법해서 더 싫죠",
          "original_form": "있을법해서 더 싫죠",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.25",
          "form": "전자에요?!?",
          "original_form": "전자에요?!?",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.26",
          "form": "저도 후자요",
          "original_form": "저도 후자요",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.27",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.28",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.29",
          "form": "뭔데 시켜 ㅠ",
          "original_form": "뭔데 시켜 ㅠ",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.30",
          "form": "꼭 한명씩 있다그러대요",
          "original_form": "꼭 한명씩 있다그러대요",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.31",
          "form": "이랬어요",
          "original_form": "이랬어요",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.32",
          "form": "ㅋㅋㅋㅋㅋ해먹어ㅠ 해먹으라고ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ해먹어ㅠ 해먹으라고ㅠ",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018471.1.33",
          "form": "그러면 그러면",
          "original_form": "그러면 그러면",
          "speaker_id": "1",
          "time": "20210827 02:37"
        }
      ]
