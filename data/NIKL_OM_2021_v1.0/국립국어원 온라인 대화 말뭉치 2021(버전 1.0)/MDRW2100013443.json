        {
          "id": "MDRW2100013443.1.1",
          "form": "저 여행가고싶어요",
          "original_form": "저 여행가고싶어요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013443.1.2",
          "form": "해외 여행가보셨나요?",
          "original_form": "해외 여행가보셨나요?",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013443.1.3",
          "form": "네 가봤어요",
          "original_form": "네 가봤어요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013443.1.4",
          "form": "name2은",
          "original_form": "&name2&은",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.5",
          "form": "비행기 타고싶어요",
          "original_form": "비행기 타고싶어요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.6",
          "form": "비행기~~~ 부우웅",
          "original_form": "비행기~~~ 부우웅",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.7",
          "form": "기억나는 행선지가 있으신가요",
          "original_form": "기억나는 행선지가 있으신가요",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.8",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.9",
          "form": "저는 어릴때 공항에갔던기억과",
          "original_form": "저는 어릴때 공항에갔던기억과",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.10",
          "form": "제주도 수학여행,",
          "original_form": "제주도 수학여행,",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.11",
          "form": "하지만 쥐어짜보니 있내요",
          "original_form": "하지만 쥐어짜보니 있내요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013443.1.12",
          "form": "해외여행갔었을때가 있어요",
          "original_form": "해외여행갔었을때가 있어요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013443.1.13",
          "form": "제주도 수학여행때 생각너내요",
          "original_form": "제주도 수학여행때 생각너내요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013443.1.14",
          "form": "저는 제주도에서",
          "original_form": "저는 제주도에서",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013443.1.15",
          "form": "비행기 소리 아세요?",
          "original_form": "비행기 소리 아세요?",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013443.1.16",
          "form": "비행기탔던 그",
          "original_form": "비행기탔던 그",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013443.1.17",
          "form": "기분이 아직도 생생해요",
          "original_form": "기분이 아직도 생생해요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013443.1.18",
          "form": "그날 먹으려고 슈퍼에서 골든볼을 샀었거든요",
