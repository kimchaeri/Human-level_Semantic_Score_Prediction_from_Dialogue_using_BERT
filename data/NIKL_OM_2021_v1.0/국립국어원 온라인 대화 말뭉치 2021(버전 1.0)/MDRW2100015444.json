      "utterance": [
        {
          "id": "MDRW2100015444.1.1",
          "form": "뭐 사고 깊은거 있어?",
          "original_form": "뭐 사고 깊은거 있어?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015444.1.2",
          "form": "난 게임좋아해서",
          "original_form": "난 게임좋아해서",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015444.1.3",
          "form": "발매 기다리고있는 게임 있어",
          "original_form": "발매 기다리고있는 게임 있어",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015444.1.4",
          "form": "어떤거? 닌텐도겜?",
          "original_form": "어떤거? 닌텐도겜?",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015444.1.5",
          "form": "응 닌텐도!",
          "original_form": "응 닌텐도!",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015444.1.6",
          "form": "포켓몬스터도 다음 시리즈 나올예정이구",
          "original_form": "포켓몬스터도 다음 시리즈 나올예정이구",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015444.1.7",
          "form": "포켓몬 좋아해서 집에 인형이랑 소품들도 많아ㅋㅋㅋ",
          "original_form": "포켓몬 좋아해서 집에 인형이랑 소품들도 많아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015444.1.8",
          "form": "포켓몬인형 더 사고싶엉",
          "original_form": "포켓몬인형 더 사고싶엉",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015444.1.9",
          "form": "그래서 라이쮸구낭..",
          "original_form": "그래서 라이쮸구낭..",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015444.1.10",
          "form": "포켓몬인형 종류가 별로 엄던데...",
          "original_form": "포켓몬인형 종류가 별로 엄던데...",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015444.1.11",
          "form": "난 얼마전 ㄲ고부기 삿지",
          "original_form": "난 얼마전 ㄲ고부기 삿지",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015444.1.12",
          "form": "아 맞아 라이츄 좋아",
          "original_form": "아 맞아 라이츄 좋아",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015444.1.13",
          "form": "꼬부기 어떤거?",
          "original_form": "꼬부기 어떤거?",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015444.1.14",
          "form": "인형.. 샀엉...",
          "original_form": "인형.. 샀엉...",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015444.1.15",
          "form": "꼬부기 너무 귀여워서..",
          "original_form": "꼬부기 너무 귀여워서..",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015444.1.16",
          "form": "꼬북꼬북",
          "original_form": "꼬북꼬북",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015444.1.17",
          "form": "포켓몬 캐릭터 넘나 잘뽑았지",
          "original_form": "포켓몬 캐릭터 넘나 잘뽑았지",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015444.1.18",
          "form": "닌텐도는 기계 뭐갖고잇어?",
          "original_form": "닌텐도는 기계 뭐갖고잇어?",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015444.1.19",
          "form": "닌텐도스위치랑 플스랑 다 있긴해",
          "original_form": "닌텐도스위치랑 플스랑 다 있긴해",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015444.1.20",
          "form": "게임사고싶당..",
          "original_form": "게임사고싶당..",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015444.1.21",
          "form": "플스 최근꺼?",
          "original_form": "플스 최근꺼?",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015444.1.22",
          "form": "아니 5는 아직 안샀어",
          "original_form": "아니 5는 아직 안샀어",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015444.1.23",
          "form": "플스는 먼가 잘 안하게되더라구",
          "original_form": "플스는 먼가 잘 안하게되더라구",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015444.1.24",
          "form": "요즘 닌텐도는 핵안쓰나?",
          "original_form": "요즘 닌텐도는 핵안쓰나?",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015444.1.25",
          "form": "음 나는 안써서 모르겠네",
          "original_form": "음 나는 안써서 모르겠네",
          "speaker_id": "2",
          "time": "20210825 23:36"
        }
      ]
    }
