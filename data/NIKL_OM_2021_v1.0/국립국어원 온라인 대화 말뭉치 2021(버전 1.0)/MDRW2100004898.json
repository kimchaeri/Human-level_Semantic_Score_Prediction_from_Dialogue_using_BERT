        },
        {
          "id": "MDRW2100004898.1.2",
          "form": "들어왔어",
          "original_form": "들어왔어",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004898.1.3",
          "form": "그걸로 미용실갈거야",
          "original_form": "그걸로 미용실갈거야",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004898.1.4",
          "form": "오 어떤 머리할건데?",
          "original_form": "오 어떤 머리할건데?",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004898.1.5",
          "form": "내가 찾아봤는데 드디어 c컬 s컬 차이를",
          "original_form": "내가 찾아봤는데 드디어 c컬 s컬 차이를",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004898.1.6",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004898.1.7",
          "form": "펌할겨",
          "original_form": "펌할겨",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.8",
          "form": "답장느리당",
          "original_form": "답장느리당",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.9",
          "form": "c펌이랑 s컬 중에 모할겨?",
          "original_form": "c펌이랑 s컬 중에 모할겨?",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.10",
          "form": "기다리라구-!",
          "original_form": "기다리라구-!",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ나 s컬!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ나 s컬!!",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.12",
          "form": "근데 머리가 더 길면",
          "original_form": "근데 머리가 더 길면",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.13",
          "form": "좋을거같긴해",
          "original_form": "좋을거같긴해",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.14",
          "form": "장발 s컬?",
          "original_form": "장발 s컬?",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004898.1.15",
          "form": "그냥 할거지만",
          "original_form": "그냥 할거지만",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
