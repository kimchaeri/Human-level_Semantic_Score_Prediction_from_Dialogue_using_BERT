      "utterance": [
        {
          "id": "MDRW2100035801.1.1",
          "form": "밀키트로 음식해보셨어요?",
          "original_form": "밀키트로 음식해보셨어요?",
          "speaker_id": "1",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100035801.1.2",
          "form": "네네 감바스 사서 먹어봤어요ㅋㅋㄱ괜찮더라구요!  사보셨어용?",
          "original_form": "네네 감바스 사서 먹어봤어요ㅋㅋㄱ괜찮더라구요!  사보셨어용?",
          "speaker_id": "2",
          "time": "20210830 11:22"
        },
        {
          "id": "MDRW2100035801.1.3",
          "form": "아뇨 못만들어봤어요",
          "original_form": "아뇨 못만들어봤어요",
          "speaker_id": "1",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100035801.1.4",
          "form": "아하 밀키트진짜편해서 사보면 좋을거같아요ㅎㅎ",
          "original_form": "아하 밀키트진짜편해서 사보면 좋을거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100035801.1.5",
          "form": "코로나되고 좋아진거 거의 유일한게 밀키트같네요",
          "original_form": "코로나되고 좋아진거 거의 유일한게 밀키트같네요",
          "speaker_id": "1",
          "time": "20210830 14:30"
        },
        {
          "id": "MDRW2100035801.1.6",
          "form": "아 그쵸 정말 포장도완벽하고 순서도잘적혀있구 똥손이리도 누구나 할수있는 ㅋㅋㅋ",
          "original_form": "아 그쵸 정말 포장도완벽하고 순서도잘적혀있구 똥손이리도 누구나 할수있는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100035801.1.7",
          "form": "혹시 담꾹 들어보셨어요?",
          "original_form": "혹시 담꾹 들어보셨어요?",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100035801.1.8",
          "form": "담꾹은 모예용? 못들어봤어용!",
          "original_form": "담꾹은 모예용? 못들어봤어용!",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100035801.1.9",
          "form": "밀키트 업체 이름이요 네이버에서 검색해보세요",
          "original_form": "밀키트 업체 이름이요 네이버에서 검색해보세요",
          "speaker_id": "1",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100035801.1.10",
          "form": "유명한가봐요!",
          "original_form": "유명한가봐요!",
          "speaker_id": "2",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100035801.1.11",
          "form": "검색해봐야겠네요ㅎㅎ",
          "original_form": "검색해봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100035801.1.12",
          "form": "지점이 꽤 많더라구요",
          "original_form": "지점이 꽤 많더라구요",
          "speaker_id": "1",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100035801.1.13",
          "form": "와 그냥인터넷으로 파는거말구 지점도있는곳이예요,? 헙",
          "original_form": "와 그냥인터넷으로 파는거말구 지점도있는곳이예요,? 헙",
          "speaker_id": "2",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100035801.1.14",
          "form": "넹넹 담꾹으로 네이버검색해보세요",
          "original_form": "넹넹 담꾹으로 네이버검색해보세요",
          "speaker_id": "1",
          "time": "20210830 18:15"
        },
        {
          "id": "MDRW2100035801.1.15",
          "form": "와 저희동네에 최근에 생겼네요 대박 가봐야겠어요ㅋㅋㅋ",
          "original_form": "와 저희동네에 최근에 생겼네요 대박 가봐야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100035801.1.16",
          "form": "오오 가보시고 얘기해주세요",
          "original_form": "오오 가보시고 얘기해주세요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100035801.1.17",
          "form": "저도 먹어보고 싶은데 집에선 음식을 다 해먹으니 먹을 일이 없어요",
          "original_form": "저도 먹어보고 싶은데 집에선 음식을 다 해먹으니 먹을 일이 없어요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100035801.1.18",
          "form": "사실 그게 맞는거같은데 ㅠㅠ 흑흑 \n덕분에 개업했다는걸알게됐어요ㅋㅋㅋㅋ",
          "original_form": "사실 그게 맞는거같은데 ㅠㅠ 흑흑 \n덕분에 개업했다는걸알게됐어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        }
      ]
