      "utterance": [
        {
          "id": "MDRW2100033297.1.1",
          "form": "이거로 대표적인게 우리 도서정가제도 엮여있었지",
          "original_form": "이거로 대표적인게 우리 도서정가제도 엮여있었지",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100033297.1.2",
          "form": "우웨",
          "original_form": "우웨",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033297.1.3",
          "form": "이거는 하긴 잘했는데",
          "original_form": "이거는 하긴 잘했는데",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033297.1.4",
          "form": "너도 알고 나도 알고 아주 엄청난 일이 벌어질줄알았지",
          "original_form": "너도 알고 나도 알고 아주 엄청난 일이 벌어질줄알았지",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033297.1.5",
          "form": "했는데??",
          "original_form": "했는데??",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033297.1.6",
          "form": "구글 정책이 그대로 시행됐으면 앞으로 이북 함부로 결제 못했겠지..",
          "original_form": "구글 정책이 그대로 시행됐으면 앞으로 이북 함부로 결제 못했겠지..",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100033297.1.7",
          "form": "ㅇㅇㅇ 그랬을거야",
          "original_form": "ㅇㅇㅇ 그랬을거야",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033297.1.8",
          "form": "40프로 인상이라니..",
          "original_form": "40프로 인상이라니..",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033297.1.9",
          "form": "진짜 끔찍",
          "original_form": "진짜 끔찍",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033297.1.10",
          "form": "구글 막은건 잘했는데",
          "original_form": "구글 막은건 잘했는데",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033297.1.11",
          "form": "이참에 도정제도 없애면 안되나 싶은",
          "original_form": "이참에 도정제도 없애면 안되나 싶은",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033297.1.12",
          "form": "눙데?",
          "original_form": "눙데?",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033297.1.13",
          "form": "아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033297.1.14",
          "form": "마저용....",
          "original_form": "마저용....",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100033297.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100033297.1.16",
          "form": "아예 없애는건 무링 수도 있지만...",
          "original_form": "아예 없애는건 무링 수도 있지만...",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100033297.1.17",
          "form": "아 좀 우리 협상좀 하자",
          "original_form": "아 좀 우리 협상좀 하자",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100033297.1.18",
          "form": "진쟈ㅜㅜ",
          "original_form": "진쟈ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100033297.1.19",
          "form": "인간적으로 할인률 10프로 제한이 뭔데ㅜㅜ",
          "original_form": "인간적으로 할인률 10프로 제한이 뭔데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100033297.1.20",
          "form": "손해는 작가님등이 보는걸",
          "original_form": "손해는 작가님등이 보는걸",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.21",
          "form": "이번에 앱마켓 갑질 방지가 나왔는데",
          "original_form": "이번에 앱마켓 갑질 방지가 나왔는데",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.22",
          "form": "그래서 지금 도정제로 혜택 본 인간이 누구죠..?",
          "original_form": "그래서 지금 도정제로 혜택 본 인간이 누구죠..?",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.23",
          "form": "대형 출판사..?",
          "original_form": "대형 출판사..?",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.24",
          "form": "카카오톡에 톡 화면에",
          "original_form": "카카오톡에 톡 화면에",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.25",
          "form": "광고 띄운다는거 봤어?",
          "original_form": "광고 띄운다는거 봤어?",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.26",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.27",
          "form": "동영상 광고",
          "original_form": "동영상 광고",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.28",
          "form": "아.. 그거 최선인가요",
          "original_form": "아.. 그거 최선인가요",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033297.1.29",
          "form": "채팅목록창에 띄운댓나",
          "original_form": "채팅목록창에 띄운댓나",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033297.1.30",
          "form": "어찌됐든 ㅠ",
          "original_form": "어찌됐든 ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033297.1.31",
          "form": "그러면 굳이 카톡쓸 이유가 없어지는걸",
          "original_form": "그러면 굳이 카톡쓸 이유가 없어지는걸",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033297.1.32",
          "form": "진짜 누구 말대로",
          "original_form": "진짜 누구 말대로",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033297.1.33",
          "form": "나중엔 카카오톡 pro 해서",
          "original_form": "나중엔 카카오톡 pro 해서",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033297.1.34",
          "form": "광고제거 2500/월",
          "original_form": "광고제거 2500/월",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033297.1.35",
          "form": "이런거 나오게 생겼엌ㅋㅋㅋㅋ",
          "original_form": "이런거 나오게 생겼엌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033297.1.36",
