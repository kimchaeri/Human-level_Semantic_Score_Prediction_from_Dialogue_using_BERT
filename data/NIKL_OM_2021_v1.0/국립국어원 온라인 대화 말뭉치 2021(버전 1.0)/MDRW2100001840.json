      "utterance": [
        {
          "id": "MDRW2100001840.1.1",
          "form": "여행가서 기념품 뭐 사와요?",
          "original_form": "여행가서 기념품 뭐 사와요?",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001840.1.2",
          "form": "전 각 나라별 마그넷 사요",
          "original_form": "전 각 나라별 마그넷 사요",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001840.1.3",
          "form": "싸고 모으는 재미가 있거든요",
          "original_form": "싸고 모으는 재미가 있거든요",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001840.1.4",
          "form": "저두 냉장고에 붙여놓았어요 몇개안되어서",
          "original_form": "저두 냉장고에 붙여놓았어요 몇개안되어서",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001840.1.5",
          "form": "맞아요 저도 냉장고에 있어요",
          "original_form": "맞아요 저도 냉장고에 있어요",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001840.1.6",
          "form": "많은 나라 간게 아니라서 그냥 장식용이에요",
          "original_form": "많은 나라 간게 아니라서 그냥 장식용이에요",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001840.1.7",
          "form": "난중에 현관문에 짝 붙이는걸로 하고 싶어요",
          "original_form": "난중에 현관문에 짝 붙이는걸로 하고 싶어요",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001840.1.8",
          "form": "오 그거 멋있겠다",
          "original_form": "오 그거 멋있겠다",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001840.1.9",
          "form": "현관문은 철문이니깐",
          "original_form": "현관문은 철문이니깐",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001840.1.10",
          "form": "그 지역마다 표식이 이쁘더라구요",
          "original_form": "그 지역마다 표식이 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001840.1.11",
          "form": "맞아요 스위스갔을때 못사온게 후회되요",
          "original_form": "맞아요 스위스갔을때 못사온게 후회되요",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001840.1.12",
          "form": "스위스 이쁘겠다",
          "original_form": "스위스 이쁘겠다",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001840.1.13",
          "form": "산 위로 갔어요?",
          "original_form": "산 위로 갔어요?",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001840.1.14",
          "form": "네 증기기관차 타고 올라갔어요",
          "original_form": "네 증기기관차 타고 올라갔어요",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001840.1.15",
          "form": "자연 풍경 멋있겠다",
          "original_form": "자연 풍경 멋있겠다",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001840.1.16",
          "form": "절경이겠구요",
          "original_form": "절경이겠구요",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001840.1.17",
          "form": "네 완전 풍경사진이 그림처럼 나오더라고요",
          "original_form": "네 완전 풍경사진이 그림처럼 나오더라고요",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001840.1.18",
          "form": "산 정상은 춥지 않아?",
          "original_form": "산 정상은 춥지 않아?",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001840.1.19",
          "form": "어떤 계절에 갔어요?",
          "original_form": "어떤 계절에 갔어요?",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001840.1.20",
          "form": "5월에 갔는데 추운지방은 추워서 핫팩 붙였어요",
          "original_form": "5월에 갔는데 추운지방은 추워서 핫팩 붙였어요",
          "speaker_id": "2",
          "time": "20210819 21:46"
        }
      ]
