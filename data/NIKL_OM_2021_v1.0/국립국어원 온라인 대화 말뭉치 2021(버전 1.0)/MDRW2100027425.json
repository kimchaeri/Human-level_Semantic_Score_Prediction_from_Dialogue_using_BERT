      "utterance": [
        {
          "id": "MDRW2100027425.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027425.1.2",
          "form": "필라테스 배우고싶어요",
          "original_form": "필라테스 배우고싶어요",
          "speaker_id": "2",
          "time": "20210902 12:58"
        },
        {
          "id": "MDRW2100027425.1.3",
          "form": "수영은 할줄알아요",
          "original_form": "수영은 할줄알아요",
          "speaker_id": "2",
          "time": "20210902 12:58"
        },
        {
          "id": "MDRW2100027425.1.4",
          "form": "요즘은 걷기운동만해요",
          "original_form": "요즘은 걷기운동만해요",
          "speaker_id": "2",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100027425.1.5",
          "form": "전 홈트요",
          "original_form": "전 홈트요",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027425.1.6",
          "form": "홈트면 영상보고 하시나요",
          "original_form": "홈트면 영상보고 하시나요",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027425.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027425.1.8",
          "form": "유투브 영상인가요? 집에선 운동 잘 안하게돼요",
          "original_form": "유투브 영상인가요? 집에선 운동 잘 안하게돼요",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027425.1.9",
          "form": "산책이 유일한 낙이에오",
          "original_form": "산책이 유일한 낙이에오",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027425.1.10",
          "form": "저녁에 선선해서 걷기좋아요",
          "original_form": "저녁에 선선해서 걷기좋아요",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027425.1.11",
          "form": "플랭크해요",
          "original_form": "플랭크해요",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027425.1.12",
          "form": "와. 저는 못하겠던데 뱃살이 들어간다면서요",
          "original_form": "와. 저는 못하겠던데 뱃살이 들어간다면서요",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027425.1.13",
          "form": "이소라 영상 따라 몇번 해본적은 있어요",
          "original_form": "이소라 영상 따라 몇번 해본적은 있어요",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027425.1.14",
          "form": "효과있어요?",
          "original_form": "효과있어요?",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027425.1.15",
          "form": "네 오래된 영상인데 서서 팔다리 운동해요",
          "original_form": "네 오래된 영상인데 서서 팔다리 운동해요",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027425.1.16",
          "form": "구령에 맞춰서요",
          "original_form": "구령에 맞춰서요",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027425.1.17",
          "form": "다리랑 팔운동이에요  다이어트",
          "original_form": "다리랑 팔운동이에요  다이어트",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027425.1.18",
          "form": "플랭크해요",
          "original_form": "플랭크해요",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027425.1.19",
          "form": "저는 플랭크하니 허리가 아픈거보면 코어힘이 약한가봐요",
          "original_form": "저는 플랭크하니 허리가 아픈거보면 코어힘이 약한가봐요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027425.1.20",
          "form": "코어힘 좋으신가요",
          "original_form": "코어힘 좋으신가요",
          "speaker_id": "2",
          "time": "20210902 13:29"
