      "utterance": [
        {
          "id": "MDRW2100037961.1.1",
          "form": "니오늘 끝낼꺼가?ㅋㅋㅋㅈ",
          "original_form": "니오늘 끝낼꺼가?ㅋㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037961.1.2",
          "form": "윽시달리노ㅋㅋ",
          "original_form": "윽시달리노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037961.1.3",
          "form": "오늘 많이 하고 내일 쉴라고요ㅋㅋㅋ",
          "original_form": "오늘 많이 하고 내일 쉴라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037961.1.4",
          "form": "영화 뭐 봤어요?",
          "original_form": "영화 뭐 봤어요?",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037961.1.5",
          "form": "기기괴괴",
          "original_form": "기기괴괴",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037961.1.6",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037961.1.7",
          "form": "잼있드라섬득하고",
          "original_form": "잼있드라섬득하고",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037961.1.8",
          "form": "공포스럽다ㅋ",
          "original_form": "공포스럽다ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037961.1.9",
          "form": "언니 나 이제 잘라 하니까",
          "original_form": "언니 나 이제 잘라 하니까",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100037961.1.10",
          "form": "왔어요ㅠ",
          "original_form": "왔어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100037961.1.11",
          "form": "잠들기 전에",
          "original_form": "잠들기 전에",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100037961.1.12",
          "form": "공포 영화 보면",
          "original_form": "공포 영화 보면",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037961.1.13",
          "form": "쉬 싸요",
          "original_form": "쉬 싸요",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037961.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037961.1.15",
          "form": "아고나",
          "original_form": "아고나",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037961.1.16",
          "form": "쏴리",
          "original_form": "쏴리",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037961.1.17",
          "form": "얼른자라",
          "original_form": "얼른자라",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037961.1.18",
          "form": "멜로보다 공포",
          "original_form": "멜로보다 공포",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037961.1.19",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037961.1.20",
          "form": "감각이 살아 있구만요ㅋㅋㅋ",
          "original_form": "감각이 살아 있구만요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100037961.1.21",
          "form": "맞아요 멜로는 죽었어요ㅋㅋ",
          "original_form": "맞아요 멜로는 죽었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100037961.1.22",
          "form": "어ㅋ 요즘은 스릴러",
          "original_form": "어ㅋ 요즘은 스릴러",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100037961.1.23",
          "form": "감정 매마름ㅋ",
          "original_form": "감정 매마름ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100037961.1.24",
          "form": "무료영화라서ㅋㅋ봤다",
          "original_form": "무료영화라서ㅋㅋ봤다",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100037961.1.25",
          "form": "무료가 큰일 했구만요 ㅋㅋㅋㅋ",
          "original_form": "무료가 큰일 했구만요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037961.1.26",
          "form": "언넝주무시라여 온니~",
          "original_form": "언넝주무시라여 온니~",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037961.1.27",
          "form": "그래ㅋ",
          "original_form": "그래ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037961.1.28",
          "form": "놓아주께ㅋㅋ",
          "original_form": "놓아주께ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037961.1.29",
          "form": "마지막 영화와함께ㅋ",
          "original_form": "마지막 영화와함께ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100037961.1.30",
          "form": "마지막영화는 뭔데요??",
          "original_form": "마지막영화는 뭔데요??",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100037961.1.31",
          "form": "마지막은 드라마",
          "original_form": "마지막은 드라마",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037961.1.32",
          "form": "고백부부정주행ㅋ",
          "original_form": "고백부부정주행ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037961.1.33",
          "form": "고백부부 잼있쥬~",
          "original_form": "고백부부 잼있쥬~",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037961.1.34",
          "form": "또본다",
          "original_form": "또본다",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037961.1.35",
          "form": "그거 정말 와닿음 ㅋㅋㅋㅋ",
          "original_form": "그거 정말 와닿음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037961.1.36",
          "form": "이번엔 쿠팡플레이",
          "original_form": "이번엔 쿠팡플레이",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037961.1.37",
          "form": "울신랑도 그거보고 울고 ㅋㅋㅋㅋ",
          "original_form": "울신랑도 그거보고 울고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:55"
        }
      ]
