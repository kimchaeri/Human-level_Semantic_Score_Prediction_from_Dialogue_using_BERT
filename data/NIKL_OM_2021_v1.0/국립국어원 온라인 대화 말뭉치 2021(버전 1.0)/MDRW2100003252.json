      "utterance": [
        {
          "id": "MDRW2100003252.1.1",
          "form": "최근에 구매하신거중에 맘에드는 제품 있으세요?",
          "original_form": "최근에 구매하신거중에 맘에드는 제품 있으세요?",
          "speaker_id": "1",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003252.1.2",
          "form": "저는 아직 도착하진 않았지만.. 갤럭시워치요ㅋㅋㅋ",
          "original_form": "저는 아직 도착하진 않았지만.. 갤럭시워치요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003252.1.3",
          "form": "실물로 있는건 반지!",
          "original_form": "실물로 있는건 반지!",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003252.1.4",
          "form": "우와 워치4 사셨어요?",
          "original_form": "우와 워치4 사셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003252.1.5",
          "form": "이번 광고봤는데 엄청 잘뽑은거같더라구요",
          "original_form": "이번 광고봤는데 엄청 잘뽑은거같더라구요",
          "speaker_id": "1",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003252.1.6",
          "form": "원래 스마트워치 쓰셨나요?",
          "original_form": "원래 스마트워치 쓰셨나요?",
          "speaker_id": "1",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003252.1.7",
          "form": "저 원랜 미밴드 썼었어요",
          "original_form": "저 원랜 미밴드 썼었어요",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003252.1.8",
          "form": "갑자기 꽂혀서 결제까지 가버렸어요ㅋㅋㅋ",
          "original_form": "갑자기 꽂혀서 결제까지 가버렸어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003252.1.9",
          "form": "오 저는 미밴드랑 비슷한 갤럭시핏 쓰고있어요",
          "original_form": "오 저는 미밴드랑 비슷한 갤럭시핏 쓰고있어요",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003252.1.10",
          "form": "핸드폰도 갤럭시이신가요?",
          "original_form": "핸드폰도 갤럭시이신가요?",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003252.1.11",
          "form": "네 저 노트10 쓰고있어요",
          "original_form": "네 저 노트10 쓰고있어요",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003252.1.12",
          "form": "갤럭시핏 좋나요?",
          "original_form": "갤럭시핏 좋나요?",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003252.1.13",
          "form": "음 저는 적당히 기능있어서 만족해요",
          "original_form": "음 저는 적당히 기능있어서 만족해요",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003252.1.14",
          "form": "가격대비 괜찮은 것 같아요",
          "original_form": "가격대비 괜찮은 것 같아요",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003252.1.15",
          "form": "간단한 답장기능도 있어서 편해요",
          "original_form": "간단한 답장기능도 있어서 편해요",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003252.1.16",
          "form": "우와 진짜요?",
          "original_form": "우와 진짜요?",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003252.1.17",
          "form": "넘 신기해요",
          "original_form": "넘 신기해요",
          "speaker_id": "2",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003252.1.18",
          "form": "미리 설정해둔 답만 할 수 있어요!",
          "original_form": "미리 설정해둔 답만 할 수 있어요!",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003252.1.19",
          "form": "왠지 워치사면 운동할 거 같아서 샀는데 과연..",
          "original_form": "왠지 워치사면 운동할 거 같아서 샀는데 과연..",
          "speaker_id": "2",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003252.1.20",
          "form": "리리님은 최근에 뭐 사셨어요?",
          "original_form": "리리님은 최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003252.1.21",
          "form": "저는 최근에 집게핀 샀어요",
          "original_form": "저는 최근에 집게핀 샀어요",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003252.1.22",
          "form": "요즘 하고다니는거 유행이라 사봤는데",
          "original_form": "요즘 하고다니는거 유행이라 사봤는데",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003252.1.23",
          "form": "머리에 자국이 덜남아서 좋더라구요",
          "original_form": "머리에 자국이 덜남아서 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003252.1.24",
          "form": "오 맞아요 저도 집게핀 사용해요!",
          "original_form": "오 맞아요 저도 집게핀 사용해요!",
          "speaker_id": "2",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003252.1.25",
          "form": "어떤 색 사셨어요?",
          "original_form": "어떤 색 사셨어요?",
          "speaker_id": "2",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003252.1.26",
          "form": "어떤색 쓰세요?",
          "original_form": "어떤색 쓰세요?",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003252.1.27",
          "form": "어 저는 무난하게 까만색 사용해요",
          "original_form": "어 저는 무난하게 까만색 사용해요",
          "speaker_id": "1",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003252.1.28",
          "form": "보냈었는데 안가졌네요",
          "original_form": "보냈었는데 안가졌네요",
          "speaker_id": "1",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003252.1.29",
          "form": "저는 초록색인데 그.. 크로우캐년? 그 물방울 튄 거 같은",
          "original_form": "저는 초록색인데 그.. 크로우캐년? 그 물방울 튄 거 같은",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003252.1.30",
          "form": "그런 느낌이에요",
          "original_form": "그런 느낌이에요",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003252.1.31",
          "form": "우와 예쁠 것 같아요",
          "original_form": "우와 예쁠 것 같아요",
          "speaker_id": "1",
          "time": "20210820 21:19"
        }
      ]
