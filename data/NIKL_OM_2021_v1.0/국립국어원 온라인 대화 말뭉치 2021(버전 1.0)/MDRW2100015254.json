      "utterance": [
        {
          "id": "MDRW2100015254.1.1",
          "form": "연애 해보셨어요?",
          "original_form": "연애 해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015254.1.2",
          "form": "해봤긴 했는데 했다고 하기 민망하네요.",
          "original_form": "해봤긴 했는데 했다고 하기 민망하네요.",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015254.1.3",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015254.1.4",
          "form": "안 해봤어요",
          "original_form": "안 해봤어요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015254.1.5",
          "form": "썰 풀어주세요",
          "original_form": "썰 풀어주세요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015254.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015254.1.7",
          "form": "흠 좋아요",
          "original_form": "흠 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.8",
          "form": "와 좋아요",
          "original_form": "와 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.9",
          "form": "제가 초등학교 5학년 때 처음으로 사귀어봤어요",
          "original_form": "제가 초등학교 5학년 때 처음으로 사귀어봤어요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.10",
          "form": "빼빼로데이 직전에 헤어졌지만요",
          "original_form": "빼빼로데이 직전에 헤어졌지만요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.11",
          "form": "그 때 한창 연애혁명이라는 웹툰짤이 유행이었거든요",
          "original_form": "그 때 한창 연애혁명이라는 웹툰짤이 유행이었거든요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.12",
          "form": "걔가 그걸로 고백했어요",
          "original_form": "걔가 그걸로 고백했어요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.13",
          "form": "웃기지 않나요",
          "original_form": "웃기지 않나요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.14",
          "form": "정말 순수했죠",
          "original_form": "정말 순수했죠",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015254.1.15",
          "form": "아 그거 말하시나요",
          "original_form": "아 그거 말하시나요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015254.1.16",
          "form": "4귀자",
          "original_form": "4귀자",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015254.1.17",
          "form": "님은 연애 할 생각 있으세요 ?",
          "original_form": "님은 연애 할 생각 있으세요 ?",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015254.1.18",
          "form": "무슨 그거요?",
          "original_form": "무슨 그거요?",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015254.1.19",
          "form": "음 사람이 있으면 해보고는 싶네요",
          "original_form": "음 사람이 있으면 해보고는 싶네요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015254.1.20",
          "form": "오 정말요 ?",
          "original_form": "오 정말요 ?",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015254.1.21",
          "form": "그럼 결혼은요?",
          "original_form": "그럼 결혼은요?",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015254.1.22",
          "form": "저는 사실 결혼은 안하고 싶어요",
          "original_form": "저는 사실 결혼은 안하고 싶어요",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015254.1.23",
          "form": "안해요",
          "original_form": "안해요",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015254.1.24",
          "form": "님은 왜요?",
          "original_form": "님은 왜요?",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015254.1.25",
          "form": "비혼주의자라서요",
          "original_form": "비혼주의자라서요",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015254.1.26",
          "form": "오 저도예요",
          "original_form": "오 저도예요",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015254.1.27",
          "form": "왜 비혼을 생각하시게 되었나요?",
          "original_form": "왜 비혼을 생각하시게 되었나요?",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015254.1.28",
          "form": "음 어렸을 때부터 그랬어요",
          "original_form": "음 어렸을 때부터 그랬어요",
          "speaker_id": "1",
          "time": "20210825 22:27"
        }
      ]
    }
  ]
