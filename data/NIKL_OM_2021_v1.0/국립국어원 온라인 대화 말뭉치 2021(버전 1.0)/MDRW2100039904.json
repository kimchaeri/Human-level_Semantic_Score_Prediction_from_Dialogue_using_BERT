      "utterance": [
        {
          "id": "MDRW2100039904.1.1",
          "form": "그러게 거의 조연아니면 단역이었네",
          "original_form": "그러게 거의 조연아니면 단역이었네",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039904.1.2",
          "form": "근데 갑자기 주연으로 가네요",
          "original_form": "근데 갑자기 주연으로 가네요",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039904.1.3",
          "form": "심지어 남주는 이준영 유키즈의",
          "original_form": "심지어 남주는 이준영 유키즈의",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039904.1.4",
          "form": "유키스",
          "original_form": "유키스",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039904.1.5",
          "form": "완전 신인들이 주인공이네요",
          "original_form": "완전 신인들이 주인공이네요",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039904.1.6",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100039904.1.7",
          "form": "요샌 배우빨 중요한데. 내용이 별로면 거의 안볼텐데",
          "original_form": "요샌 배우빨 중요한데. 내용이 별로면 거의 안볼텐데",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100039904.1.8",
          "form": "태권도를 알리기위해 태권도연맹 제작지원 ㅋㅋ",
          "original_form": "태권도를 알리기위해 태권도연맹 제작지원 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100039904.1.9",
          "form": "ㅋㅋ 근데 올림픽에서 태권도 보면 진짜 재미없어요",
          "original_form": "ㅋㅋ 근데 올림픽에서 태권도 보면 진짜 재미없어요",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039904.1.10",
          "form": "둘다 태권도 선수인가봐",
          "original_form": "둘다 태권도 선수인가봐",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039904.1.11",
          "form": "태권도 연습 많이 해야겠다",
          "original_form": "태권도 연습 많이 해야겠다",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039904.1.12",
          "form": "경기방식이 너무 바뀌어서 재미없더라",
          "original_form": "경기방식이 너무 바뀌어서 재미없더라",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039904.1.13",
          "form": "그래서 신인 배우들을 캐스팅 했을까요?",
          "original_form": "그래서 신인 배우들을 캐스팅 했을까요?",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039904.1.14",
          "form": "그런듯하다",
