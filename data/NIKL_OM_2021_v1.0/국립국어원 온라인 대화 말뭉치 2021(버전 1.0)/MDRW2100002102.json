      "utterance": [
        {
          "id": "MDRW2100002102.1.1",
          "form": "서울에 맛집 많아서 좋아용",
          "original_form": "서울에 맛집 많아서 좋아용",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002102.1.2",
          "form": "오 전 서울 사람이 아니라서",
          "original_form": "오 전 서울 사람이 아니라서",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002102.1.3",
          "form": "맛집 많은 서울이 너무 부럽네요ㅠ",
          "original_form": "맛집 많은 서울이 너무 부럽네요ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002102.1.4",
          "form": "ㅡ그래도 요즘은 갬성있는 음식점 많아져서 좋아요!",
          "original_form": "ㅡ그래도 요즘은 갬성있는 음식점 많아져서 좋아요!",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002102.1.5",
          "form": "맞아요! 특히 카페는 진짜",
          "original_form": "맞아요! 특히 카페는 진짜",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002102.1.6",
          "form": "인스타 감성 카페가 많은 거 같아요",
          "original_form": "인스타 감성 카페가 많은 거 같아요",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002102.1.7",
          "form": "맛보다 더 중요한 분위기ㅋㅋ",
          "original_form": "맛보다 더 중요한 분위기ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002102.1.8",
          "form": "완전요ㅋㅋㅋ분위기 좋으면 맛이 살짝 모자라도 용서되니까요",
          "original_form": "완전요ㅋㅋㅋ분위기 좋으면 맛이 살짝 모자라도 용서되니까요",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002102.1.9",
          "form": "맞아요 하지만 맛이 아예 없으면",
          "original_form": "맞아요 하지만 맛이 아예 없으면",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002102.1.10",
          "form": "사진만 많이 찍고 다신 안 갑니다",
          "original_form": "사진만 많이 찍고 다신 안 갑니다",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002102.1.11",
          "form": "용서불가ㅋㅋㅋㅋ",
          "original_form": "용서불가ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002102.1.12",
          "form": "ㅋㅋㅋㅋㅋ당연하죠 누가뭐래도 기본은 해야지!!",
          "original_form": "ㅋㅋㅋㅋㅋ당연하죠 누가뭐래도 기본은 해야지!!",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002102.1.13",
          "form": "ㅎㅎ 카페가면 주로 뭐 드세요?",
          "original_form": "ㅎㅎ 카페가면 주로 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002102.1.14",
          "form": "커피를 잘 못마셔서 라떼나 에이드종류 마셔요!!",
          "original_form": "커피를 잘 못마셔서 라떼나 에이드종류 마셔요!!",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002102.1.15",
          "form": "오 저두요 달달한 커피 아니면",
          "original_form": "오 저두요 달달한 커피 아니면",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002102.1.16",
          "form": "주로 티 종류..",
          "original_form": "주로 티 종류..",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002102.1.17",
          "form": "주로 디저트를 먹으러 가죠ㅎㅎ",
          "original_form": "주로 디저트를 먹으러 가죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002102.1.18",
          "form": "ㅋㅋ맞아요 요즘 케이크가 왜그리 이쁘고 맛도 다양한지",
          "original_form": "ㅋㅋ맞아요 요즘 케이크가 왜그리 이쁘고 맛도 다양한지",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002102.1.19",
          "form": "보는 재미 먹는 재미",
          "original_form": "보는 재미 먹는 재미",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002102.1.20",
          "form": "하지만 그만큼 가격도..ㅎㅎ",
          "original_form": "하지만 그만큼 가격도..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002102.1.21",
