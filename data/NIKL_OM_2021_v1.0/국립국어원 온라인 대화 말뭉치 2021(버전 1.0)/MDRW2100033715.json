      "utterance": [
        {
          "id": "MDRW2100033715.1.1",
          "form": "name1님은 저녁에 뭐 드셨어요?",
          "original_form": "&name1&님은 저녁에 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033715.1.2",
          "form": "전 삼겹살이요~ 오랜만에 먹으니 맛있더라구요",
          "original_form": "전 삼겹살이요~ 오랜만에 먹으니 맛있더라구요",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033715.1.3",
          "form": "와~맛있으셨겠어요!!!",
          "original_form": "와~맛있으셨겠어요!!!",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033715.1.4",
          "form": "네ㅋㅋ근데 반전이 있었어요",
          "original_form": "네ㅋㅋ근데 반전이 있었어요",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033715.1.5",
          "form": "무슨 반전이요???",
          "original_form": "무슨 반전이요???",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033715.1.6",
          "form": "고기를 시어른분들이 사오셨거든요",
          "original_form": "고기를 시어른분들이 사오셨거든요",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033715.1.7",
          "form": "집에들릴일이 있으셔서",
          "original_form": "집에들릴일이 있으셔서",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033715.1.8",
          "form": "아~네!! 굿굿이네요",
          "original_form": "아~네!! 굿굿이네요",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.9",
          "form": "사오셨는데ㅋㅋㅋ냉장고 넣고 구우려고보니",
          "original_form": "사오셨는데ㅋㅋㅋ냉장고 넣고 구우려고보니",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.11",
          "form": "바베큐용이었어요",
          "original_form": "바베큐용이었어요",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.12",
          "form": "세상에3줄 들었는데",
          "original_form": "세상에3줄 들었는데",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.13",
          "form": "2근이더라구요ㅠㅠ",
          "original_form": "2근이더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.14",
          "form": "두께가",
          "original_form": "두께가",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.15",
          "form": "어마어마",
          "original_form": "어마어마",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.16",
          "form": "오~~~대박이네요!",
          "original_form": "오~~~대박이네요!",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033715.1.17",
          "form": "집에서 캠핑용 고기를",
          "original_form": "집에서 캠핑용 고기를",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033715.1.18",
          "form": "마주하니 난감 했어요",
          "original_form": "마주하니 난감 했어요",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033715.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033715.1.20",
          "form": "어떻게 구우셨어요??",
          "original_form": "어떻게 구우셨어요??",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033715.1.21",
          "form": "얇게 썰어서 구웠어요ㅋㅋㅋㅋ",
          "original_form": "얇게 썰어서 구웠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033715.1.22",
          "form": "아~써는것도 일이셨겠어요 ㅎㅎ",
          "original_form": "아~써는것도 일이셨겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033715.1.23",
          "form": "신랑이 구웠는데 넘굴게썰어서 첨에 볶고 있더라구요 ㅠ",
          "original_form": "신랑이 구웠는데 넘굴게썰어서 첨에 볶고 있더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033715.1.24",
          "form": "3줄에 2근짜리라 얇게 썰어도 두꺼웠을것 같아요~ㅎ",
          "original_form": "3줄에 2근짜리라 얇게 썰어도 두꺼웠을것 같아요~ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:48"
        }
      ]
