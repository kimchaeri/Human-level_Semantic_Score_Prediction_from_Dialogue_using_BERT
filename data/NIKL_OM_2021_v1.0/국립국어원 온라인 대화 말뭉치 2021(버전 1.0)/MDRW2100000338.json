          "speaker_id": "1",
          "time": "20210819 10:56"
        },
        {
          "id": "MDRW2100000338.1.8",
          "form": "오.. 전 푸들이요 ㅋ ㅋ",
          "original_form": "오.. 전 푸들이요 ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210819 11:09"
        },
        {
          "id": "MDRW2100000338.1.9",
          "form": "믹스요?",
          "original_form": "믹스요?",
          "speaker_id": "1",
          "time": "20210819 11:09"
        },
        {
          "id": "MDRW2100000338.1.10",
          "form": "아니요 ㅋ ㅋ 찐이요!",
          "original_form": "아니요 ㅋ ㅋ 찐이요!",
          "speaker_id": "2",
          "time": "20210819 11:09"
        },
        {
          "id": "MDRW2100000338.1.11",
          "form": "귀엽겠네여ㅋㅋㅋ",
          "original_form": "귀엽겠네여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 11:09"
        },
        {
