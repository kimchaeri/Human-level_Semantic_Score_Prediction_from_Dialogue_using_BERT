        {
          "id": "MDRW2100000338.1.1",
          "form": "강아지 키우실것같아요",
          "original_form": "강아지 키우실것같아요",
          "speaker_id": "2",
          "time": "20210819 09:28"
        },
        {
          "id": "MDRW2100000338.1.2",
          "form": "ㅋㅋㅋ어떻게 아셨어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ어떻게 아셨어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 10:54"
        },
        {
          "id": "MDRW2100000338.1.3",
          "form": "뭘요ㅋㅋ",
          "original_form": "뭘요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 10:55"
        },
        {
          "id": "MDRW2100000338.1.4",
          "form": "넵 강아지키웁니다",
          "original_form": "넵 강아지키웁니다",
          "speaker_id": "1",
          "time": "20210819 10:55"
        },
        {
          "id": "MDRW2100000338.1.5",
          "form": "아니에여",
          "original_form": "아니에여",
          "speaker_id": "2",
          "time": "20210819 10:56"
        },
        {
          "id": "MDRW2100000338.1.6",
          "form": "무슨종 키우시나요",
          "original_form": "무슨종 키우시나요",
          "speaker_id": "2",
          "time": "20210819 10:56"
        },
        {
          "id": "MDRW2100000338.1.7",
          "form": "믹스인데 스피츠믹스요ㅋㅋ",
