      "utterance": [
        {
          "id": "MDRW2100028140.1.1",
          "form": "니 생일때 고기 먹으려고 하는데",
          "original_form": "니 생일때 고기 먹으려고 하는데",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028140.1.2",
          "form": "오 고기 좋지ㅋㅋ 올만에",
          "original_form": "오 고기 좋지ㅋㅋ 올만에",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028140.1.3",
          "form": "어디 먹을 데 있나 봐봐",
          "original_form": "어디 먹을 데 있나 봐봐",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028140.1.4",
          "form": "집 근처에 고기집 가봤던데 가면 되지",
          "original_form": "집 근처에 고기집 가봤던데 가면 되지",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028140.1.5",
          "form": "아 나 초계국수도 먹고 싶은데 그거 있는데 찾아볼까",
          "original_form": "아 나 초계국수도 먹고 싶은데 그거 있는데 찾아볼까",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028140.1.6",
          "form": "아이고 먹고 싶은 것도 많다",
          "original_form": "아이고 먹고 싶은 것도 많다",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028140.1.7",
          "form": "너 먹고 싶은데 맛있는 데 찾아봐",
          "original_form": "너 먹고 싶은데 맛있는 데 찾아봐",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028140.1.8",
          "form": "아 근데 하도 까다로워서 걱정이다",
          "original_form": "아 근데 하도 까다로워서 걱정이다",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028140.1.9",
          "form": "다들 말이 겁나 많지",
          "original_form": "다들 말이 겁나 많지",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028140.1.10",
          "form": "엄마도 말 많거든?",
          "original_form": "엄마도 말 많거든?",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028140.1.11",
          "form": "난 아니다",
          "original_form": "난 아니다",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028140.1.12",
          "form": "대패 삽겹먹을까?",
          "original_form": "대패 삽겹먹을까?",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028140.1.13",
          "form": "저기 국가대패 갔을때 다들 잘 먹던데",
          "original_form": "저기 국가대패 갔을때 다들 잘 먹던데",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028140.1.14",
          "form": "거기도 괜찮았지",
          "original_form": "거기도 괜찮았지",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028140.1.15",
          "form": "근데 거기 대패만 하지 않냐?",
          "original_form": "근데 거기 대패만 하지 않냐?",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028140.1.16",
          "form": "대패로 되겠어?",
          "original_form": "대패로 되겠어?",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028140.1.17",
          "form": "목살이 좋긴한데 나능",
          "original_form": "목살이 좋긴한데 나능",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028140.1.18",
          "form": "거기 초계냉면있음",
          "original_form": "거기 초계냉면있음",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028140.1.19",
          "form": "어이구 벌써 다 조사해놨냐",
          "original_form": "어이구 벌써 다 조사해놨냐",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028140.1.20",
          "form": "엄마 이번에 산 메론",
          "original_form": "엄마 이번에 산 메론",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028140.1.21",
          "form": "짱 맛있더라",
          "original_form": "짱 맛있더라",
          "speaker_id": "1",
          "time": "20210902 22:17"
        }
