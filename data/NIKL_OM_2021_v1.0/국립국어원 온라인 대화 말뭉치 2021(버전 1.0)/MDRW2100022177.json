      "utterance": [
        {
          "id": "MDRW2100022177.1.1",
          "form": "애기는 뭘 하고 있나요",
          "original_form": "애기는 뭘 하고 있나요",
          "speaker_id": "1",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022177.1.2",
          "form": "잠을 자고 있나요 놀고 있나요",
          "original_form": "잠을 자고 있나요 놀고 있나요",
          "speaker_id": "1",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022177.1.3",
          "form": "지금 잠 자고있어요",
          "original_form": "지금 잠 자고있어요",
          "speaker_id": "2",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022177.1.4",
          "form": "방금까지 놀다가 재웠습니다",
          "original_form": "방금까지 놀다가 재웠습니다",
          "speaker_id": "2",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022177.1.5",
          "form": "15개월이라 아직 재워줘야합니다",
          "original_form": "15개월이라 아직 재워줘야합니다",
          "speaker_id": "2",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022177.1.6",
          "form": "가족이 있나요?",
          "original_form": "가족이 있나요?",
          "speaker_id": "2",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022177.1.7",
          "form": "아육아에 참여를 많이 하시는군요",
          "original_form": "아육아에 참여를 많이 하시는군요",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022177.1.8",
          "form": "네 인생 한번인데 이시기에 사랑하는 아들과 함께하고 싶네요",
          "original_form": "네 인생 한번인데 이시기에 사랑하는 아들과 함께하고 싶네요",
          "speaker_id": "2",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022177.1.9",
          "form": "맞아요 맞습니다",
          "original_form": "맞아요 맞습니다",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022177.1.10",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022177.1.11",
          "form": "그시기는 지나가면 오지 않지요",
          "original_form": "그시기는 지나가면 오지 않지요",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022177.1.12",
          "form": "내 애가 둘 입니다",
          "original_form": "내 애가 둘 입니다",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.13",
          "form": "아 맞는 말씀입니다",
          "original_form": "아 맞는 말씀입니다",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.14",
          "form": "아 몇살인가요",
          "original_form": "아 몇살인가요",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.15",
          "form": "개인 정보는 발설할 수 없습니다",
          "original_form": "개인 정보는 발설할 수 없습니다",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.16",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.17",
          "form": "내 네 아버님",
          "original_form": "내 네 아버님",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.18",
          "form": "애기 둘 키우면서 가장",
          "original_form": "애기 둘 키우면서 가장",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.19",
          "form": "행복했을때가 언제 인가요?",
          "original_form": "행복했을때가 언제 인가요?",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022177.1.20",
          "form": "신생아 때가 아니었나 싶습니다",
          "original_form": "신생아 때가 아니었나 싶습니다",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022177.1.21",
          "form": "클수록 돈을 많이 요구합니다",
          "original_form": "클수록 돈을 많이 요구합니다",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022177.1.22",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022177.1.23",
          "form": "해 저도 티가 나지 않습니다",
          "original_form": "해 저도 티가 나지 않습니다",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022177.1.24",
          "form": "해줘도 ㅋㅋ",
          "original_form": "해줘도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022177.1.25",
          "form": "슬프지만 어쩝니까",
          "original_form": "슬프지만 어쩝니까",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022177.1.26",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210829 10:34"
        }
      ]
    }
