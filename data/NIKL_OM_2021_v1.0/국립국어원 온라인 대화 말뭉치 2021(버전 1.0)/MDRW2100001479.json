        },
        {
          "id": "MDRW2100001479.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001479.1.3",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001479.1.4",
          "form": "반려동물 키우고 계신가요?",
          "original_form": "반려동물 키우고 계신가요?",
          "speaker_id": "2",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001479.1.5",
          "form": "아니오",
          "original_form": "아니오",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001479.1.6",
          "form": "엇 사실 저도 안키워요",
          "original_form": "엇 사실 저도 안키워요",
          "speaker_id": "2",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001479.1.7",
          "form": "저는 안키우는데",
          "original_form": "저는 안키우는데",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001479.1.8",
          "form": "키우고싶네요",
          "original_form": "키우고싶네요",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001479.1.9",
          "form": "ㅎㅎ 저두요..",
          "original_form": "ㅎㅎ 저두요..",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001479.1.10",
          "form": "길거리에",
          "original_form": "길거리에",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001479.1.11",
          "form": "강아지들과 산책하는 분들을",
          "original_form": "강아지들과 산책하는 분들을",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001479.1.12",
          "form": "부러워할뿐..",
          "original_form": "부러워할뿐..",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001479.1.13",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001479.1.14",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001479.1.15",
          "form": "강아지vs고양이",
          "original_form": "강아지vs고양이",
          "speaker_id": "1",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001479.1.16",
          "form": "아 저는",
          "original_form": "아 저는",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001479.1.17",
          "form": "강경 댕댕이파입니다.",
          "original_form": "강경 댕댕이파입니다.",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001479.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001479.1.19",
          "form": "저한테 막 들이댔으면 좋겠어요",
          "original_form": "저한테 막 들이댔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001479.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001479.1.21",
          "form": "저도 댕댕이  파엥ㅅ",
          "original_form": "저도 댕댕이  파엥ㅅ",
          "speaker_id": "1",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001479.1.22",
          "form": "에요",
          "original_form": "에요",
          "speaker_id": "1",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001479.1.23",
          "form": "호오 반갑네요",
          "original_form": "호오 반갑네요",
          "speaker_id": "2",
          "time": "20211016 00:54"
        },
        {
          "id": "MDRW2100001479.1.24",
          "form": "요즘은",
          "original_form": "요즘은",
          "speaker_id": "2",
          "time": "20211016 00:54"
        },
        {
          "id": "MDRW2100001479.1.25",
          "form": "냥집사들 세력이 커가고있는데..",
          "original_form": "냥집사들 세력이 커가고있는데..",
          "speaker_id": "2",
          "time": "20211016 00:54"
        },
        {
          "id": "MDRW2100001479.1.26",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:54"
        },
        {
          "id": "MDRW2100001479.1.27",
          "form": "물론 고양이들도 귀엽지만..ㅎ",
          "original_form": "물론 고양이들도 귀엽지만..ㅎ",
          "speaker_id": "2",
          "time": "20211016 00:54"
        },
        {
          "id": "MDRW2100001479.1.28",
          "form": "고양이도 귀엽지만",
          "original_form": "고양이도 귀엽지만",
          "speaker_id": "1",
          "time": "20211016 00:55"
        },
        {
          "id": "MDRW2100001479.1.29",
          "form": "말잘듣고 사람좋아하는",
          "original_form": "말잘듣고 사람좋아하는",
          "speaker_id": "1",
          "time": "20211016 00:55"
        },
        {
          "id": "MDRW2100001479.1.30",
          "form": "강아지가 더 좋아요",
          "original_form": "강아지가 더 좋아요",
          "speaker_id": "1",
          "time": "20211016 00:55"
        },
        {
          "id": "MDRW2100001479.1.31",
          "form": "개인적으로 ㅎ",
          "original_form": "개인적으로 ㅎ",
          "speaker_id": "1",
          "time": "20211016 00:55"
        },
        {
          "id": "MDRW2100001479.1.32",
          "form": "ㅋㅋㅋㅋ저두요 ㅠㅠ 강아지들은 천사야...",
          "original_form": "ㅋㅋㅋㅋ저두요 ㅠㅠ 강아지들은 천사야...",
