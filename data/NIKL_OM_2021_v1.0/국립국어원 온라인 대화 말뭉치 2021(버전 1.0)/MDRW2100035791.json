      "utterance": [
        {
          "id": "MDRW2100035791.1.1",
          "form": "안녕하세용 재밌게보신 드라마 있으신가용?",
          "original_form": "안녕하세용 재밌게보신 드라마 있으신가용?",
          "speaker_id": "1",
          "time": "20210830 02:06"
        },
        {
          "id": "MDRW2100035791.1.2",
          "form": "음 최근에는 펜트하우스 보고있어요",
          "original_form": "음 최근에는 펜트하우스 보고있어요",
          "speaker_id": "2",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100035791.1.3",
          "form": "아하 그거 재밌다고하던데",
          "original_form": "아하 그거 재밌다고하던데",
          "speaker_id": "1",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100035791.1.4",
          "form": "시즌 원부터 안봐서 못따라갈까봐",
          "original_form": "시즌 원부터 안봐서 못따라갈까봐",
          "speaker_id": "1",
          "time": "20210830 02:08"
        },
        {
          "id": "MDRW2100035791.1.5",
          "form": "시도를 못하고있어용 ㅎㅎ",
          "original_form": "시도를 못하고있어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100035791.1.6",
          "form": "앗 그러시군요ㅎㅎ",
          "original_form": "앗 그러시군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100035791.1.7",
          "form": "저도 투 중간부터 보기 시작했어요ㅎㅎ",
          "original_form": "저도 투 중간부터 보기 시작했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100035791.1.8",
          "form": "전개가 마라맛이라고 하더라구요ㅋㅋㅋ",
          "original_form": "전개가 마라맛이라고 하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100035791.1.9",
          "form": "원~투 초반은 하이라이트나 모아보기로 잠깐씩 보았구요",
          "original_form": "원~투 초반은 하이라이트나 모아보기로 잠깐씩 보았구요",
          "speaker_id": "2",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100035791.1.10",
          "form": "시즌 4도 나올까용?",
          "original_form": "시즌 4도 나올까용?",
          "speaker_id": "1",
          "time": "20210830 02:09"
        },
        {
          "id": "MDRW2100035791.1.11",
          "form": "시즌 3으로 끝인걸로 알고 있는데 정확히는 모르겠네요..",
          "original_form": "시즌 3으로 끝인걸로 알고 있는데 정확히는 모르겠네요..",
          "speaker_id": "2",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100035791.1.12",
          "form": "지금 보시는게 시즌3 맞죵?",
          "original_form": "지금 보시는게 시즌3 맞죵?",
          "speaker_id": "1",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100035791.1.13",
          "form": "아항ㅠㅠ",
          "original_form": "아항ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100035791.1.14",
          "form": "저는 방금 대탈출 봤어요ㅎㅎ",
          "original_form": "저는 방금 대탈출 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100035791.1.15",
          "form": "저는 아직 대탈출 본적이없어서..\n대탈출 재미있나요?",
          "original_form": "저는 아직 대탈출 본적이없어서..\n대탈출 재미있나요?",
          "speaker_id": "2",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100035791.1.16",
          "form": "네네! 저번주랑 이번주랑 이어지는 회차인데",
          "original_form": "네네! 저번주랑 이번주랑 이어지는 회차인데",
          "speaker_id": "1",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100035791.1.17",
          "form": "이건 전체적으로 이어지는 스토리가 따로 있는건 아니라서",
          "original_form": "이건 전체적으로 이어지는 스토리가 따로 있는건 아니라서",
          "speaker_id": "1",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100035791.1.18",
          "form": "추천드려용!",
          "original_form": "추천드려용!",
          "speaker_id": "1",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100035791.1.19",
          "form": "아 이어지는 스토리는 아니군요!",
          "original_form": "아 이어지는 스토리는 아니군요!",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100035791.1.20",
          "form": "다음에 시간있을때 봐야겠네용",
          "original_form": "다음에 시간있을때 봐야겠네용",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100035791.1.21",
          "form": "넹넹 방탈출 게임에서 착안한 예능이라 재밌게 보실거에용!",
          "original_form": "넹넹 방탈출 게임에서 착안한 예능이라 재밌게 보실거에용!",
          "speaker_id": "1",
          "time": "20210830 02:16"
        }
      ]
    }
