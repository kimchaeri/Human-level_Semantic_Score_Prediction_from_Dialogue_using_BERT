      "utterance": [
        {
          "id": "MDRW2100007497.1.1",
          "form": "직장 다니시고 계신가요?",
          "original_form": "직장 다니시고 계신가요?",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007497.1.2",
          "form": "네~ name1님은요?",
          "original_form": "네~ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007497.1.3",
          "form": "저는 학생이라서 아직이에요ㅎㅎ",
          "original_form": "저는 학생이라서 아직이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007497.1.4",
          "form": "코로나 때문에 재택근무하시나요?",
          "original_form": "코로나 때문에 재택근무하시나요?",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007497.1.5",
          "form": "아니요ㅎㅎ 재택못하는 직종이라서",
          "original_form": "아니요ㅎㅎ 재택못하는 직종이라서",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007497.1.6",
          "form": "학생 부럽습니다",
          "original_form": "학생 부럽습니다",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007497.1.7",
          "form": "ㅋㅋㅋㅋㅋ저도 곧 일하게 될텐데요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ저도 곧 일하게 될텐데요ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007497.1.8",
          "form": "전 집순이라 재택근무 일자리도 솔깃한 것같아요",
          "original_form": "전 집순이라 재택근무 일자리도 솔깃한 것같아요",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007497.1.9",
          "form": "재택근무 일자리는 뭐가 있을까요..",
          "original_form": "재택근무 일자리는 뭐가 있을까요..",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007497.1.10",
          "form": "음 대면업무가 아니여야죠?ㅎㅎ",
          "original_form": "음 대면업무가 아니여야죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007497.1.11",
          "form": "아 그렇죠ㅎㅎ 제가 바로 떠오르는건",
          "original_form": "아 그렇죠ㅎㅎ 제가 바로 떠오르는건",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007497.1.12",
          "form": "작가인 것같아요",
          "original_form": "작가인 것같아요",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007497.1.13",
          "form": "그쵸ㅎㅎ 국문과이신가보네요",
          "original_form": "그쵸ㅎㅎ 국문과이신가보네요",
          "speaker_id": "2",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007497.1.14",
          "form": "앗 전 국문과는 아닌데",
          "original_form": "앗 전 국문과는 아닌데",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007497.1.15",
          "form": "뭔가 만화작가가 떠올라서요",
          "original_form": "뭔가 만화작가가 떠올라서요",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007497.1.16",
          "form": "방금 웹툰을 보고 있었거든요ㅋㅋㅋㅋ",
          "original_form": "방금 웹툰을 보고 있었거든요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007497.1.17",
          "form": "아ㅎㅎ 그렇군요~~",
          "original_form": "아ㅎㅎ 그렇군요~~",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007497.1.18",
          "form": "재택은 음",
          "original_form": "재택은 음",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007497.1.19",
          "form": "무역쪽도 가능할듯해요",
          "original_form": "무역쪽도 가능할듯해요",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007497.1.20",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007497.1.21",
          "form": "일반 업무를 보면 가능할까요",
          "original_form": "일반 업무를 보면 가능할까요",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007497.1.22",
          "form": "네 서류 업무만 하면!!",
          "original_form": "네 서류 업무만 하면!!",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007497.1.23",
          "form": "오오 제가 미생같은 드라마를 많이 봐서 그런지",
          "original_form": "오오 제가 미생같은 드라마를 많이 봐서 그런지",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007497.1.24",
          "form": "무역회사에서 애초에 재택근무한다는걸 생각해본 적이 없는 것같아요",
          "original_form": "무역회사에서 애초에 재택근무한다는걸 생각해본 적이 없는 것같아요",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007497.1.25",
          "form": "무역회사에도 페이퍼워크 있어요ㅎㅎ",
          "original_form": "무역회사에도 페이퍼워크 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007497.1.26",
          "form": "앗 그렇구나 페이퍼워크가 뭔가요??",
          "original_form": "앗 그렇구나 페이퍼워크가 뭔가요??",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007497.1.27",
          "form": "서류 업무요!",
          "original_form": "서류 업무요!",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007497.1.28",
          "form": "재택 가능한 ㅎㅎ",
          "original_form": "재택 가능한 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007497.1.29",
          "form": "단순히 이런 용어만 써도 멋져보이는....ㅋㅋㅋㅋㅋㅋ",
          "original_form": "단순히 이런 용어만 써도 멋져보이는....ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007497.1.30",
          "form": "재택근무든 뭐든 일을 가졌으면 좋겠어요",
          "original_form": "재택근무든 뭐든 일을 가졌으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007497.1.31",
          "form": "ㅎㅎ 아직 어리셔서 그런걸거에요",
          "original_form": "ㅎㅎ 아직 어리셔서 그런걸거에요",
          "speaker_id": "2",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007497.1.32",
          "form": "때되면 다 멋진 어른이 될겁니다",
          "original_form": "때되면 다 멋진 어른이 될겁니다",
          "speaker_id": "2",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007497.1.33",
          "form": "우왕 그랬으면 좋겠어요",
          "original_form": "우왕 그랬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 02:55"
        }
      ]
