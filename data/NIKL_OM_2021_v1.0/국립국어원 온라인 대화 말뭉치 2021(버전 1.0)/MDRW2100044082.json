      "utterance": [
        {
          "id": "MDRW2100044082.1.1",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044082.1.2",
          "form": "잘은 못 먹어요. 먹고나면 속 아파서",
          "original_form": "잘은 못 먹어요. 먹고나면 속 아파서",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044082.1.3",
          "form": "저도 매운거 잘못먹는데 자꾸 땡겨서ㅋㅋ",
          "original_form": "저도 매운거 잘못먹는데 자꾸 땡겨서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044082.1.4",
          "form": "ㅋㅋ 안 먹어버릇 하니까 점점 더 못 먹게 되더라구요",
          "original_form": "ㅋㅋ 안 먹어버릇 하니까 점점 더 못 먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044082.1.5",
          "form": "전 그래도 부들부들떨면서 먹어요 중독ㅋㅋㅋ",
          "original_form": "전 그래도 부들부들떨면서 먹어요 중독ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044082.1.6",
          "form": "ㅋㅋ 위장이 튼튼하시군요",
          "original_form": "ㅋㅋ 위장이 튼튼하시군요",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044082.1.7",
          "form": "참고 먹는거죠ㅋㅋㅋ\r\n라면은 그럼 어떤거 드세요?",
          "original_form": "참고 먹는거죠ㅋㅋㅋ\r\n라면은 그럼 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044082.1.8",
          "form": "라면 잘 안 먹기는 하는데 그냥 진라면이나 스낵면 먹어요",
          "original_form": "라면 잘 안 먹기는 하는데 그냥 진라면이나 스낵면 먹어요",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044082.1.9",
          "form": "저도 진라면이요\r\n비빔면에 고기먹는것도 좋아해요ㅋ",
          "original_form": "저도 진라면이요\r\n비빔면에 고기먹는것도 좋아해요ㅋ",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044082.1.10",
          "form": "배홍동 비빔면 처음 먹어봤는데 괜찮더라구요",
          "original_form": "배홍동 비빔면 처음 먹어봤는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044082.1.11",
          "form": "그건안먹어봣는데 먹어봐야겟네요",
          "original_form": "그건안먹어봣는데 먹어봐야겟네요",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044082.1.12",
          "form": "매콤하니 맛있었어요",
          "original_form": "매콤하니 맛있었어요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044082.1.13",
          "form": "광고하는거 보니 그럴거 같아여",
          "original_form": "광고하는거 보니 그럴거 같아여",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044082.1.14",
          "form": "신라면비빔면은 좀 맵더라고요",
          "original_form": "신라면비빔면은 좀 맵더라고요",
