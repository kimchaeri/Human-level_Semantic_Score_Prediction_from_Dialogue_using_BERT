      "utterance": [
        {
          "id": "MDRW2100037444.1.1",
          "form": "나는 순두부찌개 먹음 넌",
          "original_form": "나는 순두부찌개 먹음 넌",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100037444.1.2",
          "form": "난 삼겹살 배달시켜 먹었음",
          "original_form": "난 삼겹살 배달시켜 먹었음",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100037444.1.3",
          "form": "고기도 배달해서 먹어",
          "original_form": "고기도 배달해서 먹어",
          "speaker_id": "2",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037444.1.4",
          "form": "응 편해",
          "original_form": "응 편해",
          "speaker_id": "1",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037444.1.5",
          "form": "뒷처리가 없으니",
          "original_form": "뒷처리가 없으니",
          "speaker_id": "2",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037444.1.6",
          "form": "분리수거는 있는데 기름이나 냄새등이 없으니",
          "original_form": "분리수거는 있는데 기름이나 냄새등이 없으니",
          "speaker_id": "1",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037444.1.7",
          "form": "그래 맛은 있었어",
          "original_form": "그래 맛은 있었어",
          "speaker_id": "2",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037444.1.8",
          "form": "첨에 먹었을땐 괜찮앗는데 오늘은",
          "original_form": "첨에 먹었을땐 괜찮앗는데 오늘은",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037444.1.9",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037444.1.10",
          "form": "조금 고기누린내가 날려고 했어",
          "original_form": "조금 고기누린내가 날려고 했어",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037444.1.11",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037444.1.12",
          "form": "그래서 열심히 쌈으로 싸서 먹음",
          "original_form": "그래서 열심히 쌈으로 싸서 먹음",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037444.1.13",
          "form": "다음에는 다른집으로 시켜봐",
          "original_form": "다음에는 다른집으로 시켜봐",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037444.1.14",
          "form": "언니 낼 올때 베스킨에 들려서 파인트 포장 좀 해와",
          "original_form": "언니 낼 올때 베스킨에 들려서 파인트 포장 좀 해와",
