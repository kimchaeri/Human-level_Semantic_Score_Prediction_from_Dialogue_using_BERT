        },
        {
          "id": "MDRW2100031006.1.26",
          "form": "맞아요~~",
          "original_form": "맞아요~~",
          "speaker_id": "1",
          "time": "20210905 00:42"
        },
        {
          "id": "MDRW2100031006.1.27",
          "form": "저는 일대일만남이 좋긴하더라구요",
          "original_form": "저는 일대일만남이 좋긴하더라구요",
