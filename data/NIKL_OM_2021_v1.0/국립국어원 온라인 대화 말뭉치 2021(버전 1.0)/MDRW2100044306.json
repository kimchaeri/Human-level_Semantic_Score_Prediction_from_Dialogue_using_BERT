      "utterance": [
        {
          "id": "MDRW2100044306.1.1",
          "form": "최근에 영화 보셧너요?",
          "original_form": "최근에 영화 보셧너요?",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044306.1.2",
          "form": "아뇨 최근엔 영화 본 게 없어요",
          "original_form": "아뇨 최근엔 영화 본 게 없어요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044306.1.3",
          "form": "영화 보셨나요?",
          "original_form": "영화 보셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044306.1.4",
          "form": "영화는 못봣구 ㅠㅠ 모가디슈랑 블랙위도우 보고싶어요 ㅠㅠ",
          "original_form": "영화는 못봣구 ㅠㅠ 모가디슈랑 블랙위도우 보고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044306.1.5",
          "form": "아 저두 모가디슈 보고 싶어요",
          "original_form": "아 저두 모가디슈 보고 싶어요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044306.1.6",
          "form": "영화관 자주 가세요?",
          "original_form": "영화관 자주 가세요?",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044306.1.7",
          "form": "코로나 전애는 자주 갔는데 코로나 이후로는 진짜 드물게 가본 것 같아요",
          "original_form": "코로나 전애는 자주 갔는데 코로나 이후로는 진짜 드물게 가본 것 같아요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044306.1.8",
          "form": "저도요 ㅠㅠ 가격도 너무 올라서 ㅠㅠ",
          "original_form": "저도요 ㅠㅠ 가격도 너무 올라서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044306.1.9",
          "form": "모가디슈는 집에서 나오면 봐야겟어요 ㅜ",
          "original_form": "모가디슈는 집에서 나오면 봐야겟어요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044306.1.10",
          "form": "그러게 말이에요  ㅜ",
          "original_form": "그러게 말이에요  ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044306.1.11",
          "form": "저번에 빵원쿠폰 이벤트하길래 참가해봤는데",
          "original_form": "저번에 빵원쿠폰 이벤트하길래 참가해봤는데",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044306.1.12",
          "form": "30초도 안돼서 끝나더라구요",
          "original_form": "30초도 안돼서 끝나더라구요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044306.1.13",
          "form": "저도 나름 시간맞춰서 대기타서 시도한거였는데도요 ㅜ",
          "original_form": "저도 나름 시간맞춰서 대기타서 시도한거였는데도요 ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044306.1.14",
          "form": "와그거 선착순 그거죠!!",
          "original_form": "와그거 선착순 그거죠!!",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044306.1.15",
          "form": "저도 그런거 누르자마자 끝나드라구요 ㅠㅠ",
          "original_form": "저도 그런거 누르자마자 끝나드라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044306.1.16",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044306.1.17",
          "form": "그거 아니면 영화보기 돈아까워요",
          "original_form": "그거 아니면 영화보기 돈아까워요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044306.1.18",
          "form": "그니깐요 사람들 진짜 빨라 ㅋㅋㄱㄱ",
          "original_form": "그니깐요 사람들 진짜 빨라 ㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044306.1.19",
          "form": "그쵸그쵸 ㅋㅋㅋㅋ 다들 그걸로 영화 보나바요",
          "original_form": "그쵸그쵸 ㅋㅋㅋㅋ 다들 그걸로 영화 보나바요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044306.1.20",
          "form": "영화비 진짜 많이 올랐더라구요",
