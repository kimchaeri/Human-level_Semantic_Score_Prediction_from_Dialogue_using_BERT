      "utterance": [
        {
          "id": "MDRW2100027930.1.1",
          "form": "점심은 무얼 드셨어요?",
          "original_form": "점심은 무얼 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027930.1.2",
          "form": "저는 아점으로 두부 스테이크 먹었어요",
          "original_form": "저는 아점으로 두부 스테이크 먹었어요",
          "speaker_id": "1",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027930.1.3",
          "form": "name2은 뭐 드셨나요?",
          "original_form": "&name2&은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027930.1.4",
          "form": "오 직접 요리하셨어요?",
          "original_form": "오 직접 요리하셨어요?",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027930.1.5",
          "form": "저는 점저로 전복죽 먹었어요",
          "original_form": "저는 점저로 전복죽 먹었어요",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027930.1.6",
          "form": "어머니께서 요리해주셨어요",
          "original_form": "어머니께서 요리해주셨어요",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027930.1.7",
          "form": "저는 아빠가 해주셨어요",
          "original_form": "저는 아빠가 해주셨어요",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027930.1.8",
          "form": "캬 멋져오",
          "original_form": "캬 멋져오",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027930.1.9",
          "form": "전복죽 맛있죠. 보양식 좋아요",
          "original_form": "전복죽 맛있죠. 보양식 좋아요",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027930.1.10",
          "form": "ㅋㅋㅋㅋ 감사합니다",
          "original_form": "ㅋㅋㅋㅋ 감사합니다",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027930.1.11",
          "form": "두부스테이크 소스는 간장소스인가요",
          "original_form": "두부스테이크 소스는 간장소스인가요",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027930.1.12",
          "form": "아 소스는 없이 먹었어요",
          "original_form": "아 소스는 없이 먹었어요",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.13",
          "form": "전복중에 내장이 들어가서 초록색이었어요",
          "original_form": "전복중에 내장이 들어가서 초록색이었어요",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.14",
          "form": "통구이?같은게아니라",
          "original_form": "통구이?같은게아니라",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.15",
          "form": "진짜 건강식이군요",
          "original_form": "진짜 건강식이군요",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.16",
          "form": "아 진짜 전복죽이네요",
          "original_form": "아 진짜 전복죽이네요",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.17",
          "form": "다진요리인가요",
          "original_form": "다진요리인가요",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.18",
          "form": "집에서 요리한거니 건강해요",
          "original_form": "집에서 요리한거니 건강해요",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.19",
          "form": "두부 으깨서 참치 햄 계란 부침가루같은거 섞고",
          "original_form": "두부 으깨서 참치 햄 계란 부침가루같은거 섞고",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027930.1.20",
          "form": "요리도 하세요?",
          "original_form": "요리도 하세요?",
          "speaker_id": "2",
          "time": "20210902 16:23"
        }
