      "utterance": [
        {
          "id": "MDRW2100045043.1.1",
          "form": "당근이나 중고거래해보셨어요?",
          "original_form": "당근이나 중고거래해보셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045043.1.2",
          "form": "네네 당근 작년에 엄청 팔았어요 ㅋㅋ",
          "original_form": "네네 당근 작년에 엄청 팔았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045043.1.3",
          "form": "물건이 넘 많아서..",
          "original_form": "물건이 넘 많아서..",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045043.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ오 어떤거파셨어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오 어떤거파셨어요",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045043.1.5",
          "form": "하나씩얘기해줘보세여!!궁금해요",
          "original_form": "하나씩얘기해줘보세여!!궁금해요",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045043.1.6",
          "form": "저는 잡동사니가 젤 많았던거 같아요 ㅋㅋㅋ 소품 같은거 ㅋㅋ",
          "original_form": "저는 잡동사니가 젤 많았던거 같아요 ㅋㅋㅋ 소품 같은거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045043.1.7",
          "form": "아하 어떤 소품들 파셨어요??",
          "original_form": "아하 어떤 소품들 파셨어요??",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045043.1.8",
          "form": "신발도 팔고 가방 모자",
          "original_form": "신발도 팔고 가방 모자",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045043.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045043.1.10",
          "form": "소품은 그냥 장식픔 같은거요",
          "original_form": "소품은 그냥 장식픔 같은거요",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045043.1.11",
          "form": "인테리어소품이여?",
          "original_form": "인테리어소품이여?",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045043.1.12",
          "form": "기념품이나.. 커피 같은것도 팔았네요",
          "original_form": "기념품이나.. 커피 같은것도 팔았네요",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045043.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045043.1.14",
          "form": "커피는 캡슐같은거말인가여?",
          "original_form": "커피는 캡슐같은거말인가여?",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045043.1.15",
          "form": "인스턴트커피요 봉지류",
          "original_form": "인스턴트커피요 봉지류",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045043.1.16",
          "form": "그냥 쓰다가 남은걸 거의 팔았어요 ㅎㅎ",
          "original_form": "그냥 쓰다가 남은걸 거의 팔았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
