      "utterance": [
        {
          "id": "MDRW2100035374.1.1",
          "form": "엉 잔잔하면서도 여러 얘기를 담고 있다",
          "original_form": "엉 잔잔하면서도 여러 얘기를 담고 있다",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035374.1.2",
          "form": "울아빠 영화가 조잡하네 이런다",
          "original_form": "울아빠 영화가 조잡하네 이런다",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035374.1.3",
          "form": "그러나 볼게 없어 본다고",
          "original_form": "그러나 볼게 없어 본다고",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100035374.1.4",
          "form": "옆집 남자애가 동 성 애 자 로 성장",
          "original_form": "옆집 남자애가 동 성 애 자 로 성장",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100035374.1.5",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100035374.1.6",
          "form": "영화 한나 말하는거가 ㅋㅋㅋ",
          "original_form": "영화 한나 말하는거가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100035374.1.7",
          "form": "어 한나ㅋ",
          "original_form": "어 한나ㅋ",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100035374.1.8",
          "form": "빌리 엘리어트 보시라고 해라",
          "original_form": "빌리 엘리어트 보시라고 해라",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100035374.1.9",
          "form": "아마 봤을지도",
          "original_form": "아마 봤을지도",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100035374.1.10",
          "form": "이건 또봐도 재밌더라 부성애",
          "original_form": "이건 또봐도 재밌더라 부성애",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100035374.1.11",
          "form": "왜만하게 티비에서 하는건 다 봤음ㅋ 재바을 워낙하니",
          "original_form": "왜만하게 티비에서 하는건 다 봤음ㅋ 재바을 워낙하니",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035374.1.12",
          "form": "맞다 ㅋㅋ",
          "original_form": "맞다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035374.1.13",
          "form": "나 이거 극장에서 본거 같다ㅋ",
          "original_form": "나 이거 극장에서 본거 같다ㅋ",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035374.1.14",
          "form": "진짜 ㅋㅋ 근데 가물가물하나",
          "original_form": "진짜 ㅋㅋ 근데 가물가물하나",
          "speaker_id": "2",
