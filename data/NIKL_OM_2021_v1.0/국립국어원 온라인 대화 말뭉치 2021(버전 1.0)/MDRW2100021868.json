      "utterance": [
        {
          "id": "MDRW2100021868.1.1",
          "form": "칼국수 좋아하시나요?",
          "original_form": "칼국수 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021868.1.2",
          "form": "네 엄청 좋아해요!",
          "original_form": "네 엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021868.1.3",
          "form": "잔치국수 보다 칼국수인가요?",
          "original_form": "잔치국수 보다 칼국수인가요?",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021868.1.4",
          "form": "굳이 따지면 그런데 둘 다 엄청 좋아해요",
          "original_form": "굳이 따지면 그런데 둘 다 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021868.1.5",
          "form": "닉네임은 그때그때 생각나는 음식으로 지어서 name2예요ㅎㅎ",
          "original_form": "닉네임은 그때그때 생각나는 음식으로 지어서 &name2&예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021868.1.6",
          "form": "장? 칼국수는 뭐에요? 일반 칼국수랑 달라요?",
          "original_form": "장? 칼국수는 뭐에요? 일반 칼국수랑 달라요?",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021868.1.7",
          "form": "음 국물에 고추장을 풀어서 끓인것같아요",
          "original_form": "음 국물에 고추장을 풀어서 끓인것같아요",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021868.1.8",
          "form": "강릉 음식입니다",
          "original_form": "강릉 음식입니다",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021868.1.9",
          "form": "아.. 보통 육개장 칼국수라 하는거랑 비슥한거 같네여?",
          "original_form": "아.. 보통 육개장 칼국수라 하는거랑 비슥한거 같네여?",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021868.1.10",
          "form": "맛은 좀 다른 계열이에요",
          "original_form": "맛은 좀 다른 계열이에요",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021868.1.11",
          "form": "기회가 되시면 한번 드셔보세요",
          "original_form": "기회가 되시면 한번 드셔보세요",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021868.1.12",
          "form": "아.. 먹는게 얘기하니까.. 엄청 먹고싶네요..",
          "original_form": "아.. 먹는게 얘기하니까.. 엄청 먹고싶네요..",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021868.1.13",
          "form": "낼은 장칼국수로.. ㅋㅋㅋ",
          "original_form": "낼은 장칼국수로.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021868.1.14",
          "form": "그쵸 저도 배가 고프네뇨",
          "original_form": "그쵸 저도 배가 고프네뇨",
          "speaker_id": "2",
