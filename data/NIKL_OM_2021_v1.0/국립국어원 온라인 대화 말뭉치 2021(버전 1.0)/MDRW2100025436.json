      "utterance": [
        {
          "id": "MDRW2100025436.1.1",
          "form": "거기는 오늘 날씨가 어떤가요?",
          "original_form": "거기는 오늘 날씨가 어떤가요?",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025436.1.2",
          "form": "아침에 좀 비 내리다가 지금은 그쳤어요",
          "original_form": "아침에 좀 비 내리다가 지금은 그쳤어요",
          "speaker_id": "2",
          "time": "20210901 11:25"
        },
        {
          "id": "MDRW2100025436.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210901 11:25"
        },
        {
          "id": "MDRW2100025436.1.4",
          "form": "저도 아침 출근할때는 조금 내렸는데 지금은 안오는것같아요",
          "original_form": "저도 아침 출근할때는 조금 내렸는데 지금은 안오는것같아요",
          "speaker_id": "1",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100025436.1.5",
          "form": "내일도 비가 온다는것같아요",
          "original_form": "내일도 비가 온다는것같아요",
          "speaker_id": "1",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100025436.1.6",
          "form": "지금도 구름이 아직 끼어있긴 해요",
          "original_form": "지금도 구름이 아직 끼어있긴 해요",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025436.1.7",
          "form": "비가 엄청 자주오는 느낌이에요",
          "original_form": "비가 엄청 자주오는 느낌이에요",
          "speaker_id": "1",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025436.1.8",
          "form": "저번주에는 번개 엄청치더라구요",
          "original_form": "저번주에는 번개 엄청치더라구요",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025436.1.9",
          "form": "그래서 뒷산 나무에 떨어져서 불이 났었어요",
          "original_form": "그래서 뒷산 나무에 떨어져서 불이 났었어요",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025436.1.10",
          "form": "화재도 나구 위험하네요",
          "original_form": "화재도 나구 위험하네요",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025436.1.11",
          "form": "비 계속 내릴거라고 하더라구요",
          "original_form": "비 계속 내릴거라고 하더라구요",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025436.1.12",
          "form": "또 많이 온다고 해서 무섭네요",
          "original_form": "또 많이 온다고 해서 무섭네요",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025436.1.13",
          "form": "그래도 인명피해는 없다해서 다행입니다",
          "original_form": "그래도 인명피해는 없다해서 다행입니다",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025436.1.14",
          "form": "어떤 계절 좋아하시나요",
          "original_form": "어떤 계절 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025436.1.15",
          "form": "봄이요. 꽃구경하기 좋아서요.",
          "original_form": "봄이요. 꽃구경하기 좋아서요.",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025436.1.16",
          "form": "벚꽃구경 좋지요 ㅎㅎ",
          "original_form": "벚꽃구경 좋지요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025436.1.17",
          "form": "저는 가을 선선하니 좋아요",
          "original_form": "저는 가을 선선하니 좋아요",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025436.1.18",
          "form": "곧이네요",
          "original_form": "곧이네요",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025436.1.19",
          "form": "비만 그치면 딱 좋을 날씨일거 같아요",
          "original_form": "비만 그치면 딱 좋을 날씨일거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025436.1.20",
          "form": "가을 전어도 기대가 돼요",
          "original_form": "가을 전어도 기대가 돼요",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025436.1.21",
          "form": "비가 또 많이 오기 시작하네요",
          "original_form": "비가 또 많이 오기 시작하네요",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025436.1.22",
          "form": "가을엔 먹거리도 많죠ㅋㅋ",
          "original_form": "가을엔 먹거리도 많죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025436.1.23",
          "form": "그래서 살찌는 계절인가봐요",
          "original_form": "그래서 살찌는 계절인가봐요",
          "speaker_id": "1",
          "time": "20210901 13:03"
        }
      ]
