      "utterance": [
        {
          "id": "MDRW2100014107.1.1",
          "form": "재택근무...이러면안되는데",
          "original_form": "재택근무...이러면안되는데",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014107.1.2",
          "form": "재택근무할때 컴퓨터 켜놓고 방바닥에 누워있던적 있어요",
          "original_form": "재택근무할때 컴퓨터 켜놓고 방바닥에 누워있던적 있어요",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014107.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014107.1.4",
          "form": "ㅋㅋㅋ 일없을때는 그래도되죠ㅎㅎ",
          "original_form": "ㅋㅋㅋ 일없을때는 그래도되죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014107.1.5",
          "form": "할일만 잘하면 괜찮은거 아닐까요? ㅎㅎ",
          "original_form": "할일만 잘하면 괜찮은거 아닐까요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014107.1.6",
          "form": "저도 티비 볼때 있어요",
          "original_form": "저도 티비 볼때 있어요",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014107.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014107.1.8",
          "form": "그것이 재택의 장점이니깐요",
          "original_form": "그것이 재택의 장점이니깐요",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014107.1.9",
          "form": "백색소음처럼 뭐틀어두면 일도 잘되더라구요ㅋㅋ",
          "original_form": "백색소음처럼 뭐틀어두면 일도 잘되더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014107.1.10",
          "form": "음악을 항상켜두죠ㅋㅋ",
          "original_form": "음악을 항상켜두죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014107.1.11",
          "form": "유투브에 있는 카페음악 같은것두있고",
          "original_form": "유투브에 있는 카페음악 같은것두있고",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014107.1.12",
          "form": "얘기만들어도 좋아요",
          "original_form": "얘기만들어도 좋아요",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014107.1.13",
          "form": "거기에 아아 한잔!",
          "original_form": "거기에 아아 한잔!",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014107.1.14",
          "form": "코로나가끝나두 재택근무는 병행할수있는 시스템으로 갈수있으면좋겠어요",
          "original_form": "코로나가끝나두 재택근무는 병행할수있는 시스템으로 갈수있으면좋겠어요",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014107.1.15",
          "form": "맞아요!  집에서 커피 내리면 거기가 카페죠",
          "original_form": "맞아요!  집에서 커피 내리면 거기가 카페죠",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014107.1.16",
          "form": "맞아요!! 재택근무는 좀더 유연하게 일할수있어서그런지",
          "original_form": "맞아요!! 재택근무는 좀더 유연하게 일할수있어서그런지",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014107.1.17",
          "form": "+ 출퇴근을 안해서 되서 덜피곤한거같아요",
          "original_form": "+ 출퇴근을 안해서 되서 덜피곤한거같아요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014107.1.18",
          "form": "장점이 더 많은것같아요ㅎㅎ",
          "original_form": "장점이 더 많은것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014107.1.19",
          "form": "일주일에 2,3일은 재택근무하면좋겠어요",
          "original_form": "일주일에 2,3일은 재택근무하면좋겠어요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014107.1.20",
          "form": "맞아요ㅋㅋ 출퇴근을 안해도되는게 진짜좋아요!",
          "original_form": "맞아요ㅋㅋ 출퇴근을 안해도되는게 진짜좋아요!",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014107.1.21",
          "form": "그래서 옷도 덜 사는 것 같아요ㅋㅋ",
          "original_form": "그래서 옷도 덜 사는 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014107.1.22",
          "form": "아 맞아요ㅋㅋㅋㅋㄱ 그리고",
          "original_form": "아 맞아요ㅋㅋㅋㅋㄱ 그리고",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014107.1.23",
          "form": "밖에 덜나가서그런가 겉옷 빨래도 덜해요ㅋㅋ",
          "original_form": "밖에 덜나가서그런가 겉옷 빨래도 덜해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014107.1.24",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014107.1.25",
          "form": "재택근무를 할수없는업종은 좀 힘들것같기도해요",
          "original_form": "재택근무를 할수없는업종은 좀 힘들것같기도해요",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014107.1.26",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014107.1.27",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "2",
