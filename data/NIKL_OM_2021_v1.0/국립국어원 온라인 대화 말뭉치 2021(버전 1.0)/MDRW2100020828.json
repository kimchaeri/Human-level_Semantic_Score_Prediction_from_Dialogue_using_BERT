      "utterance": [
        {
          "id": "MDRW2100020828.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020828.1.2",
          "form": "공연이나 전시 보는거 좋아하세요?",
          "original_form": "공연이나 전시 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020828.1.3",
          "form": "전시회는 가끔 찾아봐요",
          "original_form": "전시회는 가끔 찾아봐요",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020828.1.4",
          "form": "최근 보러가신거 있나요?",
          "original_form": "최근 보러가신거 있나요?",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020828.1.5",
          "form": "아 보러가고싶은게 있었는데 못봤어요 ㅠㅠ",
          "original_form": "아 보러가고싶은게 있었는데 못봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020828.1.6",
          "form": "그래요? 어떤거에요?",
          "original_form": "그래요? 어떤거에요?",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020828.1.7",
          "form": "요시고 사진전이요!",
          "original_form": "요시고 사진전이요!",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020828.1.8",
          "form": "갔더니 대기가 3시간이더라구요 ㅠ",
          "original_form": "갔더니 대기가 3시간이더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020828.1.9",
          "form": "아ㅠㅠ 너무 기네요",
          "original_form": "아ㅠㅠ 너무 기네요",
          "speaker_id": "2",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020828.1.10",
          "form": "요샌 시국때문에도 가보기가 어려워요",
          "original_form": "요샌 시국때문에도 가보기가 어려워요",
          "speaker_id": "2",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020828.1.11",
          "form": "맞아요 인원 수 제한도 있고 조심스럽더라구요 ㅠ",
          "original_form": "맞아요 인원 수 제한도 있고 조심스럽더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020828.1.12",
          "form": "가장 좋았던 전시 있으세요?",
          "original_form": "가장 좋았던 전시 있으세요?",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020828.1.13",
          "form": "예전에 지브리 전시 본적 있는데",
          "original_form": "예전에 지브리 전시 본적 있는데",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020828.1.14",
          "form": "신기하더라구요ㅋㅋ",
          "original_form": "신기하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020828.1.15",
          "form": "오 그렇군요 저는 픽사전시 기억에남아요! 우디랑 버즈 모형잇구 그러더라구요 ㅋㅋㅋㅋ",
          "original_form": "오 그렇군요 저는 픽사전시 기억에남아요! 우디랑 버즈 모형잇구 그러더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020828.1.16",
          "form": "재밌을 거 같아요. 보고싶네요ㅋㅋ",
          "original_form": "재밌을 거 같아요. 보고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020828.1.17",
          "form": "피카소 전시도 가보고싶었는데 결국 못 갔어요",
          "original_form": "피카소 전시도 가보고싶었는데 결국 못 갔어요",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020828.1.18",
          "form": "아아 저두여 ㅠㅠ 예전에 고흐전인가 그것두요",
          "original_form": "아아 저두여 ㅠㅠ 예전에 고흐전인가 그것두요",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020828.1.19",
          "form": "전시일정같은거 자주 찾아보세요?",
          "original_form": "전시일정같은거 자주 찾아보세요?",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020828.1.20",
          "form": "자주는 아니지만 가끔씩 찾아보는 편이에요",
          "original_form": "자주는 아니지만 가끔씩 찾아보는 편이에요",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020828.1.21",
          "form": "저는 지방이라 전시가 많이없어서 아쉬워요",
          "original_form": "저는 지방이라 전시가 많이없어서 아쉬워요",
          "speaker_id": "1",
          "time": "20210828 14:06"
        }
      ]
    }
