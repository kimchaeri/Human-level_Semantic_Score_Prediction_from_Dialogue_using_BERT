      "utterance": [
        {
          "id": "MDRW2100043252.1.1",
          "form": "곧 퇴근이다",
          "original_form": "곧 퇴근이다",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043252.1.2",
          "form": "오늘진짜 도른줄알았어",
          "original_form": "오늘진짜 도른줄알았어",
          "speaker_id": "2",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043252.1.3",
          "form": "시간이 너무 안가는거지",
          "original_form": "시간이 너무 안가는거지",
          "speaker_id": "2",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043252.1.4",
          "form": "진짜 왜이렇게 시간안가냐 겨우 화요일",
          "original_form": "진짜 왜이렇게 시간안가냐 겨우 화요일",
          "speaker_id": "1",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043252.1.5",
          "form": "우울해 3일이나 더 가야돼",
          "original_form": "우울해 3일이나 더 가야돼",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043252.1.6",
          "form": "퇴근하고 집가서 맥주하나까고싶은데 어때",
          "original_form": "퇴근하고 집가서 맥주하나까고싶은데 어때",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043252.1.7",
          "form": "좋은생각인데? 맥주 마셔",
          "original_form": "좋은생각인데? 맥주 마셔",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043252.1.8",
          "form": "샤브샤브먹는다며?",
          "original_form": "샤브샤브먹는다며?",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043252.1.9",
          "form": "ㅋㅋㅋㅋ먹어야지.....근데 엄빠가 대답을안해",
          "original_form": "ㅋㅋㅋㅋ먹어야지.....근데 엄빠가 대답을안해",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043252.1.10",
          "form": "밭에가면 나혼자 샤브샤브먹기는 쫌...너무바쁘겠는걸",
          "original_form": "밭에가면 나혼자 샤브샤브먹기는 쫌...너무바쁘겠는걸",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043252.1.11",
          "form": "너무맛있겠는데? 그냥 다 때려넣고먹어야해",
          "original_form": "너무맛있겠는데? 그냥 다 때려넣고먹어야해",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043252.1.12",
          "form": "청경채 없어...사가?",
          "original_form": "청경채 없어...사가?",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043252.1.13",
          "form": "배추는?",
          "original_form": "배추는?",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043252.1.14",
          "form": "일끝나고 제일큰행복 솔직히 음식밖에없어",
          "original_form": "일끝나고 제일큰행복 솔직히 음식밖에없어",
          "speaker_id": "2",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043252.1.15",
          "form": "맞아 그래서 나는 불행해",
          "original_form": "맞아 그래서 나는 불행해",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043252.1.16",
          "form": "배추있어집에 호호호",
          "original_form": "배추있어집에 호호호",
          "speaker_id": "2",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043252.1.17",
          "form": "오늘도밥을안먹을거라",
          "original_form": "오늘도밥을안먹을거라",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043252.1.18",
          "form": "개웃겨",
          "original_form": "개웃겨",
          "speaker_id": "2",
