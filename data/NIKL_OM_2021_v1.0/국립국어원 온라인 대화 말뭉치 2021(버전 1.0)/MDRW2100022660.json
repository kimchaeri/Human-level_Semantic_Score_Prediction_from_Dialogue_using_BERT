      "utterance": [
        {
          "id": "MDRW2100022660.1.1",
          "form": "런닝맨 보세요?",
          "original_form": "런닝맨 보세요?",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022660.1.2",
          "form": "아니요 ㅜ 예능은 또 잘 안봐요 ㅜ",
          "original_form": "아니요 ㅜ 예능은 또 잘 안봐요 ㅜ",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022660.1.3",
          "form": "아하~ 요즘 런닝맨 엄청 재밌어요ㅋㅋㅋㅋ 나중에 레전드편 보세요",
          "original_form": "아하~ 요즘 런닝맨 엄청 재밌어요ㅋㅋㅋㅋ 나중에 레전드편 보세요",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022660.1.4",
          "form": "진짜 배찢어짐 ㅜ",
          "original_form": "진짜 배찢어짐 ㅜ",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022660.1.5",
          "form": "무도는 많이 봤었는데 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "무도는 많이 봤었는데 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022660.1.6",
          "form": "그정도로 재밌어요ㅋㅋㅋ",
          "original_form": "그정도로 재밌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022660.1.7",
          "form": "놀면뭐하니 이제 약간 무도식으로 하는거 같더라구요",
          "original_form": "놀면뭐하니 이제 약간 무도식으로 하는거 같더라구요",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022660.1.8",
          "form": "기존 놀뭐처럼 하면서 무도방식도 섞고",
          "original_form": "기존 놀뭐처럼 하면서 무도방식도 섞고",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022660.1.9",
          "form": "맞아요 플러스가 붙었다고 들었어요 ㅋㅋㅋ",
          "original_form": "맞아요 플러스가 붙었다고 들었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022660.1.10",
          "form": "어제 방송분에서 무도처럼 나오긴했는데ㅋㅋㅋ",
          "original_form": "어제 방송분에서 무도처럼 나오긴했는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022660.1.11",
          "form": "저는 정준하 별로 안 좋아해서ㅠㅠㅠㅠ",
          "original_form": "저는 정준하 별로 안 좋아해서ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022660.1.12",
          "form": "전 무도 박명수때문에 봤었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 무도 박명수때문에 봤었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022660.1.13",
          "form": "앜ㅋㅋㅋ츤츤거림",
          "original_form": "앜ㅋㅋㅋ츤츤거림",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022660.1.14",
          "form": "그럼 활명수 보세요?",
          "original_form": "그럼 활명수 보세요?",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022660.1.15",
          "form": "그거 유튜브던가요? 뭔지는 아는데 안봤어요 ㅋㅋㅋ",
          "original_form": "그거 유튜브던가요? 뭔지는 아는데 안봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022660.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022660.1.17",
          "form": "박명수 날아다녀요ㅋㅋㅋ",
          "original_form": "박명수 날아다녀요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022660.1.18",
          "form": "찐경규도 재밌어요! 이건 카카오티비꺼",
          "original_form": "찐경규도 재밌어요! 이건 카카오티비꺼",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022660.1.19",
          "form": "ㅋㅋㅋㅋ 아 둘다 재밌을거같은데 왜 유튜브는 잘 안봐지는지 모르겠어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 아 둘다 재밌을거같은데 왜 유튜브는 잘 안봐지는지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022660.1.20",
          "form": "개인적으로 찐경규가 더 재밌어요ㅋㅋㅋ피디가 모트모트pd라고",
          "original_form": "개인적으로 찐경규가 더 재밌어요ㅋㅋㅋ피디가 모트모트pd라고",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022660.1.21",
          "form": "마리텔에서 핫했던분ㅋㅋㅋㅋ",
          "original_form": "마리텔에서 핫했던분ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022660.1.22",
          "form": "저도 유튜브 예능같은건 자주 보지않고",
          "original_form": "저도 유튜브 예능같은건 자주 보지않고",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022660.1.23",
          "form": "웃기다는 컨텐츠 나오거나 누가 추천해주면 가끔봐요 ㅋㅋㅋ",
          "original_form": "웃기다는 컨텐츠 나오거나 누가 추천해주면 가끔봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:03"
        }
      ]
