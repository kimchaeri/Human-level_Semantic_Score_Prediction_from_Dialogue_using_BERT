      "utterance": [
        {
          "id": "MDRW2100011446.1.1",
          "form": "유튜브 자주보시나요",
          "original_form": "유튜브 자주보시나요",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011446.1.2",
          "form": "네 게임좋아해서 게임쪽 유튜브랑 요리유튜브 자주봐요",
          "original_form": "네 게임좋아해서 게임쪽 유튜브랑 요리유튜브 자주봐요",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011446.1.3",
          "form": "가구님은요?",
          "original_form": "가구님은요?",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011446.1.4",
          "form": "저는 여행유튜브 자주봐요",
          "original_form": "저는 여행유튜브 자주봐요",
          "speaker_id": "1",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100011446.1.5",
          "form": "여행유튜브 추천해주실 채널있나요?",
          "original_form": "여행유튜브 추천해주실 채널있나요?",
          "speaker_id": "2",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100011446.1.6",
          "form": "빠니보틀 재밋어요!",
          "original_form": "빠니보틀 재밋어요!",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100011446.1.7",
          "form": "추천 감사해요",
          "original_form": "추천 감사해요",
          "speaker_id": "2",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011446.1.8",
          "form": "추천 해주세요!",
          "original_form": "추천 해주세요!",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011446.1.9",
          "form": "먹방은 안보시나요?",
          "original_form": "먹방은 안보시나요?",
          "speaker_id": "2",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011446.1.10",
          "form": "먹방도 봅니다ㅎㅎ",
          "original_form": "먹방도 봅니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011446.1.11",
          "form": "저는 먹방은 유명하신 햇님하고 쯔양님만 봐서",
          "original_form": "저는 먹방은 유명하신 햇님하고 쯔양님만 봐서",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011446.1.12",
          "form": "이미 아실거같아요ㅋㅋㅋ",
          "original_form": "이미 아실거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011446.1.13",
          "form": "쯔양님은 그렇게 날씬한데..",
          "original_form": "쯔양님은 그렇게 날씬한데..",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011446.1.14",
          "form": "사람마다 체질다른거보면 너무 신기해요",
          "original_form": "사람마다 체질다른거보면 너무 신기해요",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011446.1.15",
          "form": "메뉴다양하게드셔서 보기 좋구요",
          "original_form": "메뉴다양하게드셔서 보기 좋구요",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011446.1.16",
          "form": "정원분식? 가게도 내셨던데",
          "original_form": "정원분식? 가게도 내셨던데",
          "speaker_id": "1",
