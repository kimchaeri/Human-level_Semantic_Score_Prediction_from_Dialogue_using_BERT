      "utterance": [
        {
          "id": "MDRW2100037806.1.1",
          "form": "니들이 뭔 치료를 하냐 하서..",
          "original_form": "니들이 뭔 치료를 하냐 하서..",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037806.1.2",
          "form": "ㅎㅎ..",
          "original_form": "ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037806.1.3",
          "form": "해외로 가자...",
          "original_form": "해외로 가자...",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037806.1.4",
          "form": "그럴꺼야..아님",
          "original_form": "그럴꺼야..아님",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037806.1.5",
          "form": "내가 잘해서",
          "original_form": "내가 잘해서",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037806.1.6",
          "form": "한국에서 시간당 많이버는",
          "original_form": "한국에서 시간당 많이버는",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037806.1.7",
          "form": "언어치료사 되면",
          "original_form": "언어치료사 되면",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037806.1.8",
          "form": "여기에 박혀있지 무ㅜ",
          "original_form": "여기에 박혀있지 무ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037806.1.9",
          "form": "한국에서 뜨면 외국에서는 더 받을 수 있을 거야",
          "original_form": "한국에서 뜨면 외국에서는 더 받을 수 있을 거야",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100037806.1.10",
          "form": "우리 시야 넓게 보고 다니자",
          "original_form": "우리 시야 넓게 보고 다니자",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100037806.1.11",
          "form": "근데 그걸 모르겠으",
          "original_form": "근데 그걸 모르겠으",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037806.1.12",
          "form": "근데 내가 언어치료사로 하려면",
          "original_form": "근데 내가 언어치료사로 하려면",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037806.1.13",
          "form": "외국에서 시험 또 봐야할지도 몰러",
          "original_form": "외국에서 시험 또 봐야할지도 몰러",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037806.1.14",
          "form": "인정해주는지 안해주는지 몰라서",
          "original_form": "인정해주는지 안해주는지 몰라서",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037806.1.15",
          "form": "하다보면 어떻개든 되겠지!ㅎㅎ",
          "original_form": "하다보면 어떻개든 되겠지!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037806.1.16",
          "form": "그지!",
          "original_form": "그지!",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037806.1.17",
          "form": "영어 회화 열심히 공부하자...ㅋㅋ",
          "original_form": "영어 회화 열심히 공부하자...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100037806.1.18",
          "form": "물론 결제한 인강부터 다시 들어야 하는데ㅋㅋㅋ",
          "original_form": "물론 결제한 인강부터 다시 들어야 하는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100037806.1.19",
          "form": "영어..?",
          "original_form": "영어..?",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.20",
          "form": "아 증망 하기싫다",
          "original_form": "아 증망 하기싫다",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.21",
          "form": "그냥 한국에서 살까...",
          "original_form": "그냥 한국에서 살까...",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.22",
          "form": "ㅎㅎㅎㅎ...",
          "original_form": "ㅎㅎㅎㅎ...",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.23",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.24",
          "form": "영어가 너무 싫은걸",
          "original_form": "영어가 너무 싫은걸",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.25",
          "form": "리얼클래스 요즘 해?",
          "original_form": "리얼클래스 요즘 해?",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.26",
          "form": "난 해커스 포기했어",
          "original_form": "난 해커스 포기했어",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.27",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.28",
          "form": "히힣",
          "original_form": "히힣",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037806.1.29",
          "form": "매일 하는게 쉬운 게 아니여~",
          "original_form": "매일 하는게 쉬운 게 아니여~",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037806.1.30",
          "form": "마자~",
          "original_form": "마자~",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037806.1.31",
          "form": "여행 얘기가 제일 재밌다",
          "original_form": "여행 얘기가 제일 재밌다",
          "speaker_id": "1",
          "time": "20210901 19:52"
        }
      ]
