      "utterance": [
        {
          "id": "MDRW2100019095.1.1",
          "form": "향기나는거 진짜 신세계였어요",
          "original_form": "향기나는거 진짜 신세계였어요",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019095.1.2",
          "form": "그니깐요!!! 신문물 접해보고 신났었죠 ㅋㅋㅋㅋㅋ",
          "original_form": "그니깐요!!! 신문물 접해보고 신났었죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019095.1.3",
          "form": "넷플이나 왓쳐에서",
          "original_form": "넷플이나 왓쳐에서",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019095.1.4",
          "form": "요즘 보시는건 무엇?!",
          "original_form": "요즘 보시는건 무엇?!",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019095.1.5",
          "form": "요새는 굿걸스라는 넷플 드라마보는데",
          "original_form": "요새는 굿걸스라는 넷플 드라마보는데",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019095.1.6",
          "form": "곧 다음 시즌 나와서 재탕중이에요 ㅋㅋㅋ",
          "original_form": "곧 다음 시즌 나와서 재탕중이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019095.1.7",
          "form": "오!! 그거 그 마트터는거 맞나요???",
          "original_form": "오!! 그거 그 마트터는거 맞나요???",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019095.1.8",
          "form": "오맞아요 아세요??",
          "original_form": "오맞아요 아세요??",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019095.1.9",
          "form": "친구한테 들어봤는데 아직 보진않았어요!! 재밌나요?!?!",
          "original_form": "친구한테 들어봤는데 아직 보진않았어요!! 재밌나요?!?!",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019095.1.10",
          "form": "인생드라마 중 하나에요 ㅠㅠ",
          "original_form": "인생드라마 중 하나에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019095.1.11",
          "form": "나중에 잘생긴 갱이랑 이쁜 주부랑 바람도 핀답니다",
          "original_form": "나중에 잘생긴 갱이랑 이쁜 주부랑 바람도 핀답니다",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019095.1.12",
          "form": "오 ㅋㅋㅋㅋㅋ 흥미 진진 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋ 흥미 진진 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019095.1.13",
          "form": "나중에 꼭 봐볼게요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나중에 꼭 봐볼게요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019095.1.14",
          "form": "넘 재밌어서 시즌1을 하루만에 다 봤답니다",
          "original_form": "넘 재밌어서 시즌1을 하루만에 다 봤답니다",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019095.1.15",
          "form": "넷플 보세요?",
          "original_form": "넷플 보세요?",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019095.1.16",
          "form": "네네!! 저는 최근에 로스쿨 끝냈고",
          "original_form": "네네!! 저는 최근에 로스쿨 끝냈고",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019095.1.17",
          "form": "로스쿨이랑 비슷한 미드가 범죄의 재구성이라고 들어서",
          "original_form": "로스쿨이랑 비슷한 미드가 범죄의 재구성이라고 들어서",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019095.1.18",
          "form": "그거 보구있어요!!",
          "original_form": "그거 보구있어요!!",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019095.1.19",
          "form": "제목이 로스쿨인가여??",
          "original_form": "제목이 로스쿨인가여??",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019095.1.20",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019095.1.21",
          "form": "네네 로스쿨은 한국드라마에요!!",
          "original_form": "네네 로스쿨은 한국드라마에요!!",
