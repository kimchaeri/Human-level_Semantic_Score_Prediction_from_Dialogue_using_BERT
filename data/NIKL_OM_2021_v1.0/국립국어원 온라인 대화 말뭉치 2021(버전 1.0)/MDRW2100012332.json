      "utterance": [
        {
          "id": "MDRW2100012332.1.1",
          "form": "저랑 비슷하게",
          "original_form": "저랑 비슷하게",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100012332.1.2",
          "form": "할레루야~~~싸와디캅..",
          "original_form": "할레루야~~~싸와디캅..",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012332.1.3",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012332.1.4",
          "form": "무교요.. 모 믿으시나여?",
          "original_form": "무교요.. 모 믿으시나여?",
          "speaker_id": "2",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012332.1.5",
          "form": "아뇨 저도 무교에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 저도 무교에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012332.1.6",
          "form": "교회나 절 가보셨어요?",
          "original_form": "교회나 절 가보셨어요?",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012332.1.7",
          "form": "가보기야 가봤죠...",
          "original_form": "가보기야 가봤죠...",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012332.1.8",
          "form": "관광? 같은걸로... ㅎㅎㅎ",
          "original_form": "관광? 같은걸로... ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012332.1.9",
          "form": "아하 저는 절가서",
          "original_form": "아하 저는 절가서",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012332.1.10",
          "form": "무교집안인가여?",
          "original_form": "무교집안인가여?",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012332.1.11",
          "form": "템플스테이 해보고싶어요",
          "original_form": "템플스테이 해보고싶어요",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012332.1.12",
          "form": "네 무교에요ㅋㅋㅋㅋㅋ",
          "original_form": "네 무교에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012332.1.13",
          "form": "템플스테이는.좋은거같아요..",
          "original_form": "템플스테이는.좋은거같아요..",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012332.1.14",
          "form": "산속에들어가서.. 마음도 씻어내고",
          "original_form": "산속에들어가서.. 마음도 씻어내고",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012332.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 전 절밥이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 전 절밥이",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012332.1.16",
          "form": "정말 궁금하거든요",
          "original_form": "정말 궁금하거든요",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012332.1.17",
          "form": "절밥은 주말에 가면 주는데 많아요..",
          "original_form": "절밥은 주말에 가면 주는데 많아요..",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100012332.1.18",
          "form": "전 등산다니는데 일부러 절 들려서 얻어먹곤해요..",
          "original_form": "전 등산다니는데 일부러 절 들려서 얻어먹곤해요..",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100012332.1.19",
          "form": "아 진짜요? 몰랐어요",
          "original_form": "아 진짜요? 몰랐어요",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012332.1.20",
          "form": "그냥 주너요..?",
          "original_form": "그냥 주너요..?",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012332.1.21",
          "form": "서울은 관악산 유명해요.. 네 무료에요",
          "original_form": "서울은 관악산 유명해요.. 네 무료에요",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100012332.1.22",
          "form": "지금은 코로나땜에 안하는뎅",
          "original_form": "지금은 코로나땜에 안하는뎅",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100012332.1.23",
          "form": "아 아쉽게",
          "original_form": "아 아쉽게",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012332.1.24",
          "form": "서울은 머네유",
          "original_form": "서울은 머네유",
          "speaker_id": "1",
          "time": "20210824 23:28"
        }
      ]
    }
