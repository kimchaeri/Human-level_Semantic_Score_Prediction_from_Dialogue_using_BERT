      "utterance": [
        {
          "id": "MDRW2100037318.1.1",
          "form": "돼지바 먹고 싶다",
          "original_form": "돼지바 먹고 싶다",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037318.1.2",
          "form": "아이스크림 좋아하는구나",
          "original_form": "아이스크림 좋아하는구나",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037318.1.3",
          "form": "맞아 넌 무슨 아이스크림 좋아해",
          "original_form": "맞아 넌 무슨 아이스크림 좋아해",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037318.1.4",
          "form": "나는 초코맛이 좋아",
          "original_form": "나는 초코맛이 좋아",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037318.1.5",
          "form": "나는  오렌지맛이 좋아",
          "original_form": "나는  오렌지맛이 좋아",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037318.1.6",
          "form": "베스킨라빈스에서 무슨 맛 사먹어",
          "original_form": "베스킨라빈스에서 무슨 맛 사먹어",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037318.1.7",
          "form": "초코 나무 숲",
          "original_form": "초코 나무 숲",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.8",
          "form": "아몬드 봉봉 도 맛있는데",
          "original_form": "아몬드 봉봉 도 맛있는데",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.9",
          "form": "그건 부동의 일 디어",
          "original_form": "그건 부동의 일 디어",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.10",
          "form": "일등은",
          "original_form": "일등은",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.11",
          "form": "엄마는 외계인 도 맛있어",
          "original_form": "엄마는 외계인 도 맛있어",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.12",
          "form": "러브미 도 맛있는데",
          "original_form": "러브미 도 맛있는데",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.13",
          "form": "그건 잘 안 보이더라",
          "original_form": "그건 잘 안 보이더라",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.14",
          "form": "치즈들도 맛있어",
          "original_form": "치즈들도 맛있어",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.15",
          "form": "애플민트만 먹어봤어?",
          "original_form": "애플민트만 먹어봤어?",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.16",
          "form": "먹어봤어",
          "original_form": "먹어봤어",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037318.1.17",
          "form": "여름에 파는데 상큼해",
          "original_form": "여름에 파는데 상큼해",
