      "utterance": [
        {
          "id": "MDRW2100035755.1.1",
          "form": "안녕하세용 주말에 뭐 드셨어용~?",
          "original_form": "안녕하세용 주말에 뭐 드셨어용~?",
          "speaker_id": "1",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100035755.1.2",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "2",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100035755.1.3",
          "form": "주말에 손두부집가서",
          "original_form": "주말에 손두부집가서",
          "speaker_id": "2",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100035755.1.4",
          "form": "우와 손두부 고소한데 맛있는거 드셨네용",
          "original_form": "우와 손두부 고소한데 맛있는거 드셨네용",
          "speaker_id": "1",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100035755.1.5",
          "form": "매콤순두부 맛나게먹었고",
          "original_form": "매콤순두부 맛나게먹었고",
          "speaker_id": "2",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100035755.1.6",
          "form": "치킨쿠폰있어서",
          "original_form": "치킨쿠폰있어서",
          "speaker_id": "2",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100035755.1.7",
          "form": "쿠폰쓸겸 치킨도먹었네요 ㅋ",
          "original_form": "쿠폰쓸겸 치킨도먹었네요 ㅋ",
          "speaker_id": "2",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100035755.1.8",
          "form": "ㅎㅎ 비오는데 순두부랑 치킨 완전 좋죠",
          "original_form": "ㅎㅎ 비오는데 순두부랑 치킨 완전 좋죠",
          "speaker_id": "1",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100035755.1.9",
          "form": "치킨은 교촌이었는데  너어어어무 짰어요..흥...",
          "original_form": "치킨은 교촌이었는데  너어어어무 짰어요..흥...",
          "speaker_id": "2",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100035755.1.10",
          "form": "저는 빵이랑",
          "original_form": "저는 빵이랑",
          "speaker_id": "1",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100035755.1.11",
          "form": "아아 무슨 치킨 드셨어요?",
          "original_form": "아아 무슨 치킨 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100035755.1.12",
          "form": "오리지널?",
          "original_form": "오리지널?",
          "speaker_id": "1",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100035755.1.13",
          "form": "레드콤보1/2+허니오리웨지감자세트용ㅋ",
          "original_form": "레드콤보1/2+허니오리웨지감자세트용ㅋ",
          "speaker_id": "2",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100035755.1.14",
          "form": "구성은 너무 좋았는데 너무 짜서ㅠㅠㅠ",
          "original_form": "구성은 너무 좋았는데 너무 짜서ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100035755.1.15",
          "form": "저도 교총시킬때",
          "original_form": "저도 교총시킬때",
          "speaker_id": "1",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100035755.1.16",
          "form": "웨지감자 꼭 시켜요ㅋㅋ",
          "original_form": "웨지감자 꼭 시켜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100035755.1.17",
          "form": "저희동네만 그랬을까요?흑흑ㅜㅜ",
          "original_form": "저희동네만 그랬을까요?흑흑ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100035755.1.18",
          "form": "ㅋㅋ웨지감자맛나죵",
          "original_form": "ㅋㅋ웨지감자맛나죵",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100035755.1.19",
          "form": "저도 빵먹고싶네요~~",
          "original_form": "저도 빵먹고싶네요~~",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100035755.1.20",
          "form": "그러게요ㅠㅠ 다음엔 소스 덜 발라달라고 하시면 좋지않을까용? ㅎㅎㅎ",
          "original_form": "그러게요ㅠㅠ 다음엔 소스 덜 발라달라고 하시면 좋지않을까용? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100035755.1.21",
          "form": "치킨 원래 어느브랜드 좋아하세요?",
          "original_form": "치킨 원래 어느브랜드 좋아하세요?",
