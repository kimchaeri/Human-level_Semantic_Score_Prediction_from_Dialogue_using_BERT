      "utterance": [
        {
          "id": "MDRW2100007464.1.1",
          "form": "이번에 삼성에서",
          "original_form": "이번에 삼성에서",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007464.1.2",
          "form": "폴드3이랑 플립3",
          "original_form": "폴드3이랑 플립3",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007464.1.3",
          "form": "나온거 아시죠",
          "original_form": "나온거 아시죠",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007464.1.4",
          "form": "아 봤어봤어",
          "original_form": "아 봤어봤어",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007464.1.5",
          "form": "귀엽게 생겼더라",
          "original_form": "귀엽게 생겼더라",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007464.1.6",
          "form": "예약한 사람들 엄청 많더라고",
          "original_form": "예약한 사람들 엄청 많더라고",
          "speaker_id": "2",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007464.1.7",
          "form": "제 친구도 사전예약",
          "original_form": "제 친구도 사전예약",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007464.1.8",
          "form": "성공해서",
          "original_form": "성공해서",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007464.1.9",
          "form": "엊그제 폴드3 받았어요 ㅋㅋㅋㅋㅋ",
          "original_form": "엊그제 폴드3 받았어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007464.1.10",
          "form": "어 정말? 대박이다",
          "original_form": "어 정말? 대박이다",
          "speaker_id": "2",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007464.1.11",
          "form": "이번에 사전예약 하려는 사람들 많아서",
          "original_form": "이번에 사전예약 하려는 사람들 많아서",
          "speaker_id": "2",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007464.1.12",
          "form": "나름 경쟁률 치열했다고 들었는데",
          "original_form": "나름 경쟁률 치열했다고 들었는데",
          "speaker_id": "2",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007464.1.13",
          "form": "삼성 홈페이지 다운돼서",
          "original_form": "삼성 홈페이지 다운돼서",
          "speaker_id": "1",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007464.1.14",
          "form": "그거 한창 사전예약할 때",
          "original_form": "그거 한창 사전예약할 때",
          "speaker_id": "1",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007464.1.15",
          "form": "이게 대기업 홈페이지 맞냐고 저한테 투덜거리더라구요",
          "original_form": "이게 대기업 홈페이지 맞냐고 저한테 투덜거리더라구요",
          "speaker_id": "1",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007464.1.16",
          "form": "그런 선착순 물량제한 걸린거 살 때나 예약할 때는",
          "original_form": "그런 선착순 물량제한 걸린거 살 때나 예약할 때는",
          "speaker_id": "2",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007464.1.17",
          "form": "어느 기업 홈페이지고 다 터지는거같아",
          "original_form": "어느 기업 홈페이지고 다 터지는거같아",
          "speaker_id": "2",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007464.1.18",
          "form": "타 브랜드 같은 경우엔 전날부터 매장 앞에서 기다리는 사람들도 많잖아 ㅋㅋ",
          "original_form": "타 브랜드 같은 경우엔 전날부터 매장 앞에서 기다리는 사람들도 많잖아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007464.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ암튼 이번에 받은거 보니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ암튼 이번에 받은거 보니까",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007464.1.20",
          "form": "폰 진짜 크더라고요",
          "original_form": "폰 진짜 크더라고요",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007464.1.21",
          "form": "그걸로 게임하는거 보는데 그냥 패드로 게임하는 것 같은 엄청 큰 화면 ㅋㅋㅋㅋ",
          "original_form": "그걸로 게임하는거 보는데 그냥 패드로 게임하는 것 같은 엄청 큰 화면 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007464.1.22",
          "form": "플립은 게임할때 중간에 접혀서 불편하다던데",
          "original_form": "플립은 게임할때 중간에 접혀서 불편하다던데",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007464.1.23",
          "form": "폴드는 덜 그런가?",
          "original_form": "폴드는 덜 그런가?",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007464.1.24",
          "form": "여튼 부럽다 무슨 색깔로 샀대?",
          "original_form": "여튼 부럽다 무슨 색깔로 샀대?",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007464.1.25",
          "form": "제 친구는 블랙 좋아해서",
          "original_form": "제 친구는 블랙 좋아해서",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007464.1.26",
          "form": "묻지도 따지지도 않고",
          "original_form": "묻지도 따지지도 않고",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007464.1.27",
          "form": "블랙샀어요 ㅋㅋㅋㅋㅋㅋ 예쁘더만요",
          "original_form": "블랙샀어요 ㅋㅋㅋㅋㅋㅋ 예쁘더만요",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007464.1.28",
          "form": "ㅋㅋㅋㅋㅋ 개인적으로 삼성 스마트폰은",
          "original_form": "ㅋㅋㅋㅋㅋ 개인적으로 삼성 스마트폰은",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007464.1.29",
          "form": "블랙 화이트 아니면 색을 미묘하게 뽑는거같아서",
          "original_form": "블랙 화이트 아니면 색을 미묘하게 뽑는거같아서",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007464.1.30",
          "form": "블랙 화이트가 짱이긴 해",
          "original_form": "블랙 화이트가 짱이긴 해",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007464.1.31",
          "form": "실버를 너무 못뽑는 것 같아요",
          "original_form": "실버를 너무 못뽑는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007464.1.32",
          "form": "솔직히 말해서  ...",
          "original_form": "솔직히 말해서  ...",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007464.1.33",
          "form": "그래도 이번 그린은 괜찮더라구요",
          "original_form": "그래도 이번 그린은 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007464.1.34",
          "form": "다른 색깔들 뭐뭐 있나 보고왔는데",
          "original_form": "다른 색깔들 뭐뭐 있나 보고왔는데",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007464.1.35",
          "form": "블랙이 가장 잘 나왔도 라벤더랑 그린도 무난한데",
          "original_form": "블랙이 가장 잘 나왔도 라벤더랑 그린도 무난한데",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007464.1.36",
          "form": "크림색이 십년된 에어컨 실외기 색깔이네... 좀 더 예쁘게 뽑을 수 있었을텐데 ㅋㅋㅋ",
          "original_form": "크림색이 십년된 에어컨 실외기 색깔이네... 좀 더 예쁘게 뽑을 수 있었을텐데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007464.1.37",
          "form": "그니까요 ㅋㅋㅋㅋㅋㅋ 크림 너무 별로야...",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋㅋ 크림 너무 별로야...",
          "speaker_id": "1",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007464.1.38",
          "form": "라벤더는 예뻐서 안그래도 인기 많더라구요",
          "original_form": "라벤더는 예뻐서 안그래도 인기 많더라구요",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007464.1.39",
          "form": "근데 사실 블랙이나 화이트가 제일 안 질리는 색인 것 같아요...",
          "original_form": "근데 사실 블랙이나 화이트가 제일 안 질리는 색인 것 같아요...",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007464.1.40",
          "form": "맞아맞아 취향 호불호도 크게 안갈리고",
          "original_form": "맞아맞아 취향 호불호도 크게 안갈리고",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007464.1.41",
          "form": "질리면 케이스를 바꾸면 되니까 ㅋㅋㅋ",
          "original_form": "질리면 케이스를 바꾸면 되니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:21"
        }
      ]
