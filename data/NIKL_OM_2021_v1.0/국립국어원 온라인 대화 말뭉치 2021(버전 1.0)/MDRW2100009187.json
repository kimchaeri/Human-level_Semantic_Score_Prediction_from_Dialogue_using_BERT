      "utterance": [
        {
          "id": "MDRW2100009187.1.1",
          "form": "좋아하는 차 종류 있으세여~?",
          "original_form": "좋아하는 차 종류 있으세여~?",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009187.1.2",
          "form": "저는 최근에 자몽티에빠졌어요",
          "original_form": "저는 최근에 자몽티에빠졌어요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009187.1.3",
          "form": "자몽이맛잇더라구요",
          "original_form": "자몽이맛잇더라구요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009187.1.4",
          "form": "오! 자몽 맛있어요~~ 저는 청귤차! ㅋㅋ",
          "original_form": "오! 자몽 맛있어요~~ 저는 청귤차! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009187.1.5",
          "form": "아는분이 청귤차를 담아주셨는데 너무 맛있더라구요~~~",
          "original_form": "아는분이 청귤차를 담아주셨는데 너무 맛있더라구요~~~",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009187.1.6",
          "form": "그래서 지난주에 엄마랑 같이 청귤차 담았어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 지난주에 엄마랑 같이 청귤차 담았어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009187.1.7",
          "form": "우와 청귤차는안먹어봤어요",
          "original_form": "우와 청귤차는안먹어봤어요",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009187.1.8",
          "form": "귤이랑 다른가요?",
          "original_form": "귤이랑 다른가요?",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009187.1.9",
          "form": "새콤하려나",
          "original_form": "새콤하려나",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009187.1.10",
          "form": "새콤 쌉쌀 달콤!",
          "original_form": "새콤 쌉쌀 달콤!",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009187.1.11",
          "form": "약간 유자차 비슷한데 유자차보다 맛있어욤",
          "original_form": "약간 유자차 비슷한데 유자차보다 맛있어욤",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009187.1.12",
          "form": "유자차보다 더 상큼!",
          "original_form": "유자차보다 더 상큼!",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009187.1.13",
          "form": "청귤차 일단 파는걸먼저먹어봐야겟어요",
          "original_form": "청귤차 일단 파는걸먼저먹어봐야겟어요",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009187.1.14",
          "form": "궁금해졌어요ㅎㅎ",
          "original_form": "궁금해졌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009187.1.15",
          "form": "맛있어요~ ㅋㅋㅋ 저는 유자차 좋아하는데 유자차보다 맛있었어요!",
          "original_form": "맛있어요~ ㅋㅋㅋ 저는 유자차 좋아하는데 유자차보다 맛있었어요!",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009187.1.16",
          "form": "몰랐는데 청귤차 많이 팔더라구요!!",
          "original_form": "몰랐는데 청귤차 많이 팔더라구요!!",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009187.1.17",
          "form": "드셔보세용 ㅋㅋ",
          "original_form": "드셔보세용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009187.1.18",
          "form": "커피 종류는 안드세요?",
          "original_form": "커피 종류는 안드세요?",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009187.1.19",
          "form": "커피는아메리카노만마셔요",
          "original_form": "커피는아메리카노만마셔요",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009187.1.20",
          "form": "오! 아이스로 드세요? 아님 뜨아?",
          "original_form": "오! 아이스로 드세요? 아님 뜨아?",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009187.1.21",
          "form": "얼죽아입니닷",
          "original_form": "얼죽아입니닷",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009187.1.22",
          "form": "얼죽아 뭐니뭐니해도 아이스가 최고죠! ㅋㅋㅋ",
          "original_form": "얼죽아 뭐니뭐니해도 아이스가 최고죠! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009187.1.23",
          "form": "저는 라떼 매니아입니당 ㅋㅋ",
          "original_form": "저는 라떼 매니아입니당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009187.1.24",
          "form": "아아는 한여름에 주로 땡기는거 같아용ㄹ",
          "original_form": "아아는 한여름에 주로 땡기는거 같아용ㄹ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009187.1.25",
          "form": "아무리 라떼가 좋아도 너무 더울땐 텁텁함도 싫더라구요 ㅎㅎ",
          "original_form": "아무리 라떼가 좋아도 너무 더울땐 텁텁함도 싫더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009187.1.26",
          "form": "맞아요 저 라떼는 달고나라떼먹어봣어요ㅋㅋ당땡길때",
          "original_form": "맞아요 저 라떼는 달고나라떼먹어봣어요ㅋㅋ당땡길때",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009187.1.27",
          "form": "아 ㅋㅋㅋ 작년에 핫했던 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ 작년에 핫했던 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009187.1.28",
          "form": "저는 그냥 라떼 좋아해서 안먹어봤는데 엄청 핫했잖아요~ ㅎㅎ",
          "original_form": "저는 그냥 라떼 좋아해서 안먹어봤는데 엄청 핫했잖아요~ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:08"
        }
      ]
