        },
        {
          "id": "MDRW2100015157.1.2",
          "form": "심즈도 팩이비싸군요",
          "original_form": "심즈도 팩이비싸군요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015157.1.3",
          "form": "네 그리고 용량도 커서 컴이 버벅거려요ㅠ",
          "original_form": "네 그리고 용량도 커서 컴이 버벅거려요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015157.1.4",
          "form": "ㅌㅌ",
          "original_form": "ㅌㅌ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015157.1.5",
          "form": "데스크탑으로만 되나요?",
          "original_form": "데스크탑으로만 되나요?",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015157.1.6",
          "form": "넵넵 컴사양도 좋아야해요 자꾸 팅겨서",
          "original_form": "넵넵 컴사양도 좋아야해요 자꾸 팅겨서",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015157.1.7",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015157.1.8",
          "form": "게임하기힘들겠어요",
          "original_form": "게임하기힘들겠어요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015157.1.9",
          "form": "넵 그래서 전 컴사양때문에 포기했답니다ㅠㅠ",
          "original_form": "넵 그래서 전 컴사양때문에 포기했답니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015157.1.10",
          "form": "ㅠ 보통이아닌게임이군요",
          "original_form": "ㅠ 보통이아닌게임이군요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015157.1.11",
          "form": "전 카트정도라서",
          "original_form": "전 카트정도라서",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015157.1.12",
          "form": "게임하고싶네요ㅎㅎ오랜만에",
          "original_form": "게임하고싶네요ㅎㅎ오랜만에",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015157.1.13",
          "form": "ㅋㅋㅋ어떤걸로요?",
          "original_form": "ㅋㅋㅋ어떤걸로요?",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015157.1.14",
          "form": "음 뭔가 롤플레잉같은거요ㅎ",
          "original_form": "음 뭔가 롤플레잉같은거요ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015157.1.15",
          "form": "오딘은 어떤가요",
          "original_form": "오딘은 어떤가요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015157.1.16",
          "form": "온라인게임인가요?",
