      "utterance": [
        {
          "id": "MDRW2100025404.1.1",
          "form": "중고거래 좋아하세요?",
          "original_form": "중고거래 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025404.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025404.1.3",
          "form": "저당근마켓 주로 이용합니다",
          "original_form": "저당근마켓 주로 이용합니다",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025404.1.4",
          "form": "오 주로 어떤 제품 구매하세요??",
          "original_form": "오 주로 어떤 제품 구매하세요??",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025404.1.5",
          "form": "저는 휴대폰 구매만 해봤어요!!!",
          "original_form": "저는 휴대폰 구매만 해봤어요!!!",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100025404.1.6",
          "form": "파는것도 해보셧오요???",
          "original_form": "파는것도 해보셧오요???",
          "speaker_id": "1",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025404.1.7",
          "form": "네네 저 최근에에어팟 팔았어요",
          "original_form": "네네 저 최근에에어팟 팔았어요",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025404.1.8",
          "form": "오오 금방 팔리셧어요??",
          "original_form": "오오 금방 팔리셧어요??",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025404.1.9",
          "form": "저는 올려도 구매자가 없드라고요 ㅋㅋ",
          "original_form": "저는 올려도 구매자가 없드라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025404.1.10",
          "form": "저도 늦게 팔리는편이더라구요 ㅠㅠ",
          "original_form": "저도 늦게 팔리는편이더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025404.1.11",
          "form": "주로 머 파시나요??",
          "original_form": "주로 머 파시나요??",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025404.1.12",
          "form": "저는 자주는 못하고 가방 올렸는데 ㅜㅜ 안팔려서 내렸어요 ㅋㅋ",
          "original_form": "저는 자주는 못하고 가방 올렸는데 ㅜㅜ 안팔려서 내렸어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025404.1.13",
          "form": "맞어요 안팔리면 민망해지더라구요..ㅎ",
          "original_form": "맞어요 안팔리면 민망해지더라구요..ㅎ",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025404.1.14",
          "form": "직거래로 하시나용?",
          "original_form": "직거래로 하시나용?",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025404.1.15",
          "form": "네 먼가 택배거래는 무서워서요 ㅠㅠ",
          "original_form": "네 먼가 택배거래는 무서워서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025404.1.16",
          "form": "오오 직거래가 안전하죠 ㅠㅜ",
