          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.30",
          "form": "였었어요!!",
          "original_form": "였었어요!!",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.31",
          "form": "거기에",
          "original_form": "거기에",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.32",
          "form": "전부...엔시티즌",
          "original_form": "전부...엔시티즌",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.33",
          "form": "근데 그 애들 다 대포들고 있는데",
          "original_form": "근데 그 애들 다 대포들고 있는데",
          "speaker_id": "2",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.34",
          "form": "사다리 큼직한거 들고",
          "original_form": "사다리 큼직한거 들고",
          "speaker_id": "2",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.35",
          "form": "달리잖아요",
          "original_form": "달리잖아요",
          "speaker_id": "2",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.36",
          "form": "얼굴에 나재민 이런 슬로건 덮고 주무시는 분들",
          "original_form": "얼굴에 {emoji:♡}나재민{emoji:♡} 이런 슬로건 덮고 주무시는 분들",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.37",
          "form": "이게.... 진짜",
          "original_form": "이게.... 진짜",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.38",
          "form": "원아페가 진짜",
          "original_form": "원아페가 진짜",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.39",
          "form": "찐 무서움",
          "original_form": "찐 무서움",
          "speaker_id": "2",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.40",
          "form": "헬이었거든요",
          "original_form": "헬이었거든요",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.41",
          "form": "엑소 워너원 엔시티",
          "original_form": "엑소 워너원 엔시티",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.42",
          "form": "지옥시을 겪으셨군요",
          "original_form": "지옥시을 겪으셨군요",
          "speaker_id": "2",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009680.1.43",
          "form": "나왔어서 스탠딩이",
          "original_form": "나왔어서 스탠딩이",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
