      "utterance": [
        {
          "id": "MDRW2100009680.1.1",
          "form": "근데 그 사람들 뭔가 지금 다 엔시티 좋아할 것 겉아요",
          "original_form": "근데 그 사람들 뭔가 지금 다 엔시티 좋아할 것 겉아요",
          "speaker_id": "2",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.2",
          "form": "내 돌 앞에서는",
          "original_form": "내 돌 앞에서는",
          "speaker_id": "1",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.4",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.5",
          "form": "아... 공감합니다",
          "original_form": "아... 공감합니다",
          "speaker_id": "1",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.6",
          "form": "격하게",
          "original_form": "격하게",
          "speaker_id": "1",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.8",
          "form": "아육대같은 건 참여해보셨나요",
          "original_form": "아육대같은 건 참여해보셨나요",
          "speaker_id": "2",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.9",
          "form": "SM은",
          "original_form": "SM은",
          "speaker_id": "1",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.10",
          "form": "그대로 내려가더라고요",
          "original_form": "그대로 내려가더라고요",
          "speaker_id": "1",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.11",
          "form": "셉틴..한번 쯤은 나왔을 것 같은데",
          "original_form": "셉틴..한번 쯤은 나왔을 것 같은데",
          "speaker_id": "2",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.13",
          "form": "내리사랑이라는데",
          "original_form": "내리사랑이라는데",
          "speaker_id": "2",
          "time": "20210823 03:12"
        },
        {
          "id": "MDRW2100009680.1.14",
          "form": "팬들 점점 고인물되고 쌈닭되는",
          "original_form": "팬들 점점 고인물되고 쌈닭되는",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.15",
          "form": "세븐틴은 여러번 나왔었던 거 같은데",
          "original_form": "세븐틴은 여러번 나왔었던 거 같은데",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.16",
          "form": "그런 시스템이더라고요",
          "original_form": "그런 시스템이더라고요",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.17",
          "form": "아 근데 진짜",
          "original_form": "아 근데 진짜",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.18",
          "form": "저 부산에",
          "original_form": "저 부산에",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.19",
          "form": "서가대 갔었는데",
          "original_form": "서가대 갔었는데",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.21",
          "form": "엔시티즌 진짜 기세더라고요",
          "original_form": "엔시티즌 진짜 기세더라고요",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.23",
          "form": "아니지 서가대는",
          "original_form": "아니지 서가대는",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.24",
          "form": "건들면 안돼요",
          "original_form": "건들면 안돼요",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.25",
          "form": "서울... 인데",
          "original_form": "서울... 인데",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.26",
          "form": "부산 페스티벌인가요",
          "original_form": "부산 페스티벌인가요",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.27",
          "form": "부산... 원아페",
          "original_form": "부산... 원아페",
          "speaker_id": "1",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.28",
          "form": "뭔가 그런",
          "original_form": "뭔가 그런",
          "speaker_id": "2",
          "time": "20210823 03:13"
        },
        {
          "id": "MDRW2100009680.1.29",
