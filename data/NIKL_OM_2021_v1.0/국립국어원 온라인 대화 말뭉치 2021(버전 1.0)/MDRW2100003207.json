      "utterance": [
        {
          "id": "MDRW2100003207.1.1",
          "form": "주말부터 비소식이 있나봐요",
          "original_form": "주말부터 비소식이 있나봐요",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003207.1.2",
          "form": "네 늦은장마가 시작된데요 ㅜㅠ",
          "original_form": "네 늦은장마가 시작된데요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003207.1.3",
          "form": "비오는거 싫은데 비오고 모기가 기승을 부리겠어요",
          "original_form": "비오는거 싫은데 비오고 모기가 기승을 부리겠어요",
          "speaker_id": "1",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003207.1.4",
          "form": "날도 선선해져서",
          "original_form": "날도 선선해져서",
          "speaker_id": "1",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003207.1.5",
          "form": "그래서 본사에서 지시공문내려왔어요",
          "original_form": "그래서 본사에서 지시공문내려왔어요",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003207.1.6",
          "form": "대비하라고 ㅋ",
          "original_form": "대비하라고 ㅋ",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003207.1.7",
          "form": "비님이 오신다는데 우리가 막을수도 없고",
          "original_form": "비님이 오신다는데 우리가 막을수도 없고",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003207.1.8",
          "form": "모기를요?기",
          "original_form": "모기를요?기",
          "speaker_id": "1",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003207.1.9",
          "form": "아뇨 비많이 온다고 장마대비하래요 ㅎ",
          "original_form": "아뇨 비많이 온다고 장마대비하래요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 20:36"
        },
        {
          "id": "MDRW2100003207.1.10",
          "form": "아ㅎ 많이온대요?",
          "original_form": "아ㅎ 많이온대요?",
          "speaker_id": "1",
          "time": "20210820 20:36"
        },
        {
          "id": "MDRW2100003207.1.11",
          "form": "네 일본이랑 중국 난리라는데요",
          "original_form": "네 일본이랑 중국 난리라는데요",
          "speaker_id": "2",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003207.1.12",
          "form": "비가 많이 와서 피해가 많은가봐요",
          "original_form": "비가 많이 와서 피해가 많은가봐요",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003207.1.13",
          "form": "홍수나고 그렇더라구요",
          "original_form": "홍수나고 그렇더라구요",
          "speaker_id": "1",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003207.1.14",
          "form": "일본은 천미리씩왔데요",
          "original_form": "일본은 천미리씩왔데요",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003207.1.15",
          "form": "헐 천미리요?",
          "original_form": "헐 천미리요?",
          "speaker_id": "1",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003207.1.16",
          "form": "일주일내내 와서 지반도 약해졌다고 심각한가봐요",
          "original_form": "일주일내내 와서 지반도 약해졌다고 심각한가봐요",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003207.1.17",
          "form": "지구가 난리네요 홍수에 산불에",
          "original_form": "지구가 난리네요 홍수에 산불에",
          "speaker_id": "1",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003207.1.18",
          "form": "천미리는 심한거같은데 우산써도 다 젖을듯해요",
          "original_form": "천미리는 심한거같은데 우산써도 다 젖을듯해요",
          "speaker_id": "2",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003207.1.19",
          "form": "진짜 재앙이 오려는지 ㅎㅎ",
          "original_form": "진짜 재앙이 오려는지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003207.1.20",
          "form": "무서워요 ㅎㅎ",
          "original_form": "무서워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:43"
        }
