      "utterance": [
        {
          "id": "MDRW2100018810.1.1",
          "form": "비가 내리네요",
          "original_form": "비가 내리네요",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018810.1.2",
          "form": "여긴 잠시 멈췄어요 가을장마라 계속 날씨에 비가 들어 있네요",
          "original_form": "여긴 잠시 멈췄어요 가을장마라 계속 날씨에 비가 들어 있네요",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018810.1.3",
          "form": "그러게요 가을장마는 약간 낯서네요",
          "original_form": "그러게요 가을장마는 약간 낯서네요",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018810.1.4",
          "form": "맞아요 장마하면 주로 여름이라는 느낌이죠",
          "original_form": "맞아요 장마하면 주로 여름이라는 느낌이죠",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018810.1.5",
          "form": "작년처럼 길면 안 되는데 말이에요",
          "original_form": "작년처럼 길면 안 되는데 말이에요",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018810.1.6",
          "form": "맞아요 진작 끝났어야하는데 말이에요",
          "original_form": "맞아요 진작 끝났어야하는데 말이에요",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018810.1.7",
          "form": "빨리 지나가길 바라고 있어요ㅜ",
          "original_form": "빨리 지나가길 바라고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018810.1.8",
          "form": "저도요 장마철에는 컨디션이 별로에요ㅠ",
          "original_form": "저도요 장마철에는 컨디션이 별로에요ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018810.1.9",
          "form": "빨리 좋은 날씨가 되면 좋겠어요",
          "original_form": "빨리 좋은 날씨가 되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018810.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018810.1.11",
          "form": "비오니 축 쳐지기만해요",
          "original_form": "비오니 축 쳐지기만해요",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018810.1.12",
          "form": "맞아요 그래서 전 비 올 때는 따뜻한 음식을 먹어요",
          "original_form": "맞아요 그래서 전 비 올 때는 따뜻한 음식을 먹어요",
          "speaker_id": "1",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018810.1.13",
          "form": "오 좋네요! 저는 찬거만 먹게되네요ㅠ",
          "original_form": "오 좋네요! 저는 찬거만 먹게되네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018810.1.14",
          "form": "저도 찬 것 좋아했어요 지금은 사계절 따뜻하게 먹으려고 노력해요ㅠㅋㅋ",
          "original_form": "저도 찬 것 좋아했어요 지금은 사계절 따뜻하게 먹으려고 노력해요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100018810.1.15",
          "form": "크으ㅠ 힘들죠ㅠ",
          "original_form": "크으ㅠ 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100018810.1.16",
          "form": "미지근한 물 마시는게 제일 힘들어요",
          "original_form": "미지근한 물 마시는게 제일 힘들어요",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100018810.1.17",
          "form": "물 마시는 것 환절기에 딱 좋죠 평소에도 그렇지만요",
          "original_form": "물 마시는 것 환절기에 딱 좋죠 평소에도 그렇지만요",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018810.1.18",
          "form": "다시 비가 내리네요",
          "original_form": "다시 비가 내리네요",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018810.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018810.1.20",
          "form": "비와서 그런지 좀 춥네요 환절기라 한여름에 비오는 거랑 다르네요",
          "original_form": "비와서 그런지 좀 춥네요 환절기라 한여름에 비오는 거랑 다르네요",
          "speaker_id": "1",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018810.1.21",
          "form": "그러게요 이럴때 옷입는게 참 힘들어요",
          "original_form": "그러게요 이럴때 옷입는게 참 힘들어요",
          "speaker_id": "2",
          "time": "20210827 13:19"
        }
      ]
