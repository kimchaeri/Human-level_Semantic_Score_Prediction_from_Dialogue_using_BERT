      "utterance": [
        {
          "id": "MDRW2100009131.1.1",
          "form": "요즘 코로나라 뭐든 쉽지않죠",
          "original_form": "요즘 코로나라 뭐든 쉽지않죠",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009131.1.2",
          "form": "아르바이트 찾는것도",
          "original_form": "아르바이트 찾는것도",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009131.1.3",
          "form": "맞아요 버티는게 제일인거같아요",
          "original_form": "맞아요 버티는게 제일인거같아요",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009131.1.4",
          "form": "요새 실업급여도 많이 준다고 하긴하더라고요",
          "original_form": "요새 실업급여도 많이 준다고 하긴하더라고요",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009131.1.5",
          "form": "액수가 늘었다고",
          "original_form": "액수가 늘었다고",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009131.1.6",
          "form": "아무래도 상황이 그렇네요ㅠㅠ",
          "original_form": "아무래도 상황이 그렇네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009131.1.7",
          "form": "요새 키오스크가 늘어서 ㅜ",
          "original_form": "요새 키오스크가 늘어서 ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009131.1.8",
          "form": "일자리 자체가 많이 줄어들었죠",
          "original_form": "일자리 자체가 많이 줄어들었죠",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009131.1.9",
          "form": "캐셔도 많이 필요없고ㅜ",
          "original_form": "캐셔도 많이 필요없고ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009131.1.10",
          "form": "아 서빙도 로봇이 하더라고요",
          "original_form": "아 서빙도 로봇이 하더라고요",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009131.1.11",
          "form": "맞아요 보면서 신기라기도 하고 벌써 그런 시대가 왔구나 싶구",
          "original_form": "맞아요 보면서 신기라기도 하고 벌써 그런 시대가 왔구나 싶구",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009131.1.12",
          "form": "곧 진짜 그런 시대가 와야할지도요",
          "original_form": "곧 진짜 그런 시대가 와야할지도요",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009131.1.13",
          "form": "그냥 살아있으면 나라에서돈주는?",
          "original_form": "그냥 살아있으면 나라에서돈주는?",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009131.1.14",
          "form": "무섭네요ㅎㅎ 일을 안햐도 되는 사회",
          "original_form": "무섭네요ㅎㅎ 일을 안햐도 되는 사회",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009131.1.15",
          "form": "근데 건물주들은 이미 그러고있으니 음",
          "original_form": "근데 건물주들은 이미 그러고있으니 음",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009131.1.16",
          "form": "ㅎㅎ그건 뭐 시대불명..ㅋㅋㅋ",
          "original_form": "ㅎㅎ그건 뭐 시대불명..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009131.1.17",
          "form": "소득격차 없애는건 어느시대든 사실 불가능했던거 같기도해욥 ㅋㅋ",
          "original_form": "소득격차 없애는건 어느시대든 사실 불가능했던거 같기도해욥 ㅋㅋ",
