      "utterance": [
        {
          "id": "MDRW2100038940.1.1",
          "form": "안녕하세요\r\n반려동물 키우시나요?",
          "original_form": "안녕하세요\r\n반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100038940.1.2",
          "form": "아니요ㅠ 키워본적도 없어요",
          "original_form": "아니요ㅠ 키워본적도 없어요",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100038940.1.3",
          "form": "앗 그리시군요ㅜㅜ 저도 키워본적 없어요ㅠㅠ",
          "original_form": "앗 그리시군요ㅜㅜ 저도 키워본적 없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100038940.1.4",
          "form": "그럼 평소에 동물 좋아하시나요?",
          "original_form": "그럼 평소에 동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100038940.1.5",
          "form": "동물은 엄청 좋아해요",
          "original_form": "동물은 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038940.1.6",
          "form": "그래서 영상 엄청 찾아봐요",
          "original_form": "그래서 영상 엄청 찾아봐요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038940.1.7",
          "form": "오 어떤 동물 좋아하세요?",
          "original_form": "오 어떤 동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038940.1.8",
          "form": "웰시코기 제일 좋아해요",
          "original_form": "웰시코기 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100038940.1.9",
          "form": "아! 강아지 좋아하시는군요",
          "original_form": "아! 강아지 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100038940.1.10",
          "form": "웰시코기 너무 귀엽죠!",
          "original_form": "웰시코기 너무 귀엽죠!",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100038940.1.11",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100038940.1.12",
          "form": "웃는 얼굴이랑 빵실한 궁디...",
          "original_form": "웃는 얼굴이랑 빵실한 궁디...",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100038940.1.13",
          "form": "상상만해도 웃음이나죠ㅎㅎ",
          "original_form": "상상만해도 웃음이나죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100038940.1.14",
          "form": "걷는 모습도 넘 귀엽고ㅎㅎ",
          "original_form": "걷는 모습도 넘 귀엽고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038940.1.15",
          "form": "다리가 짧으니 퐁퐁 걸어다니죠",
          "original_form": "다리가 짧으니 퐁퐁 걸어다니죠",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038940.1.16",
          "form": "맞아요!  그 모습이 매력이죠",
          "original_form": "맞아요!  그 모습이 매력이죠",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100038940.1.17",
          "form": "ㅎㅎ 보기만 해도 행복해져요",
          "original_form": "ㅎㅎ 보기만 해도 행복해져요",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100038940.1.18",
          "form": "웰시코기 보면서 힐링하시는거네요ㅎㅎ",
          "original_form": "웰시코기 보면서 힐링하시는거네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100038940.1.19",
          "form": "네ㅎㅎ name1님은 동물으로 힐링해보신적 있으세요?",
          "original_form": "네ㅎㅎ &name1&님은 동물으로 힐링해보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038940.1.20",
          "form": "저는 특별히 보면서 힐링하는 동물이 정해져있는건아니지만",
          "original_form": "저는 특별히 보면서 힐링하는 동물이 정해져있는건아니지만",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038940.1.21",
          "form": "귀여운 동물들 보는거 좋아해요!\r\n자연스레 기분 좋아지더라구요\r\n그게 힐링인거겠지만요ㅎㅎ",
          "original_form": "귀여운 동물들 보는거 좋아해요!\r\n자연스레 기분 좋아지더라구요\r\n그게 힐링인거겠지만요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038940.1.22",
          "form": "ㅎㅎ 귀여운 친구들이 가진 힘이 있죠",
          "original_form": "ㅎㅎ 귀여운 친구들이 가진 힘이 있죠",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038940.1.23",
          "form": "맞아요!  그래서 소품들이나 물건들 고를때도 귀여운 동물들이 그려져있거나 동물들 캐릭터들에 눈길이 한 번씩 더 가곤 하더라구요",
          "original_form": "맞아요!  그래서 소품들이나 물건들 고를때도 귀여운 동물들이 그려져있거나 동물들 캐릭터들에 눈길이 한 번씩 더 가곤 하더라구요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038940.1.24",
          "form": "맞아요오 그래서 얼마전에 동물인형 하나 찜해놨어요",
          "original_form": "맞아요오 그래서 얼마전에 동물인형 하나 찜해놨어요",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100038940.1.25",
          "form": "오오!  어떤 동물인형인가요ㅉ",
          "original_form": "오오!  어떤 동물인형인가요ㅉ",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100038940.1.26",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100038940.1.27",
          "form": "알파카요! 겨울에 안고자면 좋을거 같아서요",
          "original_form": "알파카요! 겨울에 안고자면 좋을거 같아서요",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038940.1.28",
          "form": "오!ㄱ계절과도 잘 어울리는 선택이에요",
          "original_form": "오!ㄱ계절과도 잘 어울리는 선택이에요",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100038940.1.29",
          "form": "그래서 여름에는 구경만 했죠ㅋㅋ",
          "original_form": "그래서 여름에는 구경만 했죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038940.1.30",
          "form": "ㅋㅋㅋㅋ 현명하신 선택이네요",
          "original_form": "ㅋㅋㅋㅋ 현명하신 선택이네요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038940.1.31",
          "form": "전 오늘 귀여운 다람쥐 캐릭터 물건을 하나 구매했어요ㅋㅋ",
          "original_form": "전 오늘 귀여운 다람쥐 캐릭터 물건을 하나 구매했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:10"
        }
      ]
