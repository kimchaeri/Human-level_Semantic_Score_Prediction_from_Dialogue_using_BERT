      "utterance": [
        {
          "id": "MDRW2100023624.1.1",
          "form": "요즘은 반팔입으세용??",
          "original_form": "요즘은 반팔입으세용??",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023624.1.2",
          "form": "반팔입고 가디건 들고다녀요",
          "original_form": "반팔입고 가디건 들고다녀요",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100023624.1.3",
          "form": "오오 저는 가디건 귀찮아서",
          "original_form": "오오 저는 가디건 귀찮아서",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100023624.1.4",
          "form": "저녁에는 좀 춥고 사무실 에어컨도 세고",
          "original_form": "저녁에는 좀 춥고 사무실 에어컨도 세고",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100023624.1.5",
          "form": "그냥 긴팔 하나로 퉁치는데",
          "original_form": "그냥 긴팔 하나로 퉁치는데",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100023624.1.6",
          "form": "어쩔땐 덥고 어쩔땐 추워요ㅠㅠㅋㅋㅋ",
          "original_form": "어쩔땐 덥고 어쩔땐 추워요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100023624.1.7",
          "form": "그건 그래요.",
          "original_form": "그건 그래요.",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100023624.1.8",
          "form": "일교차가 너무 크죠",
          "original_form": "일교차가 너무 크죠",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100023624.1.9",
          "form": "넹넹ㅋㅋㅋ 이불은 바꾸셨어요?",
          "original_form": "넹넹ㅋㅋㅋ 이불은 바꾸셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100023624.1.10",
          "form": "아뇨.  아직은 얇은 홑이불 써요",
          "original_form": "아뇨.  아직은 얇은 홑이불 써요",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100023624.1.11",
          "form": "저도 아직은 이불은 그대로에요",
          "original_form": "저도 아직은 이불은 그대로에요",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023624.1.12",
          "form": "또 언제 더워질지 몰라서요ㅋㅋㅋ",
          "original_form": "또 언제 더워질지 몰라서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023624.1.13",
          "form": "아마 한 10월 되면 이불 바꿀것 같네요",
          "original_form": "아마 한 10월 되면 이불 바꿀것 같네요",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023624.1.14",
          "form": "엇 생각보다 늦게 바꾸시네용",
          "original_form": "엇 생각보다 늦게 바꾸시네용",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023624.1.15",
          "form": "그때쯤이면 많이 추울텐데요ㅠㅠ",
          "original_form": "그때쯤이면 많이 추울텐데요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023624.1.16",
          "form": "집이 외풍드는 집은 아니고 걍 잠옷을 길게 입으려구요",
          "original_form": "집이 외풍드는 집은 아니고 걍 잠옷을 길게 입으려구요",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023624.1.17",
          "form": "지금 있는곳은 해쨍쨍한데",
          "original_form": "지금 있는곳은 해쨍쨍한데",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023624.1.18",
          "form": "name2 계신곳은 지금 어떠세요?",
          "original_form": "&name2& 계신곳은 지금 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023624.1.19",
          "form": "제가 있는곳은 쨍쨍까진 아니고 구름이 좀 꼈어요",
          "original_form": "제가 있는곳은 쨍쨍까진 아니고 구름이 좀 꼈어요",
          "speaker_id": "2",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100023624.1.20",
          "form": "조금있으면 비가 오겠네요ㅠ",
          "original_form": "조금있으면 비가 오겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023624.1.21",
          "form": "아, 이번주 비온댔죠",
          "original_form": "아, 이번주 비온댔죠",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023624.1.22",
          "form": "넹넹 이번주 내내요ㅠㅠ",
          "original_form": "넹넹 이번주 내내요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023624.1.23",
          "form": "그래서 우울했는데 해가 짱짱해서",
          "original_form": "그래서 우울했는데 해가 짱짱해서",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023624.1.24",
          "form": "그나마 기분이 괜찮아요ㅎㅎ",
          "original_form": "그나마 기분이 괜찮아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023624.1.25",
          "form": "전 해가 쨍쨍한 날씨보단 조금 흐린게 좋아요",
          "original_form": "전 해가 쨍쨍한 날씨보단 조금 흐린게 좋아요",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023624.1.26",
          "form": "눈부신거 싫어해서 그런가",
          "original_form": "눈부신거 싫어해서 그런가",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100023624.1.27",
          "form": "구름낀날도 차분한 느낌이 들도 매력적이긴 한것같아용!",
          "original_form": "구름낀날도 차분한 느낌이 들도 매력적이긴 한것같아용!",
          "speaker_id": "1",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023624.1.28",
          "form": "잠도 잘오고ㅋㅋㅋ",
          "original_form": "잠도 잘오고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:42"
        }
      ]
    }
