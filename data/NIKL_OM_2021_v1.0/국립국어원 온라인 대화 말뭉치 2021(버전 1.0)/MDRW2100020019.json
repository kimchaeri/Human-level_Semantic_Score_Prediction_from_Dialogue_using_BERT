        },
        {
          "id": "MDRW2100020019.1.2",
          "form": "나지금 오월의청춘봄",
          "original_form": "나지금 오월의청춘봄",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020019.1.3",
          "form": "슬기로운 의사생활",
          "original_form": "슬기로운 의사생활",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020019.1.4",
          "form": "아ㅜㅜ",
          "original_form": "아ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020019.1.5",
          "form": "그건 슬퍼서 못보겠어",
          "original_form": "그건 슬퍼서 못보겠어",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020019.1.6",
          "form": "결말이 정해진건데도 막 안그랬음좋겠다 이런마음으로 봄...",
          "original_form": "결말이 정해진건데도 막 안그랬음좋겠다 이런마음으로 봄...",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020019.1.7",
          "form": "슬프고 처절해서 못보겠어ㅜㅜ",
          "original_form": "슬프고 처절해서 못보겠어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020019.1.8",
          "form": "광주 지역 분들은 이거진짜 엄청 슬퍼하며 보셨다더라 ㅜㅜ",
          "original_form": "광주 지역 분들은 이거진짜 엄청 슬퍼하며 보셨다더라 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020019.1.9",
          "form": "그랬겠다ㅜㅜ 불과 얼마안됐잖아",
          "original_form": "그랬겠다ㅜㅜ 불과 얼마안됐잖아",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020019.1.10",
          "form": "그니까 ㅜㅜ 근데 슬퍼도 자꾸 역사적 소재를 가지고 영상매체 만드는건 의미있는거같아",
          "original_form": "그니까 ㅜㅜ 근데 슬퍼도 자꾸 역사적 소재를 가지고 영상매체 만드는건 의미있는거같아",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020019.1.11",
          "form": "그건그렇지 그 남주때문에 애들도 많이 본것같던데",
          "original_form": "그건그렇지 그 남주때문에 애들도 많이 본것같던데",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020019.1.12",
          "form": "이도현인가?",
          "original_form": "이도현인가?",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020019.1.13",
          "form": "이 뭐기기 였던거같은데 모르겠다",
          "original_form": "이 뭐기기 였던거같은데 모르겠다",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020019.1.14",
          "form": "ㅇㅇ나도 도현만 알겠엌ㅋ",
          "original_form": "ㅇㅇ나도 도현만 알겠엌ㅋ",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020019.1.15",
          "form": "이도현인지 김도현인지",
          "original_form": "이도현인지 김도현인지",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020019.1.16",
          "form": "중요치않어...",
          "original_form": "중요치않어...",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020019.1.17",
          "form": "역사왜곡없이만 만들면 되야..",
          "original_form": "역사왜곡없이만 만들면 되야..",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020019.1.18",
          "form": "ㅋㅋㅋㅋㅋ말투왜그래ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ말투왜그래ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020019.1.19",
          "form": "광주드라마보는중이라섴ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "광주드라마보는중이라섴ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020019.1.20",
          "form": "말투가 잘 옮는 타입 ㅋㅋㅋㅋ",
          "original_form": "말투가 잘 옮는 타입 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020019.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ충청도인줄ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ충청도인줄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020019.1.22",
          "form": "충청도인가 ㅋㅋㅋㅋ",
          "original_form": "충청도인가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020019.1.23",
          "form": "나 그제 무한도전에서 더빙하는거 봤는데",
          "original_form": "나 그제 무한도전에서 더빙하는거 봤는데",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020019.1.24",
          "form": "충청도인거같네 ㅋㅋㅋㅋ",
          "original_form": "충청도인거같네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020019.1.25",
          "form": "더빙말투 따라하게됨ㅋㅋ",
          "original_form": "더빙말투 따라하게됨ㅋㅋ",
