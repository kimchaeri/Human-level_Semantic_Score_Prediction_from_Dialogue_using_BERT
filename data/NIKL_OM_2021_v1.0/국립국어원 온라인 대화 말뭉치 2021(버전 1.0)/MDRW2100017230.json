      "utterance": [
        {
          "id": "MDRW2100017230.1.1",
          "form": "거기에 돈 넣는거지",
          "original_form": "거기에 돈 넣는거지",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017230.1.2",
          "form": "티끌모아 티끌이라고 하는 사람도 많은데",
          "original_form": "티끌모아 티끌이라고 하는 사람도 많은데",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017230.1.3",
          "form": "맞아 사람들 돈관리할때 통장분리부터 하잖아",
          "original_form": "맞아 사람들 돈관리할때 통장분리부터 하잖아",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017230.1.4",
          "form": "진짜 괜찮은거같음",
          "original_form": "진짜 괜찮은거같음",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017230.1.5",
          "form": "돈 모아본 사람이랑 안 모아본 사람은 차이 진짜 큰거 같음",
          "original_form": "돈 모아본 사람이랑 안 모아본 사람은 차이 진짜 큰거 같음",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017230.1.6",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017230.1.7",
          "form": "근데 나 적금 할때는",
          "original_form": "근데 나 적금 할때는",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017230.1.8",
          "form": "진짜 열심히 돈 관리 했거든",
          "original_form": "진짜 열심히 돈 관리 했거든",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017230.1.9",
          "form": "적금에 얼마 넣고, 자유 입출금에 얼마 넣고 이러면서",
          "original_form": "적금에 얼마 넣고, 자유 입출금에 얼마 넣고 이러면서",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017230.1.10",
          "form": "지출 많은 달은 다음달에 돈 아끼고 그랬는데",
          "original_form": "지출 많은 달은 다음달에 돈 아끼고 그랬는데",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017230.1.11",
          "form": "적금 끝나자 마자 좀 흥청망청 쓰는것 같아",
          "original_form": "적금 끝나자 마자 좀 흥청망청 쓰는것 같아",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017230.1.12",
          "form": "헐 적금도 했었구나",
          "original_form": "헐 적금도 했었구나",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017230.1.13",
          "form": "근데 적금은 그냥 돈을 안쓰고 모았다 이정도 의미지",
          "original_form": "근데 적금은 그냥 돈을 안쓰고 모았다 이정도 의미지",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017230.1.14",
          "form": "이자율이 높고 그런것도 아니라서",
          "original_form": "이자율이 높고 그런것도 아니라서",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017230.1.15",
          "form": "맞아 너무 저금리라서..",
          "original_form": "맞아 너무 저금리라서..",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017230.1.16",
          "form": "너는 마세라티 사라",
          "original_form": "너는 마세라티 사라",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017230.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ그건 얼마정도해?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그건 얼마정도해?",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017230.1.18",
          "form": "아니면 오픈카 사줘",
          "original_form": "아니면 오픈카 사줘",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017230.1.19",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017230.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017230.1.21",
          "form": "중고차 말고",
          "original_form": "중고차 말고",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017230.1.22",
          "form": "돈많이벌면",
          "original_form": "돈많이벌면",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017230.1.23",
          "form": "새차로 사야하는거 알지",
          "original_form": "새차로 사야하는거 알지",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017230.1.24",
          "form": "차에 리본묶어서",
          "original_form": "차에 리본묶어서",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017230.1.25",
          "form": "ㄴ니네집으로 끌고갈게",
          "original_form": "ㄴ니네집으로 끌고갈게",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017230.1.26",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017230.1.27",
          "form": "우리 선글라스 끼고 달리자",
          "original_form": "우리 선글라스 끼고 달리자",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017230.1.28",
          "form": "아 선글라스도 사고 싶다",
          "original_form": "아 선글라스도 사고 싶다",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.30",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.31",
          "form": "선글라스 머리에 올려두고",
          "original_form": "선글라스 머리에 올려두고",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.32",
          "form": "금목걸이차고",
          "original_form": "금목걸이차고",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.33",
          "form": "맞아맞아마자아",
          "original_form": "맞아맞아마자아",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.35",
          "form": "금목걸이요...?",
          "original_form": "금목걸이요...?",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017230.1.36",
          "form": "나 비키니 입고 타도 돼?",
          "original_form": "나 비키니 입고 타도 돼?",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017230.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네??",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017230.1.38",
          "form": "근육짱짱걸 몸 만들고",
          "original_form": "근육짱짱걸 몸 만들고",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017230.1.39",
          "form": "비키니 입고 탈래",
          "original_form": "비키니 입고 탈래",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017230.1.40",
          "form": "내가졌다",
          "original_form": "내가졌다",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017230.1.41",
          "form": "걱정마 제모 다 하고 탈게",
          "original_form": "걱정마 제모 다 하고 탈게",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017230.1.42",
          "form": "ㄱㅊㄱㅊ",
          "original_form": "ㄱㅊㄱㅊ",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017230.1.43",
          "form": "암튼 선글라스가 좀 필요할것 같다",
          "original_form": "암튼 선글라스가 좀 필요할것 같다",
          "speaker_id": "2",
          "time": "20210826 18:29"
        }
      ]
