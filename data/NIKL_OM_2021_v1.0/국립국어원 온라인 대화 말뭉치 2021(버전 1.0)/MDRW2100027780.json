      "utterance": [
        {
          "id": "MDRW2100027780.1.1",
          "form": "점심은 뭐드셨어요?ㅋ",
          "original_form": "점심은 뭐드셨어요?ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027780.1.2",
          "form": "뭐 먹었더라.. 당근 케이크 먹었어요!",
          "original_form": "뭐 먹었더라.. 당근 케이크 먹었어요!",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027780.1.3",
          "form": "오 당근케이크 맛있었겠어요.ㅋ",
          "original_form": "오 당근케이크 맛있었겠어요.ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027780.1.4",
          "form": "저는 열무비빔밥먹었어요ㅋㅋ",
          "original_form": "저는 열무비빔밥먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027780.1.5",
          "form": "당근케이크는 어디껀가요?",
          "original_form": "당근케이크는 어디껀가요?",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027780.1.6",
          "form": "당근케이크 맛이 궁금해서 디저트39꺼랑 파스쿠찌꺼 두 종류를 사봤어요.",
          "original_form": "당근케이크 맛이 궁금해서 디저트39꺼랑 파스쿠찌꺼 두 종류를 사봤어요.",
          "speaker_id": "2",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027780.1.7",
          "form": "아아 둘다 어떤가요?",
          "original_form": "아아 둘다 어떤가요?",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027780.1.8",
          "form": "어디꺼가 맛있어요?",
          "original_form": "어디꺼가 맛있어요?",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027780.1.9",
          "form": "저는 디저트39꺼가 더 맛있었어요 왜냐면 크림이 많아서?",
          "original_form": "저는 디저트39꺼가 더 맛있었어요 왜냐면 크림이 많아서?",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027780.1.10",
          "form": "파스쿠찌는 생긴게 귀엽게 생겼어요",
          "original_form": "파스쿠찌는 생긴게 귀엽게 생겼어요",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027780.1.11",
          "form": "아 그렇구나ㅋㅋ",
          "original_form": "아 그렇구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027780.1.12",
          "form": "디저트39 집 가까이에 있는데",
          "original_form": "디저트39 집 가까이에 있는데",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027780.1.13",
          "form": "나중에 한번 먹으러가봐야겠네요",
          "original_form": "나중에 한번 먹으러가봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027780.1.14",
          "form": "좋은 생각이에요 ㅋㅋㅋ 중간중간 견과류도 있고 달긴하지만 아주 달진 않아서 무난하게 좋아요!",
          "original_form": "좋은 생각이에요 ㅋㅋㅋ 중간중간 견과류도 있고 달긴하지만 아주 달진 않아서 무난하게 좋아요!",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027780.1.15",
          "form": "아주 달지 않은거 좋아해요",
          "original_form": "아주 달지 않은거 좋아해요",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027780.1.16",
          "form": "너무 달면은 덜 먹게 되더라구요",
          "original_form": "너무 달면은 덜 먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027780.1.17",
          "form": "맞아요 ㅜ.ㅜ 저도 단거는 많이 못먹어서 너무 달면 그만먹게 되는..",
          "original_form": "맞아요 ㅜ.ㅜ 저도 단거는 많이 못먹어서 너무 달면 그만먹게 되는..",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027780.1.18",
          "form": "열무 비빔밥은 어떠셨나요!",
          "original_form": "열무 비빔밥은 어떠셨나요!",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027780.1.19",
          "form": "열무비빔밥 잘 익은거라서 ㅋㅋ 맛있엇어욬",
          "original_form": "열무비빔밥 잘 익은거라서 ㅋㅋ 맛있엇어욬",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027780.1.20",
          "form": "덕분에 커피도 막 땡겨서 ㅋㅋㅋㅋ 커피도 많이 마셨네요 ㅋㅋ",
          "original_form": "덕분에 커피도 막 땡겨서 ㅋㅋㅋㅋ 커피도 많이 마셨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027780.1.21",
          "form": "막 입맛이 돌더라구요 ㅋㅋ",
          "original_form": "막 입맛이 돌더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027780.1.22",
          "form": "앗 ㅋㅋ 무슨 느낌인지 알거 같아요. 저도 열무비빔밥 좋아하는데 언제 한번 먹어야 겠어요.",
          "original_form": "앗 ㅋㅋ 무슨 느낌인지 알거 같아요. 저도 열무비빔밥 좋아하는데 언제 한번 먹어야 겠어요.",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027780.1.23",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027780.1.24",
          "form": "맛있죠ㅋㅋ",
          "original_form": "맛있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027780.1.25",
          "form": "저녁에는 소고기,삼겹살 먹으려구요 ㅋ",
          "original_form": "저녁에는 소고기,삼겹살 먹으려구요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027780.1.26",
          "form": "헉 고기좋죠 맛있게 드세요!!",
          "original_form": "헉 고기좋죠 맛있게 드세요!!",
          "speaker_id": "2",
