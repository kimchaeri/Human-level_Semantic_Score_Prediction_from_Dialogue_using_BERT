        {
          "id": "MDRW2100004481.1.24",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004481.1.25",
          "form": "또 배달 음식중에 뭐 좋아하세요?",
