      "utterance": [
        {
          "id": "MDRW2100014649.1.1",
          "form": "name2님은 유튜브 보세요?",
          "original_form": "&name2&님은 유튜브 보세요?",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014649.1.2",
          "form": "많이 보진않는데ㅎ",
          "original_form": "많이 보진않는데ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014649.1.3",
          "form": "그래도 가끔 봐요ㅎㅎ",
          "original_form": "그래도 가끔 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014649.1.4",
          "form": "어떤 채널 보세요?",
          "original_form": "어떤 채널 보세요?",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014649.1.5",
          "form": "전 코인방송이요ㅎ",
          "original_form": "전 코인방송이요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014649.1.6",
          "form": "알고리즘 뜨는대로봐요",
          "original_form": "알고리즘 뜨는대로봐요",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014649.1.7",
          "form": "누구요?",
          "original_form": "누구요?",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014649.1.8",
          "form": "매억남이라고 유투버인데",
          "original_form": "매억남이라고 유투버인데",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014649.1.9",
          "form": "매달 억을 번다네요",
          "original_form": "매달 억을 번다네요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014649.1.10",
          "form": "계속보고는있는데 왜 저는 못버는걸까요?",
          "original_form": "계속보고는있는데 왜 저는 못버는걸까요?",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014649.1.11",
          "form": "저도 그사람 본적있어요ㅎ",
          "original_form": "저도 그사람 본적있어요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014649.1.12",
          "form": "그렇군요 ㅎㅎㅎ",
          "original_form": "그렇군요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014649.1.13",
          "form": "잘하긴하는것같은데ㅎㅎ",
          "original_form": "잘하긴하는것같은데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014649.1.14",
          "form": "전 왜 못따라가는지ㅎㅎ",
          "original_form": "전 왜 못따라가는지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014649.1.15",
          "form": "좋아하는 연예인 있나요?",
          "original_form": "좋아하는 연예인 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014649.1.16",
          "form": "좋아하는 연예인이라고 딱히 없는데ㅎㅎ 아이유 좋아해요",
          "original_form": "좋아하는 연예인이라고 딱히 없는데ㅎㅎ 아이유 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014649.1.17",
          "form": "노래도 잘부르고ㅎㅎ",
          "original_form": "노래도 잘부르고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014649.1.18",
          "form": "저도 예전에 아이유 덕질했어요",
          "original_form": "저도 예전에 아이유 덕질했어요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014649.1.19",
          "form": "전 덕질은 안했어요ㅎ",
          "original_form": "전 덕질은 안했어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014649.1.20",
          "form": "콘서트도 가셨어요?",
          "original_form": "콘서트도 가셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014649.1.21",
          "form": "저 너무 가고싶내요ㅠ",
          "original_form": "저 너무 가고싶내요ㅠ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014649.1.22",
          "form": "첫콘서트부터 분홍신 활동할때까지",
          "original_form": "첫콘서트부터 분홍신 활동할때까지",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014649.1.23",
          "form": "매년 갔었어요",
          "original_form": "매년 갔었어요",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014649.1.24",
          "form": "우와 정말 찐팬이네여ㅎ",
          "original_form": "우와 정말 찐팬이네여ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014649.1.25",
          "form": "그럼 지금은?",
          "original_form": "그럼 지금은?",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014649.1.26",
          "form": "요즘은 안하죠",
          "original_form": "요즘은 안하죠",
          "speaker_id": "1",
          "time": "20210825 20:21"
        }
      ]
    }
  ]
