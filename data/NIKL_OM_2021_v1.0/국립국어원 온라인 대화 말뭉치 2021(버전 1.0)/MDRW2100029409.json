      "utterance": [
        {
          "id": "MDRW2100029409.1.1",
          "form": "좋은 아침입니다",
          "original_form": "좋은 아침입니다",
          "speaker_id": "1",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100029409.1.2",
          "form": "평소에 건강을 챙기시나요?",
          "original_form": "평소에 건강을 챙기시나요?",
          "speaker_id": "1",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100029409.1.3",
          "form": "챙기려고 노력해요 님은요?",
          "original_form": "챙기려고 노력해요 님은요?",
          "speaker_id": "2",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100029409.1.4",
          "form": "저는 이제 챙겨보려해요ㅎ",
          "original_form": "저는 이제 챙겨보려해요ㅎ",
          "speaker_id": "1",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100029409.1.5",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100029409.1.6",
          "form": "체력이 안좋아서 조금씩 해보려구요",
          "original_form": "체력이 안좋아서 조금씩 해보려구요",
          "speaker_id": "1",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100029409.1.7",
          "form": "님께선 운동하시나요?",
          "original_form": "님께선 운동하시나요?",
          "speaker_id": "1",
          "time": "20210903 07:19"
        },
        {
          "id": "MDRW2100029409.1.8",
          "form": "무슨 운동하실껀가요?",
          "original_form": "무슨 운동하실껀가요?",
          "speaker_id": "2",
          "time": "20210903 07:19"
        },
        {
          "id": "MDRW2100029409.1.9",
          "form": "간단하게 맨몸운동하려구요",
          "original_form": "간단하게 맨몸운동하려구요",
          "speaker_id": "1",
          "time": "20210903 07:19"
        },
        {
          "id": "MDRW2100029409.1.10",
          "form": "전 pt하나가 코로나때문에 계단타기 하고 있어요~",
          "original_form": "전 pt하나가 코로나때문에 계단타기 하고 있어요~",
          "speaker_id": "2",
          "time": "20210903 07:19"
        },
        {
          "id": "MDRW2100029409.1.11",
          "form": "우와 계단타기 정말힘들다던데",
          "original_form": "우와 계단타기 정말힘들다던데",
          "speaker_id": "1",
          "time": "20210903 07:20"
        },
        {
          "id": "MDRW2100029409.1.12",
          "form": "아침시간을 활용하시나요?",
          "original_form": "아침시간을 활용하시나요?",
          "speaker_id": "1",
          "time": "20210903 07:20"
        },
        {
          "id": "MDRW2100029409.1.13",
          "form": "아침에 하고싶은데ㅜ요즘은 못 하고 있어요ㅜ",
          "original_form": "아침에 하고싶은데ㅜ요즘은 못 하고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210903 07:21"
        },
        {
          "id": "MDRW2100029409.1.14",
          "form": "홈트 하실건가요?",
          "original_form": "홈트 하실건가요?",
          "speaker_id": "2",
          "time": "20210903 07:21"
        },
        {
          "id": "MDRW2100029409.1.15",
          "form": "아침시간에 운동하는게 생각보다 어렵죠",
          "original_form": "아침시간에 운동하는게 생각보다 어렵죠",
          "speaker_id": "1",
          "time": "20210903 07:21"
        },
        {
          "id": "MDRW2100029409.1.16",
          "form": "네 홈트하려구요",
          "original_form": "네 홈트하려구요",
          "speaker_id": "1",
          "time": "20210903 07:21"
        },
        {
          "id": "MDRW2100029409.1.17",
          "form": "네ㅜㅜ힘든일이예요ㅜ",
          "original_form": "네ㅜㅜ힘든일이예요ㅜ",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029409.1.18",
          "form": "사실 어제 잠깐 운동하고 온몸이 말을 안듣네요ㅎㅎ",
          "original_form": "사실 어제 잠깐 운동하고 온몸이 말을 안듣네요ㅎㅎ",
