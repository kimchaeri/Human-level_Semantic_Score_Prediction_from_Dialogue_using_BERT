      "utterance": [
        {
          "id": "MDRW2100035800.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210830 02:54"
        },
        {
          "id": "MDRW2100035800.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20210830 02:54"
        },
        {
          "id": "MDRW2100035800.1.3",
          "form": "감자로 조만간 뭐해드실거 있으신가여?",
          "original_form": "감자로 조만간 뭐해드실거 있으신가여?",
          "speaker_id": "1",
          "time": "20210830 02:55"
        },
        {
          "id": "MDRW2100035800.1.4",
          "form": "음 생각해둔건 없어요",
          "original_form": "음 생각해둔건 없어요",
          "speaker_id": "2",
          "time": "20210830 02:55"
        },
        {
          "id": "MDRW2100035800.1.5",
          "form": "옹심이 해보셧어요?",
          "original_form": "옹심이 해보셧어요?",
          "speaker_id": "1",
          "time": "20210830 02:55"
        },
        {
          "id": "MDRW2100035800.1.6",
          "form": "아니요 옹심이 만드는거 어렵지않나요?",
          "original_form": "아니요 옹심이 만드는거 어렵지않나요?",
          "speaker_id": "2",
          "time": "20210830 02:56"
        },
        {
          "id": "MDRW2100035800.1.7",
          "form": "ㅋㅋㅋㅋ 좀 벅차긴해여 ㅋㅋ\n근데 진짜 맛잇어요..",
          "original_form": "ㅋㅋㅋㅋ 좀 벅차긴해여 ㅋㅋ\n근데 진짜 맛잇어요..",
          "speaker_id": "1",
          "time": "20210830 02:56"
        },
        {
          "id": "MDRW2100035800.1.8",
          "form": "옹심이 먹는건 잘 먹는데 어려울거 같아서 도전해보지 않았어요",
          "original_form": "옹심이 먹는건 잘 먹는데 어려울거 같아서 도전해보지 않았어요",
          "speaker_id": "2",
          "time": "20210830 02:57"
        },
        {
          "id": "MDRW2100035800.1.9",
          "form": "복잡하고 손이 많이 가는 음식들은 재료가 있어도 잘 안 하게 되더라구요..ㅜㅜ",
          "original_form": "복잡하고 손이 많이 가는 음식들은 재료가 있어도 잘 안 하게 되더라구요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 02:58"
        },
        {
          "id": "MDRW2100035800.1.10",
          "form": "ㅋㅋㅋㅋㅋ 그쵸 저희 엄마가 좀 무리햇네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸 저희 엄마가 좀 무리햇네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:59"
        },
        {
          "id": "MDRW2100035800.1.11",
          "form": "어머니가 감자 옹심이를 위해 구매해오신건가요?",
          "original_form": "어머니가 감자 옹심이를 위해 구매해오신건가요?",
          "speaker_id": "2",
          "time": "20210830 02:59"
        },
        {
          "id": "MDRW2100035800.1.12",
          "form": "집에 감자가 많아서 이것저것 해드시는게\n이번이 세번째인가그런데 맛잇더라고요.ㅋㅋ\n예전에 강릉에 차돌 크림 옹심이",
          "original_form": "집에 감자가 많아서 이것저것 해드시는게\n이번이 세번째인가그런데 맛잇더라고요.ㅋㅋ\n예전에 강릉에 차돌 크림 옹심이",
          "speaker_id": "1",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100035800.1.13",
          "form": "라는게 있엇는데 그거는 진짜 맛잇엇을것같다고 생각햇어요 ㅋㅋ",
          "original_form": "라는게 있엇는데 그거는 진짜 맛잇엇을것같다고 생각햇어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100035800.1.14",
          "form": "오 저 그거 먹어봤던거 같아요",
          "original_form": "오 저 그거 먹어봤던거 같아요",
          "speaker_id": "2",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100035800.1.15",
          "form": "차돌까지는 모르겠는데",
          "original_form": "차돌까지는 모르겠는데",
          "speaker_id": "2",
          "time": "20210830 03:01"
        },
        {
          "id": "MDRW2100035800.1.16",
          "form": "크림옹심이 파스타였나? 비슷한거 먹어봤어요",
          "original_form": "크림옹심이 파스타였나? 비슷한거 먹어봤어요",
          "speaker_id": "2",
          "time": "20210830 03:01"
        },
        {
          "id": "MDRW2100035800.1.17",
          "form": "오 어디꺼엿죠?\n문닫은 거기인가??",
          "original_form": "오 어디꺼엿죠?\n문닫은 거기인가??",
          "speaker_id": "1",
          "time": "20210830 03:01"
        },
        {
          "id": "MDRW2100035800.1.18",
          "form": "전 소풍이라는 곳에서요",
          "original_form": "전 소풍이라는 곳에서요",
          "speaker_id": "2",
          "time": "20210830 03:01"
        },
        {
          "id": "MDRW2100035800.1.19",
          "form": "오 그러면 다른것인것같네요",
          "original_form": "오 그러면 다른것인것같네요",
          "speaker_id": "1",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035800.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035800.1.21",
          "form": "아 그렇군요ㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035800.1.22",
          "form": "아무튼 그걸 접목시킨사람은 천재같아요.",
