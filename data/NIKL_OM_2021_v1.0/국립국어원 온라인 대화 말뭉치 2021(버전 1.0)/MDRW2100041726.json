      "utterance": [
        {
          "id": "MDRW2100041726.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 14:59"
        },
        {
          "id": "MDRW2100041726.1.2",
          "form": "안녕하세요 점심에는 뭐 드셨어요?",
          "original_form": "안녕하세요 점심에는 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100041726.1.3",
          "form": "저는 육개장이요 ㅎㅎ",
          "original_form": "저는 육개장이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041726.1.4",
          "form": "저는 미역국 먹었어요 ㅎㅎ",
          "original_form": "저는 미역국 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041726.1.5",
          "form": "소고기 미역국요??",
          "original_form": "소고기 미역국요??",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041726.1.6",
          "form": "네ㅎㅎ 미역국은 소고기 미역국이 제일 맛있더라구요",
          "original_form": "네ㅎㅎ 미역국은 소고기 미역국이 제일 맛있더라구요",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041726.1.7",
          "form": "맞아요 고기가 진리죠",
          "original_form": "맞아요 고기가 진리죠",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041726.1.8",
          "form": "좋아하는 음식이 뭔가요?",
          "original_form": "좋아하는 음식이 뭔가요?",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041726.1.9",
          "form": "저 고기ㅋㅋ치킨.ㅋㅋ요런거",
          "original_form": "저 고기ㅋㅋ치킨.ㅋㅋ요런거",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100041726.1.10",
          "form": "살찌는거좋아하네여ㅠ흑",
          "original_form": "살찌는거좋아하네여ㅠ흑",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100041726.1.11",
          "form": "ㅋㅋ 고기가 맛있긴 하죠",
          "original_form": "ㅋㅋ 고기가 맛있긴 하죠",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041726.1.12",
          "form": "소, 닭, 돼지 중에 어떤 게 제일 맛있으신가요?",
          "original_form": "소, 닭, 돼지 중에 어떤 게 제일 맛있으신가요?",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041726.1.13",
          "form": "소 카우카우 하누하누",
          "original_form": "소 카우카우 하누하누",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041726.1.14",
          "form": "다른거는 자주 먹는데 소고기는 자주 못 먹어서ㅠㅠ",
          "original_form": "다른거는 자주 먹는데 소고기는 자주 못 먹어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041726.1.15",
          "form": "소고기가 비싸다보니ㅠ",
          "original_form": "소고기가 비싸다보니ㅠ",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041726.1.16",
          "form": "전 순전히 맛만 따지면 돼지고기가 젤 맛있더라구요",
          "original_form": "전 순전히 맛만 따지면 돼지고기가 젤 맛있더라구요",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041726.1.17",
          "form": "저는 맛도 소가 ㅋㅋ 삼겹이도 맛있긴하죠 ㅎㅎㅎㅎ",
          "original_form": "저는 맛도 소가 ㅋㅋ 삼겹이도 맛있긴하죠 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:42"
        }
