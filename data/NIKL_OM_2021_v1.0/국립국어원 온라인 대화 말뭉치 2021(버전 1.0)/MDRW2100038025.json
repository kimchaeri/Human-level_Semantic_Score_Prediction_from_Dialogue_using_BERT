      "utterance": [
        {
          "id": "MDRW2100038025.1.1",
          "form": "맛잇겠다 라면ㅜ 학교서 뽀글이해드셨나요",
          "original_form": "맛잇겠다 라면ㅜ 학교서 뽀글이해드셨나요",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100038025.1.2",
          "form": "아니 도시락도 안까먹음",
          "original_form": "아니 도시락도 안까먹음",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100038025.1.3",
          "form": "그럼머먹음",
          "original_form": "그럼머먹음",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100038025.1.4",
          "form": "과자 ㅋㅋ",
          "original_form": "과자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100038025.1.5",
          "form": "ㅋㅋㅋ과자로배채움?",
          "original_form": "ㅋㅋㅋ과자로배채움?",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038025.1.6",
          "form": "젊음이 넘쳤군요",
          "original_form": "젊음이 넘쳤군요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038025.1.7",
          "form": "아니 밥시간엔 도시락 먹지",
          "original_form": "아니 밥시간엔 도시락 먹지",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038025.1.8",
          "form": "학교생활은 머니머니해도 만화책이지",
          "original_form": "학교생활은 머니머니해도 만화책이지",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038025.1.9",
          "form": "오 도시락 누가쌋나요",
          "original_form": "오 도시락 누가쌋나요",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038025.1.10",
          "form": "학교생활은 머니머니해도 바느질이죠",
          "original_form": "학교생활은 머니머니해도 바느질이죠",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038025.1.11",
          "form": "아 무슨 바느질이야",
          "original_form": "아 무슨 바느질이야",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100038025.1.12",
          "form": "코스프레녀  !!!",
          "original_form": "코스프레녀  !!!",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100038025.1.13",
          "form": "마나책 머봄? 요리왕비룡?",
          "original_form": "마나책 머봄? 요리왕비룡?",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100038025.1.14",
          "form": "노우웅",
          "original_form": "노우웅",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100038025.1.15",
          "form": "취미생활이 유니크한",
          "original_form": "취미생활이 유니크한",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100038025.1.16",
          "form": "일케불러쥬",
          "original_form": "일케불러쥬",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100038025.1.17",
          "form": "미스터 초밥왕 슬램덩크 일본만화들",
          "original_form": "미스터 초밥왕 슬램덩크 일본만화들",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100038025.1.18",
          "form": "와 굿",
          "original_form": "와 굿",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100038025.1.19",
          "form": "초밥먹고싶다",
          "original_form": "초밥먹고싶다",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100038025.1.20",
          "form": "동아리.활동은 뭐함?",
          "original_form": "동아리.활동은 뭐함?",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100038025.1.21",
          "form": "코스프레요",
          "original_form": "코스프레요",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100038025.1.22",
          "form": "난 영화동아리",
          "original_form": "난 영화동아리",
