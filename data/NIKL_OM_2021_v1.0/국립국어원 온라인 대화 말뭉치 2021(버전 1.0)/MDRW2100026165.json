      "utterance": [
        {
          "id": "MDRW2100026165.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026165.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026165.1.3",
          "form": "운동 가기.. 안좋은 날씨네요",
          "original_form": "운동 가기.. 안좋은 날씨네요",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026165.1.4",
          "form": "제주시는 비가 너무 옵니다",
          "original_form": "제주시는 비가 너무 옵니다",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026165.1.5",
          "form": "오 저도 제주인데 반갑슴당 번개치구 난리네요",
          "original_form": "오 저도 제주인데 반갑슴당 번개치구 난리네요",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026165.1.6",
          "form": "네 산책가야하는데ㅜ 늦었네요",
          "original_form": "네 산책가야하는데ㅜ 늦었네요",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026165.1.7",
          "form": "건강하시지요?",
          "original_form": "건강하시지요?",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026165.1.8",
          "form": "사라봉근처살아서 가면 좋은데 가까우니 더 안가게되는 마법",
          "original_form": "사라봉근처살아서 가면 좋은데 가까우니 더 안가게되는 마법",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026165.1.9",
          "form": "엇 저도 사라봉 ㅎㅎ",
          "original_form": "엇 저도 사라봉 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026165.1.10",
          "form": "일도이동입니다!",
          "original_form": "일도이동입니다!",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026165.1.11",
          "form": "네 건강은 한데 운동이 필요합니당",
          "original_form": "네 건강은 한데 운동이 필요합니당",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026165.1.12",
          "form": "헐 저도 일도이동",
          "original_form": "헐 저도 일도이동",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026165.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026165.1.14",
          "form": "별도봉이 더 좋아서",
          "original_form": "별도봉이 더 좋아서",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026165.1.15",
          "form": "반갑습니당 ㅎㅎ",
          "original_form": "반갑습니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026165.1.16",
          "form": "오 저 제주 내려온지 2주밖에 안되서 사라봉 산책가보구 별도봉은 좀 올라가다 돌아왔어요",
          "original_form": "오 저 제주 내려온지 2주밖에 안되서 사라봉 산책가보구 별도봉은 좀 올라가다 돌아왔어요",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026165.1.17",
          "form": "헬스장은 코로나땜시 신경쓰여 못가겠어요",
          "original_form": "헬스장은 코로나땜시 신경쓰여 못가겠어요",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026165.1.18",
          "form": "별도봉이 운동하기에 더 좋아요",
          "original_form": "별도봉이 운동하기에 더 좋아요",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026165.1.19",
          "form": "네 오르막이던데요",
          "original_form": "네 오르막이던데요",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026165.1.20",
          "form": "네 저도 필라테스 하고픈데",
          "original_form": "네 저도 필라테스 하고픈데",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026165.1.21",
          "form": "올림픽보니 펜싱도 재밌어보이구요",
          "original_form": "올림픽보니 펜싱도 재밌어보이구요",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026165.1.22",
          "form": "제주가 걷기운동에 좋아요",
          "original_form": "제주가 걷기운동에 좋아요",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026165.1.23",
          "form": "호젓하고 오르막이 좀 있지만",
          "original_form": "호젓하고 오르막이 좀 있지만",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026165.1.24",
          "form": "여자다 보니 저녁에 나가기가 좀",
          "original_form": "여자다 보니 저녁에 나가기가 좀",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026165.1.25",
          "form": "맞아요. 특히 사라봉은 혼자가기엔",
          "original_form": "맞아요. 특히 사라봉은 혼자가기엔",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026165.1.26",
          "form": "그래도 동네정도는 괜찮아요ㅎㅎ",
          "original_form": "그래도 동네정도는 괜찮아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026165.1.27",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026165.1.28",
          "form": "한마음서 하나로마트까지 산책하기좋아요",
          "original_form": "한마음서 하나로마트까지 산책하기좋아요",
          "speaker_id": "2",
          "time": "20210901 20:29"
        }
      ]
    }
  ]
