      "utterance": [
        {
          "id": "MDRW2100042347.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042347.1.2",
          "form": "음 네! 저는 여행 좋아합니당",
          "original_form": "음 네! 저는 여행 좋아합니당",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042347.1.3",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042347.1.4",
          "form": "전 집순이였는데",
          "original_form": "전 집순이였는데",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042347.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042347.1.6",
          "form": "코로나로 너무 못가니 이젠 가고싶어요ㅠㅠ",
          "original_form": "코로나로 너무 못가니 이젠 가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042347.1.7",
          "form": "일단 국내 유영 관광 명소 돌고싶네요",
          "original_form": "일단 국내 유영 관광 명소 돌고싶네요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042347.1.8",
          "form": "자발적 집순이랑 이건 진짜 느낌이 다른 것 같습니다..ㅠㅠ",
          "original_form": "자발적 집순이랑 이건 진짜 느낌이 다른 것 같습니다..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042347.1.9",
          "form": "(?)",
          "original_form": "(?)",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042347.1.10",
          "form": "저도 어디든 가고싶네요",
          "original_form": "저도 어디든 가고싶네요",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042347.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042347.1.12",
          "form": "진짜 해외는 엄두도 안나요",
          "original_form": "진짜 해외는 엄두도 안나요",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042347.1.13",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042347.1.14",
          "form": "그니까요 언제 나가볼수있을지ㅠ",
          "original_form": "그니까요 언제 나가볼수있을지ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042347.1.15",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042347.1.16",
          "form": "보통 여행 계획은 타이트하게 짜시는 편이신가요?",
          "original_form": "보통 여행 계획은 타이트하게 짜시는 편이신가요?",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042347.1.17",
          "form": "전 계획을 잘 못해서",
          "original_form": "전 계획을 잘 못해서",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042347.1.18",
          "form": "짜놓은거 따라가는 편이에요",
          "original_form": "짜놓은거 따라가는 편이에요",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042347.1.19",
          "form": "아 네 그것도 좋죠! ㅎㅎ",
          "original_form": "아 네 그것도 좋죠! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042347.1.20",
          "form": "저는 계획표 만드는 건 좋아하는데 대신 널널하게 짜는 편이에요 ㅎㅎ",
          "original_form": "저는 계획표 만드는 건 좋아하는데 대신 널널하게 짜는 편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042347.1.21",
          "form": "그래서 그런지 항상 먹는게 6-7할은 되는것같습니다",
          "original_form": "그래서 그런지 항상 먹는게 6-7할은 되는것같습니다",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042347.1.22",
          "form": "타이트하면 좀 힘들긴해요",
          "original_form": "타이트하면 좀 힘들긴해요",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042347.1.23",
          "form": "여행가면 맛집 필수죠!",
          "original_form": "여행가면 맛집 필수죠!",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042347.1.24",
          "form": "ㅎㅎ 맞아요...몇번 힘들게 다녀오다보면",
          "original_form": "ㅎㅎ 맞아요...몇번 힘들게 다녀오다보면",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042347.1.25",
          "form": "절로 널널하게 짜게되는것같아요",
          "original_form": "절로 널널하게 짜게되는것같아요",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042347.1.26",
          "form": "혹시 다녀오셨던 여행지 중에 기억에 남는 곳이 있으신가요?",
          "original_form": "혹시 다녀오셨던 여행지 중에 기억에 남는 곳이 있으신가요?",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042347.1.27",
          "form": "경주가 기억남아요",
          "original_form": "경주가 기억남아요",
          "speaker_id": "1",
          "time": "20210906 22:03"
        }
      ]
