      "utterance": [
        {
          "id": "MDRW2100005387.1.1",
          "form": "ㅎㅎ 곧 묶이겠어",
          "original_form": "ㅎㅎ 곧 묶이겠어",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005387.1.2",
          "form": "가야지가야지 하면서도 계속 미루게 되네",
          "original_form": "가야지가야지 하면서도 계속 미루게 되네",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005387.1.3",
          "form": "요즘 코로나라 더하지 4단계이니",
          "original_form": "요즘 코로나라 더하지 4단계이니",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005387.1.4",
          "form": "ㅇㅇ 마스크 벗고 자르는게 왠지 꺼림칙해서",
          "original_form": "ㅇㅇ 마스크 벗고 자르는게 왠지 꺼림칙해서",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005387.1.5",
          "form": "아무래도.. 그래도 한 번 가서 잘라야 할 것 같아",
          "original_form": "아무래도.. 그래도 한 번 가서 잘라야 할 것 같아",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005387.1.6",
          "form": "ㅋㅋ 너무 길긴 하지",
          "original_form": "ㅋㅋ 너무 길긴 하지",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005387.1.7",
          "form": "ㅇㅇ 그러다 덤블도어 되겄응 ㅎㅎ",
          "original_form": "ㅇㅇ 그러다 덤블도어 되겄응 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005387.1.8",
          "form": "오메 그 정도는 아니야 ㅋㅋ",
          "original_form": "오메 그 정도는 아니야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005387.1.9",
          "form": "머리 산뜻하게 자르고 나면 기분도 업되징",
          "original_form": "머리 산뜻하게 자르고 나면 기분도 업되징",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005387.1.10",
          "form": "하아.. 머리가 무겁긴 해서 함 자르고 싶긴 한데",
          "original_form": "하아.. 머리가 무겁긴 해서 함 자르고 싶긴 한데",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005387.1.11",
          "form": "산책하다보면 블루클럽 있잖아",
          "original_form": "산책하다보면 블루클럽 있잖아",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005387.1.12",
          "form": "ㅎㅎ 내일 산책하다 들러야겠네",
          "original_form": "ㅎㅎ 내일 산책하다 들러야겠네",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005387.1.13",
          "form": "ㅇㅇ 따로 나가긴 귀찮으니 걷다가 들러",
          "original_form": "ㅇㅇ 따로 나가긴 귀찮으니 걷다가 들러",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005387.1.14",
          "form": "머리 자르는 게 세상에서 제일 싫어 ㅋㅋ",
