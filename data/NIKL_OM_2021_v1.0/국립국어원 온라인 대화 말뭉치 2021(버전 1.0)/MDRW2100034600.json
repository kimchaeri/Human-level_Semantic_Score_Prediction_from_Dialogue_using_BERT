      "utterance": [
        {
          "id": "MDRW2100034600.1.1",
          "form": "나이들수록 1년에 n키로씩.. 거대해져요 키는 그대론데 ㅋㅋㅋ ㅜㅜ",
          "original_form": "나이들수록 1년에 n키로씩.. 거대해져요 키는 그대론데 ㅋㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100034600.1.2",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100034600.1.3",
          "form": "백신은 언제 맞으세요?",
          "original_form": "백신은 언제 맞으세요?",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034600.1.4",
          "form": "저는 1차는 맞았구요",
          "original_form": "저는 1차는 맞았구요",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034600.1.5",
          "form": "2차는 추석 전주에 맞아요!",
          "original_form": "2차는 추석 전주에 맞아요!",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034600.1.6",
          "form": "맞으셨어요?",
          "original_form": "맞으셨어요?",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034600.1.7",
          "form": "부러워요 ㅜㅜ",
          "original_form": "부러워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034600.1.8",
          "form": "전 9월초에 1차에요",
          "original_form": "전 9월초에 1차에요",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034600.1.9",
          "form": "그래도 얼마 안남았네요ㅠ",
          "original_form": "그래도 얼마 안남았네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034600.1.10",
          "form": "좀만 버티세요ㅠ",
          "original_form": "좀만 버티세요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034600.1.11",
          "form": "네 원래는 추석 후였는데 ㅋㅋㅋ",
          "original_form": "네 원래는 추석 후였는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034600.1.12",
          "form": "바꾸라고 권유문잘 받았어요",
          "original_form": "바꾸라고 권유문잘 받았어요",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034600.1.13",
          "form": "저는 잔여 잡아서 일찍 맞게되었어요",
          "original_form": "저는 잔여 잡아서 일찍 맞게되었어요",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034600.1.14",
          "form": "오 잘됐네요!",
          "original_form": "오 잘됐네요!",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034600.1.15",
          "form": "오그렇군요",
          "original_form": "오그렇군요",
          "speaker_id": "1",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.16",
          "form": "추가물량 나왔다더라구요",
          "original_form": "추가물량 나왔다더라구요",
          "speaker_id": "1",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.17",
          "form": "당겨질수도 있네요",
          "original_form": "당겨질수도 있네요",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.19",
          "form": "문자 아님 몰랐을거에요",
          "original_form": "문자 아님 몰랐을거에요",
          "speaker_id": "1",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.20",
          "form": "진짜 다행이네요",
          "original_form": "진짜 다행이네요",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.21",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.22",
          "form": "추석후면 너무 늦어요ㅠ",
          "original_form": "추석후면 너무 늦어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034600.1.23",
          "form": "맞아요 추석때 위험할거같아요",
