      "utterance": [
        {
          "id": "MDRW2100015604.1.1",
          "form": "요즘날씨좋죠",
          "original_form": "요즘날씨좋죠",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015604.1.2",
          "form": "지금도 넘 선선하니 좋네요",
          "original_form": "지금도 넘 선선하니 좋네요",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015604.1.3",
          "form": "아침엔 추워요",
          "original_form": "아침엔 추워요",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015604.1.4",
          "form": "더울땐 진짜 지구 곧 망할것만같았는데",
          "original_form": "더울땐 진짜 지구 곧 망할것만같았는데",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015604.1.5",
          "form": "머입구다니세유",
          "original_form": "머입구다니세유",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015604.1.6",
          "form": "아직은 반팔입고다니는데 회사에선 긴셔츠 걸치구있구",
          "original_form": "아직은 반팔입고다니는데 회사에선 긴셔츠 걸치구있구",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015604.1.7",
          "form": "나도 바막사고싶다",
          "original_form": "나도 바막사고싶다",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015604.1.8",
          "form": "가을이왔으면좋겠는데",
          "original_form": "가을이왔으면좋겠는데",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015604.1.9",
          "form": "겨울은 싫어!! 그래도 겨울이지나야 다시 봄이 오겠죠~",
          "original_form": "겨울은 싫어!! 그래도 겨울이지나야 다시 봄이 오겠죠~",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015604.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015604.1.11",
          "form": "주말날씨는 어떨것같나요",
          "original_form": "주말날씨는 어떨것같나요",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015604.1.12",
          "form": "예보보니 먹구름가득인가봐요",
          "original_form": "예보보니 먹구름가득인가봐요",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015604.1.13",
          "form": "비 안오면 좋겠어요",
          "original_form": "비 안오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015604.1.14",
          "form": "흑흑...왜 이제와서비가와요",
          "original_form": "흑흑...왜 이제와서비가와요",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015604.1.15",
          "form": "여름에나 장마 내리지",
          "original_form": "여름에나 장마 내리지",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015604.1.16",
          "form": "이좋은가을에~~",
          "original_form": "이좋은가을에~~",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015604.1.17",
          "form": "늦장마~~ 그래도 서울은 비가 작년같이 내린적은 별로없는듯",
          "original_form": "늦장마~~ 그래도 서울은 비가 작년같이 내린적은 별로없는듯",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015604.1.18",
          "form": "글킨하다 여기도",
          "original_form": "글킨하다 여기도",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015604.1.19",
          "form": "늦장마..!아하",
          "original_form": "늦장마..!아하",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015604.1.20",
          "form": "오늘도비쫄딱 맞앗어요",
          "original_form": "오늘도비쫄딱 맞앗어요",
          "speaker_id": "1",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015604.1.21",
          "form": "엥 대전비왔나요?!?!?!",
          "original_form": "엥 대전비왔나요?!?!?!",
          "speaker_id": "2",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015604.1.22",
          "form": "넹..계속오는중",
          "original_form": "넹..계속오는중",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015604.1.23",
          "form": "그쳤길래 자전거타구 운동갓다가",
          "original_form": "그쳤길래 자전거타구 운동갓다가",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015604.1.24",
          "form": "돌아올때... 비맞으며왓어요 사람들이 다쳐다봄...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "돌아올때... 비맞으며왓어요 사람들이 다쳐다봄...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015604.1.25",
          "form": "배달오도방구도 쳐다보고",
          "original_form": "배달오도방구도 쳐다보고",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015604.1.26",
          "form": "비맞으면서 자전거탈수도있지!! 왜쳐다봤을까",
          "original_form": "비맞으면서 자전거탈수도있지!! 왜쳐다봤을까",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015604.1.27",
          "form": "넘 이뻐서 쳐다봤나봐요",
          "original_form": "넘 이뻐서 쳐다봤나봐요",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015604.1.28",
          "form": "허걱 몰랏어용",
          "original_form": "허걱 몰랏어용",
