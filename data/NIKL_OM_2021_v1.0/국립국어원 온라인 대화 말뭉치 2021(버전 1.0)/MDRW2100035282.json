      "utterance": [
        {
          "id": "MDRW2100035282.1.1",
          "form": "안녕하세요? 대화시간 괜찮으신가요?",
          "original_form": "안녕하세요? 대화시간 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100035282.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100035282.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100035282.1.4",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100035282.1.5",
          "form": "집에서 만든 간단 김밥으로요",
          "original_form": "집에서 만든 간단 김밥으로요",
          "speaker_id": "1",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100035282.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100035282.1.7",
          "form": "저는 국밥 먹었어요!",
          "original_form": "저는 국밥 먹었어요!",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035282.1.8",
          "form": "김밥에 뭐뭐 넣으셨나요?",
          "original_form": "김밥에 뭐뭐 넣으셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035282.1.9",
          "form": "전 꼬마김밥 한거라 맛살 햄 우엉 단무지정도요",
          "original_form": "전 꼬마김밥 한거라 맛살 햄 우엉 단무지정도요",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100035282.1.10",
          "form": "밥 한술에 재료 두세개 넣고 말구 꼬마김밥마다 내용이 조금씩 다르게 만들았어요",
          "original_form": "밥 한술에 재료 두세개 넣고 말구 꼬마김밥마다 내용이 조금씩 다르게 만들았어요",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100035282.1.11",
          "form": "꼬마김밥 간편하게 먹기도 좋고 딱 맛 좋은 재료만 들어가서 더 맛있는 것 같아요",
          "original_form": "꼬마김밥 간편하게 먹기도 좋고 딱 맛 좋은 재료만 들어가서 더 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100035282.1.12",
          "form": "네네 간단히 해먹기 좋고 밥이라 일단 든든해요",
          "original_form": "네네 간단히 해먹기 좋고 밥이라 일단 든든해요",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100035282.1.13",
          "form": "예전에는 꼬마김밥 파는 가게도 자주 보였는데 요즘은 찾기가 힘드네요ㅠ",
          "original_form": "예전에는 꼬마김밥 파는 가게도 자주 보였는데 요즘은 찾기가 힘드네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100035282.1.14",
          "form": "갑자기 땡초꼬마김밥이 먹고싶어졌어요",
          "original_form": "갑자기 땡초꼬마김밥이 먹고싶어졌어요",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100035282.1.15",
          "form": "그건..코로나때문이에요..",
          "original_form": "그건..코로나때문이에요..",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100035282.1.16",
          "form": "꼬마김밥은 길에서 간단히 때우는 음식인데",
          "original_form": "꼬마김밥은 길에서 간단히 때우는 음식인데",
          "speaker_id": "1",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100035282.1.17",
          "form": "길에서 먹으려면 마스크를 벗어야하니까요",
          "original_form": "길에서 먹으려면 마스크를 벗어야하니까요",
          "speaker_id": "1",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100035282.1.18",
          "form": "그런 류는 지금 다 망했고 망해가요ㅜㅜ",
          "original_form": "그런 류는 지금 다 망했고 망해가요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100035282.1.19",
          "form": "그런가요ㅠ 포장해서 파셔서 길에서 먹는 가게도 있는지는 몰랐네요ㅠㅠ",
          "original_form": "그런가요ㅠ 포장해서 파셔서 길에서 먹는 가게도 있는지는 몰랐네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035282.1.20",
          "form": "코로나 때문에 모든 가게들이 다 힘들긴하죠ㅠㅠ",
          "original_form": "코로나 때문에 모든 가게들이 다 힘들긴하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035282.1.21",
          "form": "아 전 주로 전철역안에 있는 집에서 먹었어요",
          "original_form": "아 전 주로 전철역안에 있는 집에서 먹었어요",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035282.1.22",
          "form": "카운터에서 서서 먹는 가게였너요",
          "original_form": "카운터에서 서서 먹는 가게였너요",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035282.1.23",
          "form": "그런 곳에 진짜 맛집들 많은데 아쉬워요ㅠㅠ",
          "original_form": "그런 곳에 진짜 맛집들 많은데 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035282.1.24",
          "form": "얼른 끝나야 좀 짐을 더실텐데 걱정이네요ㅠ",
          "original_form": "얼른 끝나야 좀 짐을 더실텐데 걱정이네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035282.1.25",
          "form": "ㅠ이미 다들 좀 망해서 코로나 초기에 빨레 접은 집이",
          "original_form": "ㅠ이미 다들 좀 망해서 코로나 초기에 빨레 접은 집이",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035282.1.26",
          "form": "보증금이라도 챙겼다더라고요",
          "original_form": "보증금이라도 챙겼다더라고요",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035282.1.27",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 11:20"
        }
      ]
