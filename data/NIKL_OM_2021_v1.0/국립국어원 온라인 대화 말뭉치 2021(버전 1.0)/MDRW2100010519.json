      "utterance": [
        {
          "id": "MDRW2100010519.1.1",
          "form": "퇴근했나요",
          "original_form": "퇴근했나요",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010519.1.2",
          "form": "지금 퇴근해서 저녁먹으려구요",
          "original_form": "지금 퇴근해서 저녁먹으려구요",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010519.1.3",
          "form": "오늘도 피곤한 하루였어요",
          "original_form": "오늘도 피곤한 하루였어요",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010519.1.4",
          "form": "세상에 바로 퇴사하고 이직하고 싶어요",
          "original_form": "세상에 바로 퇴사하고 이직하고 싶어요",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010519.1.5",
          "form": "이직할 곳은 정했나요?",
          "original_form": "이직할 곳은 정했나요?",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010519.1.6",
          "form": "일단 내려가서 보름에서 한달정도는 쉬고 결정하고 싶어요",
          "original_form": "일단 내려가서 보름에서 한달정도는 쉬고 결정하고 싶어요",
          "speaker_id": "2",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010519.1.7",
          "form": "쉬면서 찾아보겠죠?",
          "original_form": "쉬면서 찾아보겠죠?",
          "speaker_id": "2",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010519.1.8",
          "form": "퇴사를 즐기는 것도 좋져",
          "original_form": "퇴사를 즐기는 것도 좋져",
          "speaker_id": "1",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010519.1.9",
          "form": "그쵸? 이렇게 지금까지 열일했는데 퇴사는 즐겨야한다고봐요",
          "original_form": "그쵸? 이렇게 지금까지 열일했는데 퇴사는 즐겨야한다고봐요",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010519.1.10",
          "form": "이야 스트레스가 정말 장난이 아니었어요",
          "original_form": "이야 스트레스가 정말 장난이 아니었어요",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010519.1.11",
          "form": "업무와 스트레스는 동의어가 아닐까여?",
          "original_form": "업무와 스트레스는 동의어가 아닐까여?",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010519.1.12",
          "form": "그 말에 동의해요! 업무=스트레스죠 솔직히 워라벨도 저세상 단어라고 생각해요",
          "original_form": "그 말에 동의해요! 업무=스트레스죠 솔직히 워라벨도 저세상 단어라고 생각해요",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010519.1.13",
          "form": "워가 있는 한 라벨은 없어요",
          "original_form": "워가 있는 한 라벨은 없어요",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010519.1.14",
          "form": "어떻게 그게 같이 존재할수가 있죠?",
          "original_form": "어떻게 그게 같이 존재할수가 있죠?",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010519.1.15",
          "form": "천국인가",
          "original_form": "천국인가",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010519.1.16",
          "form": "된다면 천국이긴 하겠네요",
          "original_form": "된다면 천국이긴 하겠네요",
          "speaker_id": "1",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010519.1.17",
          "form": "무직인 부자가 되고싶어요",
          "original_form": "무직인 부자가 되고싶어요",
          "speaker_id": "2",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010519.1.18",
          "form": "일하는거는 그냥 게으름을 부리기 위한 돈을 위해 하는거여서",
          "original_form": "일하는거는 그냥 게으름을 부리기 위한 돈을 위해 하는거여서",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010519.1.19",
          "form": "빠작벌고 놀고싶어요",
          "original_form": "빠작벌고 놀고싶어요",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010519.1.20",
          "form": "불로소득이 최종 목표군요?",
          "original_form": "불로소득이 최종 목표군요?",
          "speaker_id": "1",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010519.1.21",
          "form": "그렇죠 모든 사람의 꿈이 불로소득 아니겠어요?",
          "original_form": "그렇죠 모든 사람의 꿈이 불로소득 아니겠어요?",
          "speaker_id": "2",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010519.1.22",
          "form": "그래서 직장에서 받는 월급 외에 들어오는 돈이 너무 좋아요",
          "original_form": "그래서 직장에서 받는 월급 외에 들어오는 돈이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010519.1.23",
          "form": "그건 너무 좋져",
          "original_form": "그건 너무 좋져",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010519.1.24",
          "form": "일하기 너무 싫어요",
          "original_form": "일하기 너무 싫어요",
