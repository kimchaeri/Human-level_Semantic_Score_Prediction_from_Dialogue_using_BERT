      "utterance": [
        {
          "id": "MDRW2100042195.1.1",
          "form": "네네 한 3만원대",
          "original_form": "네네 한 3만원대",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042195.1.2",
          "form": "저도 포인트랑 쿠폰 모아서 샀어요ㅠ",
          "original_form": "저도 포인트랑 쿠폰 모아서 샀어요ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042195.1.3",
          "form": "악...볼터치만 3만원대인거죠?",
          "original_form": "악...볼터치만 3만원대인거죠?",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042195.1.4",
          "form": "네네 맞아요 진짜 비싸죠?",
          "original_form": "네네 맞아요 진짜 비싸죠?",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042195.1.5",
          "form": "전 썬크림 bb 볼터치 섀도 아라가 끝인데 뭘 더 해야하나요?",
          "original_form": "전 썬크림 bb 볼터치 섀도 아라가 끝인데 뭘 더 해야하나요?",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042195.1.6",
          "form": "비싼데 발색도 잘 안돼요",
          "original_form": "비싼데 발색도 잘 안돼요",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042195.1.7",
          "form": "아니요 더 안하셔도 될거같아요",
          "original_form": "아니요 더 안하셔도 될거같아요",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042195.1.8",
          "form": "마스카라?",
          "original_form": "마스카라?",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042195.1.9",
          "form": "앗.. 근데 왜 나스 사신거예요?",
          "original_form": "앗.. 근데 왜 나스 사신거예요?",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042195.1.10",
          "form": "전 아라 안해요 섀도우랑 마스카라 눈썹 볼터치",
          "original_form": "전 아라 안해요 섀도우랑 마스카라 눈썹 볼터치",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042195.1.11",
          "form": "평이 좋아서 샀죠",
          "original_form": "평이 좋아서 샀죠",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042195.1.12",
          "form": "실망했어요 ㅋㅋ발색 잘 안돼서ㅠㅠ",
          "original_form": "실망했어요 ㅋㅋ발색 잘 안돼서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042195.1.13",
          "form": "발색이 잘 안되서 막 발라도 되서 평이 좋았던걸까요??",
          "original_form": "발색이 잘 안되서 막 발라도 되서 평이 좋았던걸까요??",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042195.1.14",
          "form": "그런가봐요 ㅋㅋㅋ",
          "original_form": "그런가봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042195.1.15",
          "form": "투쿨포스큘 블러셔 조아요",
          "original_form": "투쿨포스큘 블러셔 조아요",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042195.1.16",
          "form": "저 마스카라 하면 쌍거플이 지거든요",
          "original_form": "저 마스카라 하면 쌍거플이 지거든요",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042195.1.17",
          "form": "그럼 눈이 넘나 불편해요 ㅠ",
          "original_form": "그럼 눈이 넘나 불편해요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042195.1.18",
          "form": "오? 마스카라 하면 쌍꺼풀 생겨요? 신기하다",
          "original_form": "오? 마스카라 하면 쌍꺼풀 생겨요? 신기하다",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042195.1.19",
          "form": "눈에 라인은 있는데 쌍꺼풀이 없거든요 좀만 들어 올면 생겨요 ㅋ 마스크라 하면 저 수술한줄 아셔서. .  ㅋㅋ",
          "original_form": "눈에 라인은 있는데 쌍꺼풀이 없거든요 좀만 들어 올면 생겨요 ㅋ 마스크라 하면 저 수술한줄 아셔서. .  ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042195.1.20",
          "form": "으아 ㅋㅋㅋㅋ 그렇균요",
          "original_form": "으아 ㅋㅋㅋㅋ 그렇균요",
