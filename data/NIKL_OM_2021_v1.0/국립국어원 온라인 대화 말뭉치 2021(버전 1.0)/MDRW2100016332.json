      "utterance": [
        {
          "id": "MDRW2100016332.1.1",
          "form": "둘째 바지 사야되는데 ㅋㅋㅋ",
          "original_form": "둘째 바지 사야되는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016332.1.2",
          "form": "아니 세일하는 것도 있고 안하는 것도 있는데",
          "original_form": "아니 세일하는 것도 있고 안하는 것도 있는데",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016332.1.3",
          "form": "고무줄 바지 캠핑 가서 막 입을거 6000원 주고 사고",
          "original_form": "고무줄 바지 캠핑 가서 막 입을거 6000원 주고 사고",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016332.1.4",
          "form": "첫째 학교 갈 때 입을 검은색 바지만 3000원 하길래 샀음",
          "original_form": "첫째 학교 갈 때 입을 검은색 바지만 3000원 하길래 샀음",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016332.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016332.1.6",
          "form": "좋은데?ㅋㅋ",
          "original_form": "좋은데?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016332.1.7",
          "form": "어디라고?",
          "original_form": "어디라고?",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016332.1.8",
          "form": "검은색바지 3천원아니고",
          "original_form": "검은색바지 3천원아니고",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016332.1.9",
          "form": "13천웍이다이",
          "original_form": "13천웍이다이",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016332.1.10",
          "form": "마이크로해서 ㅋㅋㅋㅋㄱ",
          "original_form": "마이크로해서 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016332.1.11",
          "form": "아하ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "아하ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016332.1.12",
          "form": "캠핑가서 막입을바지는",
          "original_form": "캠핑가서 막입을바지는",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016332.1.13",
          "form": "냉장고바지가?",
          "original_form": "냉장고바지가?",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016332.1.14",
          "form": "어 그냥 면바지",
          "original_form": "어 그냥 면바지",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016332.1.15",
          "form": "면얇은바지",
          "original_form": "면얇은바지",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016332.1.16",
          "form": "한철입고 마는거",
          "original_form": "한철입고 마는거",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016332.1.17",
          "form": "구멍나도 되는 ㅎㅎ",
          "original_form": "구멍나도 되는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016332.1.18",
          "form": "웅 좋네",
          "original_form": "웅 좋네",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016332.1.19",
          "form": "사러가야겠다ㅋ",
          "original_form": "사러가야겠다ㅋ",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016332.1.20",
          "form": "그래서 언니 ㅋㅋ어디라고?ㅋ",
          "original_form": "그래서 언니 ㅋㅋ어디라고?ㅋ",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016332.1.21",
          "form": "쇼핑가야겠다",
          "original_form": "쇼핑가야겠다",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016332.1.22",
          "form": "리틀제이니 ㅋㅋㅋ",
          "original_form": "리틀제이니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016332.1.23",
          "form": "어제 하루 세일한다고 했는데",
          "original_form": "어제 하루 세일한다고 했는데",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016332.1.24",
          "form": "왠지 오늘도 그냥 할듯 ㅋㅋㅋㅋ",
          "original_form": "왠지 오늘도 그냥 할듯 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016332.1.25",
          "form": "address",
          "original_form": "&address&",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016332.1.26",
          "form": "맞은 편",
          "original_form": "맞은 편",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016332.1.27",
          "form": "아아ㅋㅋ오키",
          "original_form": "아아ㅋㅋ오키",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016332.1.28",
          "form": "좀이따가 가봐야겠다ㅋㅋ",
          "original_form": "좀이따가 가봐야겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016332.1.29",
          "form": "굿굿ㅋㅋ",
          "original_form": "굿굿ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016332.1.30",
          "form": "좀 쉬었나??",
          "original_form": "좀 쉬었나??",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016332.1.31",
          "form": "우리 신랑은 지금 내일 캠핑 갈 준비물 사러 마트 갔음",
          "original_form": "우리 신랑은 지금 내일 캠핑 갈 준비물 사러 마트 갔음",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016332.1.32",
          "form": "서온수산에서 새우사 봤나??",
          "original_form": "서온수산에서 새우사 봤나??",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016332.1.33",
          "form": "오 좋다ㅋㅋㅋ",
          "original_form": "오 좋다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016332.1.34",
          "form": "아니 맛있나 들어는봤다ㅋ",
          "original_form": "아니 맛있나 들어는봤다ㅋ",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016332.1.35",
          "form": "어 나중에 놀러 갈 때 한번 사 봐",
          "original_form": "어 나중에 놀러 갈 때 한번 사 봐",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016332.1.36",
          "form": "아님 집에서 꾸 워 먹어도 되고",
          "original_form": "아님 집에서 꾸 워 먹어도 되고",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016332.1.37",
          "form": "크기도 크고 정말 맛있음",
          "original_form": "크기도 크고 정말 맛있음",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016332.1.38",
          "form": "응 그래야겠다. 새우맛있ㅣ",
          "original_form": "응 그래야겠다. 새우맛있ㅣ",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016332.1.39",
          "form": "맛있지",
          "original_form": "맛있지",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016332.1.40",
          "form": "지금 급 먹고싶어지네ㅋ",
          "original_form": "지금 급 먹고싶어지네ㅋ",
          "speaker_id": "1",
