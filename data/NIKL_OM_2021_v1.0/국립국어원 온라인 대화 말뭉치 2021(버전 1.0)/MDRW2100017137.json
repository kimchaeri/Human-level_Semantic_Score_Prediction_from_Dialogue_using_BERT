      "utterance": [
        {
          "id": "MDRW2100017137.1.1",
          "form": "요즘 보시는 유튜브 있으세요",
          "original_form": "요즘 보시는 유튜브 있으세요",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017137.1.2",
          "form": "유툽은 많이 안보구",
          "original_form": "유툽은 많이 안보구",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017137.1.3",
          "form": "영화 넷플 왓챠 많이봐요",
          "original_form": "영화 넷플 왓챠 많이봐요",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017137.1.4",
          "form": "저는 유튜브랑 넷플이요",
          "original_form": "저는 유튜브랑 넷플이요",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017137.1.5",
          "form": "왓챠도 넷플 같은거에요?",
          "original_form": "왓챠도 넷플 같은거에요?",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017137.1.6",
          "form": "네 ㅎㅎ 비슷해요",
          "original_form": "네 ㅎㅎ 비슷해요",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017137.1.7",
          "form": "영화가 좀더 많은 느낌?",
          "original_form": "영화가 좀더 많은 느낌?",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017137.1.8",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017137.1.9",
          "form": "넷플은 찾는 영화는 잘 안보여요요",
          "original_form": "넷플은 찾는 영화는 잘 안보여요요",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017137.1.10",
          "form": "저는 넷플 기묘한이야기 재밋더라구요",
          "original_form": "저는 넷플 기묘한이야기 재밋더라구요",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017137.1.11",
          "form": "오 기묘한 이야기 추천 많이 받았어요",
          "original_form": "오 기묘한 이야기 추천 많이 받았어요",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017137.1.12",
          "form": "저는 본 영화 또보고 또보고 해요 ㅎㅎ",
          "original_form": "저는 본 영화 또보고 또보고 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017137.1.13",
          "form": "앗 저 영화덕훈데\r\n재밋게본거 열번도 더봐요",
          "original_form": "앗 저 영화덕훈데\r\n재밋게본거 열번도 더봐요",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017137.1.14",
          "form": "보고또보고",
          "original_form": "보고또보고",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017137.1.15",
          "form": "노래도 한곡 반복이나 좋은거 무한반복 스타일 이에요 ㅋㅋ",
          "original_form": "노래도 한곡 반복이나 좋은거 무한반복 스타일 이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017137.1.16",
          "form": "오정말요 가장 좋아하는 영화 뭐에요",
          "original_form": "오정말요 가장 좋아하는 영화 뭐에요",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017137.1.17",
          "form": "덕후에게 힘든질문 ㅎㅎ",
          "original_form": "덕후에게 힘든질문 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017137.1.18",
          "form": "일단 봉감독님 영화 좋아해요",
          "original_form": "일단 봉감독님 영화 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017137.1.19",
          "form": "오 저 봉감독님 싸인있어요",
          "original_form": "오 저 봉감독님 싸인있어요",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017137.1.20",
          "form": "학교에 강연오셨응때 받아뒀어요ㅠㅠㅠㅠ근데 어딨는지 모르겠어요ㅠㅠㅠ",
          "original_form": "학교에 강연오셨응때 받아뒀어요ㅠㅠㅠㅠ근데 어딨는지 모르겠어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017137.1.21",
          "form": "헉ㅜㅜㅜㅜㅜ대박ㅠㅠ",
          "original_form": "헉ㅜㅜㅜㅜㅜ대박ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017137.1.22",
          "form": "우와우!!!!",
          "original_form": "우와우!!!!",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017137.1.23",
          "form": "다시찾아봐야겠어요 가보인데",
          "original_form": "다시찾아봐야겠어요 가보인데",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017137.1.24",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017137.1.25",
          "form": "사진도 같이 찍었었는데 저 어디 외장하드에 들어있을거같아요",
          "original_form": "사진도 같이 찍었었는데 저 어디 외장하드에 들어있을거같아요",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017137.1.26",
          "form": "좋아하는 영화 궁금해요 ㅎㅅㅎ",
          "original_form": "좋아하는 영화 궁금해요 ㅎㅅㅎ",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017137.1.27",
          "form": "저는 지금 만나러 갑니다랑 왕대륙 주연 장난스런 키스 좋아해요",
          "original_form": "저는 지금 만나러 갑니다랑 왕대륙 주연 장난스런 키스 좋아해요",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017137.1.28",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017137.1.29",
          "form": "지금만나러갑니다 슬펐어요",
