      "utterance": [
        {
          "id": "MDRW2100016703.1.1",
          "form": "최근에 쇼핑하신 거 있으세요?",
          "original_form": "최근에 쇼핑하신 거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016703.1.2",
          "form": "저는 동물의숲 타이틀 샀어요",
          "original_form": "저는 동물의숲 타이틀 샀어요",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016703.1.3",
          "form": "와 그거 재밌어보이더라구요",
          "original_form": "와 그거 재밌어보이더라구요",
          "speaker_id": "1",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016703.1.4",
          "form": "소소하게 내 섬 꾸미는 재미가있어요",
          "original_form": "소소하게 내 섬 꾸미는 재미가있어요",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016703.1.5",
          "form": "아기자기하니 귀엽더라구요 ㅋㅋㅋ",
          "original_form": "아기자기하니 귀엽더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016703.1.6",
          "form": "쇼핑하신거 있나요?",
          "original_form": "쇼핑하신거 있나요?",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016703.1.7",
          "form": "전 헤어오일 샀어요",
          "original_form": "전 헤어오일 샀어요",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016703.1.8",
          "form": "오프라인구매하셨나요?",
          "original_form": "오프라인구매하셨나요?",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016703.1.9",
          "form": "전 온라인으로 많이 사요 ㅋㅋㅋ",
          "original_form": "전 온라인으로 많이 사요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016703.1.10",
          "form": "확실히 요즘은 더 온라인구매거 좋더라구요",
          "original_form": "확실히 요즘은 더 온라인구매거 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016703.1.11",
          "form": "맞아요 최저가라 좋아요",
          "original_form": "맞아요 최저가라 좋아요",
          "speaker_id": "1",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016703.1.12",
          "form": "저는 동숲 중고거래로 샀어요",
          "original_form": "저는 동숲 중고거래로 샀어요",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016703.1.13",
          "form": "중고거라하시나요?",
          "original_form": "중고거라하시나요?",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016703.1.14",
          "form": "전 팔아본적 있어요",
          "original_form": "전 팔아본적 있어요",
          "speaker_id": "1",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016703.1.15",
          "form": "뭐 파셨나요?",
          "original_form": "뭐 파셨나요?",
