      "utterance": [
        {
          "id": "MDRW2100023404.1.1",
          "form": "금방하겟네요ㅎㅎ",
          "original_form": "금방하겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023404.1.2",
          "form": "육아는 안힘드세요?",
          "original_form": "육아는 안힘드세요?",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023404.1.3",
          "form": "아이가 좀  그래도 남아치곤 얌전해요",
          "original_form": "아이가 좀  그래도 남아치곤 얌전해요",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023404.1.4",
          "form": "좋겟어요ㅠ",
          "original_form": "좋겟어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023404.1.5",
          "form": "저희딸은... 에너지가 넘칩니다ㅠ",
          "original_form": "저희딸은... 에너지가 넘칩니다ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023404.1.6",
          "form": "비글미가 엄청 나서 맨날 데리고 나가요ㅠ",
          "original_form": "비글미가 엄청 나서 맨날 데리고 나가요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023404.1.7",
          "form": "ㅋ 에너지는 요기도 넘쳐요 큐큐",
          "original_form": "ㅋ 에너지는 요기도 넘쳐요 큐큐",
          "speaker_id": "1",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023404.1.8",
          "form": "주말에는 앱테크 안하는게ㅎ 애기랑 놀러가야해서ㅎ",
          "original_form": "주말에는 앱테크 안하는게ㅎ 애기랑 놀러가야해서ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023404.1.9",
          "form": "아까도 차에 가만잇다 햇지나요",
          "original_form": "아까도 차에 가만잇다 햇지나요",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023404.1.10",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023404.1.11",
          "form": "어디가신거에요?",
          "original_form": "어디가신거에요?",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023404.1.12",
          "form": "아뇨ㅠㅎㅎ",
          "original_form": "아뇨ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023404.1.13",
          "form": "아이 유투브를 딱 볼수 있는게",
          "original_form": "아이 유투브를 딱 볼수 있는게",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023404.1.14",
          "form": "차로 이동할때거든요",
          "original_form": "차로 이동할때거든요",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023404.1.15",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023404.1.16",
          "form": "유튜브는 우리의 찐친이죠ㅎ",
          "original_form": "유튜브는 우리의 찐친이죠ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.17",
          "form": "그래서 조금보고 싶다해서  잠깐 나간거요 ㅎㅎ",
          "original_form": "그래서 조금보고 싶다해서  잠깐 나간거요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.18",
          "form": "목욕할때 필요해요ㅎ",
          "original_form": "목욕할때 필요해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.19",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.20",
          "form": "차외엔 안보는거로 알아서",
          "original_form": "차외엔 안보는거로 알아서",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.21",
          "form": "아아ㅎㅎㅎ",
          "original_form": "아아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.22",
          "form": "규칙을 정하셨군요ㅎ",
          "original_form": "규칙을 정하셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.23",
          "form": "목욕할때 울어요?",
          "original_form": "목욕할때 울어요?",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.24",
          "form": "머리 캡 썼는대도",
          "original_form": "머리 캡 썼는대도",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.25",
          "form": "무섭대요ㅠ",
          "original_form": "무섭대요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.26",
          "form": "네네 다 규칙정해노니 스스로 잘해요",
          "original_form": "네네 다 규칙정해노니 스스로 잘해요",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023404.1.27",
          "form": "아 ㅡㅁ ㅡ",
          "original_form": "아 ㅡㅁ ㅡ",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.28",
          "form": "근데 웃긴게",
          "original_form": "근데 웃긴게",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.29",
          "form": "그럴수있어요 귀요미 ㅎㅎㅎ",
          "original_form": "그럴수있어요 귀요미 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.30",
          "form": "유튜브보묜 가만히있음ㅎㅎㅎ",
          "original_form": "유튜브보묜 가만히있음ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.31",
          "form": "아무것도안하고ㅎㅎㅎ",
          "original_form": "아무것도안하고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.32",
          "form": "아 저도 집에서 딱 볼때 잇네요",
          "original_form": "아 저도 집에서 딱 볼때 잇네요",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.33",
          "form": "아주 귀여워서ㅎ 맨날 무섭대요ㅎ",
          "original_form": "아주 귀여워서ㅎ 맨날 무섭대요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.34",
          "form": "머리 커트 할때 ㅋㅋ 제가 하거든요",
          "original_form": "머리 커트 할때 ㅋㅋ 제가 하거든요",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.35",
          "form": "뭐만하면 무섭다고하고 싫다고 삐지고ㅎ",
          "original_form": "뭐만하면 무섭다고하고 싫다고 삐지고ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.36",
          "form": "오오ㅎㅎ",
          "original_form": "오오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.37",
          "form": "귀엽겟내요ㅎㅎ",
          "original_form": "귀엽겟내요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023404.1.38",
          "form": "바가지로 짜르시는거아니죠?",
          "original_form": "바가지로 짜르시는거아니죠?",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023404.1.39",
          "form": "유부브호 배웠어요ㅠㅜㅋ",
          "original_form": "유부브호 배웠어요ㅠㅜㅋ",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023404.1.40",
          "form": "고급지게 투블럭 ㅋㅋ",
          "original_form": "고급지게 투블럭 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023404.1.41",
          "form": "같은 말임 ㅋ",
          "original_form": "같은 말임 ㅋ",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023404.1.42",
          "form": "우와ㅎㅎㅎㅎ",
          "original_form": "우와ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023404.1.43",
          "form": "머리도 짜르시고 능력자이심ㅎ",
          "original_form": "머리도 짜르시고 능력자이심ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:40"
        }
      ]
