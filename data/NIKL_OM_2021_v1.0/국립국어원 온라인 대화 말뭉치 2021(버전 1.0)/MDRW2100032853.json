      "utterance": [
        {
          "id": "MDRW2100032853.1.1",
          "form": "갤럭시 폴드가 머야",
          "original_form": "갤럭시 폴드가 머야",
          "speaker_id": "1",
          "time": "20210906 02:07"
        },
        {
          "id": "MDRW2100032853.1.2",
          "form": "접히는 그거 맞나?",
          "original_form": "접히는 그거 맞나?",
          "speaker_id": "1",
          "time": "20210906 02:07"
        },
        {
          "id": "MDRW2100032853.1.3",
          "form": "ㅇㅇ 플립은 일반 핸드폰 반접은거",
          "original_form": "ㅇㅇ 플립은 일반 핸드폰 반접은거",
          "speaker_id": "2",
          "time": "20210906 02:08"
        },
        {
          "id": "MDRW2100032853.1.4",
          "form": "ㅇㅎ 헷갈리네",
          "original_form": "ㅇㅎ 헷갈리네",
          "speaker_id": "1",
          "time": "20210906 02:09"
        },
        {
          "id": "MDRW2100032853.1.5",
          "form": "ㅋㅋㅋㅋ꾸미기 좋아보이던데",
          "original_form": "ㅋㅋㅋㅋ꾸미기 좋아보이던데",
          "speaker_id": "1",
          "time": "20210906 02:09"
        },
        {
          "id": "MDRW2100032853.1.6",
          "form": "그것때문에 지금 핸드폰 약정끝나면 바꿀까 말까 고민중...",
          "original_form": "그것때문에 지금 핸드폰 약정끝나면 바꿀까 말까 고민중...",
          "speaker_id": "2",
          "time": "20210906 02:10"
        },
        {
          "id": "MDRW2100032853.1.7",
          "form": "닌테도 배경화면 설정해둔거 보니까 너무 탐나ㅠㅠ",
          "original_form": "닌테도 배경화면 설정해둔거 보니까 너무 탐나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 02:10"
        },
        {
          "id": "MDRW2100032853.1.8",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "1",
          "time": "20210906 02:10"
        },
        {
          "id": "MDRW2100032853.1.9",
          "form": "ㅠㅠㅠ그거진짜 귀엽고 추억돋더라",
          "original_form": "ㅠㅠㅠ그거진짜 귀엽고 추억돋더라",
          "speaker_id": "1",
          "time": "20210906 02:10"
        },
        {
          "id": "MDRW2100032853.1.10",
          "form": "흠...근데 성능은 별루더라",
          "original_form": "흠...근데 성능은 별루더라",
          "speaker_id": "1",
          "time": "20210906 02:10"
        },
        {
          "id": "MDRW2100032853.1.11",
          "form": "성능 많이 별로인가? 배경이 아무리 이뻐도 성능이 개구리면 좀 그런데...",
          "original_form": "성능 많이 별로인가? 배경이 아무리 이뻐도 성능이 개구리면 좀 그런데...",
          "speaker_id": "2",
          "time": "20210906 02:11"
        },
        {
          "id": "MDRW2100032853.1.12",
          "form": "유튜브 봤는데",
          "original_form": "유튜브 봤는데",
          "speaker_id": "1",
          "time": "20210906 02:12"
        },
        {
          "id": "MDRW2100032853.1.13",
          "form": "3년 쓴 아이폰보다..느리더라고....",
          "original_form": "3년 쓴 아이폰보다..느리더라고....",
          "speaker_id": "1",
          "time": "20210906 02:12"
        },
        {
          "id": "MDRW2100032853.1.14",
          "form": "삼성답지않다",
          "original_form": "삼성답지않다",
          "speaker_id": "1",
          "time": "20210906 02:12"
        },
        {
          "id": "MDRW2100032853.1.15",
          "form": "어떻게 3년 쓴 아이폰보다 신제품이 더 느릴 수가 있엌ㅋㅋㅋㅋ",
          "original_form": "어떻게 3년 쓴 아이폰보다 신제품이 더 느릴 수가 있엌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 02:13"
        },
        {
          "id": "MDRW2100032853.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 02:13"
        },
        {
          "id": "MDRW2100032853.1.17",
          "form": "렉 에바야",
          "original_form": "렉 에바야",
          "speaker_id": "1",
          "time": "20210906 02:13"
        },
        {
          "id": "MDRW2100032853.1.18",
          "form": "한번 찾아봐",
          "original_form": "한번 찾아봐",
          "speaker_id": "1",
          "time": "20210906 02:14"
        },
        {
          "id": "MDRW2100032853.1.19",
          "form": "난 정확하지 않을 수 있어",
          "original_form": "난 정확하지 않을 수 있어",
          "speaker_id": "1",
          "time": "20210906 02:14"
        },
        {
          "id": "MDRW2100032853.1.20",
          "form": "렉걸리는 것만큼 빡치는 것도 없지ㅋㅋㅋㅋ",
          "original_form": "렉걸리는 것만큼 빡치는 것도 없지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 02:14"
        },
        {
          "id": "MDRW2100032853.1.21",
          "form": "렉이랑 발열... 이것만 괜찮으면 좋은데ㅠㅠ",
          "original_form": "렉이랑 발열... 이것만 괜찮으면 좋은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 02:14"
        },
        {
          "id": "MDRW2100032853.1.22",
          "form": "헉 발열은 생각도 못했네",
          "original_form": "헉 발열은 생각도 못했네",
          "speaker_id": "1",
          "time": "20210906 02:15"
        },
        {
          "id": "MDRW2100032853.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ발열심하면 배터리 쑥쑥 닳잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ발열심하면 배터리 쑥쑥 닳잖아",
          "speaker_id": "1",
          "time": "20210906 02:15"
        },
        {
          "id": "MDRW2100032853.1.24",
          "form": "근데 뭐 엄청 느리다는 아니구",
          "original_form": "근데 뭐 엄청 느리다는 아니구",
          "speaker_id": "1",
          "time": "20210906 02:16"
        },
        {
          "id": "MDRW2100032853.1.25",
          "form": "그냥 성능이 오잉?싶다정도",
          "original_form": "그냥 성능이 오잉?싶다정도",
          "speaker_id": "1",
          "time": "20210906 02:16"
        },
        {
          "id": "MDRW2100032853.1.26",
          "form": "엄청 느린것만 아니라면 기냥저냥 쓸 수도 있을 것 같은데 어느 정도인지는 한 번 찾아봐야겠네..",
          "original_form": "엄청 느린것만 아니라면 기냥저냥 쓸 수도 있을 것 같은데 어느 정도인지는 한 번 찾아봐야겠네..",
          "speaker_id": "2",
          "time": "20210906 02:17"
        },
        {
          "id": "MDRW2100032853.1.27",
          "form": "아 그정돈 아니야",
          "original_form": "아 그정돈 아니야",
          "speaker_id": "1",
          "time": "20210906 02:18"
        },
        {
          "id": "MDRW2100032853.1.28",
          "form": "진짜 그정돈 아니고",
          "original_form": "진짜 그정돈 아니고",
          "speaker_id": "1",
          "time": "20210906 02:18"
        },
        {
          "id": "MDRW2100032853.1.29",
          "form": "아이폰에비해 후달린다고???였어",
          "original_form": "아이폰에비해 후달린다고???였어",
          "speaker_id": "1",
          "time": "20210906 02:18"
        },
        {
          "id": "MDRW2100032853.1.30",
          "form": "아이폰이 쫌 빨랐으 ㅋ",
          "original_form": "아이폰이 쫌 빨랐으 ㅋ",
          "speaker_id": "1",
          "time": "20210906 02:18"
        },
        {
          "id": "MDRW2100032853.1.31",
          "form": "확실히 성능은 아이폰이 더 괜찮나 보네",
          "original_form": "확실히 성능은 아이폰이 더 괜찮나 보네",
          "speaker_id": "2",
          "time": "20210906 02:18"
        },
        {
          "id": "MDRW2100032853.1.32",
          "form": "아이폰도 신제품이었으면 비교가 더 됐으려나??",
          "original_form": "아이폰도 신제품이었으면 비교가 더 됐으려나??",
          "speaker_id": "2",
          "time": "20210906 02:18"
        },
        {
