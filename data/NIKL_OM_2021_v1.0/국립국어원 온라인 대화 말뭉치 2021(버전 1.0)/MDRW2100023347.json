      "utterance": [
        {
          "id": "MDRW2100023347.1.1",
          "form": "근데 이젠 제가더 수다 스러운거 같음 ㅋ",
          "original_form": "근데 이젠 제가더 수다 스러운거 같음 ㅋ",
          "speaker_id": "1",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023347.1.2",
          "form": "name1이 열정적으로 이야기하셔서ㅎㅎ 수다쟁이인 제가 할말이없음ㅎㅎㅎ",
          "original_form": "&name1&이 열정적으로 이야기하셔서ㅎㅎ 수다쟁이인 제가 할말이없음ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023347.1.3",
          "form": "엇ㅎㅎ 통했당ㅎ",
          "original_form": "엇ㅎㅎ 통했당ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023347.1.4",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023347.1.5",
          "form": "수다쟁이가 좋아요ㅎㅎ",
          "original_form": "수다쟁이가 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023347.1.6",
          "form": "아 글구 사적이라거 이야기한건 음음",
          "original_form": "아 글구 사적이라거 이야기한건 음음",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.7",
          "form": "뒤끗없고 ㅡㅎㅎ",
          "original_form": "뒤끗없고 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.8",
          "form": "왜요?ㄹ",
          "original_form": "왜요?ㄹ",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.9",
          "form": "name2도 뭔가 아픔이 잇듯이 저도 아젝 풀지못한 숙제가좀 많아서 ㅎㅎㅎ",
          "original_form": "&name2&도 뭔가 아픔이 잇듯이 저도 아젝 풀지못한 숙제가좀 많아서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.10",
          "form": "이야기안하셔도되요 ㅡㅎ",
          "original_form": "이야기안하셔도되요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.11",
          "form": "아아ㅎ",
          "original_form": "아아ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.12",
          "form": "네네 ㅎㅎㅎ",
          "original_form": "네네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.13",
          "form": "그럼 나중에 더 친해지면 해주세요ㅎ",
          "original_form": "그럼 나중에 더 친해지면 해주세요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.14",
          "form": "지금은 남남임ㅎㅎㅎ",
          "original_form": "지금은 남남임ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023347.1.15",
          "form": "수다쟁이일뿐ㅎ",
          "original_form": "수다쟁이일뿐ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023347.1.16",
          "form": "아 친해서 못하는게 아니라",
          "original_form": "아 친해서 못하는게 아니라",
          "speaker_id": "1",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023347.1.17",
          "form": "아아~",
          "original_form": "아아~",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023347.1.18",
          "form": "저 조차도 정리가 안되있는 상태거든요 ㅎㅎ",
          "original_form": "저 조차도 정리가 안되있는 상태거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023347.1.19",
          "form": "에이 민감한 부분이면 굳이 이야기하실필요없어요ㅎ",
          "original_form": "에이 민감한 부분이면 굳이 이야기하실필요없어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023347.1.20",
          "form": "얍얍 ㅎㅎ",
