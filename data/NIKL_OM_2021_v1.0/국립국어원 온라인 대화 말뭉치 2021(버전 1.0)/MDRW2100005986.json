        },
        {
          "id": "MDRW2100005986.1.3",
          "form": "아 정기적으로 신청하세요?",
          "original_form": "아 정기적으로 신청하세요?",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100005986.1.4",
          "form": "저는 쌀국슈 배달 기다려요",
          "original_form": "저는 쌀국슈 배달 기다려요",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100005986.1.5",
          "form": "와이프가 애들반찬으로 간간히 시켜요",
          "original_form": "와이프가 애들반찬으로 간간히 시켜요",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005986.1.6",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005986.1.7",
          "form": "저희집 근초엔 반찬 노맛이라ㅠㅠ",
          "original_form": "저희집 근초엔 반찬 노맛이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005986.1.8",
          "form": "그러시군요 배달쌀국수는 맛있는집인가요",
          "original_form": "그러시군요 배달쌀국수는 맛있는집인가요",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100005986.1.9",
          "form": "네네 저희 상가에있는 맛집이에요",
          "original_form": "네네 저희 상가에있는 맛집이에요",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100005986.1.10",
          "form": "저도 쌀국수 좋아하는데 부럽네요",
          "original_form": "저도 쌀국수 좋아하는데 부럽네요",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100005986.1.11",
          "form": "배불렀어요",
          "original_form": "배불렀어요",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100005986.1.12",
