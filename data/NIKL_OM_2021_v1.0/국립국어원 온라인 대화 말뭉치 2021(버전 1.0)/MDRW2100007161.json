      "utterance": [
        {
          "id": "MDRW2100007161.1.1",
          "form": "헐...! 정말요?",
          "original_form": "헐...! 정말요?",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007161.1.2",
          "form": "카카오사장님 빙모상이라고 사진이 나오는데",
          "original_form": "카카오사장님 빙모상이라고 사진이 나오는데",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007161.1.3",
          "form": "박지윤이랑 나란히 있는사진이더라구요",
          "original_form": "박지윤이랑 나란히 있는사진이더라구요",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007161.1.4",
          "form": "내가 아는 그 박지윤맞나? 싶었어요",
          "original_form": "내가 아는 그 박지윤맞나? 싶었어요",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007161.1.5",
          "form": "언제 결혼했데? 했더니 비공개로 했다고",
          "original_form": "언제 결혼했데? 했더니 비공개로 했다고",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007161.1.6",
          "form": "와 소식도 모르다가 갑자기 그렇게 보이면 완전 놀라셨겠어요",
          "original_form": "와 소식도 모르다가 갑자기 그렇게 보이면 완전 놀라셨겠어요",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007161.1.7",
          "form": "네네 카카오사모님이라니 부럽다며 ㅎㅎㅎ",
          "original_form": "네네 카카오사모님이라니 부럽다며 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007161.1.8",
          "form": "그러게요 카카오 사모님... 완전 대기업... 대박이에요",
          "original_form": "그러게요 카카오 사모님... 완전 대기업... 대박이에요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007161.1.9",
          "form": "그러니까요 인생사 알수가 없다며 ㅎㅎㅎ",
          "original_form": "그러니까요 인생사 알수가 없다며 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007161.1.10",
          "form": "님 아뒤보니 온앤오프 프로그램에 성시경 생각나네요 ㅎㅎ",
          "original_form": "님 아뒤보니 온앤오프 프로그램에 성시경 생각나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007161.1.11",
          "form": "아 온앤오프 프로그램!! 즐겨보셨나요?",
          "original_form": "아 온앤오프 프로그램!! 즐겨보셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007161.1.12",
          "form": "가끔 좋아하는배우나오면 봤어요",
          "original_form": "가끔 좋아하는배우나오면 봤어요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007161.1.13",
          "form": "어떻게 사는가 궁금해서요 ㅎ",
          "original_form": "어떻게 사는가 궁금해서요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007161.1.14",
          "form": "성시경 어떻게보세요?",
          "original_form": "성시경 어떻게보세요?",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007161.1.15",
          "form": "성시경 노래 너무 좋아해요!!",
          "original_form": "성시경 노래 너무 좋아해요!!",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007161.1.16",
          "form": "목소리가 좋아요",
          "original_form": "목소리가 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007161.1.17",
          "form": "글쵸 ㅎㅎ",
          "original_form": "글쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007161.1.18",
          "form": "그래서 저 실은 아들이름 성시경으로 하고싶었어요 ㅎㅎ",
          "original_form": "그래서 저 실은 아들이름 성시경으로 하고싶었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007161.1.19",
          "form": "헉 정말요?",
          "original_form": "헉 정말요?",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007161.1.20",
          "form": "멋진데요!!",
          "original_form": "멋진데요!!",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007161.1.21",
          "form": "근데 결과적으로는 name3이 되었지만요 ㅎ",
          "original_form": "근데 결과적으로는 &name3&이 되었지만요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007161.1.22",
          "form": "헐 이름 완전 예뻐요",
          "original_form": "헐 이름 완전 예뻐요",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007161.1.23",
          "form": "일부러 비슷하게 지으신 건가요?",
          "original_form": "일부러 비슷하게 지으신 건가요?",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007161.1.24",
          "form": "신랑성이 다행이 성씨라 ㅋ",
          "original_form": "신랑성이 다행이 성씨라 ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007161.1.25",
          "form": "성시경으로하자고 제가 그랬거든요 ㅋ",
          "original_form": "성시경으로하자고 제가 그랬거든요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007161.1.26",
          "form": "성시경처럼 목소리도 체격도 머찌라고 ㅎㅎ",
          "original_form": "성시경처럼 목소리도 체격도 머찌라고 ㅎㅎ",
