        {
          "id": "MDRW2100030908.1.1",
          "form": "결국 뭐",
          "original_form": "결국 뭐",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030908.1.2",
          "form": "아트집같은 거 보면",
          "original_form": "아트집같은 거 보면",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030908.1.3",
          "form": "다시 만난 거 같긴 하던데",
          "original_form": "다시 만난 거 같긴 하던데",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030908.1.4",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030908.1.5",
          "form": "아닌가...",
          "original_form": "아닌가...",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030908.1.6",
          "form": "해피엔딩 너무 좋아요",
          "original_form": "해피엔딩 너무 좋아요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030908.1.7",
          "form": "사실 기억이 잘 안나네요",
          "original_form": "사실 기억이 잘 안나네요",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030908.1.8",
          "form": "그냥 그렇게 생각할겡6",
          "original_form": "그냥 그렇게 생각할겡6",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
