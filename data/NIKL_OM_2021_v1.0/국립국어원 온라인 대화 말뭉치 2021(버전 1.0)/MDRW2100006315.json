      "utterance": [
        {
          "id": "MDRW2100006315.1.1",
          "form": "안녕하세요 게임 하시나요",
          "original_form": "안녕하세요 게임 하시나요",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006315.1.2",
          "form": "예 물론요",
          "original_form": "예 물론요",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006315.1.3",
          "form": "어떤 게임하시나요",
          "original_form": "어떤 게임하시나요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006315.1.4",
          "form": "대전략 게임 합니다",
          "original_form": "대전략 게임 합니다",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006315.1.5",
          "form": "대전략 게임이면 롤인가요?",
          "original_form": "대전략 게임이면 롤인가요?",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006315.1.6",
          "form": "어...",
          "original_form": "어...",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006315.1.7",
          "form": "문명이라던가...",
          "original_form": "문명이라던가...",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006315.1.8",
          "form": "그런거요",
          "original_form": "그런거요",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006315.1.9",
          "form": "아하 문명.. 한번 본거같아요",
          "original_form": "아하 문명.. 한번 본거같아요",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006315.1.10",
          "form": "시작하면 한달은 기본이라던데",
          "original_form": "시작하면 한달은 기본이라던데",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006315.1.11",
          "form": "에이 한달은",
          "original_form": "에이 한달은",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006315.1.12",
          "form": "과장이죠",
          "original_form": "과장이죠",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006315.1.13",
          "form": "라지만 실제로",
          "original_form": "라지만 실제로",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006315.1.14",
          "form": "한 세션(?)을 그만큼 오래 끈 적이 있긴 있군요",
          "original_form": "한 세션(?)을 그만큼 오래 끈 적이 있긴 있군요",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006315.1.15",
          "form": "ㄷㄷ 인내력이 대단하시네요",
          "original_form": "ㄷㄷ 인내력이 대단하시네요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006315.1.16",
          "form": "저는 그런식의 알피지게임이랑 오래하는걸 못해서",
          "original_form": "저는 그런식의 알피지게임이랑 오래하는걸 못해서",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006315.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006315.1.18",
          "form": "롤만 열심히합니다 ㅋㅋㅋ",
          "original_form": "롤만 열심히합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006315.1.19",
          "form": "주로 뭐 하시나요",
          "original_form": "주로 뭐 하시나요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006315.1.20",
          "form": "아아 롤",
          "original_form": "아아 롤",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006315.1.21",
          "form": "저는 예전에 했었는데",
          "original_form": "저는 예전에 했었는데",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006315.1.22",
          "form": "시즌 2~3때",
          "original_form": "시즌 2~3때",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006315.1.23",
          "form": "그땐 저는 이런걸 왜하지 하다가",
          "original_form": "그땐 저는 이런걸 왜하지 하다가",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006315.1.24",
          "form": "어쩌다보니 아직도 할게없어서",
          "original_form": "어쩌다보니 아직도 할게없어서",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006315.1.25",
          "form": "하고있네요 ㅋㅋㅋㅋ",
          "original_form": "하고있네요 ㅋㅋㅋㅋ",
