        },
        {
          "id": "MDRW2100028190.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028190.1.3",
          "form": "아뇨! 취준해요!",
          "original_form": "아뇨! 취준해요!",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028190.1.4",
          "form": "전 일해오",
          "original_form": "전 일해오",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028190.1.5",
          "form": "집안일이요?",
          "original_form": "집안일이요?",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028190.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028190.1.7",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028190.1.8",
          "form": "그렇구나 청소 하고나면 재밌는데 힘든 일이죠",
          "original_form": "그렇구나 청소 하고나면 재밌는데 힘든 일이죠",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028190.1.9",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028190.1.10",
          "form": "맞아요 좁아서 움직이기도 힘들고 습하고",
          "original_form": "맞아요 좁아서 움직이기도 힘들고 습하고",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028190.1.11",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028190.1.12",
          "form": "맞아요 곰팡이는 더싫어요",
          "original_form": "맞아요 곰팡이는 더싫어요",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028190.1.13",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028190.1.14",
          "form": "물때 지우는것도 일이구..",
          "original_form": "물때 지우는것도 일이구..",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028190.1.15",
          "form": "곰팡이안 생기게 자주햐죠",
          "original_form": "곰팡이안 생기게 자주햐죠",
          "speaker_id": "1",
