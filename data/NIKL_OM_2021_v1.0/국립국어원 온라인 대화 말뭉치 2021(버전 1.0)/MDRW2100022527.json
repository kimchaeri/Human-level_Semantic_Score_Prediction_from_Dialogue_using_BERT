      "utterance": [
        {
          "id": "MDRW2100022527.1.1",
          "form": "강아지입장엔 좋은선물이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "강아지입장엔 좋은선물이네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022527.1.2",
          "form": "다이소 가성비 갑 물건은~?",
          "original_form": "다이소 가성비 갑 물건은~?",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022527.1.3",
          "form": "전 1위 갑티슈요 ㅋㅋ",
          "original_form": "전 1위 갑티슈요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022527.1.4",
          "form": "음...전부 가성비있는거같아욬ㅋㅋㅋㅋ",
          "original_form": "음...전부 가성비있는거같아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022527.1.5",
          "form": "ㅋㅋㅋ크리넥스꺼 쓰다가 다이소꺼 써봤는데",
          "original_form": "ㅋㅋㅋ크리넥스꺼 쓰다가 다이소꺼 써봤는데",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022527.1.6",
          "form": "저는 면봉..?",
          "original_form": "저는 면봉..?",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022527.1.7",
          "form": "품질 차이 못느끼겠어요",
          "original_form": "품질 차이 못느끼겠어요",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.8",
          "form": "오오 그렇군요 저도 사봐야겟어요",
          "original_form": "오오 그렇군요 저도 사봐야겟어요",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.9",
          "form": "갑티슈 엄청 싸요 하나에 천원밖에 안해요",
          "original_form": "갑티슈 엄청 싸요 하나에 천원밖에 안해요",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.10",
          "form": "근데 200매에요",
          "original_form": "근데 200매에요",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.11",
          "form": "저도 다른 가게에서 면봉샀는데",
          "original_form": "저도 다른 가게에서 면봉샀는데",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.13",
          "form": "종이면봉좋던데요 진짜",
          "original_form": "종이면봉좋던데요 진짜",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ추처",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ추처",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.15",
          "form": "추천합니다",
          "original_form": "추천합니다",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.16",
          "form": "아 ㅋㅋㅋ 종이면봉 진짜 사봐야겠어요",
          "original_form": "아 ㅋㅋㅋ 종이면봉 진짜 사봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.17",
          "form": "400개씩 들어있다고 했죠?",
          "original_form": "400개씩 들어있다고 했죠?",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.18",
          "form": "네 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.19",
          "form": "휴 지금도 집에 400개 정도 남았는데 언제 다 쓰지",
          "original_form": "휴 지금도 집에 400개 정도 남았는데 언제 다 쓰지",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.20",
          "form": "아 그리구 미니휴지알아요?",
          "original_form": "아 그리구 미니휴지알아요?",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022527.1.21",
          "form": "미니휴지는 뭐에요?",
          "original_form": "미니휴지는 뭐에요?",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022527.1.22",
          "form": "그 휴대용 작은 휴지에요",
          "original_form": "그 휴대용 작은 휴지에요",
          "speaker_id": "2",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022527.1.23",
          "form": "아 그걸 미니휴지라고 불러요?",
          "original_form": "아 그걸 미니휴지라고 불러요?",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022527.1.24",
          "form": "나갈때마다 들고다니기좋아요!",
          "original_form": "나갈때마다 들고다니기좋아요!",
          "speaker_id": "2",
          "time": "20210829 15:26"
        }
      ]
    }
