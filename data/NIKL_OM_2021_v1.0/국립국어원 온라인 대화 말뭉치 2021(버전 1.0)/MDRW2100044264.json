      "utterance": [
        {
          "id": "MDRW2100044264.1.1",
          "form": "오늘은 비가 안와서",
          "original_form": "오늘은 비가 안와서",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100044264.1.2",
          "form": "훨 낫네요 출근길이",
          "original_form": "훨 낫네요 출근길이",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100044264.1.3",
          "form": "오늘부터 맑을거같더라구요",
          "original_form": "오늘부터 맑을거같더라구요",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100044264.1.4",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100044264.1.5",
          "form": "비 안왔음 좋겠어요",
          "original_form": "비 안왔음 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100044264.1.6",
          "form": "가을날씨가 이번엔 좀 오래갔으면좋겠네요ㅜ",
          "original_form": "가을날씨가 이번엔 좀 오래갔으면좋겠네요ㅜ",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100044264.1.7",
          "form": "금세 겨울 오는건 아닌지 ㅠ",
          "original_form": "금세 겨울 오는건 아닌지 ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100044264.1.8",
          "form": "걱정돼요",
          "original_form": "걱정돼요",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100044264.1.9",
          "form": "겨울옷꺼내놓으셨나요",
          "original_form": "겨울옷꺼내놓으셨나요",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100044264.1.10",
          "form": "넼ㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100044264.1.11",
          "form": "겨울 싫어해서 벌써 좀 추운거같아요",
          "original_form": "겨울 싫어해서 벌써 좀 추운거같아요",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100044264.1.12",
          "form": "비오는날엔",
          "original_form": "비오는날엔",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100044264.1.13",
          "form": "저도 겨울옷꺼내놨어요ㅜ 여름옷은 얼마입지도 못하구요ㅜ",
          "original_form": "저도 겨울옷꺼내놨어요ㅜ 여름옷은 얼마입지도 못하구요ㅜ",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100044264.1.14",
          "form": "이번 여름 진짜 짧았던것같아요",
          "original_form": "이번 여름 진짜 짧았던것같아요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100044264.1.15",
          "form": "코로나때문에 더 짧게 느껴진것같아요ㅋㅋ",
          "original_form": "코로나때문에 더 짧게 느껴진것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100044264.1.16",
          "form": "잘ㄴㅏ가질 못해서..",
          "original_form": "잘ㄴㅏ가질 못해서..",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100044264.1.17",
          "form": "맞아요 비도 자주오고해서",
          "original_form": "맞아요 비도 자주오고해서",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100044264.1.18",
          "form": "쨍한 날씨 본적이 별로 없네요",
          "original_form": "쨍한 날씨 본적이 별로 없네요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100044264.1.19",
          "form": "햇빛나는 날씨가 자주 있으면 좋겠는데 이번에 태풍 또  오게되면 비오겠죠ㅜ?",
          "original_form": "햇빛나는 날씨가 자주 있으면 좋겠는데 이번에 태풍 또  오게되면 비오겠죠ㅜ?",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100044264.1.20",
          "form": "헉 태풍이 또 오나요?ㅠ",
          "original_form": "헉 태풍이 또 오나요?ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100044264.1.21",
          "form": "이번에 뭐 생겼다고 또 그러는거같더라구요ㅜ?",
          "original_form": "이번에 뭐 생겼다고 또 그러는거같더라구요ㅜ?",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100044264.1.22",
          "form": "세상에...비가 또",
          "original_form": "세상에...비가 또",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100044264.1.23",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
