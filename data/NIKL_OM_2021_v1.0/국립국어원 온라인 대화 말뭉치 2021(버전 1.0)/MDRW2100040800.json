      "utterance": [
        {
          "id": "MDRW2100040800.1.1",
          "form": "저랑 비슷하네요",
          "original_form": "저랑 비슷하네요",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040800.1.2",
          "form": "카페인에 약해서 커피 마시면 잠을 잘 못자요",
          "original_form": "카페인에 약해서 커피 마시면 잠을 잘 못자요",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040800.1.3",
          "form": "근데  향은 너무 좋지 않나요?",
          "original_form": "근데  향은 너무 좋지 않나요?",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040800.1.4",
          "form": "네 커피향은 좋아해요~",
          "original_form": "네 커피향은 좋아해요~",
          "speaker_id": "1",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040800.1.5",
          "form": "그래서 주변 사람들 커피 내리면 향 맡고 좋아해요ㅎㅎ",
          "original_form": "그래서 주변 사람들 커피 내리면 향 맡고 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040800.1.6",
          "form": "저도 향은 포기 못해서 원두 내려서 향만 맡은적도 있어요",
          "original_form": "저도 향은 포기 못해서 원두 내려서 향만 맡은적도 있어요",
          "speaker_id": "2",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040800.1.7",
          "form": "카페인 상관없는 사람이 참 부러워요",
          "original_form": "카페인 상관없는 사람이 참 부러워요",
          "speaker_id": "2",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040800.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040800.1.9",
          "form": "카페인에 강한 분들",
          "original_form": "카페인에 강한 분들",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040800.1.10",
          "form": "커피 10잔 마셔도 끄덕없다는 분도 있더라구요",
          "original_form": "커피 10잔 마셔도 끄덕없다는 분도 있더라구요",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040800.1.11",
          "form": "어떻게 그럴 수 있는지",
          "original_form": "어떻게 그럴 수 있는지",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040800.1.12",
          "form": "와 진짜 세시네요 그분은",
          "original_form": "와 진짜 세시네요 그분은",
          "speaker_id": "1",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040800.1.13",
          "form": "가족들 일어나면 아침식사 하시는 건가요?",
          "original_form": "가족들 일어나면 아침식사 하시는 건가요?",
          "speaker_id": "1",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100040800.1.14",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100040800.1.15",
          "form": "그런데 우리 가족들이 다들 일찍 일어나요",
          "original_form": "그런데 우리 가족들이 다들 일찍 일어나요",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100040800.1.16",
          "form": "그래서 아침도 일찍 먹어요",
          "original_form": "그래서 아침도 일찍 먹어요",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100040800.1.17",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100040800.1.18",
          "form": "저희도 평일엔 일찍 먹는데",
          "original_form": "저희도 평일엔 일찍 먹는데",
          "speaker_id": "1",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100040800.1.19",
          "form": "주말엔 9시쯤 먹어요",
          "original_form": "주말엔 9시쯤 먹어요",
          "speaker_id": "1",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100040800.1.20",
          "form": "저희는 평일 주말 따로 없이 아침 6시쯤이면 다들 일어나서",
          "original_form": "저희는 평일 주말 따로 없이 아침 6시쯤이면 다들 일어나서",
          "speaker_id": "2",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100040800.1.21",
          "form": "7시엔 먹어야 해요",
          "original_form": "7시엔 먹어야 해요",
          "speaker_id": "2",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100040800.1.22",
          "form": "와 주말에도 다들 일찍 일어나서 식사하시는군요",
          "original_form": "와 주말에도 다들 일찍 일어나서 식사하시는군요",
          "speaker_id": "1",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100040800.1.23",
          "form": "좋아하시는 음식 있으세요?",
          "original_form": "좋아하시는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100040800.1.24",
          "form": "저는 국을 좋아해요. 그래서 밥할때 국을 꼭 하죠",
          "original_form": "저는 국을 좋아해요. 그래서 밥할때 국을 꼭 하죠",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040800.1.25",
          "form": "그러시군요! 국 있으면 확실히 밥 먹기에 좋죠",
          "original_form": "그러시군요! 국 있으면 확실히 밥 먹기에 좋죠",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100040800.1.26",
          "form": "식구들 중에 제가 제일 국을 좋아해서요",
          "original_form": "식구들 중에 제가 제일 국을 좋아해서요",
          "speaker_id": "2",
          "time": "20210905 07:59"
        }
      ]
    }
  ]
