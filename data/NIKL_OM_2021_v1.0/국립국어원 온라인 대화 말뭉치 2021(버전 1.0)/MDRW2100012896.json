      "utterance": [
        {
          "id": "MDRW2100012896.1.1",
          "form": "아 같이 부부가 균등하면 괜찮죠",
          "original_form": "아 같이 부부가 균등하면 괜찮죠",
          "speaker_id": "2",
          "time": "20210825 07:32"
        },
        {
          "id": "MDRW2100012896.1.2",
          "form": "이따 8시 50분정도에 움직임 봐야겠어요",
          "original_form": "이따 8시 50분정도에 움직임 봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 07:32"
        },
        {
          "id": "MDRW2100012896.1.3",
          "form": "사람수로 받으니",
          "original_form": "사람수로 받으니",
          "speaker_id": "2",
          "time": "20210825 07:32"
        },
        {
          "id": "MDRW2100012896.1.4",
          "form": "오늘 한개 또 있던데",
          "original_form": "오늘 한개 또 있던데",
          "speaker_id": "1",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.5",
          "form": "네 요샌 1분전에 막 빠져서",
          "original_form": "네 요샌 1분전에 막 빠져서",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.6",
          "form": "일진하이솔루스",
          "original_form": "일진하이솔루스",
          "speaker_id": "1",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.7",
          "form": "장난질 심해요",
          "original_form": "장난질 심해요",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.9",
          "form": "저도 눈치껐넣게",
          "original_form": "저도 눈치껐넣게",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.10",
          "form": "대신이랑 현대차가",
          "original_form": "대신이랑 현대차가",
          "speaker_id": "1",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.11",
          "form": "이체 대기중요",
          "original_form": "이체 대기중요",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.13",
          "form": "경쟁 낮아뵈던데ㅎㅎ",
          "original_form": "경쟁 낮아뵈던데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.14",
          "form": "많이 받을것 같죠",
          "original_form": "많이 받을것 같죠",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.15",
          "form": "비례도 하시나요?",
          "original_form": "비례도 하시나요?",
          "speaker_id": "1",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.16",
          "form": "조금요 ㅎㅎ",
          "original_form": "조금요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:33"
        },
        {
          "id": "MDRW2100012896.1.17",
          "form": "공모주을 친구가 알려줘서 학원비 벌어요 ㅎㅎ",
          "original_form": "공모주을 친구가 알려줘서 학원비 벌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:34"
        },
        {
          "id": "MDRW2100012896.1.18",
          "form": "비례는 한 몇백 넣어도",
          "original_form": "비례는 한 몇백 넣어도",
          "speaker_id": "1",
          "time": "20210825 07:34"
        },
        {
          "id": "MDRW2100012896.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 07:34"
        },
        {
          "id": "MDRW2100012896.1.20",
          "form": "1개 이래서 ㅋㅋ",
          "original_form": "1개 이래서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 07:34"
        },
        {
          "id": "MDRW2100012896.1.21",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:34"
        },
        {
          "id": "MDRW2100012896.1.22",
          "form": "블로그 보니까",
          "original_form": "블로그 보니까",
          "speaker_id": "1",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.23",
          "form": "공모주 잘하는 분들은",
          "original_form": "공모주 잘하는 분들은",
          "speaker_id": "1",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.24",
          "form": "오늘 잘팔아서 소고기 먹어야 하는데 ㅋㅋ",
          "original_form": "오늘 잘팔아서 소고기 먹어야 하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.25",
          "form": "진짜 직장 다니는 급으로",
          "original_form": "진짜 직장 다니는 급으로",
          "speaker_id": "1",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.26",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "2",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.27",
          "form": "돈 버시던데ㅋㅋ",
          "original_form": "돈 버시던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.28",
          "form": "전 그정도는 못돼요 ㅋㅋ",
          "original_form": "전 그정도는 못돼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.29",
          "form": "부러워요 ㅋㅋ",
          "original_form": "부러워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.30",
          "form": "가족 명의로 몇개씩들 하셔서 ㅎㅎ",
          "original_form": "가족 명의로 몇개씩들 하셔서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:35"
        },
        {
          "id": "MDRW2100012896.1.31",
          "form": "제친구는 그래요 ㅎㅎ",
          "original_form": "제친구는 그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:36"
        },
        {
          "id": "MDRW2100012896.1.32",
          "form": "돈의 규모가 커야 그런거 같아요",
          "original_form": "돈의 규모가 커야 그런거 같아요",
          "speaker_id": "2",
          "time": "20210825 07:36"
        },
        {
          "id": "MDRW2100012896.1.33",
          "form": "가족도많고",
          "original_form": "가족도많고",
          "speaker_id": "2",
          "time": "20210825 07:36"
        },
        {
          "id": "MDRW2100012896.1.34",
          "form": "일하실때 오찌 파세요?",
          "original_form": "일하실때 오찌 파세요?",
          "speaker_id": "2",
          "time": "20210825 07:36"
        },
        {
          "id": "MDRW2100012896.1.35",
          "form": "그때 눈치 보이죠 ㅋㅋ저희동생 피해서 파는데 맨날 눈치 보인다고 그러더라구요",
          "original_form": "그때 눈치 보이죠 ㅋㅋ저희동생 피해서 파는데 맨날 눈치 보인다고 그러더라구요",
          "speaker_id": "2",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.36",
          "form": "그 눈치껏 ㅋㅋㅋㅋ",
          "original_form": "그 눈치껏 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.37",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.38",
          "form": "젊은 직원들이랑",
          "original_form": "젊은 직원들이랑",
          "speaker_id": "1",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.39",
          "form": "있을때는",
          "original_form": "있을때는",
          "speaker_id": "1",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.40",
          "form": "같이 팔고 그래요 ㅎㅎ",
          "original_form": "같이 팔고 그래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.41",
          "form": "오 같이 하면 두배 잼난데",
          "original_form": "오 같이 하면 두배 잼난데",
          "speaker_id": "2",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.42",
          "form": "오좋네요",
          "original_form": "오좋네요",
          "speaker_id": "2",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012896.1.43",
          "form": "저도 같이 중계하며 팔아요 ㅋㅋ",
          "original_form": "저도 같이 중계하며 팔아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.44",
          "form": "오떨어잔가 팔자 ㅋㅋ이러며요",
          "original_form": "오떨어잔가 팔자 ㅋㅋ이러며요",
          "speaker_id": "2",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.45",
          "form": "같이 막 올라가서",
          "original_form": "같이 막 올라가서",
          "speaker_id": "1",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.46",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.47",
          "form": "진다",
          "original_form": "진다",
          "speaker_id": "2",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.48",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.49",
          "form": "올라가도 그렇죠",
          "original_form": "올라가도 그렇죠",
          "speaker_id": "2",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.50",
          "form": "근데 카뱅때 너무",
          "original_form": "근데 카뱅때 너무",
          "speaker_id": "1",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.51",
          "form": "빨리 팔아서ㅠ",
          "original_form": "빨리 팔아서ㅠ",
          "speaker_id": "1",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.52",
          "form": "빨리 팔았어요",
          "original_form": "빨리 팔았어요",
          "speaker_id": "2",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.53",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:38"
        },
        {
          "id": "MDRW2100012896.1.54",
          "form": "전 지나감 뭐 안봐요",
          "original_form": "전 지나감 뭐 안봐요",
          "speaker_id": "2",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.55",
          "form": "배아프니까 ㅋㅋ",
          "original_form": "배아프니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.56",
          "form": "그게 맞는 자세인데",
          "original_form": "그게 맞는 자세인데",
          "speaker_id": "1",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.57",
          "form": "갯수도 많았는데 ㅡㅡ헉 수입이 반토막 났죠 ㅋㅋ",
          "original_form": "갯수도 많았는데 ㅡㅡ헉 수입이 반토막 났죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.58",
          "form": "전 아무리 그래도",
          "original_form": "전 아무리 그래도",
          "speaker_id": "2",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.59",
          "form": "오르면 열받고",
          "original_form": "오르면 열받고",
          "speaker_id": "1",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.60",
          "form": "내리면 꼬시고",
          "original_form": "내리면 꼬시고",
          "speaker_id": "1",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.61",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.62",
          "form": "무조건 빨리고고입니다",
          "original_form": "무조건 빨리고고입니다",
          "speaker_id": "2",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.63",
          "form": "네 ㅋㅋ내리면 깨봈죠",
          "original_form": "네 ㅋㅋ내리면 깨봈죠",
          "speaker_id": "2",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.64",
          "form": "볶죠",
          "original_form": "볶죠",
          "speaker_id": "2",
          "time": "20210825 07:39"
        },
        {
          "id": "MDRW2100012896.1.65",
          "form": "전 이게 하나의 일이라 매일 방송보고 그래요",
          "original_form": "전 이게 하나의 일이라 매일 방송보고 그래요",
          "speaker_id": "2",
          "time": "20210825 07:40"
        },
        {
          "id": "MDRW2100012896.1.66",
          "form": "다른 알바보다 편하고 좋은거 같아요",
          "original_form": "다른 알바보다 편하고 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210825 07:40"
        },
        {
          "id": "MDRW2100012896.1.67",
          "form": "ㅋㅋ우리 엄마 같으신데요",
          "original_form": "ㅋㅋ우리 엄마 같으신데요",
          "speaker_id": "1",
          "time": "20210825 07:40"
        },
        {
          "id": "MDRW2100012896.1.68",
          "form": "네 그런가요 ㅋㅋ",
          "original_form": "네 그런가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:40"
        },
        {
          "id": "MDRW2100012896.1.69",
          "form": "엄마도 유튜브 라이브 보고",
          "original_form": "엄마도 유튜브 라이브 보고",
          "speaker_id": "1",
          "time": "20210825 07:40"
        },
