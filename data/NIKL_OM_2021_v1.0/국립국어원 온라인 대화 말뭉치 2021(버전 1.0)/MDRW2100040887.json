      "utterance": [
        {
          "id": "MDRW2100040887.1.1",
          "form": "소스는 우리가 만들면 되는디",
          "original_form": "소스는 우리가 만들면 되는디",
          "speaker_id": "2",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100040887.1.2",
          "form": "고민이 되는구먼",
          "original_form": "고민이 되는구먼",
          "speaker_id": "2",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100040887.1.3",
          "form": "오늘 점심은 백신퍼슨이 쏩니다",
          "original_form": "오늘 점심은 백신퍼슨이 쏩니다",
          "speaker_id": "2",
          "time": "20210905 11:19"
        },
        {
          "id": "MDRW2100040887.1.4",
          "form": "사랑합니다~ 4인에 우동사리",
          "original_form": "사랑합니다~ 4인에 우동사리",
          "speaker_id": "1",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100040887.1.5",
          "form": "부탁드려요",
          "original_form": "부탁드려요",
          "speaker_id": "1",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100040887.1.6",
          "form": "철판주꾸미?",
          "original_form": "철판주꾸미?",
          "speaker_id": "2",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100040887.1.7",
          "form": "쭈삼?",
          "original_form": "쭈삼?",
          "speaker_id": "2",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100040887.1.8",
          "form": "쿠폰 받아주세용",
          "original_form": "쿠폰 받아주세용",
          "speaker_id": "1",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100040887.1.9",
          "form": "4인?",
          "original_form": "4인?",
          "speaker_id": "2",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100040887.1.10",
          "form": "넹 4인",
          "original_form": "넹 4인",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100040887.1.11",
          "form": "콩나물은 따로, 포함?",
          "original_form": "콩나물은 따로, 포함?",
          "speaker_id": "2",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100040887.1.12",
          "form": "콩나물 따로 하시쥬",
          "original_form": "콩나물 따로 하시쥬",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100040887.1.13",
          "form": "쭈꾸미로",
          "original_form": "쭈꾸미로",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100040887.1.14",
          "form": "노삼겹",
          "original_form": "노삼겹",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100040887.1.15",
          "form": "계란찜은 기본/게살/치즈?",
          "original_form": "계란찜은 기본/게살/치즈?",
          "speaker_id": "2",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100040887.1.16",
          "form": "게살은 일단 탈락",
          "original_form": "게살은 일단 탈락",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100040887.1.17",
          "form": "엄마 게살 드셔도 괜찬ㄹ나?",
          "original_form": "엄마 게살 드셔도 괜찬ㄹ나?",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040887.1.18",
          "form": "우동사리 추가?",
          "original_form": "우동사리 추가?",
          "speaker_id": "2",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040887.1.19",
          "form": "ㄴㅔㅇ",
          "original_form": "ㄴㅔㅇ",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040887.1.20",
          "form": "우동사리 추가~~~~~",
          "original_form": "우동사리 추가~~~~~",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040887.1.21",
          "form": "꼭 조리로 합쉬다",
          "original_form": "꼭 조리로 합쉬다",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040887.1.22",
          "form": "리뷰이벤트는 음료/날치알/공기밥",
          "original_form": "리뷰이벤트는 음료/날치알/공기밥",
          "speaker_id": "2",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040887.1.23",
          "form": "중 뭘로?",
          "original_form": "중 뭘로?",
          "speaker_id": "2",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040887.1.24",
          "form": "날치밥",
          "original_form": "날치밥",
          "speaker_id": "1",
          "time": "20210905 11:24"
        },
        {
          "id": "MDRW2100040887.1.25",
          "form": "아빠가 사랑하심",
