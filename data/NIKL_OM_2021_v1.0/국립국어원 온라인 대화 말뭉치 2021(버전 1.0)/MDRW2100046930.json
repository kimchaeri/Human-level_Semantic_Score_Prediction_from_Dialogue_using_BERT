        },
        {
          "id": "MDRW2100046930.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046930.1.3",
          "form": "틱톡 써보셨나요?",
          "original_form": "틱톡 써보셨나요?",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046930.1.4",
          "form": "이벤트 한다고 한번 깔았다가 하고 지웠어요",
          "original_form": "이벤트 한다고 한번 깔았다가 하고 지웠어요",
          "speaker_id": "1",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046930.1.5",
          "form": "아핰ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아핰ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046930.1.6",
          "form": "무슨 이벤트였나요?",
          "original_form": "무슨 이벤트였나요?",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046930.1.7",
          "form": "롤 관련 아이템 주는 이벤트였던거 같아요",
          "original_form": "롤 관련 아이템 주는 이벤트였던거 같아요",
          "speaker_id": "1",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046930.1.8",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20211108 13:26"
        },
        {
          "id": "MDRW2100046930.1.9",
          "form": "롤 하시나요??ㅋㅋㅋㅋ",
          "original_form": "롤 하시나요??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 13:26"
        },
        {
          "id": "MDRW2100046930.1.10",
          "form": "자주 했었는데 안한지 조금 됐네요 ㅎ",
          "original_form": "자주 했었는데 안한지 조금 됐네요 ㅎ",
          "speaker_id": "1",
          "time": "20211108 13:26"
        },
        {
          "id": "MDRW2100046930.1.11",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046930.1.12",
          "form": "롤이 진짜",
          "original_form": "롤이 진짜",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046930.1.13",
          "form": "스테디하게 잘나가네요...",
          "original_form": "스테디하게 잘나가네요...",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046930.1.14",
          "form": "오버워치나 배그는 좀 시들해지는거 같은데",
          "original_form": "오버워치나 배그는 좀 시들해지는거 같은데",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046930.1.15",
          "form": "롤은 정말 꾸준한거같아요 인기가",
          "original_form": "롤은 정말 꾸준한거같아요 인기가",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046930.1.16",
          "form": "맞아요 몇년동안 꾸준한거 같아요",
          "original_form": "맞아요 몇년동안 꾸준한거 같아요",
          "speaker_id": "1",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046930.1.17",
          "form": "롤을 능가할 재밌는 게임이 없어서 그런거겠죠 ㅎㅎ",
          "original_form": "롤을 능가할 재밌는 게임이 없어서 그런거겠죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.18",
          "form": "주로 어디 가시나요",
          "original_form": "주로 어디 가시나요",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.20",
          "form": "탑 미드 정글 또 뭐있더라",
          "original_form": "탑 미드 정글 또 뭐있더라",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.21",
          "form": "기억이 안나네요 너무 옛날에 해서 ㅎ",
          "original_form": "기억이 안나네요 너무 옛날에 해서 ㅎ",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.22",
          "form": "그런가봐요 ㅎㅎㅎ",
          "original_form": "그런가봐요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.23",
          "form": "그걸 만들어낸 제작진들이 대단하게 느껴져요",
          "original_form": "그걸 만들어낸 제작진들이 대단하게 느껴져요",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.24",
          "form": "전 주로 미드 탑 했었어요",
          "original_form": "전 주로 미드 탑 했었어요",
          "speaker_id": "1",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046930.1.25",
          "form": "요즘은 디아블로2 리저렉션 하는중인데 얼른 디아4가 나왔음 좋겠네요",
          "original_form": "요즘은 디아블로2 리저렉션 하는중인데 얼른 디아4가 나왔음 좋겠네요",
          "speaker_id": "1",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046930.1.26",
          "form": "와 디아블로 하시는군요",
          "original_form": "와 디아블로 하시는군요",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046930.1.27",
          "form": "ㅋㅋㅋㅋ 제 친구 중 한 명도 지금",
          "original_form": "ㅋㅋㅋㅋ 제 친구 중 한 명도 지금",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046930.1.28",
          "form": "디아블로에 빠졌던데...",
          "original_form": "디아블로에 빠졌던데...",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046930.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046930.1.30",
          "form": "위험한 게임이에요",
          "original_form": "위험한 게임이에요",
          "speaker_id": "2",
          "time": "20211108 13:37"
        }
      ]
