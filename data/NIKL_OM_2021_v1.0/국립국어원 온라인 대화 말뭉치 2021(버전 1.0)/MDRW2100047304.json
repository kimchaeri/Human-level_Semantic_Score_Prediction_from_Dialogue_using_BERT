      "utterance": [
        {
          "id": "MDRW2100047304.1.1",
          "form": "아이들 어린이집 다니나요?",
          "original_form": "아이들 어린이집 다니나요?",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100047304.1.2",
          "form": "첫째는 다니구요:)",
          "original_form": "첫째는 다니구요:)",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100047304.1.3",
          "form": "둘째는 내년부터 보낼까 생각중이에요",
          "original_form": "둘째는 내년부터 보낼까 생각중이에요",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100047304.1.4",
          "form": "둘째는 집에있군요\r\n힘드시겠어요",
          "original_form": "둘째는 집에있군요\r\n힘드시겠어요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100047304.1.5",
          "form": "코로나때문에\r\n보내기도 참 그래요ㅠㅠ",
          "original_form": "코로나때문에\r\n보내기도 참 그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100047304.1.6",
          "form": "밥챙겨주는것도 쉽지않더라구요",
          "original_form": "밥챙겨주는것도 쉽지않더라구요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100047304.1.7",
          "form": "맞아요 애기들 마스크쓰기도 힘든데 보내기 참그렇죠",
          "original_form": "맞아요 애기들 마스크쓰기도 힘든데 보내기 참그렇죠",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100047304.1.8",
          "form": "맞아요! 밥 하나만 해결되도\r\n엄만 진짜 할 일이 하나 줄죵",
          "original_form": "맞아요! 밥 하나만 해결되도\r\n엄만 진짜 할 일이 하나 줄죵",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100047304.1.9",
          "form": "조리원 동기들 아가들은\r\n지금 2명 원에 다니고 있는데\r\n원에서도 확진자 나오니",
          "original_form": "조리원 동기들 아가들은\r\n지금 2명 원에 다니고 있는데\r\n원에서도 확진자 나오니",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100047304.1.10",
          "form": "델고있는게 맞는것도 같고ㅠ",
          "original_form": "델고있는게 맞는것도 같고ㅠ",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100047304.1.11",
          "form": "매일 고민이에요",
          "original_form": "매일 고민이에요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100047304.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100047304.1.13",
          "form": "시기가 이러니...더그래요",
          "original_form": "시기가 이러니...더그래요",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047304.1.14",
          "form": "보내도 데리고있어도 힘드니요",
          "original_form": "보내도 데리고있어도 힘드니요",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047304.1.15",
          "form": "여기는 대구거든요..\r\n요며칠 100명가까이 찍고있어요 ㅠ",
          "original_form": "여기는 대구거든요..\r\n요며칠 100명가까이 찍고있어요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047304.1.16",
          "form": "헉... 하루에요?",
          "original_form": "헉... 하루에요?",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047304.1.17",
          "form": "네 ㅜ",
          "original_form": "네 ㅜ",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047304.1.18",
          "form": "얼마전까진 30명정도였는데",
          "original_form": "얼마전까진 30명정도였는데",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047304.1.19",
          "form": "지역이 어디세요?",
          "original_form": "지역이 어디세요?",
          "speaker_id": "1",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100047304.1.20",
          "form": "저는 경기에요ㅜㅜ",
          "original_form": "저는 경기에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100047304.1.21",
          "form": "와... 하루 100명은\r\n진짜 너무 심하네요ㅠㅠ",
          "original_form": "와... 하루 100명은\r\n진짜 너무 심하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.22",
          "form": "경기도 많지않아요??",
          "original_form": "경기도 많지않아요??",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.23",
          "form": "교회나 마트같은곳\r\n집단 감염 있었던건가요? ㅠ",
          "original_form": "교회나 마트같은곳\r\n집단 감염 있었던건가요? ㅠ",
          "speaker_id": "2",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.24",
          "form": "언니네가 용인인데.. 거기도 많은거같더라구요",
          "original_form": "언니네가 용인인데.. 거기도 많은거같더라구요",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.25",
          "form": "네 병원집단감염 이라고하네요 ㅠ",
          "original_form": "네 병원집단감염 이라고하네요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.26",
          "form": "저희는 요새 20명 초반대요 ㅠㅠ",
          "original_form": "저희는 요새 20명 초반대요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.27",
          "form": "아고ㅠㅠ 병원에서ㅠㅠ",
          "original_form": "아고ㅠㅠ 병원에서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.28",
          "form": "아 그래도 괜찮네요 그정도면",
          "original_form": "아 그래도 괜찮네요 그정도면",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047304.1.29",
          "form": "서울은 장난아니겠죠 ㅜ",
          "original_form": "서울은 장난아니겠죠 ㅜ",
          "speaker_id": "1",
          "time": "20210826 11:14"
        }
      ]
    }
  ]
