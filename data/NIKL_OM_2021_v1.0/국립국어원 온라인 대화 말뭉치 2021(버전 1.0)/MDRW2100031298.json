      "utterance": [
        {
          "id": "MDRW2100031298.1.1",
          "form": "아니요 무슨 책이죠",
          "original_form": "아니요 무슨 책이죠",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031298.1.2",
          "form": "나는 자연인이다 해서 나왔는데요",
          "original_form": "나는 자연인이다 해서 나왔는데요",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.3",
          "form": "어 그 TV 봤어요",
          "original_form": "어 그 TV 봤어요",
          "speaker_id": "2",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.4",
          "form": "거기 출연한 자연인이었어요",
          "original_form": "거기 출연한 자연인이었어요",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.5",
          "form": "자연에 대한 책인가요",
          "original_form": "자연에 대한 책인가요",
          "speaker_id": "2",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.6",
          "form": "아니요 거기 나오는 요 환이라는 사람이 좋은 일을 너무 많이 했더라구요",
          "original_form": "아니요 거기 나오는 요 환이라는 사람이 좋은 일을 너무 많이 했더라구요",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.7",
          "form": "무슨 일을 한 거죠",
          "original_form": "무슨 일을 한 거죠",
          "speaker_id": "2",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.8",
          "form": "삼풍백화점 붕괴 사건 아시죠",
          "original_form": "삼풍백화점 붕괴 사건 아시죠",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.9",
          "form": "알죠",
          "original_form": "알죠",
          "speaker_id": "2",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031298.1.10",
          "form": "그때도 자연인이었는데 선뜻 나서서 인문 구조를 하셨더라구요",
          "original_form": "그때도 자연인이었는데 선뜻 나서서 인문 구조를 하셨더라구요",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031298.1.11",
          "form": "훌륭하신 분이네요",
          "original_form": "훌륭하신 분이네요",
          "speaker_id": "2",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031298.1.12",
          "form": "뿐만 아니라 사회 민주화운동도 하셨더라구요",
          "original_form": "뿐만 아니라 사회 민주화운동도 하셨더라구요",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031298.1.13",
          "form": "근데 왜 자연인이 되셨을까요",
          "original_form": "근데 왜 자연인이 되셨을까요",
          "speaker_id": "2",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031298.1.14",
          "form": "요새 공호주에 관련된 책을 읽고 있어요",
          "original_form": "요새 공호주에 관련된 책을 읽고 있어요",
          "speaker_id": "1",
          "time": "20210904 20:14"
