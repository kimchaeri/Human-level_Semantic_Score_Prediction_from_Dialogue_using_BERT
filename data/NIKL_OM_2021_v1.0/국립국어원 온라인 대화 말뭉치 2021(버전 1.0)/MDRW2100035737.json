          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100035737.1.21",
          "form": "그죠!",
          "original_form": "그죠!",
          "speaker_id": "1",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100035737.1.22",
