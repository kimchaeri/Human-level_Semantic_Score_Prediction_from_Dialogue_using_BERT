      "utterance": [
        {
          "id": "MDRW2100035737.1.1",
          "form": "방탈출게임 좋아하시나요?",
          "original_form": "방탈출게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100035737.1.2",
          "form": "오프라인으로 하는거 좋아해요!",
          "original_form": "오프라인으로 하는거 좋아해요!",
          "speaker_id": "2",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100035737.1.3",
          "form": "이시국이라 친구들과 온라인으로 이야기하면서 어플로하는 게임있는데",
          "original_form": "이시국이라 친구들과 온라인으로 이야기하면서 어플로하는 게임있는데",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035737.1.4",
          "form": "해보셨나요 그런거?",
          "original_form": "해보셨나요 그런거?",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035737.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035737.1.6",
          "form": "해봤어요",
          "original_form": "해봤어요",
          "speaker_id": "2",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035737.1.7",
          "form": "리얼게임",
          "original_form": "리얼게임",
          "speaker_id": "2",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035737.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100035737.1.9",
          "form": "저도 그이야기하려고한거예요",
          "original_form": "저도 그이야기하려고한거예요",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100035737.1.10",
          "form": "리얼월드네요",
          "original_form": "리얼월드네요",
          "speaker_id": "2",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100035737.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
