      "utterance": [
        {
          "id": "MDRW2100030001.1.1",
          "form": "일 한지 몇년 되셧어요",
          "original_form": "일 한지 몇년 되셧어요",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100030001.1.2",
          "form": "구년입니다",
          "original_form": "구년입니다",
          "speaker_id": "2",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100030001.1.3",
          "form": "아이고지겹겟다",
          "original_form": "아이고지겹겟다",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100030001.1.4",
          "form": "ㅜㅜㅋㅋㅋㅋ",
          "original_form": "ㅜㅜㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100030001.1.5",
          "form": "사람들 조아요?",
          "original_form": "사람들 조아요?",
          "speaker_id": "1",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100030001.1.6",
          "form": "관련 공공기관 연구원 같은 곳 찾아서 가시면 좋을 거 같아요",
          "original_form": "관련 공공기관 연구원 같은 곳 찾아서 가시면 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100030001.1.7",
          "form": "여자 분이 다니시에게는 진짜 좋습니다",
          "original_form": "여자 분이 다니시에게는 진짜 좋습니다",
          "speaker_id": "2",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100030001.1.8",
          "form": "그춍 ,,  사기업은 넘 빡셀듯 ㅋㅋ",
          "original_form": "그춍 ,,  사기업은 넘 빡셀듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100030001.1.9",
          "form": "건설 연구원 이런 정출연 같은 연구원 있나요있나요",
          "original_form": "건설 연구원 이런 정출연 같은 연구원 있나요있나요",
          "speaker_id": "2",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100030001.1.10",
          "form": "아 잇어용...ㅋㅋㅋ 딱하나 ㅋㅋㅋㅋㅋ",
          "original_form": "아 잇어용...ㅋㅋㅋ 딱하나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100030001.1.11",
          "form": "돈 별로 안준대요",
          "original_form": "돈 별로 안준대요",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100030001.1.12",
          "form": "뭔가요",
          "original_form": "뭔가요",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ ㅋ구려 ㅡㅡ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ ㅋ구려 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.14",
          "form": "건설기술연구원이요",
          "original_form": "건설기술연구원이요",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.15",
          "form": "이름이",
          "original_form": "이름이",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.16",
          "form": "윗사람들만 돈 마니받아간다네요",
          "original_form": "윗사람들만 돈 마니받아간다네요",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.17",
          "form": "아들어 봤습니다",
          "original_form": "아들어 봤습니다",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.18",
          "form": "그래도 거기 가시죠",
          "original_form": "그래도 거기 가시죠",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 왜욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 왜욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.20",
          "form": "저는 전기 연구원인데",
          "original_form": "저는 전기 연구원인데",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.21",
          "form": "나쁘지 않습니다",
          "original_form": "나쁘지 않습니다",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.22",
          "form": "다닐만 하실 거예요 분명히",
          "original_form": "다닐만 하실 거예요 분명히",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100030001.1.23",
          "form": "아 정출연이에요?",
          "original_form": "아 정출연이에요?",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100030001.1.24",
          "form": "어 정출현으로 알고 있습니다",
          "original_form": "어 정출현으로 알고 있습니다",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100030001.1.25",
          "form": "박사 하시고 가시면 6000원 넘으실 텐데요",
          "original_form": "박사 하시고 가시면 6000원 넘으실 텐데요",
          "speaker_id": "2",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100030001.1.26",
          "form": "ㅋㅋㅋㅋㅋ 더받고시퍼요.... 후 ㅜㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ 더받고시퍼요.... 후 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100030001.1.27",
          "form": "그럼 사기업으로 가셔서 일 박스에 가셔야 됩니다 냅니다 그 그",
          "original_form": "그럼 사기업으로 가셔서 일 박스에 가셔야 됩니다 냅니다 그 그",
          "speaker_id": "2",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100030001.1.28",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100030001.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100030001.1.30",
          "form": "으엉 정녕 그런덴 없는것인가",
          "original_form": "으엉 정녕 그런덴 없는것인가",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100030001.1.31",
          "form": "어 회사 차리시면 됩니다 그그그그 크크",
          "original_form": "어 회사 차리시면 됩니다 그그그그 크크",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100030001.1.32",
          "form": "아이템 없나요 ㅋㅋㅋㅋㅋ",
          "original_form": "아이템 없나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100030001.1.33",
          "form": "어 그거 좋을 거 같은데요 생각해보니까",
          "original_form": "어 그거 좋을 거 같은데요 생각해보니까",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100030001.1.34",
          "form": "우선 국회의원을 한명섭외해서 구조 진단 구조 검토에 대한 법적 의무와 강제력을 강화하여서 진단 수수료로 먹고 사는 거죠",
          "original_form": "우선 국회의원을 한명섭외해서 구조 진단 구조 검토에 대한 법적 의무와 강제력을 강화하여서 진단 수수료로 먹고 사는 거죠",
          "speaker_id": "2",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100030001.1.35",
          "form": "돈 욕심 많으시면 안됩니다 일편하실려면 ㅋㅋㅋ",
          "original_form": "돈 욕심 많으시면 안됩니다 일편하실려면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100030001.1.36",
          "form": "그리고 좋은 남자 곧 많은 남자 만나면 되죠 ㅋㅋㅋ",
          "original_form": "그리고 좋은 남자 곧 많은 남자 만나면 되죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100030001.1.37",
          "form": "돈 많은 남자",
          "original_form": "돈 많은 남자",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100030001.1.38",
          "form": "ㅋㅋㅋ 국회의원 로비할돈이 없내.  ...",
          "original_form": "ㅋㅋㅋ 국회의원 로비할돈이 없내.  ...",
