      "utterance": [
        {
          "id": "MDRW2100036071.1.1",
          "form": "최근 사신거 있으세요??",
          "original_form": "최근 사신거 있으세요??",
          "speaker_id": "1",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100036071.1.2",
          "form": "강아지 사료와 영양제 주문했어요",
          "original_form": "강아지 사료와 영양제 주문했어요",
          "speaker_id": "2",
          "time": "20210830 15:59"
        },
        {
          "id": "MDRW2100036071.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210830 15:59"
        },
        {
          "id": "MDRW2100036071.1.4",
          "form": "아 강아지키우시나봐요 저는 오늘 더치커피요",
          "original_form": "아 강아지키우시나봐요 저는 오늘 더치커피요",
          "speaker_id": "1",
          "time": "20210830 16:01"
        },
        {
          "id": "MDRW2100036071.1.5",
          "form": "오오 어디서 주문하셨어요",
          "original_form": "오오 어디서 주문하셨어요",
          "speaker_id": "2",
          "time": "20210830 16:01"
        },
        {
          "id": "MDRW2100036071.1.6",
          "form": "소자매커피요 맛보기도 있어서 같이 담았거든요",
          "original_form": "소자매커피요 맛보기도 있어서 같이 담았거든요",
          "speaker_id": "1",
          "time": "20210830 16:02"
        },
        {
          "id": "MDRW2100036071.1.7",
          "form": "검색하니 샘플 파우치가 좋네요",
          "original_form": "검색하니 샘플 파우치가 좋네요",
          "speaker_id": "2",
          "time": "20210830 16:03"
        },
        {
          "id": "MDRW2100036071.1.8",
          "form": "홈카페 즐기시는군요ㅎ",
          "original_form": "홈카페 즐기시는군요ㅎ",
          "speaker_id": "2",
          "time": "20210830 16:03"
        },
        {
          "id": "MDRW2100036071.1.9",
          "form": "네네 일회분 딱 들어있어서 좋긴해보였어요 ㅎㅎ 애가 있어서 자주 못 나가니 집에서 잘 먹어요",
          "original_form": "네네 일회분 딱 들어있어서 좋긴해보였어요 ㅎㅎ 애가 있어서 자주 못 나가니 집에서 잘 먹어요",
          "speaker_id": "1",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036071.1.10",
          "form": "아메리카노로 드시나요 아님 어레인지해서?",
          "original_form": "아메리카노로 드시나요 아님 어레인지해서?",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036071.1.11",
          "form": "아메로요 ㅎ",
          "original_form": "아메로요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100036071.1.12",
          "form": "아 본연의 맛을 즐기시는군요",
          "original_form": "아 본연의 맛을 즐기시는군요",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036071.1.13",
          "form": "네ㅎㅎ 깔끔하게",
          "original_form": "네ㅎㅎ 깔끔하게",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036071.1.14",
          "form": "근데 나가서사먹으면 라떼나 프라푸치노도 먹어요",
          "original_form": "근데 나가서사먹으면 라떼나 프라푸치노도 먹어요",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036071.1.15",
          "form": "단것을 아주 즐기지는 않나봐요",
          "original_form": "단것을 아주 즐기지는 않나봐요",
          "speaker_id": "2",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100036071.1.16",
          "form": "저 단거 많이 좋아하는데 아메도 괜찮더라구요 ㅎㅎ",
          "original_form": "저 단거 많이 좋아하는데 아메도 괜찮더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 16:43"
        }
