      "utterance": [
        {
          "id": "MDRW2100034573.1.1",
          "form": "첫찌 자전거 몇인치고??",
          "original_form": "첫찌 자전거 몇인치고??",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100034573.1.2",
          "form": "20인치 똑같은거다ㅋㅋ",
          "original_form": "20인치 똑같은거다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034573.1.3",
          "form": "날이 좀 흐리길래 나가볼랬는데..해뜨길랠 맘접었다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "날이 좀 흐리길래 나가볼랬는데..해뜨길랠 맘접었다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034573.1.4",
          "form": "와..두찌 오늘 낮잠 안자노ㅠㅠ",
          "original_form": "와..두찌 오늘 낮잠 안자노ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034573.1.5",
          "form": "하루 느무 길다ㅠㅋㅋㅋ",
          "original_form": "하루 느무 길다ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034573.1.6",
          "form": "우리 두찌도 잠을안잔다",
          "original_form": "우리 두찌도 잠을안잔다",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034573.1.7",
          "form": "옆에서 아이스크림먹고있다",
          "original_form": "옆에서 아이스크림먹고있다",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034573.1.8",
          "form": "난감하군",
          "original_form": "난감하군",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034573.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이제 안자는거가?? 뭐이래 주말에 안자너 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이제 안자는거가?? 뭐이래 주말에 안자너 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034573.1.10",
          "form": "집에서만 그래안자네",
