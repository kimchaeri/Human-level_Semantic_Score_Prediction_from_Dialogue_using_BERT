      "utterance": [
        {
          "id": "MDRW2100034816.1.1",
          "form": "유튜브는 보시나요?",
          "original_form": "유튜브는 보시나요?",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100034816.1.2",
          "form": "네 유튜브 봐요!",
          "original_form": "네 유튜브 봐요!",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100034816.1.3",
          "form": "주로 어떤 유튜브 보세요?",
          "original_form": "주로 어떤 유튜브 보세요?",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034816.1.4",
          "form": "게임 방송 봐요!",
          "original_form": "게임 방송 봐요!",
          "speaker_id": "2",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100034816.1.5",
          "form": "게임 좋아하시는구나",
          "original_form": "게임 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100034816.1.6",
          "form": "어떤 게임 방송 즐겨보세요?",
          "original_form": "어떤 게임 방송 즐겨보세요?",
          "speaker_id": "1",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100034816.1.7",
          "form": "보통 뜨는 아무 게임 방송 보는것 같아요",
          "original_form": "보통 뜨는 아무 게임 방송 보는것 같아요",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034816.1.8",
          "form": "전 게임 무식자예요 ㅋㅋㅋㅋ",
          "original_form": "전 게임 무식자예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034816.1.9",
          "form": "앜ㅋㅋㅋ 그럴 수 있죠",
          "original_form": "앜ㅋㅋㅋ 그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034816.1.10",
          "form": "전 워낙 겜 하는걸 좋아해서요",
          "original_form": "전 워낙 겜 하는걸 좋아해서요",
          "speaker_id": "2",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034816.1.11",
          "form": "몇년전에 친구따라 오버워치 한번 해봣는데",
          "original_form": "몇년전에 친구따라 오버워치 한번 해봣는데",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034816.1.12",
          "form": "멀미만 나더라구요.. ㅋㅋㅋ ...",
          "original_form": "멀미만 나더라구요.. ㅋㅋㅋ ...",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034816.1.13",
          "form": "전 그래서 그런 겜은 잘 안해요",
          "original_form": "전 그래서 그런 겜은 잘 안해요",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034816.1.14",
          "form": "주로 어떤 게임하세요?",
          "original_form": "주로 어떤 게임하세요?",
          "speaker_id": "1",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034816.1.15",
          "form": "농사하는 게임 자주 해요",
          "original_form": "농사하는 게임 자주 해요",
          "speaker_id": "2",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100034816.1.16",
          "form": "그 레알팜인가? 하는 게임 광고는 본적 잇는것 같아요",
          "original_form": "그 레알팜인가? 하는 게임 광고는 본적 잇는것 같아요",
          "speaker_id": "1",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034816.1.17",
          "form": "그것도 예전에 해봤어요!",
          "original_form": "그것도 예전에 해봤어요!",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100034816.1.18",
          "form": "그러고보니 저는 요리하는 게임은 햇엇네요",
          "original_form": "그러고보니 저는 요리하는 게임은 햇엇네요",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100034816.1.19",
          "form": "요리중독? 그거햇엇어요",
          "original_form": "요리중독? 그거햇엇어요",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100034816.1.20",
          "form": "오오 처음 들어봐요",
          "original_form": "오오 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100034816.1.21",
          "form": "약간 노동이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "약간 노동이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034816.1.22",
          "form": "손님 주문에 맞춰서 햄버거에 상추넣고 케챂뿌리고 주고 ㅋㅋㅋㅋㅋ",
          "original_form": "손님 주문에 맞춰서 햄버거에 상추넣고 케챂뿌리고 주고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034816.1.23",
          "form": "오오 재밌을것 같아요",
          "original_form": "오오 재밌을것 같아요",
          "speaker_id": "2",
          "time": "20210828 21:32"
        }
      ]
    }
