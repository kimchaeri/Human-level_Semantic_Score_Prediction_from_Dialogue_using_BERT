      "utterance": [
        {
          "id": "MDRW2100001900.1.1",
          "form": "동물 키우세용?",
          "original_form": "동물 키우세용?",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001900.1.2",
          "form": "아니요 안키우는데 키우고싶어요!!!",
          "original_form": "아니요 안키우는데 키우고싶어요!!!",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001900.1.3",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001900.1.4",
          "form": "아뇨 ㅠㅠ저두요",
          "original_form": "아뇨 ㅠㅠ저두요",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001900.1.5",
          "form": "강아지 키워볼까 고민만하는중 ...",
          "original_form": "강아지 키워볼까 고민만하는중 ...",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001900.1.6",
          "form": "고민만 ..ㅋㅋ",
          "original_form": "고민만 ..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001900.1.7",
          "form": "강아지 귀엽죠 ㅠㅠㅠㅠㅠ",
          "original_form": "강아지 귀엽죠 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001900.1.8",
          "form": "전 요새 고양이한테 푹 빠져있습니다",
          "original_form": "전 요새 고양이한테 푹 빠져있습니다",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001900.1.9",
          "form": "고양이는 어떤종류요 ?",
          "original_form": "고양이는 어떤종류요 ?",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001900.1.10",
          "form": "고양이 그냥 다 좋아요 ㅎ",
          "original_form": "고양이 그냥 다 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001900.1.11",
          "form": "드냥 귀여운 생물체 같아요",
          "original_form": "드냥 귀여운 생물체 같아요",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001900.1.12",
          "form": "하지만 반려동물로 키우기에는 많은 고민이 ..",
          "original_form": "하지만 반려동물로 키우기에는 많은 고민이 ..",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001900.1.13",
          "form": "저는 아직 그 밤에 고양이눈이 넘무서워용 ㅠㅋㅋ",
          "original_form": "저는 아직 그 밤에 고양이눈이 넘무서워용 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001900.1.14",
          "form": "저도 예전에 그랬는데 자꾸 보면서 마음이 바꼈어요!!!",
          "original_form": "저도 예전에 그랬는데 자꾸 보면서 마음이 바꼈어요!!!",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001900.1.15",
          "form": "특이한 동물 좋아하는건 없어여?",
          "original_form": "특이한 동물 좋아하는건 없어여?",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001900.1.16",
          "form": "특이한 동물은 아직.. ㅎ 뭐 있으신가요?",
          "original_form": "특이한 동물은 아직.. ㅎ 뭐 있으신가요?",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
