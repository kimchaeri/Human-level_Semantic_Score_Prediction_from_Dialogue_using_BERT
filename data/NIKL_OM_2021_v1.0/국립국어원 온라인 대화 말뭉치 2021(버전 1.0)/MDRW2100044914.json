      "utterance": [
        {
          "id": "MDRW2100044914.1.1",
          "form": "저는 중국 저런식으로 가다간",
          "original_form": "저는 중국 저런식으로 가다간",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044914.1.2",
          "form": "결국 중국컨텐츠 다 망할거같던데",
          "original_form": "결국 중국컨텐츠 다 망할거같던데",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.3",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.4",
          "form": "결국 우리꺼 어중간히 베껴하는것도 핑핑이 하는거보면",
          "original_form": "결국 우리꺼 어중간히 베껴하는것도 핑핑이 하는거보면",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.5",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.6",
          "form": "사상 단위에서부터",
          "original_form": "사상 단위에서부터",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.7",
          "form": "강요를 하잖아",
          "original_form": "강요를 하잖아",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.8",
          "form": "막을거같고",
          "original_form": "막을거같고",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.9",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.10",
          "form": "창작활동을",
          "original_form": "창작활동을",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.11",
          "form": "한국 컨텐츠 막으려 하ㅣ않을까",
          "original_form": "한국 컨텐츠 막으려 하ㅣ않을까",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.12",
          "form": "어케 해",
          "original_form": "어케 해",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.13",
          "form": "겱국",
          "original_form": "겱국",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.14",
          "form": "말로는",
          "original_form": "말로는",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.15",
          "form": "막는다 하면서",
          "original_form": "막는다 하면서",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.16",
          "form": "그리고 제발",
          "original_form": "그리고 제발",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.17",
          "form": "다 베껴가니꺼",
          "original_form": "다 베껴가니꺼",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.18",
          "form": "까",
          "original_form": "까",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.19",
          "form": "아이돌 회수점",
          "original_form": "아이돌 회수점",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.20",
          "form": "열받아",
          "original_form": "열받아",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044914.1.21",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044914.1.22",
          "form": "아 그러고보니 제가 유툽에서 봤는데",
          "original_form": "아 그러고보니 제가 유툽에서 봤는데",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044914.1.23",
          "form": "중국 정치상황이 요새 심각한거같던데여",
          "original_form": "중국 정치상황이 요새 심각한거같던데여",
