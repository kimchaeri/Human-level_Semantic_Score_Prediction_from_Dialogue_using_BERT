      "utterance": [
        {
          "id": "MDRW2100026470.1.1",
          "form": "아메리카노 좋아하세요??",
          "original_form": "아메리카노 좋아하세요??",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.2",
          "form": "라떼는 노우-?",
          "original_form": "라떼는 노우-?",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.3",
          "form": "사실 전 라떼파에요",
          "original_form": "사실 전 라떼파에요",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.4",
          "form": "온리 라떼",
          "original_form": "온리 라떼",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.6",
          "form": "ㅋㅋㅋㅋ 아메 나온김에 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 아메 나온김에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.7",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.8",
          "form": "name1은요??",
          "original_form": "&name1&은요??",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026470.1.9",
          "form": "라떼파 ㅎㅎ",
          "original_form": "라떼파 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.10",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.11",
          "form": "존맛이에요 ㅎㅎ",
          "original_form": "존맛이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.13",
          "form": "카페에서 거의 무조건 라떼에요",
          "original_form": "카페에서 거의 무조건 라떼에요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.14",
          "form": "대부분 전 카페라떼가 일순위긴 한데",
          "original_form": "대부분 전 카페라떼가 일순위긴 한데",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.15",
          "form": "어디 라떼 좋아하세용??",
          "original_form": "어디 라떼 좋아하세용??",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.16",
          "form": "라때 종류는 기본으로 다 좋아요",
          "original_form": "라때 종류는 기본으로 다 좋아요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.17",
          "form": "넨넴 ㅋㅋㅋㅋ",
          "original_form": "넨넴 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026470.1.18",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026470.1.19",
          "form": "사실 전 먹는걸론 별로 안가려서...",
          "original_form": "사실 전 먹는걸론 별로 안가려서...",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026470.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럼 카페가시면 항상 라떼시키시나용?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럼 카페가시면 항상 라떼시키시나용?",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026470.1.21",
          "form": "크게 어디가 좋다고 생각해본 적이 없는것 같아요",
          "original_form": "크게 어디가 좋다고 생각해본 적이 없는것 같아요",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026470.1.22",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026470.1.23",
          "form": "아항 ㅋㅋㅋ",
          "original_form": "아항 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026470.1.24",
          "form": "라떼가 젤 맛있어요",
          "original_form": "라떼가 젤 맛있어요",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026470.1.25",
          "form": "좋아요 ㅋㅋ 저도 주로 라떼 시키긴한데",
          "original_form": "좋아요 ㅋㅋ 저도 주로 라떼 시키긴한데",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026470.1.26",
          "form": "가끔은 에이드 땡기더라구용",
          "original_form": "가끔은 에이드 땡기더라구용",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026470.1.27",
          "form": "오 에이드도 맛있죠",
          "original_form": "오 에이드도 맛있죠",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026470.1.28",
          "form": "여름에 더울 땐 상큼한거 땡길 때 있죠",
          "original_form": "여름에 더울 땐 상큼한거 땡길 때 있죠",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026470.1.29",
          "form": "특히 스벅 패션탱고티레몬에이드 피지오 젤 좋아해요 ㅎㅎ",
          "original_form": "특히 스벅 패션탱고티레몬에이드 피지오 젤 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026470.1.30",
          "form": "맞아요 여름은 시원한거!!",
          "original_form": "맞아요 여름은 시원한거!!",
          "speaker_id": "1",
          "time": "20210901 22:34"
        }
      ]
