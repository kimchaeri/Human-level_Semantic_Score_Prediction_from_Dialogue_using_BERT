      "utterance": [
        {
          "id": "MDRW2100012653.1.1",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012653.1.2",
          "form": "하이하이요",
          "original_form": "하이하이요",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012653.1.3",
          "form": "요새 즐겨보는 프로 있으세용?",
          "original_form": "요새 즐겨보는 프로 있으세용?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012653.1.4",
          "form": "바라는바다요 ㅎ",
          "original_form": "바라는바다요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012653.1.5",
          "form": "검색해보고 왔어요:)",
          "original_form": "검색해보고 왔어요:)",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012653.1.6",
          "form": "비긴어게인 같은 프로인건가요?",
          "original_form": "비긴어게인 같은 프로인건가요?",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012653.1.7",
          "form": "네네 ㅎㅎ 비슷해요 ㅎㅎ",
          "original_form": "네네 ㅎㅎ 비슷해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012653.1.8",
          "form": "오 저도 봐야겠어요:)",
          "original_form": "오 저도 봐야겠어요:)",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012653.1.9",
          "form": "비긴어게인 완전 좋아했었는데",
          "original_form": "비긴어게인 완전 좋아했었는데",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012653.1.10",
          "form": "기대되네요",
          "original_form": "기대되네요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012653.1.11",
          "form": "전 선우정아편은 보고 울었어요 ㅎㅎㅎ",
          "original_form": "전 선우정아편은 보고 울었어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012653.1.12",
          "form": "꼭보셔요~~",
          "original_form": "꼭보셔요~~",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012653.1.13",
          "form": "흐엉ㅜㅜ 그 정도였나요? ㅠㅠ",
          "original_form": "흐엉ㅜㅜ 그 정도였나요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012653.1.14",
          "form": "네네  ㅎㅎ",
          "original_form": "네네  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012653.1.15",
          "form": "기억에 남는 곡 있으세요?",
          "original_form": "기억에 남는 곡 있으세요?",
          "speaker_id": "1",
          "time": "20210825 00:21"
        },
        {
          "id": "MDRW2100012653.1.16",
          "form": "도망가자",
          "original_form": "도망가자",
          "speaker_id": "2",
          "time": "20210825 00:21"
        },
        {
          "id": "MDRW2100012653.1.17",
          "form": "너와 나의 거리  - 선우정아 랑 온유랑 부른거요",
          "original_form": "너와 나의 거리  - 선우정아 랑 온유랑 부른거요",
          "speaker_id": "2",
          "time": "20210825 00:21"
        },
        {
          "id": "MDRW2100012653.1.18",
          "form": "와....  저 지금 유툽으로",
          "original_form": "와....  저 지금 유툽으로",
          "speaker_id": "1",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100012653.1.19",
          "form": "영상찾아보고 왔어요ㅠㅠ",
          "original_form": "영상찾아보고 왔어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100012653.1.20",
          "form": "뭉클해여 보고나면 ㅜㅜ",
          "original_form": "뭉클해여 보고나면 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012653.1.21",
          "form": "ㅠㅠ그러게요 ㅠㅠ",
          "original_form": "ㅠㅠ그러게요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012653.1.22",
          "form": "근데 영상보니\n김고은님이랑 이지아님도 있는거 같던데",
          "original_form": "근데 영상보니\n김고은님이랑 이지아님도 있는거 같던데",
          "speaker_id": "1",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012653.1.23",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012653.1.24",
          "form": "그 분들도 노래 하는건가요?",
          "original_form": "그 분들도 노래 하는건가요?",
          "speaker_id": "1",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012653.1.25",
          "form": "아뇨 카페 운영??",
          "original_form": "아뇨 카페 운영??",
          "speaker_id": "2",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012653.1.26",
          "form": "저승사자 씨도 있어요 ㅎㅎ",
          "original_form": "저승사자 씨도 있어요 ㅎㅎ",
