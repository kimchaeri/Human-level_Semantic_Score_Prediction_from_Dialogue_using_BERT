      "utterance": [
        {
          "id": "MDRW2100037009.1.1",
          "form": "메탈 남 유튜버 보니",
          "original_form": "메탈 남 유튜버 보니",
          "speaker_id": "2",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100037009.1.2",
          "form": "나도 그거 봐",
          "original_form": "나도 그거 봐",
          "speaker_id": "1",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100037009.1.3",
          "form": "이번에 눈썹이 가 나왔어",
          "original_form": "이번에 눈썹이 가 나왔어",
          "speaker_id": "2",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100037009.1.4",
          "form": "하얀 고양이 말하는구나",
          "original_form": "하얀 고양이 말하는구나",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.5",
          "form": "맞아 경계심 심한 고양이",
          "original_form": "맞아 경계심 심한 고양이",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.6",
          "form": "많이 풀렸니",
          "original_form": "많이 풀렸니",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.7",
          "form": "적응을 잘했더라",
          "original_form": "적응을 잘했더라",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.8",
          "form": "주인이 워낙 잘해서 그런가봐",
          "original_form": "주인이 워낙 잘해서 그런가봐",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.9",
          "form": "열 마리 케어하는 게 너무 대단해",
          "original_form": "열 마리 케어하는 게 너무 대단해",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.10",
          "form": "직장도 다니면서 말이야",
          "original_form": "직장도 다니면서 말이야",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.11",
          "form": "직장이 심지어 멀다더라",
          "original_form": "직장이 심지어 멀다더라",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.12",
          "form": "이사 때문에 더 힘들어 보여",
          "original_form": "이사 때문에 더 힘들어 보여",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037009.1.13",
          "form": "그러게 말이야",
          "original_form": "그러게 말이야",
          "speaker_id": "2",
          "time": "20210831 17:39"
        }
