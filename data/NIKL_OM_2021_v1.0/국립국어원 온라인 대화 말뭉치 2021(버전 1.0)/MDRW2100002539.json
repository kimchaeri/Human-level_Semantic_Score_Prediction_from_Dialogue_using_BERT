      "utterance": [
        {
          "id": "MDRW2100002539.1.1",
          "form": "주로 쇼핑은 어떻게 하세요?",
          "original_form": "주로 쇼핑은 어떻게 하세요?",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002539.1.2",
          "form": "전 인터넷쇼핑요",
          "original_form": "전 인터넷쇼핑요",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002539.1.3",
          "form": "가장 최근에 구매하셨던건 뭐에요?",
          "original_form": "가장 최근에 구매하셨던건 뭐에요?",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002539.1.4",
          "form": "얼마전에 이사를 해서요",
          "original_form": "얼마전에 이사를 해서요",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002539.1.5",
          "form": "쇼파샀어요",
          "original_form": "쇼파샀어요",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002539.1.6",
          "form": "쇼파는 오프라인에서 구입했어요ㅋㅋ",
          "original_form": "쇼파는 오프라인에서 구입했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002539.1.7",
          "form": "그쵸ㅋㅋㅋ\r\n쇼파는 직접 느껴봐야쥬",
          "original_form": "그쵸ㅋㅋㅋ\r\n쇼파는 직접 느껴봐야쥬",
          "speaker_id": "1",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002539.1.8",
          "form": "ㅋㅋ 근데 충동구매한거라",
          "original_form": "ㅋㅋ 근데 충동구매한거라",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002539.1.9",
          "form": "잘산게맞나",
          "original_form": "잘산게맞나",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002539.1.10",
          "form": "얼마전에 이사하셨으면\r\n인테리어 소품들도\r\n많이 사게 되지 않아요?",
          "original_form": "얼마전에 이사하셨으면\r\n인테리어 소품들도\r\n많이 사게 되지 않아요?",
          "speaker_id": "1",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002539.1.11",
          "form": "걱정되긴해요",
          "original_form": "걱정되긴해요",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002539.1.12",
          "form": "앗 충동구매였나여",
          "original_form": "앗 충동구매였나여",
          "speaker_id": "1",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002539.1.13",
          "form": "처리할일도 많고",
          "original_form": "처리할일도 많고",
          "speaker_id": "2",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002539.1.14",
          "form": "돈도 넘 많이 써서",
          "original_form": "돈도 넘 많이 써서",
          "speaker_id": "2",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002539.1.15",
          "form": "그냥....있는대로 살고있어요",
          "original_form": "그냥....있는대로 살고있어요",
          "speaker_id": "2",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002539.1.16",
          "form": "새로사는건 자제하는걸로...ㅜㅜ",
          "original_form": "새로사는건 자제하는걸로...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002539.1.17",
          "form": "쇼핑도 하다보면 끝이 없어서",
          "original_form": "쇼핑도 하다보면 끝이 없어서",
          "speaker_id": "1",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002539.1.18",
          "form": "손꾸락 단속 잘하세용 ㅜㅜ",
          "original_form": "손꾸락 단속 잘하세용 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002539.1.19",
          "form": "ㅋㅋ 넵 조심하겠습니다 ㅠ",
          "original_form": "ㅋㅋ 넵 조심하겠습니다 ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002539.1.20",
          "form": "침대랑 커튼 아직",
          "original_form": "침대랑 커튼 아직",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002539.1.21",
          "form": "못샀는데",
          "original_form": "못샀는데",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002539.1.22",
          "form": "막막하네요",
          "original_form": "막막하네요",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002539.1.23",
          "form": "너무 다양해서..뭘사야할지..",
          "original_form": "너무 다양해서..뭘사야할지..",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002539.1.24",
          "form": "사실 저는 결정장애가 있어서",
          "original_form": "사실 저는 결정장애가 있어서",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002539.1.25",
          "form": "쇼핑할때도 구매까지가는데\r\n많은 시간이 걸리는거 같아요ㅋㅋ",
          "original_form": "쇼핑할때도 구매까지가는데\r\n많은 시간이 걸리는거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002539.1.26",
          "form": "헉 저도요ㅋㅋ",
          "original_form": "헉 저도요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002539.1.27",
          "form": "그래서 마음에 드는건\r\n아예 깔별로 산적도 있어요",
          "original_form": "그래서 마음에 드는건\r\n아예 깔별로 산적도 있어요",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002539.1.28",
          "form": "정말오래걸려요",
          "original_form": "정말오래걸려요",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002539.1.29",
          "form": "고민하는게 너무 힘들어서ㅠㅋㅋ",
          "original_form": "고민하는게 너무 힘들어서ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002539.1.30",
          "form": "도전하는걸두려워하고...",
          "original_form": "도전하는걸두려워하고...",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002539.1.31",
          "form": "근데 소파는 충동구매...",
          "original_form": "근데 소파는 충동구매...",
          "speaker_id": "2",
          "time": "20210820 13:32"
        }
      ]
