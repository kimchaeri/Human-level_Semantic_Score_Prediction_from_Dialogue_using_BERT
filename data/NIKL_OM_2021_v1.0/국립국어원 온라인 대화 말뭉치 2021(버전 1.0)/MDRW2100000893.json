      "utterance": [
        {
          "id": "MDRW2100000893.1.1",
          "form": "요즘 날씨가 많이 풀렸죠?",
          "original_form": "요즘 날씨가 많이 풀렸죠?",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000893.1.2",
          "form": "산책하기 딱 좋은 날씨가 됐어요~",
          "original_form": "산책하기 딱 좋은 날씨가 됐어요~",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000893.1.3",
          "form": "여름이 가니까 섭섭해요",
          "original_form": "여름이 가니까 섭섭해요",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000893.1.4",
          "form": "올해 여름은 여름으로 치지 않았습니다",
          "original_form": "올해 여름은 여름으로 치지 않았습니다",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000893.1.5",
          "form": "휴가때 여행도 제대로 못가서 아쉬웠죠...",
          "original_form": "휴가때 여행도 제대로 못가서 아쉬웠죠...",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000893.1.6",
          "form": "확실히 복날 지나니까 날이 풀리는게",
          "original_form": "확실히 복날 지나니까 날이 풀리는게",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000893.1.7",
          "form": "절기가 참 신기해요",
          "original_form": "절기가 참 신기해요",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000893.1.8",
          "form": "입추가 시작되고 바로 서늘해진거 보니까",
          "original_form": "입추가 시작되고 바로 서늘해진거 보니까",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000893.1.9",
          "form": "조상님들 대단하신것같아요",
          "original_form": "조상님들 대단하신것같아요",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000893.1.10",
          "form": "그러니까요! 그 옛날에 이런걸 어떻게 다 정리하셨는지...",
          "original_form": "그러니까요! 그 옛날에 이런걸 어떻게 다 정리하셨는지...",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000893.1.11",
          "form": "옛날하니까.. 예전에 미세먼지 없을때가 그립네요",
          "original_form": "옛날하니까.. 예전에 미세먼지 없을때가 그립네요",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000893.1.12",
          "form": "우와 그러게요 오히려 황사가 더 좋았네요",
          "original_form": "우와 그러게요 오히려 황사가 더 좋았네요",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000893.1.13",
          "form": "황사때 기억 나세요?",
          "original_form": "황사때 기억 나세요?",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000893.1.14",
          "form": "어우....진짜 황사가 낫죠",
          "original_form": "어우....진짜 황사가 낫죠",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000893.1.15",
          "form": "그땐 황사가 최악인줄알았는데..",
          "original_form": "그땐 황사가 최악인줄알았는데..",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000893.1.16",
          "form": "나쁜말이 뭔지 모르겠네요ㅠㅠ",
          "original_form": "나쁜말이 뭔지 모르겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000893.1.17",
          "form": "맞아요 오히려 황사가 나아요",
          "original_form": "맞아요 오히려 황사가 나아요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000893.1.18",
          "form": "비내리면 미세먼지 잔여물들 너무 끔찍해요",
          "original_form": "비내리면 미세먼지 잔여물들 너무 끔찍해요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000893.1.19",
          "form": "맞아요.. 환경오염때문에 날씨가 더 안좋아질거라는데",
          "original_form": "맞아요.. 환경오염때문에 날씨가 더 안좋아질거라는데",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000893.1.20",
          "form": "기관지 안좋은편이라.. 질병이 걱정되네요",
          "original_form": "기관지 안좋은편이라.. 질병이 걱정되네요",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000893.1.21",
          "form": "전 그래서 공기청정기를 사서 대비하고있어요",
          "original_form": "전 그래서 공기청정기를 사서 대비하고있어요",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000893.1.22",
          "form": "엇 공기청정기 어떤거 쓰세요?",
          "original_form": "엇 공기청정기 어떤거 쓰세요?",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000893.1.23",
          "form": "샤오미 쓰고있어요 10만원 짜리?",
          "original_form": "샤오미 쓰고있어요 10만원 짜리?",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000893.1.24",
          "form": "올해부터는 집안에도 먼지가 늘어나는게 느껴져서.. 저도 구매하려구요",
