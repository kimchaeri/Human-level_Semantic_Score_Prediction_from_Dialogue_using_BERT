      "utterance": [
        {
          "id": "MDRW2100027927.1.1",
          "form": "오 제가 애기 데려오고 오래 안해서 그런줄 알았는데",
          "original_form": "오 제가 애기 데려오고 오래 안해서 그런줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027927.1.2",
          "form": "애가 안자라나요",
          "original_form": "애가 안자라나요",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027927.1.3",
          "form": "그렇다고 본거같아요?",
          "original_form": "그렇다고 본거같아요?",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027927.1.4",
          "form": "저는 스위치",
          "original_form": "저는 스위치",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.5",
          "form": "하긴 와인이랑 치즈가 몇번이나 익었는데",
          "original_form": "하긴 와인이랑 치즈가 몇번이나 익었는데",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.6",
          "form": "짱구게임 하고 싶어요",
          "original_form": "짱구게임 하고 싶어요",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.7",
          "form": "애는 그대로네요",
          "original_form": "애는 그대로네요",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.9",
          "form": "너무해 귀찮았나봐요",
          "original_form": "너무해 귀찮았나봐요",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.10",
          "form": "짱구게임이 요즘도 있어요?",
          "original_form": "짱구게임이 요즘도 있어요?",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.11",
          "form": "1인 개발이라고 들어서ㅋㅋ 납득은 갑니다",
          "original_form": "1인 개발이라고 들어서ㅋㅋ 납득은 갑니다",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027927.1.12",
          "form": "최근에 나왔어요 무슨 여름방학어쩌고",
          "original_form": "최근에 나왔어요 무슨 여름방학어쩌고",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027927.1.13",
          "form": "그분 부자됐을듯 ㄷㄷ",
          "original_form": "그분 부자됐을듯 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027927.1.14",
          "form": "진짜 수익나눌필요가 없으니 보통부자가 아니겠네요",
          "original_form": "진짜 수익나눌필요가 없으니 보통부자가 아니겠네요",
          "speaker_id": "2",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027927.1.15",
          "form": "근데 일본판만 있어요",
          "original_form": "근데 일본판만 있어요",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027927.1.16",
          "form": "아앗 짱구게임은 평가가 아주 좋지는 않네요",
          "original_form": "아앗 짱구게임은 평가가 아주 좋지는 않네요",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027927.1.17",
          "form": "플레이시간이 5~7시간이래요",
          "original_form": "플레이시간이 5~7시간이래요",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027927.1.18",
          "form": "헐 진짠가요",
          "original_form": "헐 진짠가요",
          "speaker_id": "1",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027927.1.19",
          "form": "비싸던데?!",
          "original_form": "비싸던데?!",
          "speaker_id": "1",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027927.1.20",
          "form": "컨텐츠가 많이 적은가봐요ㅜㅜ",
          "original_form": "컨텐츠가 많이 적은가봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027927.1.21",
          "form": "그럼 유투브나 봐야겠네요 ㅜㅜ",
          "original_form": "그럼 유투브나 봐야겠네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027927.1.22",
          "form": "와리오 게임도 기다리고 있어요",
          "original_form": "와리오 게임도 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210902 16:17"
        }
      ]
