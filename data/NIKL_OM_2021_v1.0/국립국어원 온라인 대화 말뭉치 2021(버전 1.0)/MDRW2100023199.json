      "utterance": [
        {
          "id": "MDRW2100023199.1.1",
          "form": "풀보이스도 아니더만",
          "original_form": "풀보이스도 아니더만",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.2",
          "form": "ㅇㅇ 최소한 게임 스토리 완결이라도 냈어야 함",
          "original_form": "ㅇㅇ 최소한 게임 스토리 완결이라도 냈어야 함",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.3",
          "form": "왜 그런식으로 한건지 모르겠어",
          "original_form": "왜 그런식으로 한건지 모르겠어",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.4",
          "form": "사람들이 엄청 오래 기다렸는대",
          "original_form": "사람들이 엄청 오래 기다렸는대",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.5",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.6",
          "form": "오래 기다리고 펀딩까지 1억 받은것 치고는",
          "original_form": "오래 기다리고 펀딩까지 1억 받은것 치고는",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.7",
          "form": "ㅇㅇcbt 왜 안한건지",
          "original_form": "ㅇㅇcbt 왜 안한건지",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.8",
          "form": "너무 뭐가 없어",
          "original_form": "너무 뭐가 없어",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.9",
          "form": "알겠음",
          "original_form": "알겠음",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.10",
          "form": "스토리말고는",
          "original_form": "스토리말고는",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.11",
          "form": "할게없는데",
          "original_form": "할게없는데",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023199.1.12",
          "form": "그걸 cbt로 까버리면",
          "original_form": "그걸 cbt로 까버리면",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.13",
          "form": "ㄹㅇ 밑천팔리는거라",
          "original_form": "ㄹㅇ 밑천팔리는거라",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.14",
          "form": "걍 안했구만",
          "original_form": "걍 안했구만",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.15",
          "form": "그럴듯 ㅋㅋㅋㅋㅋ",
          "original_form": "그럴듯 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.16",
          "form": "솔직히 이 퀄은",
          "original_form": "솔직히 이 퀄은",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.17",
          "form": "너무 준비된 양이 적어서 ㅅ",
          "original_form": "너무 준비된 양이 적어서 ㅅ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.18",
          "form": "누가 중간에 삥땅쳤대도",
          "original_form": "누가 중간에 삥땅쳤대도",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.19",
          "form": "안이상한듯",
          "original_form": "안이상한듯",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.20",
          "form": "아 나도 방금 그거 쓰고 있었는데 ㅋㅋㅋㅋ",
          "original_form": "아 나도 방금 그거 쓰고 있었는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.21",
          "form": "앋",
          "original_form": "앋",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.23",
          "form": "성우중에 젤 네임드가",
          "original_form": "성우중에 젤 네임드가",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.24",
          "form": "김영선인데",
          "original_form": "김영선인데",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023199.1.25",
          "form": "사람생각하는거 다 똑같구만",
          "original_form": "사람생각하는거 다 똑같구만",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.26",
          "form": "선하는 프롤로그 주고 바로 퇴장이잖아",
          "original_form": "선하는 프롤로그 주고 바로 퇴장이잖아",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.27",
          "form": "ㅇㅇ성우 비용으로 다 썼다기엔",
          "original_form": "ㅇㅇ성우 비용으로 다 썼다기엔",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.28",
          "form": "그거 말고는 다 낯선 성우던데",
          "original_form": "그거 말고는 다 낯선 성우던데",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.29",
          "form": "다 초짜에",
          "original_form": "다 초짜에",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.30",
          "form": "결과물에서도",
          "original_form": "결과물에서도",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.31",
          "form": "티나",
          "original_form": "티나",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.32",
          "form": "초짜인거",
          "original_form": "초짜인거",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023199.1.33",
          "form": "거기다 풀보이스도 아닌데",
          "original_form": "거기다 풀보이스도 아닌데",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.34",
          "form": "그렇게 돈들었을것같진 않거든",
          "original_form": "그렇게 돈들었을것같진 않거든",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.35",
          "form": "거기다 작화가도 중간에 바뀌고",
          "original_form": "거기다 작화가도 중간에 바뀌고",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.36",
          "form": "이거 ㄹㅇ",
          "original_form": "이거 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.37",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.38",
          "form": "거대한 횡령의 냄새가 나.....",
          "original_form": "거대한 횡령의 냄새가 나.....",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.39",
          "form": "보현이라는 npc 봄?",
          "original_form": "보현이라는 npc 봄?",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.40",
          "form": "나 이름 기억 안나",
          "original_form": "나 이름 기억 안나",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.41",
          "form": "대학원생?",
          "original_form": "대학원생?",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023199.1.42",
          "form": "ㄴㄴ 아직 안봒을 수도 있는데",
          "original_form": "ㄴㄴ 아직 안봒을 수도 있는데",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023199.1.43",
          "form": "게임 스토리에서",
          "original_form": "게임 스토리에서",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023199.1.44",
          "form": "하얀 티에 왼쪽 상단에 고양이 그려진거 입고 나와",
          "original_form": "하얀 티에 왼쪽 상단에 고양이 그려진거 입고 나와",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023199.1.45",
          "form": "안본듯",
          "original_form": "안본듯",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023199.1.46",
          "form": "좀 후반부 등장캐릭터이긴 함",
          "original_form": "좀 후반부 등장캐릭터이긴 함",
