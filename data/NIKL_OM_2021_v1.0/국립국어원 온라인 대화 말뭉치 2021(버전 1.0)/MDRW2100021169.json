      "utterance": [
        {
          "id": "MDRW2100021169.1.1",
          "form": "마카롱 좋아하는데 두어개가 한계예요",
          "original_form": "마카롱 좋아하는데 두어개가 한계예요",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021169.1.2",
          "form": "많이 못먹어서",
          "original_form": "많이 못먹어서",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021169.1.3",
          "form": "무슨 마카롱을 좋아하시나요",
          "original_form": "무슨 마카롱을 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021169.1.4",
          "form": "솔티카라멜 맛이 가장 입맛에 맞아요",
          "original_form": "솔티카라멜 맛이 가장 입맛에 맞아요",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021169.1.5",
          "form": "아 단짠의 조화가 환상이지요",
          "original_form": "아 단짠의 조화가 환상이지요",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021169.1.6",
          "form": "저도 그거 좋아해요",
          "original_form": "저도 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021169.1.7",
          "form": "맞아요 단짠이 중독되는 맛이더라고요",
          "original_form": "맞아요 단짠이 중독되는 맛이더라고요",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021169.1.8",
          "form": "솔직히 그것만 좋아해요",
          "original_form": "솔직히 그것만 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021169.1.9",
          "form": "뚱카롱이 좋으신가요?",
          "original_form": "뚱카롱이 좋으신가요?",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021169.1.10",
          "form": "아님 기본?",
          "original_form": "아님 기본?",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021169.1.11",
          "form": "둘다 좋아요 조화만 잘되면",
          "original_form": "둘다 좋아요 조화만 잘되면",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021169.1.12",
          "form": "전 사실 뚱카롱 파입니다ㅎㅎ",
          "original_form": "전 사실 뚱카롱 파입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021169.1.13",
          "form": "필링 듬뿍이 좋아요",
          "original_form": "필링 듬뿍이 좋아요",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021169.1.14",
          "form": "마카롱맛집 추천할곳 있나요",
          "original_form": "마카롱맛집 추천할곳 있나요",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021169.1.15",
          "form": "집근처 개인카페라 추천이 어렵네요",
          "original_form": "집근처 개인카페라 추천이 어렵네요",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021169.1.16",
          "form": "체인이 아니라서ㅜ",
          "original_form": "체인이 아니라서ㅜ",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021169.1.17",
          "form": "아 전 홍대서 먹은곳이 괜찮았어요",
          "original_form": "아 전 홍대서 먹은곳이 괜찮았어요",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021169.1.18",
          "form": "이름이 당장 기억이 안 나네요",
          "original_form": "이름이 당장 기억이 안 나네요",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021169.1.19",
          "form": "홍대 가면 한번 찾아봐야겠어요",
          "original_form": "홍대 가면 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100021169.1.20",
          "form": "역근처지요?",
          "original_form": "역근처지요?",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100021169.1.21",
          "form": "네 역에서 한 5분 거리예요",
          "original_form": "네 역에서 한 5분 거리예요",
