      "utterance": [
        {
          "id": "MDRW2100020942.1.1",
          "form": "휴 집안일은 넘 많아여",
          "original_form": "휴 집안일은 넘 많아여",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020942.1.2",
          "form": "ㅋㅋ애기는 할아버지",
          "original_form": "ㅋㅋ애기는 할아버지",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020942.1.3",
          "form": "ㅋㅋㅋㅋ 역시 엄빠찬스",
          "original_form": "ㅋㅋㅋㅋ 역시 엄빠찬스",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020942.1.4",
          "form": "친정은 사랑이죠",
          "original_form": "친정은 사랑이죠",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020942.1.5",
          "form": "나도 집안일 널부르고 나옴",
          "original_form": "나도 집안일 널부르고 나옴",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020942.1.6",
          "form": "집에 잇으면 일개미",
          "original_form": "집에 잇으면 일개미",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020942.1.7",
          "form": "가만 냅둘수가 없음",
          "original_form": "가만 냅둘수가 없음",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020942.1.8",
          "form": "엄마집 나오면 베짱이",
          "original_form": "엄마집 나오면 베짱이",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020942.1.9",
          "form": "오늘도 일주일 밀린 빨래 처리하고옴",
          "original_form": "오늘도 일주일 밀린 빨래 처리하고옴",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020942.1.10",
          "form": "다른건 손도 안댐ㅎㅎ",
          "original_form": "다른건 손도 안댐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020942.1.11",
          "form": "못나가지",
          "original_form": "못나가지",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020942.1.12",
          "form": "하지만 집안일냅두고 당당히 나왔지",
          "original_form": "하지만 집안일냅두고 당당히 나왔지",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020942.1.13",
          "form": "ㅋㅋㅋ못본척해야함",
          "original_form": "ㅋㅋㅋ못본척해야함",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020942.1.14",
          "form": "ㅎㅎ 눈감고 나왔지",
          "original_form": "ㅎㅎ 눈감고 나왔지",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020942.1.15",
          "form": "ㅋㅋㅋ그럴때는 과감해야해",
          "original_form": "ㅋㅋㅋ그럴때는 과감해야해",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020942.1.16",
          "form": "하지만 돌아가면 다시 내차지",
          "original_form": "하지만 돌아가면 다시 내차지",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020942.1.17",
          "form": "저도 청소기만돌리고 과감히나옴",
          "original_form": "저도 청소기만돌리고 과감히나옴",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020942.1.18",
          "form": "그것이 현명한 선택이지 ㅋㅋㅋ",
          "original_form": "그것이 현명한 선택이지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020942.1.19",
          "form": "ㅋㅋㅋ돌아가면 내차지는 맞지",
          "original_form": "ㅋㅋㅋ돌아가면 내차지는 맞지",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020942.1.20",
          "form": "까꿍",
          "original_form": "까꿍",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020942.1.21",
          "form": "닦아야하는데 귀찮",
          "original_form": "닦아야하는데 귀찮",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020942.1.22",
          "form": "ㅋㅋ하지만 결국하고잇겟지",
          "original_form": "ㅋㅋ하지만 결국하고잇겟지",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020942.1.23",
          "form": "집에 얼마 없는데 먼지 넘 심해",
          "original_form": "집에 얼마 없는데 먼지 넘 심해",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020942.1.24",
          "form": "ㅋㅋㅋ그니까 왜그러죠?",
          "original_form": "ㅋㅋㅋ그니까 왜그러죠?",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020942.1.25",
          "form": "누가 몰래 왔다가나 ㅋㅋ",
          "original_form": "누가 몰래 왔다가나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020942.1.26",
          "form": "빨래 건조기에서 꺼내야되는데 세상 귀찮네",
          "original_form": "빨래 건조기에서 꺼내야되는데 세상 귀찮네",
          "speaker_id": "1",
          "time": "20210829 19:22"
        }
      ]
    }
  ]
