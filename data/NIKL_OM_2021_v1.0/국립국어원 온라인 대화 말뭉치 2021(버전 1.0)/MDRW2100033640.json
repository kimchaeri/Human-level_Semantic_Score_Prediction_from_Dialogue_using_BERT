      "utterance": [
        {
          "id": "MDRW2100033640.1.1",
          "form": "저희 비 아예 그쳤어요",
          "original_form": "저희 비 아예 그쳤어요",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100033640.1.2",
          "form": "거기돈가요?",
          "original_form": "거기돈가요?",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100033640.1.3",
          "form": "네~ 아침엔 좀 왔는데",
          "original_form": "네~ 아침엔 좀 왔는데",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100033640.1.4",
          "form": "점심부터 그쳤아요",
          "original_form": "점심부터 그쳤아요",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100033640.1.5",
          "form": "날씨가 선선하니 좋아요",
          "original_form": "날씨가 선선하니 좋아요",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100033640.1.6",
          "form": "그쵸 집 바로 안가고 좀 걷는 중인데",
          "original_form": "그쵸 집 바로 안가고 좀 걷는 중인데",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100033640.1.7",
          "form": "바람 살랑살랑 불어서 딱 좋아요",
          "original_form": "바람 살랑살랑 불어서 딱 좋아요",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100033640.1.8",
          "form": "오 산책중이시군요",
          "original_form": "오 산책중이시군요",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100033640.1.9",
          "form": "좀만 더 지나면",
          "original_form": "좀만 더 지나면",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100033640.1.10",
          "form": "밤에 난리나겠어요",
          "original_form": "밤에 난리나겠어요",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100033640.1.11",
          "form": "그쵸 곧 선풍기 치울 날씨가..",
          "original_form": "그쵸 곧 선풍기 치울 날씨가..",
          "speaker_id": "1",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100033640.1.12",
          "form": "여름 다 지난줄 알았는데 모기가 있네요ㅠㅠ",
          "original_form": "여름 다 지난줄 알았는데 모기가 있네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100033640.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100033640.1.14",
          "form": "저 오늘 물렸어요",
          "original_form": "저 오늘 물렸어요",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100033640.1.15",
          "form": "저도 엄청 물렸어요ㅠㅠㅠ",
          "original_form": "저도 엄청 물렸어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100033640.1.16",
          "form": "아직 여름인가봐요ㅠ",
          "original_form": "아직 여름인가봐요ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100033640.1.17",
          "form": "ㅠㅠ 그러니까요",
          "original_form": "ㅠㅠ 그러니까요",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100033640.1.18",
          "form": "모기등이 이제야 나와요",
          "original_form": "모기등이 이제야 나와요",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100033640.1.19",
          "form": "올 여름엔 모기 많이 안물린 편인데",
          "original_form": "올 여름엔 모기 많이 안물린 편인데",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100033640.1.20",
          "form": "늦여음에 이렇게..ㅎㅎ",
          "original_form": "늦여음에 이렇게..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100033640.1.21",
          "form": "더위 풀리니까 모기가 돌아다니는거래요!!",
          "original_form": "더위 풀리니까 모기가 돌아다니는거래요!!",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100033640.1.22",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033640.1.23",
          "form": "너무 더워서 못돌아다녔나보다",
          "original_form": "너무 더워서 못돌아다녔나보다",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033640.1.24",
          "form": "네.. 근데 이제 좀 풀리니까 난리내요",
          "original_form": "네.. 근데 이제 좀 풀리니까 난리내요",
