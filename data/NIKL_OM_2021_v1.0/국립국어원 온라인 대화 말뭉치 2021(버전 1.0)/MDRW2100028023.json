        },
        {
          "id": "MDRW2100028023.1.2",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028023.1.3",
          "form": "각자 알아서 그때그때 해요",
          "original_form": "각자 알아서 그때그때 해요",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028023.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028023.1.5",
          "form": "그런건 그냥하면되는데 요리는 어려워요",
          "original_form": "그런건 그냥하면되는데 요리는 어려워요",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028023.1.6",
          "form": "요리힘들죠",
          "original_form": "요리힘들죠",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028023.1.7",
          "form": "간을 못봐요",
          "original_form": "간을 못봐요",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028023.1.8",
          "form": "그른가여?",
          "original_form": "그른가여?",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028023.1.9",
          "form": "제가한건 다 맛없는 느낌",
          "original_form": "제가한건 다 맛없는 느낌",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028023.1.10",
          "form": "하면 늘어요",
          "original_form": "하면 늘어요",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028023.1.11",
          "form": "글쎄요ㅋ 그래서 음식은 간편식으로 나온거 해먹어요ㅋ",
          "original_form": "글쎄요ㅋ 그래서 음식은 간편식으로 나온거 해먹어요ㅋ",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028023.1.12",
          "form": "3분 요리 좋죠",
          "original_form": "3분 요리 좋죠",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
