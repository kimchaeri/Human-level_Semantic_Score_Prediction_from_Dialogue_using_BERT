      "utterance": [
        {
          "id": "MDRW2100019378.1.1",
          "form": "아뇨 전 우유 좋아해요",
          "original_form": "아뇨 전 우유 좋아해요",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019378.1.2",
          "form": "두유 드세요?",
          "original_form": "두유 드세요?",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019378.1.3",
          "form": "네 전 두유를 좋ㅇ하는 편이에요",
          "original_form": "네 전 두유를 좋ㅇ하는 편이에요",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019378.1.4",
          "form": "근데 name2님은 안 좋아하신다니 우유얘기해요 ㅋㅋ",
          "original_form": "근데 &name2&님은 안 좋아하신다니 우유얘기해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019378.1.5",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019378.1.6",
          "form": "콩 냄새? 같은게 낯설더라구요",
          "original_form": "콩 냄새? 같은게 낯설더라구요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019378.1.7",
          "form": "콩 비린ㄴ내가 두유에 다 있어요",
          "original_form": "콩 비린ㄴ내가 두유에 다 있어요",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019378.1.8",
          "form": "그쵸 ㅋㅋㅋ 가족들은 좋아하는데 저만 못마셔요 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ 가족들은 좋아하는데 저만 못마셔요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019378.1.9",
          "form": "이 콩비린내를 별로 안 싫어하면 두유를 먹는데 싫어함 못먹죠",
          "original_form": "이 콩비린내를 별로 안 싫어하면 두유를 먹는데 싫어함 못먹죠",
          "speaker_id": "1",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019378.1.10",
          "form": "전 어릴땐 못먹었어요",
          "original_form": "전 어릴땐 못먹었어요",
          "speaker_id": "1",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019378.1.11",
          "form": "그럼 몇살때부터 먹기 시작햇어요?",
          "original_form": "그럼 몇살때부터 먹기 시작햇어요?",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019378.1.12",
          "form": "한 고등학생 때부터요?",
          "original_form": "한 고등학생 때부터요?",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019378.1.13",
          "form": "집에 두유를 박스로 사놔서 배고파서간식으로 먹었어요 ㅋㅋ",
          "original_form": "집에 두유를 박스로 사놔서 배고파서간식으로 먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019378.1.14",
          "form": "아하 ㅋㅋㅋㅋ 역시 배고프면 뭐든 맛있는거같아요 ㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ 역시 배고프면 뭐든 맛있는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019378.1.15",
          "form": "어느 브랜드 좋아하세요?",
          "original_form": "어느 브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019378.1.16",
          "form": "먹다보니 음 먹을만하네 싶어서 먹어요 ㅋㅋ",
          "original_form": "먹다보니 음 먹을만하네 싶어서 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019378.1.17",
          "form": "전 뭐든 덜단 두유가 조ㅎ아요",
          "original_form": "전 뭐든 덜단 두유가 조ㅎ아요",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019378.1.18",
          "form": "두유도 무가당 있나요?",
          "original_form": "두유도 무가당 있나요?",
          "speaker_id": "2",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019378.1.19",
          "form": "무가당도 있고 로우슈가도 있는데 무가당은 솔직히 콩비린내가 더 나요",
          "original_form": "무가당도 있고 로우슈가도 있는데 무가당은 솔직히 콩비린내가 더 나요",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100019378.1.20",
          "form": "아몬드브리즈 같은 느낌인가요?",
          "original_form": "아몬드브리즈 같은 느낌인가요?",
          "speaker_id": "2",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100019378.1.21",
          "form": "아몬드보다 고소한 맛은 덜해요",
          "original_form": "아몬드보다 고소한 맛은 덜해요",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019378.1.22",
          "form": "그래도 아몬드도 좀 비리죵ㅋㅋ",
          "original_form": "그래도 아몬드도 좀 비리죵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:42"
        }
      ]
