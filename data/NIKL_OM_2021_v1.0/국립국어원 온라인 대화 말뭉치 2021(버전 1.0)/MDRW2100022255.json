      "utterance": [
        {
          "id": "MDRW2100022255.1.1",
          "form": "그니까요 ㅜㅜ 페트병은 탄산보관에 취약한가 봐요",
          "original_form": "그니까요 ㅜㅜ 페트병은 탄산보관에 취약한가 봐요",
          "speaker_id": "2",
          "time": "20210829 11:40"
        },
        {
          "id": "MDRW2100022255.1.2",
          "form": "그래서 2리터 탄산음료 이런거 못사먹겠어요",
          "original_form": "그래서 2리터 탄산음료 이런거 못사먹겠어요",
          "speaker_id": "2",
          "time": "20210829 11:40"
        },
        {
          "id": "MDRW2100022255.1.3",
          "form": "아 1.5리터인가?",
          "original_form": "아 1.5리터인가?",
          "speaker_id": "2",
          "time": "20210829 11:40"
        },
        {
          "id": "MDRW2100022255.1.4",
          "form": "1.5리터도있고 2리터도 있을거에요",
          "original_form": "1.5리터도있고 2리터도 있을거에요",
          "speaker_id": "1",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.5",
          "form": "어 그래요?ㅋㅋㅋ",
          "original_form": "어 그래요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.6",
          "form": "그거 하루 안에 다 못먹으면 사면 안돼요",
          "original_form": "그거 하루 안에 다 못먹으면 사면 안돼요",
          "speaker_id": "2",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.8",
          "form": "나중엔 단물만 남더라규오",
          "original_form": "나중엔 단물만 남더라규오",
          "speaker_id": "1",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.9",
          "form": "ㅋㅋㅋ그 단물로 찜닭 해먹으면 짱 맛있어요",
          "original_form": "ㅋㅋㅋ그 단물로 찜닭 해먹으면 짱 맛있어요",
          "speaker_id": "2",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.10",
          "form": "해먹어봤어요?",
          "original_form": "해먹어봤어요?",
          "speaker_id": "2",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.11",
          "form": "아 들어는 봤는데",
          "original_form": "아 들어는 봤는데",
          "speaker_id": "1",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022255.1.12",
          "form": "맛있구나",
          "original_form": "맛있구나",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022255.1.13",
          "form": "어떤 느낌인가요",
          "original_form": "어떤 느낌인가요",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022255.1.14",
          "form": "약간 설탕에절인 닭 맛인가",
          "original_form": "약간 설탕에절인 닭 맛인가",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022255.1.15",
          "form": "ㅋㅋㅋ그 안동찜닭보다 더 맛있어요",
          "original_form": "ㅋㅋㅋ그 안동찜닭보다 더 맛있어요",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022255.1.16",
          "form": "설탕맛 전혀 안나고 달짝짭짤한 맛이에요",
          "original_form": "설탕맛 전혀 안나고 달짝짭짤한 맛이에요",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022255.1.17",
          "form": "와 단짠단짠",
          "original_form": "와 단짠단짠",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022255.1.18",
          "form": "해먹어봐야겠다",
          "original_form": "해먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022255.1.19",
          "form": "꼭 해먹어봐요",
          "original_form": "꼭 해먹어봐요",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022255.1.20",
          "form": "진짜 찜닭 성공한 거 처음이였어요 그 날",
          "original_form": "진짜 찜닭 성공한 거 처음이였어요 그 날",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022255.1.21",
          "form": "꿀팁 감삼다",
          "original_form": "꿀팁 감삼다",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022255.1.22",
          "form": "거기에 당면까지 꼭 넣어요",
          "original_form": "거기에 당면까지 꼭 넣어요",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022255.1.23",
          "form": "그럼 이전까지는..",
          "original_form": "그럼 이전까지는..",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022255.1.24",
          "form": "실패하셨나요",
          "original_form": "실패하셨나요",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022255.1.25",
          "form": "앗 ㅜㅜ 실패까진 아니였는데",
          "original_form": "앗 ㅜㅜ 실패까진 아니였는데",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022255.1.26",
          "form": "식당에 파는 그런 맛이 안났어요",
          "original_form": "식당에 파는 그런 맛이 안났어요",
          "speaker_id": "2",
          "time": "20210829 11:49"
        }
      ]
    }
