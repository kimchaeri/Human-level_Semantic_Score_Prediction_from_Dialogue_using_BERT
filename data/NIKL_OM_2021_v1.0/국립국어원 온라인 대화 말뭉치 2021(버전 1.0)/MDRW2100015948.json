      "utterance": [
        {
          "id": "MDRW2100015948.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015948.1.2",
          "form": "저는 키우지 않아요!",
          "original_form": "저는 키우지 않아요!",
          "speaker_id": "2",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100015948.1.3",
          "form": "name1님은 키우시나요?",
          "original_form": "&name1&님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100015948.1.4",
          "form": "저도 안키워요",
          "original_form": "저도 안키워요",
          "speaker_id": "1",
          "time": "20210827 02:06"
        },
        {
          "id": "MDRW2100015948.1.5",
          "form": "그럼 키우고 싶은 동물이 있나요?",
          "original_form": "그럼 키우고 싶은 동물이 있나요?",
          "speaker_id": "1",
          "time": "20210827 02:06"
        },
        {
          "id": "MDRW2100015948.1.6",
          "form": "저는 고양이 키워보고싶어요!",
          "original_form": "저는 고양이 키워보고싶어요!",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100015948.1.7",
          "form": "오 저도요! 나중에 키우실 생각이 있나요?",
          "original_form": "오 저도요! 나중에 키우실 생각이 있나요?",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100015948.1.8",
          "form": "아뇨ㅠㅠ 비염러라서 꿈도 못꿀것 같네요",
          "original_form": "아뇨ㅠㅠ 비염러라서 꿈도 못꿀것 같네요",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100015948.1.9",
          "form": "name1님은 계획있으신가요?",
          "original_form": "&name1&님은 계획있으신가요?",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100015948.1.10",
          "form": "아이구 ㅜㅜ",
          "original_form": "아이구 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100015948.1.11",
          "form": "저도 키우고 싶딘 한데 먼 미래가 될 것 같아요..",
          "original_form": "저도 키우고 싶딘 한데 먼 미래가 될 것 같아요..",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100015948.1.12",
          "form": "아하... 아직이시군요",
          "original_form": "아하... 아직이시군요",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100015948.1.13",
          "form": "정말 반려동물 있으면 힐링될거같긴해요",
          "original_form": "정말 반려동물 있으면 힐링될거같긴해요",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100015948.1.14",
          "form": "따뜻한 털뭉치.. 존재만으로 힐링....",
          "original_form": "따뜻한 털뭉치.. 존재만으로 힐링....",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100015948.1.15",
          "form": "일단 독립을 해야 키울텐데 ㅋㅋㅋㅋ큐ㅠ",
          "original_form": "일단 독립을 해야 키울텐데 ㅋㅋㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100015948.1.16",
          "form": "핫.... 맞아요.. 그게 젤 중요한 전제조건",
          "original_form": "핫.... 맞아요.. 그게 젤 중요한 전제조건",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100015948.1.17",
          "form": "반려동물 인구는 많은거같은데 이상하게 제 주변에는 별로 없더라구요",
          "original_form": "반려동물 인구는 많은거같은데 이상하게 제 주변에는 별로 없더라구요",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100015948.1.18",
          "form": "헐 저도요 고양이 키우는 친구네 집 놀러가고싶은데 제 친구들은 안키워요 흠",
          "original_form": "헐 저도요 고양이 키우는 친구네 집 놀러가고싶은데 제 친구들은 안키워요 흠",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100015948.1.19",
          "form": "누가 키우는걸까요ㅋㅋㅋㅋㅋ티비에는 엄청 나오던데",
          "original_form": "누가 키우는걸까요ㅋㅋㅋㅋㅋ티비에는 엄청 나오던데",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100015948.1.20",
          "form": "다들 숨기고 있나 ㅋㅋㅋㅋㅋ",
          "original_form": "다들 숨기고 있나 ㅋㅋㅋㅋㅋ",
