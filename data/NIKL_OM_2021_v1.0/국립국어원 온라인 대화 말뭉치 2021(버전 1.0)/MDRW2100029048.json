          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029048.1.19",
          "form": "스페인어",
          "original_form": "스페인어",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029048.1.20",
          "form": "오 ㅋㅋㅋㅋ 뭔가 멋있어요",
          "original_form": "오 ㅋㅋㅋㅋ 뭔가 멋있어요",
