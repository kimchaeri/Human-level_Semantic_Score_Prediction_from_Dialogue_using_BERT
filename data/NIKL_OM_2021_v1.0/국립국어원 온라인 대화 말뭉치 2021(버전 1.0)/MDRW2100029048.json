      "utterance": [
        {
          "id": "MDRW2100029048.1.1",
          "form": "유투브는 뭐 보시나요?",
          "original_form": "유투브는 뭐 보시나요?",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029048.1.2",
          "form": "넷플릭스 재밌는거 보신거 있나요?",
          "original_form": "넷플릭스 재밌는거 보신거 있나요?",
          "speaker_id": "1",
          "time": "20210903 08:24"
        },
        {
          "id": "MDRW2100029048.1.3",
          "form": "저는 종이의 집 재밌게 봤어요!",
          "original_form": "저는 종이의 집 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029048.1.4",
          "form": "그거 볼려다 말았는데 ㅜㅜ",
          "original_form": "그거 볼려다 말았는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100029048.1.5",
          "form": "진짜 재밌어요 ㅜㅜ 곧 시즌 새로 나온다고 하더라구요",
          "original_form": "진짜 재밌어요 ㅜㅜ 곧 시즌 새로 나온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029048.1.6",
          "form": "거의 일 년 동안 기다렸어요..",
          "original_form": "거의 일 년 동안 기다렸어요..",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029048.1.7",
          "form": "몇시즌까지 있나요",
          "original_form": "몇시즌까지 있나요",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029048.1.8",
          "form": "지금 아마 4? 5? 정도 있을거예요",
          "original_form": "지금 아마 4? 5? 정도 있을거예요",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029048.1.9",
          "form": "너무 좋아서 정주행하고 스페인어 배우려고 했어요 ㅋㅋㅋ",
          "original_form": "너무 좋아서 정주행하고 스페인어 배우려고 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029048.1.10",
          "form": "오 스페인 드라마인가바요 ㅎㅎ",
          "original_form": "오 스페인 드라마인가바요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029048.1.11",
          "form": "그거 포스터만보면? 무서운거 같았는데 ㅋㅋ",
          "original_form": "그거 포스터만보면? 무서운거 같았는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029048.1.12",
          "form": "그건 아니죠",
          "original_form": "그건 아니죠",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029048.1.13",
          "form": "네 그냥 범죄영화예요!",
          "original_form": "네 그냥 범죄영화예요!",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029048.1.14",
          "form": "근데 캐릭터들이 다 매력있고 소재도 참신해서 재밌더라구요",
          "original_form": "근데 캐릭터들이 다 매력있고 소재도 참신해서 재밌더라구요",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029048.1.15",
          "form": "스페인어 발음이 진짜 멋있잖아요 ㅎㅎ",
          "original_form": "스페인어 발음이 진짜 멋있잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029048.1.16",
          "form": "저 무슨 애니에서 봤는데",
          "original_form": "저 무슨 애니에서 봤는데",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029048.1.17",
          "form": "mi corazon",
          "original_form": "mi corazon",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029048.1.18",
          "form": "이단어 좋아해서 이것만 알아요 ㅋㅋㅋㅋㅋ",
          "original_form": "이단어 좋아해서 이것만 알아요 ㅋㅋㅋㅋㅋ",
