      "utterance": [
        {
          "id": "MDRW2100045593.1.1",
          "form": "주인공 여자애 둘이서 1화부터 손 맞잡고 우리의 사랑은...이럼",
          "original_form": "주인공 여자애 둘이서 1화부터 손 맞잡고 우리의 사랑은...이럼",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045593.1.2",
          "form": "응응 근데 사실 잘...? 모르겠어요 일단 한국기준 19인데",
          "original_form": "응응 근데 사실 잘...? 모르겠어요 일단 한국기준 19인데",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045593.1.3",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "1",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045593.1.4",
          "form": "갓파 친구들은 성인이었나??",
          "original_form": "갓파 친구들은 성인이었나??",
          "speaker_id": "1",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045593.1.5",
          "form": "그냥...성동애라 19금일수도",
          "original_form": "그냥...성동애라 19금일수도",
          "speaker_id": "2",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.6",
          "form": "우웃...",
          "original_form": "우웃...",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.7",
          "form": "근데 ㅇ/ㅑ한장면 있긴 있는걸로 알아요",
          "original_form": "근데 ㅇ/ㅑ한장면 있긴 있는걸로 알아요",
          "speaker_id": "2",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.8",
          "form": "앗 아니",
          "original_form": "앗 아니",
          "speaker_id": "2",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.9",
          "form": "개네 중학생?인가 고등학생일걸",
          "original_form": "개네 중학생?인가 고등학생일걸",
          "speaker_id": "2",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.10",
          "form": "앗 그 등급가가",
          "original_form": "앗 그 등급가가",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.11",
          "form": "성인등급이었나?",
          "original_form": "성인등급이었나?",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.12",
          "form": "걔네ㅜ학교다니는거뉴기억남",
          "original_form": "걔네ㅜ학교다니는거뉴기억남",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.13",
          "form": "학교가기전에 여장하지 않나?",
          "original_form": "학교가기전에 여장하지 않나?",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045593.1.14",
          "form": "넹 맞아요",
          "original_form": "넹 맞아요",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.15",
          "form": "그리고 눈삐죽 친구랑 학교에서ㅠ만나구",
          "original_form": "그리고 눈삐죽 친구랑 학교에서ㅠ만나구",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.16",
          "form": "아 맞아 주인공 남자애가 ㅋㅋㅋ 하지 그것도 사정이 있긴 있는데...넹",
          "original_form": "아 맞아 주인공 남자애가 ㅋㅋㅋ 하지 그것도 사정이 있긴 있는데...넹",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.18",
          "form": "아 본인이 크로스드레서가ㅜ아니라",
          "original_form": "아 본인이 크로스드레서가ㅜ아니라",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.19",
          "form": "사정이 있는거였어(?",
          "original_form": "사정이 있는거였어(?",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.20",
          "form": "아 응응 그게 ... 자기 여동생이",
          "original_form": "아 응응 그게 ... 자기 여동생이",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.21",
          "form": "모 여자아이돌을 좋아하는데",
          "original_form": "모 여자아이돌을 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045593.1.22",
          "form": "그것 때문에 그 여자아이돌로 여장해서",
          "original_form": "그것 때문에 그 여자아이돌로 여장해서",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045593.1.23",
          "form": "뭐.... 그 여동생과 소통해주고 여동생을 기쁘게 하는...그런 사정이었었다",
          "original_form": "뭐.... 그 여동생과 소통해주고 여동생을 기쁘게 하는...그런 사정이었었다",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045593.1.24",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045593.1.25",
          "form": "그런 좋은 오빠였네...",
          "original_form": "그런 좋은 오빠였네...",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045593.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045593.1.27",
          "form": "하지만 사칭은 나빠욜",
          "original_form": "하지만 사칭은 나빠욜",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045593.1.28",
          "form": "심지어여장까지해주는....그치나쁘지 그래서 나중에 곤란해지긴 함(ㅋㅋㅋ",
          "original_form": "심지어여장까지해주는....그치나쁘지 그래서 나중에 곤란해지긴 함(ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045593.1.29",
          "form": "그리고...그 뭐냐",
          "original_form": "그리고...그 뭐냐",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045593.1.30",
          "form": "스노우볼 굴러간다...",
          "original_form": "스노우볼 굴러간다...",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045593.1.31",
          "form": "주인공 중에 안경잽이 있었는데",
          "original_form": "주인공 중에 안경잽이 있었는데",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
