        },
        {
          "id": "MDRW2100037308.1.2",
          "form": "파스타 좋아하니",
          "original_form": "파스타 좋아하니",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.3",
          "form": "가끔 해먹기도 해",
          "original_form": "가끔 해먹기도 해",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.4",
          "form": "요리 좋아하는구나",
          "original_form": "요리 좋아하는구나",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.5",
          "form": "그건 아닌데 먹을 만해",
          "original_form": "그건 아닌데 먹을 만해",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.6",
          "form": "나는 로제 파스타 먹고 싶다",
          "original_form": "나는 로제 파스타 먹고 싶다",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.7",
          "form": "나는 크림파스타 좋아해",
          "original_form": "나는 크림파스타 좋아해",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.8",
          "form": "토마토도 맛있어",
          "original_form": "토마토도 맛있어",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.9",
          "form": "난 토마토는 좋아하지는 않아",
          "original_form": "난 토마토는 좋아하지는 않아",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.10",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037308.1.11",
          "form": "크림이 더 맛있더라고",
          "original_form": "크림이 더 맛있더라고",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037308.1.12",
          "form": "넙적한 면을 좋아하니",
          "original_form": "넙적한 면을 좋아하니",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037308.1.13",
          "form": "응 얇은 거 보단 그게 더 맛있어",
          "original_form": "응 얇은 거 보단 그게 더 맛있어",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037308.1.14",
          "form": "나는 면을 직접 뽑아 먹어",
          "original_form": "나는 면을 직접 뽑아 먹어",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037308.1.15",
          "form": "애명까지 하다니 차원이 다르네",
          "original_form": "애명까지 하다니 차원이 다르네",
