      "utterance": [
        {
          "id": "MDRW2100032485.1.1",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210905 19:20"
        },
        {
          "id": "MDRW2100032485.1.2",
          "form": "뭐가 나쁜말인데",
          "original_form": "뭐가 나쁜말인데",
          "speaker_id": "2",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100032485.1.3",
          "form": "여튼 지금 막",
          "original_form": "여튼 지금 막",
          "speaker_id": "2",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100032485.1.4",
          "form": "새로운 일 터짐",
          "original_form": "새로운 일 터짐",
          "speaker_id": "2",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100032485.1.5",
          "form": "아르바이트쌤 갑자기 좀전에 카톡으류 월욜에 못나온댄다",
          "original_form": "아르바이트쌤 갑자기 좀전에 카톡으류 월욜에 못나온댄다",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100032485.1.6",
          "form": "오...러닝...",
          "original_form": "오...러닝...",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032485.1.7",
          "form": "도주...",
          "original_form": "도주...",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032485.1.8",
          "form": "도주퇴사",
          "original_form": "도주퇴사",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032485.1.9",
          "form": "알바 도주..대단...심지어 전에 일해보고 자기가 먼저 하고싶다고 연락왔던거",
          "original_form": "알바 도주..대단...심지어 전에 일해보고 자기가 먼저 하고싶다고 연락왔던거",
          "speaker_id": "2",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100032485.1.10",
          "form": "대단하다 대단햐",
          "original_form": "대단하다 대단햐",
          "speaker_id": "2",
          "time": "20210906 09:25"
        },
        {
          "id": "MDRW2100032485.1.11",
          "form": "병원도 그런일많구나...",
          "original_form": "병원도 그런일많구나...",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100032485.1.12",
          "form": "알바잖아..책임감도 없구..",
          "original_form": "알바잖아..책임감도 없구..",
          "speaker_id": "2",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100032485.1.13",
          "form": "그랴도 이 바닥 좁은거 알면서 참 대단하다고 생각햐",
          "original_form": "그랴도 이 바닥 좁은거 알면서 참 대단하다고 생각햐",
          "speaker_id": "2",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100032485.1.14",
          "form": "나 지금 넘나 힘드러 흑...",
          "original_form": "나 지금 넘나 힘드러 흑...",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100032485.1.15",
          "form": "할일 쌓였겠네",
          "original_form": "할일 쌓였겠네",
          "speaker_id": "1",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100032485.1.16",
          "form": "아니.... 다른사람구할시간만이라도좀줘라...",
          "original_form": "아니.... 다른사람구할시간만이라도좀줘라...",
          "speaker_id": "1",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100032485.1.17",
          "form": "다시는 만나고싶지않아",
          "original_form": "다시는 만나고싶지않아",
          "speaker_id": "2",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100032485.1.18",
          "form": "난 진짜 어디가서 도주퇴사는 안한다",
          "original_form": "난 진짜 어디가서 도주퇴사는 안한다",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100032485.1.19",
          "form": "마감도 짼적없다",
          "original_form": "마감도 짼적없다",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100032485.1.20",
          "form": "으른은 그런거닉가",
          "original_form": "으른은 그런거닉가",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100032485.1.21",
          "form": "하지만 세상엔 어른이 못된 사람이 많아",
          "original_form": "하지만 세상엔 어른이 못된 사람이 많아",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100032485.1.22",
          "form": "마자...나이만 먹었지 어른이 되지 못한 사람들 너무 많아",
          "original_form": "마자...나이만 먹었지 어른이 되지 못한 사람들 너무 많아",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100032485.1.23",
          "form": "그 책임감없는 알바때문에 지금 우리는 죽어나가",
          "original_form": "그 책임감없는 알바때문에 지금 우리는 죽어나가",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100032485.1.24",
          "form": "이미 금욜 퇴근하면서부터 안나올 마음 만반이었드만",
          "original_form": "이미 금욜 퇴근하면서부터 안나올 마음 만반이었드만",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100032485.1.25",
          "form": "짐 다 챙겨가놓고선 그럼 토욜에라도 말해줬어야지",
          "original_form": "짐 다 챙겨가놓고선 그럼 토욜에라도 말해줬어야지",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100032485.1.26",
          "form": "양심도없어 진짜",
          "original_form": "양심도없어 진짜",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100032485.1.27",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100032485.1.28",
          "form": "퇴사는 2주전 통보예요 요사람아",
          "original_form": "퇴사는 2주전 통보예요 요사람아",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100032485.1.29",
          "form": "놀라운 소식, 어제 밤 10시에 또 연락옴",
          "original_form": "놀라운 소식, 어제 밤 10시에 또 연락옴",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100032485.1.30",
          "form": "다시 아르바이트 구하면 연락달래ㅋㅋ우리가 왜...?",
          "original_form": "다시 아르바이트 구하면 연락달래ㅋㅋ우리가 왜...?",
          "speaker_id": "2",
