      "utterance": [
        {
          "id": "MDRW2100000710.1.1",
          "form": "연애하는 중인가요?",
          "original_form": "연애하는 중인가요?",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000710.1.2",
          "form": "네! 연애중입니다ㅎㅎ",
          "original_form": "네! 연애중입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000710.1.3",
          "form": "며칠째인가요?",
          "original_form": "며칠째인가요?",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000710.1.4",
          "form": "횟수로 지금 4년차에요!",
          "original_form": "횟수로 지금 4년차에요!",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000710.1.5",
          "form": "오래됐네요?",
          "original_form": "오래됐네요?",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000710.1.6",
          "form": "저는 3년차에요",
          "original_form": "저는 3년차에요",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000710.1.7",
          "form": "오오 그렇군요!",
          "original_form": "오오 그렇군요!",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000710.1.8",
          "form": "소개팅으로 만난건가요?",
          "original_form": "소개팅으로 만난건가요?",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000710.1.9",
          "form": "아니요!ㅎㅎ 원래 알던 오빠 동생 사이에서 발전해나가게 됐어요",
          "original_form": "아니요!ㅎㅎ 원래 알던 오빠 동생 사이에서 발전해나가게 됐어요",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000710.1.10",
          "form": "그게 보통이긴하죠 아는 여사친 남사친에서 연인으로",
          "original_form": "그게 보통이긴하죠 아는 여사친 남사친에서 연인으로",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000710.1.11",
          "form": "맞아요ㅎㅎㅎ 어떻게 연애 하시게되셨어요?",
          "original_form": "맞아요ㅎㅎㅎ 어떻게 연애 하시게되셨어요?",
          "speaker_id": "1",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100000710.1.12",
          "form": "저는 카페에서 만났어요",
          "original_form": "저는 카페에서 만났어요",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100000710.1.13",
          "form": "우오아 그래요? 소개팅으로 만나신거에요? 아니면 우연히?",
          "original_form": "우오아 그래요? 소개팅으로 만나신거에요? 아니면 우연히?",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100000710.1.14",
          "form": "거기서 번호 받았어요",
          "original_form": "거기서 번호 받았어요",
          "speaker_id": "2",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100000710.1.15",
          "form": "우와ㅎㅎ 상상으로만 생각했던 그런 만남이네요",
          "original_form": "우와ㅎㅎ 상상으로만 생각했던 그런 만남이네요",
          "speaker_id": "1",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100000710.1.16",
          "form": "이런 만남을 좋아하시나봐요",
          "original_form": "이런 만남을 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100000710.1.17",
          "form": "그냥 본 아직 번호를 물어보고 그런다는게 환상이라서ㅎㅋㅋㅋㅋ",
          "original_form": "그냥 본 아직 번호를 물어보고 그런다는게 환상이라서ㅎㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100000710.1.18",
          "form": "좋아한다기 보다는 저한테는 일어나지 않을 일 같아서 현실감이 좀 없달까요",
          "original_form": "좋아한다기 보다는 저한테는 일어나지 않을 일 같아서 현실감이 좀 없달까요",
          "speaker_id": "1",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100000710.1.19",
          "form": "아 근데 그때 일이 있어서 빡세게 꾸미긴했어요",
          "original_form": "아 근데 그때 일이 있어서 빡세게 꾸미긴했어요",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100000710.1.20",
          "form": "저도 친구사이에서 시작하는게 이제까지는 주로 그래왔어서 어색하긴했어요",
          "original_form": "저도 친구사이에서 시작하는게 이제까지는 주로 그래왔어서 어색하긴했어요",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100000710.1.21",
          "form": "오오 그렇군요ㅎㅎㅎ 그래도 오래 연애하신 만큼 서로에 대해 잘 알고 말하지 않아도 통하는 그런게 있지 않아요?",
          "original_form": "오오 그렇군요ㅎㅎㅎ 그래도 오래 연애하신 만큼 서로에 대해 잘 알고 말하지 않아도 통하는 그런게 있지 않아요?",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100000710.1.22",
          "form": "맞아요 3년이나 됐으니",
          "original_form": "맞아요 3년이나 됐으니",
          "speaker_id": "2",
          "time": "20210819 18:23"
        }
      ]
