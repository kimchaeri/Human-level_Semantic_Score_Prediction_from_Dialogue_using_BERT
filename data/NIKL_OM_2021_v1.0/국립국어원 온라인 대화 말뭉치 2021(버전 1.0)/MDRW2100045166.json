        {
          "id": "MDRW2100045166.1.1",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045166.1.2",
          "form": "이상형이 뭐세요",
          "original_form": "이상형이 뭐세요",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045166.1.3",
          "form": "전",
          "original_form": "전",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045166.1.4",
          "form": "외모요 ?",
          "original_form": "외모요 ?",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045166.1.5",
          "form": "저도 와꾸",
          "original_form": "저도 와꾸",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045166.1.6",
          "form": "와꾸가 취향타야해요",
          "original_form": "와꾸가 취향타야해요",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045166.1.7",
          "form": "그쵸 근데무쌍이 좋았는데",
          "original_form": "그쵸 근데무쌍이 좋았는데",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045166.1.8",
          "form": "또 무쌍만 좋은 건 아니더라구요",
          "original_form": "또 무쌍만 좋은 건 아니더라구요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045166.1.9",
          "form": "ㄴ저도 무쌍인줄알았는데",
          "original_form": "ㄴ저도 무쌍인줄알았는데",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045166.1.10",
          "form": "유쌍인거같더라고요",
          "original_form": "유쌍인거같더라고요",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045166.1.11",
          "form": "더보이즈 현재가 완식임",
          "original_form": "더보이즈 현재가 완식임",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045166.1.12",
          "form": "사실 무쌍유쌍을 떠나서",
          "original_form": "사실 무쌍유쌍을 떠나서",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045166.1.13",
          "form": "잘생기면 되는거 아닌가 싶어요 ~",
          "original_form": "잘생기면 되는거 아닌가 싶어요 ~",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045166.1.14",
          "form": "띵언입니다",
          "original_form": "띵언입니다",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045166.1.15",
          "form": "근데 신기하게 아이돌좋아할땐",
          "original_form": "근데 신기하게 아이돌좋아할땐",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045166.1.16",
          "form": "이상형이랑 멀어져요",
          "original_form": "이상형이랑 멀어져요",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045166.1.17",
          "form": "원래 이상형은 못 만난다잖아요",
          "original_form": "원래 이상형은 못 만난다잖아요",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045166.1.18",
          "form": "그런게어딨어여",
          "original_form": "그런게어딨어여",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045166.1.19",
          "form": "내가해냄",
          "original_form": "내가해냄",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045166.1.20",
          "form": "오 해내셨어요 ?",
          "original_form": "오 해내셨어요 ?",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045166.1.21",
          "form": "장난입니다",
          "original_form": "장난입니다",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045166.1.22",
          "form": "어케해내요 ㅡㅡ",
          "original_form": "어케해내요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045166.1.23",
          "form": "맞아요 어려워요",
          "original_form": "맞아요 어려워요",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045166.1.24",
          "form": "아이돌 좋아하는데 연애운이라잖아요",
          "original_form": "아이돌 좋아하는데 연애운이라잖아요",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045166.1.25",
          "form": "전 연애운이 좋은가봐여 하핫",
          "original_form": "전 연애운이 좋은가봐여 하핫",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045166.1.26",
          "form": "허얼 그런거에여?",
          "original_form": "허얼 그런거에여?",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045166.1.27",
          "form": "고딩때부터 연애운이..어째 여고라 그런가여",
          "original_form": "고딩때부터 연애운이..어째 여고라 그런가여",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045166.1.28",
          "form": "그런가봐여 그게 다 아이돌로 갔나봐여",
          "original_form": "그런가봐여 그게 다 아이돌로 갔나봐여",
          "speaker_id": "2",
          "time": "20210908 01:21"
        }
      ]
    }
  ]
