      "utterance": [
        {
          "id": "MDRW2100006164.1.1",
          "form": "기억에 남는 선물 있나요",
          "original_form": "기억에 남는 선물 있나요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006164.1.2",
          "form": "친구가 매년생일때마다",
          "original_form": "친구가 매년생일때마다",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100006164.1.3",
          "form": "소소한선물을 했는데",
          "original_form": "소소한선물을 했는데",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100006164.1.4",
          "form": "큰건아니지만 고맙더라구요",
          "original_form": "큰건아니지만 고맙더라구요",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006164.1.5",
          "form": "어떤 선물들이었나요?",
          "original_form": "어떤 선물들이었나요?",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006164.1.6",
          "form": "음 그냥 귀걸이나 향수 같은거였어요",
          "original_form": "음 그냥 귀걸이나 향수 같은거였어요",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006164.1.7",
          "form": "비싼건 아니고 가볍게 받을수있는것들",
          "original_form": "비싼건 아니고 가볍게 받을수있는것들",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006164.1.8",
          "form": "가벼운게 아닌데요?정성이나 관심이 필요한거네요",
          "original_form": "가벼운게 아닌데요?정성이나 관심이 필요한거네요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006164.1.9",
          "form": "아 근데 친구도 어디 뭐 이벤트 당첨되고 필요없어서 남은거주는거라ㅋㅋ",
          "original_form": "아 근데 친구도 어디 뭐 이벤트 당첨되고 필요없어서 남은거주는거라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006164.1.10",
          "form": "그래도 제생일을 안잊고 챙겨주는거자체가 고마웠어요",
          "original_form": "그래도 제생일을 안잊고 챙겨주는거자체가 고마웠어요",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006164.1.11",
          "form": "그러셨군요 받고싶은 선물은 있나요?",
          "original_form": "그러셨군요 받고싶은 선물은 있나요?",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100006164.1.12",
          "form": "음ㅋㅋ 현금주면 제일좋고 지금 필요한건 식세기네요",
          "original_form": "음ㅋㅋ 현금주면 제일좋고 지금 필요한건 식세기네요",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006164.1.13",
          "form": "받고싶은 선물있으세요?",
          "original_form": "받고싶은 선물있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006164.1.14",
          "form": "식세기 좋죠..ㅎㅎ 저도 현금이..",
          "original_form": "식세기 좋죠..ㅎㅎ 저도 현금이..",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006164.1.15",
          "form": "최근에 선물받은경험 있으세요?",
          "original_form": "최근에 선물받은경험 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006164.1.16",
          "form": "생일이면 온 가족에게 현금...친구들에겐 기프티콘..받았네요",
          "original_form": "생일이면 온 가족에게 현금...친구들에겐 기프티콘..받았네요",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006164.1.17",
          "form": "와 유용한 걸로 많이 받았네요",
          "original_form": "와 유용한 걸로 많이 받았네요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100006164.1.18",
          "form": "치킨기프티콘 받으면 기분좋을거같아요",
          "original_form": "치킨기프티콘 받으면 기분좋을거같아요",
