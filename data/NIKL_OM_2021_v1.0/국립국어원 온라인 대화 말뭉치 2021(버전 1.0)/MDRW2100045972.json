          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045972.1.3",
          "form": "피부가 엉망이에여",
          "original_form": "피부가 엉망이에여",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045972.1.4",
          "form": "ㅠㅜ속상해요",
          "original_form": "ㅠㅜ속상해요",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045972.1.5",
          "form": "점점심해져요ㅠ마스크좀안쓰고싶네요",
          "original_form": "점점심해져요ㅠ마스크좀안쓰고싶네요",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045972.1.6",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045972.1.7",
          "form": "팩같은거하시나요",
          "original_form": "팩같은거하시나요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045972.1.8",
          "form": "아녀ㅠ",
          "original_form": "아녀ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045972.1.9",
          "form": "사실 ㅋㅋ 관리도 잘안해여뭐든 부지런해야겟죠ㅠ",
          "original_form": "사실 ㅋㅋ 관리도 잘안해여뭐든 부지런해야겟죠ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045972.1.10",
          "form": "ㅎㅎㅎ일찍자고잘먹는게최고죠",
          "original_form": "ㅎㅎㅎ일찍자고잘먹는게최고죠",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045972.1.11",
          "form": "ㅋㅋㅋㅋ 운동하세요?",
          "original_form": "ㅋㅋㅋㅋ 운동하세요?",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045972.1.12",
          "form": "음 산책...?",
          "original_form": "음 산책...?",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045972.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045972.1.14",
          "form": "ㅎㅎㅎ 그게 어디에요 걷는게 최고에요",
          "original_form": "ㅎㅎㅎ 그게 어디에요 걷는게 최고에요",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045972.1.15",
          "form": "ㅎㅎㄷㅎ어제는플랭크를하긴했어요",
          "original_form": "ㅎㅎㄷㅎ어제는플랭크를하긴했어요",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045972.1.16",
          "form": "대단해요 ㅎㅎ",
          "original_form": "대단해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045972.1.17",
          "form": "홈트도 맘먹기가",
          "original_form": "홈트도 맘먹기가",
