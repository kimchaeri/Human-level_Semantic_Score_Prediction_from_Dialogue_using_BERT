      "utterance": [
        {
          "id": "MDRW2100025529.1.1",
          "form": "오늘같은날은",
          "original_form": "오늘같은날은",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025529.1.2",
          "form": "샤브샤브나 훠궈가 국룰인디",
          "original_form": "샤브샤브나 훠궈가 국룰인디",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025529.1.3",
          "form": "사실 향호면옥가서 바지락 칼국수먹을라했는디",
          "original_form": "사실 향호면옥가서 바지락 칼국수먹을라했는디",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025529.1.4",
          "form": "밀가루투머치라",
          "original_form": "밀가루투머치라",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025529.1.5",
          "form": "패쓰혔음",
          "original_form": "패쓰혔음",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025529.1.6",
          "form": "아 칼국수",
          "original_form": "아 칼국수",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025529.1.7",
          "form": "투머치긴하지",
          "original_form": "투머치긴하지",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025529.1.8",
          "form": "근데넘맛있어",
          "original_form": "근데넘맛있어",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025529.1.9",
          "form": "얼큰칼먹고프다",
          "original_form": "얼큰칼먹고프다",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025529.1.10",
          "form": "장칼국수",
          "original_form": "장칼국수",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025529.1.11",
          "form": "나 한번도 안먹어봤다",
          "original_form": "나 한번도 안먹어봤다",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025529.1.12",
