      "utterance": [
        {
          "id": "MDRW2100016012.1.1",
          "form": "마트가면 주로 뭐 사시나요",
          "original_form": "마트가면 주로 뭐 사시나요",
          "speaker_id": "2",
          "time": "20210826 07:36"
        },
        {
          "id": "MDRW2100016012.1.2",
          "form": "대형마트요",
          "original_form": "대형마트요",
          "speaker_id": "2",
          "time": "20210826 07:36"
        },
        {
          "id": "MDRW2100016012.1.3",
          "form": "저는 마트 주로 과일사러 자주가요~",
          "original_form": "저는 마트 주로 과일사러 자주가요~",
          "speaker_id": "1",
          "time": "20210826 07:37"
        },
        {
          "id": "MDRW2100016012.1.4",
          "form": "어느마트 자주가세요??",
          "original_form": "어느마트 자주가세요??",
          "speaker_id": "1",
          "time": "20210826 07:37"
        },
        {
          "id": "MDRW2100016012.1.5",
          "form": "전 이마트 근처에 있어서 이마트가요",
          "original_form": "전 이마트 근처에 있어서 이마트가요",
          "speaker_id": "2",
          "time": "20210826 07:37"
        },
        {
          "id": "MDRW2100016012.1.6",
          "form": "어디마트 자주가세요?",
          "original_form": "어디마트 자주가세요?",
          "speaker_id": "2",
          "time": "20210826 07:37"
        },
        {
          "id": "MDRW2100016012.1.7",
          "form": "오오 저두요! 집앞이 이마트에요",
          "original_form": "오오 저두요! 집앞이 이마트에요",
          "speaker_id": "1",
          "time": "20210826 07:37"
        },
        {
          "id": "MDRW2100016012.1.8",
          "form": "역시 이마트가 지점이 많긴해요",
          "original_form": "역시 이마트가 지점이 많긴해요",
          "speaker_id": "2",
          "time": "20210826 07:37"
        },
        {
          "id": "MDRW2100016012.1.9",
          "form": "요즘 이마트에 과일 뭐가 맛있던가요",
          "original_form": "요즘 이마트에 과일 뭐가 맛있던가요",
          "speaker_id": "2",
          "time": "20210826 07:38"
        },
        {
          "id": "MDRW2100016012.1.10",
          "form": "맞아요~ 최근에는 복숭아 맛있었어요! 특히 황도요!!",
          "original_form": "맞아요~ 최근에는 복숭아 맛있었어요! 특히 황도요!!",
          "speaker_id": "1",
          "time": "20210826 07:38"
        },
        {
          "id": "MDRW2100016012.1.11",
          "form": "황도나올 때 됐구나",
          "original_form": "황도나올 때 됐구나",
          "speaker_id": "2",
          "time": "20210826 07:38"
        },
        {
          "id": "MDRW2100016012.1.12",
          "form": "황도 너무 맛있어요ㅎㅎ",
          "original_form": "황도 너무 맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 07:38"
        },
        {
          "id": "MDRW2100016012.1.13",
          "form": "음 제가알기론 복숭아 지금 끝물이긴해요",
          "original_form": "음 제가알기론 복숭아 지금 끝물이긴해요",
          "speaker_id": "1",
          "time": "20210826 07:38"
        },
        {
          "id": "MDRW2100016012.1.14",
          "form": "달고 맛났어요 ㅎㅎ",
          "original_form": "달고 맛났어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 07:39"
        },
        {
          "id": "MDRW2100016012.1.15",
          "form": "아앗.. 끝물이구나 열심히 사먹어야겠네요",
          "original_form": "아앗.. 끝물이구나 열심히 사먹어야겠네요",
          "speaker_id": "2",
          "time": "20210826 07:39"
        },
        {
          "id": "MDRW2100016012.1.16",
          "form": "보통 2개입 들은거 사시나요",
          "original_form": "보통 2개입 들은거 사시나요",
          "speaker_id": "2",
          "time": "20210826 07:39"
        },
        {
          "id": "MDRW2100016012.1.17",
          "form": "ㅋㅋㅋ 기회되면 먹으면되죠뭐~ㅎㅎ",
          "original_form": "ㅋㅋㅋ 기회되면 먹으면되죠뭐~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 07:39"
        },
        {
          "id": "MDRW2100016012.1.18",
          "form": "아뇨 4개씩들어있는거 샀어요~",
          "original_form": "아뇨 4개씩들어있는거 샀어요~",
          "speaker_id": "1",
          "time": "20210826 07:39"
        },
        {
          "id": "MDRW2100016012.1.19",
          "form": "아무래도 2개는 아쉬우니까",
          "original_form": "아무래도 2개는 아쉬우니까",
          "speaker_id": "2",
          "time": "20210826 07:39"
        },
        {
          "id": "MDRW2100016012.1.20",
          "form": "4개짜리가 좋겠어요",
          "original_form": "4개짜리가 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 07:40"
        },
        {
          "id": "MDRW2100016012.1.21",
          "form": "맞아요ㅋㅋ 그리구 가족도 있으니깐요ㅎㅎ",
          "original_form": "맞아요ㅋㅋ 그리구 가족도 있으니깐요ㅎㅎ",
