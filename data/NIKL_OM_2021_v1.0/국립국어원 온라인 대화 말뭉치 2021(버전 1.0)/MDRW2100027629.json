      "utterance": [
        {
          "id": "MDRW2100027629.1.1",
          "form": "저는 유투브에서 그냥 최신곡 모음이나",
          "original_form": "저는 유투브에서 그냥 최신곡 모음이나",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027629.1.2",
          "form": "세븐틴 모음 들어요 ㅎㅎ",
          "original_form": "세븐틴 모음 들어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027629.1.3",
          "form": "유튜브에서 노래 모음 이런거 듣는데 새로운 노래 알게되는게 좋더라고요",
          "original_form": "유튜브에서 노래 모음 이런거 듣는데 새로운 노래 알게되는게 좋더라고요",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027629.1.4",
          "form": "오 주로 어떤거 들으세요",
          "original_form": "오 주로 어떤거 들으세요",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027629.1.5",
          "form": "기운이 없을땐 비욘세 자주듣는건 리한나",
          "original_form": "기운이 없을땐 비욘세 자주듣는건 리한나",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027629.1.6",
          "form": "팝음악좋아해요",
          "original_form": "팝음악좋아해요",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027629.1.7",
          "form": "오 고런계열 좋아하시는군요",
          "original_form": "오 고런계열 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027629.1.8",
          "form": "에센셜인가 유투브 아세요?",
          "original_form": "에센셜인가 유투브 아세요?",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027629.1.9",
          "form": "누군지 모르는데 어떤분이신가요?",
          "original_form": "누군지 모르는데 어떤분이신가요?",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027629.1.10",
          "form": "팝 모아놓은곳",
          "original_form": "팝 모아놓은곳",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027629.1.11",
          "form": "인데 괜찮더라구요",
          "original_form": "인데 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027629.1.12",
          "form": "한번 들어봐야겠네요",
          "original_form": "한번 들어봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027629.1.13",
          "form": "그러고보니 이렇게 노래 모아두는곳들 좋더라고요 힘들게 찾지않아도되고",
          "original_form": "그러고보니 이렇게 노래 모아두는곳들 좋더라고요 힘들게 찾지않아도되고",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027629.1.14",
          "form": "요즘 집꾸미는 영상보면 다 이거 티비에 틀어놓고 있어요 ㅋㅋ",
          "original_form": "요즘 집꾸미는 영상보면 다 이거 티비에 틀어놓고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027629.1.15",
          "form": "그러게요 편한세상",
          "original_form": "그러게요 편한세상",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027629.1.16",
          "form": "그래서 인가 신곡 기다리는 가수는 없고 그냥 두루두루 듣게되어요",
          "original_form": "그래서 인가 신곡 기다리는 가수는 없고 그냥 두루두루 듣게되어요",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027629.1.17",
          "form": "좋아하는 한국 가수는 없으신가요",
          "original_form": "좋아하는 한국 가수는 없으신가요",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027629.1.18",
          "form": "브라운아이드소울이랑 방탄",
          "original_form": "브라운아이드소울이랑 방탄",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027629.1.19",
          "form": "현재생각나는건 그 둘 뿐이네요",
          "original_form": "현재생각나는건 그 둘 뿐이네요",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027629.1.20",
          "form": "방탄 노래 신나고 좋더라구요 ㅎㅎ",
          "original_form": "방탄 노래 신나고 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:35"
        }
      ]
    }
