        {
          "id": "MDRW2100011276.1.1",
          "form": "반려동물 기르시나요",
          "original_form": "반려동물 기르시나요",
          "speaker_id": "1",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100011276.1.2",
          "form": "아니요 근데 한번 키워보고싶어요",
          "original_form": "아니요 근데 한번 키워보고싶어요",
          "speaker_id": "2",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100011276.1.3",
          "form": "어떤 동물 키워보고 싶나요",
          "original_form": "어떤 동물 키워보고 싶나요",
          "speaker_id": "1",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100011276.1.4",
          "form": "저는 고양이 키워보고싶어요",
          "original_form": "저는 고양이 키워보고싶어요",
          "speaker_id": "2",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100011276.1.5",
          "form": "고양이 좋아하시나 봐요",
          "original_form": "고양이 좋아하시나 봐요",
          "speaker_id": "1",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100011276.1.6",
          "form": "고양이 눈이 너무 귀여워요ㅠㅠ",
          "original_form": "고양이 눈이 너무 귀여워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 10:38"
        },
        {
          "id": "MDRW2100011276.1.7",
          "form": "맞아요 동그랗고 크고 보석 같아요",
          "original_form": "맞아요 동그랗고 크고 보석 같아요",
          "speaker_id": "1",
          "time": "20210824 10:38"
        },
        {
          "id": "MDRW2100011276.1.8",
          "form": "고양이에 또 어떤  점이 좋으신가요",
          "original_form": "고양이에 또 어떤  점이 좋으신가요",
          "speaker_id": "1",
          "time": "20210824 10:40"
        },
        {
          "id": "MDRW2100011276.1.9",
          "form": "소리도 귀여워요ㅎㅎ",
          "original_form": "소리도 귀여워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011276.1.10",
          "form": "야옹 하는 소리 말씀이시죠",
          "original_form": "야옹 하는 소리 말씀이시죠",
          "speaker_id": "1",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011276.1.11",
          "form": "맞아요 너무 귀여워요",
          "original_form": "맞아요 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011276.1.12",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011276.1.13",
          "form": "진돗개 한 마리 키우고 있어요",
          "original_form": "진돗개 한 마리 키우고 있어요",
          "speaker_id": "1",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011276.1.14",
          "form": "와 진돗개 엄청 멋있을것같아요",
          "original_form": "와 진돗개 엄청 멋있을것같아요",
          "speaker_id": "2",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011276.1.15",
          "form": "진돗개 좋아하시나요",
          "original_form": "진돗개 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100011276.1.16",
          "form": "네 좋아하죠ㅎㅎ 늠름한 진돗개",
