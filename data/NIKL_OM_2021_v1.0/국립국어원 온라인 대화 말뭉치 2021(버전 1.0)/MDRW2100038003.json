      "utterance": [
        {
          "id": "MDRW2100038003.1.1",
          "form": "안녕하세요 저녁드셨나요",
          "original_form": "안녕하세요 저녁드셨나요",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100038003.1.2",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100038003.1.3",
          "form": "어제저녁은 치킨에 똥집이었어요",
          "original_form": "어제저녁은 치킨에 똥집이었어요",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100038003.1.4",
          "form": "아침식사하셨나요??",
          "original_form": "아침식사하셨나요??",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100038003.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100038003.1.6",
          "form": "맛났겠어요 전 아침안먹어요 잠이 더 중해서 ㅋ",
          "original_form": "맛났겠어요 전 아침안먹어요 잠이 더 중해서 ㅋ",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100038003.1.7",
          "form": "ㅎㅎ 저도 아침은 늘 못먹고 출근합니다",
          "original_form": "ㅎㅎ 저도 아침은 늘 못먹고 출근합니다",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038003.1.8",
          "form": "빈속에 커피로 때우는데",
          "original_form": "빈속에 커피로 때우는데",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038003.1.9",
          "form": "요새는 건강이 좀 걱정되긴 하더라구요",
          "original_form": "요새는 건강이 좀 걱정되긴 하더라구요",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100038003.1.10",
          "form": "아유 역류성식도염  걸리십니다ㅜㅜㅜㅜㅜ",
          "original_form": "아유 역류성식도염  걸리십니다ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038003.1.11",
          "form": "빈속엔 넣지마세요",
          "original_form": "빈속엔 넣지마세요",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038003.1.12",
          "form": "네 ㅠㅠ 견과류든 뭐든 하나 사야겠어요",
          "original_form": "네 ㅠㅠ 견과류든 뭐든 하나 사야겠어요",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038003.1.13",
          "form": "음식 뭐 좋아하세요??",
          "original_form": "음식 뭐 좋아하세요??",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038003.1.14",
          "form": "배고플때먹는거요",
          "original_form": "배고플때먹는거요",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038003.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅅㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅅㅋ",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038003.1.16",
          "form": "그렇죠 배고플 때는 다 맛있죠",
          "original_form": "그렇죠 배고플 때는 다 맛있죠",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038003.1.17",
          "form": "저도 지금 배가 너무 고프네요",
          "original_form": "저도 지금 배가 너무 고프네요",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038003.1.18",
          "form": "ㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038003.1.19",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038003.1.20",
          "form": "아침 드시는편인데 오늘 거르셨나요",
          "original_form": "아침 드시는편인데 오늘 거르셨나요",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038003.1.21",
          "form": "아니요 시간이 늘 촉박해서 못먹고 나와요",
          "original_form": "아니요 시간이 늘 촉박해서 못먹고 나와요",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038003.1.22",
          "form": "나올때 간단하게 먹을 오예스나 빵조각 들고 나오는데",
          "original_form": "나올때 간단하게 먹을 오예스나 빵조각 들고 나오는데",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038003.1.23",
          "form": "오늘은 그것도 못챙겨서 ㅠ",
          "original_form": "오늘은 그것도 못챙겨서 ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038003.1.24",
          "form": "ㅜㅜ시리얼도 간편한데",
          "original_form": "ㅜㅜ시리얼도 간편한데",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038003.1.25",
          "form": "점심기다려지시겠네요",
          "original_form": "점심기다려지시겠네요",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038003.1.26",
          "form": "네 늘 점심을 기다리고 있습니다^^",
          "original_form": "네 늘 점심을 기다리고 있습니다^^",
          "speaker_id": "2",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038003.1.27",
          "form": "점심은 뭐 드시나요??",
          "original_form": "점심은 뭐 드시나요??",
          "speaker_id": "2",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038003.1.28",
          "form": "이제 봐야죠 누가 만들어주시는거라",
          "original_form": "이제 봐야죠 누가 만들어주시는거라",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038003.1.29",
          "form": "아 좋겠어요",
          "original_form": "아 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100038003.1.30",
          "form": "근처에 잘하는가정식있응좋을텐데 다 간이 쎄서 외식하는분들힘들겠어요",
          "original_form": "근처에 잘하는가정식있응좋을텐데 다 간이 쎄서 외식하는분들힘들겠어요",
          "speaker_id": "1",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100038003.1.31",
