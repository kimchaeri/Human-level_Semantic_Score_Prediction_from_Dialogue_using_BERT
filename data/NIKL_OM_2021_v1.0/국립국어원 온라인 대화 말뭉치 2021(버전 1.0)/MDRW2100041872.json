      "utterance": [
        {
          "id": "MDRW2100041872.1.1",
          "form": "원신 이거 2장 들어가는 중인데 맨날 죽는거 레벨이 낮아서 그렇지..? 다른 거 먼저 하고 메인 밀어도 되려나",
          "original_form": "원신 이거 2장 들어가는 중인데 맨날 죽는거 레벨이 낮아서 그렇지..? 다른 거 먼저 하고 메인 밀어도 되려나",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100041872.1.2",
          "form": "다른거 해도 죽을거 같긴 한데 용의 눈물 얻다가 계속 죽어서",
          "original_form": "다른거 해도 죽을거 같긴 한데 용의 눈물 얻다가 계속 죽어서",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100041872.1.3",
          "form": "(박치기하고 들어옴",
          "original_form": "(박치기하고 들어옴",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.4",
          "form": "지금 월렙 얼마야?",
          "original_form": "지금 월렙 얼마야?",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.5",
          "form": "월렙 13인가",
          "original_form": "월렙 13인가",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.6",
          "form": "그래서 아직 낮아서 안되나 싶어",
          "original_form": "그래서 아직 낮아서 안되나 싶어",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.7",
          "form": "ㅇㅎ그럼 걍 필드 돌아다니면서",
          "original_form": "ㅇㅎ그럼 걍 필드 돌아다니면서",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.8",
          "form": "요리 좀 하고",
          "original_form": "요리 좀 하고",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.9",
          "form": "애들 강화도 좀 시켜",
          "original_form": "애들 강화도 좀 시켜",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.10",
          "form": "아직 쪼랩이 덤비는 중이었군",
          "original_form": "아직 쪼랩이 덤비는 중이었군",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041872.1.11",
          "form": "레벨은 높이는 중인데 돌파에서 막혀서",
          "original_form": "레벨은 높이는 중인데 돌파에서 막혀서",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041872.1.12",
          "form": "월랩15랩부터 돌파가능하더라고",
          "original_form": "월랩15랩부터 돌파가능하더라고",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041872.1.13",
          "form": "ㅇㅎ돌파는 빡세지",
          "original_form": "ㅇㅎ돌파는 빡세지",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041872.1.14",
          "form": "이거 같이하기가 15렙부터 가능하던가?",
          "original_form": "이거 같이하기가 15렙부터 가능하던가?",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041872.1.15",
          "form": "요리 맥이면서 그때그때 싸워도 맨날 죽어가지고 엉엉 울면서 도망치고 다른곳 가고 그래",
          "original_form": "요리 맥이면서 그때그때 싸워도 맨날 죽어가지고 엉엉 울면서 도망치고 다른곳 가고 그래",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041872.1.16",
          "form": "그건 모르겠네...",
          "original_form": "그건 모르겠네...",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041872.1.17",
          "form": "아 친구 추가 해야하는데",
          "original_form": "아 친구 추가 해야하는데",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041872.1.18",
          "form": "돌파하려면 필드에 있는 보스를 잡아야하거든?",
          "original_form": "돌파하려면 필드에 있는 보스를 잡아야하거든?",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041872.1.19",
          "form": "그거는 내가 도와줄게",
          "original_form": "그거는 내가 도와줄게",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041872.1.20",
          "form": "일단은 걍 적당히 보이는것부터 해봐",
          "original_form": "일단은 걍 적당히 보이는것부터 해봐",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041872.1.21",
          "form": "월랩이 아니라 모험등급..",
          "original_form": "월랩이 아니라 모험등급..",
          "speaker_id": "1",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041872.1.22",
          "form": "할 수 있는 것부터.. ㅇㅇ..",
          "original_form": "할 수 있는 것부터.. ㅇㅇ..",
          "speaker_id": "1",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041872.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041872.1.24",
          "form": "무기 강화는",
          "original_form": "무기 강화는",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041872.1.25",
          "form": "열려있어?",
          "original_form": "열려있어?",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041872.1.26",
          "form": "일단 당장은 원석 모으지말고 몇번 더 돌려서",
          "original_form": "일단 당장은 원석 모으지말고 몇번 더 돌려서",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041872.1.27",
          "form": "4성 무기도 좀 뽑아두는게 좋아",
          "original_form": "4성 무기도 좀 뽑아두는게 좋아",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041872.1.28",
          "form": "돌리는 중인데 4성 무기 진짜 안나오더라;;;;",
          "original_form": "돌리는 중인데 4성 무기 진짜 안나오더라;;;;",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041872.1.29",
          "form": "그냉 있을때마다 돌리는 중인데..(절래",
          "original_form": "그냉 있을때마다 돌리는 중인데..(절래",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041872.1.30",
          "form": "걔도 10번에 한번 나와ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "걔도 10번에 한번 나와ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041872.1.31",
          "form": "진짜 짜",
          "original_form": "진짜 짜",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041872.1.32",
          "form": "아 그리고 요리",
          "original_form": "아 그리고 요리",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041872.1.33",
          "form": "버섯닭꼬치 케이아한테 만들게하면",
          "original_form": "버섯닭꼬치 케이아한테 만들게하면",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100041872.1.34",
          "form": "가아끔 조금 더 효과좋은거 주니까",
          "original_form": "가아끔 조금 더 효과좋은거 주니까",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100041872.1.35",
          "form": "ㅋㅋ큐ㅠ",
          "original_form": "ㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041872.1.36",
          "form": "앟",
          "original_form": "앟",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041872.1.37",
          "form": "진한테 하면 가아끔 2개 줘서",
          "original_form": "진한테 하면 가아끔 2개 줘서",
          "speaker_id": "1",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041872.1.38",
          "form": "번갈아가면서 해야겠군",
          "original_form": "번갈아가면서 해야겠군",
          "speaker_id": "1",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041872.1.39",
