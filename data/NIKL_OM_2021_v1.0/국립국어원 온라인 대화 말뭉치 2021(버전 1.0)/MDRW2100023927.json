      "utterance": [
        {
          "id": "MDRW2100023927.1.1",
          "form": "넼ㅋㅋㅋㅋ 이제 나가려고 단장중이요",
          "original_form": "넼ㅋㅋㅋㅋ 이제 나가려고 단장중이요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023927.1.2",
          "form": "미세먼지도 없고 비도 안내리고 산책 다녀오기 딱 좋네요",
          "original_form": "미세먼지도 없고 비도 안내리고 산책 다녀오기 딱 좋네요",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023927.1.3",
          "form": "습도가 그냥 미쳣어요",
          "original_form": "습도가 그냥 미쳣어요",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023927.1.4",
          "form": "ㅠㅠ 잘다녀 오십시오",
          "original_form": "ㅠㅠ 잘다녀 오십시오",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023927.1.5",
          "form": "와 잠깐 걸엇ㄱ는데 너무 덥네여",
          "original_form": "와 잠깐 걸엇ㄱ는데 너무 덥네여",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023927.1.6",
          "form": "누나 언제와요 집에",
          "original_form": "누나 언제와요 집에",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023927.1.7",
          "form": "답다해죽겟어ㅛ 사람들",
          "original_form": "답다해죽겟어ㅛ 사람들",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023927.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023927.1.9",
          "form": "저 집 도착!!",
          "original_form": "저 집 도착!!",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023927.1.10",
          "form": "아주 어항속에 있는 습도에요",
          "original_form": "아주 어항속에 있는 습도에요",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023927.1.11",
          "form": "어머 오늘 습도가 그렇게 높다구요?",
          "original_form": "어머 오늘 습도가 그렇게 높다구요?",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100023927.1.12",
          "form": "네네 아가미 있어야겠더라구요",
          "original_form": "네네 아가미 있어야겠더라구요",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100023927.1.13",
          "form": "그래도 습도 높은 덕분에",
          "original_form": "그래도 습도 높은 덕분에",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023927.1.14",
          "form": "집에 빨리 올 수 있었잖아요 ㅎㅎ",
          "original_form": "집에 빨리 올 수 있었잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023927.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023927.1.16",
          "form": "데이트보다 이벤트인 것 마냥",
          "original_form": "데이트보다 이벤트인 것 마냥",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023927.1.17",
          "form": "형식적으로 하는거에요?",
          "original_form": "형식적으로 하는거에요?",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023927.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023927.1.19",
          "form": "몸으로 날씨 느끼러 공원으로 가자 야호~!",
          "original_form": "몸으로 날씨 느끼러 공원으로 가자 야호~!",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023927.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023927.1.21",
          "form": "제가 오늘 여행퍼레이드 준비해왔어요",
          "original_form": "제가 오늘 여행퍼레이드 준비해왔어요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023927.1.22",
          "form": "기대해요",
          "original_form": "기대해요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
