      "utterance": [
        {
          "id": "MDRW2100023748.1.1",
          "form": "한섬만두 맛있다던데",
          "original_form": "한섬만두 맛있다던데",
          "speaker_id": "2",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100023748.1.2",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100023748.1.3",
          "form": "점심이 부족하지ㅠ",
          "original_form": "점심이 부족하지ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100023748.1.4",
          "form": "응 그래도 미역국 맛있더라 미역이 좋아",
          "original_form": "응 그래도 미역국 맛있더라 미역이 좋아",
          "speaker_id": "2",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100023748.1.5",
          "form": "응 재료가 뭐든 좋아야해",
          "original_form": "응 재료가 뭐든 좋아야해",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100023748.1.6",
          "form": "오늘 먹은 소고기 좋았어",
          "original_form": "오늘 먹은 소고기 좋았어",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100023748.1.7",
          "form": "결국 한섬만드도 사고 ㅋ",
          "original_form": "결국 한섬만드도 사고 ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100023748.1.8",
          "form": "만두가 있으니 든든하구만",
          "original_form": "만두가 있으니 든든하구만",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100023748.1.9",
          "form": "응 맞아 만두도 먹자",
          "original_form": "응 맞아 만두도 먹자",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100023748.1.10",
          "form": "계란먹어야지",
          "original_form": "계란먹어야지",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100023748.1.11",
          "form": "아침에 너무 안까지더라ㅡㅡ",
          "original_form": "아침에 너무 안까지더라ㅡㅡ",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100023748.1.12",
          "form": "웅 미안",
          "original_form": "웅 미안",
          "speaker_id": "2",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100023748.1.13",
          "form": "100원에 6알이라 덜신선한듯",
          "original_form": "100원에 6알이라 덜신선한듯",
          "speaker_id": "2",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100023748.1.14",
          "form": "근데 그 속껍질은 또 엄청 억셌어ㅋㅋ",
          "original_form": "근데 그 속껍질은 또 엄청 억셌어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100023748.1.15",
          "form": "응 ㅋㅋㅋ노계가 난 알인라봐",
          "original_form": "응 ㅋㅋㅋ노계가 난 알인라봐",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100023748.1.16",
          "form": "좋은 계란사자",
          "original_form": "좋은 계란사자",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100023748.1.17",
          "form": "ㅎㅎ 겉으로는 좋아 보이는데",
          "original_form": "ㅎㅎ 겉으로는 좋아 보이는데",
