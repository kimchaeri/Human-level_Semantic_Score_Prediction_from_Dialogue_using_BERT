      "utterance": [
        {
          "id": "MDRW2100017776.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017776.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017776.1.3",
          "form": "기억에 남는 여행지 있으세요?",
          "original_form": "기억에 남는 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017776.1.4",
          "form": "전에 제주도 갔던게 좋았어요!",
          "original_form": "전에 제주도 갔던게 좋았어요!",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017776.1.5",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017776.1.6",
          "form": "전 부산 여행갔었는데 바다보고 좋았어요!",
          "original_form": "전 부산 여행갔었는데 바다보고 좋았어요!",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017776.1.7",
          "form": "제주도 맛집들 가셨나요?",
          "original_form": "제주도 맛집들 가셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017776.1.8",
          "form": "오오 부산에서 어디 가셨어요?",
          "original_form": "오오 부산에서 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017776.1.9",
          "form": "제주도 맛집 갔을때 문닫았던 기억이 제일 크게 남았어요ㅋㅋ",
          "original_form": "제주도 맛집 갔을때 문닫았던 기억이 제일 크게 남았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017776.1.10",
          "form": "헉 문 닫은 곳이 있었나요??",
          "original_form": "헉 문 닫은 곳이 있었나요??",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017776.1.11",
          "form": "전 수산시장에서 회 떠다 먹었어요 ㅋㅋㅋㅋ",
          "original_form": "전 수산시장에서 회 떠다 먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017776.1.12",
          "form": "네ㅋㅋ 돈까스 집이였는데 문이 닫혀있엇어요ㅋㅋㅋ",
          "original_form": "네ㅋㅋ 돈까스 집이였는데 문이 닫혀있엇어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017776.1.13",
          "form": "역시! 바닷가 근처는 회가 최고죠",
          "original_form": "역시! 바닷가 근처는 회가 최고죠",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017776.1.14",
          "form": "아 돈까스집이면 혹시 연돈도 가보셨나요?",
          "original_form": "아 돈까스집이면 혹시 연돈도 가보셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017776.1.15",
          "form": "밀치 처음먹어봤는데 맛있더라구요",
          "original_form": "밀치 처음먹어봤는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017776.1.16",
          "form": "아ㅠ 그때는 없을때였어요ㅜㅜ",
          "original_form": "아ㅠ 그때는 없을때였어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017776.1.17",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017776.1.18",
          "form": "오.. 밀치! 먹어본적이 없어서 궁금해요!",
          "original_form": "오.. 밀치! 먹어본적이 없어서 궁금해요!",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017776.1.19",
          "form": "아뇨 예약방식이나 대기방식이 계속 바뀌는거같아서",
          "original_form": "아뇨 예약방식이나 대기방식이 계속 바뀌는거같아서",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017776.1.20",
          "form": "가야겠다고 생각만 하고 못갔어요 ㅋㅋㅋ",
          "original_form": "가야겠다고 생각만 하고 못갔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017776.1.21",
          "form": "이번 여름에는 어디 가신 곳 있나요?",
          "original_form": "이번 여름에는 어디 가신 곳 있나요?",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017776.1.22",
          "form": "아하ㅋㅋ 저도 가봐야지했는데 못갔어요",
          "original_form": "아하ㅋㅋ 저도 가봐야지했는데 못갔어요",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017776.1.23",
          "form": "이번엔 방콕이였어요ㅜㅜㅜ",
          "original_form": "이번엔 방콕이였어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017776.1.24",
          "form": "앗 그쵸 아무래도 시국이 시국인지라 ㅠ",
          "original_form": "앗 그쵸 아무래도 시국이 시국인지라 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017776.1.25",
          "form": "전 호캉스 짧게 갔다왔는데 방 안에만 있다보니 아쉬웠어요",
          "original_form": "전 호캉스 짧게 갔다왔는데 방 안에만 있다보니 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017776.1.26",
          "form": "네ㅜㅠ 아무래도 나가기 눈치보이더라구요",
          "original_form": "네ㅜㅠ 아무래도 나가기 눈치보이더라구요",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017776.1.27",
          "form": "오오 호캉스! 가보고 싶은데 못가봤어요ㅠ",
          "original_form": "오오 호캉스! 가보고 싶은데 못가봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017776.1.28",
          "form": "얼른 코로나 끝나고 해외여행도 맘껏 갈 수 있으면 좋겠어요",
          "original_form": "얼른 코로나 끝나고 해외여행도 맘껏 갈 수 있으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017776.1.29",
          "form": "그러게요!!",
          "original_form": "그러게요!!",
          "speaker_id": "1",
          "time": "20210826 21:20"
        }
      ]
    }
