      "utterance": [
        {
          "id": "MDRW2100041356.1.1",
          "form": "오늘은 새송이버섯 구워드셨나용",
          "original_form": "오늘은 새송이버섯 구워드셨나용",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100041356.1.2",
          "form": "오늘은 새송이버섯 드셨나용",
          "original_form": "오늘은 새송이버섯 드셨나용",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100041356.1.3",
          "form": "오늘은 그 길죽한 흰 버섯인데",
          "original_form": "오늘은 그 길죽한 흰 버섯인데",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100041356.1.4",
          "form": "이름이 기억이안나네요",
          "original_form": "이름이 기억이안나네요",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100041356.1.5",
          "form": "팽이버섯인가영?",
          "original_form": "팽이버섯인가영?",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100041356.1.6",
          "form": "네네 맞아요!",
          "original_form": "네네 맞아요!",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100041356.1.7",
          "form": "맞나죠 ㅠㅠ",
          "original_form": "맞나죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100041356.1.8",
          "form": "전 오늘도 온라인으로 표고 세일해서 구매했어요 한박스 ㅋㅋㅋ버섯킬러",
          "original_form": "전 오늘도 온라인으로 표고 세일해서 구매했어요 한박스 ㅋㅋㅋ버섯킬러",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100041356.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100041356.1.10",
          "form": "정말 버섯킬러",
          "original_form": "정말 버섯킬러",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100041356.1.11",
          "form": "ㅋㅋㅋㅋ고기보다 더좋아합니당 ..",
          "original_form": "ㅋㅋㅋㅋ고기보다 더좋아합니당 ..",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041356.1.12",
          "form": "해산물류도 좋아하세요?",
          "original_form": "해산물류도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041356.1.13",
          "form": "와 ㅋㅋㅋ 고기보다 좋아하신다니 신기해요",
          "original_form": "와 ㅋㅋㅋ 고기보다 좋아하신다니 신기해요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041356.1.14",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041356.1.15",
          "form": "해산물도 맛있죠",
          "original_form": "해산물도 맛있죠",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041356.1.16",
          "form": "ㅋㅋㅋㅋ예전엔 고기가 훨씬 좋았는데",
          "original_form": "ㅋㅋㅋㅋ예전엔 고기가 훨씬 좋았는데",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041356.1.17",
          "form": "입맛이 확실히 변하네요",
          "original_form": "입맛이 확실히 변하네요",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041356.1.18",
          "form": "아 그쵸 입맛은 항상 변해요",
          "original_form": "아 그쵸 입맛은 항상 변해요",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041356.1.19",
          "form": "어릴땐 다 편식하고 먹는거 별로없었는데",
          "original_form": "어릴땐 다 편식하고 먹는거 별로없었는데",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041356.1.20",
          "form": "지금은 모조리 다먹어요",
          "original_form": "지금은 모조리 다먹어요",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041356.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041356.1.22",
          "form": "맞아요 어릴땐 뜨거운음식먹고 시원하다~하는 의미를 모른다잖아요 ㅋㅋ",
          "original_form": "맞아요 어릴땐 뜨거운음식먹고 시원하다~하는 의미를 모른다잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041356.1.23",
          "form": "그쵸그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
