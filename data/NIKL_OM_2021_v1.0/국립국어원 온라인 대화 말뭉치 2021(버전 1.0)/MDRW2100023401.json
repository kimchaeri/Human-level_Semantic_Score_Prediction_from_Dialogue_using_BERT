      "utterance": [
        {
          "id": "MDRW2100023401.1.1",
          "form": "헉 인절미는 못참죠",
          "original_form": "헉 인절미는 못참죠",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023401.1.2",
          "form": "저는 마켓컬리에서 시켰는데",
          "original_form": "저는 마켓컬리에서 시켰는데",
          "speaker_id": "1",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023401.1.3",
          "form": "ㅠㅠ 진짜좋아하는데",
          "original_form": "ㅠㅠ 진짜좋아하는데",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023401.1.4",
          "form": "꼭 드셔보세용",
          "original_form": "꼭 드셔보세용",
          "speaker_id": "1",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023401.1.5",
          "form": "그냥 실온에서 30분만 자연해동하면",
          "original_form": "그냥 실온에서 30분만 자연해동하면",
          "speaker_id": "1",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023401.1.6",
          "form": "알아서 말랑해져요",
          "original_form": "알아서 말랑해져요",
          "speaker_id": "1",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023401.1.7",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023401.1.8",
          "form": "저지금 메모장에 써놓고왔어요",
          "original_form": "저지금 메모장에 써놓고왔어요",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023401.1.9",
          "form": "ㅎㅎㅎ 입맛에 맞으셨으면 좋겠네용ㅎㅎ",
          "original_form": "ㅎㅎㅎ 입맛에 맞으셨으면 좋겠네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023401.1.10",
          "form": "무슨 음식 제일 좋아하세용?",
          "original_form": "무슨 음식 제일 좋아하세용?",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023401.1.11",
          "form": "소울푸드같은거",
          "original_form": "소울푸드같은거",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023401.1.12",
          "form": "와 비쥬얼 장난 아니네요 말랑말랑 진짜 맛있겠다",
          "original_form": "와 비쥬얼 장난 아니네요 말랑말랑 진짜 맛있겠다",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023401.1.13",
          "form": "그쳐ㅋㅋㅋㅋ",
          "original_form": "그쳐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023401.1.14",
          "form": "딱 저렇게 와요",
          "original_form": "딱 저렇게 와요",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023401.1.15",
          "form": "음.. 소울푸드.. 어렵네여.. 밥?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "음.. 소울푸드.. 어렵네여.. 밥?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.16",
          "form": "아ㅋㅋㅋ 저도 국밥이용",
          "original_form": "아ㅋㅋㅋ 저도 국밥이용",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.17",
          "form": "콩나물국밥",
          "original_form": "콩나물국밥",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.19",
          "form": "뜨끈한거... 육개장이랑...",
          "original_form": "뜨끈한거... 육개장이랑...",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.20",
          "form": "저는순대국밥인거가타여",
          "original_form": "저는순대국밥인거가타여",
          "speaker_id": "2",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.21",
          "form": "캬....",
          "original_form": "캬....",
          "speaker_id": "2",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.22",
          "form": "침고이네여..",
          "original_form": "침고이네여..",
          "speaker_id": "2",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023401.1.23",
          "form": "우왕ㅋㅋㅋ 맛잘알이시네요ㅋㅋㅋ",
          "original_form": "우왕ㅋㅋㅋ 맛잘알이시네요ㅋㅋㅋ",
