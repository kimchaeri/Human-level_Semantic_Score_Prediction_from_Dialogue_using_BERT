        {
          "id": "MDRW2100003166.1.1",
          "form": "아 맞다 나 최근에 간장 인터넷쇼핑했어",
          "original_form": "아 맞다 나 최근에 간장 인터넷쇼핑했어",
          "speaker_id": "2",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003166.1.2",
          "form": "간장?",
          "original_form": "간장?",
          "speaker_id": "1",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003166.1.3",
          "form": "간장은 왜?",
          "original_form": "간장은 왜?",
          "speaker_id": "1",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003166.1.4",
          "form": "간장이 세일하더라고",
          "original_form": "간장이 세일하더라고",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003166.1.5",
          "form": "얼마길래",
          "original_form": "얼마길래",
          "speaker_id": "1",
          "time": "20210820 20:01"
        },
        {
          "id": "MDRW2100003166.1.6",
          "form": "1.8리터 500미리 두개에 8천원 정도",
          "original_form": "1.8리터 500미리 두개에 8천원 정도",
          "speaker_id": "2",
          "time": "20210820 20:01"
        },
        {
          "id": "MDRW2100003166.1.7",
          "form": "저렴한거야?",
          "original_form": "저렴한거야?",
          "speaker_id": "1",
          "time": "20210820 20:01"
        },
        {
          "id": "MDRW2100003166.1.8",
          "form": "응 엄청 저렴한 고",
          "original_form": "응 엄청 저렴한 고",
          "speaker_id": "2",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003166.1.9",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003166.1.10",
          "form": "간장도 인터넷으로 사구나",
          "original_form": "간장도 인터넷으로 사구나",
          "speaker_id": "1",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003166.1.11",
          "form": "이게 701이라고",
          "original_form": "이게 701이라고",
          "speaker_id": "2",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003166.1.12",
          "form": "합성 뭐가 없는 건데",
          "original_form": "합성 뭐가 없는 건데",
          "speaker_id": "2",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003166.1.13",
          "form": "아 알아 비싼 간장!",
          "original_form": "아 알아 비싼 간장!",
          "speaker_id": "1",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003166.1.14",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003166.1.15",
          "form": "오 701 샀구나",
          "original_form": "오 701 샀구나",
          "speaker_id": "1",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003166.1.16",
          "form": "일반 슈퍼에서는 17000원이래",
          "original_form": "일반 슈퍼에서는 17000원이래",
          "speaker_id": "2",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003166.1.17",
          "form": "헉 그렇게 비쌌어?",
          "original_form": "헉 그렇게 비쌌어?",
          "speaker_id": "1",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003166.1.18",
          "form": "그렇다더라",
          "original_form": "그렇다더라",
          "speaker_id": "2",
          "time": "20210820 20:03"
        }
      ]
