      "utterance": [
        {
          "id": "MDRW2100037406.1.1",
          "form": "아니 너무 짜서 싫어해",
          "original_form": "아니 너무 짜서 싫어해",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037406.1.2",
          "form": "난 좋아하는데 요즘 건강이 걱정돼",
          "original_form": "난 좋아하는데 요즘 건강이 걱정돼",
          "speaker_id": "1",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037406.1.3",
          "form": "물을 많이 마셔",
          "original_form": "물을 많이 마셔",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037406.1.4",
          "form": "짠 건 몸에 안좋아",
          "original_form": "짠 건 몸에 안좋아",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037406.1.5",
          "form": "넌 그럼 국물도 잘 안먹니?",
          "original_form": "넌 그럼 국물도 잘 안먹니?",
          "speaker_id": "1",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037406.1.6",
          "form": "국을 데울 때에도 물좀붓고 데워야해",
          "original_form": "국을 데울 때에도 물좀붓고 데워야해",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.7",
          "form": "근데 좋아하는건 먹어",
          "original_form": "근데 좋아하는건 먹어",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.8",
          "form": "그러게 물도 자주 마셔야하는데",
          "original_form": "그러게 물도 자주 마셔야하는데",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.9",
          "form": "감자탕이런건 맛있오",
          "original_form": "감자탕이런건 맛있오",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.10",
          "form": "나는 물마시기가 너무 귀찮아",
          "original_form": "나는 물마시기가 너무 귀찮아",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.11",
          "form": "근데 화장실가기가 귀찮아서",
          "original_form": "근데 화장실가기가 귀찮아서",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.12",
          "form": "너는 하루에 얼마나마셔?",
          "original_form": "너는 하루에 얼마나마셔?",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.13",
          "form": "물도 귀찮긴해",
          "original_form": "물도 귀찮긴해",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.14",
          "form": "나는 물은 밥먹을때 마쇼",
          "original_form": "나는 물은 밥먹을때 마쇼",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.15",
          "form": "자기전에도 마시고",
          "original_form": "자기전에도 마시고",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.16",
          "form": "주로 탄산이나 커피를 마셔",
          "original_form": "주로 탄산이나 커피를 마셔",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.17",
          "form": "맞아 나도 그때만 마셔",
          "original_form": "맞아 나도 그때만 마셔",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.18",
          "form": "사실 지금도 목마른데 참고있어",
          "original_form": "사실 지금도 목마른데 참고있어",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.19",
          "form": "아 탄산은 탄산수를 말하는거야",
          "original_form": "아 탄산은 탄산수를 말하는거야",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.20",
          "form": "요즘 탄산음료들은 너무 달아",
          "original_form": "요즘 탄산음료들은 너무 달아",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.21",
          "form": "참지마ㅠ",
          "original_form": "참지마ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.22",
          "form": "탄산수 맛있니???",
          "original_form": "탄산수 맛있니???",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037406.1.23",
          "form": "너는 머리가 많이 안빠지니?",
          "original_form": "너는 머리가 많이 안빠지니?",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100037406.1.24",
          "form": "나는 요즘 머리카락이 너무 많이 빠져",
          "original_form": "나는 요즘 머리카락이 너무 많이 빠져",
          "speaker_id": "1",
          "time": "20210901 23:09"
        }
      ]
