      "utterance": [
        {
          "id": "MDRW2100043785.1.1",
          "form": "목이 마른데 팩 붙이고 있어서 못마시네요",
          "original_form": "목이 마른데 팩 붙이고 있어서 못마시네요",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043785.1.2",
          "form": "아 오늘 쭈꾸미 볶음밥 맛있었다",
          "original_form": "아 오늘 쭈꾸미 볶음밥 맛있었다",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100043785.1.3",
          "form": "응 맛있더라",
          "original_form": "응 맛있더라",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043785.1.4",
          "form": "도시락도 좋았어",
          "original_form": "도시락도 좋았어",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043785.1.5",
          "form": "그치 내일모레 또 올테니까",
          "original_form": "그치 내일모레 또 올테니까",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043785.1.6",
          "form": "또 맛있게 먹자",
          "original_form": "또 맛있게 먹자",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043785.1.7",
          "form": "치킨아라비아따 세트랑",
          "original_form": "치킨아라비아따 세트랑",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043785.1.8",
          "form": "또 뭐더라 암튼 맛있는거 온당",
          "original_form": "또 뭐더라 암튼 맛있는거 온당",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043785.1.9",
          "form": "응응 기대된다",
          "original_form": "응응 기대된다",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100043785.1.10",
          "form": "새 도시락도",
          "original_form": "새 도시락도",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100043785.1.11",
          "form": "내일도 맛있는 것 먹고 행복하자",
          "original_form": "내일도 맛있는 것 먹고 행복하자",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043785.1.12",
          "form": "낼 아침은 뭐지",
          "original_form": "낼 아침은 뭐지",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043785.1.13",
          "form": "코울슬로",
          "original_form": "코울슬로",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043785.1.14",
          "form": "코울슬로인가",
          "original_form": "코울슬로인가",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043785.1.15",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043785.1.16",
          "form": "너 내일 점심메뉴는 뭐야?",
          "original_form": "너 내일 점심메뉴는 뭐야?",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043785.1.17",
          "form": "알아?",
          "original_form": "알아?",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043785.1.18",
          "form": "맛있는거면 좋겠구멈",
          "original_form": "맛있는거면 좋겠구멈",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100043785.1.19",
          "form": "나 점심 메뉴는 양념치킨",
          "original_form": "나 점심 메뉴는 양념치킨",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100043785.1.20",
          "form": "수욜은 특식이야",
          "original_form": "수욜은 특식이야",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100043785.1.21",
          "form": "오오오오오오",
          "original_form": "오오오오오오",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100043785.1.22",
          "form": "양념치킨 좋네",
          "original_form": "양념치킨 좋네",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100043785.1.23",
          "form": "우리도 집에서 짜치키 만들어서 양념치킨 먹어야지",
          "original_form": "우리도 집에서 짜치키 만들어서 양념치킨 먹어야지",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100043785.1.24",
          "form": "ㅋㅋㅋ그래",
          "original_form": "ㅋㅋㅋ그래",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100043785.1.25",
          "form": "오이 사야겠다",
          "original_form": "오이 사야겠다",
          "speaker_id": "2",
          "time": "20210908 00:08"
        }
      ]
