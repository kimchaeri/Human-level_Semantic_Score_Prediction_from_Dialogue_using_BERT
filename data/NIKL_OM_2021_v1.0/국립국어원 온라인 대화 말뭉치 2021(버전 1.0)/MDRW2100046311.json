      "utterance": [
        {
          "id": "MDRW2100046311.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.3",
          "form": "술 좋아하세용?",
          "original_form": "술 좋아하세용?",
          "speaker_id": "2",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.4",
          "form": "술 즐겨하는 편입니다!",
          "original_form": "술 즐겨하는 편입니다!",
          "speaker_id": "1",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.5",
          "form": "오 ㅎㅎ",
          "original_form": "오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.6",
          "form": "뭔가 반갑",
          "original_form": "뭔가 반갑",
          "speaker_id": "2",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.7",
          "form": "가장 즐겨 마시는",
          "original_form": "가장 즐겨 마시는",
          "speaker_id": "2",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.8",
          "form": "주종이 뭔가용",
          "original_form": "주종이 뭔가용",
          "speaker_id": "2",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.9",
          "form": "님도 술 좋아하세요?",
          "original_form": "님도 술 좋아하세요?",
          "speaker_id": "1",
          "time": "20211019 16:46"
        },
        {
          "id": "MDRW2100046311.1.10",
          "form": "넹 크크 저는 소맥파입니다",
          "original_form": "넹 크크 저는 소맥파입니다",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.11",
          "form": "저는 위스키랑 진 좋아합니다! 사실 다 좋아해요",
          "original_form": "저는 위스키랑 진 좋아합니다! 사실 다 좋아해요",
          "speaker_id": "1",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.13",
          "form": "우와 저는 아직 위스키맛을 잘 모르는 사람으로써",
          "original_form": "우와 저는 아직 위스키맛을 잘 모르는 사람으로써",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.14",
          "form": "ㅋㅋㅋㅋㅋ뭔가 으른같으시네요...",
          "original_form": "ㅋㅋㅋㅋㅋ뭔가 으른같으시네요...",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.16",
          "form": "술은 다..",
          "original_form": "술은 다..",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.17",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.18",
          "form": "캬캬",
          "original_form": "캬캬",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.19",
          "form": "으른! 사실 저도 친구들이랑 의기투합해서 아주 가끔 마신답니다 ㅋㅋ",
          "original_form": "으른! 사실 저도 친구들이랑 의기투합해서 아주 가끔 마신답니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.20",
          "form": "ㅎㅎㅎㅎ가장",
          "original_form": "ㅎㅎㅎㅎ가장",
          "speaker_id": "2",
          "time": "20211019 16:47"
        },
        {
          "id": "MDRW2100046311.1.21",
          "form": "좋아하는 술안주있으세요?",
          "original_form": "좋아하는 술안주있으세요?",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.22",
          "form": "저에게는 술이 최고의 안주랍니다 ㅋㅋㅋ",
          "original_form": "저에게는 술이 최고의 안주랍니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.23",
          "form": "고수의 냄새가 난다",
          "original_form": "고수의 냄새가 난다",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.25",
          "form": "아니 물이요!",
          "original_form": "아니 물이요!",
          "speaker_id": "1",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.26",
          "form": "물이요?",
          "original_form": "물이요?",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.27",
          "form": "아아 ㅋㅋㅋㅋㅋㅋㅋ 너무 술꾼처럼 오타났네요",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋㅋ 너무 술꾼처럼 오타났네요",
          "speaker_id": "1",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.28",
          "form": "아 물이 최고의 안주 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 물이 최고의 안주 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.29",
          "form": "물이 최고의 안주라는게",
          "original_form": "물이 최고의 안주라는게",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.30",
          "form": "네네 물이요!",
          "original_form": "네네 물이요!",
          "speaker_id": "1",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.31",
          "form": "더 술꾼같은데요?",
          "original_form": "더 술꾼같은데요?",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 16:48"
        },
        {
          "id": "MDRW2100046311.1.33",
          "form": "컥ㅋㅋㅋㅋㅋㅋㅋㅋ 들켰네요",
          "original_form": "컥ㅋㅋㅋㅋㅋㅋㅋㅋ 들켰네요",
          "speaker_id": "1",
          "time": "20211019 16:49"
        },
        {
          "id": "MDRW2100046311.1.34",
          "form": "크크크 건강챙기면서 즐기자구요 하핫\r\n",
          "original_form": "크크크 건강챙기면서 즐기자구요 하핫\r\n",
