        },
        {
          "id": "MDRW2100045422.1.2",
          "form": "주제가ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "주제가ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045422.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045422.1.4",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045422.1.5",
          "form": "내 친구중에",
          "original_form": "내 친구중에",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045422.1.6",
          "form": "역류성 식도염 잇는 애가 잇는데",
          "original_form": "역류성 식도염 잇는 애가 잇는데",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045422.1.7",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045422.1.8",
          "form": "정확히 똑같은 소리를 했거든 ㅋㅋㅋㅋ",
          "original_form": "정확히 똑같은 소리를 했거든 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045422.1.9",
          "form": "아닠",
          "original_form": "아닠",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045422.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045422.1.11",
          "form": "왼쪽으로 돌아누워자면",
          "original_form": "왼쪽으로 돌아누워자면",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045422.1.12",
          "form": "좀 괜찮다던데",
          "original_form": "좀 괜찮다던데",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045422.1.13",
          "form": "그거 위가 그쪽방향이라고 한거같긴 한데",
          "original_form": "그거 위가 그쪽방향이라고 한거같긴 한데",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045422.1.14",
          "form": "자면서 어케 뒤척일줄 알고...",
          "original_form": "자면서 어케 뒤척일줄 알고...",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045422.1.15",
          "form": "ㅇㅇ근데 난 좀",
          "original_form": "ㅇㅇ근데 난 좀",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045422.1.16",
          "form": "신경쓰긴 해야돼",
          "original_form": "신경쓰긴 해야돼",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045422.1.17",
          "form": "올해 상반기에 내가",
          "original_form": "올해 상반기에 내가",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045422.1.18",
          "form": "쭉 소화불량에 시달렸거든?",
          "original_form": "쭉 소화불량에 시달렸거든?",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045422.1.19",
          "form": "그게 역류성 식도염때문이엇대....",
          "original_form": "그게 역류성 식도염때문이엇대....",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045422.1.20",
          "form": "아 둘이 관련이 잇대?",
          "original_form": "아 둘이 관련이 잇대?",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045422.1.21",
          "form": "ㅇㅇ그런다네",
          "original_form": "ㅇㅇ그런다네",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045422.1.22",
          "form": "건강검진하러갓는데",
          "original_form": "건강검진하러갓는데",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045422.1.23",
          "form": "의사가 역류성식도염대문이란거야",
          "original_form": "의사가 역류성식도염대문이란거야",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045422.1.24",
          "form": "난 겁나 의문이엇거든",
          "original_form": "난 겁나 의문이엇거든",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.25",
          "form": "식도에 염증 좀 잇다고 소화가 안되나?",
          "original_form": "식도에 염증 좀 잇다고 소화가 안되나?",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.26",
          "form": "근데 그런 질문 하니까",
          "original_form": "근데 그런 질문 하니까",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.27",
          "form": "그러게 ㅋㅋㅋㅋㅋ",
          "original_form": "그러게 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.28",
          "form": "진짜 좀 뜬금없어보이는데...",
          "original_form": "진짜 좀 뜬금없어보이는데...",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.29",
          "form": "한심하고 건강관리 안하는 환자를 보는 눈빛으로",
          "original_form": "한심하고 건강관리 안하는 환자를 보는 눈빛으로",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.31",
          "form": "햇어",
          "original_form": "햇어",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045422.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 의사가??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 의사가??",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045422.1.33",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
