      "utterance": [
        {
          "id": "MDRW2100026825.1.1",
          "form": "우왕 다 제가 먹어보고 싶었던 것들이에요ㅋㅋㅋㅋ",
          "original_form": "우왕 다 제가 먹어보고 싶었던 것들이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026825.1.2",
          "form": "위 괜찮으신건가요",
          "original_form": "위 괜찮으신건가요",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026825.1.3",
          "form": "ㅋㅋㅋㅋㅋ매운짬뽕은 정말 너무 맵구요.... 그건 못먹어요 써요 써",
          "original_form": "ㅋㅋㅋㅋㅋ매운짬뽕은 정말 너무 맵구요.... 그건 못먹어요 써요 써",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026825.1.4",
          "form": "나머지는 담날 심판을 받긴하지만 그래도 맛있으니까..ㅎ",
          "original_form": "나머지는 담날 심판을 받긴하지만 그래도 맛있으니까..ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026825.1.5",
          "form": "아 저 그 매운 돈까스도 먹어봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 저 그 매운 돈까스도 먹어봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026825.1.6",
          "form": "ㅋㅋㅋ위가 강철이신가봐요!!",
          "original_form": "ㅋㅋㅋ위가 강철이신가봐요!!",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026825.1.7",
          "form": "와 디진다돈까스요??",
          "original_form": "와 디진다돈까스요??",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026825.1.8",
          "form": "유튭 엄청봤는데",
          "original_form": "유튭 엄청봤는데",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026825.1.9",
          "form": "저 매운거 진짜 조아하네욬ㅋㅋㅋ 말하고보니까",
          "original_form": "저 매운거 진짜 조아하네욬ㅋㅋㅋ 말하고보니까",
          "speaker_id": "1",
