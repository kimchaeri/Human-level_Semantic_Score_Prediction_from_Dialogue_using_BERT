      "utterance": [
        {
          "id": "MDRW2100020657.1.1",
          "form": "어떤게임 좋아하세요?",
          "original_form": "어떤게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020657.1.2",
          "form": "저 로스트아크 좋아해요",
          "original_form": "저 로스트아크 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020657.1.3",
          "form": "헐 그거 재밌나요?애기만 많이 들어서요!",
          "original_form": "헐 그거 재밌나요?애기만 많이 들어서요!",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020657.1.4",
          "form": "얘기만..",
          "original_form": "얘기만..",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020657.1.5",
          "form": "호불호 갈리는데",
          "original_form": "호불호 갈리는데",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020657.1.6",
          "form": "저는 되게 재밌어요!",
          "original_form": "저는 되게 재밌어요!",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020657.1.7",
          "form": "현질한 최초의 게임인데",
          "original_form": "현질한 최초의 게임인데",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020657.1.8",
          "form": "새로나온 아바타가 예뻐서 구매한정도???",
          "original_form": "새로나온 아바타가 예뻐서 구매한정도???",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020657.1.9",
          "form": "직업도 많아서 재밌어요!!!!",
          "original_form": "직업도 많아서 재밌어요!!!!",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020657.1.10",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020657.1.11",
          "form": "저는 쿠키런 진짜 열심히 했었거든요!!",
          "original_form": "저는 쿠키런 진짜 열심히 했었거든요!!",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020657.1.12",
          "form": "그 쿠키 뽑아보겠다고 현질했었다니까요,,",
          "original_form": "그 쿠키 뽑아보겠다고 현질했었다니까요,,",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020657.1.13",
          "form": "저도 쿠키런 했었어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 쿠키런 했었어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020657.1.14",
          "form": "근데 너무 약해서 포기했어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 너무 약해서 포기했어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020657.1.15",
          "form": "나중에 시작해서 그런가 샌드백된 느낌이였어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나중에 시작해서 그런가 샌드백된 느낌이였어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020657.1.16",
          "form": "아놔 맞아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아놔 맞아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020657.1.17",
          "form": "초기부터 한 거랑은 다르죠... 저도 그랬습니다....",
          "original_form": "초기부터 한 거랑은 다르죠... 저도 그랬습니다....",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020657.1.18",
          "form": "현질 버프로 남들만큼은 아니지만 반은 올려보려고 노력했답니ㅏㄷ...",
          "original_form": "현질 버프로 남들만큼은 아니지만 반은 올려보려고 노력했답니ㅏㄷ...",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020657.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020657.1.20",
          "form": "저는 아무리 뽑아도 바닐라? 그 레전드 쿠키가 안나와서 접었어요 ㅠ",
          "original_form": "저는 아무리 뽑아도 바닐라? 그 레전드 쿠키가 안나와서 접었어요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100020657.1.21",
          "form": "기간이 부족해서 1회 확정도 못뽑길래 포기하게되더라구요",
          "original_form": "기간이 부족해서 1회 확정도 못뽑길래 포기하게되더라구요",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100020657.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020657.1.23",
          "form": "저도 저도 래전드 쿠키 안나와서 접었어요ㅋㅋㅋㅋㅋㅋㅋ현질을 해도 못 뽑는...",
          "original_form": "저도 저도 래전드 쿠키 안나와서 접었어요ㅋㅋㅋㅋㅋㅋㅋ현질을 해도 못 뽑는...",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020657.1.24",
          "form": "ㅠㅠ..",
          "original_form": "ㅠㅠ..",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020657.1.25",
          "form": "확률 너무 안좋죠 쿠키런..",
          "original_form": "확률 너무 안좋죠 쿠키런..",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100020657.1.26",
          "form": "쿠키런 진짜 확률 안좋아요 ㅠ",
          "original_form": "쿠키런 진짜 확률 안좋아요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100020657.1.27",
          "form": "제가 다른 모바일게임해서 쿠키런 좀 미련없이 접었던 것 같아요",
          "original_form": "제가 다른 모바일게임해서 쿠키런 좀 미련없이 접었던 것 같아요",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100020657.1.28",
          "form": "ㅋㅋㅋㅋ 저도요! 저는 계정도 날렸답니다...",
          "original_form": "ㅋㅋㅋㅋ 저도요! 저는 계정도 날렸답니다...",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100020657.1.29",
          "form": "근데도 후회가 안되더라고요!",
          "original_form": "근데도 후회가 안되더라고요!",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100020657.1.30",
          "form": "앗... 계정을 날리셨다니 ㅠㅠㅠ",
          "original_form": "앗... 계정을 날리셨다니 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100020657.1.31",
          "form": "그런데 후회는 안되더라고욬ㅋㅋㅋㅋ 삭제했는데도 그냥 그랬어요!ㅋㅋㅋ",
          "original_form": "그런데 후회는 안되더라고욬ㅋㅋㅋㅋ 삭제했는데도 그냥 그랬어요!ㅋㅋㅋ",
