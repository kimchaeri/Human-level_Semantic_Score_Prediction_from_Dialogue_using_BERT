      "utterance": [
        {
          "id": "MDRW2100004965.1.1",
          "form": "강철의 연금술사 재밌더라고요!",
          "original_form": "강철의 연금술사 재밌더라고요!",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004965.1.2",
          "form": "유명한 작품이죠!",
          "original_form": "유명한 작품이죠!",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004965.1.3",
          "form": "맨날 미루다가 아직 못 봤는데 다음에 한 번 봐야겠어요",
          "original_form": "맨날 미루다가 아직 못 봤는데 다음에 한 번 봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004965.1.4",
          "form": "저는 요즘 라프텔로 애니 보고 있어요",
          "original_form": "저는 요즘 라프텔로 애니 보고 있어요",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004965.1.5",
          "form": "라프텔 아시나요?",
          "original_form": "라프텔 아시나요?",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004965.1.6",
          "form": "네!! 라프텔 당근 알죠!",
          "original_form": "네!! 라프텔 당근 알죠!",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004965.1.7",
          "form": "라프텔로도 애니 봤었는데 요즘엔 애니 자체를 잘 안보게됐네요...",
          "original_form": "라프텔로도 애니 봤었는데 요즘엔 애니 자체를 잘 안보게됐네요...",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004965.1.8",
          "form": "쓰르라미울적에, 조커게임 등...",
          "original_form": "쓰르라미울적에, 조커게임 등...",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004965.1.9",
          "form": "저는 이번에 올림픽 보고 스포츠 땡겨서",
          "original_form": "저는 이번에 올림픽 보고 스포츠 땡겨서",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004965.1.10",
          "form": "하이큐랑 쿠로코의 농구 봤어요ㅋㅋㅋ",
          "original_form": "하이큐랑 쿠로코의 농구 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004965.1.11",
          "form": "와!!! 하이큐!! 너무너무 재밌죠",
          "original_form": "와!!! 하이큐!! 너무너무 재밌죠",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004965.1.12",
          "form": "캐릭터도 다 매력적이고",
          "original_form": "캐릭터도 다 매력적이고",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004965.1.13",
          "form": "배구가 재밌는 스포츠인것도 알게되고",
          "original_form": "배구가 재밌는 스포츠인것도 알게되고",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004965.1.14",
          "form": "옛날에 하이큐 한창 유행할 때는 그림체가 취향이 아니라 안 봤었는데",
          "original_form": "옛날에 하이큐 한창 유행할 때는 그림체가 취향이 아니라 안 봤었는데",
          "speaker_id": "1",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004965.1.15",
          "form": "이번에 보니까 너무 재밌더라고요",
          "original_form": "이번에 보니까 너무 재밌더라고요",
          "speaker_id": "1",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004965.1.16",
          "form": "캐릭터 스토리 작화 부족할게 없는 작품이죠!!!",
          "original_form": "캐릭터 스토리 작화 부족할게 없는 작품이죠!!!",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004965.1.17",
          "form": "강철의 연금술사는 어때요?",
          "original_form": "강철의 연금술사는 어때요?",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004965.1.18",
          "form": "판타지인가요?",
          "original_form": "판타지인가요?",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004965.1.19",
          "form": "판타지맞아요!",
          "original_form": "판타지맞아요!",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004965.1.20",
          "form": "줄거리 아세요?",
          "original_form": "줄거리 아세요?",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004965.1.21",
          "form": "아직 한 번도 안 봐서 잘 몰라요",
          "original_form": "아직 한 번도 안 봐서 잘 몰라요",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004965.1.22",
          "form": "소개해주실래요?",
          "original_form": "소개해주실래요?",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004965.1.23",
          "form": "형제가 죽은 어머니를 살리려고 금지된 연금술을 쓰다가 둘 다 몸을 잃어버려요. 그래서 몸을 되찾기 위해 고군분투하는 이야기예요",
          "original_form": "형제가 죽은 어머니를 살리려고 금지된 연금술을 쓰다가 둘 다 몸을 잃어버려요. 그래서 몸을 되찾기 위해 고군분투하는 이야기예요",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004965.1.24",
          "form": "오 소재가 재밌네요",
          "original_form": "오 소재가 재밌네요",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004965.1.25",
          "form": "추천 감사해요!",
          "original_form": "추천 감사해요!",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004965.1.26",
          "form": "저는 헌터×헌터 진짜 재밌게 봤어요",
          "original_form": "저는 헌터×헌터 진짜 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004965.1.27",
          "form": "판타지+모험인데 세계관이 진짜 튼튼하고 작품도 재밌어요",
          "original_form": "판타지+모험인데 세계관이 진짜 튼튼하고 작품도 재밌어요",
          "speaker_id": "1",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100004965.1.28",
          "form": "헌터헌터 진짜 유명하더라고요!",
          "original_form": "헌터헌터 진짜 유명하더라고요!",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004965.1.29",
          "form": "캐릭터도 매력적이라 한번 봐보고 싶은데 연재가 잘 안되는모양이라 주저하게돼요...ㅋㅋ",
          "original_form": "캐릭터도 매력적이라 한번 봐보고 싶은데 연재가 잘 안되는모양이라 주저하게돼요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004965.1.30",
          "form": "에피소트 형식이라서 애니는 완결 났어요!",
          "original_form": "에피소트 형식이라서 애니는 완결 났어요!",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004965.1.31",
          "form": "약간 드레곤볼 처럼 점점 세계관 키우면서 에피소트 진행해서",
          "original_form": "약간 드레곤볼 처럼 점점 세계관 키우면서 에피소트 진행해서",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004965.1.32",
          "form": "애니만 보시면 재밌게 보실 수 있어요..ㅎㅎ",
          "original_form": "애니만 보시면 재밌게 보실 수 있어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:56"
        },
        {
          "id": "MDRW2100004965.1.33",
          "form": "아 그래요? 꼭 봐야겠어요",
          "original_form": "아 그래요? 꼭 봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 19:57"
        }
      ]
    }
