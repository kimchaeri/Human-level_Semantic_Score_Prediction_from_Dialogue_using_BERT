      "utterance": [
        {
          "id": "MDRW2100042885.1.1",
          "form": "헉 그럼 우째 그렇다면",
          "original_form": "헉 그럼 우째 그렇다면",
          "speaker_id": "2",
          "time": "20210907 11:15"
        },
        {
          "id": "MDRW2100042885.1.2",
          "form": "생귤탱귤 ?",
          "original_form": "생귤탱귤 ?",
          "speaker_id": "2",
          "time": "20210907 11:15"
        },
        {
          "id": "MDRW2100042885.1.3",
          "form": "ㅋㅋㅋ상큼하고 좋겠네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ상큼하고 좋겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100042885.1.4",
          "form": "그래도 역시 일등은 초코퍼지랄까..? 이등은 빠삐꼬..?",
          "original_form": "그래도 역시 일등은 초코퍼지랄까..? 이등은 빠삐꼬..?",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100042885.1.5",
          "form": "오 초코파세요?",
          "original_form": "오 초코파세요?",
          "speaker_id": "2",
          "time": "20210907 11:18"
        },
        {
          "id": "MDRW2100042885.1.6",
          "form": "네 초코사랑러에뇨",
          "original_form": "네 초코사랑러에뇨",
          "speaker_id": "1",
          "time": "20210907 11:18"
        },
        {
          "id": "MDRW2100042885.1.7",
          "form": "허쉬는 어떠심 ?",
          "original_form": "허쉬는 어떠심 ?",
          "speaker_id": "2",
          "time": "20210907 11:20"
        },
        {
          "id": "MDRW2100042885.1.8",
          "form": "아 한때 빠졌었죠 허쉬콘에 ㅋㅋㅋㅋ미쳤엇죠",
          "original_form": "아 한때 빠졌었죠 허쉬콘에 ㅋㅋㅋㅋ미쳤엇죠",
          "speaker_id": "1",
          "time": "20210907 11:23"
        },
        {
          "id": "MDRW2100042885.1.9",
          "form": "2+1사서 3개 한벙에 다먹고...",
          "original_form": "2+1사서 3개 한벙에 다먹고...",
          "speaker_id": "1",
          "time": "20210907 11:23"
        },
        {
          "id": "MDRW2100042885.1.10",
          "form": "크으 그죠 허쉬는 역시 콘이쥬",
          "original_form": "크으 그죠 허쉬는 역시 콘이쥬",
          "speaker_id": "2",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042885.1.11",
          "form": "ㅋㅋㅋㅋㅋ 3허쉬",
          "original_form": "ㅋㅋㅋㅋㅋ 3허쉬",
          "speaker_id": "2",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042885.1.12",
          "form": "초코퍼지 근디여즘.보기어렵",
          "original_form": "초코퍼지 근디여즘.보기어렵",
          "speaker_id": "2",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042885.1.13",
          "form": "ㅋㅋ헐 그래요? 그래도 우리회사 매점에있어서 먹구잇어여 ㅋㅋ",
          "original_form": "ㅋㅋ헐 그래요? 그래도 우리회사 매점에있어서 먹구잇어여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:42"
        },
        {
          "id": "MDRW2100042885.1.14",
          "form": "초코퍼지 쫀득 미쳤는뎅 ㅜ",
          "original_form": "초코퍼지 쫀득 미쳤는뎅 ㅜ",
          "speaker_id": "1",
          "time": "20210907 11:42"
        },
        {
          "id": "MDRW2100042885.1.15",
          "form": "훙 매점이잇으시그나 조켓다...",
          "original_form": "훙 매점이잇으시그나 조켓다...",
          "speaker_id": "2",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100042885.1.16",
          "form": "옛날에 정말 마니먹앗죠",
          "original_form": "옛날에 정말 마니먹앗죠",
          "speaker_id": "2",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100042885.1.17",
          "form": "매점",
          "original_form": "매점",
          "speaker_id": "2",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100042885.1.18",
          "form": "ㅋㅋㅋ맞아요 저 매점사랑해요",
          "original_form": "ㅋㅋㅋ맞아요 저 매점사랑해요",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100042885.1.19",
          "form": "문제는 지금도 사랑한다는거죠",
          "original_form": "문제는 지금도 사랑한다는거죠",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100042885.1.20",
          "form": "사랑은 변하지 않거등요",
          "original_form": "사랑은 변하지 않거등요",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100042885.1.21",
          "form": "저랑 매점 가실 ?",
          "original_form": "저랑 매점 가실 ?",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100042885.1.22",
          "form": "당근 콜이죠",
          "original_form": "당근 콜이죠",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100042885.1.23",
          "form": "머고르실 ?",
          "original_form": "머고르실 ?",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042885.1.24",
          "form": "초퍼 하나주세용",
          "original_form": "초퍼 하나주세용",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042885.1.25",
          "form": "전 생탱이요",
          "original_form": "전 생탱이요",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100042885.1.26",
          "form": "과잔요?",
          "original_form": "과잔요?",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100042885.1.27",
          "form": "초코송이요",
          "original_form": "초코송이요",
          "speaker_id": "1",
          "time": "20210907 18:11"
        }
      ]
    }
