      "utterance": [
        {
          "id": "MDRW2100008143.1.1",
          "form": "여름휴가는 다녀오셨어요?",
          "original_form": "여름휴가는 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008143.1.2",
          "form": "직장에서 휴가를 다같이 쉬고가는데",
          "original_form": "직장에서 휴가를 다같이 쉬고가는데",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008143.1.3",
          "form": "휴가 첫날 문제가생겨서 저만 출근했었어요 ㅠ ㅠ",
          "original_form": "휴가 첫날 문제가생겨서 저만 출근했었어요 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008143.1.4",
          "form": "그덕에 하루 송도 아는사람집가서 자고온게 다에요",
          "original_form": "그덕에 하루 송도 아는사람집가서 자고온게 다에요",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008143.1.5",
          "form": "잘보내셨나요?",
          "original_form": "잘보내셨나요?",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008143.1.6",
          "form": "송도면 부산인가요?",
          "original_form": "송도면 부산인가요?",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008143.1.7",
          "form": "에고 출근하셔서 속상하셨겠어요",
          "original_form": "에고 출근하셔서 속상하셨겠어요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008143.1.8",
          "form": "아 인천 송도에요! ㅎㅎㅎ",
          "original_form": "아 인천 송도에요! ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008143.1.9",
          "form": "대신에 돈을 좀 타냈죠",
          "original_form": "대신에 돈을 좀 타냈죠",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008143.1.10",
          "form": "전  연차쓰고 가야돼서 아직 휴가계획을 못세웠어요",
          "original_form": "전  연차쓰고 가야돼서 아직 휴가계획을 못세웠어요",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008143.1.11",
          "form": "여유있기일어나서 커피한잔하면서 산책하고 야경구경하구",
          "original_form": "여유있기일어나서 커피한잔하면서 산책하고 야경구경하구",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008143.1.12",
          "form": "오 ㅎㅎ 어디로 갈생각이세요?",
          "original_form": "오 ㅎㅎ 어디로 갈생각이세요?",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008143.1.13",
          "form": "코로나때문에 영..",
          "original_form": "코로나때문에 영..",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008143.1.14",
          "form": "아 인천 송도에 휴가보낼만한 곳이있나요?",
          "original_form": "아 인천 송도에 휴가보낼만한 곳이있나요?",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008143.1.15",
          "form": "앗 그런건 아니에요 그냥 아는 언니가 이번에 이사해서",
          "original_form": "앗 그런건 아니에요 그냥 아는 언니가 이번에 이사해서",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008143.1.16",
          "form": "저도 코로나땜에 아무데도 못가겠더라구요",
          "original_form": "저도 코로나땜에 아무데도 못가겠더라구요",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008143.1.17",
          "form": "원래는 제가 도시에 살거든요",
          "original_form": "원래는 제가 도시에 살거든요",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008143.1.18",
          "form": "코로나도 있고하니 그냥 한번 자러간건데",
          "original_form": "코로나도 있고하니 그냥 한번 자러간건데",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008143.1.19",
          "form": "옆에 다리랑 건물 야경에 공원도 잘되어있구",
          "original_form": "옆에 다리랑 건물 야경에 공원도 잘되어있구",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008143.1.20",
          "form": "송도 많이 좋아졌죠ㅎㅎ",
          "original_form": "송도 많이 좋아졌죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008143.1.21",
          "form": "눈앞에 인천바다 탁 트여있어서",
          "original_form": "눈앞에 인천바다 탁 트여있어서",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008143.1.22",
          "form": "네네 엄청나더라구요",
          "original_form": "네네 엄청나더라구요",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008143.1.23",
          "form": "차만있으면 그냥 반은 휴양지 느낌으로 살수있지않나 싶오요",
          "original_form": "차만있으면 그냥 반은 휴양지 느낌으로 살수있지않나 싶오요",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008143.1.24",
          "form": "싹바뀌고는 못가봤어요",
          "original_form": "싹바뀌고는 못가봤어요",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008143.1.25",
          "form": "예전에는 ..폐허였다니깐요 ㅎㅎ",
          "original_form": "예전에는 ..폐허였다니깐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008143.1.26",
          "form": "그럼 다른곳은 다녀오신데 없으신거예요?",
          "original_form": "그럼 다른곳은 다녀오신데 없으신거예요?",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008143.1.27",
          "form": "네네 ㅠ ㅠ",
          "original_form": "네네 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008143.1.28",
          "form": "작년까지만해도 괜찮게 다녔는데",
          "original_form": "작년까지만해도 괜찮게 다녔는데",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008143.1.29",
          "form": "올해믄 코로나가..",
          "original_form": "올해믄 코로나가..",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008143.1.30",
          "form": "작년이후로 전 아무데도 못갔어요",
          "original_form": "작년이후로 전 아무데도 못갔어요",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008143.1.31",
          "form": "일년에 한번 해외여행이 낙이였는데ㅠㅠ",
          "original_form": "일년에 한번 해외여행이 낙이였는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:40"
        }
      ]
