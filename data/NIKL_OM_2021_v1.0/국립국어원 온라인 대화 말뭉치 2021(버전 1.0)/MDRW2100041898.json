      "utterance": [
        {
          "id": "MDRW2100041898.1.1",
          "form": "저녁은 김볶인가??",
          "original_form": "저녁은 김볶인가??",
          "speaker_id": "1",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041898.1.2",
          "form": "우린 라면 끓여 달란다ㅋㅋㅋㅋ",
          "original_form": "우린 라면 끓여 달란다ㅋㅋㅋㅋ",
