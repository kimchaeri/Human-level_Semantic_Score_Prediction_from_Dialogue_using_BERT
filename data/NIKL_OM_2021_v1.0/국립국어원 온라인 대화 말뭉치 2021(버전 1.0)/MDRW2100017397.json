      "utterance": [
        {
          "id": "MDRW2100017397.1.1",
          "form": "저녁 뭐 드세요",
          "original_form": "저녁 뭐 드세요",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017397.1.2",
          "form": "집에 참치캔이 있어서",
          "original_form": "집에 참치캔이 있어서",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017397.1.3",
          "form": "참치마요비빔밥 먹을까해요!",
          "original_form": "참치마요비빔밥 먹을까해요!",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017397.1.4",
          "form": "와와 자취하시나보네여",
          "original_form": "와와 자취하시나보네여",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017397.1.5",
          "form": "뭔가 자취생 메뉴인것 같은",
          "original_form": "뭔가 자취생 메뉴인것 같은",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017397.1.6",
          "form": "엇! 어떻게 아셨지ㅋㅋㅋㅋ",
          "original_form": "엇! 어떻게 아셨지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017397.1.7",
          "form": "배달시키고 남은 단무지가 있어서 해먹으려고요ㅠ",
          "original_form": "배달시키고 남은 단무지가 있어서 해먹으려고요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017397.1.8",
          "form": "저도 자취할때 한참 해먹었기때문에ㅠㅠㅋㅋㅋ",
          "original_form": "저도 자취할때 한참 해먹었기때문에ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017397.1.9",
          "form": "저는 오늘 보쌈을 배달시켜먹었어요",
          "original_form": "저는 오늘 보쌈을 배달시켜먹었어요",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017397.1.10",
          "form": "아아ㅠㅠㅠ정말요?ㅠㅠㅠ어땠어요?",
          "original_form": "아아ㅠㅠㅠ정말요?ㅠㅠㅠ어땠어요?",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017397.1.11",
          "form": "제가 그저께부터 보쌈이 너무 먹고싶은데 가격이 사악해서 못먹고 있어요...",
          "original_form": "제가 그저께부터 보쌈이 너무 먹고싶은데 가격이 사악해서 못먹고 있어요...",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017397.1.12",
          "form": "보쌈은 정말 훌륭한 단백질 공급원이죠",
          "original_form": "보쌈은 정말 훌륭한 단백질 공급원이죠",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017397.1.13",
          "form": "우리동네에 맛있는 집이 있어서 늘 막국수와 함께 시켜먹오요",
          "original_form": "우리동네에 맛있는 집이 있어서 늘 막국수와 함께 시켜먹오요",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017397.1.14",
          "form": "하ㅜㅜ체인점 아니고 로컬맛집인가요?",
          "original_form": "하ㅜㅜ체인점 아니고 로컬맛집인가요?",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017397.1.15",
          "form": "네 로컬 맛집입니다 ㅎㅎ 로컬맛집을 좋아해요",
          "original_form": "네 로컬 맛집입니다 ㅎㅎ 로컬맛집을 좋아해요",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017397.1.16",
          "form": "보쌈은 족발보다 더비싸고 양이 적어서",
          "original_form": "보쌈은 족발보다 더비싸고 양이 적어서",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017397.1.17",
          "form": "그럴바엔 직접 해먹자 싶다가도 귀찮아서 미루게 되는...",
          "original_form": "그럴바엔 직접 해먹자 싶다가도 귀찮아서 미루게 되는...",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017397.1.18",
          "form": "와 직접 해드실수 있어요?",
          "original_form": "와 직접 해드실수 있어요?",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017397.1.19",
          "form": "요리 잘하시나 보네요",
          "original_form": "요리 잘하시나 보네요",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017397.1.20",
          "form": "아뇨ㅎㅎㅎ그냥 물에 넣고 끓이면 되는거 아닌가요ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨ㅎㅎㅎ그냥 물에 넣고 끓이면 되는거 아닌가요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017397.1.21",
          "form": "유튜브 따라하면 나도 요리사!",
          "original_form": "유튜브 따라하면 나도 요리사!",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017397.1.22",
          "form": "마자요ㅋㅋㅋ인터넷 없이 요리하시는 분들 너무 대단해요",
          "original_form": "마자요ㅋㅋㅋ인터넷 없이 요리하시는 분들 너무 대단해요",
          "speaker_id": "2",
          "time": "20210826 20:00"
        }
      ]
    }
  ]
