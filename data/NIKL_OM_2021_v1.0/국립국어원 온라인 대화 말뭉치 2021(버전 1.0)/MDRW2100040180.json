      "utterance": [
        {
          "id": "MDRW2100040180.1.1",
          "form": "최근 사신 템 있나요",
          "original_form": "최근 사신 템 있나요",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040180.1.2",
          "form": "저는 머플러를 포인트로 샀습니다ㅋㅋㅋ",
          "original_form": "저는 머플러를 포인트로 샀습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100040180.1.3",
          "form": "잃어버렸다더니 구매하셨나요",
          "original_form": "잃어버렸다더니 구매하셨나요",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100040180.1.4",
          "form": "네 결국못참고하나샀습니다\r\n",
          "original_form": "네 결국못참고하나샀습니다\r\n",
          "speaker_id": "1",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100040180.1.5",
          "form": "어떤거 사셨나요",
          "original_form": "어떤거 사셨나요",
          "speaker_id": "2",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100040180.1.6",
          "form": "무슨투톤으로된희한한거샀는데",
          "original_form": "무슨투톤으로된희한한거샀는데",
          "speaker_id": "1",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040180.1.7",
          "form": "실물이어떨지모르겠어요",
          "original_form": "실물이어떨지모르겠어요",
          "speaker_id": "1",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100040180.1.8",
          "form": "너무궁금하네요 왜 희한한걸 사셨죠?ㅋㅋㅋ",
          "original_form": "너무궁금하네요 왜 희한한걸 사셨죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040180.1.9",
          "form": "ㅋㅋㄱㅋ포인트쓰려니까 그가격대에 그거뿐이었어요ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ포인트쓰려니까 그가격대에 그거뿐이었어요ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100040180.1.10",
          "form": "앜ㅋㅋㅋㅋ그랬나요",
          "original_form": "앜ㅋㅋㅋㅋ그랬나요",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040180.1.11",
          "form": "추위만 피하면 되어요 담에 보여쥬세요",
          "original_form": "추위만 피하면 되어요 담에 보여쥬세요",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040180.1.12",
          "form": "네네 뚭도 담에 산옷입고나오세용",
          "original_form": "네네 뚭도 담에 산옷입고나오세용",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040180.1.13",
          "form": "네네알겠습니다 가지마요",
          "original_form": "네네알겠습니다 가지마요",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040180.1.14",
          "form": "또사고싶은거있나용",
          "original_form": "또사고싶은거있나용",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040180.1.15",
          "form": "어디가지마요?",
          "original_form": "어디가지마요?",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040180.1.16",
          "form": "음 운동복이요",
          "original_form": "음 운동복이요",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040180.1.17",
          "form": "카톡으로 가지마요",
          "original_form": "카톡으로 가지마요",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
