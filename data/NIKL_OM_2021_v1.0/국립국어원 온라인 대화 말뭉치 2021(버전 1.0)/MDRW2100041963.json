        },
        {
          "id": "MDRW2100041963.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041963.1.3",
          "form": "곧 저녁 먹을 시간이네요!  메뉴는 정하셨어요?",
          "original_form": "곧 저녁 먹을 시간이네요!  메뉴는 정하셨어요?",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041963.1.4",
          "form": "저는 며칠전부터 먹고싶었던 버팔로 윙을 사왔어요",
          "original_form": "저는 며칠전부터 먹고싶었던 버팔로 윙을 사왔어요",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041963.1.5",
          "form": "오늘 저녁으로 구워먹으려구요",
          "original_form": "오늘 저녁으로 구워먹으려구요",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041963.1.6",
          "form": "어 맛있뎄어요ㅠㅠㅠ",
          "original_form": "어 맛있뎄어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041963.1.7",
          "form": "버팔로윙 저는",
          "original_form": "버팔로윙 저는",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041963.1.8",
          "form": "사세꺼였나?",
          "original_form": "사세꺼였나?",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041963.1.9",
          "form": "그거 에어프라이어",
          "original_form": "그거 에어프라이어",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041963.1.10",
          "form": "해먹음마ㅛ잇더라구요",
          "original_form": "해먹음마ㅛ잇더라구요",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041963.1.11",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041963.1.12",
          "form": "사세 버팔로윙",
          "original_form": "사세 버팔로윙",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041963.1.13",
          "form": "그집께  진짜 최고더라구요\n친구한테 영업당해서 사먹었는데\n완전 밖에서 사먹는맛 그자체!!",
          "original_form": "그집께  진짜 최고더라구요\n친구한테 영업당해서 사먹었는데\n완전 밖에서 사먹는맛 그자체!!",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041963.1.14",
          "form": "저두 그거 왕좋아해요 가격대가.. 있어거 살때마다 망설이지만 ",
          "original_form": "저두 그거 왕좋아해요 가격대가.. 있어거 살때마다 망설이지만 ",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041963.1.15",
          "form": "하 맞아요...\n엄마랑 장보러갈때 스리슬쩍 카트에 집어넣고..",
          "original_form": "하 맞아요...\n엄마랑 장보러갈때 스리슬쩍 카트에 집어넣고..",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041963.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저드요 ㅎ... 아 갑자기 얘기들으니까 먹구싶어요ㅠㅠ 담에ㅜ장보라가서 사와야겟어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저드요 ㅎ... 아 갑자기 얘기들으니까 먹구싶어요ㅠㅠ 담에ㅜ장보라가서 사와야겟어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100041963.1.17",
          "form": "한봉지사면 온가족이 먹기 좋은듯요 진짜..\n너무 맛있어요",
          "original_form": "한봉지사면 온가족이 먹기 좋은듯요 진짜..\n너무 맛있어요",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100041963.1.18",
          "form": "아 점심을 너무 늦게먹어서 저녁을 거르려했는데 먹는거 얘기하니까 배고파지네요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 점심을 너무 늦게먹어서 저녁을 거르려했는데 먹는거 얘기하니까 배고파지네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100041963.1.19",
          "form": "저녁 먹어야 해욧!",
          "original_form": "저녁 먹어야 해욧!",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041963.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ...그쵸..... 뭐라두 먹어야겠어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ...그쵸..... 뭐라두 먹어야겠어요 ㅠㅠ",
