        {
          "id": "MDRW2100025977.1.2",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025977.1.3",
          "form": "네 홈트요",
          "original_form": "네 홈트요",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100025977.1.4",
          "form": "유튜브를 보고 하시나요?",
          "original_form": "유튜브를 보고 하시나요?",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100025977.1.5",
          "form": "네 땅크봐요",
          "original_form": "네 땅크봐요",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100025977.1.6",
          "form": "아 칼소폭?",
          "original_form": "아 칼소폭?",
          "speaker_id": "2",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100025977.1.7",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100025977.1.8",
          "form": "저도 칼소폭 봐요 ㅋㅋㅋㅋ",
          "original_form": "저도 칼소폭 봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100025977.1.9",
          "form": "부부케미가",
          "original_form": "부부케미가",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100025977.1.10",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025977.1.11",
          "form": "그리고 단계별로 잇어서 좋아요",
          "original_form": "그리고 단계별로 잇어서 좋아요",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025977.1.12",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100025977.1.13",
          "form": "네 꾸준히 하고 있습니다",
          "original_form": "네 꾸준히 하고 있습니다",
