      "utterance": [
        {
          "id": "MDRW2100007012.1.1",
          "form": "학생이에요~",
          "original_form": "학생이에요~",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007012.1.2",
          "form": "저도 대학생이에요!",
          "original_form": "저도 대학생이에요!",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007012.1.3",
          "form": "저는 과외 하는데 아르바이트 하시나요?",
          "original_form": "저는 과외 하는데 아르바이트 하시나요?",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007012.1.4",
          "form": "아뇨! 아르바이트 안 해요",
          "original_form": "아뇨! 아르바이트 안 해요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007012.1.5",
          "form": "과외 말고는 다른 아르바이트 안 해보셨나요?",
          "original_form": "과외 말고는 다른 아르바이트 안 해보셨나요?",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007012.1.6",
          "form": "파리바게트에서 했는데 다신 하고 싶지 않아요 ㅎㅋㅋㅋㅋㅋ",
          "original_form": "파리바게트에서 했는데 다신 하고 싶지 않아요 ㅎㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007012.1.7",
          "form": "친구가 파바 알바 하는데 진짜 힘들어하더라구요",
          "original_form": "친구가 파바 알바 하는데 진짜 힘들어하더라구요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007012.1.8",
          "form": "과외는 어때요?",
          "original_form": "과외는 어때요?",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007012.1.9",
          "form": "과외는 그냥 하다보니까 할만해요 ㅎㅎㅎ",
          "original_form": "과외는 그냥 하다보니까 할만해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007012.1.10",
          "form": "벌써 2년 정도 흘렀네요~",
          "original_form": "벌써 2년 정도 흘렀네요~",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007012.1.11",
          "form": "오 과외만 2년째 하고 계시는 거예요?",
          "original_form": "오 과외만 2년째 하고 계시는 거예요?",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007012.1.12",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007012.1.13",
          "form": "쉬다가 하다가 멘토링도 하고 ㅎㅎㅎ 여러가지 가르치는 일 하는 것 같아요!",
          "original_form": "쉬다가 하다가 멘토링도 하고 ㅎㅎㅎ 여러가지 가르치는 일 하는 것 같아요!",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007012.1.14",
          "form": "가르치는 게 적성에 맞으시나 봐요!",
          "original_form": "가르치는 게 적성에 맞으시나 봐요!",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007012.1.15",
          "form": "저도 학원 보조강사 알바는 해봤었는데",
          "original_form": "저도 학원 보조강사 알바는 해봤었는데",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007012.1.16",
          "form": "누구 가르치는 걸 잘 못하겠더라구요",
          "original_form": "누구 가르치는 걸 잘 못하겠더라구요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007012.1.17",
          "form": "그렇군요 저는 이제 가르치는게 익숙해서",
          "original_form": "그렇군요 저는 이제 가르치는게 익숙해서",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100007012.1.18",
          "form": "이제는 그냥 숨 쉬는 것 처럼 하고 있어요 ㅎㅎㅎ",
          "original_form": "이제는 그냥 숨 쉬는 것 처럼 하고 있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100007012.1.19",
          "form": "처음부터 가르치는 걸 잘 하시는 편이셨나요?",
          "original_form": "처음부터 가르치는 걸 잘 하시는 편이셨나요?",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100007012.1.20",
          "form": "음 그냥 공부를 잘하는 편이어서 계속 가르치는 역할을 했던 것 같아요 ㅎㅎ",
          "original_form": "음 그냥 공부를 잘하는 편이어서 계속 가르치는 역할을 했던 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100007012.1.21",
          "form": "멋지시네요!",
          "original_form": "멋지시네요!",
          "speaker_id": "2",
