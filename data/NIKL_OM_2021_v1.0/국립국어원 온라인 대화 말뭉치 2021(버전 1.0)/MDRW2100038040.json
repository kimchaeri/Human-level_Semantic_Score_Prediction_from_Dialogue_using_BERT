          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100038040.1.36",
          "form": "전기의 정령 드랍템ㅇ인가봐",
          "original_form": "전기의 정령 드랍템ㅇ인가봐",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100038040.1.37",
          "form": "ㅇㅇ근데",
          "original_form": "ㅇㅇ근데",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100038040.1.38",
