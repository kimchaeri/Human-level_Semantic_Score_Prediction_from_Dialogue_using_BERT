        },
        {
          "id": "MDRW2100038040.1.3",
          "form": "사안 부작용 뭔데;;;",
          "original_form": "사안 부작용 뭔데;;;",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038040.1.4",
          "form": "이런걸 타탈은 대체 어케 쓴거임",
          "original_form": "이런걸 타탈은 대체 어케 쓴거임",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038040.1.5",
          "form": "나름 우인단이다 이건가",
          "original_form": "나름 우인단이다 이건가",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038040.1.6",
          "form": "그니까.....",
          "original_form": "그니까.....",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038040.1.7",
          "form": "그래도 나름",
          "original_form": "그래도 나름",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038040.1.8",
          "form": "11인자.......?",
          "original_form": "11인자.......?",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038040.1.9",
          "form": "ㅇㅏ간지안나",
          "original_form": "ㅇㅏ간지안나",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038040.1.10",
          "form": "야예가",
          "original_form": "야예가",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038040.1.11",
          "form": "여기서",
          "original_form": "여기서",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038040.1.12",
          "form": "왜나와요...",
          "original_form": "왜나와요...",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038040.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038040.1.14",
          "form": "무슨일이 일어나는거야",
          "original_form": "무슨일이 일어나는거야",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038040.1.15",
          "form": "ㅇ ㅏ",
          "original_form": "ㅇ ㅏ",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038040.1.16",
          "form": "라이덴이랑 싸워보기도 전에",
          "original_form": "라이덴이랑 싸워보기도 전에",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038040.1.17",
          "form": "미니게임에서 막혀버림....",
          "original_form": "미니게임에서 막혀버림....",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038040.1.18",
          "form": "10번 이하로 피격당하라는데",
          "original_form": "10번 이하로 피격당하라는데",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038040.1.19",
          "form": "번개 한번 맞을때마다 횟수가 2~3 회씩 올라가는데요 ㅡㅡ",
          "original_form": "번개 한번 맞을때마다 횟수가 2~3 회씩 올라가는데요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038040.1.20",
          "form": "큐ㅜㅠㅠㅠㅠㅠㅠ",
          "original_form": "큐ㅜㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038040.1.21",
          "form": "나 라이덴 들고 라이덴이랑 싸우기",
          "original_form": "나 라이덴 들고 라이덴이랑 싸우기",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038040.1.22",
          "form": "해보고싶었는데",
          "original_form": "해보고싶었는데",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038040.1.23",
          "form": "안될듯",
          "original_form": "안될듯",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038040.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038040.1.25",
          "form": "얘네 갈수록",
          "original_form": "얘네 갈수록",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038040.1.26",
          "form": "유저한테 너무 고도의 컨트롤을 요두해...",
          "original_form": "유저한테 너무 고도의 컨트롤을 요두해...",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038040.1.27",
          "form": "요구해...",
          "original_form": "요구해...",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038040.1.28",
          "form": "아니 없는 보스 부산물을 내놓아야",
          "original_form": "아니 없는 보스 부산물을 내놓아야",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038040.1.29",
          "form": "렙업시켜준대",
          "original_form": "렙업시켜준대",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038040.1.30",
          "form": "멘스 밀어야 생기는 보스인가봐",
          "original_form": "멘스 밀어야 생기는 보스인가봐",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038040.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038040.1.32",
          "form": "그거 시뇨라 드랍템 아냐?",
          "original_form": "그거 시뇨라 드랍템 아냐?",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038040.1.33",
          "form": "전기템같았는데",
          "original_form": "전기템같았는데",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038040.1.34",
          "form": "아 그래?",
          "original_form": "아 그래?",
