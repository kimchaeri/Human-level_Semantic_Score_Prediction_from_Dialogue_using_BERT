        {
          "id": "MDRW2100004098.1.1",
          "form": "혹시.....연애하시나요?",
          "original_form": "혹시.....연애하시나요?",
          "speaker_id": "2",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004098.1.2",
          "form": "아뇨 안해요ㅋㅋ",
          "original_form": "아뇨 안해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004098.1.3",
          "form": "저도 안해요 ㅋㅋㅋㅋ",
          "original_form": "저도 안해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004098.1.4",
          "form": "뭔가 시간이 아깝더라고요",
          "original_form": "뭔가 시간이 아깝더라고요",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004098.1.5",
          "form": "저는사실 연애를안해봤어요",
          "original_form": "저는사실 연애를안해봤어요",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004098.1.6",
          "form": "근데 안하고싶으신거죠?",
          "original_form": "근데 안하고싶으신거죠?",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004098.1.7",
          "form": "지금은 별생각없는데",
          "original_form": "지금은 별생각없는데",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.8",
          "form": "원래 하고싶긴했어요",
          "original_form": "원래 하고싶긴했어요",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.9",
          "form": "오 그라면 이상형은 어떻게 되세요?",
          "original_form": "오 그라면 이상형은 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.10",
          "form": "잘생긴사람..이긴한데 실제로는걍",
          "original_form": "잘생긴사람..이긴한데 실제로는걍",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.11",
          "form": "좋죠 잘생긴사람",
          "original_form": "좋죠 잘생긴사람",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.12",
          "form": "선한 사람 좋아하는거같아요ㅋㅋ",
          "original_form": "선한 사람 좋아하는거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.13",
          "form": "키도 크면 좋겠어요 저는",
          "original_form": "키도 크면 좋겠어요 저는",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.14",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004098.1.15",
          "form": "아 그쵸 키가크면 좋죠ㅠ 솔직히180되면좋겠어요",
          "original_form": "아 그쵸 키가크면 좋죠ㅠ 솔직히180되면좋겠어요",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
