      "utterance": [
        {
          "id": "MDRW2100007475.1.1",
          "form": "학교는 무슨과나오셨어요?",
          "original_form": "학교는 무슨과나오셨어요?",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.2",
          "form": "전공이랑 같은 일해요?",
          "original_form": "전공이랑 같은 일해요?",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.3",
          "form": "전자과나왔어요 ㅎㅎ",
          "original_form": "전자과나왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.4",
          "form": "전기쪽으로 비슷한 일하고있죠 ㅎ",
          "original_form": "전기쪽으로 비슷한 일하고있죠 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.5",
          "form": "와대박",
          "original_form": "와대박",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.6",
          "form": "공대여신이었어요?",
          "original_form": "공대여신이었어요?",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.7",
          "form": "전공이랑 같은세요?",
          "original_form": "전공이랑 같은세요?",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.8",
          "form": "공대라니 멋있어요",
          "original_form": "공대라니 멋있어요",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007475.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007475.1.10",
          "form": "고민이예요",
          "original_form": "고민이예요",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007475.1.11",
          "form": "석사를 해야되나",
          "original_form": "석사를 해야되나",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007475.1.12",
          "form": "여신은 ㅋㅋ 여자사람이였죠 ㅋㅋ",
          "original_form": "여신은 ㅋㅋ 여자사람이였죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007475.1.13",
          "form": "전공이 머세요?",
          "original_form": "전공이 머세요?",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007475.1.14",
          "form": "심리요!",
          "original_form": "심리요!",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007475.1.15",
          "form": "다들 석박사해서 제가 낄자리가없어요ㅠ",
          "original_form": "다들 석박사해서 제가 낄자리가없어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007475.1.16",
          "form": "석사하면 회사에서 더 주나요?",
          "original_form": "석사하면 회사에서 더 주나요?",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007475.1.17",
          "form": "네 비영리기관이면 돈을 더 줘요",
          "original_form": "네 비영리기관이면 돈을 더 줘요",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007475.1.18",
          "form": "영리기관이면!",
          "original_form": "영리기관이면!",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007475.1.19",
          "form": "지금은 비영리기관에 있어서 똑같긴한데\r\n더 호봉 쌓이기전에 이직을 해야되서",
          "original_form": "지금은 비영리기관에 있어서 똑같긴한데\r\n더 호봉 쌓이기전에 이직을 해야되서",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007475.1.20",
          "form": "그럼 석사하셔요",
          "original_form": "그럼 석사하셔요",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007475.1.21",
          "form": "전공이랑 같은일하신다니 멋있어오",
          "original_form": "전공이랑 같은일하신다니 멋있어오",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007475.1.22",
          "form": "힘들어도 하셔요",
          "original_form": "힘들어도 하셔요",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007475.1.23",
          "form": "하면서도 이길이 맞나 싶을때가 있는데",
          "original_form": "하면서도 이길이 맞나 싶을때가 있는데",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007475.1.24",
          "form": "저도 그생각해요 ㅎㅎ",
          "original_form": "저도 그생각해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007475.1.25",
          "form": "저는 석사도 했어요 ㅎ",
          "original_form": "저는 석사도 했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007475.1.26",
          "form": "와 멋있어요",
          "original_form": "와 멋있어요",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007475.1.27",
          "form": "그당시에도 석사가흔했죠?",
          "original_form": "그당시에도 석사가흔했죠?",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007475.1.28",
          "form": "요즘은 석박사없는 사람 찾기힘든거같아요",
          "original_form": "요즘은 석박사없는 사람 찾기힘든거같아요",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007475.1.29",
          "form": "다들 고학력 고스펙",
          "original_form": "다들 고학력 고스펙",
          "speaker_id": "1",
          "time": "20210822 02:37"
        }
      ]
    }
