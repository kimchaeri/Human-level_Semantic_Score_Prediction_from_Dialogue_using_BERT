          "original_form": "루테인",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.2",
          "form": "먹고싶도",
          "original_form": "먹고싶도",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.3",
          "form": "당근이라도 드세요",
          "original_form": "당근이라도 드세요",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.4",
          "form": "아침에 당근 하나",
          "original_form": "아침에 당근 하나",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.5",
          "form": "마그네슘 가격이 매우 오름",
          "original_form": "마그네슘 가격이 매우 오름",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.6",
          "form": "점심에 당근 두 개",
          "original_form": "점심에 당근 두 개",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.7",
          "form": "당근없어",
          "original_form": "당근없어",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.8",
          "form": "난토끼가아니야",
          "original_form": "난토끼가아니야",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.9",
          "form": "당근싫어해",
          "original_form": "당근싫어해",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.10",
          "form": "눈을 위해서",
          "original_form": "눈을 위해서",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.11",
          "form": "루테인 대신",
          "original_form": "루테인 대신",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.12",
          "form": "눈알 굴리기 운도은",
          "original_form": "눈알 굴리기 운도은",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.13",
          "form": "하겠음",
          "original_form": "하겠음",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029254.1.14",
          "form": "토끼똥 되게 많이 싸",
          "original_form": "토끼똥 되게 많이 싸",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.15",
          "form": "블루스크린 안경이랑",
          "original_form": "블루스크린 안경이랑",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.16",
          "form": "이경규가 아니에요",
          "original_form": "이경규가 아니에요",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.17",
          "form": "어플씀",
          "original_form": "어플씀",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.18",
          "form": "토끼똥 눴는데",
          "original_form": "토끼똥 눴는데",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.19",
          "form": "마그네슘 먹고",
          "original_form": "마그네슘 먹고",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.20",
          "form": "아무말 대장치",
          "original_form": "아무말 대장치",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.21",
          "form": "빈혈에는",
          "original_form": "빈혈에는",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.22",
          "form": "절분",
          "original_form": "절분",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.23",
          "form": "철분약먹고",
          "original_form": "철분약먹고",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.24",
          "form": "토끼가 마그네슘 먹었다고요",
          "original_form": "토끼가 마그네슘 먹었다고요",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.25",
          "form": "변비얻음",
          "original_form": "변비얻음",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.26",
          "form": "철분 약이 변비가 있군요",
          "original_form": "철분 약이 변비가 있군요",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029254.1.27",
          "form": "변비는 마그네슘으로 치료",
          "original_form": "변비는 마그네슘으로 치료",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
