          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100033972.1.3",
          "form": "마카롱 좋아해?",
          "original_form": "마카롱 좋아해?",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100033972.1.4",
          "form": "가끔 생각나는데",
          "original_form": "가끔 생각나는데",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100033972.1.5",
          "form": "사먹진 않아",
          "original_form": "사먹진 않아",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100033972.1.6",
          "form": "난 나름 자주 사먹는 듯",
          "original_form": "난 나름 자주 사먹는 듯",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100033972.1.7",
          "form": "어떤 맛 제일 좋아해?",
          "original_form": "어떤 맛 제일 좋아해?",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033972.1.8",
          "form": "나 안 가리고 잘 먹어",
          "original_form": "나 안 가리고 잘 먹어",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033972.1.9",
          "form": "초코나 민초 제일 좋아하는 듯",
          "original_form": "초코나 민초 제일 좋아하는 듯",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033972.1.10",
          "form": "초코는 나도 맛있게 먹었어",
          "original_form": "초코는 나도 맛있게 먹었어",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033972.1.11",
          "form": "초코칩 잘게 썰려서 씹히는 게 특히 좋아",
          "original_form": "초코칩 잘게 썰려서 씹히는 게 특히 좋아",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033972.1.12",
          "form": "아 맛있겠다",
          "original_form": "아 맛있겠다",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033972.1.13",
          "form": "마지막 마카롱 언제야?",
          "original_form": "마지막 마카롱 언제야?",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100033972.1.14",
          "form": "나 일주일 전?ㅋㅋㅋㅋ",
          "original_form": "나 일주일 전?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100033972.1.15",
          "form": "진짜 자주 먹네",
          "original_form": "진짜 자주 먹네",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100033972.1.16",
          "form": "음료는?",
          "original_form": "음료는?",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100033972.1.17",
          "form": "음료 없이도 잘 먹어",
          "original_form": "음료 없이도 잘 먹어",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100033972.1.18",
          "form": "요즘 쉐이크 맛있더라",
          "original_form": "요즘 쉐이크 맛있더라",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100033972.1.19",
          "form": "아 맛있지~",
          "original_form": "아 맛있지~",
          "speaker_id": "1",
