        },
        {
          "id": "MDRW2100028831.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028831.1.3",
          "form": "여행계획있으신가요?",
          "original_form": "여행계획있으신가요?",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028831.1.4",
          "form": "아니요 아직 딱히 정한건 없어요ㅠ name1은요?",
          "original_form": "아니요 아직 딱히 정한건 없어요ㅠ &name1&은요?",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028831.1.5",
          "form": "전 이번주말에 부산가려구요",
          "original_form": "전 이번주말에 부산가려구요",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028831.1.6",
          "form": "가서 해운대 구경하려구합니다 ㅎㅎ",
          "original_form": "가서 해운대 구경하려구합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028831.1.7",
          "form": "오 해운대 좋죠ㅎㅎ 전 해운대에 안개낀거도 좋더라고요 분위기 있어보여서",
          "original_form": "오 해운대 좋죠ㅎㅎ 전 해운대에 안개낀거도 좋더라고요 분위기 있어보여서",
          "speaker_id": "2",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028831.1.8",
          "form": "ㅎㅎㅎ코로나풀리면",
          "original_form": "ㅎㅎㅎ코로나풀리면",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028831.1.9",
          "form": "어디제일가고싶으세요?",
          "original_form": "어디제일가고싶으세요?",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028831.1.10",
          "form": "전 중국 한번 가보고 싶어요ㅋㅋ",
          "original_form": "전 중국 한번 가보고 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028831.1.11",
          "form": "고궁같은거 좋아해서ㅎ",
          "original_form": "고궁같은거 좋아해서ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028831.1.12",
          "form": "오 저 중국 세번가봤어요",
          "original_form": "오 저 중국 세번가봤어요",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028831.1.13",
          "form": "상하이도 되게 좋아요 ㅋㅋ",
          "original_form": "상하이도 되게 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028831.1.14",
          "form": "상하이 좋다는 얘기 많이 들었어요ㅠㅠ 야경같은거도 예쁘다고ㅠ 넘 부럽네요",
          "original_form": "상하이 좋다는 얘기 많이 들었어요ㅠㅠ 야경같은거도 예쁘다고ㅠ 넘 부럽네요",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028831.1.15",
          "form": "맞아요ㅠ코로나만 풀리면 또갈텐데ㅠ",
          "original_form": "맞아요ㅠ코로나만 풀리면 또갈텐데ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028831.1.16",
          "form": "해외여행 어디 가보셨나요 ㅎㅎ",
          "original_form": "해외여행 어디 가보셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028831.1.17",
          "form": "전 대만 일본 미국 영국 가봤어요ㅎ",
          "original_form": "전 대만 일본 미국 영국 가봤어요ㅎ",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100028831.1.18",
          "form": "오 많이가셨네요",
          "original_form": "오 많이가셨네요",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100028831.1.19",
          "form": "어디가 제일 좋으셨나요 ㅎㅎ",
          "original_form": "어디가 제일 좋으셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100028831.1.20",
          "form": "저는 일본이 가장 좋았어요ㅋㅋ",
          "original_form": "저는 일본이 가장 좋았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100028831.1.21",
          "form": "벚꽃필때 갔었는데 너무 예뻤고 교토 분위기 좋아해요",
          "original_form": "벚꽃필때 갔었는데 너무 예뻤고 교토 분위기 좋아해요",
