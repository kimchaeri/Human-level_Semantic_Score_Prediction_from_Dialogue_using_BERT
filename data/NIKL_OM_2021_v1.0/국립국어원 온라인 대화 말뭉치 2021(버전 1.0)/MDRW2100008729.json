      "utterance": [
        {
          "id": "MDRW2100008729.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008729.1.2",
          "form": "여행 좋죠!",
          "original_form": "여행 좋죠!",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008729.1.3",
          "form": "해외여행 가보셨어요?",
          "original_form": "해외여행 가보셨어요?",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008729.1.4",
          "form": "네 호주요",
          "original_form": "네 호주요",
          "speaker_id": "1",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008729.1.5",
          "form": "우와 언제 갔어요?",
          "original_form": "우와 언제 갔어요?",
          "speaker_id": "2",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008729.1.6",
          "form": "3년전인가?",
          "original_form": "3년전인가?",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008729.1.7",
          "form": "얼마안됐네요!!!",
          "original_form": "얼마안됐네요!!!",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008729.1.8",
          "form": "부러워요 전 가까운 일본 딱 한번만 가봤어요",
          "original_form": "부러워요 전 가까운 일본 딱 한번만 가봤어요",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008729.1.9",
          "form": "일본은 방사능",
          "original_form": "일본은 방사능",
          "speaker_id": "1",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008729.1.10",
          "form": "헠ㅋㅋ 맞는 말이에요",
          "original_form": "헠ㅋㅋ 맞는 말이에요",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008729.1.11",
          "form": "그래도 나중에 또 가려고요",
          "original_form": "그래도 나중에 또 가려고요",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008729.1.12",
          "form": "방사능만 아니몀 가까워서 가고 싶은네",
          "original_form": "방사능만 아니몀 가까워서 가고 싶은네",
          "speaker_id": "1",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008729.1.13",
          "form": "음식도 맛있고요",
          "original_form": "음식도 맛있고요",
          "speaker_id": "2",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008729.1.14",
          "form": "잠깐만 머무르는거면 괜찮지않을까요?",
          "original_form": "잠깐만 머무르는거면 괜찮지않을까요?",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008729.1.15",
          "form": "전 일본 여행 가봤으니까 그렇게 무섭진 않더라고요",
          "original_form": "전 일본 여행 가봤으니까 그렇게 무섭진 않더라고요",
          "speaker_id": "2",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100008729.1.16",
          "form": "제가 너무 겁네네요",
          "original_form": "제가 너무 겁네네요",
          "speaker_id": "1",
          "time": "20210823 17:10"
        }
