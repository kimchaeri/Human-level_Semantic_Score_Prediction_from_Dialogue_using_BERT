      "utterance": [
        {
          "id": "MDRW2100013921.1.1",
          "form": "요가 저는 힘들던데..\n몸이 뻣뻣해서요\n오래하셨나요",
          "original_form": "요가 저는 힘들던데..\n몸이 뻣뻣해서요\n오래하셨나요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100013921.1.2",
          "form": "얼마안됐어요 집안일하시나요?",
          "original_form": "얼마안됐어요 집안일하시나요?",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100013921.1.3",
          "form": "네 주부에요",
          "original_form": "네 주부에요",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100013921.1.4",
          "form": "집안일은...끝이없죠",
          "original_form": "집안일은...끝이없죠",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100013921.1.5",
          "form": "제일 싫은 집안일이 뭔가요",
          "original_form": "제일 싫은 집안일이 뭔가요",
          "speaker_id": "1",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100013921.1.6",
          "form": "화장실청소. 청소기먼지비우기요",
          "original_form": "화장실청소. 청소기먼지비우기요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100013921.1.7",
          "form": "님은",
          "original_form": "님은",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100013921.1.8",
          "form": "음식물 쓰레기 치우기요",
          "original_form": "음식물 쓰레기 치우기요",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100013921.1.9",
          "form": "요리하시는거 좋아하세요",
          "original_form": "요리하시는거 좋아하세요",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100013921.1.10",
          "form": "매일해요",
          "original_form": "매일해요",
          "speaker_id": "1",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100013921.1.11",
          "form": "주로 뭐해드세요",
          "original_form": "주로 뭐해드세요",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100013921.1.12",
          "form": "저는 밑반찬하는게 잘안되네요",
          "original_form": "저는 밑반찬하는게 잘안되네요",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100013921.1.13",
          "form": "채소 볶아먹어요",
          "original_form": "채소 볶아먹어요",
          "speaker_id": "1",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100013921.1.14",
          "form": "뭐해드셨어요?",
          "original_form": "뭐해드셨어요?",
          "speaker_id": "1",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100013921.1.15",
          "form": "소고기국이요",
          "original_form": "소고기국이요",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100013921.1.16",
          "form": "채소어떤거요",
          "original_form": "채소어떤거요",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100013921.1.17",
          "form": "아스파라거스요",
          "original_form": "아스파라거스요",
          "speaker_id": "1",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100013921.1.18",
          "form": "고기구울때 같이 구워먹는데",
          "original_form": "고기구울때 같이 구워먹는데",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100013921.1.19",
          "form": "어떻게 볶아요",
          "original_form": "어떻게 볶아요",
          "speaker_id": "2",
          "time": "20210826 11:38"
        }
      ]
