      "utterance": [
        {
          "id": "MDRW2100014485.1.1",
          "form": "출석으로 책가방 사신거예요?",
          "original_form": "출석으로 책가방 사신거예요?",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014485.1.2",
          "form": "출석으로 크로스 가방요",
          "original_form": "출석으로 크로스 가방요",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014485.1.3",
          "form": "반읊그걸로 결재요 ㅋㅋ",
          "original_form": "반읊그걸로 결재요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014485.1.4",
          "form": "오~ 어디서요?",
          "original_form": "오~ 어디서요?",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014485.1.5",
          "form": "아세일안되면 안사죠 ㅋㅋ",
          "original_form": "아세일안되면 안사죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014485.1.6",
          "form": "h패션몰이요",
          "original_form": "h패션몰이요",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014485.1.7",
          "form": "아...거기요",
          "original_form": "아...거기요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014485.1.8",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014485.1.9",
          "form": "공짜예요?",
          "original_form": "공짜예요?",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014485.1.10",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014485.1.11",
          "form": "보태서샀는데 반을 포인트 결재요",
          "original_form": "보태서샀는데 반을 포인트 결재요",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014485.1.12",
          "form": "요번에 출석좀 많이주고 쿠폰뭘 막줘서요",
          "original_form": "요번에 출석좀 많이주고 쿠폰뭘 막줘서요",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014485.1.13",
          "form": "와 그래도 반 포인트면",
          "original_form": "와 그래도 반 포인트면",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014485.1.14",
          "form": "50퍼할인이네요",
          "original_form": "50퍼할인이네요",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014485.1.15",
          "form": "네 ㅋㅋ보람있네요",
          "original_form": "네 ㅋㅋ보람있네요",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014485.1.16",
          "form": "뿌듯하시겠어요",
          "original_form": "뿌듯하시겠어요",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014485.1.17",
          "form": "40프로가격을 50프로더요 ㅋㅋ",
          "original_form": "40프로가격을 50프로더요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
