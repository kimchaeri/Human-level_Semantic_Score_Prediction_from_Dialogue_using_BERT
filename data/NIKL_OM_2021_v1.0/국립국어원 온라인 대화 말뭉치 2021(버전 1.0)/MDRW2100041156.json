      "utterance": [
        {
          "id": "MDRW2100041156.1.1",
          "form": "간식은 뭐 주세요",
          "original_form": "간식은 뭐 주세요",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041156.1.2",
          "form": "오이.고구마.포도빼고 각종 과일들 다 먹어요^^;;",
          "original_form": "오이.고구마.포도빼고 각종 과일들 다 먹어요^^;;",
          "speaker_id": "1",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100041156.1.3",
          "form": "와 그렇군요",
          "original_form": "와 그렇군요",
          "speaker_id": "2",
          "time": "20210905 19:16"
        },
        {
          "id": "MDRW2100041156.1.4",
          "form": "고구마는 싫어해료?",
          "original_form": "고구마는 싫어해료?",
          "speaker_id": "2",
          "time": "20210905 19:16"
        },
        {
          "id": "MDRW2100041156.1.5",
          "form": "아~  오이랑 고구마 그리고 과일(포도빼고))",
          "original_form": "아~  오이랑 고구마 그리고 과일(포도빼고))",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041156.1.6",
          "form": "입니다ㅎㅎ",
          "original_form": "입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041156.1.7",
          "form": "아아아 ㅋㅋㅋㅋ",
          "original_form": "아아아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041156.1.8",
          "form": "제가 난독이네요",
          "original_form": "제가 난독이네요",
          "speaker_id": "2",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041156.1.9",
          "form": "저흰 오이는 안먹어요ㅠ",
          "original_form": "저흰 오이는 안먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041156.1.10",
          "form": "ㅎㅎ오이. 무. 당근. 양배추 다 먹더라구요",
          "original_form": "ㅎㅎ오이. 무. 당근. 양배추 다 먹더라구요",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041156.1.11",
          "form": "심지어 두룹찐것도 먹더군요",
          "original_form": "심지어 두룹찐것도 먹더군요",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041156.1.12",
          "form": "와 두릅도 먹나요 ?? 대든하다",
          "original_form": "와 두릅도 먹나요 ?? 대든하다",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100041156.1.13",
          "form": "저흰 당근 양베투 오이 다 안먹어요 ..",
          "original_form": "저흰 당근 양베투 오이 다 안먹어요 ..",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100041156.1.14",
          "form": "근데 이렇게 먹보인데 병원에선 간식일절주지말라는 말만하네요..",
          "original_form": "근데 이렇게 먹보인데 병원에선 간식일절주지말라는 말만하네요..",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041156.1.15",
          "form": "그쵸ㅠㅠ 왜 주면 안될까요 흑",
          "original_form": "그쵸ㅠㅠ 왜 주면 안될까요 흑",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100041156.1.16",
          "form": "가공된 간식들은 그래두 야채나 과일은 괜찮을것 같은데..모르겠어요. 그냥줘요;;",
          "original_form": "가공된 간식들은 그래두 야채나 과일은 괜찮을것 같은데..모르겠어요. 그냥줘요;;",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100041156.1.17",
          "form": "그쵸ㅠㅠ 야채랑 고기 원물은 왜 안좋은건지ㅠㅠ",
          "original_form": "그쵸ㅠㅠ 야채랑 고기 원물은 왜 안좋은건지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041156.1.18",
          "form": "사료가 더 안좋을거같아요ㅠㅠ",
          "original_form": "사료가 더 안좋을거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041156.1.19",
          "form": "아 저희 개는 담관에 찌꺼기가 꽤있고",
          "original_form": "아 저희 개는 담관에 찌꺼기가 꽤있고",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041156.1.20",
          "form": "간이 선천적으로 좀 작다고",
          "original_form": "간이 선천적으로 좀 작다고",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041156.1.21",
          "form": "지방은 절대 주지말라고해서",
          "original_form": "지방은 절대 주지말라고해서",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041156.1.22",
          "form": "삼겹살이요",
          "original_form": "삼겹살이요",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041156.1.23",
          "form": "그거는 진짜 제한하구있어요",
          "original_form": "그거는 진짜 제한하구있어요",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041156.1.24",
          "form": "아아ㅠㅠ 담관 찌꺼기라니ㅜㅜ",
          "original_form": "아아ㅠㅠ 담관 찌꺼기라니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 22:12"
        }
      ]
    }
