        },
        {
          "id": "MDRW2100031894.1.2",
          "form": "네ㅎㅎ name1님은요",
          "original_form": "네ㅎㅎ &name1&님은요",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031894.1.3",
          "form": "네  근무중입니다",
          "original_form": "네  근무중입니다",
          "speaker_id": "1",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100031894.1.4",
          "form": "주말에 일하시는군요",
          "original_form": "주말에 일하시는군요",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031894.1.5",
          "form": "아 저도 주말에는 쉬어요^^;",
          "original_form": "아 저도 주말에는 쉬어요^^;",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031894.1.6",
          "form": "ㅋㅋ아 지금이 아니고",
          "original_form": "ㅋㅋ아 지금이 아니고",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031894.1.7",
          "form": "사실 전 주말도 일해요",
          "original_form": "사실 전 주말도 일해요",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031894.1.8",
          "form": "교대근무라서 ㅎㅎ",
          "original_form": "교대근무라서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031894.1.9",
          "form": "아 그러시군요 교대근무 할만하시나요?",
          "original_form": "아 그러시군요 교대근무 할만하시나요?",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100031894.1.10",
          "form": "네 저는 만족해요 ㅎㅎ",
          "original_form": "네 저는 만족해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100031894.1.11",
          "form": "쉴때 일하고 일할때 쉬고",
          "original_form": "쉴때 일하고 일할때 쉬고",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100031894.1.12",
          "form": "교대근무면 저녁근무도 있나요?",
          "original_form": "교대근무면 저녁근무도 있나요?",
          "speaker_id": "1",
          "time": "20210905 14:02"
        },
        {
          "id": "MDRW2100031894.1.13",
          "form": "네 야간이요 ㅎㅎ",
          "original_form": "네 야간이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100031894.1.14",
          "form": "담날 아침 퇴근 ㅎㅎ",
          "original_form": "담날 아침 퇴근 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100031894.1.15",
          "form": "야간이면 힘드시지 않나요?",
          "original_form": "야간이면 힘드시지 않나요?",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100031894.1.16",
          "form": "장단점이 있는데",
          "original_form": "장단점이 있는데",
          "speaker_id": "2",
          "time": "20210906 08:22"
        },
        {
          "id": "MDRW2100031894.1.17",
          "form": "밤낮 바뀌는거 적응 잘해서 ㅎㅎ",
          "original_form": "밤낮 바뀌는거 적응 잘해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 08:23"
        },
        {
          "id": "MDRW2100031894.1.18",
          "form": "오 다행이네요 교대근무하며 식사랑도 괜찮으신가요?",
          "original_form": "오 다행이네요 교대근무하며 식사랑도 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100031894.1.19",
          "form": "흥미 없으시면 주제 바꾸심이 ㅎㅎ",
          "original_form": "흥미 없으시면 주제 바꾸심이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:33"
