        },
        {
          "id": "MDRW2100012921.1.2",
          "form": "아침 맛나게 잡사용^^",
          "original_form": "아침 맛나게 잡사용^^",
          "speaker_id": "2",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012921.1.3",
          "form": "난도 밥묵고 준비해야긋당 ㅋㅋㅋㅋ",
          "original_form": "난도 밥묵고 준비해야긋당 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012921.1.4",
          "form": "응응 니도 맛있게먹어ㅋㅋ",
          "original_form": "응응 니도 맛있게먹어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100012921.1.5",
          "form": "나도 오늘은 초코우유먹이고보냈네ㅜㅋㅋ",
          "original_form": "나도 오늘은 초코우유먹이고보냈네ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:54"
        },
        {
          "id": "MDRW2100012921.1.6",
          "form": "일어날생각을 안하더라",
          "original_form": "일어날생각을 안하더라",
          "speaker_id": "1",
          "time": "20210825 09:54"
        },
        {
          "id": "MDRW2100012921.1.7",
          "form": "날씨가 이러니 ㅋㅋㅋㅋ",
          "original_form": "날씨가 이러니 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012921.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012921.1.9",
          "form": "난첫찌 안일어나가 겨우 깨워서 편의점갔다가~",
          "original_form": "난첫찌 안일어나가 겨우 깨워서 편의점갔다가~",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012921.1.10",
          "form": "삼김하고 사서 왔다 ㅋㅋㅋ",
          "original_form": "삼김하고 사서 왔다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012921.1.11",
          "form": "ㅋㅋㅋㅋ잘했다ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ잘했다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012921.1.12",
          "form": "난 아직못먹음ㅋ",
          "original_form": "난 아직못먹음ㅋ",
          "speaker_id": "1",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012921.1.13",
          "form": "이벤트한다고ㅋㅋ",
          "original_form": "이벤트한다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012921.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 이벤트에 진심이네 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 이벤트에 진심이네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.15",
          "form": "룰렛이벤트있는데 실패ㅋ",
          "original_form": "룰렛이벤트있는데 실패ㅋ",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.16",
          "form": "집에 놔두고 오고싶더라 캤는데.",
          "original_form": "집에 놔두고 오고싶더라 캤는데.",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.17",
          "form": "나쁜말이라카너ㅡㅡ",
          "original_form": "나쁜말이라카너ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.18",
          "form": "응 어제도 뭐 송금이벤트있었거든?",
          "original_form": "응 어제도 뭐 송금이벤트있었거든?",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.19",
          "form": "ㅋㅋㅋㅋ웃겨ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ웃겨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.20",
          "form": "ㅋㅋㅋㅋㅋ이벤트 뭐 은근 많네 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ이벤트 뭐 은근 많네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.21",
          "form": "송금이벤트하는데 신랑폰이 필요한거야",
          "original_form": "송금이벤트하는데 신랑폰이 필요한거야",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012921.1.22",
          "form": "그래서 두개로 했더니 울신랑 기다리다가 빨리도 이러대?",
          "original_form": "그래서 두개로 했더니 울신랑 기다리다가 빨리도 이러대?",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012921.1.23",
          "form": "잠만 했더니",
          "original_form": "잠만 했더니",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012921.1.24",
          "form": "이래가 얼마벌었는데? 이러는거야",
          "original_form": "이래가 얼마벌었는데? 이러는거야",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012921.1.25",
          "form": "8천원 캤어. 가만히있는데",
          "original_form": "8천원 캤어. 가만히있는데",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012921.1.26",
          "form": "그 표정이 뭐할라고 이런거하노? 이표정.",
          "original_form": "그 표정이 뭐할라고 이런거하노? 이표정.",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012921.1.27",
          "form": "순간 짜증났음.",
          "original_form": "순간 짜증났음.",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012921.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ8천원이면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ8천원이면",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012921.1.29",
          "form": "편위점가서 삼김에 샌드위치 커피 살수있는돈이다 ㅋㅋㅋ",
          "original_form": "편위점가서 삼김에 샌드위치 커피 살수있는돈이다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012921.1.30",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012921.1.31",
          "form": "남자들은 잘 모림 ㅋㅋㅋㅋ",
          "original_form": "남자들은 잘 모림 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012921.1.32",
          "form": "한번씩 현타온다ㅋㅋ",
          "original_form": "한번씩 현타온다ㅋㅋ",
