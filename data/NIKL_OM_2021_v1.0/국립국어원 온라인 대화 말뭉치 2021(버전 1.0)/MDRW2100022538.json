      "utterance": [
        {
          "id": "MDRW2100022538.1.1",
          "form": "한동안 비오더니 이번주말은 해가 잘보여요",
          "original_form": "한동안 비오더니 이번주말은 해가 잘보여요",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100022538.1.2",
          "form": "ㅎㅎㅎ그러게여",
          "original_form": "ㅎㅎㅎ그러게여",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022538.1.3",
          "form": "어제 새벽에도 비가 왔는데",
          "original_form": "어제 새벽에도 비가 왔는데",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022538.1.4",
          "form": "아침엔 쨍쨍하네요",
          "original_form": "아침엔 쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022538.1.5",
          "form": "어제 새벽에 비왔군요? 늦잠자서 몰랐어요",
          "original_form": "어제 새벽에 비왔군요? 늦잠자서 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022538.1.6",
          "form": "전 어제 늦게 자서 알았어요",
          "original_form": "전 어제 늦게 자서 알았어요",
          "speaker_id": "2",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022538.1.7",
          "form": "한 2시 3시쯤 비 오더라구요",
          "original_form": "한 2시 3시쯤 비 오더라구요",
          "speaker_id": "2",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022538.1.8",
          "form": "그러셨구나! 주말이라 한껏 새벽을 즐기셨군요",
          "original_form": "그러셨구나! 주말이라 한껏 새벽을 즐기셨군요",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022538.1.9",
          "form": "핳ㅎㅎㅎㅎ",
          "original_form": "핳ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022538.1.10",
          "form": "맞습니당",
          "original_form": "맞습니당",
          "speaker_id": "2",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022538.1.11",
          "form": "친구랑 산책하고 12시 넘어서 집 들어갔더니",
          "original_form": "친구랑 산책하고 12시 넘어서 집 들어갔더니",
          "speaker_id": "2",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022538.1.12",
          "form": "취침이 늦어졌네요ㅎㅎ",
          "original_form": "취침이 늦어졌네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.13",
          "form": "요즘 밤에 산책하기 너무 좋죠...",
          "original_form": "요즘 밤에 산책하기 너무 좋죠...",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.14",
          "form": "친구랑 가까이사시나보다",
          "original_form": "친구랑 가까이사시나보다",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.15",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.16",
          "form": "날씨가 딱 좋더라고요",
          "original_form": "날씨가 딱 좋더라고요",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.17",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.18",
          "form": "1분거리에",
          "original_form": "1분거리에",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.19",
          "form": "살아용ㅋㅋㅋㅋ",
          "original_form": "살아용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.20",
          "form": "우와 부러워요",
          "original_form": "우와 부러워요",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022538.1.21",
          "form": "주변에 산책할만한곳이 있나요?",
          "original_form": "주변에 산책할만한곳이 있나요?",
          "speaker_id": "1",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022538.1.22",
          "form": "아 주변에 공원이 많아서",
          "original_form": "아 주변에 공원이 많아서",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022538.1.23",
          "form": "산책하기엔 좋아요~",
          "original_form": "산책하기엔 좋아요~",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022538.1.24",
          "form": "부러워요!! 요즘 아침 저녁으로 산책하기 좋은 날씨라 저도 산책을 좀 하고싶은데",
          "original_form": "부러워요!! 요즘 아침 저녁으로 산책하기 좋은 날씨라 저도 산책을 좀 하고싶은데",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022538.1.25",
          "form": "갈만한곳이 잘없어서 마트가요",
          "original_form": "갈만한곳이 잘없어서 마트가요",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022538.1.26",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022538.1.27",
          "form": "산책으로 마트가시는구나ㅋㅋㅋㅋ",
          "original_form": "산책으로 마트가시는구나ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022538.1.28",
          "form": "주변에 공원 같은 곳 없나요??",
          "original_form": "주변에 공원 같은 곳 없나요??",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022538.1.29",
          "form": "네ㅋㅋㅋㅋ작은 놀이터밖에 없어요",
          "original_form": "네ㅋㅋㅋㅋ작은 놀이터밖에 없어요",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022538.1.30",
          "form": "아앗...ㅠㅠ",
          "original_form": "아앗...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022538.1.31",
          "form": "공원 있으면 진짜 좋은데",
          "original_form": "공원 있으면 진짜 좋은데",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022538.1.32",
          "form": "너무아쉬울거같아요ㅠㅠ",
          "original_form": "너무아쉬울거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:37"
        }
      ]
