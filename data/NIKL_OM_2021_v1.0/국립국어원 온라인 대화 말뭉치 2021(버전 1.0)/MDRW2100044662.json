      "utterance": [
        {
          "id": "MDRW2100044662.1.1",
          "form": "민초마카롱도 좋아하실텐데",
          "original_form": "민초마카롱도 좋아하실텐데",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044662.1.2",
          "form": "민초 아이스크림의 마카롱버전이에요",
          "original_form": "민초 아이스크림의 마카롱버전이에요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044662.1.3",
          "form": "헉 민초 아이스크림 꽤 입에 맞던데",
          "original_form": "헉 민초 아이스크림 꽤 입에 맞던데",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044662.1.4",
          "form": "민초마카롱도 괜찮을 것 같은걸요?!",
          "original_form": "민초마카롱도 괜찮을 것 같은걸요?!",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044662.1.5",
          "form": "민초마카롱 파는곳이 별로 없는것같긴한데",
          "original_form": "민초마카롱 파는곳이 별로 없는것같긴한데",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044662.1.6",
          "form": "만나면 도전해보셔요711",
          "original_form": "만나면 도전해보셔요711",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044662.1.7",
          "form": "맞아요 민초마카롱 생각보다 만나기가 힘들어서 못 먹어본 것 같아요ㅋㅋㅋ",
          "original_form": "맞아요 민초마카롱 생각보다 만나기가 힘들어서 못 먹어본 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044662.1.8",
          "form": "마카롱 전문점에서 드셔보신거예요?",
          "original_form": "마카롱 전문점에서 드셔보신거예요?",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044662.1.9",
          "form": "학교 앞에 뚱카롱파는곳이 있는데",
          "original_form": "학교 앞에 뚱카롱파는곳이 있는데",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044662.1.10",
          "form": "신기하게 민초맛이 있더라고요!",
          "original_form": "신기하게 민초맛이 있더라고요!",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044662.1.11",
          "form": "대박 뚱카롱집인데다가 민초까지 있다니",
          "original_form": "대박 뚱카롱집인데다가 민초까지 있다니",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044662.1.12",
          "form": "저도 데려가세요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "저도 데려가세요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044662.1.13",
          "form": "어흑 여기정말괜찮은데",
          "original_form": "어흑 여기정말괜찮은데",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044662.1.14",
          "form": "2500원인데 보통 파는뚱카롱보다",
          "original_form": "2500원인데 보통 파는뚱카롱보다",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044662.1.15",
          "form": "더 커요",
          "original_form": "더 커요",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044662.1.16",
          "form": "헐 가성비 진짜 괜찮아보여요.. 너무 부러워요 흑",
          "original_form": "헐 가성비 진짜 괜찮아보여요.. 너무 부러워요 흑",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044662.1.17",
          "form": "요즘엔 자주 안먹지만",
          "original_form": "요즘엔 자주 안먹지만",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044662.1.18",
          "form": "코로나때문에 사라질까 두려운곳이엥ᆢ",
          "original_form": "코로나때문에 사라질까 두려운곳이엥ᆢ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044662.1.19",
          "form": "아 맞아요 요즘 코로나때문에 식당들 많이 사라지던데",
          "original_form": "아 맞아요 요즘 코로나때문에 식당들 많이 사라지던데",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044662.1.20",
          "form": "우리의 뚱카롱집은 굳건했으면 좋겠어요ㅜ",
          "original_form": "우리의 뚱카롱집은 굳건했으면 좋겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044662.1.21",
          "form": "맞아요ㅜㅠ 안사라질것같은곳도",
          "original_form": "맞아요ㅜㅠ 안사라질것같은곳도",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044662.1.22",
          "form": "사라지더라고요",
          "original_form": "사라지더라고요",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044662.1.23",
          "form": "뚱카롱집 단어가 뭔가 귀여워요",
          "original_form": "뚱카롱집 단어가 뭔가 귀여워요",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044662.1.24",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044662.1.25",
          "form": "통통해보야요ㅋㅋㅋㅋㅋ",
          "original_form": "통통해보야요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044662.1.26",
          "form": "일반 마카롱은 싫어하시나요?",
          "original_form": "일반 마카롱은 싫어하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044662.1.27",
          "form": "뚱카롱파신가!",
          "original_form": "뚱카롱파신가!",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044662.1.28",
          "form": "전 뚱카롱파요!",
          "original_form": "전 뚱카롱파요!",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044662.1.29",
          "form": "소용량이 보편화된 디저트계에서 뚱카롱이 그나마 푸짐해보여요",
          "original_form": "소용량이 보편화된 디저트계에서 뚱카롱이 그나마 푸짐해보여요",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044662.1.30",
          "form": "푸짐한 뚱카롱 ㅋㅋㅋㅋㅋㅋ어감 너무 귀여워요",
          "original_form": "푸짐한 뚱카롱 ㅋㅋㅋㅋㅋㅋ어감 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044662.1.31",
          "form": "저도 뚱카롱 먹고 나서 일반 마카롱은 이제 너무 한입거리로 보여요",
          "original_form": "저도 뚱카롱 먹고 나서 일반 마카롱은 이제 너무 한입거리로 보여요",
          "speaker_id": "2",
          "time": "20210907 23:49"
        }
      ]
