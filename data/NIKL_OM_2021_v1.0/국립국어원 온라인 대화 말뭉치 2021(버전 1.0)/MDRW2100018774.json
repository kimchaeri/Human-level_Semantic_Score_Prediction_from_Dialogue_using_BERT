      "utterance": [
        {
          "id": "MDRW2100018774.1.1",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018774.1.2",
          "form": "오래가시네여...",
          "original_form": "오래가시네여...",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018774.1.3",
          "form": "저희는 집 분위기가 그렇게 못끌고가고",
          "original_form": "저희는 집 분위기가 그렇게 못끌고가고",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018774.1.4",
          "form": "얼마안돼서 화해하거나 그냥 다음날 모른척떠들어요",
          "original_form": "얼마안돼서 화해하거나 그냥 다음날 모른척떠들어요",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018774.1.5",
          "form": "ㅋㅋㅋㅋㅋ저희도 원래 그랬는데",
          "original_form": "ㅋㅋㅋㅋㅋ저희도 원래 그랬는데",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018774.1.6",
          "form": "그땐 둘다 확 예민해져서 그랬나봐요 ㅋㅋㅋㅋ",
          "original_form": "그땐 둘다 확 예민해져서 그랬나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018774.1.7",
          "form": "지금은 잘지내고 있어요 ㅎㅎ",
          "original_form": "지금은 잘지내고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018774.1.8",
          "form": "제가 아는 사람도 한번 싸운게 오래가서 같은 방 쓰면서도 말도 안섞었다고 하더라구요",
          "original_form": "제가 아는 사람도 한번 싸운게 오래가서 같은 방 쓰면서도 말도 안섞었다고 하더라구요",
          "speaker_id": "2",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018774.1.9",
          "form": "결국 언니가 결혼하고 조카가 생기면서 풀렸다고 ㅎㅎ",
          "original_form": "결국 언니가 결혼하고 조카가 생기면서 풀렸다고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018774.1.10",
          "form": "ㅋㅋㅋ저희도 강아지키우게되면서",
          "original_form": "ㅋㅋㅋ저희도 강아지키우게되면서",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018774.1.11",
          "form": "강아지 얘기하고 풀렸어요 ㅎㅎ",
          "original_form": "강아지 얘기하고 풀렸어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018774.1.12",
          "form": "아 강아지 넘 좋죠",
          "original_form": "아 강아지 넘 좋죠",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018774.1.13",
          "form": "그땐 강아지가 애기여서 뭘해도 귀여울 시기였고든요 ㅎㅎ",
          "original_form": "그땐 강아지가 애기여서 뭘해도 귀여울 시기였고든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018774.1.14",
          "form": "가정의 평화를 주는 ㅋㅋㅋ",
          "original_form": "가정의 평화를 주는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018774.1.15",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018774.1.16",
          "form": "가만히 있어도 너무 귀엽네용 ㅎㅎ",
          "original_form": "가만히 있어도 너무 귀엽네용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018774.1.17",
          "form": "강아지는 몇살이에요?",
          "original_form": "강아지는 몇살이에요?",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018774.1.18",
          "form": "강아지는 5살이에요!!",
          "original_form": "강아지는 5살이에요!!",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018774.1.19",
          "form": "으앋 너무 귀엽겠다",
          "original_form": "으앋 너무 귀엽겠다",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018774.1.20",
          "form": "심심이는 왜 사진 공유가 안될까요",
          "original_form": "심심이는 왜 사진 공유가 안될까요",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018774.1.21",
          "form": "여기서 반려동물 얘기 들을때마다 너무 궁금해요",
          "original_form": "여기서 반려동물 얘기 들을때마다 너무 궁금해요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018774.1.22",
          "form": "그러게요..",
          "original_form": "그러게요..",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018774.1.23",
          "form": "저도 사진만 줄수있다면",
          "original_form": "저도 사진만 줄수있다면",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018774.1.24",
          "form": "벌써 여기저기 뿌리고 다녔을텐데..",
          "original_form": "벌써 여기저기 뿌리고 다녔을텐데..",
          "speaker_id": "1",
          "time": "20210827 12:07"
        }
      ]
    }
