      "utterance": [
        {
          "id": "MDRW2100018581.1.1",
          "form": "다이어트 중인데 쉽지않네요ㅠ",
          "original_form": "다이어트 중인데 쉽지않네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018581.1.2",
          "form": "저도 평생해요",
          "original_form": "저도 평생해요",
          "speaker_id": "1",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018581.1.3",
          "form": "그런 마음으로 운동해야겠어요",
          "original_form": "그런 마음으로 운동해야겠어요",
          "speaker_id": "2",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018581.1.4",
          "form": "단기로보니 안되네요",
          "original_form": "단기로보니 안되네요",
          "speaker_id": "2",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018581.1.5",
          "form": "저도 단기로하다 ㅜㅜ",
          "original_form": "저도 단기로하다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018581.1.6",
          "form": "막심하게요",
          "original_form": "막심하게요",
          "speaker_id": "1",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018581.1.7",
          "form": "다시 요요로 엄청 쪘는데",
          "original_form": "다시 요요로 엄청 쪘는데",
          "speaker_id": "1",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018581.1.8",
          "form": "찰모가 왔오요",
          "original_form": "찰모가 왔오요",
          "speaker_id": "1",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018581.1.9",
          "form": "탈모가",
          "original_form": "탈모가",
          "speaker_id": "1",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018581.1.10",
          "form": "굶거나 극단적 시사제한은 안되겠더라고요",
          "original_form": "굶거나 극단적 시사제한은 안되겠더라고요",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.11",
          "form": "다이어트는?언제든 하는데",
          "original_form": "다이어트는?언제든 하는데",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.12",
          "form": "아ㅠ 맞아요ㅠ 피부도 뒤집히고..",
          "original_form": "아ㅠ 맞아요ㅠ 피부도 뒤집히고..",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.13",
          "form": "몸도 나빠지더라구요",
          "original_form": "몸도 나빠지더라구요",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.14",
          "form": "살은 빠지는데",
          "original_form": "살은 빠지는데",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.15",
          "form": "탈모는 못고쳐요",
          "original_form": "탈모는 못고쳐요",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.16",
          "form": "힘도 업사ㅡ",
          "original_form": "힘도 업사ㅡ",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.17",
          "form": "없고",
          "original_form": "없고",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.18",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.19",
          "form": "탈모.. 진짜 관리 잘해야겠어요ㅠ",
          "original_form": "탈모.. 진짜 관리 잘해야겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.20",
          "form": "저도 맨날 무기력했어요",
          "original_form": "저도 맨날 무기력했어요",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.21",
          "form": "여성탈모도 빨리 시작한다더라구요",
          "original_form": "여성탈모도 빨리 시작한다더라구요",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018581.1.22",
          "form": "네 절대 굶거나 하지 마세요",
          "original_form": "네 절대 굶거나 하지 마세요",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018581.1.23",
          "form": "탈모는 불치병이예요",
          "original_form": "탈모는 불치병이예요",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018581.1.24",
          "form": "넵 명심할게요!",
          "original_form": "넵 명심할게요!",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018581.1.25",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018581.1.26",
          "form": "이번에는 꾸준히해서 예쁜 옷 사입어야겠어요",
          "original_form": "이번에는 꾸준히해서 예쁜 옷 사입어야겠어요",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018581.1.27",
          "form": "네 이쁜옷 사입으세요",
          "original_form": "네 이쁜옷 사입으세요",
          "speaker_id": "1",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100018581.1.28",
          "form": "다욧은 언제나 홧팅요",
          "original_form": "다욧은 언제나 홧팅요",
          "speaker_id": "1",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100018581.1.29",
          "form": "전 유산균 꼭먹어요",
          "original_form": "전 유산균 꼭먹어요",
          "speaker_id": "1",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100018581.1.30",
          "form": "다욧하면",
          "original_form": "다욧하면",
          "speaker_id": "1",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100018581.1.31",
          "form": "변비 때문에요",
          "original_form": "변비 때문에요",
          "speaker_id": "1",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100018581.1.32",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210827 09:09"
        },
        {
          "id": "MDRW2100018581.1.33",
          "form": "저도요 안먹다가 변비가 생겨서 먹고 있어요",
          "original_form": "저도요 안먹다가 변비가 생겨서 먹고 있어요",
          "speaker_id": "2",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018581.1.34",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 09:10"
        }
      ]
