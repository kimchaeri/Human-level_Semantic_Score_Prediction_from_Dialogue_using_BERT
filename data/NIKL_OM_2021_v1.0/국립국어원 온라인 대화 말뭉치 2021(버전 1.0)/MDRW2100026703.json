      "utterance": [
        {
          "id": "MDRW2100026703.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026703.1.2",
          "form": "여행어디가고싶으세요?",
          "original_form": "여행어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026703.1.3",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026703.1.4",
          "form": "저는 부산가고싶네용ㅎㅎ",
          "original_form": "저는 부산가고싶네용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026703.1.5",
          "form": "오늘 개강했는데 벌써 놀고싶어요..!",
          "original_form": "오늘 개강했는데 벌써 놀고싶어요..!",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026703.1.6",
          "form": "부산 어디용???",
          "original_form": "부산 어디용???",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026703.1.7",
          "form": "언제나 놀고싶죠 ㅋㅋㅋ",
          "original_form": "언제나 놀고싶죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026703.1.8",
          "form": "그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026703.1.9",
          "form": "전에 부산갔을때 기억이 좋아ㅅ서 막연히 그냥 가보고싶네요!ㅋㅋㅋ",
          "original_form": "전에 부산갔을때 기억이 좋아ㅅ서 막연히 그냥 가보고싶네요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026703.1.10",
          "form": "name1은 학교다니시나요?",
          "original_form": "&name1&은 학교다니시나요?",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026703.1.11",
          "form": "아녕 ㅋㅋㅋ 학교 안다닌지 아주 오래됐어요 ㅋㅋ",
          "original_form": "아녕 ㅋㅋㅋ 학교 안다닌지 아주 오래됐어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026703.1.12",
          "form": "아하 그러시군요!ㅋㅋㅋ",
          "original_form": "아하 그러시군요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026703.1.13",
          "form": "저는 대학생이에용!ㅎㅎ",
          "original_form": "저는 대학생이에용!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026703.1.14",
          "form": "네 ㅎㅎ 개강부터 ㅎㅎ 대학생인거같았어요 ㅎㅎ",
          "original_form": "네 ㅎㅎ 개강부터 ㅎㅎ 대학생인거같았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026703.1.15",
          "form": "제가 부산 살아서 ㅎㅎ",
          "original_form": "제가 부산 살아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026703.1.16",
          "form": "부산 놀러오시는분들은",
          "original_form": "부산 놀러오시는분들은",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026703.1.17",
          "form": "어딜가나 궁금해요",
          "original_form": "어딜가나 궁금해요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026703.1.18",
          "form": "아하 부산사시는군요? 부산 추천할만한곳 있나요?",
          "original_form": "아하 부산사시는군요? 부산 추천할만한곳 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026703.1.19",
          "form": "음......글쎄요..?",
          "original_form": "음......글쎄요..?",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026703.1.20",
          "form": "아무래도 학생분이시니까",
          "original_form": "아무래도 학생분이시니까",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026703.1.21",
          "form": "서면이나 남포가면 재미있지않을까요?",
          "original_form": "서면이나 남포가면 재미있지않을까요?",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026703.1.22",
          "form": "아하 그렇군요!",
          "original_form": "아하 그렇군요!",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026703.1.23",
          "form": "종강하면 한번 가봐야겠어요! 서면 가보고싶네요!",
          "original_form": "종강하면 한번 가봐야겠어요! 서면 가보고싶네요!",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026703.1.24",
          "form": "부산 오셨을때 어디가셨어요?",
          "original_form": "부산 오셨을때 어디가셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026703.1.25",
          "form": "전에 방학때 친구들이랑 해운대 갔었어요!",
          "original_form": "전에 방학때 친구들이랑 해운대 갔었어요!",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026703.1.26",
          "form": "해운대 ㅋㅋㅋㅋ 바다 이쁘죠",
          "original_form": "해운대 ㅋㅋㅋㅋ 바다 이쁘죠",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026703.1.27",
          "form": "술 집도 많고 ㅎㅎ",
          "original_form": "술 집도 많고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026703.1.28",
          "form": "맞아요! 재미있게 놀았던 기억이 나네용ㅎㅎ",
          "original_form": "맞아요! 재미있게 놀았던 기억이 나네용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:56"
        }
      ]
