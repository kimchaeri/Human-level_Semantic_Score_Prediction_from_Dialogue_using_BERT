        {
          "id": "MDRW2100012054.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100012054.1.2",
          "form": "저 심즈좋아해요",
          "original_form": "저 심즈좋아해요",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100012054.1.3",
          "form": "아 그렇군요~심즈는 어떤 게임인가요?",
          "original_form": "아 그렇군요~심즈는 어떤 게임인가요?",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012054.1.4",
          "form": "사람사는거랑 똑같아용",
          "original_form": "사람사는거랑 똑같아용",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012054.1.5",
          "form": "아 그렇군요 처음 들어봐요",
          "original_form": "아 그렇군요 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012054.1.6",
          "form": "전 로블록스 좋아하는 아들들때문에 그것만 알아요",
          "original_form": "전 로블록스 좋아하는 아들들때문에 그것만 알아요",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012054.1.7",
          "form": "로블록스는 어떤 게임인가요?",
          "original_form": "로블록스는 어떤 게임인가요?",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012054.1.8",
          "form": "집짓는 게임이예요...",
          "original_form": "집짓는 게임이예요...",
          "speaker_id": "2",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012054.1.9",
          "form": "아 재밌을거 같아요ㅎㅎ",
          "original_form": "아 재밌을거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012054.1.10",
          "form": "모바일겜인가요?",
          "original_form": "모바일겜인가요?",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012054.1.11",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100012054.1.12",
          "form": "요즘 초등학생들은 좋아하더라구요",
          "original_form": "요즘 초등학생들은 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100012054.1.13",
          "form": "요즘 초등학생들 모바일겜 엄청 좋아하죠?",
          "original_form": "요즘 초등학생들 모바일겜 엄청 좋아하죠?",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012054.1.14",
          "form": "길가다보면 다들 휴대폰보고 있더라구요",
          "original_form": "길가다보면 다들 휴대폰보고 있더라구요",
          "speaker_id": "1",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012054.1.15",
          "form": "네 그렇죠~유튜브가 키운 세대잖아요ㅜ",
          "original_form": "네 그렇죠~유튜브가 키운 세대잖아요ㅜ",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012054.1.16",
          "form": "요즘 인기게임은 무엇인가요?",
          "original_form": "요즘 인기게임은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210828 05:16"
        },
        {
          "id": "MDRW2100012054.1.17",
          "form": "저도 겜을 잘 안하는데 애들은 마인크래프트 같은거 하더라구요",
          "original_form": "저도 겜을 잘 안하는데 애들은 마인크래프트 같은거 하더라구요",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100012054.1.18",
          "form": "마인크래프트가 인기가 많군요",
