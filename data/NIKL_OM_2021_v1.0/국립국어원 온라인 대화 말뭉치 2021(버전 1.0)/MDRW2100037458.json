      "utterance": [
        {
          "id": "MDRW2100037458.1.1",
          "form": "오로바시는 근데",
          "original_form": "오로바시는 근데",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037458.1.2",
          "form": "이미 죽었잖아",
          "original_form": "이미 죽었잖아",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037458.1.3",
          "form": "죽은 오로바시가 산 여자를 쫒는다임?",
          "original_form": "죽은 오로바시가 산 여자를 쫒는다임?",
          "speaker_id": "2",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100037458.1.4",
          "form": "억케 가능하지",
          "original_form": "억케 가능하지",
          "speaker_id": "2",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100037458.1.5",
          "form": "ㄴㄴ 쟤는 오로바시가 아니라",
          "original_form": "ㄴㄴ 쟤는 오로바시가 아니라",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100037458.1.6",
          "form": "어린애가 살던 마을의 촌장임...",
          "original_form": "어린애가 살던 마을의 촌장임...",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100037458.1.7",
          "form": "아니 쟤가",
          "original_form": "아니 쟤가",
          "speaker_id": "2",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100037458.1.8",
          "form": "오로바시 이래봬도 착한 마신이라서",
          "original_form": "오로바시 이래봬도 착한 마신이라서",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100037458.1.9",
          "form": "말하는 게",
          "original_form": "말하는 게",
          "speaker_id": "2",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100037458.1.10",
          "form": "산제물 안받을걸",
          "original_form": "산제물 안받을걸",
          "speaker_id": "1",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100037458.1.11",
          "form": "눈 18개에 독니가 있다잖아",
          "original_form": "눈 18개에 독니가 있다잖아",
          "speaker_id": "2",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100037458.1.12",
          "form": "독니가 있으면 오로바시 아니야?",
          "original_form": "독니가 있으면 오로바시 아니야?",
          "speaker_id": "2",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100037458.1.13",
          "form": "오로바시가 죽은 땅에 달리 잡 마신이 살것같진 않은데",
          "original_form": "오로바시가 죽은 땅에 달리 잡 마신이 살것같진 않은데",
          "speaker_id": "2",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100037458.1.14",
          "form": "나 걍 라이덴한테 도망가는 주인공얘긴가? 했더니",
          "original_form": "나 걍 라이덴한테 도망가는 주인공얘긴가? 했더니",
          "speaker_id": "2",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100037458.1.15",
          "form": "눈이 18개에 독니가 있고 입이 셀수없다는거야",
          "original_form": "눈이 18개에 독니가 있고 입이 셀수없다는거야",
          "speaker_id": "2",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100037458.1.16",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100037458.1.17",
          "form": "쟤가 원래 마을 촌장인데",
          "original_form": "쟤가 원래 마을 촌장인데",
          "speaker_id": "1",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100037458.1.18",
          "form": "아니 마을촌장이 왜",
          "original_form": "아니 마을촌장이 왜",
          "speaker_id": "2",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100037458.1.19",
          "form": "산호궁이랑 라이덴 싸우는 와중에 피란민을 받았고",
          "original_form": "산호궁이랑 라이덴 싸우는 와중에 피란민을 받았고",
          "speaker_id": "1",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100037458.1.20",
          "form": "일본전통 귀면무사가 된것이지",
          "original_form": "일본전통 귀면무사가 된것이지",
          "speaker_id": "2",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100037458.1.21",
          "form": "아 좀 자세히 읽을걸",
          "original_form": "아 좀 자세히 읽을걸",
          "speaker_id": "2",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100037458.1.22",
          "form": "저 감실은 재앙신 근처에 있다고 다들 안가려고 해서",
          "original_form": "저 감실은 재앙신 근처에 있다고 다들 안가려고 해서",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.23",
          "form": "그리고 그런애한테 기도해서 보물상자 얻는게",
          "original_form": "그리고 그런애한테 기도해서 보물상자 얻는게",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.24",
          "form": "저사람이 가있던것",
          "original_form": "저사람이 가있던것",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.25",
          "form": "맞는 일인가....?",
          "original_form": "맞는 일인가....?",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.26",
          "form": "근데 마을 사람들이 하나씩 미쳐가면서",
          "original_form": "근데 마을 사람들이 하나씩 미쳐가면서",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.27",
          "form": "재앙신이 오로바시 아니야?",
          "original_form": "재앙신이 오로바시 아니야?",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.28",
          "form": "재앙신의 목소리를 들었다고 다른 사람을 공격하려고 들음",
          "original_form": "재앙신의 목소리를 들었다고 다른 사람을 공격하려고 들음",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.29",
          "form": "재앙신은 따로있어?",
          "original_form": "재앙신은 따로있어?",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.30",
          "form": "오로바시일걸?",
          "original_form": "오로바시일걸?",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037458.1.31",
          "form": "전쟁으로 주술도구가 파괴되면서",
          "original_form": "전쟁으로 주술도구가 파괴되면서",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037458.1.32",
          "form": "재앙신의 힘이 흘러나와서 오염된? 느낌인듯",
          "original_form": "재앙신의 힘이 흘러나와서 오염된? 느낌인듯",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037458.1.33",
          "form": "오로바시는 산호궁주가 모시던 애니까 선한쪽이긴 할텐데",
          "original_form": "오로바시는 산호궁주가 모시던 애니까 선한쪽이긴 할텐데",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037458.1.34",
          "form": "그럼 저 촌장은 대체",
          "original_form": "그럼 저 촌장은 대체",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037458.1.35",
          "form": "뭐에 씌인거지",
          "original_form": "뭐에 씌인거지",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037458.1.36",
          "form": "걍 독기때문에 미친건가?",
          "original_form": "걍 독기때문에 미친건가?",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037458.1.37",
          "form": "ㅇㅇ 독기때문에 마을 사람들이랑 같이 미쳐버린듯",
          "original_form": "ㅇㅇ 독기때문에 마을 사람들이랑 같이 미쳐버린듯",
          "speaker_id": "1",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037458.1.38",
          "form": "ㅇㅎ....그럼 그 꼬맹이는",
          "original_form": "ㅇㅎ....그럼 그 꼬맹이는",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037458.1.39",
          "form": "왜 멀쩡하지?",
          "original_form": "왜 멀쩡하지?",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037458.1.40",
          "form": "엄마가 감실에 벚꽃수구 뿌리고 기원해서?",
          "original_form": "엄마가 감실에 벚꽃수구 뿌리고 기원해서?",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037458.1.41",
          "form": "그럴지도?",
          "original_form": "그럴지도?",
          "speaker_id": "1",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037458.1.42",
          "form": "근데 엄마도 멀쩡했던거같음",
          "original_form": "근데 엄마도 멀쩡했던거같음",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037458.1.43",
          "form": "그럼 기도하는건",
          "original_form": "그럼 기도하는건",
          "speaker_id": "2",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037458.1.44",
          "form": "무슨 의미가 있는거지...?",
          "original_form": "무슨 의미가 있는거지...?",
          "speaker_id": "2",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037458.1.45",
          "form": "나중에 다시 마을로 가면 촌장의 일지?가 있는데",
          "original_form": "나중에 다시 마을로 가면 촌장의 일지?가 있는데",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037458.1.46",
          "form": "일종의 산제물 공양임",
          "original_form": "일종의 산제물 공양임",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037458.1.47",
          "form": "기도가???",
          "original_form": "기도가???",
          "speaker_id": "2",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037458.1.48",
          "form": "저 상자는 주인공이 꼬박꼬박 찾아오게 유인하는 용인거같고",
          "original_form": "저 상자는 주인공이 꼬박꼬박 찾아오게 유인하는 용인거같고",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037458.1.49",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.50",
          "form": "그래서 내일",
          "original_form": "그래서 내일",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.51",
          "form": "싸우는거야?",
          "original_form": "싸우는거야?",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.52",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.53",
          "form": "날 산제물로",
          "original_form": "날 산제물로",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.54",
          "form": "바치겠다고?",
          "original_form": "바치겠다고?",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.55",
          "form": "이거 안되겠네",
          "original_form": "이거 안되겠네",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.56",
          "form": "불도장 맛을 봐라",
          "original_form": "불도장 맛을 봐라",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.57",
          "form": "불도장 이제 재료만 있으면 대량생산 가능하다",
          "original_form": "불도장 이제 재료만 있으면 대량생산 가능하다",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037458.1.58",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100037458.1.59",
          "form": "전투하고나서 마을가면",
          "original_form": "전투하고나서 마을가면",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100037458.1.60",
          "form": "주민 명부 있는데",
          "original_form": "주민 명부 있는데",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100037458.1.61",
          "form": "처음에는",
          "original_form": "처음에는",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100037458.1.62",
          "form": "000 21세 병사, 0000 43세 병사,",
          "original_form": "000 21세 병사, 0000 43세 병사,",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037458.1.63",
          "form": "이런식이다가",
          "original_form": "이런식이다가",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037458.1.64",
          "form": "어느 순간부터",
          "original_form": "어느 순간부터",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
