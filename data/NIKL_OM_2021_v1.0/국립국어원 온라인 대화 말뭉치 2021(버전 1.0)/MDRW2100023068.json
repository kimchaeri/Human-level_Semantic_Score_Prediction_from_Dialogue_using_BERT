      "utterance": [
        {
          "id": "MDRW2100023068.1.1",
          "form": "몰라....그시절엔 또렷하게 들렸는데",
          "original_form": "몰라....그시절엔 또렷하게 들렸는데",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023068.1.2",
          "form": "보통 앨범사진 풍화된 이미지로 올라와 있는 음원들이",
          "original_form": "보통 앨범사진 풍화된 이미지로 올라와 있는 음원들이",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023068.1.3",
          "form": "음질이 좀 낮더라",
          "original_form": "음질이 좀 낮더라",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023068.1.4",
          "form": "다시 들어보면 뭔가 좀 파열음 심하고",
          "original_form": "다시 들어보면 뭔가 좀 파열음 심하고",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.5",
          "form": "근데 그거말고는 이제",
          "original_form": "근데 그거말고는 이제",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.6",
          "form": "찾을수가없어......",
          "original_form": "찾을수가없어......",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.7",
          "form": "그동안 우리가 깨끗한 음질에 많이 익숙해진듯",
          "original_form": "그동안 우리가 깨끗한 음질에 많이 익숙해진듯",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.8",
          "form": "ㅇㅇ그것도 그런듯",
          "original_form": "ㅇㅇ그것도 그런듯",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.9",
          "form": "영상도 그때는 전혀 이상한거 못느꼈는데",
          "original_form": "영상도 그때는 전혀 이상한거 못느꼈는데",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.10",
          "form": "이제 브라운관 티비로는",
          "original_form": "이제 브라운관 티비로는",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.11",
          "form": "다시 틀면 디지털풍화 장난 아니잖아",
          "original_form": "다시 틀면 디지털풍화 장난 아니잖아",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.12",
          "form": "다시 뭐 못볼거야",
          "original_form": "다시 뭐 못볼거야",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.13",
          "form": "ㅇㅇ그때의 기술로는 다시 돌아갈 수 없음....",
          "original_form": "ㅇㅇ그때의 기술로는 다시 돌아갈 수 없음....",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023068.1.14",
          "form": "ㅇㅇ....예능같은것도",
          "original_form": "ㅇㅇ....예능같은것도",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023068.1.15",
          "form": "방송사에서 틀어주는건",
          "original_form": "방송사에서 틀어주는건",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023068.1.16",
          "form": "원본이라 디지털 풍화 안일어났을텐데",
          "original_form": "원본이라 디지털 풍화 안일어났을텐데",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023068.1.17",
          "form": "화질구지",
          "original_form": "화질구지",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023068.1.18",
          "form": "엄청 심해",
          "original_form": "엄청 심해",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023068.1.19",
          "form": "ㅇㅇ 원본인데 그래서 좀 충굑",
          "original_form": "ㅇㅇ 원본인데 그래서 좀 충굑",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023068.1.20",
          "form": "충격",
          "original_form": "충격",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023068.1.21",
          "form": "기술이 발달한건 알앗지만 일케 체감하고 싶진 않앗는데...",
          "original_form": "기술이 발달한건 알앗지만 일케 체감하고 싶진 않앗는데...",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023068.1.22",
          "form": "ㅇㅇ......",
          "original_form": "ㅇㅇ......",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023068.1.23",
          "form": "무도같은것도 가끔",
          "original_form": "무도같은것도 가끔",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023068.1.24",
          "form": "채널 돌리다보면 옛날방송 재방나오는데",
          "original_form": "채널 돌리다보면 옛날방송 재방나오는데",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023068.1.25",
          "form": "일단 화면 비율부터가",
          "original_form": "일단 화면 비율부터가",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023068.1.26",
          "form": "충격적이야",
          "original_form": "충격적이야",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023068.1.27",
          "form": "뭔가 사람들이 찌부되어있어",
          "original_form": "뭔가 사람들이 찌부되어있어",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023068.1.28",
          "form": "아 맞아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023068.1.29",
          "form": "뭔가 가로세로가 안맞아",
          "original_form": "뭔가 가로세로가 안맞아",
          "speaker_id": "1",
          "time": "20210829 21:23"
        }
      ]
    }
