      "utterance": [
        {
          "id": "MDRW2100008419.1.1",
          "form": "혹시 우유 좋아하세요?",
          "original_form": "혹시 우유 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008419.1.2",
          "form": "네 저는 우유 좋아해요 근데 키는 안컸네요",
          "original_form": "네 저는 우유 좋아해요 근데 키는 안컸네요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008419.1.3",
          "form": "우유 먹는다고 다 키가 큰건 아니니까요",
          "original_form": "우유 먹는다고 다 키가 큰건 아니니까요",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008419.1.4",
          "form": "그럼 세상에서 제일 긴 동물은",
          "original_form": "그럼 세상에서 제일 긴 동물은",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008419.1.5",
          "form": "그니까요 인생은 불합리해요",
          "original_form": "그니까요 인생은 불합리해요",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008419.1.6",
          "form": "기린이 아니라 젖소가 되었을텐대",
          "original_form": "기린이 아니라 젖소가 되었을텐대",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008419.1.7",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008419.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008419.1.9",
          "form": "풀만 먹어도 살찌는 동물도 잇는데 인생은 불공평해",
          "original_form": "풀만 먹어도 살찌는 동물도 잇는데 인생은 불공평해",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008419.1.10",
          "form": "가장 좋아하는 음료수는? 두두둥탁",
          "original_form": "가장 좋아하는 음료수는? 두두둥탁",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008419.1.11",
          "form": "요새 제일 맛있게 먹은 음식은?",
          "original_form": "요새 제일 맛있게 먹은 음식은?",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008419.1.12",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008419.1.13",
          "form": "저는 어제 먹은 탄탄면 맛있게 먹었어요",
          "original_form": "저는 어제 먹은 탄탄면 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008419.1.14",
          "form": "오늘 파스타 먹기로 했는데",
          "original_form": "오늘 파스타 먹기로 했는데",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008419.1.15",
          "form": "그거 못먹은건 조금 아쉽네요",
          "original_form": "그거 못먹은건 조금 아쉽네요",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008419.1.16",
          "form": "탄탄면 먹으면 탄탄해지나~?",
          "original_form": "탄탄면 먹으면 탄탄해지나~?",
