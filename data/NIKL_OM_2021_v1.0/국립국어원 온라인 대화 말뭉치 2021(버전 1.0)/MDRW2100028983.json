      "utterance": [
        {
          "id": "MDRW2100028983.1.1",
          "form": "환절기입니다",
          "original_form": "환절기입니다",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028983.1.2",
          "form": "비염은 어떠신가요",
          "original_form": "비염은 어떠신가요",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028983.1.3",
          "form": "춥슴니다",
          "original_form": "춥슴니다",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028983.1.4",
          "form": "아직은 버틸만합니다",
          "original_form": "아직은 버틸만합니다",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028983.1.5",
          "form": "님은여",
          "original_form": "님은여",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028983.1.6",
          "form": "전기장판 꺼내셨나요?",
          "original_form": "전기장판 꺼내셨나요?",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028983.1.7",
          "form": "저는 아직 괜찮아요",
          "original_form": "저는 아직 괜찮아요",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.8",
          "form": "아직 선풍기랑 작별인사를 못했슴니다",
          "original_form": "아직 선풍기랑 작별인사를 못했슴니다",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.9",
          "form": "사실 샤워하고 나면 더워서 선풍기 킵니다",
          "original_form": "사실 샤워하고 나면 더워서 선풍기 킵니다",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.10",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.11",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.12",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.13",
          "form": "선풍기는 아직 켜야 합니다",
          "original_form": "선풍기는 아직 켜야 합니다",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.14",
          "form": "가을모기 무서워요",
          "original_form": "가을모기 무서워요",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.15",
          "form": "가디건 챙기나요?",
          "original_form": "가디건 챙기나요?",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028983.1.16",
          "form": "아니요 가디건 사야합니다",
          "original_form": "아니요 가디건 사야합니다",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028983.1.17",
          "form": "요새 모기 정말 많더라구요",
          "original_form": "요새 모기 정말 많더라구요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028983.1.18",
          "form": "여름에는 없었는데",
          "original_form": "여름에는 없었는데",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028983.1.19",
          "form": "비올 때 가디건 입으시는지...?",
          "original_form": "비올 때 가디건 입으시는지...?",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028983.1.20",
          "form": "가을모기가 그리 무섭다고..",
          "original_form": "가을모기가 그리 무섭다고..",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028983.1.21",
          "form": "이제 챙길려구여",
          "original_form": "이제 챙길려구여",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028983.1.22",
          "form": "가디건 꺼내야 함니댜",
          "original_form": "가디건 꺼내야 함니댜",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028983.1.23",
          "form": "후리스는 언제 꺼내나요?",
          "original_form": "후리스는 언제 꺼내나요?",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028983.1.24",
          "form": "더 추워지면?",
          "original_form": "더 추워지면?",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028983.1.25",
          "form": "후리스는....무리",
          "original_form": "후리스는....무리",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028983.1.26",
          "form": "네넹 더 추워지면",
          "original_form": "네넹 더 추워지면",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028983.1.27",
          "form": "가을옷 장만 하셨나오",
          "original_form": "가을옷 장만 하셨나오",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028983.1.28",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028983.1.29",
          "form": "가을에도 여름옷을 입을 것 같습니다",
          "original_form": "가을에도 여름옷을 입을 것 같습니다",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028983.1.30",
          "form": "네....?",
          "original_form": "네....?",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028983.1.31",
          "form": "가을옷 뭐 사면 좋을까오?",
          "original_form": "가을옷 뭐 사면 좋을까오?",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028983.1.32",
          "form": "자켓이오",
          "original_form": "자켓이오",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028983.1.33",
          "form": "자켓이 무난하죠",
          "original_form": "자켓이 무난하죠",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028983.1.34",
          "form": "해링본 자켓인가요...?",
          "original_form": "해링본 자켓인가요...?",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028983.1.35",
          "form": "헤링본 자켓 좋져ㅕㅕ",
          "original_form": "헤링본 자켓 좋져ㅕㅕ",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028983.1.36",
          "form": "저는 이번에 트위드 자켓 장만 했슴니다",
          "original_form": "저는 이번에 트위드 자켓 장만 했슴니다",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028983.1.37",
          "form": "가을 기다립니댜",
          "original_form": "가을 기다립니댜",
          "speaker_id": "2",
          "time": "20210902 23:18"
        }
      ]
    }
  ]
