      "utterance": [
        {
          "id": "MDRW2100035417.1.1",
          "form": "아 저기에서 춤춘다 아빠 앞에서",
          "original_form": "아 저기에서 춤춘다 아빠 앞에서",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035417.1.2",
          "form": "아빠한테 혼나는건 아니지?",
          "original_form": "아빠한테 혼나는건 아니지?",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100035417.1.3",
          "form": "아니 저거 보고 아빠가 빌리 발레하게 한다",
          "original_form": "아니 저거 보고 아빠가 빌리 발레하게 한다",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100035417.1.4",
          "form": "아 그래? 잘됐네",
          "original_form": "아 그래? 잘됐네",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100035417.1.5",
          "form": "엉 그래서 아내 반지도 전당포에 맡기고 다시 일하러 감",
          "original_form": "엉 그래서 아내 반지도 전당포에 맡기고 다시 일하러 감",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100035417.1.6",
          "form": "애 보고 정신 차렸네",
          "original_form": "애 보고 정신 차렸네",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035417.1.7",
          "form": "큰아들이 뭐라고 하니까 빌리도 우리처럼 살아야하냐고 한다",
          "original_form": "큰아들이 뭐라고 하니까 빌리도 우리처럼 살아야하냐고 한다",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035417.1.8",
          "form": "그래 맞는 말이다",
          "original_form": "그래 맞는 말이다",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035417.1.9",
          "form": "응 2천파운드면 우리나라 돈으로 얼마고",
          "original_form": "응 2천파운드면 우리나라 돈으로 얼마고",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035417.1.10",
          "form": "320만원이네",
          "original_form": "320만원이네",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100035417.1.11",
          "form": "1파운드에 1600원",
          "original_form": "1파운드에 1600원",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100035417.1.12",
          "form": "에궁 저돈이 없는거네",
          "original_form": "에궁 저돈이 없는거네",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100035417.1.13",
          "form": "이제 일하는거니깐",
          "original_form": "이제 일하는거니깐",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100035417.1.14",
          "form": "그래서 배신자라고 하는거네?",
          "original_form": "그래서 배신자라고 하는거네?",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100035417.1.15",
          "form": "응 노조해서 자기네들 권리를 찾자고 하기로 했는데",
          "original_form": "응 노조해서 자기네들 권리를 찾자고 하기로 했는데",
          "speaker_id": "2",
          "time": "20210829 16:44"
