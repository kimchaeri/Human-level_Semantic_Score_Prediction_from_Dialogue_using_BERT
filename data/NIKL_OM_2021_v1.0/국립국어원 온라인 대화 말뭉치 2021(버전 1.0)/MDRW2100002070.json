      "utterance": [
        {
          "id": "MDRW2100002070.1.1",
          "form": "저녁 뭐드셨나용",
          "original_form": "저녁 뭐드셨나용",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002070.1.2",
          "form": "쭈꾸미 볶음이요",
          "original_form": "쭈꾸미 볶음이요",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002070.1.3",
          "form": "name2님은 뭐 드셨나요",
          "original_form": "&name2&님은 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002070.1.4",
          "form": "오 전 어제쭈꾸미 먹었어요",
          "original_form": "오 전 어제쭈꾸미 먹었어요",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002070.1.5",
          "form": "오늘은 그냥 무국이랑 젓갈요",
          "original_form": "오늘은 그냥 무국이랑 젓갈요",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002070.1.6",
          "form": "무국 맛있겠다",
          "original_form": "무국 맛있겠다",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002070.1.7",
          "form": "저 무요리 되게 좋아하거든요",
          "original_form": "저 무요리 되게 좋아하거든요",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002070.1.8",
          "form": "젓갈은 무슨 젓갈 드셨어요",
          "original_form": "젓갈은 무슨 젓갈 드셨어요",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002070.1.9",
          "form": "오 ㅎㅎ 창란젓 먹었어요 맛나요 !",
          "original_form": "오 ㅎㅎ 창란젓 먹었어요 맛나요 !",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002070.1.10",
          "form": "맛있는거 드셨다.. 맛있겠다..",
          "original_form": "맛있는거 드셨다.. 맛있겠다..",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002070.1.11",
          "form": "좋아하는 반찬 뭐 있어요?",
          "original_form": "좋아하는 반찬 뭐 있어요?",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002070.1.12",
          "form": "저는 다좋아하는데 해산물류 좋아해요",
          "original_form": "저는 다좋아하는데 해산물류 좋아해요",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002070.1.13",
          "form": "안가리고 다먹어용 ㅎㅎ",
          "original_form": "안가리고 다먹어용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002070.1.14",
          "form": "멍게 해삼 이런것두요?",
          "original_form": "멍게 해삼 이런것두요?",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002070.1.15",
          "form": "저도 해산물 좋아하는데 멍게 해삼은 음 어렵더라구요",
          "original_form": "저도 해산물 좋아하는데 멍게 해삼은 음 어렵더라구요",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002070.1.16",
          "form": "네네 먹어요 ㅎㅎ 낙지 개불이 최애입니당 !!",
          "original_form": "네네 먹어요 ㅎㅎ 낙지 개불이 최애입니당 !!",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002070.1.17",
          "form": "낙지는 저도 좋은데 개불 어..먹어본 적 없어요 무슨 맛인가요",
          "original_form": "낙지는 저도 좋은데 개불 어..먹어본 적 없어요 무슨 맛인가요",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002070.1.18",
          "form": "딱히 맛은 없는데 식감이 넘 꼬들하고좋아요",
          "original_form": "딱히 맛은 없는데 식감이 넘 꼬들하고좋아요",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002070.1.19",
          "form": "참기름맛에 먹어여 ㅋㅋ 맛나요",
          "original_form": "참기름맛에 먹어여 ㅋㅋ 맛나요",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002070.1.20",
          "form": "한 번 도전해봐야겠어요ㅎㅎ",
          "original_form": "한 번 도전해봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002070.1.21",
          "form": "초장이 아니라 참기름이라니",
          "original_form": "초장이 아니라 참기름이라니",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002070.1.22",
          "form": "매력있을거같아요",
          "original_form": "매력있을거같아요",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002070.1.23",
          "form": "네네 맛나요 거부감없이 드실수있을거예요",
          "original_form": "네네 맛나요 거부감없이 드실수있을거예요",
          "speaker_id": "2",
          "time": "20210819 23:48"
        }
