      "utterance": [
        {
          "id": "MDRW2100023336.1.1",
          "form": "그래서요?ㅎㅎ 일매출 30만원ㅎ",
          "original_form": "그래서요?ㅎㅎ 일매출 30만원ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023336.1.2",
          "form": "궁금해용~~",
          "original_form": "궁금해용~~",
          "speaker_id": "2",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023336.1.3",
          "form": "팀장이 3번 바꼇는데",
          "original_form": "팀장이 3번 바꼇는데",
          "speaker_id": "1",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023336.1.4",
          "form": "1번 팀장은  회사가 어려우니 나갔고",
          "original_form": "1번 팀장은  회사가 어려우니 나갔고",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023336.1.5",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023336.1.6",
          "form": "2번 팀장은  저랑 너무 안맞은",
          "original_form": "2번 팀장은  저랑 너무 안맞은",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023336.1.7",
          "form": "우와 단군신화같애ㅎㅎㅎㅎ",
          "original_form": "우와 단군신화같애ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023336.1.8",
          "form": "역사적 이야기ㅎㅎ",
          "original_form": "역사적 이야기ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023336.1.9",
          "form": "ㅡㅡㅋ",
          "original_form": "ㅡㅡㅋ",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023336.1.10",
          "form": "3번은요?",
          "original_form": "3번은요?",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023336.1.11",
          "form": "3번째는 저랑 동갑짜리  팀장이 왓죠",
          "original_form": "3번째는 저랑 동갑짜리  팀장이 왓죠",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.12",
          "form": "ㅎㅎ 죄송요ㅎ",
          "original_form": "ㅎㅎ 죄송요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.13",
          "form": "근데 재밋는게 둘다 애기죠 ㅋㅋㅋ",
          "original_form": "근데 재밋는게 둘다 애기죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.14",
          "form": "동갑이면 능력이 좋은건가요?",
          "original_form": "동갑이면 능력이 좋은건가요?",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.15",
          "form": "그때가 제가 23~4?",
          "original_form": "그때가 제가 23~4?",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.16",
          "form": "ㅎㅎㅎ 열정적이셨군요ㅎ",
          "original_form": "ㅎㅎㅎ 열정적이셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.17",
          "form": "우와ㅎ",
          "original_form": "우와ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.18",
          "form": "엄청 어리렸네요ㅎ",
          "original_form": "엄청 어리렸네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.19",
          "form": "근데 어찌 팀장이 저리 어리나 싶은게",
          "original_form": "근데 어찌 팀장이 저리 어리나 싶은게",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023336.1.20",
          "form": "글죠",
          "original_form": "글죠",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.21",
          "form": "팀장이 너무어렸네요",
          "original_form": "팀장이 너무어렸네요",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.22",
          "form": "과장님이 예전에 같이 일했는데 일잘해서",
          "original_form": "과장님이 예전에 같이 일했는데 일잘해서",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.23",
          "form": "스카웃",
          "original_form": "스카웃",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.24",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.25",
          "form": "그래서 둘이 으쌰해샤 2위까지 만들고",
          "original_form": "그래서 둘이 으쌰해샤 2위까지 만들고",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.26",
          "form": "역시 똑부러지시니깐",
          "original_form": "역시 똑부러지시니깐",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.27",
          "form": "잘하셔서ㅎ",
          "original_form": "잘하셔서ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.28",
          "form": "이야ㅎ 대단하네요ㅎ",
          "original_form": "이야ㅎ 대단하네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.29",
          "form": "전국2위인거잖아요?",
          "original_form": "전국2위인거잖아요?",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.30",
          "form": "근데 문제는 어리지나여 ㅋ",
          "original_form": "근데 문제는 어리지나여 ㅋ",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.31",
          "form": "팀장님이 대학생엿음 ㅋㅋㅋ",
          "original_form": "팀장님이 대학생엿음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.32",
          "form": "복학 ㅋㅋㅋ",
          "original_form": "복학 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023336.1.33",
          "form": "어리니깐 더 대단한거죠ㅎ",
          "original_form": "어리니깐 더 대단한거죠ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023336.1.34",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023336.1.35",
          "form": "ㅎㅎ 그럴수있나요?",
          "original_form": "ㅎㅎ 그럴수있나요?",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023336.1.36",
          "form": "그럼,알바했었나?ㅎ",
          "original_form": "그럼,알바했었나?ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023336.1.37",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023336.1.38",
          "form": "과장님이 부탁해서",
          "original_form": "과장님이 부탁해서",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023336.1.39",
          "form": "일하다가",
          "original_form": "일하다가",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023336.1.40",
          "form": "다시 본업인 학생으로ㅠ",
          "original_form": "다시 본업인 학생으로ㅠ",
          "speaker_id": "1",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100023336.1.41",
          "form": "햔 1년 반 같이 일한거 같아요",
          "original_form": "햔 1년 반 같이 일한거 같아요",
          "speaker_id": "1",
          "time": "20210830 00:24"
        }
      ]
