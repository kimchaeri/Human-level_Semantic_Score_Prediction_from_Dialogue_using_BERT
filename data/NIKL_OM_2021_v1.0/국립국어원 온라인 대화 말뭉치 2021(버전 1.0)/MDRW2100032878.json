      "utterance": [
        {
          "id": "MDRW2100032878.1.1",
          "form": "라면먹고싶다",
          "original_form": "라면먹고싶다",
          "speaker_id": "1",
          "time": "20210906 03:25"
        },
        {
          "id": "MDRW2100032878.1.2",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210906 03:25"
        },
        {
          "id": "MDRW2100032878.1.3",
          "form": "진짜로...절실하게",
          "original_form": "진짜로...절실하게",
          "speaker_id": "2",
          "time": "20210906 03:25"
        },
        {
          "id": "MDRW2100032878.1.4",
          "form": "불닭좋아해?",
          "original_form": "불닭좋아해?",
          "speaker_id": "2",
          "time": "20210906 03:25"
        },
        {
          "id": "MDRW2100032878.1.5",
          "form": "치즈계란대파송송후춧가르톡고춧가루톡고추장반스푼",
          "original_form": "치즈계란대파송송후춧가르톡고춧가루톡고추장반스푼",
          "speaker_id": "1",
          "time": "20210906 03:25"
        },
        {
          "id": "MDRW2100032878.1.6",
          "form": "넣은 신다면 스으으읍하",
          "original_form": "넣은 신다면 스으으읍하",
          "speaker_id": "1",
          "time": "20210906 03:25"
        },
        {
          "id": "MDRW2100032878.1.7",
          "form": "불닭 좋아하는데 매워서 못머거ㅜ",
          "original_form": "불닭 좋아하는데 매워서 못머거ㅜ",
          "speaker_id": "1",
          "time": "20210906 03:25"
        },
        {
          "id": "MDRW2100032878.1.8",
          "form": "앗ㅋㅋㅋ매운거힘들구만",
          "original_form": "앗ㅋㅋㅋ매운거힘들구만",
          "speaker_id": "2",
          "time": "20210906 03:26"
        },
        {
          "id": "MDRW2100032878.1.9",
          "form": "도토리면인가 그것도맛잇는데",
          "original_form": "도토리면인가 그것도맛잇는데",
          "speaker_id": "2",
          "time": "20210906 03:26"
        },
        {
          "id": "MDRW2100032878.1.10",
          "form": "단종됏을라나ㅠ",
          "original_form": "단종됏을라나ㅠ",
          "speaker_id": "2",
          "time": "20210906 03:26"
        },
        {
          "id": "MDRW2100032878.1.11",
          "form": "도토리....?",
          "original_form": "도토리....?",
          "speaker_id": "1",
          "time": "20210906 03:27"
        },
        {
          "id": "MDRW2100032878.1.12",
          "form": "묵같은 거야?",
          "original_form": "묵같은 거야?",
          "speaker_id": "1",
          "time": "20210906 03:27"
        },
        {
          "id": "MDRW2100032878.1.13",
          "form": "ㄴㄴ비빔면",
          "original_form": "ㄴㄴ비빔면",
          "speaker_id": "2",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032878.1.14",
          "form": "팔도비빔면같은건데 도토리면이라서 쫄깃하고 매콤하고",
          "original_form": "팔도비빔면같은건데 도토리면이라서 쫄깃하고 매콤하고",
          "speaker_id": "2",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032878.1.15",
          "form": "아...라면",
          "original_form": "아...라면",
          "speaker_id": "2",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032878.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032878.1.17",
          "form": "군침도네.....",
          "original_form": "군침도네.....",
          "speaker_id": "1",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032878.1.18",
          "form": "사람 홀려놓고 단종이라니",
          "original_form": "사람 홀려놓고 단종이라니",
          "speaker_id": "1",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032878.1.19",
          "form": "ㅋㅋㅋ예의가아니지",
          "original_form": "ㅋㅋㅋ예의가아니지",
          "speaker_id": "2",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032878.1.20",
          "form": "그리고 오니기리가",
          "original_form": "그리고 오니기리가",
          "speaker_id": "2",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032878.1.21",
          "form": "먹고싶네",
          "original_form": "먹고싶네",
          "speaker_id": "2",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032878.1.22",
          "form": "그게모야?",
          "original_form": "그게모야?",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032878.1.23",
          "form": "오니기리",
          "original_form": "오니기리",
          "speaker_id": "2",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032878.1.24",
          "form": "양배추 계란물 입혀서 굽는 그건가",
          "original_form": "양배추 계란물 입혀서 굽는 그건가",
          "speaker_id": "1",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032878.1.25",
          "form": "일본식 주먹밥!",
          "original_form": "일본식 주먹밥!",
          "speaker_id": "2",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032878.1.26",
          "form": "오니기리집 다망햇나보네...엄마야",
          "original_form": "오니기리집 다망햇나보네...엄마야",
          "speaker_id": "2",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032878.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032878.1.28",
          "form": "내 주변엔 없더라",
          "original_form": "내 주변엔 없더라",
          "speaker_id": "1",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032878.1.29",
          "form": "어디서 먹어?",
          "original_form": "어디서 먹어?",
          "speaker_id": "1",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032878.1.30",
          "form": "이거 오니기리집이 따로있음",
          "original_form": "이거 오니기리집이 따로있음",
          "speaker_id": "2",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032878.1.31",
          "form": "일식집에서!",
          "original_form": "일식집에서!",
          "speaker_id": "2",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032878.1.32",
          "form": "ㅇㅎ ㅇㅎㅇㅎ 그런 일식집을 잘 안 가서 몰랐나",
          "original_form": "ㅇㅎ ㅇㅎㅇㅎ 그런 일식집을 잘 안 가서 몰랐나",
          "speaker_id": "1",
          "time": "20210906 03:33"
        }
      ]
