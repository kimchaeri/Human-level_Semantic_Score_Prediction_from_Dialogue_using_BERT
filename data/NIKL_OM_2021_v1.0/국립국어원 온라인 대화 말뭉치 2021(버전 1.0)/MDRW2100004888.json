        {
          "id": "MDRW2100004888.1.1",
          "form": "나 오늘 비와서",
          "original_form": "나 오늘 비와서",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004888.1.2",
          "form": "사람 되게 조금 왔었다",
          "original_form": "사람 되게 조금 왔었다",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004888.1.3",
          "form": "알바 오늘 완전 한가했어",
          "original_form": "알바 오늘 완전 한가했어",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004888.1.4",
          "form": "오 잘됐네",
          "original_form": "오 잘됐네",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004888.1.5",
          "form": "오늘 한가한데 주중 야간 알바 바뀌었나봐",
          "original_form": "오늘 한가한데 주중 야간 알바 바뀌었나봐",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004888.1.6",
          "form": "음식 소분 완전 많이 준비되어있었음",
          "original_form": "음식 소분 완전 많이 준비되어있었음",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004888.1.7",
          "form": "그래서 완전 편했음",
          "original_form": "그래서 완전 편했음",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004888.1.8",
          "form": "축하축하",
          "original_form": "축하축하",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004888.1.9",
          "form": "사람도 없고 그래서 청소만 하고 거의 앉아있었음",
          "original_form": "사람도 없고 그래서 청소만 하고 거의 앉아있었음",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004888.1.10",
          "form": "편했겠자",
          "original_form": "편했겠자",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004888.1.11",
          "form": "편했겠다",
          "original_form": "편했겠다",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004888.1.12",
          "form": "내일도 오늘같이 사람 없었으면 좋겠다",
          "original_form": "내일도 오늘같이 사람 없었으면 좋겠다",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004888.1.13",
          "form": "나 토익 공부해야겠지?ㅠㅠ",
          "original_form": "나 토익 공부해야겠지?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004888.1.14",
          "form": "미리 해두는게 좋아",
          "original_form": "미리 해두는게 좋아",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004888.1.15",
          "form": "RC 책 조금씩만 봐도 될까",
          "original_form": "RC 책 조금씩만 봐도 될까",
          "speaker_id": "1",
          "time": "20210821 19:14"
