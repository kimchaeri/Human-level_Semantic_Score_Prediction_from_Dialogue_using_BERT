        {
          "id": "MDRW2100037253.1.2",
          "form": "나 과자 샀어",
          "original_form": "나 과자 샀어",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037253.1.3",
          "form": "무슨 과자 먹어",
          "original_form": "무슨 과자 먹어",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037253.1.4",
          "form": "뽀또",
          "original_form": "뽀또",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037253.1.5",
          "form": "그 과자 맛없지 않나",
          "original_form": "그 과자 맛없지 않나",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037253.1.6",
          "form": "빨간색은 별론데 노랑색은 맛있어",
          "original_form": "빨간색은 별론데 노랑색은 맛있어",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037253.1.7",
          "form": "둘 다 맛없던데",
          "original_form": "둘 다 맛없던데",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037253.1.8",
          "form": "치즈 맛이 진짜 맛있는데",
          "original_form": "치즈 맛이 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037253.1.9",
          "form": "빨간거는 무슨 맛이야",
          "original_form": "빨간거는 무슨 맛이야",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037253.1.10",
          "form": "잘 몰라 근데 노랑색은 이제 안나와",
          "original_form": "잘 몰라 근데 노랑색은 이제 안나와",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037253.1.11",
          "form": "좀 아쉽겠다",
          "original_form": "좀 아쉽겠다",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037253.1.12",
          "form": "썬 칩 제 출시한 거 하니",
          "original_form": "썬 칩 제 출시한 거 하니",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037253.1.13",
          "form": "그게 단종됐었나",
          "original_form": "그게 단종됐었나",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037253.1.14",
          "form": "공장에 불탔다고 하지 않았나",
          "original_form": "공장에 불탔다고 하지 않았나",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037253.1.15",
          "form": "몰랐는데 왠지 먹어보고 싶네",
          "original_form": "몰랐는데 왠지 먹어보고 싶네",
