      "utterance": [
        {
          "id": "MDRW2100024046.1.1",
          "form": "뱀좋아하세요?",
          "original_form": "뱀좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024046.1.2",
          "form": "매탈남님이제 영상편집잘하시네요",
          "original_form": "매탈남님이제 영상편집잘하시네요",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024046.1.3",
          "form": "설마요",
          "original_form": "설마요",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024046.1.4",
          "form": "마시는용으로 물으시는건가요? ^^",
          "original_form": "마시는용으로 물으시는건가요? ^^",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024046.1.5",
          "form": "? 아 뱀술이요?",
          "original_form": "? 아 뱀술이요?",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024046.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024046.1.7",
          "form": "안마셔봐서 모르겠네요",
          "original_form": "안마셔봐서 모르겠네요",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024046.1.8",
          "form": "이재명이 당선되면 보신탕없애겠대요",
          "original_form": "이재명이 당선되면 보신탕없애겠대요",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024046.1.9",
          "form": "저도.. 뱀치우면 입에넣을수있을듯요",
          "original_form": "저도.. 뱀치우면 입에넣을수있을듯요",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024046.1.10",
          "form": "반려로 키우는사람이 많으니",
          "original_form": "반려로 키우는사람이 많으니",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024046.1.11",
          "form": "보신탕이 좀안좋은문화겠죠 그분들께는",
          "original_form": "보신탕이 좀안좋은문화겠죠 그분들께는",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024046.1.12",
          "form": "근데 유기견입양하시고",
          "original_form": "근데 유기견입양하시고",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024046.1.13",
          "form": "광고잘하셨으면서",
          "original_form": "광고잘하셨으면서",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024046.1.14",
          "form": "메탈남님 재활용으로 고양이 타워만드시는게 실력파시네요",
          "original_form": "메탈남님 재활용으로 고양이 타워만드시는게 실력파시네요",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024046.1.15",
          "form": "이제는 방치했다고 뉴스떴던데",
          "original_form": "이제는 방치했다고 뉴스떴던데",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024046.1.16",
          "form": "재명님 그분의 머릿속은 참",
          "original_form": "재명님 그분의 머릿속은 참",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.17",
          "form": "그분은 신의 손을 가지셨어요",
          "original_form": "그분은 신의 손을 가지셨어요",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.18",
          "form": "알다가도 모르겠네요",
          "original_form": "알다가도 모르겠네요",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.19",
          "form": "전 확연히 알거같은데",
          "original_form": "전 확연히 알거같은데",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.20",
          "form": "그것이 알고싶다",
          "original_form": "그것이 알고싶다",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.21",
          "form": "맞아요 재명님보다 인간미 넘쳐요",
          "original_form": "맞아요 재명님보다 인간미 넘쳐요",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.22",
          "form": "코리안마피아편 보시면",
          "original_form": "코리안마피아편 보시면",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.23",
          "form": "무서우실꺼에요",
          "original_form": "무서우실꺼에요",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.25",
          "form": "아..벌써부터 겁나네요",
          "original_form": "아..벌써부터 겁나네요",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.26",
          "form": "보셨나요?ㅋ",
          "original_form": "보셨나요?ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024046.1.27",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024046.1.28",
          "form": "그걸 먼저 찾아보고",
          "original_form": "그걸 먼저 찾아보고",
          "speaker_id": "2",
          "time": "20210830 21:38"
        }
      ]
    }
  ]
