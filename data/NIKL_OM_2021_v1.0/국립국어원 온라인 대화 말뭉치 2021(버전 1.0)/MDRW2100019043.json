      "utterance": [
        {
          "id": "MDRW2100019043.1.1",
          "form": "제로콜라 좋아하세요?",
          "original_form": "제로콜라 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.4",
          "form": "맛으로 먹는데",
          "original_form": "맛으로 먹는데",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.5",
          "form": "맨날 후려치기 당하구,,",
          "original_form": "맨날 후려치기 당하구,,",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.6",
          "form": "ㅎㅎ 맞아요",
          "original_form": "ㅎㅎ 맞아요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.7",
          "form": "펩시랑 코카콜라도 ㅎㅎ",
          "original_form": "펩시랑 코카콜라도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.8",
          "form": "네 전 다 상관없는데 ㅋㅋ",
          "original_form": "네 전 다 상관없는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019043.1.9",
          "form": "맞아요 구분도 안 가던데",
          "original_form": "맞아요 구분도 안 가던데",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.10",
          "form": "네 ㅋㅋㅋ 솔직히 다 허세같아요",
          "original_form": "네 ㅋㅋㅋ 솔직히 다 허세같아요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.11",
          "form": "ㅎㅎ 그런가요",
          "original_form": "ㅎㅎ 그런가요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.12",
          "form": "제로펩시도 먹어보셨어요?",
          "original_form": "제로펩시도 먹어보셨어요?",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.13",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.14",
          "form": "제로 코크만 먹어봤어요",
          "original_form": "제로 코크만 먹어봤어요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.15",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.16",
          "form": "라임맛 난다던데",
          "original_form": "라임맛 난다던데",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.17",
          "form": "라임이요?",
          "original_form": "라임이요?",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.18",
          "form": "흠..",
          "original_form": "흠..",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.19",
          "form": "ㅋㅋㅋ 근데 맛있대요!",
          "original_form": "ㅋㅋㅋ 근데 맛있대요!",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019043.1.20",
          "form": "좀 미심쩍네요 ㅎㅎ",
          "original_form": "좀 미심쩍네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019043.1.21",
          "form": "제로 스프라이트요?",
          "original_form": "제로 스프라이트요?",
          "speaker_id": "2",
          "time": "20210827 14:50"
        }
      ]
    }
