      "utterance": [
        {
          "id": "MDRW2100044582.1.1",
          "form": "name1님 배스킨 좋아하세요?",
          "original_form": "&name1&님 배스킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044582.1.2",
          "form": "네 배스킨 좋아해요!!",
          "original_form": "네 배스킨 좋아해요!!",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044582.1.3",
          "form": "지금도 냉동실에 있어요 ㅋㄱㅋㅋ",
          "original_form": "지금도 냉동실에 있어요 ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044582.1.4",
          "form": "꺄ㅋㅋㅋㅋㅋ 뮤슨맛인가요 하나만 말해주세요",
          "original_form": "꺄ㅋㅋㅋㅋㅋ 뮤슨맛인가요 하나만 말해주세요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044582.1.5",
          "form": "하나만 말하자면 ㅋㅋㄱㅋㄱ 바람과함께사라지다요",
          "original_form": "하나만 말하자면 ㅋㅋㄱㅋㄱ 바람과함께사라지다요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044582.1.6",
          "form": "맨날 꼭 이건 넣어요",
          "original_form": "맨날 꼭 이건 넣어요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044582.1.7",
          "form": "헐 저도 최애에요",
          "original_form": "헐 저도 최애에요",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044582.1.8",
          "form": "맛있죠~ 치즈케잌 씹히는 것도 좋고",
          "original_form": "맛있죠~ 치즈케잌 씹히는 것도 좋고",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044582.1.9",
          "form": "name2님은 또 무슨 맛 좋아하세요?",
          "original_form": "&name2&님은 또 무슨 맛 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044582.1.10",
          "form": "사빠딸 뉴치케 그린티 레샤",
          "original_form": "사빠딸 뉴치케 그린티 레샤",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044582.1.11",
          "form": "얘네는 꼭 먹는애들이요 ㅋㅋㅋ",
          "original_form": "얘네는 꼭 먹는애들이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044582.1.12",
          "form": "오 저도 그린티 좋아해요!!",
          "original_form": "오 저도 그린티 좋아해요!!",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044582.1.13",
          "form": "저 지금 냉동실에 있는 게 애플민트 맥심 폴라베어? 초콜릿무스 그린티 바함사 이런 조합이거든요 ㅋㅋㅋㅋ",
          "original_form": "저 지금 냉동실에 있는 게 애플민트 맥심 폴라베어? 초콜릿무스 그린티 바함사 이런 조합이거든요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044582.1.14",
          "form": "민트는 어떻게 생각하시나요?",
          "original_form": "민트는 어떻게 생각하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044582.1.15",
          "form": "저 애플민트도 좋아요!!!",
          "original_form": "저 애플민트도 좋아요!!!",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044582.1.16",
          "form": "name1님 목록에 민트 없는데요ㅋㅋㅋㅋㅋ",
          "original_form": "&name1&님 목록에 민트 없는데요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044582.1.17",
          "form": "배척단이시군요",
          "original_form": "배척단이시군요",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044582.1.18",
          "form": "ㅋㅋㅋㅋㅋ 아니에요 굳이 찾진 않을 뿐이죠",
          "original_form": "ㅋㅋㅋㅋㅋ 아니에요 굳이 찾진 않을 뿐이죠",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044582.1.19",
          "form": "민트는 좋아해요",
          "original_form": "민트는 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044582.1.20",
          "form": "근데 초코랑 섞인건 굳이 ㅋㅋㄱㄱ",
          "original_form": "근데 초코랑 섞인건 굳이 ㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044582.1.21",
          "form": "폴라베어도 민트맛이에요",
          "original_form": "폴라베어도 민트맛이에요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044582.1.22",
          "form": "앗 그러샸구나ㅋㅋㅋㅋ 저는 가리는거 없이 다 잘먹어요ㅋㅋㅋ 폴라베어는 뭐랑 민트랑 섞인거에요?",
          "original_form": "앗 그러샸구나ㅋㅋㅋㅋ 저는 가리는거 없이 다 잘먹어요ㅋㅋㅋ 폴라베어는 뭐랑 민트랑 섞인거에요?",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044582.1.23",
          "form": "사실 폴라베어는 이번에 처음으로 먹어봤는데 약간 슈팅스타 민트버전같은 느낌이었어옷",
          "original_form": "사실 폴라베어는 이번에 처음으로 먹어봤는데 약간 슈팅스타 민트버전같은 느낌이었어옷",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044582.1.24",
          "form": "그 슈팅스타 파바박 터지는 알갱이가 여기도 있더라구요",
          "original_form": "그 슈팅스타 파바박 터지는 알갱이가 여기도 있더라구요",
