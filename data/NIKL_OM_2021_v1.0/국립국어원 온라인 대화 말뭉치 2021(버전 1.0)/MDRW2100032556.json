      "utterance": [
        {
          "id": "MDRW2100032556.1.1",
          "form": "음 음원 좋지",
          "original_form": "음 음원 좋지",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032556.1.2",
          "form": "드라마도 좋고?",
          "original_form": "드라마도 좋고?",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032556.1.3",
          "form": "최근에 본 드라마는?",
          "original_form": "최근에 본 드라마는?",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032556.1.4",
          "form": "아 펜트하우스 아주 살짝 보다 말았어",
          "original_form": "아 펜트하우스 아주 살짝 보다 말았어",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032556.1.5",
          "form": "완결 낸 드라마는 호텔델루나?",
          "original_form": "완결 낸 드라마는 호텔델루나?",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032556.1.6",
          "form": "담주 펜트 막방인데ㅋㅋㅋ 포기했어?",
          "original_form": "담주 펜트 막방인데ㅋㅋㅋ 포기했어?",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032556.1.7",
          "form": "응응 처음부터 안보고 따라 잡으려했는데",
          "original_form": "응응 처음부터 안보고 따라 잡으려했는데",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032556.1.8",
          "form": "너무 길더라 ㅋㅋㅋㅋ",
          "original_form": "너무 길더라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032556.1.9",
          "form": "ㅋㅋㅋ근데 내용별거없어서 아마",
          "original_form": "ㅋㅋㅋ근데 내용별거없어서 아마",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032556.1.10",
          "form": "몇시간요약 가능할걸ㅋㅋㅋ",
          "original_form": "몇시간요약 가능할걸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032556.1.11",
          "form": "맞아 그냥",
          "original_form": "맞아 그냥",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032556.1.12",
          "form": "반전에 뭐 배신에 배신에",
          "original_form": "반전에 뭐 배신에 배신에",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032556.1.13",
          "form": "그런 내용이더라",
          "original_form": "그런 내용이더라",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032556.1.14",
          "form": "거기에 사망",
          "original_form": "거기에 사망",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032556.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032556.1.16",
          "form": "맞아 ㅋㅋㅋㅋㅋ그렇대",
          "original_form": "맞아 ㅋㅋㅋㅋㅋ그렇대",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032556.1.17",
          "form": "계속 누군가는 사망하더라ㅠ",
          "original_form": "계속 누군가는 사망하더라ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032556.1.18",
          "form": "다 죽던데? 그 드라마 ㅋㅋㅋㅋ",
          "original_form": "다 죽던데? 그 드라마 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100032556.1.19",
          "form": "맞아ㅋㅋㅋ 진짜 선역이 불쌍해ㅠ",
          "original_form": "맞아ㅋㅋㅋ 진짜 선역이 불쌍해ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032556.1.20",
          "form": "선역이 있어?? 나 잘 몰라",
          "original_form": "선역이 있어?? 나 잘 몰라",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032556.1.21",
          "form": "일찍 하차해서 ㅋㅋㅋㅋ",
          "original_form": "일찍 하차해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
