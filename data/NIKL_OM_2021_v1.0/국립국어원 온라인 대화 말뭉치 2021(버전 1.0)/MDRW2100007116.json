      "utterance": [
        {
          "id": "MDRW2100007116.1.1",
          "form": "혹시 하는 게임 있어?",
          "original_form": "혹시 하는 게임 있어?",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007116.1.2",
          "form": "나는 쿠킹덤 하다가 그만 뒀어 ㅋㅋㅋ",
          "original_form": "나는 쿠킹덤 하다가 그만 뒀어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007116.1.3",
          "form": "게임에 금방 질려하는 편이라ㅠ",
          "original_form": "게임에 금방 질려하는 편이라ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007116.1.4",
          "form": "쿠킹덤... 나도 했지...",
          "original_form": "쿠킹덤... 나도 했지...",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007116.1.5",
          "form": "앗... 이런 이게 걸리다니...",
          "original_form": "앗... 이런 이게 걸리다니...",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007116.1.6",
          "form": "역시 과거형이네",
          "original_form": "역시 과거형이네",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007116.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007116.1.8",
          "form": "요즘 하는 게임 따로 있어?",
          "original_form": "요즘 하는 게임 따로 있어?",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007116.1.9",
          "form": "나는 롤 배우는 중이야! 혹시 롤 하니?",
          "original_form": "나는 롤 배우는 중이야! 혹시 롤 하니?",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007116.1.10",
          "form": "나 롤 한 3판인가 하다가 관뒀어 ㅋㅋㅋ ㅜㅜ",
          "original_form": "나 롤 한 3판인가 하다가 관뒀어 ㅋㅋㅋ ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007116.1.11",
          "form": "너무 어려워서",
          "original_form": "너무 어려워서",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007116.1.12",
          "form": "근데 캐릭터들은 몇몇 알고 있어",
          "original_form": "근데 캐릭터들은 몇몇 알고 있어",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007116.1.13",
          "form": "엌ㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007116.1.14",
          "form": "무슨 캐릭터 알아?",
          "original_form": "무슨 캐릭터 알아?",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007116.1.15",
          "form": "그 문도박사??랑 트래쉬??랑",
          "original_form": "그 문도박사??랑 트래쉬??랑",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007116.1.16",
          "form": "잔나 럭스 하 많이 알았는데",
          "original_form": "잔나 럭스 하 많이 알았는데",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007116.1.17",
          "form": "롤이 은근 보는게 재밌더라",
          "original_form": "롤이 은근 보는게 재밌더라",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007116.1.18",
          "form": "ㅋㅋㅋㅋㅋ트래슄ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ트래슄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007116.1.19",
          "form": "맞아 롤 너무 재밌지... 보는 것도 재밌고 하는 것도 재밌어",
          "original_form": "맞아 롤 너무 재밌지... 보는 것도 재밌고 하는 것도 재밌어",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007116.1.20",
          "form": "사실 나도 몇판 안했지만ㅎㅎ",
          "original_form": "사실 나도 몇판 안했지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007116.1.21",
          "form": "그럼 롤토체스도 해봤어?",
          "original_form": "그럼 롤토체스도 해봤어?",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007116.1.22",
          "form": "내 주변에서 롤 하면 롤토체스도 많이 하더라",
          "original_form": "내 주변에서 롤 하면 롤토체스도 많이 하더라",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007116.1.23",
          "form": "아 맞아 나도 롤 입문을 롤체로 했어",
          "original_form": "아 맞아 나도 롤 입문을 롤체로 했어",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007116.1.24",
          "form": "내 최애 조합이 별수호자였는데 그게 사실 스킨이었다니 충격이었지",
          "original_form": "내 최애 조합이 별수호자였는데 그게 사실 스킨이었다니 충격이었지",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007116.1.25",
          "form": "와 ㅋㅋㅋㅋ 다 이어지네",
          "original_form": "와 ㅋㅋㅋㅋ 다 이어지네",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007116.1.26",
          "form": "나 그 럭스 스킬중에 빔 쏘는거 있잖아",
          "original_form": "나 그 럭스 스킬중에 빔 쏘는거 있잖아",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007116.1.27",
          "form": "그거 좋아해 물론 남이 하면 좀 화나지만",
          "original_form": "그거 좋아해 물론 남이 하면 좀 화나지만",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007116.1.28",
          "form": "응응ㅋㅋㅋㅋ",
          "original_form": "응응ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007116.1.29",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 맞아 그거 써서 죽일때 쾌감쩔지",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 맞아 그거 써서 죽일때 쾌감쩔지",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007116.1.30",
          "form": "스팀 게임은 안해??",
          "original_form": "스팀 게임은 안해??",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007116.1.31",
          "form": "응... 스팀에 게임 너무 많은데 그중에 뭘 해야할지 모르겠더라",
          "original_form": "응... 스팀에 게임 너무 많은데 그중에 뭘 해야할지 모르겠더라",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007116.1.32",
          "form": "난 공포 게임 좋아해서",
          "original_form": "난 공포 게임 좋아해서",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007116.1.33",
          "form": "리틀나이트메어 무료로 풀었을 때 해봤는데",
          "original_form": "리틀나이트메어 무료로 풀었을 때 해봤는데",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007116.1.34",
          "form": "공포보단 퍼즐게임 같아서 어려워서 공략법 찾아봤어 ㅋㅋㅋ",
          "original_form": "공포보단 퍼즐게임 같아서 어려워서 공략법 찾아봤어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007116.1.35",
          "form": "오오 리나메 이름만 들어봤는데 어때? 어렵지만 재밌어?",
          "original_form": "오오 리나메 이름만 들어봤는데 어때? 어렵지만 재밌어?",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007116.1.36",
          "form": "완전 재밌더라",
          "original_form": "완전 재밌더라",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007116.1.37",
          "form": "근데 약간 뭐라고 하지 제3자 시점?이라서 가끔 답답했어",
          "original_form": "근데 약간 뭐라고 하지 제3자 시점?이라서 가끔 답답했어",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007116.1.38",
          "form": "그래? 나도 담에 해봐야겠다",
          "original_form": "그래? 나도 담에 해봐야겠다",
          "speaker_id": "1",
          "time": "20210822 01:02"
        }
      ]
