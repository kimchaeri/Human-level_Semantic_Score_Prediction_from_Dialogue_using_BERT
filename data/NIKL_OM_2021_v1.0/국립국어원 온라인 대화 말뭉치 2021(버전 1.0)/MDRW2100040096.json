      "utterance": [
        {
          "id": "MDRW2100040096.1.1",
          "form": "백신.. 다음달이면 다 맞는다",
          "original_form": "백신.. 다음달이면 다 맞는다",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100040096.1.2",
          "form": "밥 먹었어?",
          "original_form": "밥 먹었어?",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100040096.1.3",
          "form": "나는 언재 맞ㅈ지",
          "original_form": "나는 언재 맞ㅈ지",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040096.1.4",
          "form": "연락이없내",
          "original_form": "연락이없내",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040096.1.5",
          "form": "나...잤어",
          "original_form": "나...잤어",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040096.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040096.1.7",
          "form": "점심 안먹었어?",
          "original_form": "점심 안먹었어?",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040096.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100040096.1.9",
          "form": "나 요즘 넘 배고파",
          "original_form": "나 요즘 넘 배고파",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100040096.1.10",
          "form": "간식 줏어먹고 밥은 제대로 안먹엇어",
          "original_form": "간식 줏어먹고 밥은 제대로 안먹엇어",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100040096.1.11",
          "form": "으아앙",
          "original_form": "으아앙",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100040096.1.12",
          "form": "밥을 먹으면 되잖아....?",
          "original_form": "밥을 먹으면 되잖아....?",
          "speaker_id": "2",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100040096.1.13",
          "form": "나 열라면 먹고싶었는데 집에 없어 슬퍼",
          "original_form": "나 열라면 먹고싶었는데 집에 없어 슬퍼",
          "speaker_id": "2",
          "time": "20210904 18:51"
        },
        {
          "id": "MDRW2100040096.1.14",
          "form": "이미 간식을 많이먹어서",
          "original_form": "이미 간식을 많이먹어서",
          "speaker_id": "1",
          "time": "20210904 19:15"
        },
        {
          "id": "MDRW2100040096.1.15",
          "form": "열라면 사러가자",
          "original_form": "열라면 사러가자",
          "speaker_id": "1",
          "time": "20210904 19:15"
        },
        {
          "id": "MDRW2100040096.1.16",
          "form": "집에 라면이 너무 많아,,",
          "original_form": "집에 라면이 너무 많아,,",
          "speaker_id": "2",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100040096.1.17",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100040096.1.18",
          "form": "우우..",
          "original_form": "우우..",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100040096.1.19",
          "form": "그것부터 무거야갯내..",
          "original_form": "그것부터 무거야갯내..",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100040096.1.20",
          "form": "밥 먹었어??? 뭐먹었어",
          "original_form": "밥 먹었어??? 뭐먹었어",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100040096.1.21",
          "form": "맨날 똑같은거 먹지",
          "original_form": "맨날 똑같은거 먹지",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100040096.1.22",
          "form": "달ㄷ슴살이랑 현미..",
          "original_form": "달ㄷ슴살이랑 현미..",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100040096.1.23",
          "form": "ㅋㅋㅋㅋㅋ ㅜㅜ. 현미 맛잇다매 좋갰다~",
          "original_form": "ㅋㅋㅋㅋㅋ ㅜㅜ. 현미 맛잇다매 좋갰다~",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100040096.1.24",
          "form": "나 팥빙수 먹었는데 넘 춥다",
          "original_form": "나 팥빙수 먹었는데 넘 춥다",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100040096.1.25",
          "form": "도겸 그거 언재 다먹어",
          "original_form": "도겸 그거 언재 다먹어",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100040096.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100040096.1.27",
          "form": "그러다 감기걸랴요",
          "original_form": "그러다 감기걸랴요",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100040096.1.28",
          "form": "응..... 교훈을 얻었너",
          "original_form": "응..... 교훈을 얻었너",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100040096.1.29",
          "form": "이제 안먿을거야 너무 추어",
          "original_form": "이제 안먿을거야 너무 추어",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100040096.1.30",
          "form": "욱ㄱ기다",
          "original_form": "욱ㄱ기다",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100040096.1.31",
          "form": "얼마나남앗어?",
          "original_form": "얼마나남앗어?",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100040096.1.32",
          "form": "한 4번 먹을 정도? 근데 다 안먹어두듀ㅐ 얼려놓음 돼서",
          "original_form": "한 4번 먹을 정도? 근데 다 안먹어두듀ㅐ 얼려놓음 돼서",
