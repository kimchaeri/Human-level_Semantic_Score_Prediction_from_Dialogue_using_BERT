      "utterance": [
        {
          "id": "MDRW2100006703.1.1",
          "form": "추석 얼마 안남았네ㅠ",
          "original_form": "추석 얼마 안남았네ㅠ",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006703.1.2",
          "form": "그러게 이번에는 제사 지내려나 모르겠네",
          "original_form": "그러게 이번에는 제사 지내려나 모르겠네",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006703.1.3",
          "form": "바쁘시면 안지내실거고",
          "original_form": "바쁘시면 안지내실거고",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006703.1.4",
          "form": "안바쁘시면 지내시겠지ㅋㅋ",
          "original_form": "안바쁘시면 지내시겠지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006703.1.5",
          "form": "뭐 하나마나 힘든건 없으니까",
          "original_form": "뭐 하나마나 힘든건 없으니까",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006703.1.6",
          "form": "맞아 어차피 전도 다 사서 굽기만하고 그래서 뭐 일도 아니징!",
          "original_form": "맞아 어차피 전도 다 사서 굽기만하고 그래서 뭐 일도 아니징!",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006703.1.7",
          "form": "온가족이 다 달라붙어서하니까 금방 끝나기도하고",
          "original_form": "온가족이 다 달라붙어서하니까 금방 끝나기도하고",
          "speaker_id": "1",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006703.1.8",
          "form": "단지 기름냄새가 싫다는거",
          "original_form": "단지 기름냄새가 싫다는거",
          "speaker_id": "1",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006703.1.9",
          "form": "인젱... 전부치면 기름 냄새가 온몸에서 나",
          "original_form": "인젱... 전부치면 기름 냄새가 온몸에서 나",
          "speaker_id": "2",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100006703.1.10",
          "form": "그치 에어프라이어 돌릴수도 없고ㅋㅋ",
          "original_form": "그치 에어프라이어 돌릴수도 없고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100006703.1.11",
          "form": "오 좋은 생각인데?ㅋㅋㅋ근데 전 다 터지고 난리날듯ㅋㅋㅋ",
          "original_form": "오 좋은 생각인데?ㅋㅋㅋ근데 전 다 터지고 난리날듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100006703.1.12",
          "form": "그렇겠지? ㅋㅋ 그래도 간단하게 진행하니까 다행이야",
          "original_form": "그렇겠지? ㅋㅋ 그래도 간단하게 진행하니까 다행이야",
          "speaker_id": "1",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006703.1.13",
          "form": "너가 덜 힘드니까",
          "original_form": "너가 덜 힘드니까",
          "speaker_id": "1",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006703.1.14",
          "form": "생각해보면 우리 제사 지낼때 처음에만 전부치고 뭐하고 했지 한두번 하다가 죄다 사서 하지 않았나?",
          "original_form": "생각해보면 우리 제사 지낼때 처음에만 전부치고 뭐하고 했지 한두번 하다가 죄다 사서 하지 않았나?",
          "speaker_id": "2",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006703.1.15",
          "form": "다들 준비하는게 시간 많이들고 힘드니까",
          "original_form": "다들 준비하는게 시간 많이들고 힘드니까",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006703.1.16",
          "form": "간소하게하자 이래서 지금처럼 사다가 하게 된거지ㅋㅋ",
          "original_form": "간소하게하자 이래서 지금처럼 사다가 하게 된거지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006703.1.17",
          "form": "ㅋㅋ나야 좋지 뭐 근데 요즘에는 아예  제사 음식 배달해주는데가 있더라",
          "original_form": "ㅋㅋ나야 좋지 뭐 근데 요즘에는 아예  제사 음식 배달해주는데가 있더라",
          "speaker_id": "2",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100006703.1.18",
          "form": "진짜? 완전 편하겠다",
          "original_form": "진짜? 완전 편하겠다",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100006703.1.19",
          "form": "우리도 이번에 제사 지낸다고하면 배달 시켜야겠네",
          "original_form": "우리도 이번에 제사 지낸다고하면 배달 시켜야겠네",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100006703.1.20",
          "form": "장보러 다니는것보다는 낫지",
          "original_form": "장보러 다니는것보다는 낫지",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100006703.1.21",
          "form": "발품만팔고",
          "original_form": "발품만팔고",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100006703.1.22",
          "form": "아오 아래집때문에 스트레스 받아 ㅠ",
          "original_form": "아오 아래집때문에 스트레스 받아 ㅠ",
