      "utterance": [
        {
          "id": "MDRW2100028862.1.1",
          "form": "제일 좋아하는 음료는?",
          "original_form": "제일 좋아하는 음료는?",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028862.1.2",
          "form": "포카리스웨트",
          "original_form": "포카리스웨트",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028862.1.3",
          "form": "이온음료 좋아해요",
          "original_form": "이온음료 좋아해요",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028862.1.4",
          "form": "오 카페에선 뭐 마셔요?",
          "original_form": "오 카페에선 뭐 마셔요?",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028862.1.5",
          "form": "커피는 카페라떼 좋아했는데 요샌 아아에 적응하고 있어요",
          "original_form": "커피는 카페라떼 좋아했는데 요샌 아아에 적응하고 있어요",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028862.1.6",
          "form": "근데 잠을 못자서 되도록 디카페인으루",
          "original_form": "근데 잠을 못자서 되도록 디카페인으루",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028862.1.7",
          "form": "오 효과가 잘 듣는 편인가봐요 ㅇ0ㅇ",
          "original_form": "오 효과가 잘 듣는 편인가봐요 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028862.1.8",
          "form": "졸려서 커피 마시면 계속 졸린데 밤이 되면 카페인이 돌기 시작해요",
          "original_form": "졸려서 커피 마시면 계속 졸린데 밤이 되면 카페인이 돌기 시작해요",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028862.1.9",
          "form": "눈새랄까",
          "original_form": "눈새랄까",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028862.1.10",
          "form": "플라세보 아니에요?",
          "original_form": "플라세보 아니에요?",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028862.1.11",
          "form": "아니에요 진짜 안 졸리단 말이에요",
          "original_form": "아니에요 진짜 안 졸리단 말이에요",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028862.1.12",
          "form": "까르륵 아아를 마시자...",
          "original_form": "까르륵 아아를 마시자...",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028862.1.13",
          "form": "요새 배달비가 너무 비싸요",
          "original_form": "요새 배달비가 너무 비싸요",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028862.1.14",
          "form": "그래서 가까우면 포장해오는 편이에요",
          "original_form": "그래서 가까우면 포장해오는 편이에요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
