      "utterance": [
        {
          "id": "MDRW2100013802.1.1",
          "form": "곰표맥주 먹어봤어?",
          "original_form": "곰표맥주 먹어봤어?",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013802.1.2",
          "form": "저 미지근한거 한입먹어봤는데 밀맥주랑 큰 차이 모르겠더라구요",
          "original_form": "저 미지근한거 한입먹어봤는데 밀맥주랑 큰 차이 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013802.1.3",
          "form": "약간 상콤한느낌?",
          "original_form": "약간 상콤한느낌?",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013802.1.4",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013802.1.5",
          "form": "곰표야 진즉 먹었지",
          "original_form": "곰표야 진즉 먹었지",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013802.1.6",
          "form": "요즘 편의점에 많이 있더라",
          "original_form": "요즘 편의점에 많이 있더라",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013802.1.7",
          "form": "편의점 찾아가면서 먹을 맛은 아닌것같고",
          "original_form": "편의점 찾아가면서 먹을 맛은 아닌것같고",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013802.1.8",
          "form": "허니버터칩과 비슷한 흥행 아닐까요?",
          "original_form": "허니버터칩과 비슷한 흥행 아닐까요?",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013802.1.9",
          "form": "말표맥주도 있던데 흑맥주맛이야",
          "original_form": "말표맥주도 있던데 흑맥주맛이야",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013802.1.10",
          "form": "근데 그건 구두약!!",
          "original_form": "근데 그건 구두약!!",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013802.1.11",
          "form": "제겐 아직은 진로가 최고인듯",
          "original_form": "제겐 아직은 진로가 최고인듯",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013802.1.12",
          "form": "예능에도 나오고 다들 궁금하니깐 많이들찾는듯",
          "original_form": "예능에도 나오고 다들 궁금하니깐 많이들찾는듯",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013802.1.13",
          "form": "근데 이제 좀 풀린다니 온전히 한캔 마셔보고싶긴하네요",
          "original_form": "근데 이제 좀 풀린다니 온전히 한캔 마셔보고싶긴하네요",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013802.1.14",
          "form": "진로도 요즘 콜라보 많이해서 저번에 보니깐 디퓨저 나왔더라 ㅋㅋ",
          "original_form": "진로도 요즘 콜라보 많이해서 저번에 보니깐 디퓨저 나왔더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013802.1.15",
          "form": "ㅋㅋ 디퓨저 ㅎ 소주향??  ㅋㅋ",
          "original_form": "ㅋㅋ 디퓨저 ㅎ 소주향??  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013802.1.16",
          "form": "향은 모르겠고 용기는 소주병 그대로",
          "original_form": "향은 모르겠고 용기는 소주병 그대로",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013802.1.17",
          "form": "진로컵으로 디퓨저해도 이쁘겠다!!",
          "original_form": "진로컵으로 디퓨저해도 이쁘겠다!!",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013802.1.18",
          "form": "소주병 디퓨저는 엄마 등짝각 아닌가요",
          "original_form": "소주병 디퓨저는 엄마 등짝각 아닌가요",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013802.1.19",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013802.1.20",
          "form": "진로는 두꺼비 캐릭터가 귀여워서",
          "original_form": "진로는 두꺼비 캐릭터가 귀여워서",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013802.1.21",
          "form": "제품콜라보하면 귀엽드라구",
          "original_form": "제품콜라보하면 귀엽드라구",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013802.1.22",
          "form": "병이랑 디쟈인 잘뽑았어요\r\n만든사람 상줘야함",
          "original_form": "병이랑 디쟈인 잘뽑았어요\r\n만든사람 상줘야함",
          "speaker_id": "2",
