          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020270.1.31",
          "form": "진짜 좋더",
          "original_form": "진짜 좋더",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020270.1.32",
          "form": "님이 근처에 살면 좋겟어요",
          "original_form": "님이 근처에 살면 좋겟어요",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020270.1.33",
          "form": "망하거나 남으면 주게",
          "original_form": "망하거나 남으면 주게",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020270.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020270.1.35",
          "form": "나도 구럈으면 좋갰다",
          "original_form": "나도 구럈으면 좋갰다",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020270.1.36",
          "form": "룸메가 필요하면",
          "original_form": "룸메가 필요하면",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020270.1.37",
          "form": "저를 불러주새요",
          "original_form": "저를 불러주새요",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020270.1.38",
