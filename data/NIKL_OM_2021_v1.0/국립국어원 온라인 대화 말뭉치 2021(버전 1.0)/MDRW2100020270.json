      "utterance": [
        {
          "id": "MDRW2100020270.1.1",
          "form": "예쁜 가구를 만들기시작한사람 누구야",
          "original_form": "예쁜 가구를 만들기시작한사람 누구야",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.2",
          "form": "그니가....",
          "original_form": "그니가....",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.3",
          "form": "청소기는 어때요",
          "original_form": "청소기는 어때요",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.4",
          "form": "글고 행거보다 장롱을 갖고싶고",
          "original_form": "글고 행거보다 장롱을 갖고싶고",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.5",
          "form": "무선청소기 사고싶음",
          "original_form": "무선청소기 사고싶음",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.6",
          "form": "유선은 고문이다",
          "original_form": "유선은 고문이다",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.7",
          "form": "와 글쿠나",
          "original_form": "와 글쿠나",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.8",
          "form": "ㄹㅇ..",
          "original_form": "ㄹㅇ..",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.9",
          "form": "집은 복층 안살거임",
          "original_form": "집은 복층 안살거임",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.10",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.11",
          "form": "계단 힘들어요",
          "original_form": "계단 힘들어요",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.12",
          "form": "넘어지면 속상함",
          "original_form": "넘어지면 속상함",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020270.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아글쿠나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아글쿠나",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.14",
          "form": "맨바닥에거 넘어지는거보다",
          "original_form": "맨바닥에거 넘어지는거보다",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.15",
          "form": "ㄱ계단이 더 서러움..",
          "original_form": "ㄱ계단이 더 서러움..",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.16",
          "form": "글고 정혈같은거 하는게",
          "original_form": "글고 정혈같은거 하는게",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.17",
          "form": "계단오르면 기분 구릴듯",
          "original_form": "계단오르면 기분 구릴듯",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.18",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.19",
          "form": "침대는 있으면 좋겠어요",
          "original_form": "침대는 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.20",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.21",
          "form": "근데 밑에 공간이 없는 프레임을 사고싶음",
          "original_form": "근데 밑에 공간이 없는 프레임을 사고싶음",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.22",
          "form": "먼지속터져",
          "original_form": "먼지속터져",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020270.1.23",
          "form": "잇을거면",
          "original_form": "잇을거면",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
