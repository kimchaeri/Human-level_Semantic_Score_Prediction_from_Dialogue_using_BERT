      "utterance": [
        {
          "id": "MDRW2100003068.1.1",
          "form": "현재 집에서 키우는\r\n반려동물은 뭔가용?",
          "original_form": "현재 집에서 키우는\r\n반려동물은 뭔가용?",
          "speaker_id": "1",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003068.1.2",
          "form": "우리 깜쮝이 호야와 거북스",
          "original_form": "우리 깜쮝이 호야와 거북스",
          "speaker_id": "2",
          "time": "20210820 19:36"
        },
        {
          "id": "MDRW2100003068.1.3",
          "form": "냥이와 거북이ㅋㅋㅋㅋ",
          "original_form": "냥이와 거북이ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:38"
        },
        {
          "id": "MDRW2100003068.1.4",
          "form": "요새 거북이는 탈출 안하나요?",
          "original_form": "요새 거북이는 탈출 안하나요?",
          "speaker_id": "1",
          "time": "20210820 19:38"
        },
        {
          "id": "MDRW2100003068.1.5",
          "form": "다주해요",
          "original_form": "다주해요",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003068.1.6",
          "form": "자주함요",
          "original_form": "자주함요",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003068.1.7",
          "form": "얼마전에",
          "original_form": "얼마전에",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003068.1.8",
          "form": "또 무정란 낳음 ㅋㅋ",
          "original_form": "또 무정란 낳음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003068.1.9",
          "form": "거북이도 진짜 신기방기하네요",
          "original_form": "거북이도 진짜 신기방기하네요",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100003068.1.10",
          "form": "결혼하심 호야랑 거북이 다 델고 가실거에영?",
          "original_form": "결혼하심 호야랑 거북이 다 델고 가실거에영?",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100003068.1.11",
          "form": "당욘하죳",
          "original_form": "당욘하죳",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003068.1.12",
          "form": "솔까 나말고 제대로 챙기는사람이읍ㅇ음",
          "original_form": "솔까 나말고 제대로 챙기는사람이읍ㅇ음",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003068.1.13",
          "form": "ㅋㅋㅋㅋㅋ그것도 혼수로 봐야할까용?",
          "original_form": "ㅋㅋㅋㅋㅋ그것도 혼수로 봐야할까용?",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003068.1.14",
          "form": "돈이 들긴하지만",
          "original_form": "돈이 들긴하지만",
          "speaker_id": "2",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003068.1.15",
          "form": "그보다 더 행복할순있으욬ㅋㅋ",
          "original_form": "그보다 더 행복할순있으욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003068.1.16",
          "form": "무슨 말씀이신건지ㅋㅋㅋ",
          "original_form": "무슨 말씀이신건지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003068.1.17",
          "form": "돈이많이들어도 호야랑 거북이가 귀엽단겁니당ㅋㅋㅋ",
          "original_form": "돈이많이들어도 호야랑 거북이가 귀엽단겁니당ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003068.1.18",
          "form": "아!!!!! 귀여운걸 좋아하시는군영",
          "original_form": "아!!!!! 귀여운걸 좋아하시는군영",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003068.1.19",
          "form": "결국",
          "original_form": "결국",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003068.1.20",
          "form": "강쥐",
          "original_form": "강쥐",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003068.1.21",
          "form": "데려오기로 했으염",
          "original_form": "데려오기로 했으염",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003068.1.22",
          "form": "헷",
          "original_form": "헷",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003068.1.23",
          "form": "네...? 아예 키우신단건가요?",
          "original_form": "네...? 아예 키우신단건가요?",
          "speaker_id": "1",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003068.1.24",
          "form": "내일 보는게 아니고?",
          "original_form": "내일 보는게 아니고?",
          "speaker_id": "1",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003068.1.25",
          "form": "네 ㅎ..ㅎ",
          "original_form": "네 ㅎ..ㅎ",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003068.1.26",
          "form": "청소 대가로...",
          "original_form": "청소 대가로...",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003068.1.27",
          "form": "부모님 허락 받으신거에여? ㅋㅋ",
          "original_form": "부모님 허락 받으신거에여? ㅋㅋ",
