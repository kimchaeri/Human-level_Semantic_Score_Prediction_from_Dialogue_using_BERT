      "utterance": [
        {
          "id": "MDRW2100044977.1.1",
          "form": "겨울이 다가오면 또 귤덕후는 설레임니다 ㅋㅋㅋㅋ",
          "original_form": "겨울이 다가오면 또 귤덕후는 설레임니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044977.1.2",
          "form": "오 귤 좋아하시나봐요!!",
          "original_form": "오 귤 좋아하시나봐요!!",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044977.1.3",
          "form": "네!! 귤좋아하시나요?",
          "original_form": "네!! 귤좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044977.1.4",
          "form": "좋아하죠!!!!!",
          "original_form": "좋아하죠!!!!!",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044977.1.5",
          "form": "새콤달콤....",
          "original_form": "새콤달콤....",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044977.1.6",
          "form": "겨울엔 귤만한게 없는거 같아요",
          "original_form": "겨울엔 귤만한게 없는거 같아요",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044977.1.7",
          "form": "ㅋㅋㅋ저두요 ㅋㅋㅋ혼자 앉아서 한바구니 다먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저두요 ㅋㅋㅋ혼자 앉아서 한바구니 다먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044977.1.8",
          "form": "겨울엔 귤로 살찌는거 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "겨울엔 귤로 살찌는거 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044977.1.9",
          "form": "저두요 ㅋㅋㅋ여름엔 수박 겨울엔 귤ㅋㅋㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ여름엔 수박 겨울엔 귤ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044977.1.10",
          "form": "아 과일 땡기네요ㅠㅠ",
          "original_form": "아 과일 땡기네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044977.1.11",
          "form": "딸기 좋아하시나요?",
          "original_form": "딸기 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044977.1.12",
          "form": "네 딸기는 또 생크림사서 찍어먹어야죠ㅠㅠㅍ",
          "original_form": "네 딸기는 또 생크림사서 찍어먹어야죠ㅠㅠㅍ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044977.1.13",
          "form": "아 최고죠",
          "original_form": "아 최고죠",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044977.1.14",
          "form": "이번 겨울에 꼭 먹어야겠어요",
          "original_form": "이번 겨울에 꼭 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044977.1.15",
          "form": "ㅋㅋㅋ맞아요 ㅋㅋㅋ 아 맛있는 딸기 생크림케익도 땡겨요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요 ㅋㅋㅋ 아 맛있는 딸기 생크림케익도 땡겨요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044977.1.16",
          "form": "딸기생크림케익 좋죠ㅠㅠㅠ",
          "original_form": "딸기생크림케익 좋죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044977.1.17",
          "form": "케이크도 좋아하시나요?",
          "original_form": "케이크도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044977.1.18",
          "form": "네ㅠㅠㅠㅠ저 빵 다 사랑해요ㅠㅠ케익사랑 ㅠㅠ",
