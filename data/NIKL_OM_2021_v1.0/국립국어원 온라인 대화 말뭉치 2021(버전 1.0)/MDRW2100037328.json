      "utterance": [
        {
          "id": "MDRW2100037328.1.1",
          "form": "맞아 지금이 생일시즌이지",
          "original_form": "맞아 지금이 생일시즌이지",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037328.1.2",
          "form": "나는...그때그때 달라",
          "original_form": "나는...그때그때 달라",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037328.1.3",
          "form": "나는 어제랑 오늘 생일인 친구가 둘이나 있었어",
          "original_form": "나는 어제랑 오늘 생일인 친구가 둘이나 있었어",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037328.1.4",
          "form": "너는 선물때문에 고민중이야?",
          "original_form": "너는 선물때문에 고민중이야?",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.5",
          "form": "한명은 카메라가 갖고싶더고했고",
          "original_form": "한명은 카메라가 갖고싶더고했고",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.6",
          "form": "다른 하나는 핸드크림이 갖고싶대",
          "original_form": "다른 하나는 핸드크림이 갖고싶대",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.7",
          "form": "요즘 비싼 핸드크림이 유행인가봐",
          "original_form": "요즘 비싼 핸드크림이 유행인가봐",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.8",
          "form": "나 얼마전에 일화용 카메라 썼는데 좋도라",
          "original_form": "나 얼마전에 일화용 카메라 썼는데 좋도라",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.9",
          "form": "나로서는 조금 이해가 안돼",
          "original_form": "나로서는 조금 이해가 안돼",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.10",
          "form": "그럼 어떤 카메라를 선물했어??",
          "original_form": "그럼 어떤 카메라를 선물했어??",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.11",
          "form": "나는 핸드크림은 아무더나 쓰는 편이야",
          "original_form": "나는 핸드크림은 아무더나 쓰는 편이야",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.12",
          "form": "손이 잘 건조하지않아",
          "original_form": "손이 잘 건조하지않아",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.13",
          "form": "나두 그냥 있으면 바르는편이야",
          "original_form": "나두 그냥 있으면 바르는편이야",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.14",
          "form": "나는 다회용 코닥!",
          "original_form": "나는 다회용 코닥!",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.15",
          "form": "귀엽더라",
          "original_form": "귀엽더라",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.16",
          "form": "마음에 들어했어",
          "original_form": "마음에 들어했어",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.17",
          "form": "니꺼랑 같은거니?",
          "original_form": "니꺼랑 같은거니?",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.18",
          "form": "친구들에게 선택지를 쥬는 선물이 참 좋더라",
          "original_form": "친구들에게 선택지를 쥬는 선물이 참 좋더라",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.19",
          "form": "아니 드건 비싸장ㅎ아.",
          "original_form": "아니 드건 비싸장ㅎ아.",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037328.1.20",
          "form": "ㅠㅠ그렇구나",
          "original_form": "ㅠㅠ그렇구나",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037328.1.21",
          "form": "내 건 그리고 폴라로이드야.",
          "original_form": "내 건 그리고 폴라로이드야.",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037328.1.22",
          "form": "오해해서 미안래!!",
          "original_form": "오해해서 미안래!!",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037328.1.23",
          "form": "나 사고싶은게 또 있어",
          "original_form": "나 사고싶은게 또 있어",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037328.1.24",
          "form": "뭔지 알아내서 사줄 수 있어?",
          "original_form": "뭔지 알아내서 사줄 수 있어?",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037328.1.25",
          "form": "쇼핑은 정말 해도해도 끝이 없어",
          "original_form": "쇼핑은 정말 해도해도 끝이 없어",
          "speaker_id": "1",
          "time": "20210831 22:41"
        }
      ]
    }
