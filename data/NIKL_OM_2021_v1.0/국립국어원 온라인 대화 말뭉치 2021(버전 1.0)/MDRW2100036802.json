        },
        {
          "id": "MDRW2100036802.1.2",
          "form": "잠은 잘 주무셧ㄱ나요?",
          "original_form": "잠은 잘 주무셧ㄱ나요?",
          "speaker_id": "1",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036802.1.3",
          "form": "네.. 저는 잠만 빠지면 거의 잘자요",
          "original_form": "네.. 저는 잠만 빠지면 거의 잘자요",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036802.1.4",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036802.1.5",
          "form": "저는 잘 못잤어요ㅠ",
          "original_form": "저는 잘 못잤어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036802.1.6",
          "form": "무슨일잇으세여?",
          "original_form": "무슨일잇으세여?",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036802.1.7",
          "form": "9시 반부터 알람이 너무 시끄러웠어요",
          "original_form": "9시 반부터 알람이 너무 시끄러웠어요",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036802.1.8",
          "form": "헐.. 누구알람이죠?",
          "original_form": "헐.. 누구알람이죠?",
          "speaker_id": "2",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036802.1.9",
          "form": "제 알람이요",
          "original_form": "제 알람이요",
          "speaker_id": "1",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036802.1.10",
          "form": "잠이 보약이고 만병통치제인데",
          "original_form": "잠이 보약이고 만병통치제인데",
          "speaker_id": "2",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036802.1.11",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036802.1.12",
          "form": "무슨일이세요 ㅠ",
          "original_form": "무슨일이세요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036802.1.13",
          "form": "무슨일잇으세여?",
          "original_form": "무슨일잇으세여?",
