        {
          "id": "MDRW2100004878.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004878.1.2",
          "form": "네 엄청요!",
          "original_form": "네 엄청요!",
          "speaker_id": "2",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004878.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004878.1.4",
          "form": "다녀오신 곳중 좋았던 곳 이야기해주세요~",
          "original_form": "다녀오신 곳중 좋았던 곳 이야기해주세요~",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100004878.1.5",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100004878.1.6",
          "form": "오~ 호주 좋죠",
          "original_form": "오~ 호주 좋죠",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100004878.1.7",
          "form": "청정지역이에요",
          "original_form": "청정지역이에요",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100004878.1.8",
          "form": "맞아요ㅎ 저도 한번가봤는데 기회가 되면 또 가고싶어요",
          "original_form": "맞아요ㅎ 저도 한번가봤는데 기회가 되면 또 가고싶어요",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100004878.1.9",
          "form": "넘 좋았어요",
          "original_form": "넘 좋았어요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004878.1.10",
          "form": "호주 어디쪽 보셨어요?",
          "original_form": "호주 어디쪽 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100004878.1.11",
          "form": "브리즈번이요",
          "original_form": "브리즈번이요",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100004878.1.12",
          "form": "저는 시드니랑 멜번이요ㅋ",
          "original_form": "저는 시드니랑 멜번이요ㅋ",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100004878.1.13",
          "form": "좋은덴 가셨네요",
          "original_form": "좋은덴 가셨네요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100004878.1.14",
          "form": "서핑도 하셨나요?",
          "original_form": "서핑도 하셨나요?",
          "speaker_id": "2",
          "time": "20210821 22:17"
        }
