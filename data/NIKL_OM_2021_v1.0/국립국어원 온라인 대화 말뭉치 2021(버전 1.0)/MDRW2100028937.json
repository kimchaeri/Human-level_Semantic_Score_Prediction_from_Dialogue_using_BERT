      "utterance": [
        {
          "id": "MDRW2100028937.1.1",
          "form": "초등학교 때 우유 급식 만족스러우셨나요?",
          "original_form": "초등학교 때 우유 급식 만족스러우셨나요?",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028937.1.2",
          "form": "아뇨 먹고 배탈는 기억만 있네오",
          "original_form": "아뇨 먹고 배탈는 기억만 있네오",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.3",
          "form": "name2의 초등학교에는 석탄난로가 있었나요?",
          "original_form": "&name2&의 초등학교에는 석탄난로가 있었나요?",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.4",
          "form": "배탈난",
          "original_form": "배탈난",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.5",
          "form": "앗 배탈....",
          "original_form": "앗 배탈....",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.6",
          "form": "세상에...",
          "original_form": "세상에...",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.7",
          "form": "저는 그 산 두개 넘어야 학교 도착 했습니다",
          "original_form": "저는 그 산 두개 넘어야 학교 도착 했습니다",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.8",
          "form": "초등학교도 락토프리 우유를 공급했으면 좋았을텐데...",
          "original_form": "초등학교도 락토프리 우유를 공급했으면 좋았을텐데...",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.9",
          "form": "힘들죠...",
          "original_form": "힘들죠...",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.10",
          "form": "정말요???",
          "original_form": "정말요???",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.11",
          "form": "name1은 학교 멀었나요?",
          "original_form": "&name1&은 학교 멀었나요?",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.12",
          "form": "저는 걸어서 150m 였습니다",
          "original_form": "저는 걸어서 150m 였습니다",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028937.1.13",
          "form": "교가는 기억하시나요?",
          "original_form": "교가는 기억하시나요?",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.14",
          "form": "이런 코앞....",
          "original_form": "이런 코앞....",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.15",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.16",
          "form": "교가는 기억 안나고 그 버디버디는 기억납니다",
          "original_form": "교가는 기억 안나고 그 버디버디는 기억납니다",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.17",
          "form": "버디버디라뇨...",
          "original_form": "버디버디라뇨...",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.18",
          "form": "세이클럽이랕",
          "original_form": "세이클럽이랕",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.19",
          "form": "저희는 중딩 때 처음 봤는데",
          "original_form": "저희는 중딩 때 처음 봤는데",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.20",
          "form": "랑",
          "original_form": "랑",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.21",
          "form": "네...?",
          "original_form": "네...?",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028937.1.22",
          "form": "아닌가....",
          "original_form": "아닌가....",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028937.1.23",
          "form": "초딩때 안했...?",
          "original_form": "초딩때 안했...?",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028937.1.24",
          "form": "저희 세대차가 조금 나네요 ^^;;;",
          "original_form": "저희 세대차가 조금 나네요 ^^;;;",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028937.1.25",
          "form": "제가 연상인듯?",
          "original_form": "제가 연상인듯?",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028937.1.26",
          "form": "이게 아마",
          "original_form": "이게 아마",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028937.1.27",
          "form": "지역간 차이 일걸요",
          "original_form": "지역간 차이 일걸요",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028937.1.28",
          "form": "저는 그때 부산과 대전이였슴니다",
          "original_form": "저는 그때 부산과 대전이였슴니다",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028937.1.29",
          "form": "역시 대전이네요",
          "original_form": "역시 대전이네요",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028937.1.30",
          "form": "과학의 도시",
          "original_form": "과학의 도시",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028937.1.31",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028937.1.32",
          "form": "칭찬 감사함니다...",
          "original_form": "칭찬 감사함니다...",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028937.1.33",
          "form": "에이뭘요",
