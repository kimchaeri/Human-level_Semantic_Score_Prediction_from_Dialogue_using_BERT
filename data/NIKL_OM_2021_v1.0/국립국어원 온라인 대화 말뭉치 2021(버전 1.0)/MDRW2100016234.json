      "utterance": [
        {
          "id": "MDRW2100016234.1.1",
          "form": "안녕세요? 여행자주가세요?",
          "original_form": "안녕세요? 여행자주가세요?",
          "speaker_id": "1",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016234.1.2",
          "form": "코로나라 자주 못다니네요ㅠ",
          "original_form": "코로나라 자주 못다니네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016234.1.3",
          "form": "그죠 좋아하는데 못가니 아쉽네요",
          "original_form": "그죠 좋아하는데 못가니 아쉽네요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016234.1.4",
          "form": "기억나는 데가 있으세요?",
          "original_form": "기억나는 데가 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016234.1.5",
          "form": "파리가 그냥 좋더라고요",
          "original_form": "파리가 그냥 좋더라고요",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016234.1.6",
          "form": "오 저도요!",
          "original_form": "오 저도요!",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016234.1.7",
          "form": "며칠 있으셨어요?",
          "original_form": "며칠 있으셨어요?",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016234.1.8",
          "form": "오 파리 넘좋죠? 전 두번갔어요 한번은 3일 한번은 한달이요",
          "original_form": "오 파리 넘좋죠? 전 두번갔어요 한번은 3일 한번은 한달이요",
          "speaker_id": "1",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016234.1.9",
          "form": "와 한달살기!!",
          "original_form": "와 한달살기!!",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016234.1.10",
          "form": "3일은 넘 아쉬웠겠어요",
          "original_form": "3일은 넘 아쉬웠겠어요",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016234.1.11",
          "form": "그래서 한번 더 갔어요",
          "original_form": "그래서 한번 더 갔어요",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016234.1.12",
          "form": "숙소는 어떻게 하셨어요?",
          "original_form": "숙소는 어떻게 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016234.1.13",
          "form": "숙소는 에어비앤비했어요",
          "original_form": "숙소는 에어비앤비했어요",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016234.1.14",
          "form": "와 전 외곽 민박이어서ㅠ",
          "original_form": "와 전 외곽 민박이어서ㅠ",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016234.1.15",
          "form": "외곽 아쉽네요",
          "original_form": "외곽 아쉽네요",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016234.1.16",
          "form": "왕복 시간이들어서요ㅠ",
          "original_form": "왕복 시간이들어서요ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:28"
        }
