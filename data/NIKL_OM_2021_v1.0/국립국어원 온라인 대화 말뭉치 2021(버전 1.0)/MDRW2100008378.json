      "utterance": [
        {
          "id": "MDRW2100008378.1.1",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008378.1.2",
          "form": "앗 사실 지금도 먹고있는데...!",
          "original_form": "앗 사실 지금도 먹고있는데...!",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008378.1.3",
          "form": "육회비빔밥이요!",
          "original_form": "육회비빔밥이요!",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008378.1.4",
          "form": "것도 배달해요?",
          "original_form": "것도 배달해요?",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008378.1.5",
          "form": "아잇 그럼요! 회도 배달되는데 육회라고 못할거 있나요!",
          "original_form": "아잇 그럼요! 회도 배달되는데 육회라고 못할거 있나요!",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008378.1.6",
          "form": "맛있어요 추천추천합니다",
          "original_form": "맛있어요 추천추천합니다",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008378.1.7",
          "form": "먹어야하겠네여",
          "original_form": "먹어야하겠네여",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008378.1.8",
          "form": "아직 좀 더워서 물회도 가끔 시켜먹는데 그것도 괜찮게 잘 와요!",
          "original_form": "아직 좀 더워서 물회도 가끔 시켜먹는데 그것도 괜찮게 잘 와요!",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008378.1.9",
          "form": "보통 배달시키면 머시켜드세요??",
          "original_form": "보통 배달시키면 머시켜드세요??",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008378.1.10",
          "form": "치킨요",
          "original_form": "치킨요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008378.1.11",
          "form": "아하 치킨도 괜찮죠",
          "original_form": "아하 치킨도 괜찮죠",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008378.1.12",
          "form": "족발도 좋구오",
          "original_form": "족발도 좋구오",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008378.1.13",
          "form": "아 마늘족발 진짜 좋아해요 저!",
          "original_form": "아 마늘족발 진짜 좋아해요 저!",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008378.1.14",
          "form": "같이오는 막국수도 진짜 최고 조합 ㅜㅡㅜ",
          "original_form": "같이오는 막국수도 진짜 최고 조합 ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008378.1.15",
          "form": "군침이 도네요",
          "original_form": "군침이 도네요",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008378.1.16",
          "form": "전 오늘 저녁으로 시켜먹을까 합니다!",
          "original_form": "전 오늘 저녁으로 시켜먹을까 합니다!",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008378.1.17",
          "form": "오오 머요?",
          "original_form": "오오 머요?",
          "speaker_id": "1",
          "time": "20210822 16:23"
        }
      ]
