      "utterance": [
        {
          "id": "MDRW2100020489.1.1",
          "form": "빅맥도 맛있긴 한데 상하이에 따라오진 못하는거 같아요",
          "original_form": "빅맥도 맛있긴 한데 상하이에 따라오진 못하는거 같아요",
          "speaker_id": "1",
          "time": "20210828 08:20"
        },
        {
          "id": "MDRW2100020489.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.3",
          "form": "맘스터치는 안가시나요",
          "original_form": "맘스터치는 안가시나요",
          "speaker_id": "2",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.4",
          "form": "치킨패티의 명가",
          "original_form": "치킨패티의 명가",
          "speaker_id": "1",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.5",
          "form": "맘스터치를 안갈 수 없죠",
          "original_form": "맘스터치를 안갈 수 없죠",
          "speaker_id": "1",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.6",
          "form": "맘스터치하면 다른거 다 제쳐두고",
          "original_form": "맘스터치하면 다른거 다 제쳐두고",
          "speaker_id": "1",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.7",
          "form": "싸이버거 아니겠습니까",
          "original_form": "싸이버거 아니겠습니까",
          "speaker_id": "1",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.8",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.9",
          "form": "기본을 아시는 분이시군요",
          "original_form": "기본을 아시는 분이시군요",
          "speaker_id": "2",
          "time": "20210828 08:21"
        },
        {
          "id": "MDRW2100020489.1.10",
          "form": "그리고 그거아시죠",
          "original_form": "그리고 그거아시죠",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.11",
          "form": "맘스터치 감튀는 케이준이라 겁나맛있는거",
          "original_form": "맘스터치 감튀는 케이준이라 겁나맛있는거",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.12",
          "form": "뭐죠 맘터는 감자가 맛있다는거?",
          "original_form": "뭐죠 맘터는 감자가 맛있다는거?",
          "speaker_id": "2",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.13",
          "form": "하 제가 말하기도 전에 치시네요",
          "original_form": "하 제가 말하기도 전에 치시네요",
          "speaker_id": "2",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.14",
          "form": "아 아시는구나",
          "original_form": "아 아시는구나",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.15",
          "form": "역시 맛잘알은 다르시네요",
          "original_form": "역시 맛잘알은 다르시네요",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.16",
          "form": "햄버거에대한 심도깊은 토론 좋네요",
          "original_form": "햄버거에대한 심도깊은 토론 좋네요",
          "speaker_id": "2",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.17",
          "form": "케이준 감자 매콤짭짤 완벽한조화를 이루잖아요",
          "original_form": "케이준 감자 매콤짭짤 완벽한조화를 이루잖아요",
          "speaker_id": "1",
          "time": "20210828 08:22"
        },
        {
          "id": "MDRW2100020489.1.18",
          "form": "그 맛이 좋아 가끔 감튀만먹으러도 갑니다",
          "original_form": "그 맛이 좋아 가끔 감튀만먹으러도 갑니다",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020489.1.19",
          "form": "맞아요 첨 나왔을 때 그래서 맘터만 갔어요",
          "original_form": "맞아요 첨 나왔을 때 그래서 맘터만 갔어요",
          "speaker_id": "2",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020489.1.20",
          "form": "진짜 와 이런감자가 다있네 하고",
          "original_form": "진짜 와 이런감자가 다있네 하고",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020489.1.21",
          "form": "열심히 먹었던거같아요",
          "original_form": "열심히 먹었던거같아요",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020489.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ공감되네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ공감되네요",
          "speaker_id": "2",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100020489.1.23",
          "form": "이제 메이저 브랜드는 얼추 나온거같네요",
          "original_form": "이제 메이저 브랜드는 얼추 나온거같네요",
          "speaker_id": "2",
          "time": "20210828 08:24"
        }
      ]
