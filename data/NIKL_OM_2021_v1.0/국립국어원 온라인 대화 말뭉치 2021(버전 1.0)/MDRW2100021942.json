      "utterance": [
        {
          "id": "MDRW2100021942.1.1",
          "form": "휴가 어디다녀오셨어요?",
          "original_form": "휴가 어디다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021942.1.2",
          "form": "휴가는 아직이요ㅜㅜ",
          "original_form": "휴가는 아직이요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021942.1.3",
          "form": "에고ㅠㅍ왜요ㅎ",
          "original_form": "에고ㅠㅍ왜요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021942.1.4",
          "form": "코로나때문에?",
          "original_form": "코로나때문에?",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021942.1.5",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021942.1.6",
          "form": "어쩔수없죠ㅍ",
          "original_form": "어쩔수없죠ㅍ",
          "speaker_id": "1",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021942.1.7",
          "form": "사시는곳이 코로나가 심한가요?",
          "original_form": "사시는곳이 코로나가 심한가요?",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021942.1.8",
          "form": "수도권이라서요 ㅋㅋ",
          "original_form": "수도권이라서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021942.1.9",
          "form": "아아 그럼 엄청 신경쓰이시겟군요ㅎ",
          "original_form": "아아 그럼 엄청 신경쓰이시겟군요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021942.1.10",
          "form": "코로나끝나면 어디가고싶은세요?ㅎㅎ",
          "original_form": "코로나끝나면 어디가고싶은세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021942.1.11",
          "form": "넵 좀 신경 쓰여요 ㅋㅋㅋㅋ",
          "original_form": "넵 좀 신경 쓰여요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021942.1.12",
          "form": "저는 해외여행이용...",
          "original_form": "저는 해외여행이용...",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021942.1.13",
          "form": "원래 하와이 가려고 계획해놨다 못 가게 돼서ㅜ 가고싶ㄴㅔ요",
          "original_form": "원래 하와이 가려고 계획해놨다 못 가게 돼서ㅜ 가고싶ㄴㅔ요",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021942.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021942.1.15",
          "form": "취소하신거죠?",
          "original_form": "취소하신거죠?",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021942.1.16",
          "form": "하와이 한번도 안가봤는데ㅎ 에고 엄청 아쉽겟어요ㅠ",
          "original_form": "하와이 한번도 안가봤는데ㅎ 에고 엄청 아쉽겟어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021942.1.17",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021942.1.18",
          "form": "저두요 한 번도 안 가봐서 맘먹고 다 준비했는데",
          "original_form": "저두요 한 번도 안 가봐서 맘먹고 다 준비했는데",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021942.1.19",
          "form": "작년에 가장 심할때라 ㅋㅋ",
          "original_form": "작년에 가장 심할때라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.20",
          "form": "에고ㅠㅠ",
          "original_form": "에고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.21",
          "form": "그렇죠뭐ㅠ",
          "original_form": "그렇죠뭐ㅠ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.22",
          "form": "코로나가 여러 사람 피해주네요",
          "original_form": "코로나가 여러 사람 피해주네요",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.23",
          "form": "헌터님은 여행 많이 다녀보셨어요?",
          "original_form": "헌터님은 여행 많이 다녀보셨어요?",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.24",
          "form": "빨리 없어져서 마느리님 해외 가야하는데ㅎㅎ",
          "original_form": "빨리 없어져서 마느리님 해외 가야하는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.25",
          "form": "많이가보지는않았어요ㅎㅎ",
          "original_form": "많이가보지는않았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.26",
          "form": "해외는ㅎㅎ 국내는 많이가봤구요",
          "original_form": "해외는ㅎㅎ 국내는 많이가봤구요",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.27",
          "form": "저도요 ㅋㅋ 많이 안 가봐서 더 기대했는데 아쉽네용",
          "original_form": "저도요 ㅋㅋ 많이 안 가봐서 더 기대했는데 아쉽네용",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021942.1.28",
          "form": "국내는 많이 다니시는 구나",
          "original_form": "국내는 많이 다니시는 구나",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021942.1.29",
          "form": "최근엔 어디 다녀오셨나요?",
          "original_form": "최근엔 어디 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021942.1.30",
          "form": "최근에는 전남에만 돌았어요ㅎㅎ",
          "original_form": "최근에는 전남에만 돌았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021942.1.31",
          "form": "사는쪽이 전남이라ㅎ 코로나시국인데도 많이 돌아다녔어요ㅎ",
          "original_form": "사는쪽이 전남이라ㅎ 코로나시국인데도 많이 돌아다녔어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021942.1.32",
          "form": "장흥 고흥 여수 광양 순천 등등ㅎㅎ",
          "original_form": "장흥 고흥 여수 광양 순천 등등ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021942.1.33",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021942.1.34",
          "form": "전남 넘 좋죠 전 3월에 순천 다녀왔어요 ㅋㅋ",
          "original_form": "전남 넘 좋죠 전 3월에 순천 다녀왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021942.1.35",
          "form": "전남지방은 아직 많이 안심해서ㅎ",
          "original_form": "전남지방은 아직 많이 안심해서ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021942.1.36",
          "form": "오오ㅎㅎ",
          "original_form": "오오ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021942.1.37",
          "form": "어디어디가셨어요?",
          "original_form": "어디어디가셨어요?",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021942.1.38",
          "form": "그냥 남들 가는대로... 순천만이랑 그 공원? 거기랑",
          "original_form": "그냥 남들 가는대로... 순천만이랑 그 공원? 거기랑",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021942.1.39",
          "form": "낙안읍성? 거기하구 옛날 거리 촬영지? 거기용ㅋㅋㅋ",
          "original_form": "낙안읍성? 거기하구 옛날 거리 촬영지? 거기용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021942.1.40",
          "form": "저 순천살아서ㅎㅎ 거기안가요ㅎㅎㅎㅎㅎㅎ",
          "original_form": "저 순천살아서ㅎㅎ 거기안가요ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021942.1.41",
          "form": "엄청많이갔거든요ㅎㅎㅎㅎ",
          "original_form": "엄청많이갔거든요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021942.1.42",
          "form": "전망대는 가보셨어요,",
          "original_form": "전망대는 가보셨어요,",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021942.1.43",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021942.1.44",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021942.1.45",
          "form": "아 아무래도 현지분들은 안 가실거 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 아무래도 현지분들은 안 가실거 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021942.1.46",
          "form": "저도 차이나타운 안 가요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 차이나타운 안 가요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021942.1.47",
          "form": "오오ㅎㅎ 인천이시네요ㅎㅎ",
          "original_form": "오오ㅎㅎ 인천이시네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021942.1.48",
          "form": "짜장면맛있던데요ㅎㅎㅎ",
          "original_form": "짜장면맛있던데요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021942.1.49",
          "form": "가서 잼께놀았는뎅ㅎㅎ",
          "original_form": "가서 잼께놀았는뎅ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021942.1.50",
          "form": "네 ㅋㅋㅋㅋ 차이나타운 월미도.. 이런데 잘 안 가요 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 차이나타운 월미도.. 이런데 잘 안 가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021942.1.51",
          "form": "인천 와보신 적 있으세용?",
          "original_form": "인천 와보신 적 있으세용?",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021942.1.52",
          "form": "저 예전에 여자친구가 인천에 살았어서ㅎㅎㄹ",
          "original_form": "저 예전에 여자친구가 인천에 살았어서ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021942.1.53",
          "form": "엄청 많이갔어요ㅎㅎㅎ",
          "original_form": "엄청 많이갔어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021942.1.54",
          "form": "여담이지만.. 전 순천 여자친구는 인천... 라임보소?",
          "original_form": "여담이지만.. 전 순천 여자친구는 인천... 라임보소?",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021942.1.55",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
