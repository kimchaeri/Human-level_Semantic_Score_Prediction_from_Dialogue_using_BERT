      "utterance": [
        {
          "id": "MDRW2100047314.1.1",
          "form": "너무해 샄",
          "original_form": "너무해 샄",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100047314.1.2",
          "form": "설레는맘으로",
          "original_form": "설레는맘으로",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100047314.1.3",
          "form": "최애보러가는데 사기나치고ㅜㅜ",
          "original_form": "최애보러가는데 사기나치고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100047314.1.4",
          "form": "그놈은 그렇게 하루종일 중나 잡고살면서",
          "original_form": "그놈은 그렇게 하루종일 중나 잡고살면서",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100047314.1.5",
          "form": "제법 번다고함",
          "original_form": "제법 번다고함",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100047314.1.6",
          "form": "이놈한테 사기치고",
          "original_form": "이놈한테 사기치고",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100047314.1.7",
          "form": "공짜로 갈취한 상품권은",
          "original_form": "공짜로 갈취한 상품권은",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100047314.1.8",
          "form": "선량한 사람인척팔고",
          "original_form": "선량한 사람인척팔고",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100047314.1.9",
          "form": "ㅜ 잡히면좋겠다..",
          "original_form": "ㅜ 잡히면좋겠다..",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100047314.1.10",
          "form": "사기치고 돈버는놈들 잡아주세요",
          "original_form": "사기치고 돈버는놈들 잡아주세요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100047314.1.11",
          "form": "그래서 중나에서 기프트콘 살때는 긴장하면서 사고 팜 ㅋㅋ",
          "original_form": "그래서 중나에서 기프트콘 살때는 긴장하면서 사고 팜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047314.1.12",
          "form": "해지마 해지마 당근해",
          "original_form": "해지마 해지마 당근해",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047314.1.13",
          "form": "기도하는맘으로",
          "original_form": "기도하는맘으로",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047314.1.14",
          "form": "일반인 걸려라",
          "original_form": "일반인 걸려라",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047314.1.15",
          "form": "여긴 지방이라",
          "original_form": "여긴 지방이라",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047314.1.16",
          "form": "당근은사기못만남 아직",
          "original_form": "당근은사기못만남 아직",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100047314.1.17",
          "form": "당근은 잘안팔림",
          "original_form": "당근은 잘안팔림",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047314.1.18",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047314.1.19",
          "form": "사는건 매물이 좀적고 ㅋㅋ",
          "original_form": "사는건 매물이 좀적고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047314.1.20",
          "form": "늘 그렇지..ㅎ..",
          "original_form": "늘 그렇지..ㅎ..",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047314.1.21",
          "form": "언니한테 부탁해볼까 생각했는데",
          "original_form": "언니한테 부탁해볼까 생각했는데",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047314.1.22",
          "form": "동네인증되니까",
          "original_form": "동네인증되니까",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047314.1.23",
          "form": "오 윗동네 사람좀 있겠네",
          "original_form": "오 윗동네 사람좀 있겠네",
          "speaker_id": "2",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100047314.1.24",
          "form": "또 너무 귀찮잖아요",
          "original_form": "또 너무 귀찮잖아요",
          "speaker_id": "1",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100047314.1.25",
          "form": "걸치면그럴수도.",
          "original_form": "걸치면그럴수도.",
          "speaker_id": "2",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100047314.1.26",
          "form": "구매희망자 연락오면 답장도 해줘야하고 등등",
          "original_form": "구매희망자 연락오면 답장도 해줘야하고 등등",
          "speaker_id": "1",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100047314.1.27",
          "form": "그래서 위험의 부담을 안고 중나에 올릴수밖에 없음",
          "original_form": "그래서 위험의 부담을 안고 중나에 올릴수밖에 없음",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100047314.1.28",
          "form": "사는것도",
          "original_form": "사는것도",
          "speaker_id": "1",
          "time": "20210826 11:13"
        }
      ]
    }
  ]
