      "utterance": [
        {
          "id": "MDRW2100017156.1.1",
          "form": "저녁메뉴 정하셨나요?ㅎㅎ",
          "original_form": "저녁메뉴 정하셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017156.1.2",
          "form": "아 저는 치킨 좋아해요 교촌",
          "original_form": "아 저는 치킨 좋아해요 교촌",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017156.1.3",
          "form": "오 교촌 맛있죠!!",
          "original_form": "오 교촌 맛있죠!!",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017156.1.4",
          "form": "저 오늘 친구 생일이라 순대볶음 먹으려구요 ㅎㅎ",
          "original_form": "저 오늘 친구 생일이라 순대볶음 먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017156.1.5",
          "form": "오~ 저는 카레 해먹으려구요",
          "original_form": "오~ 저는 카레 해먹으려구요",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017156.1.6",
          "form": "오 맛있겠다 저 카레 진짜 좋아해요",
          "original_form": "오 맛있겠다 저 카레 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017156.1.7",
          "form": "일본식 인도식 한국식 할거 없이 다 맛있는것같아요 카레는",
          "original_form": "일본식 인도식 한국식 할거 없이 다 맛있는것같아요 카레는",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017156.1.8",
          "form": "카레 맛있죠ㅎㅎ",
          "original_form": "카레 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017156.1.9",
          "form": "요리잘하세요?",
          "original_form": "요리잘하세요?",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017156.1.10",
          "form": "아뇨 그냥 엄마 옆에서 거드는 역할이에요",
          "original_form": "아뇨 그냥 엄마 옆에서 거드는 역할이에요",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017156.1.11",
          "form": "요리는..할때보다 먹을때를 더 좋아하는편ㅎ",
          "original_form": "요리는..할때보다 먹을때를 더 좋아하는편ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017156.1.12",
          "form": "아하 ㅎㅎ",
          "original_form": "아하 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017156.1.13",
          "form": "맞아요 먹는게 짱",
          "original_form": "맞아요 먹는게 짱",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017156.1.14",
          "form": "전 어제 복숭아 사왔는데 진짜 달고 맛있더라구요",
          "original_form": "전 어제 복숭아 사왔는데 진짜 달고 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017156.1.15",
          "form": "디저트로 복숭아 어떠세여",
          "original_form": "디저트로 복숭아 어떠세여",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017156.1.16",
          "form": "복숭아 말랑vs딱딱 어느쪽 좋아하세요?",
          "original_form": "복숭아 말랑vs딱딱 어느쪽 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017156.1.17",
          "form": "전 말랑이요ㅎㅎ",
          "original_form": "전 말랑이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017156.1.18",
          "form": "저도 말랑이요!!",
          "original_form": "저도 말랑이요!!",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017156.1.19",
          "form": "인터넷에서 딱복러들한테 소고기딱복국이나 먹으라고 하는거 보셨어요? ㅋㅋㅋㅋㅋ",
          "original_form": "인터넷에서 딱복러들한테 소고기딱복국이나 먹으라고 하는거 보셨어요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017156.1.20",
          "form": "ㄲㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄲㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017156.1.21",
          "form": "그치만 취향은 존중해주는 걸로,,",
          "original_form": "그치만 취향은 존중해주는 걸로,,",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017156.1.22",
          "form": "혹시 민트초코 호vs불호",
          "original_form": "혹시 민트초코 호vs불호",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017156.1.23",
          "form": "맞아요,, 전 사실 말랑파긴 하지만 복숭아라면 다 좋거든요",
          "original_form": "맞아요,, 전 사실 말랑파긴 하지만 복숭아라면 다 좋거든요",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017156.1.24",
          "form": "호.(단호)",
          "original_form": "호.(단호)",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017156.1.25",
          "form": "저도 호에요",
          "original_form": "저도 호에요",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017156.1.26",
          "form": "뭘 아신다 진짜로",
          "original_form": "뭘 아신다 진짜로",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017156.1.27",
          "form": "하와이안피자 호/불호",
          "original_form": "하와이안피자 호/불호",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017156.1.28",
          "form": "사실 굳이 제돈 내고 주문하진 않는데.. 먹긴먹어요",
