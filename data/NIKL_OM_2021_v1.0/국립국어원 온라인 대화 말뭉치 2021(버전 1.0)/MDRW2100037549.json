      "utterance": [
        {
          "id": "MDRW2100037549.1.1",
          "form": "비온다",
          "original_form": "비온다",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100037549.1.2",
          "form": "첫째 우산은 잘 들고 갔어요",
          "original_form": "첫째 우산은 잘 들고 갔어요",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100037549.1.3",
          "form": "하긴 등교할 때 비 너무 많이 오긴 했다ㅋㅋㅋ",
          "original_form": "하긴 등교할 때 비 너무 많이 오긴 했다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100037549.1.4",
          "form": "우산....",
          "original_form": "우산....",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037549.1.5",
          "form": "우산 하나는 도서관 인지 어딘지 놓고 왔고",
          "original_form": "우산 하나는 도서관 인지 어딘지 놓고 왔고",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037549.1.6",
          "form": "또 어제 들고 간 우산은 학교에 놔두고 왔고",
          "original_form": "또 어제 들고 간 우산은 학교에 놔두고 왔고",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037549.1.7",
          "form": "저 저번 주가 들고 간 우산은 반도 놀이터에 놓고 왔음",
          "original_form": "저 저번 주가 들고 간 우산은 반도 놀이터에 놓고 왔음",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037549.1.8",
          "form": "이제 긴 우산이 없어가지고",
          "original_form": "이제 긴 우산이 없어가지고",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037549.1.9",
          "form": "오늘 친정엄마가 학교 앞까지 데려다 줬어",
          "original_form": "오늘 친정엄마가 학교 앞까지 데려다 줬어",
          "speaker_id": "2",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100037549.1.10",
          "form": "언니 우산은 박스 사놔야 되겠네ㅋㅋㅋㅋ",
          "original_form": "언니 우산은 박스 사놔야 되겠네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100037549.1.11",
          "form": "학교 근처에 돌아다니는 우산 있으면은 수거해 놓을게요-ㅋㅋㅋㅋㅋ",
          "original_form": "학교 근처에 돌아다니는 우산 있으면은 수거해 놓을게요-ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100037549.1.12",
          "form": "그래 어디 내버려진 우산 있으면 내 임택에 우리 아들 이름 적혀 있는지 확인 좀 해줘",
          "original_form": "그래 어디 내버려진 우산 있으면 내 임택에 우리 아들 이름 적혀 있는지 확인 좀 해줘",
          "speaker_id": "2",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100037549.1.13",
          "form": "안 그래도 오늘 우산 두 개 주문하려고",
          "original_form": "안 그래도 오늘 우산 두 개 주문하려고",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100037549.1.14",
          "form": "그냥 많이 사 놓는게 낫겠다",
          "original_form": "그냥 많이 사 놓는게 낫겠다",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100037549.1.15",
          "form": "그럴게요 언니ㅋㅋㅋㅋㅋ",
          "original_form": "그럴게요 언니ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037549.1.16",
          "form": "빨리 여름이 지나가야 되겠네요ㅋㅋㅋㅋ",
          "original_form": "빨리 여름이 지나가야 되겠네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037549.1.17",
          "form": "그래 내가 장 마랑 싸우는지 아들이랑 싸우는지 알 수가 없음",
          "original_form": "그래 내가 장 마랑 싸우는지 아들이랑 싸우는지 알 수가 없음",
          "speaker_id": "2",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100037549.1.18",
          "form": "오늘은 계속 비가 오니",
          "original_form": "오늘은 계속 비가 오니",
          "speaker_id": "2",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100037549.1.19",
          "form": "우산 들고 오겠지??",
          "original_form": "우산 들고 오겠지??",
          "speaker_id": "2",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100037549.1.20",
          "form": "지금은 비가 안 오는 거 같아요ㅋㅋㅋ",
          "original_form": "지금은 비가 안 오는 거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100037549.1.21",
          "form": "오늘도 기증하고 오는 걸로ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오늘도 기증하고 오는 걸로ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037549.1.22",
          "form": "여긴 지금 해가 떴어요ㅋㅋㅋ",
          "original_form": "여긴 지금 해가 떴어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100037549.1.23",
          "form": "ㅋㅋㅋㅋㅋ 해떴나??",
          "original_form": "ㅋㅋㅋㅋㅋ 해떴나??",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100037549.1.24",
          "form": "아 여기는 비오는데....",
          "original_form": "아 여기는 비오는데....",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100037549.1.25",
          "form": "예 오늘 그 우산은.. 다시는 볼 수 없을 거 같아요",
          "original_form": "예 오늘 그 우산은.. 다시는 볼 수 없을 거 같아요",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100037549.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100037549.1.27",
          "form": "그래....휴....어딘가엔가 있겠지",
          "original_form": "그래....휴....어딘가엔가 있겠지",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100037549.1.28",
          "form": "우산을 주문하는게 빠르겠다",
          "original_form": "우산을 주문하는게 빠르겠다",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100037549.1.29",
          "form": "그우산이 돌아오길 바라느니...",
          "original_form": "그우산이 돌아오길 바라느니...",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100037549.1.30",
          "form": "2학년 올라갈때\r\n선생님이 우산을 한가득 내어주실듯...",
          "original_form": "2학년 올라갈때\r\n선생님이 우산을 한가득 내어주실듯...",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100037549.1.31",
          "form": "그렇죠 언니ㅋㅋㅋㅋ",
          "original_form": "그렇죠 언니ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100037549.1.32",
          "form": "아 너무 웃긴다ㅋㅋㅋㅋㅋ",
          "original_form": "아 너무 웃긴다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100037549.1.33",
          "form": "비 올 때마다 우산 걱정해야됨ㅋㅋㅋ",
          "original_form": "비 올 때마다 우산 걱정해야됨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100037549.1.34",
          "form": "아침에 비왔으니\r\n우산 가져온애들은 우산 챙겨가라고 말 좀 해주면 안되는가...",
          "original_form": "아침에 비왔으니\r\n우산 가져온애들은 우산 챙겨가라고 말 좀 해주면 안되는가...",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100037549.1.35",
          "form": "난 늘 그게 아쉬움",
          "original_form": "난 늘 그게 아쉬움",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100037549.1.36",
          "form": "우산꽂이에 있는 우산을 안보는가??",
          "original_form": "우산꽂이에 있는 우산을 안보는가??",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100037549.1.37",
          "form": "나중에 되면 누구 우산 지도 모를 듯요 ㅋㅋㅋㅋ",
          "original_form": "나중에 되면 누구 우산 지도 모를 듯요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:27"
        }
      ]
