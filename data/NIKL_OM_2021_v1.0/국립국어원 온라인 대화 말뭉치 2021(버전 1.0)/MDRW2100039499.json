      "utterance": [
        {
          "id": "MDRW2100039499.1.1",
          "form": "오늘 날씨가 너무 좋네요",
          "original_form": "오늘 날씨가 너무 좋네요",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039499.1.2",
          "form": "그죠...진짜...근데집에만잇네요",
          "original_form": "그죠...진짜...근데집에만잇네요",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039499.1.3",
          "form": "하필이런날은 ..",
          "original_form": "하필이런날은 ..",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039499.1.4",
          "form": "저도 일때매 집에만 있었어요ㅠ",
          "original_form": "저도 일때매 집에만 있었어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100039499.1.5",
          "form": "교육다들으셧나요",
          "original_form": "교육다들으셧나요",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039499.1.6",
          "form": "네 이제 다음부부터 다시 출근이에요",
          "original_form": "네 이제 다음부부터 다시 출근이에요",
          "speaker_id": "1",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039499.1.7",
          "form": "ㅜㅡㅜ",
          "original_form": "ㅜㅡㅜ",
          "speaker_id": "1",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039499.1.8",
          "form": "ㅜㅜ 에휴",
          "original_form": "ㅜㅜ 에휴",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039499.1.9",
          "form": "날씨가 그래도 갠타나서..출퇴근딜은 좋겟어요",
          "original_form": "날씨가 그래도 갠타나서..출퇴근딜은 좋겟어요",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039499.1.10",
          "form": "네 제가 일주일 재택하는동안 여름에서 가을이 온것같아요 정말",
          "original_form": "네 제가 일주일 재택하는동안 여름에서 가을이 온것같아요 정말",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039499.1.11",
          "form": "머입고출근하실건가요",
          "original_form": "머입고출근하실건가요",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039499.1.12",
          "form": "모르겠어요 ㅋㅋ 집 가면 옷장을 또 헤집어야겠어요",
          "original_form": "모르겠어요 ㅋㅋ 집 가면 옷장을 또 헤집어야겠어요",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100039499.1.13",
          "form": "가을이다아~~~~!!",
          "original_form": "가을이다아~~~~!!",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039499.1.14",
          "form": "검도하러 첫외출",
          "original_form": "검도하러 첫외출",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039499.1.15",
          "form": "ㅋㅋㅋㅋ저도 방금 케익 사러 다녀왔어요!!",
          "original_form": "ㅋㅋㅋㅋ저도 방금 케익 사러 다녀왔어요!!",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039499.1.16",
          "form": "선선하니 좋네요ㅠㅋㅋ",
          "original_form": "선선하니 좋네요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039499.1.17",
          "form": "우와 케익사셧구낭 머사셧어요",
          "original_form": "우와 케익사셧구낭 머사셧어요",
