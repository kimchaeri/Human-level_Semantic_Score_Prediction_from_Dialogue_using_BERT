      "utterance": [
        {
          "id": "MDRW2100001003.1.1",
          "form": "name3ㅡ는 아직도 구박댕이ㅠ",
          "original_form": "&name3&ㅡ는 아직도 구박댕이ㅠ",
          "speaker_id": "2",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100001003.1.2",
          "form": "이런 나슬프게",
          "original_form": "이런 나슬프게",
          "speaker_id": "1",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001003.1.3",
          "form": "하지만 name3는 귀엽지",
          "original_form": "하지만 &name3&는 귀엽지",
          "speaker_id": "1",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001003.1.4",
          "form": "나도여ㅜㅜㅡ",
          "original_form": "나도여ㅜㅜㅡ",
          "speaker_id": "2",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001003.1.5",
          "form": "키울수있는 환경이어야 데려오는데",
          "original_form": "키울수있는 환경이어야 데려오는데",
          "speaker_id": "1",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001003.1.6",
          "form": "아 어머님땜에 안되겠구먼",
          "original_form": "아 어머님땜에 안되겠구먼",
          "speaker_id": "1",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001003.1.7",
          "form": "그쳐 엄마딸",
          "original_form": "그쳐 엄마딸",
          "speaker_id": "2",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001003.1.8",
          "form": "name4이는 알레르기",
          "original_form": "&name4&이는 알레르기",
          "speaker_id": "2",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001003.1.9",
          "form": "내동생 엄마딸",
          "original_form": "내동생 엄마딸",
          "speaker_id": "1",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001003.1.10",
          "form": "나도 요즘 애견사업을 해볼까 고민중",
          "original_form": "나도 요즘 애견사업을 해볼까 고민중",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100001003.1.11",
          "form": "ㅋ머하게여?",
          "original_form": "ㅋ머하게여?",
          "speaker_id": "2",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100001003.1.12",
          "form": "간식 학원이 있더라고",
          "original_form": "간식 학원이 있더라고",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100001003.1.13",
          "form": "수제간식",
          "original_form": "수제간식",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100001003.1.14",
          "form": "근데 절차가 까다롭대 먹는거라",
          "original_form": "근데 절차가 까다롭대 먹는거라",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001003.1.15",
          "form": "그쳐 아무래도",
          "original_form": "그쳐 아무래도",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001003.1.16",
          "form": "사람들이 예민하니까",
          "original_form": "사람들이 예민하니까",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001003.1.17",
          "form": "그리고 제조매장이 필수래",
          "original_form": "그리고 제조매장이 필수래",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001003.1.18",
          "form": "집에서 만들어서는 못판대",
          "original_form": "집에서 만들어서는 못판대",
          "speaker_id": "1",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100001003.1.19",
          "form": "그렁  어디서 해요?",
          "original_form": "그렁  어디서 해요?",
          "speaker_id": "2",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100001003.1.20",
          "form": "매장이 있어야하고 인증을 받아야한대",
          "original_form": "매장이 있어야하고 인증을 받아야한대",
          "speaker_id": "1",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100001003.1.21",
          "form": "언니도 매장 만들어야해여?",
          "original_form": "언니도 매장 만들어야해여?",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100001003.1.22",
          "form": "웅 할거면",
          "original_form": "웅 할거면",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100001003.1.23",
          "form": "매장이 필요해서 안되겠다",
          "original_form": "매장이 필요해서 안되겠다",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100001003.1.24",
          "form": "애견사업이 짱인디",
          "original_form": "애견사업이 짱인디",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100001003.1.25",
          "form": "ㅋㅋ수요가 많으니까",
          "original_form": "ㅋㅋ수요가 많으니까",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100001003.1.26",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100001003.1.27",
          "form": "하면좋은데 매장은  좀 아쉽다",
          "original_form": "하면좋은데 매장은  좀 아쉽다",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100001003.1.28",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100001003.1.29",
          "form": "그게 오프강의가 있는데 150이래 창업반",
          "original_form": "그게 오프강의가 있는데 150이래 창업반",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100001003.1.30",
          "form": "헐 비싸당",
          "original_form": "헐 비싸당",
          "speaker_id": "2",
          "time": "20210820 14:46"
        },
        {
          "id": "MDRW2100001003.1.31",
          "form": "웅 비싸서 못함",
          "original_form": "웅 비싸서 못함",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100001003.1.32",
          "form": "창업도 다 돈이야",
          "original_form": "창업도 다 돈이야",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100001003.1.33",
          "form": "애견쪽 사업 해보구싶은디",
          "original_form": "애견쪽 사업 해보구싶은디",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100001003.1.34",
          "form": "언니는 잘할꺼같은데",
          "original_form": "언니는 잘할꺼같은데",
          "speaker_id": "2",
          "time": "20210820 15:01"
        }
      ]
    }
