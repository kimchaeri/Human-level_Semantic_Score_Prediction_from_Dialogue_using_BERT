      "utterance": [
        {
          "id": "MDRW2100029699.1.1",
          "form": "안녕하세요 가장 좋아하는 음식있으신가요?",
          "original_form": "안녕하세요 가장 좋아하는 음식있으신가요?",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029699.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029699.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029699.1.4",
          "form": "전 녹차들어간 디저트 진짜 좋아해요ㅎㅎ",
          "original_form": "전 녹차들어간 디저트 진짜 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029699.1.5",
          "form": "그쪽은 뭘좋아하세료?",
          "original_form": "그쪽은 뭘좋아하세료?",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029699.1.6",
          "form": "아 녹차 좋아하시는구나 가장 좋아하는 디저트는뭔가요?",
          "original_form": "아 녹차 좋아하시는구나 가장 좋아하는 디저트는뭔가요?",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029699.1.7",
          "form": "저는 가리는거 없이 다 잘먹는데 등촌칼국수를 제일 좋아합니다",
          "original_form": "저는 가리는거 없이 다 잘먹는데 등촌칼국수를 제일 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029699.1.8",
          "form": "오오 등촌칼국수 맛있죠!!!!",
          "original_form": "오오 등촌칼국수 맛있죠!!!!",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029699.1.9",
          "form": "전 녹차 케이크를 젤 좋아해요!!",
          "original_form": "전 녹차 케이크를 젤 좋아해요!!",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029699.1.10",
          "form": "두툼한 쿠키도 좋아하고요 ㅎㅎ",
          "original_form": "두툼한 쿠키도 좋아하고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029699.1.11",
          "form": "오 녹차 케이크를 좋아하시는구나 약간 깔끔한 맛 좋아하시나봐요",
          "original_form": "오 녹차 케이크를 좋아하시는구나 약간 깔끔한 맛 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210903 11:53"
        },
        {
          "id": "MDRW2100029699.1.12",
          "form": "민트도 좋아하세요?",
          "original_form": "민트도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 11:53"
        },
        {
          "id": "MDRW2100029699.1.13",
          "form": "민트는 좋아하는데 엄청 좋아하진않아서",
          "original_form": "민트는 좋아하는데 엄청 좋아하진않아서",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029699.1.14",
          "form": "자주먹진 않아요!",
          "original_form": "자주먹진 않아요!",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029699.1.15",
          "form": "근데 스타벅스에 민트 음료는 진짜 맛있더라구요....bbb",
          "original_form": "근데 스타벅스에 민트 음료는 진짜 맛있더라구요....bbb",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029699.1.16",
          "form": "그쪽은요??",
          "original_form": "그쪽은요??",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029699.1.17",
          "form": "저는 민트 처돌이에요 안데스 초콜릿도 좋아하고",
          "original_form": "저는 민트 처돌이에요 안데스 초콜릿도 좋아하고",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029699.1.18",
          "form": "민트티에 샷추가 해서 마셔요",
          "original_form": "민트티에 샷추가 해서 마셔요",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029699.1.19",
          "form": "숙취해 좋답니다 강추",
          "original_form": "숙취해 좋답니다 강추",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029699.1.20",
          "form": "오늘 아침은 뭐드셨나요",
          "original_form": "오늘 아침은 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029699.1.21",
          "form": "오늘은 늦잠자서 아무것도 못먹었어요ㅜㅜ",
          "original_form": "오늘은 늦잠자서 아무것도 못먹었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029699.1.22",
          "form": "11시에 일어났답니다ㅎㅎ",
          "original_form": "11시에 일어났답니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029699.1.23",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029699.1.24",
          "form": "헉 저는 늦게 일어나도 아침은 꼭 먹어야하는 사람이랔ㅋㅋㅋ 급하게 시리얼 챙겨먹었습니다",
          "original_form": "헉 저는 늦게 일어나도 아침은 꼭 먹어야하는 사람이랔ㅋㅋㅋ 급하게 시리얼 챙겨먹었습니다",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029699.1.25",
          "form": "오늘 금요일인데 뭐 맛있는거 드실계획있으신가요",
          "original_form": "오늘 금요일인데 뭐 맛있는거 드실계획있으신가요",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029699.1.26",
          "form": "앟ㅋㅋㅋㅋㅋ그래야 하루종일 든든하죠",
          "original_form": "앟ㅋㅋㅋㅋㅋ그래야 하루종일 든든하죠",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029699.1.27",
          "form": "회사 다니시나요?",
          "original_form": "회사 다니시나요?",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029699.1.28",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029699.1.29",
          "form": "전 좀이따 삼계탕먹으려고요ㅎㅎ",
          "original_form": "전 좀이따 삼계탕먹으려고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029699.1.30",
          "form": "어제부터 먹고싶어서요ㅎㅎ",
          "original_form": "어제부터 먹고싶어서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029699.1.31",
          "form": "아니요 저는 다음주부터 출근을 기다리는 백수입니다",
          "original_form": "아니요 저는 다음주부터 출근을 기다리는 백수입니다",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029699.1.32",
          "form": "아 삼계탕 너무 맛있겠네요ㅜ",
          "original_form": "아 삼계탕 너무 맛있겠네요ㅜ",
