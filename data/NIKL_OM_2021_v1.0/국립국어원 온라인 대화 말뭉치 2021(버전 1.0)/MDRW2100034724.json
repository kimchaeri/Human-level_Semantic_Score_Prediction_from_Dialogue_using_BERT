      "utterance": [
        {
          "id": "MDRW2100034724.1.1",
          "form": "아 ㅜㅜ 아쉬우실거같아요",
          "original_form": "아 ㅜㅜ 아쉬우실거같아요",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034724.1.2",
          "form": "맛집이 사라지다니",
          "original_form": "맛집이 사라지다니",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034724.1.3",
          "form": "네ㅠㅠ 소울푸드였는데",
          "original_form": "네ㅠㅠ 소울푸드였는데",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034724.1.4",
          "form": "사라지면 슬프죠 ㅜㅜ",
          "original_form": "사라지면 슬프죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034724.1.5",
          "form": "저도 좋아하던 집이 사라지면 아쉽더라구요",
          "original_form": "저도 좋아하던 집이 사라지면 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034724.1.6",
          "form": "네 그 셰프님이 시카고 피자로 유명한 셰프셨오요",
          "original_form": "네 그 셰프님이 시카고 피자로 유명한 셰프셨오요",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034724.1.7",
          "form": "헉 시카고피자 너무 맛있는데",
          "original_form": "헉 시카고피자 너무 맛있는데",
          "speaker_id": "1",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034724.1.8",
          "form": "급 먹고싶네요",
          "original_form": "급 먹고싶네요",
          "speaker_id": "1",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034724.1.9",
          "form": "서울에 오시게리따라는 식당으로 다시 식당 여셨네요",
          "original_form": "서울에 오시게리따라는 식당으로 다시 식당 여셨네요",
          "speaker_id": "2",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034724.1.10",
          "form": "저도 생각난김에 검색을 해봤어요 ㅋㅋㅋㅋ",
          "original_form": "저도 생각난김에 검색을 해봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034724.1.11",
          "form": "오 검색해볼게요 궁금하네요",
          "original_form": "오 검색해볼게요 궁금하네요",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034724.1.12",
          "form": "이름이 멋진데요 ㅋㅋㅋㅋ뭔가 장인의 냄새가..",
          "original_form": "이름이 멋진데요 ㅋㅋㅋㅋ뭔가 장인의 냄새가..",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034724.1.13",
          "form": "ㅋㅋㅋㅋ저희 지역에서 영업하실때 오시게 양식당이었어요",
          "original_form": "ㅋㅋㅋㅋ저희 지역에서 영업하실때 오시게 양식당이었어요",
          "speaker_id": "2",
          "time": "20210828 18:16"
        },
        {
          "id": "MDRW2100034724.1.14",
          "form": "오시게 시장있는 동네라",
          "original_form": "오시게 시장있는 동네라",
          "speaker_id": "2",
          "time": "20210828 18:16"
        },
        {
          "id": "MDRW2100034724.1.15",
          "form": "휴 암튼 서울 놀러가면 꼭 들릴거에여",
          "original_form": "휴 암튼 서울 놀러가면 꼭 들릴거에여",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034724.1.16",
          "form": "아 이름도 바꾸면서 가셨군요",
          "original_form": "아 이름도 바꾸면서 가셨군요",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034724.1.17",
          "form": "네 그래도 오시게 이름을 살리면서 갔다니 놀랍네요...ㅋㅋ",
          "original_form": "네 그래도 오시게 이름을 살리면서 갔다니 놀랍네요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034724.1.18",
          "form": "그러게요 ㅋㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034724.1.19",
          "form": "서울로 오셨는데 ㅋㅋㅋ",
          "original_form": "서울로 오셨는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:18"
        }
