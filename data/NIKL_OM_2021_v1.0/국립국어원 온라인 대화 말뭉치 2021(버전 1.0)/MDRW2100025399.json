      "utterance": [
        {
          "id": "MDRW2100025399.1.1",
          "form": "취업 하기 힘들네요",
          "original_form": "취업 하기 힘들네요",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025399.1.2",
          "form": "그렇죠??ㅠ",
          "original_form": "그렇죠??ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025399.1.3",
          "form": "살기도 힘드네요;;",
          "original_form": "살기도 힘드네요;;",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025399.1.4",
          "form": "ㅜㅜ name2님은 취업 하셧어요??",
          "original_form": "ㅜㅜ &name2&님은 취업 하셧어요??",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025399.1.5",
          "form": "전 지금 애기 땜에 아르바이트 하고 있어요ㅠ",
          "original_form": "전 지금 애기 땜에 아르바이트 하고 있어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025399.1.6",
          "form": "지역 준비 중이신가요?",
          "original_form": "지역 준비 중이신가요?",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025399.1.7",
          "form": "취업",
          "original_form": "취업",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025399.1.8",
          "form": "넵! 저는 면접 보고있는데",
          "original_form": "넵! 저는 면접 보고있는데",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025399.1.9",
          "form": "복지가 너무 구리네요 ㅜㅜ",
          "original_form": "복지가 너무 구리네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025399.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025399.1.11",
          "form": "돈은 쥐꼬리만큼...물가는 너무 비싸고.",
          "original_form": "돈은 쥐꼬리만큼...물가는 너무 비싸고.",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025399.1.12",
          "form": "코로나라서 더 그런가봐요",
          "original_form": "코로나라서 더 그런가봐요",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025399.1.13",
          "form": "이제 돈도 없어서 알바하면서 면접 봐야겠어요 ㅋㅋ",
          "original_form": "이제 돈도 없어서 알바하면서 면접 봐야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025399.1.14",
          "form": "그렇죠 너도 나도 다 힘든 시기라서ㅠ",
          "original_form": "그렇죠 너도 나도 다 힘든 시기라서ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025399.1.15",
          "form": "알바는 어떤거 하고 계세요??",
          "original_form": "알바는 어떤거 하고 계세요??",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025399.1.16",
          "form": "지인 카페에서 일하고 있어요~",
          "original_form": "지인 카페에서 일하고 있어요~",
          "speaker_id": "2",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025399.1.17",
          "form": "애들 학교 보내고 오전에만~",
          "original_form": "애들 학교 보내고 오전에만~",
          "speaker_id": "2",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025399.1.18",
          "form": "아르바이트해보셨어요??",
          "original_form": "아르바이트해보셨어요??",
          "speaker_id": "2",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025399.1.19",
          "form": "저는 부모님이 가게하셔서",
          "original_form": "저는 부모님이 가게하셔서",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100025399.1.20",
          "form": "부모님 가게에서 도와드렷어요!!",
          "original_form": "부모님 가게에서 도와드렷어요!!",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100025399.1.21",
          "form": "그럼 부모님이 일도 도와드리고 돈도 벌고",
          "original_form": "그럼 부모님이 일도 도와드리고 돈도 벌고",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100025399.1.22",
          "form": "일석이조 네요~",
          "original_form": "일석이조 네요~",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100025399.1.23",
