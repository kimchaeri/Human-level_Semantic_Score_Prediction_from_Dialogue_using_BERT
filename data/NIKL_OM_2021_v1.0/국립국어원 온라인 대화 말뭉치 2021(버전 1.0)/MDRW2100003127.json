      "utterance": [
        {
          "id": "MDRW2100003127.1.1",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20210820 19:12"
        },
        {
          "id": "MDRW2100003127.1.2",
          "form": "네 아주 간단하게 오이소박이를..ㅋ",
          "original_form": "네 아주 간단하게 오이소박이를..ㅋ",
          "speaker_id": "1",
          "time": "20210820 19:46"
        },
        {
          "id": "MDRW2100003127.1.3",
          "form": "오 오이소박이  좋아하는데ㅎ 설마 그것만 드신거예요?",
          "original_form": "오 오이소박이  좋아하는데ㅎ 설마 그것만 드신거예요?",
          "speaker_id": "2",
          "time": "20210820 19:47"
        },
        {
          "id": "MDRW2100003127.1.4",
          "form": "네 더워서 다 귀찮네요ㅋㅋ",
          "original_form": "네 더워서 다 귀찮네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:47"
        },
        {
          "id": "MDRW2100003127.1.5",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210820 19:47"
        },
        {
          "id": "MDRW2100003127.1.6",
          "form": "전 생선까스 먹고 퇴근요ㅎ",
          "original_form": "전 생선까스 먹고 퇴근요ㅎ",
          "speaker_id": "2",
          "time": "20210820 19:47"
        },
        {
          "id": "MDRW2100003127.1.7",
          "form": "앗 회사에서 밥이 나오나요?",
          "original_form": "앗 회사에서 밥이 나오나요?",
          "speaker_id": "1",
          "time": "20210820 19:48"
        },
        {
          "id": "MDRW2100003127.1.8",
          "form": "그냥 저녁은 법카로 먹을수있어요 대신 야근해야되는데 몰래먹고 튀었죠ㅎㅎ",
          "original_form": "그냥 저녁은 법카로 먹을수있어요 대신 야근해야되는데 몰래먹고 튀었죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003127.1.9",
          "form": "ㅋㅋㅋㅋㅋ  먹튀네요",
          "original_form": "ㅋㅋㅋㅋㅋ  먹튀네요",
          "speaker_id": "1",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003127.1.10",
          "form": "생선까스만도 파나요?",
          "original_form": "생선까스만도 파나요?",
          "speaker_id": "1",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003127.1.11",
          "form": "생선까스랑 밥주는데 전 밥은 패스했어요",
          "original_form": "생선까스랑 밥주는데 전 밥은 패스했어요",
          "speaker_id": "2",
          "time": "20210820 19:50"
        },
        {
          "id": "MDRW2100003127.1.12",
          "form": "근데 느끼해서 오이소박이 넘 먹고싶은데요?ㅋㅋ",
          "original_form": "근데 느끼해서 오이소박이 넘 먹고싶은데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:50"
        },
        {
          "id": "MDRW2100003127.1.13",
          "form": "네 오이소박이를요",
          "original_form": "네 오이소박이를요",
          "speaker_id": "1",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003127.1.14",
          "form": "소면을 삶아서",
          "original_form": "소면을 삶아서",
          "speaker_id": "1",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003127.1.15",
          "form": "냉면육수 파는거 넣고",
          "original_form": "냉면육수 파는거 넣고",
          "speaker_id": "1",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003127.1.16",
          "form": "같이 먹으면 좋아요",
          "original_form": "같이 먹으면 좋아요",
          "speaker_id": "1",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003127.1.17",
          "form": "오ㅎ  맛있겠네요 시원하고",
          "original_form": "오ㅎ  맛있겠네요 시원하고",
          "speaker_id": "2",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003127.1.18",
          "form": "아삭아삭 식감이랑",
          "original_form": "아삭아삭 식감이랑",
