      "utterance": [
        {
          "id": "MDRW2100039048.1.1",
          "form": "중고거래 해보셨나요?",
          "original_form": "중고거래 해보셨나요?",
          "speaker_id": "2",
          "time": "20210903 07:21"
        },
        {
          "id": "MDRW2100039048.1.2",
          "form": "해본적은 있는데",
          "original_form": "해본적은 있는데",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039048.1.3",
          "form": "별로 좋아하지는 않아요",
          "original_form": "별로 좋아하지는 않아요",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039048.1.4",
          "form": "중고보다는 새물건 사는 걸 좋아하는군요",
          "original_form": "중고보다는 새물건 사는 걸 좋아하는군요",
          "speaker_id": "2",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100039048.1.5",
          "form": "미개봉",
          "original_form": "미개봉",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039048.1.6",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039048.1.7",
          "form": "별로 좋아하지 않아서요",
          "original_form": "별로 좋아하지 않아서요",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039048.1.8",
          "form": "근데 생필품은 중고거래 해요",
          "original_form": "근데 생필품은 중고거래 해요",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039048.1.9",
          "form": "당근마켓ㅋㅋㅋ",
          "original_form": "당근마켓ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039048.1.10",
          "form": "저희집도 당근마켓을  새로 알게돼서 요새 해요",
          "original_form": "저희집도 당근마켓을  새로 알게돼서 요새 해요",
          "speaker_id": "2",
          "time": "20210904 05:24"
        },
        {
          "id": "MDRW2100039048.1.11",
          "form": "집에 있는 물건 좀 정리하고 싶으데 그냥 버리기 아까워서요",
          "original_form": "집에 있는 물건 좀 정리하고 싶으데 그냥 버리기 아까워서요",
          "speaker_id": "2",
          "time": "20210904 05:24"
        },
        {
          "id": "MDRW2100039048.1.12",
          "form": "당근 추천받아서 해보려고",
          "original_form": "당근 추천받아서 해보려고",
          "speaker_id": "1",
          "time": "20210904 07:57"
        },
        {
          "id": "MDRW2100039048.1.13",
          "form": "옷 깨끗한거 위주로 진짜 싸게 내놨는데도",
          "original_form": "옷 깨끗한거 위주로 진짜 싸게 내놨는데도",
          "speaker_id": "1",
          "time": "20210904 07:57"
        },
        {
          "id": "MDRW2100039048.1.14",
          "form": "안팔리더라고요.....하하",
          "original_form": "안팔리더라고요.....하하",
          "speaker_id": "1",
          "time": "20210904 07:57"
        },
        {
          "id": "MDRW2100039048.1.15",
          "form": "그래요? 그게 참 알수가 없어요",
          "original_form": "그래요? 그게 참 알수가 없어요",
          "speaker_id": "2",
          "time": "20210904 07:59"
        },
        {
          "id": "MDRW2100039048.1.16",
          "form": "어떤건 금방 나가고 어떤건 오랫동안 안나가고",
          "original_form": "어떤건 금방 나가고 어떤건 오랫동안 안나가고",
          "speaker_id": "2",
          "time": "20210904 07:59"
        },
        {
          "id": "MDRW2100039048.1.17",
          "form": "동네분들의 필요를 파악해야하나봐요..ㅎㅎ",
          "original_form": "동네분들의 필요를 파악해야하나봐요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 07:59"
        },
        {
          "id": "MDRW2100039048.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 08:03"
        },
        {
          "id": "MDRW2100039048.1.19",
          "form": "그리고 오래된 책이긴 하지만",
          "original_form": "그리고 오래된 책이긴 하지만",
          "speaker_id": "1",
          "time": "20210904 08:03"
        },
        {
          "id": "MDRW2100039048.1.20",
          "form": "역사전집은 꼭 팔리긴 하더라고요?",
          "original_form": "역사전집은 꼭 팔리긴 하더라고요?",
          "speaker_id": "1",
          "time": "20210904 08:03"
        },
        {
          "id": "MDRW2100039048.1.21",
          "form": "동네 거래내역이 드물지만 있길래",
          "original_form": "동네 거래내역이 드물지만 있길래",
          "speaker_id": "1",
          "time": "20210904 08:04"
        },
        {
          "id": "MDRW2100039048.1.22",
          "form": "가격도 제일 착하게 올렸는데",
          "original_form": "가격도 제일 착하게 올렸는데",
          "speaker_id": "1",
          "time": "20210904 08:04"
        },
        {
          "id": "MDRW2100039048.1.23",
          "form": "거의 일년째 안 나가요ㅋㅋㅋㅋ",
          "original_form": "거의 일년째 안 나가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 08:04"
        },
        {
          "id": "MDRW2100039048.1.24",
          "form": "자리차지만 하고 그냥 버릴까 싶어요",
          "original_form": "자리차지만 하고 그냥 버릴까 싶어요",
          "speaker_id": "1",
          "time": "20210904 08:04"
        },
        {
          "id": "MDRW2100039048.1.25",
          "form": "일년이면 꽤 오래네요",
          "original_form": "일년이면 꽤 오래네요",
          "speaker_id": "2",
          "time": "20210904 08:05"
        },
        {
          "id": "MDRW2100039048.1.26",
          "form": "당근하면서 전 장사는 하지 말아야겠다고 느꼈어요",
          "original_form": "당근하면서 전 장사는 하지 말아야겠다고 느꼈어요",
          "speaker_id": "2",
          "time": "20210904 08:06"
        },
        {
          "id": "MDRW2100039048.1.27",
          "form": "고객의 니즈를 파악하는게 이리 어려울 줄이야",
          "original_form": "고객의 니즈를 파악하는게 이리 어려울 줄이야",
          "speaker_id": "2",
          "time": "20210904 08:06"
        },
        {
          "id": "MDRW2100039048.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 08:54"
        },
        {
          "id": "MDRW2100039048.1.29",
          "form": "그리고 당근마켓같은 동네중고거래는",
          "original_form": "그리고 당근마켓같은 동네중고거래는",
          "speaker_id": "1",
          "time": "20210904 08:54"
        },
        {
          "id": "MDRW2100039048.1.30",
          "form": "집근처에서 하는게 장점인데",
          "original_form": "집근처에서 하는게 장점인데",
          "speaker_id": "1",
          "time": "20210904 08:54"
        },
        {
          "id": "MDRW2100039048.1.31",
          "form": "상대방 오라고 하면 되는데",
          "original_form": "상대방 오라고 하면 되는데",
          "speaker_id": "1",
          "time": "20210904 08:54"
        },
        {
          "id": "MDRW2100039048.1.32",
          "form": "제가 자꾸 그쪽으로ㅠ가게되더라고요ㅋㅋㅋㅋ",
          "original_form": "제가 자꾸 그쪽으로ㅠ가게되더라고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 08:54"
        },
        {
          "id": "MDRW2100039048.1.33",
          "form": "천원짜리 바지파는데 ㅋㅋㅋㅋㅋㅋㅋ버스비가 천원이 넘더라고요 이런...",
          "original_form": "천원짜리 바지파는데 ㅋㅋㅋㅋㅋㅋㅋ버스비가 천원이 넘더라고요 이런...",
          "speaker_id": "1",
          "time": "20210904 08:55"
        },
        {
          "id": "MDRW2100039048.1.34",
          "form": "이런.. 맘이 약하시군요",
          "original_form": "이런.. 맘이 약하시군요",
          "speaker_id": "2",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100039048.1.35",
          "form": "전 무조건 저희 아파트로 안오면  거래 안해요",
          "original_form": "전 무조건 저희 아파트로 안오면  거래 안해요",
          "speaker_id": "2",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100039048.1.36",
          "form": "아예 글에 그렇게 써놨어요",
          "original_form": "아예 글에 그렇게 써놨어요",
          "speaker_id": "2",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100039048.1.37",
          "form": "저는 그냥 빨리 팔아버리고싶은마음에..ㅋㅋㅋㅋ",
          "original_form": "저는 그냥 빨리 팔아버리고싶은마음에..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100039048.1.38",
          "form": "그후론 거의 안하고",
          "original_form": "그후론 거의 안하고",
