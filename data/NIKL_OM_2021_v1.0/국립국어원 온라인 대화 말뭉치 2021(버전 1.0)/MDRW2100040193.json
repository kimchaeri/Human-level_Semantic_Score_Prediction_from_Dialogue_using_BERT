          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100040193.1.2",
          "form": "구구는요",
          "original_form": "구구는요",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100040193.1.3",
          "form": "구구도 먹었어요",
          "original_form": "구구도 먹었어요",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100040193.1.4",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100040193.1.5",
          "form": "솥밥요",
          "original_form": "솥밥요",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100040193.1.6",
          "form": "구구는머요",
          "original_form": "구구는머요",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100040193.1.7",
          "form": "구구는 감바스요",
          "original_form": "구구는 감바스요",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100040193.1.8",
          "form": "ㅠ대박 밀키트?",
          "original_form": "ㅠ대박 밀키트?",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100040193.1.9",
          "form": "넹넹 밀키트",
          "original_form": "넹넹 밀키트",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100040193.1.10",
          "form": "근데 스테이크가 더 맛있었어요",
          "original_form": "근데 스테이크가 더 맛있었어요",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100040193.1.11",
          "form": "어헝 혼자드셧나요",
          "original_form": "어헝 혼자드셧나요",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100040193.1.12",
          "form": "아니용 어머니랑 먹었어요",
          "original_form": "아니용 어머니랑 먹었어요",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100040193.1.13",
          "form": "제가 오늘 점심 해드린다고해서 ㅋㅋ",
          "original_form": "제가 오늘 점심 해드린다고해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100040193.1.14",
          "form": "효녀군요",
          "original_form": "효녀군요",
          "speaker_id": "2",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040193.1.15",
          "form": "아보진요",
          "original_form": "아보진요",
          "speaker_id": "2",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040193.1.16",
          "form": "아보진 나갓어용",
          "original_form": "아보진 나갓어용",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040193.1.17",
          "form": "엄마는 내가 집에있는다고해서",
          "original_form": "엄마는 내가 집에있는다고해서",
          "speaker_id": "1",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100040193.1.18",
          "form": "안나갔는데 저 점심먹고 친구들보러 나가요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "안나갔는데 저 점심먹고 친구들보러 나가요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100040193.1.19",
          "form": "어머낭",
          "original_form": "어머낭",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100040193.1.20",
          "form": "어머니 데리구가용",
          "original_form": "어머니 데리구가용",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100040193.1.21",
          "form": "제가 나간다고 하니까 엄마도 자기도 나가겠대용",
          "original_form": "제가 나간다고 하니까 엄마도 자기도 나가겠대용",
          "speaker_id": "1",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100040193.1.22",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "1",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100040193.1.23",
          "form": "ㅋㅋㅋ 어디가신대요 마싯는거무러?",
          "original_form": "ㅋㅋㅋ 어디가신대요 마싯는거무러?",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100040193.1.24",
          "form": "아니용 그냥 친구랑 마트가나봐용ㅋㅋㅋㅋㅋ참외 먹고싶다고 과일사러간대용ㅋㅋㅋ",
          "original_form": "아니용 그냥 친구랑 마트가나봐용ㅋㅋㅋㅋㅋ참외 먹고싶다고 과일사러간대용ㅋㅋㅋ",
