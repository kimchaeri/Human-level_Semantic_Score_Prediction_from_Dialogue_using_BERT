      "utterance": [
        {
          "id": "MDRW2100029618.1.1",
          "form": "벌들의 수를",
          "original_form": "벌들의 수를",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029618.1.2",
          "form": "늘릴수도 있어요",
          "original_form": "늘릴수도 있어요",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029618.1.3",
          "form": "농경도 할수 있어요",
          "original_form": "농경도 할수 있어요",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029618.1.4",
          "form": "왜 늘리나요",
          "original_form": "왜 늘리나요",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029618.1.5",
          "form": "경작 좋네요",
          "original_form": "경작 좋네요",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029618.1.6",
          "form": "절 공격하고 죽으면",
          "original_form": "절 공격하고 죽으면",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.7",
          "form": "일꾼이 줄잗ㅇ아요",
          "original_form": "일꾼이 줄잗ㅇ아요",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.8",
          "form": "딸기 키우게",
          "original_form": "딸기 키우게",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.9",
          "form": "늘려야죠",
          "original_form": "늘려야죠",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.10",
          "form": "경작 할수 있는",
          "original_form": "경작 할수 있는",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.11",
          "form": "벌처럼 일하는군요",
          "original_form": "벌처럼 일하는군요",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.12",
          "form": "종류가 몇개없어요",
          "original_form": "종류가 몇개없어요",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.13",
          "form": "밀 감자 당근 호박",
          "original_form": "밀 감자 당근 호박",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.14",
          "form": "재밌는게임이네요",
          "original_form": "재밌는게임이네요",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.15",
          "form": "감자는 지겹군요",
          "original_form": "감자는 지겹군요",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.16",
          "form": "고기 구워먹을수 있어요",
          "original_form": "고기 구워먹을수 있어요",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.17",
          "form": "구운감자도 먹을수있어요",
          "original_form": "구운감자도 먹을수있어요",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029618.1.18",
          "form": "오 현실에서 먹어야하는디",
          "original_form": "오 현실에서 먹어야하는디",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029618.1.19",
          "form": "호박 가위로 자르면 할로윈 호박얼굴되는데",
          "original_form": "호박 가위로 자르면 할로윈 호박얼굴되는데",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029618.1.20",
          "form": "한번 해보고싶네요",
          "original_form": "한번 해보고싶네요",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029618.1.21",
          "form": "초넣으면 등불되고",
          "original_form": "초넣으면 등불되고",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029618.1.22",
          "form": "이쁘군요",
          "original_form": "이쁘군요",
          "speaker_id": "1",
          "time": "20210903 11:00"
        }
      ]
