        {
          "id": "MDRW2100005418.1.1",
          "form": "ㅋㅋㅋㅋㅋㄱ 의견반영 감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 의견반영 감사합니다",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005418.1.2",
          "form": "ㅋㅋㅋㅋㅋ근데여 name2언니",
          "original_form": "ㅋㅋㅋㅋㅋ근데여 &name2&언니",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005418.1.3",
          "form": "나 잠와여 ㅋㅋㅋ",
          "original_form": "나 잠와여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005418.1.4",
          "form": "ㅋㅋㅋㅋ 어여자라",
          "original_form": "ㅋㅋㅋㅋ 어여자라",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005418.1.5",
          "form": "잠들었겠네 ㅋㅋ 난 이제 육퇴했다\r\n빠잉",
          "original_form": "잠들었겠네 ㅋㅋ 난 이제 육퇴했다\r\n빠잉",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005418.1.6",
          "form": "육아 시작 ㅋㅋㅋ집안일 시작ㅋㅋ",
          "original_form": "육아 시작 ㅋㅋㅋ집안일 시작ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100005418.1.7",
          "form": "일어나요 온니 ㅋㅋ",
          "original_form": "일어나요 온니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100005418.1.8",
          "form": "ㅋㅋㅋㅋ 눈떴다",
          "original_form": "ㅋㅋㅋㅋ 눈떴다",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100005418.1.9",
          "form": "대구왔나??",
          "original_form": "대구왔나??",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100005418.1.10",
          "form": "눈뜨자 마자 ㅋㅋ시작ㅋㅋ",
          "original_form": "눈뜨자 마자 ㅋㅋ시작ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005418.1.11",
          "form": "아뇨~우린 저녁먹고 버틸수있는만큼 버티고 갈꺼임 ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨~우린 저녁먹고 버틸수있는만큼 버티고 갈꺼임 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005418.1.12",
          "form": "집에 밥도 없는데 ㅋㅋㅋ",
          "original_form": "집에 밥도 없는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005418.1.13",
          "form": "가면 할꺼 늠.많음 ㅋㅋㅋ",
          "original_form": "가면 할꺼 늠.많음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005418.1.14",
          "form": "ㅋㅋㅋㅋ 그래 버티다가야지",
          "original_form": "ㅋㅋㅋㅋ 그래 버티다가야지",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005418.1.15",
          "form": "잘하고 있네 ㅋㅋㅋㅋㄱ",
          "original_form": "잘하고 있네 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005418.1.16",
          "form": "애들은 일찍 일어났나??",
          "original_form": "애들은 일찍 일어났나??",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005418.1.17",
          "form": "name3는 6시 ㅋㅋㅋㅋname4이는 요즘 7시 ㅋㅋㅋㅋ",
          "original_form": "&name3&는 6시 ㅋㅋㅋㅋ&name4&이는 요즘 7시 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005418.1.18",
          "form": "겨울되믄 진짜 늦잠좀 잤으면 ㅋㅋ",
          "original_form": "겨울되믄 진짜 늦잠좀 잤으면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005418.1.19",
          "form": "그래도name3 많이 나아졌네",
          "original_form": "그래도&name3& 많이 나아졌네",
          "speaker_id": "2",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005418.1.20",
          "form": "낮잠은 자나??",
          "original_form": "낮잠은 자나??",
          "speaker_id": "2",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005418.1.21",
          "form": "우리가족은 신랑빼고 다 초저녁잠 많은듯여~",
          "original_form": "우리가족은 신랑빼고 다 초저녁잠 많은듯여~",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005418.1.22",
          "form": "낮잠은 잡니당 ㅋㅋ",
          "original_form": "낮잠은 잡니당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005418.1.23",
          "form": "짧게 자나 길게자나 자는시간은 같고..",
          "original_form": "짧게 자나 길게자나 자는시간은 같고..",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005418.1.24",
          "form": "ㅋㅋㅋㅋ 초저녁 ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋ 초저녁 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100005418.1.25",
          "form": "일찍 자본적이 잘 없어서",
          "original_form": "일찍 자본적이 잘 없어서",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100005418.1.26",
          "form": "일찍 자는것도 좋은듯",
          "original_form": "일찍 자는것도 좋은듯",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100005418.1.27",
          "form": "늦게자면 많이자도 뭔가 피곤함",
          "original_form": "늦게자면 많이자도 뭔가 피곤함",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100005418.1.28",
          "form": "울 신랑이 그려요~근데",
          "original_form": "울 신랑이 그려요~근데",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100005418.1.29",
          "form": "나는 하루가 너무 짧아요ㅠ",
          "original_form": "나는 하루가 너무 짧아요ㅠ",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100005418.1.30",
          "form": "진짜 저녁에 버티고 싶은데",
          "original_form": "진짜 저녁에 버티고 싶은데",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100005418.1.31",
          "form": "ㅋㅋㅋㅋ 그렇다",
          "original_form": "ㅋㅋㅋㅋ 그렇다",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100005418.1.32",
          "form": "내도록 애들이랑 함께 하는..",
          "original_form": "내도록 애들이랑 함께 하는..",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100005418.1.33",
          "form": "너무 피곤해가 눈물이 계속 남 ㅋㅋㅋㅋ",
          "original_form": "너무 피곤해가 눈물이 계속 남 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100005418.1.34",
          "form": "ㅋㅋㅋㅋㅋㄱㄱ 슬프노",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱ 슬프노",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100005418.1.35",
          "form": "애들 재우기 전에 집안일 다 끝내놓고 자야하니",
          "original_form": "애들 재우기 전에 집안일 다 끝내놓고 자야하니",
          "speaker_id": "1",
          "time": "20210822 10:13"
        },
        {
          "id": "MDRW2100005418.1.36",
          "form": "6시부터 겁나 바쁨 ㅋㅋㅋ",
          "original_form": "6시부터 겁나 바쁨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:13"
        },
        {
          "id": "MDRW2100005418.1.37",
          "form": "요즘 빨래고 겁나 쌓아둬요 ㅋㅋㅋ",
