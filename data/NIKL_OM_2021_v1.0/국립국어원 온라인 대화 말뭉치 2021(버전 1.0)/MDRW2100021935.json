      "utterance": [
        {
          "id": "MDRW2100021935.1.1",
          "form": "그니까 난 괜찮은데",
          "original_form": "그니까 난 괜찮은데",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021935.1.2",
          "form": "그러니까 나만 그래 나만",
          "original_form": "그러니까 나만 그래 나만",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021935.1.3",
          "form": "ㅋㅋㅋㅋㅋ 장만 좋으면 좋을텐데",
          "original_form": "ㅋㅋㅋㅋㅋ 장만 좋으면 좋을텐데",
          "speaker_id": "2",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021935.1.4",
          "form": "살도 안 찌면 좋을 텐데",
          "original_form": "살도 안 찌면 좋을 텐데",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021935.1.5",
          "form": "난 살이 안쩌서",
          "original_form": "난 살이 안쩌서",
          "speaker_id": "2",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021935.1.6",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021935.1.7",
          "form": "ㅋㅋㅋ 감사",
          "original_form": "ㅋㅋㅋ 감사",
          "speaker_id": "2",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021935.1.8",
          "form": "다이어트 해야 하는데 나는",
          "original_form": "다이어트 해야 하는데 나는",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021935.1.9",
          "form": "다이어트 하고 있지 않았어?",
          "original_form": "다이어트 하고 있지 않았어?",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021935.1.10",
          "form": "요즘 안 했어",
          "original_form": "요즘 안 했어",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021935.1.11",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021935.1.12",
          "form": "응 난 요즘은 안했지",
          "original_form": "응 난 요즘은 안했지",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021935.1.13",
          "form": "근데 요즘은 밖엔 잘 안나가니까 다이어트 안해도 되지 않나?",
          "original_form": "근데 요즘은 밖엔 잘 안나가니까 다이어트 안해도 되지 않나?",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021935.1.14",
          "form": "영원히 안 나갈게 아니니까",
          "original_form": "영원히 안 나갈게 아니니까",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021935.1.15",
          "form": "그건 그런데 빡세게 안해도 된다는 거지",
          "original_form": "그건 그런데 빡세게 안해도 된다는 거지",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021935.1.16",
          "form": "건강 때문에 하는 것도 있어",
          "original_form": "건강 때문에 하는 것도 있어",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021935.1.17",
          "form": "머리를 진짜 잘라 볼까 생각중이야",
          "original_form": "머리를 진짜 잘라 볼까 생각중이야",
