      "utterance": [
        {
          "id": "MDRW2100029088.1.1",
          "form": "학교 일 하느라 바빴는데",
          "original_form": "학교 일 하느라 바빴는데",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029088.1.2",
          "form": "너랑 얘기하려고 미루고 왔어.",
          "original_form": "너랑 얘기하려고 미루고 왔어.",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029088.1.3",
          "form": "스윗하지",
          "original_form": "스윗하지",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029088.1.4",
          "form": "웃겨 뭐하는데",
          "original_form": "웃겨 뭐하는데",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029088.1.5",
          "form": "스윗하다...",
          "original_form": "스윗하다...",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029088.1.6",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029088.1.7",
          "form": "그... 비품 폐기를 해줘야 하는데",
          "original_form": "그... 비품 폐기를 해줘야 하는데",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.8",
          "form": "그걸 학교에서 반년째 안해주고 있어서",
          "original_form": "그걸 학교에서 반년째 안해주고 있어서",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.9",
          "form": "분노의 자료 수집과... 상황 작성을 하고 있었어",
          "original_form": "분노의 자료 수집과... 상황 작성을 하고 있었어",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.10",
          "form": "아오 학교 일해",
          "original_form": "아오 학교 일해",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.11",
          "form": "안타깝다",
          "original_form": "안타깝다",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.12",
          "form": "괜찮아",
          "original_form": "괜찮아",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.13",
          "form": "원래 학생회에서 하는 일인가요",
          "original_form": "원래 학생회에서 하는 일인가요",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.14",
          "form": "그런 편이죠",
          "original_form": "그런 편이죠",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029088.1.15",
          "form": "이걸 작년에 넣은 건데",
          "original_form": "이걸 작년에 넣은 건데",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029088.1.16",
          "form": "계속 말 바꾸고",
          "original_form": "계속 말 바꾸고",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029088.1.17",
          "form": "폐기 결정 됐다는 말 들은지 3일만에",
          "original_form": "폐기 결정 됐다는 말 들은지 3일만에",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029088.1.18",
          "form": "절차가 바뀌어서<< 요...러고 계셔서",
          "original_form": "절차가 바뀌어서<< 요...러고 계셔서",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029088.1.19",
          "form": "ㅋㅋ...",
          "original_form": "ㅋㅋ...",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029088.1.20",
          "form": "부회장이랑 분노의 타이핑을 치고 있었음",
          "original_form": "부회장이랑 분노의 타이핑을 치고 있었음",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029088.1.21",
          "form": "왤케 주먹구구식이야",
          "original_form": "왤케 주먹구구식이야",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029088.1.22",
          "form": "ㅁㄹ겠음",
          "original_form": "ㅁㄹ겠음",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029088.1.23",
          "form": "우리 과 최고의 파이터가 기사님이랑 학과장님이신데",
          "original_form": "우리 과 최고의 파이터가 기사님이랑 학과장님이신데",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029088.1.24",
          "form": "두 분을 섭외해서 싸움하려고.",
          "original_form": "두 분을 섭외해서 싸움하려고.",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029088.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.26",
          "form": "화이팅",
          "original_form": "화이팅",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.27",
          "form": "약간 그 타 팀가서 싸우는 대리처럼",
          "original_form": "약간 그 타 팀가서 싸우는 대리처럼",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.28",
          "form": "그래도 학과장이 싸우는 데라도 쓸모가 있다는 건",
          "original_form": "그래도 학과장이 싸우는 데라도 쓸모가 있다는 건",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.29",
          "form": "좋은 일이야",
          "original_form": "좋은 일이야",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.30",
          "form": "맞는 말이야",
          "original_form": "맞는 말이야",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.31",
          "form": "평소에는 우리랑 싸우시지만",
          "original_form": "평소에는 우리랑 싸우시지만",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.32",
          "form": "우리과 전 학과장 이런 놈이면",
          "original_form": "우리과 전 학과장 이런 놈이면",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.33",
          "form": "싸우는 데에도 쓸모가 없는,",
          "original_form": "싸우는 데에도 쓸모가 없는,",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029088.1.34",
          "form": "진짜안타깝다,",
          "original_form": "진짜안타깝다,",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029088.1.35",
          "form": "이젠 아니니까 괜찮아",
          "original_form": "이젠 아니니까 괜찮아",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029088.1.36",
          "form": "다음 주 개강총회라서",
          "original_form": "다음 주 개강총회라서",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029088.1.37",
          "form": "그거 준비해야해",
          "original_form": "그거 준비해야해",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029088.1.38",
          "form": "할 일이 산더미",
          "original_form": "할 일이 산더미",
          "speaker_id": "2",
          "time": "20210902 23:43"
        }
      ]
    }
