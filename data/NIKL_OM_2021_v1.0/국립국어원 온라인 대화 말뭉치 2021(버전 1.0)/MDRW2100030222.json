      "utterance": [
        {
          "id": "MDRW2100030222.1.1",
          "form": "닭목맛나요",
          "original_form": "닭목맛나요",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.2",
          "form": "페리카나에서닭목살로",
          "original_form": "페리카나에서닭목살로",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.3",
          "form": "양념인가요",
          "original_form": "양념인가요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.4",
          "form": "튀긴거",
          "original_form": "튀긴거",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.5",
          "form": "신상인가요",
          "original_form": "신상인가요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.6",
          "form": "출시한지 조금됐는데",
          "original_form": "출시한지 조금됐는데",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.7",
          "form": "처음듣네요",
          "original_form": "처음듣네요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.8",
          "form": "인기가 매우많다네요",
          "original_form": "인기가 매우많다네요",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.9",
          "form": "저도 먹어보려했는데",
          "original_form": "저도 먹어보려했는데",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.10",
          "form": "게임과 음식중 고른다면?",
          "original_form": "게임과 음식중 고른다면?",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.11",
          "form": "없어서 갔는데도 못먹었머묘",
          "original_form": "없어서 갔는데도 못먹었머묘",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.12",
          "form": "음식이죠",
          "original_form": "음식이죠",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030222.1.13",
          "form": "둘다 전문가포스가 나요",
          "original_form": "둘다 전문가포스가 나요",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.14",
          "form": "먹어먀살죠",
          "original_form": "먹어먀살죠",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.15",
          "form": "진심입니다",
          "original_form": "진심입니다",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.16",
          "form": "닭목 양많나요",
          "original_form": "닭목 양많나요",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.17",
          "form": "맛집많이압니다",
          "original_form": "맛집많이압니다",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.18",
          "form": "한박스에 몇개죠",
          "original_form": "한박스에 몇개죠",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.19",
          "form": "담에같이가시죠",
          "original_form": "담에같이가시죠",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.20",
          "form": "가격대비 많대요",
          "original_form": "가격대비 많대요",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.21",
          "form": "기별도 안가게 생겨서요",
          "original_form": "기별도 안가게 생겨서요",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.22",
          "form": "맛있다던데요",
          "original_form": "맛있다던데요",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030222.1.23",
          "form": "저는 똥집 좋아합니다",
          "original_form": "저는 똥집 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 23:03"
        }
      ]
