        {
          "id": "MDRW2100045630.1.1",
          "form": "힌트 나오자마자 바로 맞추시다니 대단해요",
          "original_form": "힌트 나오자마자 바로 맞추시다니 대단해요",
          "speaker_id": "2",
          "time": "20210908 04:25"
        },
        {
          "id": "MDRW2100045630.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 토마토달걀볶음 좋아해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 토마토달걀볶음 좋아해서",
          "speaker_id": "1",
          "time": "20210908 04:26"
        },
        {
          "id": "MDRW2100045630.1.3",
          "form": "ㅋㅋㅋㅋㅋ마음으로 통했군요",
          "original_form": "ㅋㅋㅋㅋㅋ마음으로 통했군요",
          "speaker_id": "2",
          "time": "20210908 04:26"
        },
        {
          "id": "MDRW2100045630.1.4",
          "form": "앗 미래아침식사라니",
          "original_form": "앗 미래아침식사라니",
          "speaker_id": "2",
          "time": "20210908 04:26"
        },
        {
          "id": "MDRW2100045630.1.5",
          "form": "좋아요 ㅋㅋㅋㅋ맞춰볼게요",
          "original_form": "좋아요 ㅋㅋㅋㅋ맞춰볼게요",
          "speaker_id": "2",
          "time": "20210908 04:26"
        },
        {
          "id": "MDRW2100045630.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ미래형질문",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ미래형질문",
          "speaker_id": "1",
          "time": "20210908 04:26"
        },
        {
          "id": "MDRW2100045630.1.7",
          "form": "빵 드실건가요?",
          "original_form": "빵 드실건가요?",
          "speaker_id": "2",
          "time": "20210908 04:27"
        },
        {
          "id": "MDRW2100045630.1.8",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 04:27"
        },
        {
          "id": "MDRW2100045630.1.9",
          "form": "밀가루를 드실건가요?",
          "original_form": "밀가루를 드실건가요?",
          "speaker_id": "2",
          "time": "20210908 04:27"
        },
        {
          "id": "MDRW2100045630.1.10",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 04:27"
        },
        {
          "id": "MDRW2100045630.1.11",
          "form": "그렇담 아침도 쌀 드시나요",
          "original_form": "그렇담 아침도 쌀 드시나요",
          "speaker_id": "2",
          "time": "20210908 04:27"
        },
        {
          "id": "MDRW2100045630.1.12",
          "form": "쌀인데 밥은 아니에요",
          "original_form": "쌀인데 밥은 아니에요",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045630.1.13",
          "form": "쫀득해요?",
          "original_form": "쫀득해요?",
          "speaker_id": "2",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045630.1.14",
          "form": "네 쫀득해요",
          "original_form": "네 쫀득해요",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045630.1.15",
          "form": "떡이로군요! 맞죠?",
          "original_form": "떡이로군요! 맞죠?",
