      "utterance": [
        {
          "id": "MDRW2100041026.1.1",
          "form": "ㅋㅋㅋ 그래서 좋아요",
          "original_form": "ㅋㅋㅋ 그래서 좋아요",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100041026.1.2",
          "form": "아 그러고 보니 박막례할머니 국수상품 출시했다고 들었던것같아요",
          "original_form": "아 그러고 보니 박막례할머니 국수상품 출시했다고 들었던것같아요",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100041026.1.3",
          "form": "할머니 음식은 만드는거 보기만해도 재밌어요",
          "original_form": "할머니 음식은 만드는거 보기만해도 재밌어요",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100041026.1.4",
          "form": "국수도 맛있을거같아요",
          "original_form": "국수도 맛있을거같아요",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100041026.1.5",
          "form": "한번 사먹고 싶네요",
          "original_form": "한번 사먹고 싶네요",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100041026.1.6",
          "form": "name2 물국수vs비빔국수",
          "original_form": "&name2& 물국수vs비빔국수",
          "speaker_id": "1",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100041026.1.7",
          "form": "비빔국수!!!",
          "original_form": "비빔국수!!!",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100041026.1.8",
          "form": "어렵네요... 그래도 찐한 맛이 최고",
          "original_form": "어렵네요... 그래도 찐한 맛이 최고",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100041026.1.9",
          "form": "꺄 저는 물국이요ㅋㅋ",
          "original_form": "꺄 저는 물국이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100041026.1.10",
          "form": "비빔은 오이 많이 넣어주는거 좋아요",
          "original_form": "비빔은 오이 많이 넣어주는거 좋아요",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100041026.1.11",
          "form": "실은 둘 다 맛있어서 당장 끌리는 걸 골랐어요",
          "original_form": "실은 둘 다 맛있어서 당장 끌리는 걸 골랐어요",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100041026.1.12",
          "form": "아삭한 오이 좋죠!!",
          "original_form": "아삭한 오이 좋죠!!",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100041026.1.13",
          "form": "ㅋㅋ 계란두요",
          "original_form": "ㅋㅋ 계란두요",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100041026.1.14",
          "form": "삶아서 반넣어주시는거",
          "original_form": "삶아서 반넣어주시는거",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100041026.1.15",
          "form": "피자 드실때 하나만 먹어야한다면?\r\n피클vs콜라vs핫소스",
          "original_form": "피자 드실때 하나만 먹어야한다면?\r\n피클vs콜라vs핫소스",
          "speaker_id": "1",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100041026.1.16",
          "form": "핫소스!!",
          "original_form": "핫소스!!",
          "speaker_id": "2",
          "time": "20210905 15:32"
        },
        {
          "id": "MDRW2100041026.1.17",
          "form": "이건 무조건 핫소스예요",
          "original_form": "이건 무조건 핫소스예요",
          "speaker_id": "2",
          "time": "20210905 15:33"
        },
        {
          "id": "MDRW2100041026.1.18",
          "form": "name2님 핫소스 두조각당 한팩 필요하신분이신가요",
          "original_form": "&name2&님 핫소스 두조각당 한팩 필요하신분이신가요",
          "speaker_id": "1",
          "time": "20210905 15:33"
        },
        {
          "id": "MDRW2100041026.1.19",
          "form": "왜냐면 제가 그렇기 때문입니다ㅋㅋ",
          "original_form": "왜냐면 제가 그렇기 때문입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:34"
        },
        {
          "id": "MDRW2100041026.1.20",
          "form": "ㅋㅋㅋㅋ 네!! 핫소스 없으면 심심해요",
          "original_form": "ㅋㅋㅋㅋ 네!! 핫소스 없으면 심심해요",
          "speaker_id": "2",
          "time": "20210905 15:34"
        },
        {
          "id": "MDRW2100041026.1.21",
          "form": "피클, 콜라는 전혀 안먹어요 ㅋㅋ",
          "original_form": "피클, 콜라는 전혀 안먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100041026.1.22",
          "form": "와 대단해요",
          "original_form": "와 대단해요",
          "speaker_id": "1",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100041026.1.23",
          "form": "저는 사실 쫌 고민했거든요 셋다 잘먹어서",
          "original_form": "저는 사실 쫌 고민했거든요 셋다 잘먹어서",
          "speaker_id": "1",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100041026.1.24",
          "form": "그러시군요!!!",
          "original_form": "그러시군요!!!",
          "speaker_id": "2",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100041026.1.25",
          "form": "피클은 원래 좋아하진않고 콜라는 일부러 안먹으려고 하는편이에요",
          "original_form": "피클은 원래 좋아하진않고 콜라는 일부러 안먹으려고 하는편이에요",
          "speaker_id": "2",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100041026.1.26",
          "form": "네 콜라도 쫌 고민했어요 저도 당음료라 줄이고싶은데 잘안돼서ㅎㅎ",
          "original_form": "네 콜라도 쫌 고민했어요 저도 당음료라 줄이고싶은데 잘안돼서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100041026.1.27",
          "form": "음료수 좋아하세요??",
          "original_form": "음료수 좋아하세요??",
          "speaker_id": "2",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100041026.1.28",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100041026.1.29",
          "form": "뭐든 마시는거 좋아해요ㅎㅎ",
          "original_form": "뭐든 마시는거 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100041026.1.30",
          "form": "저는 음료를 원래부터 잘 안마셔서 참는게 어렵지않더라고요",
          "original_form": "저는 음료를 원래부터 잘 안마셔서 참는게 어렵지않더라고요",
          "speaker_id": "2",
          "time": "20210905 15:40"
        },
        {
          "id": "MDRW2100041026.1.31",
          "form": "그래서 식당이나 어디가도 물만 마셔요 ㅋㅋ",
          "original_form": "그래서 식당이나 어디가도 물만 마셔요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:40"
        },
        {
          "id": "MDRW2100041026.1.32",
          "form": "진짜 건강한습관이에요",
          "original_form": "진짜 건강한습관이에요",
          "speaker_id": "1",
          "time": "20210905 15:41"
        },
        {
          "id": "MDRW2100041026.1.33",
          "form": "저도 요즘 노력하고있습니당ㅎㅎ",
          "original_form": "저도 요즘 노력하고있습니당ㅎㅎ",
