      "utterance": [
        {
          "id": "MDRW2100027269.1.1",
          "form": "음식얘기는",
          "original_form": "음식얘기는",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027269.1.2",
          "form": "끝이없네요",
          "original_form": "끝이없네요",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.3",
          "form": "떡볶이 무척 좋아하시나봐요",
          "original_form": "떡볶이 무척 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.4",
          "form": "네 사는게 곧 음식입니다",
          "original_form": "네 사는게 곧 음식입니다",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.5",
          "form": "견과류는요",
          "original_form": "견과류는요",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.6",
          "form": "호박씨 좋아하나요",
          "original_form": "호박씨 좋아하나요",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.7",
          "form": "전잘안먹어요",
          "original_form": "전잘안먹어요",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.8",
          "form": "콩류별로안좋아해요",
          "original_form": "콩류별로안좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.9",
          "form": "밥에 아무것도 안넣나요",
          "original_form": "밥에 아무것도 안넣나요",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.10",
          "form": "해바라기씨",
          "original_form": "해바라기씨",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.11",
          "form": "초코코티은",
          "original_form": "초코코티은",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.12",
          "form": "코팅된건",
          "original_form": "코팅된건",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.13",
          "form": "마카다미아 좋습니다",
          "original_form": "마카다미아 좋습니다",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.14",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027269.1.15",
          "form": "마카다미아 맛있어요",
          "original_form": "마카다미아 맛있어요",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027269.1.16",
          "form": "병아리콩도 건강에 좋죠",
          "original_form": "병아리콩도 건강에 좋죠",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027269.1.17",
          "form": "호두도 좋아해요",
          "original_form": "호두도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027269.1.18",
          "form": "렌틸콩 홈플 1+1합니다",
          "original_form": "렌틸콩 홈플 1+1합니다",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027269.1.19",
          "form": "곶감에 치즈 들어간거",
          "original_form": "곶감에 치즈 들어간거",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027269.1.20",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
