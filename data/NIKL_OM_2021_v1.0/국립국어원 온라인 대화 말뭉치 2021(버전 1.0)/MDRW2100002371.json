      "utterance": [
        {
          "id": "MDRW2100002371.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002371.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002371.1.3",
          "form": "최근에 재밌게 읽으신 책이 있으신가요?",
          "original_form": "최근에 재밌게 읽으신 책이 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002371.1.4",
          "form": "전 로맨스 소설만 파요",
          "original_form": "전 로맨스 소설만 파요",
          "speaker_id": "2",
          "time": "20210820 10:38"
        },
        {
          "id": "MDRW2100002371.1.5",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210820 10:38"
        },
        {
          "id": "MDRW2100002371.1.6",
          "form": "앗 저도 로맨스소설만 봐요",
          "original_form": "앗 저도 로맨스소설만 봐요",
          "speaker_id": "1",
          "time": "20210820 10:38"
        },
        {
          "id": "MDRW2100002371.1.7",
          "form": "요즘은 카카오페이지나 시리즈에 있는 로맨스판타지소설이 재밌더라구요",
          "original_form": "요즘은 카카오페이지나 시리즈에 있는 로맨스판타지소설이 재밌더라구요",
          "speaker_id": "1",
          "time": "20210820 10:39"
        },
        {
          "id": "MDRW2100002371.1.8",
          "form": "어떤 장르 좋아해요?",
          "original_form": "어떤 장르 좋아해요?",
          "speaker_id": "2",
          "time": "20210820 10:39"
        },
        {
          "id": "MDRW2100002371.1.9",
          "form": "저는 로맨스 중에서도 로맨스 판타지장르를 좋아해요",
          "original_form": "저는 로맨스 중에서도 로맨스 판타지장르를 좋아해요",
          "speaker_id": "1",
          "time": "20210820 10:40"
        },
        {
          "id": "MDRW2100002371.1.10",
          "form": "지금 보고계시는 작품있으신가요?",
          "original_form": "지금 보고계시는 작품있으신가요?",
          "speaker_id": "1",
          "time": "20210820 10:40"
        },
        {
          "id": "MDRW2100002371.1.11",
          "form": "오오~ 판타지!",
          "original_form": "오오~ 판타지!",
          "speaker_id": "2",
          "time": "20210820 10:40"
        },
        {
          "id": "MDRW2100002371.1.12",
          "form": "전 이북 위주로 읽다가 종이책 정리하려고 안읽은거 읽는중이에요",
          "original_form": "전 이북 위주로 읽다가 종이책 정리하려고 안읽은거 읽는중이에요",
          "speaker_id": "2",
          "time": "20210820 10:40"
        },
        {
          "id": "MDRW2100002371.1.13",
          "form": "현대물 쪽으로",
          "original_form": "현대물 쪽으로",
          "speaker_id": "2",
          "time": "20210820 10:40"
        },
        {
          "id": "MDRW2100002371.1.14",
          "form": "아 현대물도 좋죠",
          "original_form": "아 현대물도 좋죠",
          "speaker_id": "1",
          "time": "20210820 10:42"
        },
        {
          "id": "MDRW2100002371.1.15",
          "form": "그런데 종이책이라 자꾸 딴짓하게 되네요 ㅎ",
          "original_form": "그런데 종이책이라 자꾸 딴짓하게 되네요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 10:42"
        },
        {
          "id": "MDRW2100002371.1.16",
          "form": "소설은 이북이 최고인 것 같아요",
          "original_form": "소설은 이북이 최고인 것 같아요",
          "speaker_id": "1",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002371.1.17",
          "form": "집중도 잘되고 편하고",
          "original_form": "집중도 잘되고 편하고",
          "speaker_id": "1",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002371.1.18",
          "form": "그 예전에 드라마 나왔던 소설이름이 뭐였죠",
          "original_form": "그 예전에 드라마 나왔던 소설이름이 뭐였죠",
          "speaker_id": "1",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002371.1.19",
          "form": "이태원 클라쓰 주인공이랑 나왔던것 같은데...",
          "original_form": "이태원 클라쓰 주인공이랑 나왔던것 같은데...",
          "speaker_id": "1",
          "time": "20210820 10:44"
        },
        {
          "id": "MDRW2100002371.1.20",
          "form": "김비서?",
          "original_form": "김비서?",
          "speaker_id": "2",
          "time": "20210820 10:45"
        },
        {
          "id": "MDRW2100002371.1.21",
          "form": "왜그런대 김비서? ㅋㅋㅋ",
          "original_form": "왜그런대 김비서? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:45"
        },
        {
          "id": "MDRW2100002371.1.22",
          "form": "아니에요?",
          "original_form": "아니에요?",
          "speaker_id": "2",
          "time": "20210820 10:45"
        },
        {
          "id": "MDRW2100002371.1.23",
          "form": "ㅋㅋㅋㅋ그런 비슷한 이름이었던것 같아요",
          "original_form": "ㅋㅋㅋㅋ그런 비슷한 이름이었던것 같아요",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002371.1.24",
          "form": "식사랑 하시면서 읽으세요^^",
          "original_form": "식사랑 하시면서 읽으세요^^",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002371.1.25",
          "form": "김비서가 왜이럴까",
          "original_form": "김비서가 왜이럴까",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002371.1.26",
          "form": "박서준 박민영 나온 드라마?",
          "original_form": "박서준 박민영 나온 드라마?",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002371.1.27",
          "form": "아 맞네요 그제목!",
          "original_form": "아 맞네요 그제목!",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002371.1.28",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002371.1.29",
          "form": "드라마 보다 만화만 보다 그랫어요",
          "original_form": "드라마 보다 만화만 보다 그랫어요",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002371.1.30",
          "form": "드라마를 먼저봐서 책이 안들어오더라구요 ㅠ",
          "original_form": "드라마를 먼저봐서 책이 안들어오더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002371.1.31",
          "form": "읽으셨어요?",
          "original_form": "읽으셨어요?",
          "speaker_id": "2",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002371.1.32",
          "form": "저는 판타지물만 봐서요ㅠㅠ",
          "original_form": "저는 판타지물만 봐서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002371.1.33",
          "form": "인기가 많았던 편이라 언젠가 봐야지 이러고만있네요",
          "original_form": "인기가 많았던 편이라 언젠가 봐야지 이러고만있네요",
          "speaker_id": "1",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002371.1.34",
          "form": "혹시 이도우 작가님 아세요?",
          "original_form": "혹시 이도우 작가님 아세요?",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002371.1.35",
          "form": "아니요 처음들어봐요",
          "original_form": "아니요 처음들어봐요",
          "speaker_id": "1",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002371.1.36",
          "form": "어떤 책 쓰셨어요?",
          "original_form": "어떤 책 쓰셨어요?",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.37",
          "form": "날씨가 좋으면 찾아가겠어요",
          "original_form": "날씨가 좋으면 찾아가겠어요",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.38",
          "form": "그 드라마 아세요?",
          "original_form": "그 드라마 아세요?",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.39",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.40",
          "form": "강서준?인가 박민영이랑 같이 찍은 드라마",
          "original_form": "강서준?인가 박민영이랑 같이 찍은 드라마",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.41",
          "form": "그거 원작이에요",
          "original_form": "그거 원작이에요",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.42",
          "form": "제가 좋아하는 작가거든요 전 좀 잔잔한걸 좋아하는것 같아요 ㅎ",
          "original_form": "제가 좋아하는 작가거든요 전 좀 잔잔한걸 좋아하는것 같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.43",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002371.1.44",
          "form": "꼭 한번 읽어볼게요!",
          "original_form": "꼭 한번 읽어볼게요!",
          "speaker_id": "1",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002371.1.45",
          "form": "현대물도 후회물 같은거 남주 여주 사연있는거 ㅎ",
          "original_form": "현대물도 후회물 같은거 남주 여주 사연있는거 ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002371.1.46",
          "form": "크 좋죠",
          "original_form": "크 좋죠",
          "speaker_id": "1",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002371.1.47",
          "form": "전 약간 고구마물",
          "original_form": "전 약간 고구마물",
          "speaker_id": "1",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002371.1.48",
          "form": "ㅋㅋㅋㅋㅋ저도요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저도요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002371.1.49",
          "form": "가슴이 막 아오~ 싶은거!",
          "original_form": "가슴이 막 아오~ 싶은거!",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002371.1.50",
          "form": "시각적으로 시청하는게 좋아요\r\n눈으로 읽는게 좋아요?",
          "original_form": "시각적으로 시청하는게 좋아요\r\n눈으로 읽는게 좋아요?",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002371.1.51",
          "form": "전 책이 좋아요",
          "original_form": "전 책이 좋아요",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002371.1.52",
          "form": "드라마는 보면 자꾸 핸드폰으로 딴 짓해서 집중이 안돼요",
          "original_form": "드라마는 보면 자꾸 핸드폰으로 딴 짓해서 집중이 안돼요",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002371.1.53",
          "form": "드라마는 너무 현실성 없지요",
          "original_form": "드라마는 너무 현실성 없지요",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002371.1.54",
          "form": "자려고 누웠는데 이쁘고",
          "original_form": "자려고 누웠는데 이쁘고",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002371.1.55",
          "form": "맞아요ㅋㅋ괴리감",
          "original_form": "맞아요ㅋㅋ괴리감",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002371.1.56",
          "form": "가난한 여주가 원룸이 너무 좋아 ㅋㅋ",
          "original_form": "가난한 여주가 원룸이 너무 좋아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002371.1.57",
          "form": "읽으면서 상상하는게 더 나은편인것같아요ㅋㅋ",
          "original_form": "읽으면서 상상하는게 더 나은편인것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002371.1.58",
          "form": "저도요 같은 생각이에요",
          "original_form": "저도요 같은 생각이에요",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002371.1.59",
          "form": "내 상상이 훨씬 나아요 ㅎ",
          "original_form": "내 상상이 훨씬 나아요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002371.1.60",
          "form": "ㅋㅋㅋ대화즐거웠어요 점심먹으러가야해서 이만줄일게요 맛점하세요!",
          "original_form": "ㅋㅋㅋ대화즐거웠어요 점심먹으러가야해서 이만줄일게요 맛점하세요!",
          "speaker_id": "1",
          "time": "20210820 11:25"
        }
      ]
