      "utterance": [
        {
          "id": "MDRW2100016955.1.1",
          "form": "요즘 보시는거 잇어요?",
          "original_form": "요즘 보시는거 잇어요?",
          "speaker_id": "1",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016955.1.2",
          "form": "저 지금 넷플 체인지데이즈시작했어요^^",
          "original_form": "저 지금 넷플 체인지데이즈시작했어요^^",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016955.1.3",
          "form": "친구들이 재밌다고 추천해서 ㅋㅋ",
          "original_form": "친구들이 재밌다고 추천해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016955.1.4",
          "form": "어디까지 보셨어요!",
          "original_form": "어디까지 보셨어요!",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016955.1.5",
          "form": "아직 2화입니다!!",
          "original_form": "아직 2화입니다!!",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016955.1.6",
          "form": "보셨나요",
          "original_form": "보셨나요",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016955.1.7",
          "form": "당연하쥬",
          "original_form": "당연하쥬",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016955.1.8",
          "form": "재밌지않아요? ㅋㅋㅋ",
          "original_form": "재밌지않아요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016955.1.9",
          "form": "넹ㅋㅋㅋㅋㅋ 근데 거기 출연자 아이돌출신",
          "original_form": "넹ㅋㅋㅋㅋㅋ 근데 거기 출연자 아이돌출신",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016955.1.10",
          "form": "몬엑 기현닮았어여",
          "original_form": "몬엑 기현닮았어여",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016955.1.11",
          "form": "기현맞나",
          "original_form": "기현맞나",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016955.1.12",
          "form": "ㅋㅋㅋ 기현이 그래도 더잘생기지않앗나 훨",
          "original_form": "ㅋㅋㅋ 기현이 그래도 더잘생기지않앗나 훨",
          "speaker_id": "1",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016955.1.13",
          "form": "성호",
          "original_form": "성호",
          "speaker_id": "1",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016955.1.14",
          "form": "점점못생겨져요",
          "original_form": "점점못생겨져요",
          "speaker_id": "1",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016955.1.15",
          "form": "그건 그래요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하는짓땜에 점점 미워보이는건가",
          "original_form": "그건 그래요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하는짓땜에 점점 미워보이는건가",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016955.1.16",
          "form": "아니 진짜 그거아니라도 점점..",
          "original_form": "아니 진짜 그거아니라도 점점..",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016955.1.17",
          "form": "돌싱글즈도 보세요",
          "original_form": "돌싱글즈도 보세요",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016955.1.18",
          "form": "알았어유 그거도 시작할거에용",
          "original_form": "알았어유 그거도 시작할거에용",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016955.1.19",
          "form": "얼마전에 월간집도 완주했어요",
          "original_form": "얼마전에 월간집도 완주했어요",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016955.1.20",
          "form": "정소민 넘이뽔어요",
          "original_form": "정소민 넘이뽔어요",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016955.1.21",
          "form": "오오 그거어때요",
          "original_form": "오오 그거어때요",
          "speaker_id": "1",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016955.1.22",
          "form": "내용은 유치뽕짝이애요",
          "original_form": "내용은 유치뽕짝이애요",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016955.1.23",
          "form": "왜 망했는지 알겠음",
          "original_form": "왜 망했는지 알겠음",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016955.1.24",
