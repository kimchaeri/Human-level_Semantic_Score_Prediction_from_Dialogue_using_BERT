      "utterance": [
        {
          "id": "MDRW2100014093.1.1",
          "form": "저녁 뭐 먹남?!",
          "original_form": "저녁 뭐 먹남?!",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014093.1.2",
          "form": "맞춤법을 지켜야해",
          "original_form": "맞춤법을 지켜야해",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014093.1.3",
          "form": "아직 안정했다",
          "original_form": "아직 안정했다",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014093.1.4",
          "form": "아마 집에서 한식을 먹지 않을까",
          "original_form": "아마 집에서 한식을 먹지 않을까",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014093.1.5",
          "form": "근데 피자가 땡긴다",
          "original_form": "근데 피자가 땡긴다",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014093.1.6",
          "form": "한식과 피자는 거리가 많이 멀다",
          "original_form": "한식과 피자는 거리가 많이 멀다",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014093.1.7",
          "form": "극단은 통하는 법이지",
          "original_form": "극단은 통하는 법이지",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014093.1.8",
          "form": "지난번 먹은 피자로는 욕구가 채워지지 않았네",
          "original_form": "지난번 먹은 피자로는 욕구가 채워지지 않았네",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014093.1.9",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014093.1.10",
          "form": "이재모 피자가 맛있지",
          "original_form": "이재모 피자가 맛있지",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014093.1.11",
          "form": "name1은 저녁 뭐 먹어요??",
          "original_form": "&name1&은 저녁 뭐 먹어요??",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014093.1.12",
          "form": "흠 근데 난 이번주에 새우회가 먹고싶은걸",
          "original_form": "흠 근데 난 이번주에 새우회가 먹고싶은걸",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014093.1.13",
          "form": "내 저녁은 미정!",
          "original_form": "내 저녁은 미정!",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014093.1.14",
          "form": "새우회는 주말에 먹어야지",
          "original_form": "새우회는 주말에 먹어야지",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014093.1.15",
          "form": "새우회랑 오징어회랑 먹을까?",
          "original_form": "새우회랑 오징어회랑 먹을까?",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014093.1.16",
          "form": "식비가 많이 나오겠다",
          "original_form": "식비가 많이 나오겠다",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014093.1.17",
          "form": "민락수변공원 코로나 때문에 폐쇄 안됐나??",
          "original_form": "민락수변공원 코로나 때문에 폐쇄 안됐나??",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014093.1.18",
          "form": "공원말고 초장집을 가면 되지 않을까?",
          "original_form": "공원말고 초장집을 가면 되지 않을까?",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014093.1.19",
          "form": "그러면 되겠다.",
          "original_form": "그러면 되겠다.",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014093.1.20",
          "form": "다만, 공원에서 먹을 때보다 비싸겠군",
          "original_form": "다만, 공원에서 먹을 때보다 비싸겠군",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014093.1.21",
          "form": "광안대교를 보면서는 못먹겠다.",
          "original_form": "광안대교를 보면서는 못먹겠다.",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014093.1.22",
          "form": "그렇지 초장집 두당으로 차림비를 받으니까",
          "original_form": "그렇지 초장집 두당으로 차림비를 받으니까",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014093.1.23",
          "form": "그대신 매운탕 같은 걸 먹을 수 있지!",
          "original_form": "그대신 매운탕 같은 걸 먹을 수 있지!",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014093.1.24",
          "form": "그렇긴 하겠다.",
          "original_form": "그렇긴 하겠다.",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014093.1.25",
          "form": "name1 매운탕을 좋아하지.",
          "original_form": "&name1& 매운탕을 좋아하지.",
          "speaker_id": "2",
          "time": "20210825 17:21"
        }
      ]
    }
