          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007902.1.3",
          "form": "필요한건 해요!",
          "original_form": "필요한건 해요!",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007902.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007902.1.5",
          "form": "화장실도 담당하시나요",
          "original_form": "화장실도 담당하시나요",
          "speaker_id": "2",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007902.1.6",
          "form": "네 락스냄새싫어요",
          "original_form": "네 락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007902.1.7",
          "form": "얼마나 자주하세요",
          "original_form": "얼마나 자주하세요",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007902.1.8",
          "form": "일주일에한번",
          "original_form": "일주일에한번",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007902.1.9",
          "form": "그거랑 여름에 음쓰 힘들어요",
          "original_form": "그거랑 여름에 음쓰 힘들어요",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007902.1.10",
          "form": "으 냄새",
          "original_form": "으 냄새",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007902.1.11",
          "form": "벌레꼬이고",
          "original_form": "벌레꼬이고",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007902.1.12",
          "form": "날파리 장난아니죠",
          "original_form": "날파리 장난아니죠",
