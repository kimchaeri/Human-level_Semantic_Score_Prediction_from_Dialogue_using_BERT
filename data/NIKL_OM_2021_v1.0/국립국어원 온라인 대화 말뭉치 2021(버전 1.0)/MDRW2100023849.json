      "utterance": [
        {
          "id": "MDRW2100023849.1.1",
          "form": "만둣국에 굴 좀 안넣었으면",
          "original_form": "만둣국에 굴 좀 안넣었으면",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023849.1.2",
          "form": "좋겠어",
          "original_form": "좋겠어",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023849.1.3",
          "form": "아 굴넣어??",
          "original_form": "아 굴넣어??",
          "speaker_id": "1",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023849.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023849.1.5",
          "form": "전라도라 그런가?",
          "original_form": "전라도라 그런가?",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.6",
          "form": "생각도 못해본 조합",
          "original_form": "생각도 못해본 조합",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.7",
          "form": "떡국도",
          "original_form": "떡국도",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.8",
          "form": "매생이에 끓여먹거나",
          "original_form": "매생이에 끓여먹거나",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.9",
          "form": "굴 넣거나 해",
          "original_form": "굴 넣거나 해",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.10",
          "form": "우리는 매생이 굴국을 따로먹어",
          "original_form": "우리는 매생이 굴국을 따로먹어",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.11",
          "form": "떡국은 사골..",
          "original_form": "떡국은 사골..",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.12",
          "form": "솔직히 비린내 겁나 심해",
          "original_form": "솔직히 비린내 겁나 심해",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.13",
          "form": "또 그건",
          "original_form": "또 그건",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.14",
          "form": "우리집 스콘이",
          "original_form": "우리집 {censored}스콘이",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023849.1.15",
          "form": "고깃국물에 끓인 떡국같은건",
          "original_form": "고깃국물에 끓인 떡국같은건",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.16",
          "form": "자시지 않겠다 하시어",
          "original_form": "자시지 않겠다 하시어",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.17",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.18",
          "form": "왜야",
          "original_form": "왜야",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.19",
          "form": "사골국이 얼마나 맛잇는데",
          "original_form": "사골국이 얼마나 맛잇는데",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.20",
          "form": "해남출신이라",
          "original_form": "해남출신이라",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.21",
          "form": "굴국물이 좋대",
          "original_form": "굴국물이 좋대",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.22",
          "form": "뭔상관인지는 모르겟음",
          "original_form": "뭔상관인지는 모르겟음",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.23",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.24",
          "form": "우리집도 인천이라",
          "original_form": "우리집도 인천이라",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.25",
          "form": "바닷가 출신들이지만",
          "original_form": "바닷가 출신들이지만",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.26",
          "form": "그건 그거고 이건 이거지",
          "original_form": "그건 그거고 이건 이거지",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100023849.1.27",
          "form": "ㅇㅇ그래서 한 3년 전부터는",
          "original_form": "ㅇㅇ그래서 한 3년 전부터는",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.28",
          "form": "난 비비고 사골곰탕 사와서",
          "original_form": "난 비비고 사골곰탕 사와서",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.29",
          "form": "따로 끓여먹음ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "따로 끓여먹음ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ 잘햇어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 잘햇어",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.31",
          "form": "나는 또 비린것을 극혐해서",
          "original_form": "나는 또 비린것을 극혐해서",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.32",
          "form": "그리고 매생이국 솔직히 좀 비위상하게 생겼어",
          "original_form": "그리고 매생이국 솔직히 좀 비위상하게 생겼어",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.33",
          "form": "녹조같아",
          "original_form": "녹조같아",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.34",
          "form": "난 가리는 것도 거의 없고",
          "original_form": "난 가리는 것도 거의 없고",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023849.1.35",
          "form": "비린것도 잘먹는데",
          "original_form": "비린것도 잘먹는데",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100023849.1.36",
          "form": "굴은 적응이 안됨..",
          "original_form": "굴은 적응이 안됨..",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100023849.1.37",
          "form": "바지락은 내가 알맹이를 먹진 않아도",
          "original_form": "바지락은 내가 알맹이를 먹진 않아도",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100023849.1.38",
          "form": "국물은 시원한데",
          "original_form": "국물은 시원한데",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100023849.1.39",
          "form": "굴은 진짜 비려......",
          "original_form": "굴은 진짜 비려......",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100023849.1.40",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100023849.1.41",
          "form": "나도 바지락이랑 재첩은 잘먹는데",
          "original_form": "나도 바지락이랑 재첩은 잘먹는데",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023849.1.42",
          "form": "굴은 모르겠어....",
          "original_form": "굴은 모르겠어....",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023849.1.43",
          "form": "내가 조개류중에서 유일하게 안먹는게 굴임..",
          "original_form": "내가 조개류중에서 유일하게 안먹는게 굴임..",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023849.1.44",
          "form": "근데 그 비린맛이 좋은거래",
          "original_form": "근데 그 비린맛이 좋은거래",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023849.1.45",
          "form": "이해할수없음...",
          "original_form": "이해할수없음...",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023849.1.46",
          "form": "솔직히 말이 좋아서 바다향기지",
          "original_form": "솔직히 말이 좋아서 바다향기지",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023849.1.47",
          "form": "비린내잖아....",
          "original_form": "비린내잖아....",
          "speaker_id": "1",
          "time": "20210830 19:00"
        }
      ]
    }
