      "utterance": [
        {
          "id": "MDRW2100002386.1.1",
          "form": "마카롱을 좋아하시나요?",
          "original_form": "마카롱을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 10:57"
        },
        {
          "id": "MDRW2100002386.1.2",
          "form": "아니요 좋아하진않는데 그냥 이름이 좋아서ㅋ",
          "original_form": "아니요 좋아하진않는데 그냥 이름이 좋아서ㅋ",
          "speaker_id": "2",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002386.1.3",
          "form": "마카롱 좋아하세요?",
          "original_form": "마카롱 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002386.1.4",
          "form": "앗.. 저도 딱히.. 사실 뭔 맛으로 먹는지 모르겠어서ㅎㅎ..",
          "original_form": "앗.. 저도 딱히.. 사실 뭔 맛으로 먹는지 모르겠어서ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002386.1.5",
          "form": "좋아하는 디저트가 있으신가요?",
          "original_form": "좋아하는 디저트가 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002386.1.6",
          "form": "저도 마카롱이 그렇더라구요 ㅎ 전 크레이프 맛있던데요",
          "original_form": "저도 마카롱이 그렇더라구요 ㅎ 전 크레이프 맛있던데요",
          "speaker_id": "2",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002386.1.7",
          "form": "커피빈에 파는",
          "original_form": "커피빈에 파는",
          "speaker_id": "2",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002386.1.8",
          "form": "크레이프 좋아요",
          "original_form": "크레이프 좋아요",
          "speaker_id": "1",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002386.1.9",
          "form": "저 그 식감 되게 좋아요",
          "original_form": "저 그 식감 되게 좋아요",
          "speaker_id": "1",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002386.1.10",
          "form": "겹겹이 쌓여있는 그게 좋더라구요",
          "original_form": "겹겹이 쌓여있는 그게 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002386.1.11",
          "form": "맞아요 부드럽고 적당히 달달하고ㅎ",
          "original_form": "맞아요 부드럽고 적당히 달달하고ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002386.1.12",
          "form": "좋아하는 음료는 뭔가요?",
          "original_form": "좋아하는 음료는 뭔가요?",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002386.1.13",
          "form": "아메리카노랑 먹어야 맛있을텐데 전 달달한거좋아해서 ㅎㅎ",
          "original_form": "아메리카노랑 먹어야 맛있을텐데 전 달달한거좋아해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002386.1.14",
          "form": "달달한거요?",
          "original_form": "달달한거요?",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002386.1.15",
          "form": "마끼아또나 바닐라라떼 즐겨마셔요",
          "original_form": "마끼아또나 바닐라라떼 즐겨마셔요",
          "speaker_id": "2",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002386.1.16",
          "form": "너무 좋죠 향도 좋고 맛도 좋고 달콤해서 좋은거같아요",
          "original_form": "너무 좋죠 향도 좋고 맛도 좋고 달콤해서 좋은거같아요",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002386.1.17",
          "form": "요즘은 스벅 블론드 바닐라더블샷마끼아또 ㅎㅎ 이름도 기네요ㅋ",
          "original_form": "요즘은 스벅 블론드 바닐라더블샷마끼아또 ㅎㅎ 이름도 기네요ㅋ",
          "speaker_id": "2",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002386.1.18",
          "form": "그것도 맛있을거같아요. 음료는 역시 단게 최고에요",
          "original_form": "그것도 맛있을거같아요. 음료는 역시 단게 최고에요",
          "speaker_id": "1",
          "time": "20210820 11:02"
        }
      ]
