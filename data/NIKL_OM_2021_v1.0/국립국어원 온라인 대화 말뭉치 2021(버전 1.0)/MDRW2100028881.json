        {
          "id": "MDRW2100028881.1.1",
          "form": "요즘 여행영상봐요",
          "original_form": "요즘 여행영상봐요",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028881.1.2",
          "form": "누구거요?",
          "original_form": "누구거요?",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028881.1.3",
          "form": "그냥 다 봐요ㅋㅋ",
          "original_form": "그냥 다 봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028881.1.4",
          "form": "어디 가고싶은데 있어요?",
          "original_form": "어디 가고싶은데 있어요?",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028881.1.5",
          "form": "경주요",
          "original_form": "경주요",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028881.1.6",
          "form": "오 경주좋죠",
          "original_form": "오 경주좋죠",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028881.1.7",
          "form": "동해도  가보고싶어요",
          "original_form": "동해도  가보고싶어요",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028881.1.8",
          "form": "동해바다ㅜㅜ",
          "original_form": "동해바다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028881.1.9",
          "form": "햄튜브 동해영상으로 대리만족했어요",
          "original_form": "햄튜브 동해영상으로 대리만족했어요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028881.1.10",
          "form": "ㅋㅋㅋ 좋나요?",
          "original_form": "ㅋㅋㅋ 좋나요?",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028881.1.11",
          "form": "네 좋아하는 유듀바라",
          "original_form": "네 좋아하는 유듀바라",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028881.1.12",
          "form": "유듀바ㅋㅋㄱㄱ",
          "original_form": "유듀바ㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028881.1.13",
          "form": "한번 봐볼게요",
          "original_form": "한번 봐볼게요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028881.1.14",
          "form": "빠지실거예요",
          "original_form": "빠지실거예요",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028881.1.15",
          "form": "아가씬가요",
          "original_form": "아가씬가요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028881.1.16",
