      "utterance": [
        {
          "id": "MDRW2100034178.1.1",
          "form": "제가 사서 먹진 않는데 있으면 먹어요!",
          "original_form": "제가 사서 먹진 않는데 있으면 먹어요!",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100034178.1.2",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100034178.1.3",
          "form": "전 좋아해요 ㅎㅎ",
          "original_form": "전 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034178.1.4",
          "form": "군것질을 좋아해서 ㅎㅎ",
          "original_form": "군것질을 좋아해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034178.1.5",
          "form": "단 과자랑 짠 과자 중에 어떤 걸 더 선호하세요?",
          "original_form": "단 과자랑 짠 과자 중에 어떤 걸 더 선호하세요?",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034178.1.6",
          "form": "전 주로 짠과자요!",
          "original_form": "전 주로 짠과자요!",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034178.1.7",
          "form": "감자칩 좋아해요",
          "original_form": "감자칩 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034178.1.8",
          "form": "감자칩 맛있죠!",
          "original_form": "감자칩 맛있죠!",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034178.1.9",
          "form": "어떤 거 자주 드세요?",
          "original_form": "어떤 거 자주 드세요?",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034178.1.10",
          "form": "저 무뚝뚝감자칩 좋아하는데",
          "original_form": "저 무뚝뚝감자칩 좋아하는데",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034178.1.11",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034178.1.12",
          "form": "헉 저도 친구가 추천해줘서 먹어봤어요! 맛있더라고요!",
          "original_form": "헉 저도 친구가 추천해줘서 먹어봤어요! 맛있더라고요!",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100034178.1.13",
          "form": "맞죠 ㅎㅎ ! 양에 비해 좀 비싸긴한데 맛있어요",
          "original_form": "맞죠 ㅎㅎ ! 양에 비해 좀 비싸긴한데 맛있어요",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100034178.1.14",
          "form": "초코 꼬북칩은 드셔보셨죠?",
          "original_form": "초코 꼬북칩은 드셔보셨죠?",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100034178.1.15",
          "form": "네! 식감이 특이해서 재밌었어요",
          "original_form": "네! 식감이 특이해서 재밌었어요",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100034178.1.16",
          "form": "물리는 단맛이 아니라 딴 거 하면서 계속 먹게 되더라고요ㅋㅋㅋ",
          "original_form": "물리는 단맛이 아니라 딴 거 하면서 계속 먹게 되더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100034178.1.17",
          "form": "맞죠 ㅋㅋ ! 그거 얼려먹으면 맛있다던데",
          "original_form": "맞죠 ㅋㅋ ! 그거 얼려먹으면 맛있다던데",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100034178.1.18",
          "form": "헉 시원하게 먹으면 진짜 맛있을 거 같아요!",
          "original_form": "헉 시원하게 먹으면 진짜 맛있을 거 같아요!",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100034178.1.19",
          "form": "저도 아직 안먹어봤는데",
          "original_form": "저도 아직 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100034178.1.20",
          "form": "얼어서 우유에 말아먹으래요ㅡ ㅋㅋ",
          "original_form": "얼어서 우유에 말아먹으래요ㅡ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100034178.1.21",
          "form": "맛있을 거 같아요!",
          "original_form": "맛있을 거 같아요!",
          "speaker_id": "2",
          "time": "20210827 20:45"
        }
      ]
