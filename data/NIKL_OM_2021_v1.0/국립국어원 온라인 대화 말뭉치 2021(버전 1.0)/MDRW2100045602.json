      "utterance": [
        {
          "id": "MDRW2100045602.1.1",
          "form": "그럼요 ㅋㅋㅋ 빨리 맞추셨는걸요",
          "original_form": "그럼요 ㅋㅋㅋ 빨리 맞추셨는걸요",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045602.1.2",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045602.1.3",
          "form": "이번엔 과일 포함 야채로 범위 넓혀서 해보는 건 어때요?",
          "original_form": "이번엔 과일 포함 야채로 범위 넓혀서 해보는 건 어때요?",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.4",
          "form": "이런거 잘못하는 ㄷ데 왠일 일까요ㅋㅋ",
          "original_form": "이런거 잘못하는 ㄷ데 왠일 일까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.5",
          "form": "ㅋㅋㅋㅋㅋ아까부터 너무 빨리 맞추셔서",
          "original_form": "ㅋㅋㅋㅋㅋ아까부터 너무 빨리 맞추셔서",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.6",
          "form": "오 그럼 너무 어렵지 않을까요",
          "original_form": "오 그럼 너무 어렵지 않을까요",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.7",
          "form": "놀라잖아요",
          "original_form": "놀라잖아요",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.8",
          "form": "그래도 재밌으니 해봐욬ㅋㅋㅋㅋ",
          "original_form": "그래도 재밌으니 해봐욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ좋아요 좋아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좋아요 좋아요",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.10",
          "form": "저 이런거 최약체라구욬ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "저 이런거 최약체라구욬ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045602.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저돈데 최약체승부 갑시다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저돈데 최약체승부 갑시다",
          "speaker_id": "1",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045602.1.12",
          "form": "제가 물어볼게요!",
          "original_form": "제가 물어볼게요!",
          "speaker_id": "1",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045602.1.13",
          "form": "야채 하나 생각해주세요!",
          "original_form": "야채 하나 생각해주세요!",
          "speaker_id": "1",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045602.1.14",
          "form": "네 좋아요! 이번엔ㅌ어려운걸로 생각했습니다.",
          "original_form": "네 좋아요! 이번엔ㅌ어려운걸로 생각했습니다.",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045602.1.15",
          "form": "앗 각오해야겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 각오해야겠네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045602.1.16",
          "form": "초록색인가요?",
          "original_form": "초록색인가요?",
          "speaker_id": "1",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045602.1.17",
          "form": "엇 넵ㅋㅋㅋㅋ",
          "original_form": "엇 넵ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045602.1.18",
          "form": "아앗 나이스",
          "original_form": "아앗 나이스",
          "speaker_id": "1",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045602.1.19",
          "form": "잎채소인가요?",
          "original_form": "잎채소인가요?",
          "speaker_id": "1",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045602.1.20",
          "form": "잎채소 입니다",
          "original_form": "잎채소 입니다",
          "speaker_id": "2",
          "time": "20210908 04:14"
        },
        {
          "id": "MDRW2100045602.1.21",
          "form": "매운 맛이 나나요?",
          "original_form": "매운 맛이 나나요?",
          "speaker_id": "1",
          "time": "20210908 04:14"
        },
        {
          "id": "MDRW2100045602.1.22",
          "form": "매운맛은 없어요",
          "original_form": "매운맛은 없어요",
          "speaker_id": "2",
          "time": "20210908 04:15"
        },
        {
          "id": "MDRW2100045602.1.23",
          "form": "으으으음 고기를 싸먹을 때 많이 쓰나요?",
          "original_form": "으으으음 고기를 싸먹을 때 많이 쓰나요?",
          "speaker_id": "1",
          "time": "20210908 04:17"
        },
        {
          "id": "MDRW2100045602.1.24",
          "form": "싸먹을 수가 없어요",
          "original_form": "싸먹을 수가 없어요",
          "speaker_id": "2",
          "time": "20210908 04:18"
        },
        {
          "id": "MDRW2100045602.1.25",
          "form": "아앗 잎인데 못 싸다니 미궁으로 빠지네요 ㅋㅋㅋㅋ",
          "original_form": "아앗 잎인데 못 싸다니 미궁으로 빠지네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:18"
        },
        {
          "id": "MDRW2100045602.1.26",
          "form": "일부러 어려운걸로 생각했죠!",
          "original_form": "일부러 어려운걸로 생각했죠!",
          "speaker_id": "2",
          "time": "20210908 04:18"
        },
        {
          "id": "MDRW2100045602.1.27",
          "form": "혹시 향신료 종류인가요?",
          "original_form": "혹시 향신료 종류인가요?",
          "speaker_id": "1",
          "time": "20210908 04:20"
        }
      ]
    }
