        {
          "id": "MDRW2100003436.1.1",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003436.1.2",
          "form": "울 언니야 넘 심심할텐데 ㅠㅠ",
          "original_form": "울 언니야 넘 심심할텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003436.1.3",
          "form": "코로나 땜에 갈 수가 없네",
          "original_form": "코로나 땜에 갈 수가 없네",
          "speaker_id": "1",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003436.1.4",
          "form": "내년엔 갈수있겠지",
          "original_form": "내년엔 갈수있겠지",
          "speaker_id": "2",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003436.1.5",
          "form": "그렇길.. 미국 가서 맛난 햄버거 먹고싶당",
          "original_form": "그렇길.. 미국 가서 맛난 햄버거 먹고싶당",
          "speaker_id": "1",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003436.1.6",
          "form": "ㅋㅋ살찌는 음식인데",
          "original_form": "ㅋㅋ살찌는 음식인데",
          "speaker_id": "2",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003436.1.7",
          "form": "적당히 먹으면 괜찮다 ㅎㅎ",
          "original_form": "적당히 먹으면 괜찮다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003436.1.8",
          "form": "난 랍스터 뷔페 ㅎㅎ",
          "original_form": "난 랍스터 뷔페 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003436.1.9",
          "form": "그럴듯 ㅎㅎ",
          "original_form": "그럴듯 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003436.1.10",
          "form": "이번엔 가면 맛난거 많이많이 먹자",
          "original_form": "이번엔 가면 맛난거 많이많이 먹자",
          "speaker_id": "1",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003436.1.11",
          "form": "그러다 돼지된다",
          "original_form": "그러다 돼지된다",
          "speaker_id": "2",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003436.1.12",
          "form": "아니거든요",
          "original_form": "아니거든요",
          "speaker_id": "1",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003436.1.13",
          "form": "적당히 먹어야지",
          "original_form": "적당히 먹어야지",
          "speaker_id": "2",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003436.1.14",
          "form": "많이 돌아다니고",
          "original_form": "많이 돌아다니고",
          "speaker_id": "2",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003436.1.15",
          "form": "그래 ㅎㅎ 그럼 살안찐다 여행가면 만보 이상 걸으니까",
          "original_form": "그래 ㅎㅎ 그럼 살안찐다 여행가면 만보 이상 걸으니까",
