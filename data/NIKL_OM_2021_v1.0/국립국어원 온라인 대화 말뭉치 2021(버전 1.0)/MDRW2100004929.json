      "utterance": [
        {
          "id": "MDRW2100004929.1.1",
          "form": "키싱부스3 좀 실망스러웠어",
          "original_form": "키싱부스3 좀 실망스러웠어",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004929.1.2",
          "form": "1이 제일 재밌었다",
          "original_form": "1이 제일 재밌었다",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004929.1.3",
          "form": "응 1은 풋풋했는데",
          "original_form": "응 1은 풋풋했는데",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004929.1.4",
          "form": "3은 여주인공이 너무 갈팡질팡.. 왜 저러나 싶었지",
          "original_form": "3은 여주인공이 너무 갈팡질팡.. 왜 저러나 싶었지",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004929.1.5",
          "form": "갈팡질팡도 글코",
          "original_form": "갈팡질팡도 글코",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004929.1.6",
          "form": "끝도 너무 애매하게 어쩌라는 건지 싶더라고",
          "original_form": "끝도 너무 애매하게 어쩌라는 건지 싶더라고",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004929.1.7",
          "form": "계속 전남친 전여친 나와서 꼬이고",
          "original_form": "계속 전남친 전여친 나와서 꼬이고",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004929.1.8",
          "form": "너무 올드한 감성이었어",
          "original_form": "너무 올드한 감성이었어",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004929.1.9",
          "form": "ㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋ맞아",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100004929.1.10",
          "form": "그나마 내가 사랑한 남자들에게는 한국계 주인공이라 재미있었는데",
          "original_form": "그나마 내가 사랑한 남자들에게는 한국계 주인공이라 재미있었는데",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004929.1.11",
          "form": "이번 여름엔 넷플릭스 티빙 왓챠 화려했다",
          "original_form": "이번 여름엔 넷플릭스 티빙 왓챠 화려했다",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004929.1.12",
          "form": "키싱부스3는 뭐.. 잘사는 백인들 얘기라",
          "original_form": "키싱부스3는 뭐.. 잘사는 백인들 얘기라",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004929.1.13",
          "form": "공감이 안가는군",
          "original_form": "공감이 안가는군",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004929.1.14",
          "form": "그러게 화려했구먼",
          "original_form": "그러게 화려했구먼",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004929.1.15",
          "form": "응 애들이 연애 말고는 고민이 없음",
          "original_form": "응 애들이 연애 말고는 고민이 없음",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004929.1.16",
          "form": "연애도 사실...",
          "original_form": "연애도 사실...",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004929.1.17",
          "form": "그러게 .. 대충 살아도 하버드에 유씨엘에이에",
          "original_form": "그러게 .. 대충 살아도 하버드에 유씨엘에이에",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004929.1.18",
          "form": "게임개발자되고 뭐.ㅡ",
          "original_form": "게임개발자되고 뭐.ㅡ",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004929.1.19",
          "form": "인생 편한 애들 나오는 영화",
          "original_form": "인생 편한 애들 나오는 영화",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100004929.1.20",
          "form": "아 다시 넷플릭스 돌려보고 그런 휴가로 돌아가고 싶구만~",
          "original_form": "아 다시 넷플릭스 돌려보고 그런 휴가로 돌아가고 싶구만~",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.21",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.22",
          "form": "주말에 할 순 있지",
          "original_form": "주말에 할 순 있지",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.23",
          "form": "넷플릭스 조금씩 보기",
          "original_form": "넷플릭스 조금씩 보기",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.24",
          "form": "주말에 콘텐츠를 즐기면",
          "original_form": "주말에 콘텐츠를 즐기면",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.25",
          "form": "주중의 내가 괴롭다ㅠ",
          "original_form": "주중의 내가 괴롭다ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.26",
          "form": "옳으신 말씀..",
          "original_form": "옳으신 말씀..",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.27",
          "form": "요새 넷플릭스나 웨이브 뭐 볼거 있나",
          "original_form": "요새 넷플릭스나 웨이브 뭐 볼거 있나",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004929.1.28",
          "form": "긴거말고 짧은거면 괜찮을 텐데",
          "original_form": "긴거말고 짧은거면 괜찮을 텐데",
          "speaker_id": "2",
          "time": "20210821 20:32"
        }
      ]
    }
