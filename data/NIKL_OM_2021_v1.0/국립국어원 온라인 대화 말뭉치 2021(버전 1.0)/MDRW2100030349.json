      "utterance": [
        {
          "id": "MDRW2100030349.1.1",
          "form": "요즘 배달 음식 많이 먹는거 같음",
          "original_form": "요즘 배달 음식 많이 먹는거 같음",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030349.1.2",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030349.1.3",
          "form": "그래서 돼지가 됐지",
          "original_form": "그래서 돼지가 됐지",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030349.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ근데 편해서 먹을 수 밖에 없어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ근데 편해서 먹을 수 밖에 없어",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030349.1.5",
          "form": "아..배달음식.. 닭발 먹고 싶다",
          "original_form": "아..배달음식.. 닭발 먹고 싶다",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030349.1.6",
          "form": "무뼈가 먹기 편해서",
          "original_form": "무뼈가 먹기 편해서",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030349.1.7",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030349.1.8",
          "form": "군침이 돈다",
          "original_form": "군침이 돈다",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030349.1.9",
          "form": "먹고싶은 음식들 다 배달시키고 싶다..",
          "original_form": "먹고싶은 음식들 다 배달시키고 싶다..",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030349.1.10",
          "form": "닭발 국물닭발이 좋더라.. 넌 ㅠ?",
          "original_form": "닭발 국물닭발이 좋더라.. 넌 ㅠ?",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030349.1.11",
          "form": "난 그냥 닭발을 좋아해",
          "original_form": "난 그냥 닭발을 좋아해",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030349.1.12",
          "form": "근데 거의 시켜먹는건",
          "original_form": "근데 거의 시켜먹는건",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030349.1.13",
          "form": "떡볶이",
          "original_form": "떡볶이",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030349.1.14",
          "form": "ㅠㅠ신전떡볶이도 먹고 싶더",
          "original_form": "ㅠㅠ신전떡볶이도 먹고 싶더",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030349.1.15",
          "form": "신전은. .. 오뎅튀가 진리...",
          "original_form": "신전은. .. 오뎅튀가 진리...",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030349.1.16",
          "form": "거기 요즘 치킨텐더도 맛나더라..",
          "original_form": "거기 요즘 치킨텐더도 맛나더라..",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030349.1.17",
          "form": "치킨텐더도 있었나?",
          "original_form": "치킨텐더도 있었나?",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030349.1.18",
          "form": "몰랐네",
          "original_form": "몰랐네",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030349.1.19",
          "form": "웅 없는데도 있긴 하던데 보통 거의 있더라구",
          "original_form": "웅 없는데도 있긴 하던데 보통 거의 있더라구",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030349.1.20",
          "form": "오 맛있겠어..",
          "original_form": "오 맛있겠어..",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030349.1.21",
          "form": "완전 맛있엉... 나중에 함 먹어바",
          "original_form": "완전 맛있엉... 나중에 함 먹어바",
          "speaker_id": "1",
          "time": "20210904 00:07"
        }
      ]
