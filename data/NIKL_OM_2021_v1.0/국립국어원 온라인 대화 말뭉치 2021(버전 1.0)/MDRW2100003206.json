      "utterance": [
        {
          "id": "MDRW2100003206.1.1",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003206.1.2",
          "form": "네 매년 한번씩은 나갔는데 코로나터지고 못가고있어요ㅠㅠ",
          "original_form": "네 매년 한번씩은 나갔는데 코로나터지고 못가고있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003206.1.3",
          "form": "오 혹시 일본 가보셨어요?",
          "original_form": "오 혹시 일본 가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003206.1.4",
          "form": "네 후쿠오카랑 오사카도 가보고",
          "original_form": "네 후쿠오카랑 오사카도 가보고",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003206.1.5",
          "form": "몇군데 가봤는데 갑자기 이름생각이 안나네요",
          "original_form": "몇군데 가봤는데 갑자기 이름생각이 안나네요",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003206.1.6",
          "form": "전 한번도 못가봤는데",
          "original_form": "전 한번도 못가봤는데",
          "speaker_id": "1",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003206.1.7",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "1",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003206.1.8",
          "form": "일본 가깝고 음식도 맛있고 좋아요",
          "original_form": "일본 가깝고 음식도 맛있고 좋아요",
          "speaker_id": "2",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003206.1.9",
          "form": "원전 사태로 좀 꺼려지는데 구경할것도 많고",
          "original_form": "원전 사태로 좀 꺼려지는데 구경할것도 많고",
          "speaker_id": "2",
          "time": "20210820 20:36"
        },
        {
          "id": "MDRW2100003206.1.10",
          "form": "어떤음식이 맛있었어요?",
          "original_form": "어떤음식이 맛있었어요?",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003206.1.11",
          "form": "스시도 맛있고 라멘도 맛나요",
          "original_form": "스시도 맛있고 라멘도 맛나요",
          "speaker_id": "2",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003206.1.12",
          "form": "음식은 거의 실패안하더라구요",
          "original_form": "음식은 거의 실패안하더라구요",
          "speaker_id": "2",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003206.1.13",
          "form": "저도 초밥 되게 좋아하는데",
          "original_form": "저도 초밥 되게 좋아하는데",
          "speaker_id": "1",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003206.1.14",
          "form": "1~2년뒤에 한번 가보고싶어요",
          "original_form": "1~2년뒤에 한번 가보고싶어요",
          "speaker_id": "1",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003206.1.15",
          "form": "코로나 끝나고 방사능 덜해지면 함 다녀오세요^^",
          "original_form": "코로나 끝나고 방사능 덜해지면 함 다녀오세요^^",
          "speaker_id": "2",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003206.1.16",
          "form": "전 유럽 가고싶더라구요",
          "original_form": "전 유럽 가고싶더라구요",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003206.1.17",
          "form": "유럽 어디나라 가보고 싶어요?",
          "original_form": "유럽 어디나라 가보고 싶어요?",
          "speaker_id": "1",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003206.1.18",
          "form": "스위스랑 서유럽이요",
          "original_form": "스위스랑 서유럽이요",
          "speaker_id": "2",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003206.1.19",
          "form": "동유럽은 짧게 가봤는데 거기도 좋아요",
          "original_form": "동유럽은 짧게 가봤는데 거기도 좋아요",
          "speaker_id": "2",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003206.1.20",
          "form": "유럽은 그 특유의 분위기가 있는거같아요",
          "original_form": "유럽은 그 특유의 분위기가 있는거같아요",
          "speaker_id": "1",
          "time": "20210820 20:51"
        },
        {
          "id": "MDRW2100003206.1.21",
          "form": "동유럽이랑  서유럽도 다른가봐요",
          "original_form": "동유럽이랑  서유럽도 다른가봐요",
          "speaker_id": "2",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003206.1.22",
          "form": "서유럽이 잘사는 나라가 많더라구요",
          "original_form": "서유럽이 잘사는 나라가 많더라구요",
          "speaker_id": "1",
          "time": "20210820 20:54"
        }
