      "utterance": [
        {
          "id": "MDRW2100042880.1.1",
          "form": "스타일러는 좋을것같아요",
          "original_form": "스타일러는 좋을것같아요",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042880.1.2",
          "form": "전없어요ㅠ",
          "original_form": "전없어요ㅠ",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042880.1.3",
          "form": "요즘은 미먼도글코 바이러스가 많으니ㅠ",
          "original_form": "요즘은 미먼도글코 바이러스가 많으니ㅠ",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042880.1.4",
          "form": "진짜 찐 사용후기><",
          "original_form": "진짜 찐 사용후기><",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042880.1.5",
          "form": "공기청정기도 여쭤바두댈까욤",
          "original_form": "공기청정기도 여쭤바두댈까욤",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042880.1.6",
          "form": "일단 식세기 건조기 스타일러 합격!!",
          "original_form": "일단 식세기 건조기 스타일러 합격!!",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042880.1.7",
          "form": "공청기있어야쥬ㅎㅎ",
          "original_form": "공청기있어야쥬ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042880.1.8",
          "form": "어디께 조을까욤",
          "original_form": "어디께 조을까욤",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042880.1.9",
          "form": "저는 가전은 엘지 살생각이에욤",
          "original_form": "저는 가전은 엘지 살생각이에욤",
          "speaker_id": "1",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100042880.1.10",
          "form": "막 꼼꼼히 알아보는 성격이아니라서ㅜㅜ",
          "original_form": "막 꼼꼼히 알아보는 성격이아니라서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100042880.1.11",
          "form": "귀차니즘ㅠㅠ",
          "original_form": "귀차니즘ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100042880.1.12",
          "form": "엘지좋아여ㅋㅋ",
          "original_form": "엘지좋아여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042880.1.13",
          "form": "근데건조기는. 이번에 당했어서ㅠㅠ",
          "original_form": "근데건조기는. 이번에 당했어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042880.1.14",
          "form": "앗 ㅜㅜ",
          "original_form": "앗 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 10:59"
        },
        {
          "id": "MDRW2100042880.1.15",
          "form": "그 피해보신고객이시군여ㅜㅜㅜㅜ",
          "original_form": "그 피해보신고객이시군여ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 10:59"
        },
        {
          "id": "MDRW2100042880.1.16",
          "form": "ㅠㅠ네ㅠㅠ 대응도 구리고ㅠ",
          "original_form": "ㅠㅠ네ㅠㅠ 대응도 구리고ㅠ",
          "speaker_id": "2",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100042880.1.17",
          "form": "건조기는 삼성으로ㅠ",
          "original_form": "건조기는 삼성으로ㅠ",
          "speaker_id": "2",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100042880.1.18",
          "form": "ㅋㅋ네 명심!!!!!",
          "original_form": "ㅋㅋ네 명심!!!!!",
          "speaker_id": "1",
          "time": "20210907 11:14"
        },
        {
          "id": "MDRW2100042880.1.19",
          "form": "최근머사셨어요?",
          "original_form": "최근머사셨어요?",
          "speaker_id": "2",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100042880.1.20",
          "form": "최근에 가을옷삿엇구",
          "original_form": "최근에 가을옷삿엇구",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100042880.1.21",
          "form": "다이어트한다꼬 두유도...",
          "original_form": "다이어트한다꼬 두유도...",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100042880.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100042880.1.23",
          "form": "오 다이어트ㅠㅠ저도 좀해야는데",
          "original_form": "오 다이어트ㅠㅠ저도 좀해야는데",
          "speaker_id": "2",
          "time": "20210907 11:32"
        },
        {
          "id": "MDRW2100042880.1.24",
          "form": "주구장창 먹을것만사고ㅋㅈㅋ",
          "original_form": "주구장창 먹을것만사고ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042880.1.25",
          "form": "저두여ㅜㅜㅠ",
          "original_form": "저두여ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042880.1.26",
          "form": "먹는낙으로 살아용",
          "original_form": "먹는낙으로 살아용",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042880.1.27",
          "form": "그 낙이라도있어야지ㅠ",
          "original_form": "그 낙이라도있어야지ㅠ",
