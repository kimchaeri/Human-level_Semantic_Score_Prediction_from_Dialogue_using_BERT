      "utterance": [
        {
          "id": "MDRW2100027115.1.1",
          "form": "좋은데말이죠",
          "original_form": "좋은데말이죠",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100027115.1.2",
          "form": "그렇죠. 여기도 4단계라",
          "original_form": "그렇죠. 여기도 4단계라",
          "speaker_id": "1",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100027115.1.3",
          "form": "코로나땜에 식당서 먹기 힘드네요",
          "original_form": "코로나땜에 식당서 먹기 힘드네요",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100027115.1.4",
          "form": "포장이나 배달합니다",
          "original_form": "포장이나 배달합니다",
          "speaker_id": "1",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100027115.1.5",
          "form": "내일은 치킨뜯으려합니다",
          "original_form": "내일은 치킨뜯으려합니다",
          "speaker_id": "1",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100027115.1.6",
          "form": "포장도 좋죠",
          "original_form": "포장도 좋죠",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100027115.1.7",
          "form": "두마리치킨이요",
          "original_form": "두마리치킨이요",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.8",
          "form": "어디치킨드시나요",
          "original_form": "어디치킨드시나요",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.9",
          "form": "무슨맛으로 먹나요",
          "original_form": "무슨맛으로 먹나요",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.10",
          "form": "피자나라치킨공주가 없습니다",
          "original_form": "피자나라치킨공주가 없습니다",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.11",
          "form": "두개 한번에시켜 개이득인데",
          "original_form": "두개 한번에시켜 개이득인데",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.12",
          "form": "후라이드나 땡초 좋아합니다",
          "original_form": "후라이드나 땡초 좋아합니다",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.13",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.14",
          "form": "다리보다 날개가 맛있죠",
          "original_form": "다리보다 날개가 맛있죠",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.15",
          "form": "요즘 치킨집에서",
          "original_form": "요즘 치킨집에서",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.16",
          "form": "떡볶이 팔던데",
          "original_form": "떡볶이 팔던데",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.17",
          "form": "진짜 존맛이에요",
          "original_form": "진짜 존맛이에요",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.18",
          "form": "옛날통닭도 좋구요",
          "original_form": "옛날통닭도 좋구요",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.19",
          "form": "치킨보다 맛있더라구요",
          "original_form": "치킨보다 맛있더라구요",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027115.1.20",
          "form": "떡볶이는 로제가 궁금합니다",
          "original_form": "떡볶이는 로제가 궁금합니다",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027115.1.21",
          "form": "옛날통닭은 닭똥집 튀김도 같이 먹어야죠",
          "original_form": "옛날통닭은 닭똥집 튀김도 같이 먹어야죠",
          "speaker_id": "2",
          "time": "20210902 09:42"
        }
      ]
