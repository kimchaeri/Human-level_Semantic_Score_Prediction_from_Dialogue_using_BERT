      "utterance": [
        {
          "id": "MDRW2100024316.1.1",
          "form": "쿠팡에서 무엇을 구매하셨나요",
          "original_form": "쿠팡에서 무엇을 구매하셨나요",
          "speaker_id": "2",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024316.1.2",
          "form": "쇼핑이염",
          "original_form": "쇼핑이염",
          "speaker_id": "2",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024316.1.3",
          "form": "샤브샤브와 함께 먹으면 좋은 음식입니다",
          "original_form": "샤브샤브와 함께 먹으면 좋은 음식입니다",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024316.1.4",
          "form": "소스ㅋㅋ음식이 아니고",
          "original_form": "소스ㅋㅋ음식이 아니고",
          "speaker_id": "1",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024316.1.5",
          "form": "소스무엇?",
          "original_form": "소스무엇?",
          "speaker_id": "2",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024316.1.6",
          "form": "후이펑스리라차소스",
          "original_form": "후이펑스리라차소스",
          "speaker_id": "2",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024316.1.7",
          "form": "혹시아닌가요",
          "original_form": "혹시아닌가요",
          "speaker_id": "2",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024316.1.8",
          "form": "하이몬칠리샤브소스",
          "original_form": "하이몬칠리샤브소스",
          "speaker_id": "1",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024316.1.9",
          "form": "입니다ㅋ",
          "original_form": "입니다ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024316.1.10",
          "form": "샤브수끼소스라되어있네여ㅋㅋ",
          "original_form": "샤브수끼소스라되어있네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024316.1.11",
          "form": "칼로리는 장담 못함ㅠㅠㅋㅋ",
          "original_form": "칼로리는 장담 못함ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024316.1.12",
          "form": "샤브수끼네요",
          "original_form": "샤브수끼네요",
          "speaker_id": "2",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024316.1.13",
          "form": "소스는 소량이라 괜찮아요",
          "original_form": "소스는 소량이라 괜찮아요",
          "speaker_id": "2",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024316.1.14",
          "form": "이거 진짜 맛있어여",
          "original_form": "이거 진짜 맛있어여",
          "speaker_id": "1",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024316.1.15",
          "form": "맛집에서 먹는 기분ㅋㅋㅋ",
          "original_form": "맛집에서 먹는 기분ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024316.1.16",
          "form": "사봤어요",
          "original_form": "사봤어요",
          "speaker_id": "2",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024316.1.17",
          "form": "얼른만나서 식사하고싶어요ㅋㅋㅋ",
          "original_form": "얼른만나서 식사하고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024316.1.18",
          "form": "정해진것인가요?",
          "original_form": "정해진것인가요?",
          "speaker_id": "2",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024316.1.19",
          "form": "메뉴까지 정해놓으니 든든하네요",
          "original_form": "메뉴까지 정해놓으니 든든하네요",
          "speaker_id": "1",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024316.1.20",
          "form": "네ㅋㅋ샤브샤브 당첨ㅋㅋ",
          "original_form": "네ㅋㅋ샤브샤브 당첨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024316.1.21",
          "form": "맞아요 차에섬구이바다꺼내올게요",
          "original_form": "맞아요 차에섬구이바다꺼내올게요",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024316.1.22",
          "form": "샤브샤브먹고ㅋㄱ",
          "original_form": "샤브샤브먹고ㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024316.1.23",
          "form": "보글보글 끓여서먹어야죠",
          "original_form": "보글보글 끓여서먹어야죠",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024316.1.24",
          "form": "우리스벅가요ㅋㅋㅋ",
          "original_form": "우리스벅가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024316.1.25",
          "form": "스타벅스말인가요?",
          "original_form": "스타벅스말인가요?",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024316.1.26",
          "form": "스벅쿠폰 많아요ㅋㅋ",
          "original_form": "스벅쿠폰 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024316.1.27",
          "form": "저는 스타벅스를 정말좋아해요",
          "original_form": "저는 스타벅스를 정말좋아해요",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024316.1.28",
          "form": "그런데 안간지 오래돼었어요",
          "original_form": "그런데 안간지 오래돼었어요",
          "speaker_id": "2",
          "time": "20210831 00:44"
        }
      ]
    }
