      "utterance": [
        {
          "id": "MDRW2100042798.1.1",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.2",
          "form": "옛말이다",
          "original_form": "옛말이다",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.3",
          "form": "애들 아침은 줬어요?",
          "original_form": "애들 아침은 줬어요?",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.4",
          "form": "사람들이 다 토박이만 있어서",
          "original_form": "사람들이 다 토박이만 있어서",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.5",
          "form": "맞아요 텃셋이 맘",
          "original_form": "맞아요 텃셋이 맘",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.6",
          "form": "새로운 거 싫어해",
          "original_form": "새로운 거 싫어해",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.7",
          "form": "텃세 심함 ㅋㅋㅋㅋ",
          "original_form": "텃세 심함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.8",
          "form": "어 어제 먹던 국 말아서 줬지",
          "original_form": "어 어제 먹던 국 말아서 줬지",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.9",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042798.1.10",
          "form": "잘했네요 잘했어ㅋㅋㅋ",
          "original_form": "잘했네요 잘했어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.11",
          "form": "휴직 내는 것도 싫어하고",
          "original_form": "휴직 내는 것도 싫어하고",
          "speaker_id": "2",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.12",
          "form": "눈치 주겠지요",
          "original_form": "눈치 주겠지요",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.13",
          "form": "일단 어른들이 너무 많아서",
          "original_form": "일단 어른들이 너무 많아서",
          "speaker_id": "2",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.14",
          "form": "사람 비면 자기들이 일해야 되니",
          "original_form": "사람 비면 자기들이 일해야 되니",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.15",
          "form": "젊은 사람들 쉬는 꼴을 못 봄",
          "original_form": "젊은 사람들 쉬는 꼴을 못 봄",
          "speaker_id": "2",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.16",
          "form": "그 사람들은",
          "original_form": "그 사람들은",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.17",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.18",
          "form": "유가 휴직하고 쓰는 거 꼴까울 걸요",
          "original_form": "유가 휴직하고 쓰는 거 꼴까울 걸요",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.19",
          "form": "육아 ㅋㅋ",
          "original_form": "육아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.20",
          "form": "어 욕 많이 먹었다 거기서",
          "original_form": "어 욕 많이 먹었다 거기서",
          "speaker_id": "2",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.21",
          "form": "그나마 신랑이 있어서",
          "original_form": "그나마 신랑이 있어서",
          "speaker_id": "2",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.22",
          "form": "자기 때는 다 그냥 키웠다면서ㅋㅋ",
          "original_form": "자기 때는 다 그냥 키웠다면서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042798.1.23",
          "form": "라떼 타임",
          "original_form": "라떼 타임",
          "speaker_id": "1",
          "time": "20210907 09:20"
        }
      ]
