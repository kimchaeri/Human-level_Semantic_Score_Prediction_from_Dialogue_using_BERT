      "utterance": [
        {
          "id": "MDRW2100012918.1.1",
          "form": "연애하시나요",
          "original_form": "연애하시나요",
          "speaker_id": "1",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012918.1.2",
          "form": "아니요 안한지 오래됐어요ㅠ",
          "original_form": "아니요 안한지 오래됐어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012918.1.3",
          "form": "소개팅도 안하시나요 ㅎㅎ",
          "original_form": "소개팅도 안하시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012918.1.4",
          "form": "요새는 별 생각이 안들더라고요",
          "original_form": "요새는 별 생각이 안들더라고요",
          "speaker_id": "2",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012918.1.5",
          "form": "인연이 있을거에요",
          "original_form": "인연이 있을거에요",
          "speaker_id": "1",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012918.1.6",
          "form": "때되면 오겠죠 ㅎㅎ",
          "original_form": "때되면 오겠죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012918.1.7",
          "form": "ㅎㅎ 결혼하신지 얼마나 되셨어요?",
          "original_form": "ㅎㅎ 결혼하신지 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012918.1.8",
          "form": "저 작년에요!",
          "original_form": "저 작년에요!",
          "speaker_id": "1",
          "time": "20210825 08:42"
        },
        {
          "id": "MDRW2100012918.1.9",
          "form": "아 신혼이시네요ㅎㅎ",
          "original_form": "아 신혼이시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 08:50"
        },
        {
          "id": "MDRW2100012918.1.10",
          "form": "네 그렇습니다ㅋㅋ",
          "original_form": "네 그렇습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:52"
        },
        {
          "id": "MDRW2100012918.1.11",
          "form": "다행히 1단계때 ㅎ",
          "original_form": "다행히 1단계때 ㅎ",
          "speaker_id": "1",
          "time": "20210825 08:52"
        },
        {
          "id": "MDRW2100012918.1.12",
          "form": "아 다행이시네요",
          "original_form": "아 다행이시네요",
          "speaker_id": "2",
          "time": "20210825 09:07"
        },
        {
          "id": "MDRW2100012918.1.13",
          "form": "진짜 코로나때문에 결혼하려고 했던 친구도 쫌 미루고 그러더라고요",
          "original_form": "진짜 코로나때문에 결혼하려고 했던 친구도 쫌 미루고 그러더라고요",
          "speaker_id": "2",
          "time": "20210825 09:07"
        },
        {
          "id": "MDRW2100012918.1.14",
          "form": "될놈될이라서",
          "original_form": "될놈될이라서",
          "speaker_id": "1",
          "time": "20210825 09:08"
        },
        {
          "id": "MDRW2100012918.1.15",
          "form": "미뤄도 3단계 따라오더라고요",
          "original_form": "미뤄도 3단계 따라오더라고요",
          "speaker_id": "1",
          "time": "20210825 09:08"
        },
        {
          "id": "MDRW2100012918.1.16",
          "form": "원래는 결혼 생각 없었는데 요새는 조금씩 생기더라고요 ㅎㅎ",
          "original_form": "원래는 결혼 생각 없었는데 요새는 조금씩 생기더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012918.1.17",
          "form": "상대는 없지만 ㅋㅋㅋ",
          "original_form": "상대는 없지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012918.1.18",
          "form": "ㅋㅋ언젠가 딱 나타날거에요",
          "original_form": "ㅋㅋ언젠가 딱 나타날거에요",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012918.1.19",
          "form": "기회를 만드셔요",
          "original_form": "기회를 만드셔요",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012918.1.20",
          "form": "그럴까요? ㅎㅎ",
          "original_form": "그럴까요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:23"
        }
      ]
    }
