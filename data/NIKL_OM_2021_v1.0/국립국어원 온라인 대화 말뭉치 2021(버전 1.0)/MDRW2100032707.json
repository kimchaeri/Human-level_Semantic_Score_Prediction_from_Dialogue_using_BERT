      "utterance": [
        {
          "id": "MDRW2100032707.1.1",
          "form": "강서방이라는데요",
          "original_form": "강서방이라는데요",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032707.1.2",
          "form": "어멋",
          "original_form": "어멋",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032707.1.3",
          "form": "그 남친이 강씨 거든요",
          "original_form": "그 남친이 강씨 거든요",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032707.1.4",
          "form": "부담스러워요",
          "original_form": "부담스러워요",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032707.1.5",
          "form": "친구들한테는 소개시켜 주셨나요?",
          "original_form": "친구들한테는 소개시켜 주셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032707.1.6",
          "form": "남친 친구는 본 적 있으세요?",
          "original_form": "남친 친구는 본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032707.1.7",
          "form": "네네 친구가 제가 없어서요",
          "original_form": "네네 친구가 제가 없어서요",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032707.1.8",
          "form": "한 명 있는 친구한테 말했더니",
          "original_form": "한 명 있는 친구한테 말했더니",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.9",
          "form": "님 친구가 섭섭하시겠어요",
          "original_form": "님 친구가 섭섭하시겠어요",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.10",
          "form": "지 심심할 때 맨날 연애 얘기 물어봐요",
          "original_form": "지 심심할 때 맨날 연애 얘기 물어봐요",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.11",
          "form": "시간 떼우려고",
          "original_form": "시간 떼우려고",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.12",
          "form": "좋은 친구네요",
          "original_form": "좋은 친구네요",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.13",
          "form": "무슨 이야기꾼 같아요 제가",
          "original_form": "무슨 이야기꾼 같아요 제가",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.14",
          "form": "좋아요?",
          "original_form": "좋아요?",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.16",
          "form": "친구가 딱 님 같아요",
          "original_form": "친구가 딱 님 같아요",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.17",
          "form": "남친의 친구는 만날 생각 없나요?",
          "original_form": "남친의 친구는 만날 생각 없나요?",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032707.1.18",
          "form": "왜 만나야 하죠?",
          "original_form": "왜 만나야 하죠?",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032707.1.19",
          "form": "결혼하나요",
          "original_form": "결혼하나요",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032707.1.20",
          "form": "남친은 소개시켜 주려고 안 하나요?",
          "original_form": "남친은 소개시켜 주려고 안 하나요?",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032707.1.21",
          "form": "남친도 딱히 친구가 없는 거 같던데요",
          "original_form": "남친도 딱히 친구가 없는 거 같던데요",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032707.1.22",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032707.1.23",
          "form": "뭐가 좋아요",
          "original_form": "뭐가 좋아요",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032707.1.24",
          "form": "친구 많은 거 별로죠",
          "original_form": "친구 많은 거 별로죠",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032707.1.25",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210905 22:37"
        }
      ]
    }
