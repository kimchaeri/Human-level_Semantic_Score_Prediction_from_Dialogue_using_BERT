      "utterance": [
        {
          "id": "MDRW2100044619.1.1",
          "form": "네네 들어봤어요..!",
          "original_form": "네네 들어봤어요..!",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044619.1.2",
          "form": "혹시 써보셨나요?",
          "original_form": "혹시 써보셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044619.1.3",
          "form": "이번에 사봤는데 궁금하네요ㅎㅎ",
          "original_form": "이번에 사봤는데 궁금하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044619.1.4",
          "form": "아니요 써보진않았어요",
          "original_form": "아니요 써보진않았어요",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044619.1.5",
          "form": "수분크림은 쓰고 계신가요??",
          "original_form": "수분크림은 쓰고 계신가요??",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044619.1.6",
          "form": "진정효과가 좋다고 하느데",
          "original_form": "진정효과가 좋다고 하느데",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044619.1.7",
          "form": "전 히든랩거써요ㅋㅋㅋㅋ",
          "original_form": "전 히든랩거써요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044619.1.8",
          "form": "오 첨 들어보네요!",
          "original_form": "오 첨 들어보네요!",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044619.1.9",
          "form": "저는 아이소이 수분크림 쓰고 있어요ㅎㅎ",
          "original_form": "저는 아이소이 수분크림 쓰고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044619.1.10",
          "form": "수분감이 괜찮은가요?",
          "original_form": "수분감이 괜찮은가요?",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044619.1.11",
          "form": "네 저거 완전 수분 빵빵이에요",
          "original_form": "네 저거 완전 수분 빵빵이에요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044619.1.12",
          "form": "유분기가 하나도 없어서 걱정될 정도라고 해야할까요.....",
          "original_form": "유분기가 하나도 없어서 걱정될 정도라고 해야할까요.....",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044619.1.13",
          "form": "여니님 피부 타입이 어떻게 되시나요?",
          "original_form": "여니님 피부 타입이 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044619.1.14",
          "form": "전 볼은 중건성이고 티존이나 이마는 지성인 복합성이요.. ㅜ",
          "original_form": "전 볼은 중건성이고 티존이나 이마는 지성인 복합성이요.. ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044619.1.15",
          "form": "그래서 이마에 뭐가 나서 썬크림 유목민이였어요ㅋㅋㅋㅋ",
          "original_form": "그래서 이마에 뭐가 나서 썬크림 유목민이였어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044619.1.16",
          "form": "아 저는 파워 지성입니다ㅎㅎ",
          "original_form": "아 저는 파워 지성입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044619.1.17",
          "form": "당근마켓 이용하신적 있으신가요?",
          "original_form": "당근마켓 이용하신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044619.1.18",
          "form": "어 전 스벅쿠폰만 팔아봤어요",
          "original_form": "어 전 스벅쿠폰만 팔아봤어요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044619.1.19",
          "form": "자주 이용하시나요?",
          "original_form": "자주 이용하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044619.1.20",
          "form": "저도 자주는 아니고 팔거 있으면 가요ㅎㅎ",
          "original_form": "저도 자주는 아니고 팔거 있으면 가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044619.1.21",
          "form": "올리브영 세일때 많이 구매하셨나요?",
          "original_form": "올리브영 세일때 많이 구매하셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044619.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044619.1.23",
          "form": "되도록이면 맞출려고 해요ㅋㅋㅋㅋ",
          "original_form": "되도록이면 맞출려고 해요ㅋㅋㅋㅋ",
