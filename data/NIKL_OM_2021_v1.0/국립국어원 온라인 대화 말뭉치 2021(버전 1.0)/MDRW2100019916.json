      "utterance": [
        {
          "id": "MDRW2100019916.1.1",
          "form": "격하게 동의해요",
          "original_form": "격하게 동의해요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019916.1.2",
          "form": "저도 지금 무릎치는중임다",
          "original_form": "저도 지금 무릎치는중임다",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019916.1.3",
          "form": "싫으면 아예 같이 안 지냈다!",
          "original_form": "싫으면 아예 같이 안 지냈다!",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019916.1.4",
          "form": "저는 이걸 두고",
          "original_form": "저는 이걸 두고",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019916.1.5",
          "form": "\"네가 아직 좋아하는 사람이 안생겨봐서 그 차이를 모른다\" 는 소리를 들었는데",
          "original_form": "\"네가 아직 좋아하는 사람이 안생겨봐서 그 차이를 모른다\" 는 소리를 들었는데",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019916.1.6",
          "form": "정말일까요..??",
          "original_form": "정말일까요..??",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019916.1.7",
          "form": "음 저는 다른 사람 좋아해본 적 있는데",
          "original_form": "음 저는 다른 사람 좋아해본 적 있는데",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019916.1.8",
          "form": "오 네네 (흥미진진)",
          "original_form": "오 네네 (흥미진진)",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019916.1.9",
          "form": "그냥 설레는 게 좀 더 격했을 뿐이고",
          "original_form": "그냥 설레는 게 좀 더 격했을 뿐이고",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019916.1.10",
          "form": "그렇게 우정이랑 다를 바는 없었던 거 같아요",
          "original_form": "그렇게 우정이랑 다를 바는 없었던 거 같아요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019916.1.11",
          "form": "제 얘기를 풀자면",
          "original_form": "제 얘기를 풀자면",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019916.1.12",
          "form": "우정의 격한버전인가요 ㅋㅋㅋㅋㅋ",
          "original_form": "우정의 격한버전인가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019916.1.13",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019916.1.14",
          "form": "ㅋㅌㅌㅌㅋㅋ네 그랬어요 저는",
          "original_form": "ㅋㅌㅌㅌㅋㅋ네 그랬어요 저는",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019916.1.15",
          "form": "거의 10년 된 친구가 있는데",
          "original_form": "거의 10년 된 친구가 있는데",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019916.1.16",
          "form": "결과부터 말하면 지금 친구로 무진장 잘 지내고 있어요",
          "original_form": "결과부터 말하면 지금 친구로 무진장 잘 지내고 있어요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019916.1.17",
          "form": "오 정말 좋은 결과네요",
          "original_form": "오 정말 좋은 결과네요",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019916.1.18",
          "form": "같이 여성주의 노선을 타게 된 케이스고",
          "original_form": "같이 여성주의 노선을 타게 된 케이스고",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019916.1.19",
          "form": "지금은 아니지만 노선 타기 전에 성애로 서로 사귄 적이 있습니다 근디",
          "original_form": "지금은 아니지만 노선 타기 전에 성애로 서로 사귄 적이 있습니다 근디",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019916.1.20",
          "form": "헉ㅋㅋㅋㅋㅋㅋ 관계 유지의 비결이 궁금하네요",
          "original_form": "헉ㅋㅋㅋㅋㅋㅋ 관계 유지의 비결이 궁금하네요",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019916.1.21",
          "form": "경험한 바로는 서로를 좋아하는 마음이 크게 달라지지 않았음에도 성애로 한정짓지 않게 됐어요",
          "original_form": "경험한 바로는 서로를 좋아하는 마음이 크게 달라지지 않았음에도 성애로 한정짓지 않게 됐어요",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019916.1.22",
          "form": "사귀고 나서 깨지고 돌이켜보니 마치 역할놀이 한ㅇ 것처럼 느껴지더라구요",
          "original_form": "사귀고 나서 깨지고 돌이켜보니 마치 역할놀이 한ㅇ 것처럼 느껴지더라구요",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019916.1.23",
          "form": "우와 경험으로 알게되셨군요",
          "original_form": "우와 경험으로 알게되셨군요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        }
      ]
    }
