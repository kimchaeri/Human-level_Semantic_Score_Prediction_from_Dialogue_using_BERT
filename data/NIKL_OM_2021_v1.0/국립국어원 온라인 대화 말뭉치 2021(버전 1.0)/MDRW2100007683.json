        },
        {
          "id": "MDRW2100007683.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007683.1.4",
          "form": "언제쯤 생각요?",
          "original_form": "언제쯤 생각요?",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007683.1.5",
          "form": "이 한 몸 건사하기도 어려워서요",
          "original_form": "이 한 몸 건사하기도 어려워서요",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007683.1.6",
          "form": "솔직히 결혼할 생각이 별로 없어요;;",
          "original_form": "솔직히 결혼할 생각이 별로 없어요;;",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007683.1.7",
          "form": "결혼하고 나서 뭐가 가장 좋으신가요?",
          "original_form": "결혼하고 나서 뭐가 가장 좋으신가요?",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007683.1.8",
          "form": "함께 있는 느낌이 좋아요",
          "original_form": "함께 있는 느낌이 좋아요",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007683.1.9",
          "form": "아. 저도 마음 맞는 동반자를 찾으면 생각이 바뀌려나요",
          "original_form": "아. 저도 마음 맞는 동반자를 찾으면 생각이 바뀌려나요",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007683.1.10",
          "form": "그게 중요하죠",
          "original_form": "그게 중요하죠",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007683.1.11",
          "form": "행복해보여서 좋네요",
          "original_form": "행복해보여서 좋네요",
          "speaker_id": "2",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007683.1.12",
          "form": "결혼  일찍 하셨나요",
          "original_form": "결혼  일찍 하셨나요",
          "speaker_id": "2",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007683.1.13",
          "form": "9년됐어요",
          "original_form": "9년됐어요",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007683.1.14",
          "form": "미팅으로 만나셨나요 아님 직장 동료?",
          "original_form": "미팅으로 만나셨나요 아님 직장 동료?",
          "speaker_id": "2",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007683.1.15",
          "form": "친구 소개요",
          "original_form": "친구 소개요",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007683.1.16",
          "form": "결혼할 때 친구에게 한턱 내셨겠네요",
