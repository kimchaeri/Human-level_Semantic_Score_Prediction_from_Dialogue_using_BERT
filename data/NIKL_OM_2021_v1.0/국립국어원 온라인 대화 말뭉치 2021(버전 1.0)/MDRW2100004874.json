      "utterance": [
        {
          "id": "MDRW2100004874.1.1",
          "form": "좋았던 여행지 있나요?",
          "original_form": "좋았던 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004874.1.2",
          "form": "국내여행은 많이 안해봐서",
          "original_form": "국내여행은 많이 안해봐서",
          "speaker_id": "1",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004874.1.3",
          "form": "국내여행중에 제일 좋다고 생각한데는 동해였던것 같애요ㅎ",
          "original_form": "국내여행중에 제일 좋다고 생각한데는 동해였던것 같애요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004874.1.4",
          "form": "솔직히 말해서 뭐없어요ㅎ 관광지긴 하지만",
          "original_form": "솔직히 말해서 뭐없어요ㅎ 관광지긴 하지만",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004874.1.5",
          "form": "시골이에요ㅎ 근데 그게 너무 좋았어요ㅎ 동해항 바로 앞에 무슨 절벽이있어요ㅎ 거기에 숙소가 게스트하우스였는데 정말 조금했거든요",
          "original_form": "시골이에요ㅎ 근데 그게 너무 좋았어요ㅎ 동해항 바로 앞에 무슨 절벽이있어요ㅎ 거기에 숙소가 게스트하우스였는데 정말 조금했거든요",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004874.1.6",
          "form": "동해 좋죠",
          "original_form": "동해 좋죠",
          "speaker_id": "1",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004874.1.7",
          "form": "일출 일몰을 게하나가면 바로볼수 있었고 다닥다닥 붙은 골목길은 운치가 있을정도였어요ㅎㅎ",
          "original_form": "일출 일몰을 게하나가면 바로볼수 있었고 다닥다닥 붙은 골목길은 운치가 있을정도였어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004874.1.8",
          "form": "속초만 가본거같아요",
          "original_form": "속초만 가본거같아요",
          "speaker_id": "1",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004874.1.9",
          "form": "물론 서핑도했구요ㅎ",
          "original_form": "물론 서핑도했구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004874.1.10",
          "form": "속초에 닭강정드셨어요?",
          "original_form": "속초에 닭강정드셨어요?",
          "speaker_id": "2",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004874.1.11",
          "form": "오 서핑~",
          "original_form": "오 서핑~",
          "speaker_id": "1",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004874.1.12",
          "form": "아뇨 막돌아다니진않았던거같은데",
          "original_form": "아뇨 막돌아다니진않았던거같은데",
          "speaker_id": "1",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004874.1.13",
          "form": "속초가 아니였나 ㅋㅋ",
          "original_form": "속초가 아니였나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004874.1.14",
          "form": "쏠비치있는데가 강릉인가요?",
          "original_form": "쏠비치있는데가 강릉인가요?",
          "speaker_id": "1",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004874.1.15",
          "form": "쏠비치 어디서 많이들어봤는데",
          "original_form": "쏠비치 어디서 많이들어봤는데",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004874.1.16",
          "form": "어... 많이들어본곳인데",
          "original_form": "어... 많이들어본곳인데",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004874.1.17",
          "form": "지점이 많긴해요",
          "original_form": "지점이 많긴해요",
          "speaker_id": "1",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004874.1.18",
          "form": "설악산 케이블카 타려다 못탔는데ㅋ",
          "original_form": "설악산 케이블카 타려다 못탔는데ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004874.1.19",
          "form": "ㅎㅎ 케이블카 좋죠ㅎ 강원도는 정말 좋은것같애요ㅎ",
          "original_form": "ㅎㅎ 케이블카 좋죠ㅎ 강원도는 정말 좋은것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004874.1.20",
          "form": "철원은가보셨어요?",
          "original_form": "철원은가보셨어요?",
          "speaker_id": "2",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004874.1.21",
          "form": "겨울에 레일바이크 한번 타보세요ㅎㅎ 절벽에서 타는거랑 같애요ㅎ",
          "original_form": "겨울에 레일바이크 한번 타보세요ㅎㅎ 절벽에서 타는거랑 같애요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004874.1.22",
          "form": "철원옆이 외할머니 댁이라서",
          "original_form": "철원옆이 외할머니 댁이라서",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004874.1.23",
          "form": "한탕강은가봤는데",
          "original_form": "한탕강은가봤는데",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004874.1.24",
          "form": "철원에 그런것도 있어요?",
          "original_form": "철원에 그런것도 있어요?",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004874.1.25",
          "form": "자주가셨겠네요ㅎ",
          "original_form": "자주가셨겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004874.1.26",
          "form": "철원이 아닌가?ㅎㅎ",
          "original_form": "철원이 아닌가?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004874.1.27",
          "form": "어릴땐 한탕강가서 많이 놀았죠ㅋ",
          "original_form": "어릴땐 한탕강가서 많이 놀았죠ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004874.1.28",
          "form": "경계인거같아요 ㅎ 경기도 포천이랑",
          "original_form": "경계인거같아요 ㅎ 경기도 포천이랑",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004874.1.29",
          "form": "맞아 거기 무슨 폭포있지않아뇨?",
          "original_form": "맞아 거기 무슨 폭포있지않아뇨?",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004874.1.30",
          "form": "우리나라의 나이아가라 폭포라고",
          "original_form": "우리나라의 나이아가라 폭포라고",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004874.1.31",
          "form": "그건 잘모르겠어요ㅎㅎ",
          "original_form": "그건 잘모르겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004874.1.32",
          "form": "무슨 박물관도있었는데ㅎ",
          "original_form": "무슨 박물관도있었는데ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004874.1.33",
          "form": "친구가 부대에 있어서 갔었어요ㅎ",
          "original_form": "친구가 부대에 있어서 갔었어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004874.1.34",
          "form": "가본지가 몇십년된거같아요ㅋ",
          "original_form": "가본지가 몇십년된거같아요ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004874.1.35",
          "form": "청보리밭있는곳이었어요 ㅡㅎㅎ",
          "original_form": "청보리밭있는곳이었어요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004874.1.36",
          "form": "이참에 한번닸다오셔요ㅎㅎ",
          "original_form": "이참에 한번닸다오셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004874.1.37",
          "form": "그럼 이번에 여행어디로 가셨어요?",
          "original_form": "그럼 이번에 여행어디로 가셨어요?",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004874.1.38",
          "form": "작년부터 아무데도 못갔어요",
          "original_form": "작년부터 아무데도 못갔어요",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004874.1.39",
          "form": "집콕",
          "original_form": "집콕",
          "speaker_id": "1",
          "time": "20210821 19:11"
        }
      ]
    }
