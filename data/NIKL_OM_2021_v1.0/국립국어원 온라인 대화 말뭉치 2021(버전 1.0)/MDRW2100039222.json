        },
        {
          "id": "MDRW2100039222.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100039222.1.4",
          "form": "왜요ㅠ",
          "original_form": "왜요ㅠ",
          "speaker_id": "1",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100039222.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100039222.1.6",
          "form": "쿵쿵쿵!! 문열어!!!",
          "original_form": "쿵쿵쿵!! 문열어!!!",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100039222.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100039222.1.8",
          "form": "타르트는 어디서 사셨나요!",
          "original_form": "타르트는 어디서 사셨나요!",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100039222.1.9",
          "form": "타르트 인터넷이요......",
          "original_form": "타르트 인터넷이요......",
          "speaker_id": "1",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100039222.1.10",
          "form": "그저께 갑자기",
          "original_form": "그저께 갑자기",
          "speaker_id": "1",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100039222.1.11",
          "form": "당근케이크도 너무 먹고 싶어서 시키고",
          "original_form": "당근케이크도 너무 먹고 싶어서 시키고",
          "speaker_id": "1",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100039222.1.12",
          "form": "탄수화물 폭탄이에요 넘 맛있다ㅠㅠㅠㅠㅠㅠ",
          "original_form": "탄수화물 폭탄이에요 넘 맛있다ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100039222.1.13",
          "form": "맛있다=폭 탄 이 다",
          "original_form": "맛있다=폭 탄 이 다",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039222.1.14",
          "form": "이 공식 모르시나요",
          "original_form": "이 공식 모르시나요",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100039222.1.15",
          "form": "제가 님을 폭탄위기서 구출할게요",
          "original_form": "제가 님을 폭탄위기서 구출할게요",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100039222.1.16",
          "form": "남은것 저에게 남기고 피신하세요ㅣㅣ",
          "original_form": "남은것 저에게 남기고 피신하세요ㅣㅣ",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100039222.1.17",
          "form": "흥",
          "original_form": "흥",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.18",
          "form": "피신하면서 먹을 겁니다",
          "original_form": "피신하면서 먹을 겁니다",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.19",
          "form": "아무도 저의 식욕을 막을 수 없어여",
          "original_form": "아무도 저의 식욕을 막을 수 없어여",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.20",
          "form": "그건...남았다는 소리군요!!!",
          "original_form": "그건...남았다는 소리군요!!!",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.21",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.22",
          "form": "밤길 조심하세요",
          "original_form": "밤길 조심하세요",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.23",
          "form": "문단속 잘하시고",
          "original_form": "문단속 잘하시고",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.24",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.25",
          "form": "유도신문이엇나요..?",
          "original_form": "유도신문이엇나요..?",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039222.1.26",
          "form": "아뇽 백프로 선의입니다!!",
          "original_form": "아뇽 백프로 선의입니다!!",
          "speaker_id": "2",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100039222.1.27",
          "form": "ㅋㅋㅋㅋㅋ아닠ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아닠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039222.1.28",
          "form": "도둑이",
          "original_form": "도둑이",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039222.1.29",
          "form": "나 너털거야 조심해!",
          "original_form": "나 너털거야 조심해!",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039222.1.30",
          "form": "이러는 게 선의냐구요ㅠㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이러는 게 선의냐구요ㅠㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039222.1.31",
          "form": "네티도 선의였잖아욧!",
          "original_form": "네티도 선의였잖아욧!",
          "speaker_id": "2",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100039222.1.32",
          "form": "천사라구요!",
          "original_form": "천사라구요!",
