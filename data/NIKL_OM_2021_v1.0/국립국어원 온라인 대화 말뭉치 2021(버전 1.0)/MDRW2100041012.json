      "utterance": [
        {
          "id": "MDRW2100041012.1.1",
          "form": "ㅋㄱ그러시군요!",
          "original_form": "ㅋㄱ그러시군요!",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100041012.1.2",
          "form": "네 흑흑 그때 석사논문 쓸때였거든요",
          "original_form": "네 흑흑 그때 석사논문 쓸때였거든요",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100041012.1.3",
          "form": "최고로 미쳤던 때가요 ㅋㅋ",
          "original_form": "최고로 미쳤던 때가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041012.1.4",
          "form": "앗...아아...",
          "original_form": "앗...아아...",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041012.1.5",
          "form": "그땐 무려 발효빵을 만들었답니다",
          "original_form": "그땐 무려 발효빵을 만들었답니다",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041012.1.6",
          "form": "하루죙일걸리는 빵 만들기..",
          "original_form": "하루죙일걸리는 빵 만들기..",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041012.1.7",
          "form": "논문을 빼면 이렇게 엄청난결과가..",
          "original_form": "논문을 빼면 이렇게 엄청난결과가..",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041012.1.8",
          "form": "가족들이 다 사이좋게 오동통해졌어요",
          "original_form": "가족들이 다 사이좋게 오동통해졌어요",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041012.1.9",
          "form": "ㅋㅋㅋㅋㅋ 오동통해지고 석사도 통과하고",
          "original_form": "ㅋㅋㅋㅋㅋ 오동통해지고 석사도 통과하고",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041012.1.10",
          "form": "가스오븐을 풀로 돌리면서 동기들도 먹이고 가족들도 먹이고..",
          "original_form": "가스오븐을 풀로 돌리면서 동기들도 먹이고 가족들도 먹이고..",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041012.1.11",
          "form": "모두 햅삐해지셨군여",
          "original_form": "모두 햅삐해지셨군여",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041012.1.12",
          "form": "제일 마지막으로 만든건 안동찐빵인데",
          "original_form": "제일 마지막으로 만든건 안동찐빵인데",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041012.1.13",
          "form": "팥앙금도 만들었었어요",
          "original_form": "팥앙금도 만들었었어요",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041012.1.14",
          "form": "그리고나서 베이킹을 뚝 끊었습니다",
          "original_form": "그리고나서 베이킹을 뚝 끊었습니다",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041012.1.15",
          "form": "ㅋㅋㅋㅋ석사생의 광기는 어디까지였나",
          "original_form": "ㅋㅋㅋㅋ석사생의 광기는 어디까지였나",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100041012.1.16",
          "form": "저는 같이 논문쓰는 친구를 꼬셔서 텀블러에 맥주를 담아 연구실에서 먹는 정도만 해봤는데..",
          "original_form": "저는 같이 논문쓰는 친구를 꼬셔서 텀블러에 맥주를 담아 연구실에서 먹는 정도만 해봤는데..",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100041012.1.17",
          "form": "아니 그런건 딱히 일탈도 아니잖아요",
          "original_form": "아니 그런건 딱히 일탈도 아니잖아요",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041012.1.18",
          "form": "연구실에서 술마시는 교수님도 계시는걸요",
          "original_form": "연구실에서 술마시는 교수님도 계시는걸요",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041012.1.19",
          "form": "ㅎㅎ 저희교수님은 담배만 빵빵 피시고 술은 연구실에선 안드셨었어요",
          "original_form": "ㅎㅎ 저희교수님은 담배만 빵빵 피시고 술은 연구실에선 안드셨었어요",
          "speaker_id": "2",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100041012.1.20",
          "form": "그래서 다들 술은 나가서먹는 분위기였던거같아요",
          "original_form": "그래서 다들 술은 나가서먹는 분위기였던거같아요",
          "speaker_id": "2",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100041012.1.21",
          "form": "아놔 담배도 나가서 피세요 교수님",
          "original_form": "아놔 담배도 나가서 피세요 교수님",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100041012.1.22",
          "form": "건물 내 금연입니다",
          "original_form": "건물 내 금연입니다",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100041012.1.23",
          "form": "분명 점심먹은지 별로 안된 것 같은데",
          "original_form": "분명 점심먹은지 별로 안된 것 같은데",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100041012.1.24",
          "form": "시간 잘가네요 역시 주말..",
          "original_form": "시간 잘가네요 역시 주말..",
          "speaker_id": "1",
          "time": "20210905 16:49"
        }
      ]
    }
