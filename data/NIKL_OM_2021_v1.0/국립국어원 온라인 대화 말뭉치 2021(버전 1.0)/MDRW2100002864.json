      "utterance": [
        {
          "id": "MDRW2100002864.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002864.1.2",
          "form": "꽤나 귀차니즘 스타일이라서 특별히 가고 싶은데가 아니면 잘 안가는 편이에요",
          "original_form": "꽤나 귀차니즘 스타일이라서 특별히 가고 싶은데가 아니면 잘 안가는 편이에요",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002864.1.3",
          "form": "전 코로나때문에",
          "original_form": "전 코로나때문에",
          "speaker_id": "1",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002864.1.4",
          "form": "만약 여행가시게 되면 어디로 가고 싶으셔요?",
          "original_form": "만약 여행가시게 되면 어디로 가고 싶으셔요?",
          "speaker_id": "2",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002864.1.5",
          "form": "스페인이요",
          "original_form": "스페인이요",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002864.1.6",
          "form": "혹시 스페인어나 영어 잘하시나요?",
          "original_form": "혹시 스페인어나 영어 잘하시나요?",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002864.1.7",
          "form": "모국어만 합미다 ㅋㅋ",
          "original_form": "모국어만 합미다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002864.1.8",
          "form": "와 그런데도 해외여행이요? 대단하시네요",
          "original_form": "와 그런데도 해외여행이요? 대단하시네요",
          "speaker_id": "2",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002864.1.9",
          "form": "전 무서워서 못가겠던데요",
          "original_form": "전 무서워서 못가겠던데요",
          "speaker_id": "2",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002864.1.10",
          "form": "코로나 전에만 갔아요",
          "original_form": "코로나 전에만 갔아요",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002864.1.11",
          "form": "어디어디 가셨어요?",
          "original_form": "어디어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002864.1.12",
          "form": "괌니요",
          "original_form": "괌니요",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002864.1.13",
          "form": "오오~ 거기 가셔서 뭐하고 다니셨어요??",
          "original_form": "오오~ 거기 가셔서 뭐하고 다니셨어요??",
          "speaker_id": "2",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002864.1.14",
          "form": "바다에서 놀았죠",
          "original_form": "바다에서 놀았죠",
          "speaker_id": "1",
          "time": "20210820 16:41"
