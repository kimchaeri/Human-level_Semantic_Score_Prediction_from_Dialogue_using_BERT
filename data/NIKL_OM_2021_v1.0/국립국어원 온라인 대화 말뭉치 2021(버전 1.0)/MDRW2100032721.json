      "utterance": [
        {
          "id": "MDRW2100032721.1.1",
          "form": "웅 저녁 먹었지 ~",
          "original_form": "웅 저녁 먹었지 ~",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100032721.1.2",
          "form": "저녁 챙겨먹었어?",
          "original_form": "저녁 챙겨먹었어?",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032721.1.3",
          "form": "저는 그냥 햄버거먹었어요",
          "original_form": "저는 그냥 햄버거먹었어요",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032721.1.4",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032721.1.5",
          "form": "근데 고기들어가니까 좀 괜찮더라고요ㅋㄱㅋㅋ",
          "original_form": "근데 고기들어가니까 좀 괜찮더라고요ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032721.1.6",
          "form": "웅웅 마쟈 고기 먹으면 힘남",
          "original_form": "웅웅 마쟈 고기 먹으면 힘남",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032721.1.7",
          "form": "나는 저녁에 쭈삼먹었어",
          "original_form": "나는 저녁에 쭈삼먹었어",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032721.1.8",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032721.1.9",
          "form": "전 지금 씨유가서 아이스크림 샀어요.",
          "original_form": "전 지금 씨유가서 아이스크림 샀어요.",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032721.1.10",
          "form": "쭈꾸미삼겹살 먹고 배부르다 했는데",
          "original_form": "쭈꾸미삼겹살 먹고 배부르다 했는데",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032721.1.11",
          "form": "좀 지나니까 입이 궁금해서 커피마심",
          "original_form": "좀 지나니까 입이 궁금해서 커피마심",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032721.1.12",
          "form": "잘햇어잘햇어~~~",
          "original_form": "잘햇어잘햇어~~~",
          "speaker_id": "2",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032721.1.13",
          "form": "와 쭈꾸미삼겹살",
          "original_form": "와 쭈꾸미삼겹살",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032721.1.14",
          "form": "진짜진짜맛있겠다",
          "original_form": "진짜진짜맛있겠다",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032721.1.15",
          "form": "언니 잘드시니까 좋네용",
          "original_form": "언니 잘드시니까 좋네용",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032721.1.16",
          "form": "우웅 ㅋ 요즘 잘 먹으려고 노력중",
          "original_form": "우웅 ㅋ 요즘 잘 먹으려고 노력중",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100032721.1.17",
          "form": "진짜 그게 중요하더만요",
          "original_form": "진짜 그게 중요하더만요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032721.1.18",
          "form": "웅웅 잘먹어야 기운도 나고",
          "original_form": "웅웅 잘먹어야 기운도 나고",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032721.1.19",
          "form": "난 배라 아이스크림 좋아해서",
          "original_form": "난 배라 아이스크림 좋아해서",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032721.1.20",
          "form": "엄마는외계인으로 자주 사와",
          "original_form": "엄마는외계인으로 자주 사와",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032721.1.21",
          "form": "귀여우세요",
          "original_form": "귀여우세요",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032721.1.22",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032721.1.23",
          "form": "그래??ㅋㅋ",
          "original_form": "그래??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032721.1.24",
          "form": "초코아이스크림 중에는 그거랑 하겐다즈인가 그걸 좋아해",
          "original_form": "초코아이스크림 중에는 그거랑 하겐다즈인가 그걸 좋아해",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032721.1.25",
          "form": "아니다 허쉬 였다 ㅋㅋ",
          "original_form": "아니다 허쉬 였다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032721.1.26",
