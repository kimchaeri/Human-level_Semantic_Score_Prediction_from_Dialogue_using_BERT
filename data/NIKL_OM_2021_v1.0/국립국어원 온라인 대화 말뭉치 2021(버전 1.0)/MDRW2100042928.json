      "utterance": [
        {
          "id": "MDRW2100042928.1.1",
          "form": "비가 계속 오네요 ㅠㅠ",
          "original_form": "비가 계속 오네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100042928.1.2",
          "form": "그러게요 ㅠㅠ",
          "original_form": "그러게요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100042928.1.3",
          "form": "오늘 출근 하셧어요?",
          "original_form": "오늘 출근 하셧어요?",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100042928.1.4",
          "form": "아니영!!",
          "original_form": "아니영!!",
          "speaker_id": "2",
          "time": "20210907 16:13"
        },
        {
          "id": "MDRW2100042928.1.5",
          "form": "오오 저는 밖에 잠깐 나갓는데",
          "original_form": "오오 저는 밖에 잠깐 나갓는데",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100042928.1.6",
          "form": "비가 엄청 오드라구요 ㅠ",
          "original_form": "비가 엄청 오드라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100042928.1.7",
          "form": "여기는그래도",
          "original_form": "여기는그래도",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100042928.1.8",
          "form": "소나기 정도여서갠차나요 ㅠㅠ",
          "original_form": "소나기 정도여서갠차나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100042928.1.9",
          "form": "날씨가 완전 우중충하네여 ㅠㅠ",
          "original_form": "날씨가 완전 우중충하네여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100042928.1.10",
          "form": "헐랭 비진짜싫어요",
          "original_form": "헐랭 비진짜싫어요",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100042928.1.11",
          "form": "그쵸!!! 우산 진짜 필수템이에요 ㅋㅋ",
          "original_form": "그쵸!!! 우산 진짜 필수템이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100042928.1.12",
          "form": "저도 그래서",
          "original_form": "저도 그래서",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100042928.1.13",
          "form": "작은우산들고다녀용",
          "original_form": "작은우산들고다녀용",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100042928.1.14",
          "form": "비상용으로 들고 다녀야겟어요 ㅋㅋ",
          "original_form": "비상용으로 들고 다녀야겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100042928.1.15",
          "form": "내일도 비가 오겟죠 ?",
          "original_form": "내일도 비가 오겟죠 ?",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100042928.1.16",
          "form": "안대요 ㅠㅠ",
          "original_form": "안대요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100042928.1.17",
          "form": "비가 멈추면 좋을텐데요 ㅠ",
          "original_form": "비가 멈추면 좋을텐데요 ㅠ",
