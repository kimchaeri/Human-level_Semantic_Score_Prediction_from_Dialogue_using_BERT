      "utterance": [
        {
          "id": "MDRW2100025975.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025975.1.2",
          "form": "즐겨보는 프로그램 있으신가요?",
          "original_form": "즐겨보는 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025975.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025975.1.4",
          "form": "넷플릭스에서 보던 게 있었는데",
          "original_form": "넷플릭스에서 보던 게 있었는데",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025975.1.5",
          "form": "시즌이 끝나고 다음 시즌이 아직 안올라왔네요",
          "original_form": "시즌이 끝나고 다음 시즌이 아직 안올라왔네요",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025975.1.6",
          "form": "그쪽은 즐겨보는 게 있으신가요?",
          "original_form": "그쪽은 즐겨보는 게 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025975.1.7",
          "form": "전 심심할 때는 트위치 켜놓고",
          "original_form": "전 심심할 때는 트위치 켜놓고",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025975.1.8",
          "form": "게임 스트리머들 방송을 보기도 해요",
          "original_form": "게임 스트리머들 방송을 보기도 해요",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025975.1.9",
          "form": "네네ㅋ",
          "original_form": "네네ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025975.1.10",
          "form": "저는 넷플릭스보다는",
          "original_form": "저는 넷플릭스보다는",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025975.1.11",
          "form": "티비를 자주봐요ㅋ",
          "original_form": "티비를 자주봐요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025975.1.12",
          "form": "저랑은 반대네요ㅋㅋ",
          "original_form": "저랑은 반대네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025975.1.13",
          "form": "아 여러가지 보시네요",
          "original_form": "아 여러가지 보시네요",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025975.1.14",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025975.1.15",
          "form": "넷플릭스로는 영화 한번씩봐요ㅋ",
          "original_form": "넷플릭스로는 영화 한번씩봐요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025975.1.16",
          "form": "티비에서 요즘 재밌는 거 뭐 있나요?",
          "original_form": "티비에서 요즘 재밌는 거 뭐 있나요?",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025975.1.17",
          "form": "가끔씩 영화 보면 재밌죠ㅎㅎ",
          "original_form": "가끔씩 영화 보면 재밌죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.18",
          "form": "요즘에는 슬기로운 의사생활이 재미있어요",
          "original_form": "요즘에는 슬기로운 의사생활이 재미있어요",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.19",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.20",
          "form": "펜트하우스3랑요ㅋ",
          "original_form": "펜트하우스3랑요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.21",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.22",
          "form": "맞아요 슬생은 어머니가 좋아하셔서",
          "original_form": "맞아요 슬생은 어머니가 좋아하셔서",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.23",
          "form": "저도 가끔 옆에서 얻어보기도 해요",
          "original_form": "저도 가끔 옆에서 얻어보기도 해요",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.24",
          "form": "확실히 재밌더라고요",
          "original_form": "확실히 재밌더라고요",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025975.1.25",
          "form": "배우들 연기도 다 괜찮고",
          "original_form": "배우들 연기도 다 괜찮고",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025975.1.26",
          "form": "네 맞아요. 저도 볼때",
          "original_form": "네 맞아요. 저도 볼때",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025975.1.27",
          "form": "잘 만든 드라마같아요",
          "original_form": "잘 만든 드라마같아요",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025975.1.28",
          "form": "딸도 같이봐요ㅋ",
          "original_form": "딸도 같이봐요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025975.1.29",
          "form": "그런것같아요.ㅋㅋ",
          "original_form": "그런것같아요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025975.1.30",
          "form": "따님이랑 같이 보시는구나ㅎㅎ",
          "original_form": "따님이랑 같이 보시는구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:34"
        }
      ]
