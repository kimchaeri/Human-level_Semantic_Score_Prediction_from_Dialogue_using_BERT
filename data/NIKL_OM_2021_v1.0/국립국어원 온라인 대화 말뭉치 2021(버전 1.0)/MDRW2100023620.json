      "utterance": [
        {
          "id": "MDRW2100023620.1.1",
          "form": "진짜??모지",
          "original_form": "진짜??모지",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100023620.1.2",
          "form": "ㅠㅠ슬프군요....글쓰긴 귀찮고 ㅋㅋㅋ",
          "original_form": "ㅠㅠ슬프군요....글쓰긴 귀찮고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023620.1.3",
          "form": "어디게시판이못보는건가",
          "original_form": "어디게시판이못보는건가",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100023620.1.4",
          "form": "초수ㅏㅣ아냐?",
          "original_form": "초수ㅏㅣ아냐?",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100023620.1.5",
          "form": "입문이여 ㅋㅋㅋ",
          "original_form": "입문이여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023620.1.6",
          "form": "글 하나도 안씀..",
          "original_form": "글 하나도 안씀..",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023620.1.7",
          "form": "초수는 댓글만쓰면되네 ㅋㅋㅋ채워봐 ㅋㅋㅋ",
          "original_form": "초수는 댓글만쓰면되네 ㅋㅋㅋ채워봐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023620.1.8",
          "form": "ㅋㅋ댓글 쓸 수 없던데...뭐지..",
          "original_form": "ㅋㅋ댓글 쓸 수 없던데...뭐지..",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100023620.1.9",
          "form": "모지 ㅋㅋㅋ댓글 7개인가 쓰면자동이랴ㅠ",
          "original_form": "모지 ㅋㅋㅋ댓글 7개인가 쓰면자동이랴ㅠ",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023620.1.10",
          "form": "가입수랑 방문은 될테니ㅠㅠ",
          "original_form": "가입수랑 방문은 될테니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023620.1.11",
          "form": "ㅋㅋㅋ가능한 게시판이 따로있나보다...",
          "original_form": "ㅋㅋㅋ가능한 게시판이 따로있나보다...",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023620.1.12",
          "form": "원래보던데 못보는건가 모징",
          "original_form": "원래보던데 못보는건가 모징",
          "speaker_id": "2",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023620.1.13",
          "form": "응 원래는 보였음 ㅠㅠ",
          "original_form": "응 원래는 보였음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023620.1.14",
          "form": "글읽기 등급을 높엿나;;",
          "original_form": "글읽기 등급을 높엿나;;",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023620.1.15",
          "form": "흑흑 속상하구먼 ㅠㅠ 귀찮아지고",
          "original_form": "흑흑 속상하구먼 ㅠㅠ 귀찮아지고",
          "speaker_id": "1",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023620.1.16",
          "form": "ㅋㅋㅋ댓글만 7개써ㅠㅠ",
          "original_form": "ㅋㅋㅋ댓글만 7개써ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023620.1.17",
          "form": "그럼자동 입문되네ㅠㅠ",
          "original_form": "그럼자동 입문되네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100023620.1.18",
          "form": "ㅋㅋ흠ㅋㅋ 암튼 난 이제 씻구와야겠다...ㅠㅠ등업하면 말해드리겠소 ㅋㅋㅋ",
          "original_form": "ㅋㅋ흠ㅋㅋ 암튼 난 이제 씻구와야겠다...ㅠㅠ등업하면 말해드리겠소 ㅋㅋㅋ",
