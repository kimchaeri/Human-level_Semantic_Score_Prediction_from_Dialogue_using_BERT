      "utterance": [
        {
          "id": "MDRW2100017154.1.1",
          "form": "큰 실수에요 와인은 ㅎㅎ",
          "original_form": "큰 실수에요 와인은 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.2",
          "form": "승무원들은 보통 어디서 쉬나요",
          "original_form": "승무원들은 보통 어디서 쉬나요",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.3",
          "form": "컴플레인 레터 스트레스가 많다보니까",
          "original_form": "컴플레인 레터 스트레스가 많다보니까",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.4",
          "form": "갤리라고",
          "original_form": "갤리라고",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.5",
          "form": "제가 비행기를 타본적이 없어서",
          "original_form": "제가 비행기를 타본적이 없어서",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.6",
          "form": "갤리?",
          "original_form": "갤리?",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.7",
          "form": "부엌이 있어요",
          "original_form": "부엌이 있어요",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.8",
          "form": "아아 그러시구나",
          "original_form": "아아 그러시구나",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.9",
          "form": "공간이 따로있구나",
          "original_form": "공간이 따로있구나",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017154.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.11",
          "form": "주방같은 곳인데 거기서 쉬고",
          "original_form": "주방같은 곳인데 거기서 쉬고",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.12",
          "form": "아니면 위에 벙커라고",
          "original_form": "아니면 위에 벙커라고",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.13",
          "form": "침실 공간이 있어요",
          "original_form": "침실 공간이 있어요",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.14",
          "form": "근데 10시간 이상?",
          "original_form": "근데 10시간 이상?",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.15",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.16",
          "form": "아니면 야간 비행?",
          "original_form": "아니면 야간 비행?",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.17",
          "form": "이 두가지가 아니고서는",
          "original_form": "이 두가지가 아니고서는",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.18",
          "form": "밥 먹을 시간도 없다고 보시면 돼요 ㅎㅎ",
          "original_form": "밥 먹을 시간도 없다고 보시면 돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.19",
          "form": "아.. 늘 긴장상태셨겠어요",
          "original_form": "아.. 늘 긴장상태셨겠어요",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017154.1.20",
          "form": "밥 먹을 시간도 없거나",
          "original_form": "밥 먹을 시간도 없거나",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.21",
          "form": "아니면 지루할 정도로 시간이 남거나?",
          "original_form": "아니면 지루할 정도로 시간이 남거나?",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.22",
          "form": "둘 중 하나에요",
          "original_form": "둘 중 하나에요",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.23",
          "form": "극과 극이네요",
          "original_form": "극과 극이네요",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.24",
          "form": "두번째 경우는",
          "original_form": "두번째 경우는",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.25",
          "form": "정말 가끔",
          "original_form": "정말 가끔",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.26",
          "form": "스트레스 많이 받을 거 같아요",
          "original_form": "스트레스 많이 받을 거 같아요",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.27",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.28",
          "form": "비행기가 텅텅 빌 때가 있거든요",
          "original_form": "비행기가 텅텅 빌 때가 있거든요",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.29",
          "form": "그런 때도 있어요?",
          "original_form": "그런 때도 있어요?",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.30",
          "form": "300명 타야되는데 승객 18명!",
          "original_form": "300명 타야되는데 승객 18명!",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.31",
          "form": "거기에 야간비행!",
          "original_form": "거기에 야간비행!",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017154.1.32",
          "form": "그러면 할 게 진짜 없죠 ㅎㅎ",
          "original_form": "그러면 할 게 진짜 없죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017154.1.33",
          "form": "없어서 괴로울 정도로....",
          "original_form": "없어서 괴로울 정도로....",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017154.1.34",
          "form": "와..영화관에 혼자 앉아있는 것 만큼 텅텅이네요",
          "original_form": "와..영화관에 혼자 앉아있는 것 만큼 텅텅이네요",
          "speaker_id": "1",
          "time": "20210826 17:43"
        }
      ]
