      "utterance": [
        {
          "id": "MDRW2100015345.1.1",
          "form": "나도 몰랐어",
          "original_form": "나도 몰랐어",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015345.1.2",
          "form": "지금도 그렇게 좋아하진 않잖아",
          "original_form": "지금도 그렇게 좋아하진 않잖아",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015345.1.3",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015345.1.4",
          "form": "너무 슬프다",
          "original_form": "너무 슬프다",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015345.1.5",
          "form": "별로 슬프지 않는데?ㅋㅋ",
          "original_form": "별로 슬프지 않는데?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015345.1.6",
          "form": "아니 세상 사람들이 무화과의 맛있음을 알아주지 않는 것이 슬퍼",
          "original_form": "아니 세상 사람들이 무화과의 맛있음을 알아주지 않는 것이 슬퍼",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015345.1.7",
          "form": "광고 같은거 안하나?",
          "original_form": "광고 같은거 안하나?",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015345.1.8",
          "form": "안 먹는 사람은 아예 먹을 생각을 안 하더라고",
          "original_form": "안 먹는 사람은 아예 먹을 생각을 안 하더라고",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015345.1.9",
          "form": "특이해서 그런가",
          "original_form": "특이해서 그런가",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015345.1.10",
          "form": "아무래도 그렇지 과일인데 상큼한 계열은 아니니까",
          "original_form": "아무래도 그렇지 과일인데 상큼한 계열은 아니니까",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015345.1.11",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015345.1.12",
          "form": "근데 나는 너무 맛있던데",
          "original_form": "근데 나는 너무 맛있던데",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015345.1.13",
          "form": "취향 차이",
          "original_form": "취향 차이",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015345.1.14",
          "form": "그치 사람마다 취향이 다르니까",
