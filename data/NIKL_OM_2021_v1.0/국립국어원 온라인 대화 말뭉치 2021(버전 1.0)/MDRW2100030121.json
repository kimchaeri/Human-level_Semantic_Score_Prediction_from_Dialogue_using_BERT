      "utterance": [
        {
          "id": "MDRW2100030121.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100030121.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100030121.1.3",
          "form": "동호회 하시나여",
          "original_form": "동호회 하시나여",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100030121.1.4",
          "form": "저는 봉사활동 중학교 때 외에는 안해봐서 대화하기 어려울텐데 괜찮으시겠어요!!",
          "original_form": "저는 봉사활동 중학교 때 외에는 안해봐서 대화하기 어려울텐데 괜찮으시겠어요!!",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100030121.1.5",
          "form": "동호회는 진짜 안해봤어요..",
          "original_form": "동호회는 진짜 안해봤어요..",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100030121.1.6",
          "form": "전 회사 등산동호회에요",
          "original_form": "전 회사 등산동호회에요",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100030121.1.7",
          "form": "봉사 어떤거 하셨어요",
          "original_form": "봉사 어떤거 하셨어요",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100030121.1.8",
          "form": "헉... 혹시 연령대가 어떻게 되시는지 여쭤봐도 될까요...",
          "original_form": "헉... 혹시 연령대가 어떻게 되시는지 여쭤봐도 될까요...",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.9",
          "form": "저는 등산은ㅋㅋㅋ 힘들어서...",
          "original_form": "저는 등산은ㅋㅋㅋ 힘들어서...",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.10",
          "form": "전 30살 입니다",
          "original_form": "전 30살 입니다",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.11",
          "form": "저도요 코로나땜에 가입하고",
          "original_form": "저도요 코로나땜에 가입하고",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.12",
          "form": "봉사활동은 그 어르신들 도시락 나눔이요!",
          "original_form": "봉사활동은 그 어르신들 도시락 나눔이요!",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.13",
          "form": "한번도 못갔오요 ㅋㅋ",
          "original_form": "한번도 못갔오요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.14",
          "form": "와 도시락 나눔",
          "original_form": "와 도시락 나눔",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.15",
          "form": "와 저는 21살인데",
          "original_form": "와 저는 21살인데",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.16",
          "form": "뜻깊으셨겠다",
          "original_form": "뜻깊으셨겠다",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.17",
          "form": "대학생이에요",
          "original_form": "대학생이에요",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.18",
          "form": "대학교 동아리는 안하시나용",
          "original_form": "대학교 동아리는 안하시나용",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100030121.1.19",
          "form": "강제참여였어요ㅋㄱㅋㅋㅋㅋ",
