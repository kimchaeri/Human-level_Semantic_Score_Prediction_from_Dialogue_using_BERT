        },
        {
          "id": "MDRW2100029844.1.2",
          "form": "싱크홀 재밌나요ㅋㅋ",
          "original_form": "싱크홀 재밌나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100029844.1.3",
          "form": "이광수가 예능처럼 웃겨요 ㅋㅋ",
          "original_form": "이광수가 예능처럼 웃겨요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029844.1.4",
          "form": "광수는 희극인이죠 사실?",
          "original_form": "광수는 희극인이죠 사실?",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029844.1.5",
          "form": "유재석 후배 맞는듯요",
          "original_form": "유재석 후배 맞는듯요",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100029844.1.6",
          "form": "희극인 회비 내야합니다",
          "original_form": "희극인 회비 내야합니다",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029844.1.7",
          "form": "저는 내일 맨인더다크2 볼려구요",
          "original_form": "저는 내일 맨인더다크2 볼려구요",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029844.1.8",
          "form": "ㅋㅋ 오 그거 재밌대요",
          "original_form": "ㅋㅋ 오 그거 재밌대요",
          "speaker_id": "2",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100029844.1.9",
          "form": "개무섭다는데",
          "original_form": "개무섭다는데",
          "speaker_id": "2",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100029844.1.10",
          "form": "1을 예전에봐서 기억이 잘안나는데 잼쓸거같아요 ㅎㅎ",
          "original_form": "1을 예전에봐서 기억이 잘안나는데 잼쓸거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:00"
        },
        {
          "id": "MDRW2100029844.1.11",
          "form": "네 무서운데 재밌다고",
          "original_form": "네 무서운데 재밌다고",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100029844.1.12",
          "form": "저는 쫄본데 무서운거 보는거 좋아해요",
          "original_form": "저는 쫄본데 무서운거 보는거 좋아해요",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100029844.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ겁쟁이십니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ겁쟁이십니까",
