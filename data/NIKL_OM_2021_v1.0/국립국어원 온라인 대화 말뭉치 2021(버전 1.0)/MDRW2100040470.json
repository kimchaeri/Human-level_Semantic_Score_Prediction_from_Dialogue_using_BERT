      "utterance": [
        {
          "id": "MDRW2100040470.1.1",
          "form": "전에 스피닝 좀 했어요",
          "original_form": "전에 스피닝 좀 했어요",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100040470.1.2",
          "form": "스피닝 힘들다고 들었어요",
          "original_form": "스피닝 힘들다고 들었어요",
          "speaker_id": "1",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100040470.1.3",
          "form": "할만 했나요??",
          "original_form": "할만 했나요??",
          "speaker_id": "1",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100040470.1.4",
          "form": "음청 힘들어요. 끝나고나면 밥맛이 없어질정도로",
          "original_form": "음청 힘들어요. 끝나고나면 밥맛이 없어질정도로",
          "speaker_id": "2",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100040470.1.5",
          "form": "그러게요.. 안해봤는데 힘드다고 들었어요",
          "original_form": "그러게요.. 안해봤는데 힘드다고 들었어요",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040470.1.6",
          "form": "근데 운동은 확실히 되겠어요",
          "original_form": "근데 운동은 확실히 되겠어요",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040470.1.7",
          "form": "근육은 쫌 붙은 듯",
          "original_form": "근육은 쫌 붙은 듯",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040470.1.8",
          "form": "근데 땀은 엄청나는데. 살은 안빠지더라구요",
          "original_form": "근데 땀은 엄청나는데. 살은 안빠지더라구요",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040470.1.9",
          "form": "그당시는요",
          "original_form": "그당시는요",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040470.1.10",
          "form": "근육이 붙으면 그래도 좋은 효과네요",
          "original_form": "근육이 붙으면 그래도 좋은 효과네요",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100040470.1.11",
          "form": "저는 수영했는데 근육은 전혀 안붙더라구요",
          "original_form": "저는 수영했는데 근육은 전혀 안붙더라구요",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100040470.1.12",
          "form": "수영하고나면 배음청고프지않나요?",
          "original_form": "수영하고나면 배음청고프지않나요?",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040470.1.13",
          "form": "수영끝나고 맨날 라면먹어서 벌크업했다능",
          "original_form": "수영끝나고 맨날 라면먹어서 벌크업했다능",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040470.1.14",
          "form": "배고프죠",
          "original_form": "배고프죠",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040470.1.15",
          "form": "근데 모든 운동이 하고 나면 배고프지 않나요?",
          "original_form": "근데 모든 운동이 하고 나면 배고프지 않나요?",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040470.1.16",
          "form": "그걸 이겨내야 살을 뺄 수 있죠",
          "original_form": "그걸 이겨내야 살을 뺄 수 있죠",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040470.1.17",
          "form": "너무격하면 입맛도 사라지더라구욬ㅋ",
          "original_form": "너무격하면 입맛도 사라지더라구욬ㅋ",
          "speaker_id": "2",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100040470.1.18",
          "form": "아 그정도로 격하게 안해봐서 몰랐네요",
          "original_form": "아 그정도로 격하게 안해봐서 몰랐네요",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100040470.1.19",
          "form": "ㅋㅋ. 그 뒤론 그런 경험은 없지만요",
          "original_form": "ㅋㅋ. 그 뒤론 그런 경험은 없지만요",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100040470.1.20",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100040470.1.21",
          "form": "전 헬스장도 다녔었어요",
          "original_form": "전 헬스장도 다녔었어요",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100040470.1.22",
          "form": "근육 붙이려구요",
          "original_form": "근육 붙이려구요",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100040470.1.23",
          "form": "그런데 역시 여자라 그런지 잘 안붙더라구요",
          "original_form": "그런데 역시 여자라 그런지 잘 안붙더라구요",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100040470.1.24",
          "form": "헬스장까지 가는게 고난",
          "original_form": "헬스장까지 가는게 고난",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100040470.1.25",
          "form": "가면 어떻게든 운동을 하는데",
          "original_form": "가면 어떻게든 운동을 하는데",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100040470.1.26",
          "form": "저는 매일매일 가는거 잘하는 편이었어요\r\nㅎㅎㅎ",
          "original_form": "저는 매일매일 가는거 잘하는 편이었어요\r\nㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100040470.1.27",
          "form": "개근상",
          "original_form": "개근상",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100040470.1.28",
          "form": "돈 내놓으면 돈아까워서라도 가는 편이지요",
          "original_form": "돈 내놓으면 돈아까워서라도 가는 편이지요",
          "speaker_id": "1",
          "time": "20210905 17:35"
        }
      ]
