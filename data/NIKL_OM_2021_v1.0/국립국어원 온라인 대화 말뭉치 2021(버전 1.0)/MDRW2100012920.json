      "utterance": [
        {
          "id": "MDRW2100012920.1.1",
          "form": "우리 신랑은 오늘도 병원 간다고~",
          "original_form": "우리 신랑은 오늘도 병원 간다고~",
          "speaker_id": "2",
          "time": "20210825 08:39"
        },
        {
          "id": "MDRW2100012920.1.2",
          "form": "누릉지 끓여서",
          "original_form": "누릉지 끓여서",
          "speaker_id": "2",
          "time": "20210825 08:39"
        },
        {
          "id": "MDRW2100012920.1.3",
          "form": "된찌ㅋㅋㅋㅋㅋ 젱편하다 마 ㅋㅋㅋㅋ",
          "original_form": "된찌ㅋㅋㅋㅋㅋ 젱편하다 마 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:39"
        },
        {
          "id": "MDRW2100012920.1.4",
          "form": "첫찌는 어제 늦게자디 일어나지를 못하고.",
          "original_form": "첫찌는 어제 늦게자디 일어나지를 못하고.",
          "speaker_id": "2",
          "time": "20210825 08:39"
        },
        {
          "id": "MDRW2100012920.1.5",
          "form": "ㅋㅋㅋ 좋지좋지",
          "original_form": "ㅋㅋㅋ 좋지좋지",
          "speaker_id": "1",
          "time": "20210825 08:39"
        },
        {
          "id": "MDRW2100012920.1.6",
          "form": "내 혼자 밥 묵어야 긋네 ㅋㅋㅋ",
          "original_form": "내 혼자 밥 묵어야 긋네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012920.1.7",
          "form": "신랑도 아침먹이고 보내나?",
          "original_form": "신랑도 아침먹이고 보내나?",
          "speaker_id": "1",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012920.1.8",
          "form": "안먹는데 약먹어야되서 ㅋㅋ",
          "original_form": "안먹는데 약먹어야되서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012920.1.9",
          "form": "우린 첫찌빼고는 아침 잘 안먹음 ㅋㅋ",
          "original_form": "우린 첫찌빼고는 아침 잘 안먹음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012920.1.10",
          "form": "난 맛있는거 있을때만 먹는닼ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "난 맛있는거 있을때만 먹는닼ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012920.1.11",
          "form": "응응 약먹어야되서ㅋㅋ",
          "original_form": "응응 약먹어야되서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012920.1.12",
          "form": "아 웃겨 맛있는거 있을때만ㅋㅋ",
          "original_form": "아 웃겨 맛있는거 있을때만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012920.1.13",
          "form": "나는 자주 아침먹는데 ㅋㅋ",
          "original_form": "나는 자주 아침먹는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.14",
          "form": "맛있는거있음 내혼자 새벽에라도 먹음 ㅋㅋㅋㅋ",
          "original_form": "맛있는거있음 내혼자 새벽에라도 먹음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.15",
          "form": "고민이네 아침부터 뭘먹을지ㅋ기",
          "original_form": "고민이네 아침부터 뭘먹을지ㅋ기",
          "speaker_id": "1",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.16",
          "form": "그렇지 아 대단하네ㅋㅋ",
          "original_form": "그렇지 아 대단하네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ둘찌갔어??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ둘찌갔어??",
          "speaker_id": "2",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.18",
          "form": "같이 먹는겨??",
          "original_form": "같이 먹는겨??",
          "speaker_id": "2",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.19",
          "form": "아니 아직안갔다",
          "original_form": "아니 아직안갔다",
          "speaker_id": "1",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.20",
          "form": "같이 먹어야지ㅋㅋ",
          "original_form": "같이 먹어야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:41"
        },
        {
          "id": "MDRW2100012920.1.21",
          "form": "밥도했고ㅋ",
          "original_form": "밥도했고ㅋ",
          "speaker_id": "1",
          "time": "20210825 08:42"
        },
        {
          "id": "MDRW2100012920.1.22",
          "form": "아침은 미역국 아니가??ㅋㅋㅋㅋ",
          "original_form": "아침은 미역국 아니가??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:42"
        },
        {
          "id": "MDRW2100012920.1.23",
          "form": "미역국 없다ㅋㅋ일단 두부야채전으로 간단하게 먹고",
          "original_form": "미역국 없다ㅋㅋ일단 두부야채전으로 간단하게 먹고",
          "speaker_id": "1",
          "time": "20210825 08:42"
        },
        {
          "id": "MDRW2100012920.1.24",
          "form": "점심은 잡채밥으로 생각하고있음ㅋㅋㅋ",
          "original_form": "점심은 잡채밥으로 생각하고있음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:42"
        },
        {
          "id": "MDRW2100012920.1.25",
          "form": "이야~코스 요리구만^^",
          "original_form": "이야~코스 요리구만^^",
          "speaker_id": "2",
          "time": "20210825 08:43"
        },
        {
          "id": "MDRW2100012920.1.26",
          "form": "간단한데 젱 맛있다이~",
          "original_form": "간단한데 젱 맛있다이~",
          "speaker_id": "2",
          "time": "20210825 08:43"
        },
        {
          "id": "MDRW2100012920.1.27",
          "form": "그렇지 ㅋㅋ",
          "original_form": "그렇지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:43"
        },
        {
          "id": "MDRW2100012920.1.28",
          "form": "미역국도 하도 줘가 이제 잘 안묵음 ㅋㅋㅋㅋ",
