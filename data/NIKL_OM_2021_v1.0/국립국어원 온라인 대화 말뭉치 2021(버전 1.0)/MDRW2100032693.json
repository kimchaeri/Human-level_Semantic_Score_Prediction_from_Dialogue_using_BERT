      "utterance": [
        {
          "id": "MDRW2100032693.1.1",
          "form": "ㅋㅋㅋ노...나는 잇어 그거",
          "original_form": "ㅋㅋㅋ노...나는 잇어 그거",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032693.1.2",
          "form": "프로틴..",
          "original_form": "프로틴..",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032693.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032693.1.4",
          "form": "프로틴 삿다구 햇었나?",
          "original_form": "프로틴 삿다구 햇었나?",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032693.1.5",
          "form": "아 마자 라떼맛난다고 ㅋㅋㅋㅋㅋ",
          "original_form": "아 마자 라떼맛난다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032693.1.6",
          "form": "잘 먹고잇나 ㅋㅋㅋㅋ",
          "original_form": "잘 먹고잇나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032693.1.7",
          "form": "ㅋㅋㅋㅋ아직은 소소하다 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아직은 소소하다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032693.1.8",
          "form": "말차라떼랑 초코는 언제 먹을 수 잇을지...",
          "original_form": "말차라떼랑 초코는 언제 먹을 수 잇을지...",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032693.1.9",
          "form": "오 그맛도 같이산거?",
          "original_form": "오 그맛도 같이산거?",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032693.1.10",
          "form": "어엉ㅋㅋㅋ맛 3개 샀어여",
          "original_form": "어엉ㅋㅋㅋ맛 3개 샀어여",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032693.1.11",
          "form": "다양성 추구",
          "original_form": "다양성 추구",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032693.1.12",
          "form": "오 하나 클리어하고 다음거 먹을겨?",
          "original_form": "오 하나 클리어하고 다음거 먹을겨?",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032693.1.13",
          "form": "클리어하려면 매일먹어도...빡셀거 같은데 ㅋㅋㅋㅋ",
          "original_form": "클리어하려면 매일먹어도...빡셀거 같은데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032693.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ양이 얼마나돼",
          "original_form": "ㅋㅋㅋㅋㅋㅋ양이 얼마나돼",
          "speaker_id": "1",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.15",
          "form": "아이스라떼맛이 젤 커가지구..",
          "original_form": "아이스라떼맛이 젤 커가지구..",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.16",
          "form": "2.5킬로여",
          "original_form": "2.5킬로여",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.17",
          "form": "...네..?",
          "original_form": "...네..?",
          "speaker_id": "1",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ엄청많니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ엄청많니",
          "speaker_id": "1",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.19",
          "form": "예...장난아니예요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "예...장난아니예요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.20",
          "form": "한번에 몇그람타먹엌ㅋㅋㅋ",
          "original_form": "한번에 몇그람타먹엌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.21",
          "form": "100번쯤 먹어야할걸ㅋㅋㅋㅋ",
          "original_form": "100번쯤 먹어야할걸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032693.1.22",
          "form": "거기 든 스푼 1숟가락이요...",
          "original_form": "거기 든 스푼 1숟가락이요...",
          "speaker_id": "2",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032693.1.23",
          "form": "단백질걱정은없겟다..",
          "original_form": "단백질걱정은없겟다..",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032693.1.24",
          "form": "? 나눠드릴까요?",
          "original_form": "? 나눠드릴까요?",
          "speaker_id": "2",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032693.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032693.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ나중에 그럼 쪼오금만..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ나중에 그럼 쪼오금만..",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032693.1.27",
          "form": "맛이궁금하긴하닼ㅋㅋㅋㅋ",
          "original_form": "맛이궁금하긴하닼ㅋㅋㅋㅋ",
          "speaker_id": "1",
