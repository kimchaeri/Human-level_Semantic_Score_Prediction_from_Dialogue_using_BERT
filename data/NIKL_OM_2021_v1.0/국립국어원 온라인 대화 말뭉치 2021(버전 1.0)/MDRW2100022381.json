      "utterance": [
        {
          "id": "MDRW2100022381.1.1",
          "form": "근데 중국 김치 갖다 쓰는 식당은 다 배쪽이라서",
          "original_form": "근데 중국 김치 갖다 쓰는 식당은 다 배쪽이라서",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022381.1.2",
          "form": "저에겐 좋아요",
          "original_form": "저에겐 좋아요",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022381.1.3",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022381.1.4",
          "form": "중국김치에요?",
          "original_form": "중국김치에요?",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022381.1.5",
          "form": "네네 제가 자주가는 식당은 그래요",
          "original_form": "네네 제가 자주가는 식당은 그래요",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022381.1.6",
          "form": "몰랐던 사실이네요",
          "original_form": "몰랐던 사실이네요",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022381.1.7",
          "form": "보통 이파리는 찌개에넣던데",
          "original_form": "보통 이파리는 찌개에넣던데",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.8",
          "form": "헉! 그러면 이파리로는 찌개를 만들고",
          "original_form": "헉! 그러면 이파리로는 찌개를 만들고",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.9",
          "form": "그래서 그런거아닐까요.. (사실잘모름)",
          "original_form": "그래서 그런거아닐까요.. (사실잘모름)",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.10",
          "form": "배쪽으로는 반찬으로 내놓는건가봐요",
          "original_form": "배쪽으로는 반찬으로 내놓는건가봐요",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.11",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.12",
          "form": "오 ㅋㅋ 대박 그럴듯한 추리인데요?",
          "original_form": "오 ㅋㅋ 대박 그럴듯한 추리인데요?",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.13",
          "form": "파김치같은건 드세요?",
          "original_form": "파김치같은건 드세요?",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.14",
          "form": "파김치 있으면 먹어요 ㅎㅎ 안먹은지 좀 됐네요 ㅠ",
          "original_form": "파김치 있으면 먹어요 ㅎㅎ 안먹은지 좀 됐네요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.15",
          "form": "파김치에 삼겹살 최고죠 ㅠㅠ",
          "original_form": "파김치에 삼겹살 최고죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.16",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.17",
          "form": "파김치삼겹살 최고",
          "original_form": "파김치삼겹살 최고",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022381.1.18",
          "form": "미나리에 삼겹살도 진짜 최고인데",
          "original_form": "미나리에 삼겹살도 진짜 최고인데",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022381.1.19",
          "form": "아직 아는 사람 잘 없더라구요",
          "original_form": "아직 아는 사람 잘 없더라구요",
          "speaker_id": "2",
          "time": "20210829 13:19"
        }
      ]
