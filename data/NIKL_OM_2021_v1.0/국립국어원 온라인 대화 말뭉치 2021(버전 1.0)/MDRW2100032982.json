        {
          "id": "MDRW2100032982.1.1",
          "form": "요즘 군대는 예전 같지 않더라구요",
          "original_form": "요즘 군대는 예전 같지 않더라구요",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100032982.1.2",
          "form": "월급도 많아졌지요",
          "original_form": "월급도 많아졌지요",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100032982.1.3",
          "form": "권 캠프락 왔던데요",
          "original_form": "권 캠프락 왔던데요",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100032982.1.4",
          "form": "카카오톡도 군대에서 사용가능해요",
          "original_form": "카카오톡도 군대에서 사용가능해요",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100032982.1.5",
          "form": "군대에서 PC 방도 이용해요",
          "original_form": "군대에서 PC 방도 이용해요",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.6",
          "form": "심리치료도 해주고요",
          "original_form": "심리치료도 해주고요",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.7",
          "form": "월급이 진짜 많아진게 대단해요",
          "original_form": "월급이 진짜 많아진게 대단해요",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.8",
          "form": "돈을 벌어서 나올수도 있겠어요",
          "original_form": "돈을 벌어서 나올수도 있겠어요",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.9",
          "form": "담배도 필 수 있게 해 주고요",
          "original_form": "담배도 필 수 있게 해 주고요",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.10",
          "form": "주식도 할수있겠던데요",
          "original_form": "주식도 할수있겠던데요",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.11",
          "form": "시간도 엄청 짧아 졌죠",
          "original_form": "시간도 엄청 짧아 졌죠",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.12",
          "form": "예전엔 2년 넘었는데 이젠 1년 반이지요",
          "original_form": "예전엔 2년 넘었는데 이젠 1년 반이지요",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.13",
          "form": "그래도 가까운 시간이기는 해요",
          "original_form": "그래도 가까운 시간이기는 해요",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032982.1.14",
          "form": "가장 젊을때에 군대를가야하니까",
          "original_form": "가장 젊을때에 군대를가야하니까",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100032982.1.15",
          "form": "거기도 담배라고 나 나쁜 문어도 배워요",
