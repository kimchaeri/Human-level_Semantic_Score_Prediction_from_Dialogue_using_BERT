      "utterance": [
        {
          "id": "MDRW2100018619.1.1",
          "form": "된장국을 먹어야한다고",
          "original_form": "된장국을 먹어야한다고",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018619.1.2",
          "form": "집에서 우긴사람이 있어서ㅋㄲ",
          "original_form": "집에서 우긴사람이 있어서ㅋㄲ",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018619.1.3",
          "form": "배달 말고 된장국 먹었습니다",
          "original_form": "배달 말고 된장국 먹었습니다",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018619.1.4",
          "form": "음료 뭐 좋아하세요?",
          "original_form": "음료 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018619.1.5",
          "form": "저는 차 종류 좋아해요 캐모마일같은거요",
          "original_form": "저는 차 종류 좋아해요 캐모마일같은거요",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018619.1.6",
          "form": "어떤 음료 좋아하세요?",
          "original_form": "어떤 음료 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018619.1.7",
          "form": "저는 탄산 좋아해요",
          "original_form": "저는 탄산 좋아해요",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018619.1.8",
          "form": "탄산 좋죠 고기먹을때 필수",
          "original_form": "탄산 좋죠 고기먹을때 필수",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018619.1.9",
          "form": "차 종류 좋아하시는구나 그럼 버블티같은것도 좋아하시겠어요",
          "original_form": "차 종류 좋아하시는구나 그럼 버블티같은것도 좋아하시겠어요",
          "speaker_id": "1",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018619.1.10",
          "form": "맞아요 고기엔 탄산이죠",
          "original_form": "맞아요 고기엔 탄산이죠",
          "speaker_id": "1",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018619.1.11",
          "form": "그 타피오카펄보다는 알로에펄 좋아해서 자몽그린티 이런거 자주먹어요",
          "original_form": "그 타피오카펄보다는 알로에펄 좋아해서 자몽그린티 이런거 자주먹어요",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018619.1.12",
          "form": "버블티 좋아하세요?",
          "original_form": "버블티 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018619.1.13",
          "form": "자주먹지는 않는데 종종 먹는 편이에요",
          "original_form": "자주먹지는 않는데 종종 먹는 편이에요",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018619.1.14",
          "form": "그나저나 알로에 펄이면 알로에 음료에 들은 그거요?",
          "original_form": "그나저나 알로에 펄이면 알로에 음료에 들은 그거요?",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018619.1.15",
          "form": "저는 타피오카펄만 먹어봐서 알로에펄도 따로 파는 줄 몰랐네요",
          "original_form": "저는 타피오카펄만 먹어봐서 알로에펄도 따로 파는 줄 몰랐네요",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018619.1.16",
          "form": "그 공차나 아마스빈 이런데 가면 펄 변경이 되거든요!",
          "original_form": "그 공차나 아마스빈 이런데 가면 펄 변경이 되거든요!",
          "speaker_id": "2",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018619.1.17",
          "form": "알로에 음료에 든거랑 느낌 비슷해요!",
          "original_form": "알로에 음료에 든거랑 느낌 비슷해요!",
          "speaker_id": "2",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018619.1.18",
          "form": "언제 한 번 들려서 먹어봐야겠어요",
          "original_form": "언제 한 번 들려서 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018619.1.19",
          "form": "좀더 서걱서걱",
          "original_form": "좀더 서걱서걱",
          "speaker_id": "2",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018619.1.20",
          "form": "쫀득한거는 별로 선호하시지 않으시나보구나",
          "original_form": "쫀득한거는 별로 선호하시지 않으시나보구나",
          "speaker_id": "1",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018619.1.21",
          "form": "어떤 식감인지 알거같아요",
          "original_form": "어떤 식감인지 알거같아요",
          "speaker_id": "1",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018619.1.22",
          "form": "타피오카펄 들은 버블티 자주 먹진않지만 먹으면 그 타이거슈가!!!! 존맛이에요",
          "original_form": "타피오카펄 들은 버블티 자주 먹진않지만 먹으면 그 타이거슈가!!!! 존맛이에요",
          "speaker_id": "2",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018619.1.23",
          "form": "그거 꼭 먹어볼게요!!",
          "original_form": "그거 꼭 먹어볼게요!!",
          "speaker_id": "1",
          "time": "20210827 09:16"
        },
        {
          "id": "MDRW2100018619.1.24",
          "form": "흑당버블티 잘 안먹는데 거기거는 먹어요 ㅠㅠ 진짜 짱",
          "original_form": "흑당버블티 잘 안먹는데 거기거는 먹어요 ㅠㅠ 진짜 짱",
          "speaker_id": "2",
