      "utterance": [
        {
          "id": "MDRW2100045140.1.1",
          "form": "오늘 저녁에 뭐 드셨어요?",
          "original_form": "오늘 저녁에 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045140.1.2",
          "form": "비빔밥 먹었어요 ! 집에 나물반찬이 많았거든요",
          "original_form": "비빔밥 먹었어요 ! 집에 나물반찬이 많았거든요",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045140.1.3",
          "form": "헐 비빔밥 땡기네요",
          "original_form": "헐 비빔밥 땡기네요",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045140.1.4",
          "form": "계단 후라이도 하셨나요?",
          "original_form": "계단 후라이도 하셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045140.1.5",
          "form": "후라이 없는 비빔밤은 용서할 수 없어요",
          "original_form": "후라이 없는 비빔밤은 용서할 수 없어요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045140.1.6",
          "form": "맞는 말이네요",
          "original_form": "맞는 말이네요",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045140.1.7",
          "form": "그런데 오늘 제 아침은 용서할 수 없는 비빔밥이 될 예정입니다",
          "original_form": "그런데 오늘 제 아침은 용서할 수 없는 비빔밥이 될 예정입니다",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045140.1.8",
          "form": "name1님은 뭘 드셨나요 ?",
          "original_form": "&name1&님은 뭘 드셨나요 ?",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045140.1.9",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045140.1.10",
          "form": "집에 계란은 없는데 비빔밥은 먹고 싶거든요",
          "original_form": "집에 계란은 없는데 비빔밥은 먹고 싶거든요",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045140.1.11",
          "form": "당신의 도움으로 이 비빔밥을 용서할 수 있습니다 !",
          "original_form": "당신의 도움으로 이 비빔밥을 용서할 수 있습니다 !",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045140.1.12",
          "form": "불우한 이웃을 위해 천원을 기부하시겠나요?",
          "original_form": "불우한 이웃을 위해 천원을 기부하시겠나요?",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045140.1.13",
          "form": "그것은 나물밥일 뿐이에요",
          "original_form": "그것은 나물밥일 뿐이에요",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045140.1.14",
          "form": "아니다...아니다 비빔밥이다",
          "original_form": "아니다...아니다 비빔밥이다",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045140.1.15",
          "form": "아 고추장도 없는 것 같아요",
          "original_form": "아 고추장도 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045140.1.16",
          "form": "하지만 나물밥도 몸에 좋으니 그냥 드셔도 될것 같아요",
          "original_form": "하지만 나물밥도 몸에 좋으니 그냥 드셔도 될것 같아요",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045140.1.17",
          "form": "나물 밥이예요...",
          "original_form": "나물 밥이예요...",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045140.1.18",
          "form": "산의 정기를 잔뜩 받은 나물들을 먹는거잖아요",
          "original_form": "산의 정기를 잔뜩 받은 나물들을 먹는거잖아요",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045140.1.19",
          "form": "산타령 그만하세요",
          "original_form": "산타령 그만하세요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045140.1.20",
          "form": "나중에 자연인으로 초절임만 먹고 살것 같잖아요",
          "original_form": "나중에 자연인으로 초절임만 먹고 살것 같잖아요",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045140.1.21",
          "form": "힝",
          "original_form": "힝",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045140.1.22",
          "form": "제 꿈은 태백산의 터줏대감인걸요",
          "original_form": "제 꿈은 태백산의 터줏대감인걸요",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045140.1.23",
          "form": "터줏대감은 뭐 먹고 사는데요?",
          "original_form": "터줏대감은 뭐 먹고 사는데요?",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045140.1.24",
          "form": "나물이요 !",
          "original_form": "나물이요 !",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045140.1.25",
          "form": "고기가 없잖아요",
          "original_form": "고기가 없잖아요",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045140.1.26",
          "form": "고기가 보이면 먹겠죠 !",
          "original_form": "고기가 보이면 먹겠죠 !",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045140.1.27",
          "form": "하지만 치킨이 넚잖아요",
          "original_form": "하지만 치킨이 넚잖아요",
          "speaker_id": "1",
          "time": "20210908 01:07"
        }
      ]
