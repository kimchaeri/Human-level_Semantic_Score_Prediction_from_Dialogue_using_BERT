      "utterance": [
        {
          "id": "MDRW2100045780.1.1",
          "form": "길가에 좀비가 없으니 겜이 할만한",
          "original_form": "길가에 좀비가 없으니 겜이 할만한",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045780.1.2",
          "form": "이제 계속 베리 따시네 ㅋ",
          "original_form": "이제 계속 베리 따시네 ㅋ",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045780.1.3",
          "form": "비건식 시작 ㅋㅋㅋㅋ",
          "original_form": "비건식 시작 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045780.1.4",
          "form": "갑자기 산딸기 먹고싶네요",
          "original_form": "갑자기 산딸기 먹고싶네요",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045780.1.5",
          "form": "그거 벌레 많다던데 ㄷㄷ",
          "original_form": "그거 벌레 많다던데 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045780.1.6",
          "form": "게임이나 현실이나 힘든 다이어트 ㅜㅜ",
          "original_form": "게임이나 현실이나 힘든 다이어트 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045780.1.7",
          "form": "간에 기별로 안간대욬ㅋㅋ",
          "original_form": "간에 기별로 안간대욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045780.1.8",
          "form": "ㅋㅋㅋㅋㅋ 샐러드만 드시니",
          "original_form": "ㅋㅋㅋㅋㅋ 샐러드만 드시니",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045780.1.9",
          "form": "통조림은 아끼시는듯",
          "original_form": "통조림은 아끼시는듯",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045780.1.10",
          "form": "계속 살이 찌는데 ㅜㅜ",
          "original_form": "계속 살이 찌는데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045780.1.11",
          "form": "제 현실 같네요...",
          "original_form": "제 현실 같네요...",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045780.1.12",
          "form": "아니 ㅋㅋㅋ ㅠㅠㅠㅠㅠㅠ 그러기요",
          "original_form": "아니 ㅋㅋㅋ ㅠㅠㅠㅠㅠㅠ 그러기요",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045780.1.13",
          "form": "밤에 돌아가녀도 좀비가 없는",
          "original_form": "밤에 돌아가녀도 좀비가 없는",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045780.1.14",
          "form": "편안 그 자체",
          "original_form": "편안 그 자체",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045780.1.15",
          "form": "모드인가요",
          "original_form": "모드인가요",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045780.1.16",
          "form": "그러개요 왜 없죠",
          "original_form": "그러개요 왜 없죠",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045780.1.17",
          "form": "독베리 조심해야할듯",
          "original_form": "독베리 조심해야할듯",
          "speaker_id": "1",
          "time": "20210908 07:07"
        }
