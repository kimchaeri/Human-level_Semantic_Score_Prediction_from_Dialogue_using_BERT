      "utterance": [
        {
          "id": "MDRW2100001789.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100001789.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100001789.1.3",
          "form": "좋아하는 스포츠있나요?",
          "original_form": "좋아하는 스포츠있나요?",
          "speaker_id": "1",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100001789.1.4",
          "form": "저 수영 좋아해요",
          "original_form": "저 수영 좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001789.1.5",
          "form": "올림픽보면서 배구랑양궁도 넘 재밌어보이고 배우고싶더라구여",
          "original_form": "올림픽보면서 배구랑양궁도 넘 재밌어보이고 배우고싶더라구여",
          "speaker_id": "2",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001789.1.6",
          "form": "저도 올림픽보고 같은 생각했어요",
          "original_form": "저도 올림픽보고 같은 생각했어요",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001789.1.7",
          "form": "양궁이랑 배구 너무 매력있더라구요",
          "original_form": "양궁이랑 배구 너무 매력있더라구요",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001789.1.8",
          "form": "마자요 배구 직관하러가보고싶고",
          "original_form": "마자요 배구 직관하러가보고싶고",
          "speaker_id": "2",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001789.1.9",
          "form": "양강도 양궁카페가서 한본쏘보고싶고 그렇더라구여",
          "original_form": "양강도 양궁카페가서 한본쏘보고싶고 그렇더라구여",
          "speaker_id": "2",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001789.1.10",
          "form": "저도요 이번에 그 열정이 너무 좋았어요",
          "original_form": "저도요 이번에 그 열정이 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001789.1.11",
          "form": "양궁은 빨리쏘고 빨리 끝나는게 매력이고 배구는 끝까지 포기하지 않는 정신이 너무 좋더라구요",
          "original_form": "양궁은 빨리쏘고 빨리 끝나는게 매력이고 배구는 끝까지 포기하지 않는 정신이 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100001789.1.12",
          "form": "맞아요 저도 딱 그렇게 좋더라구요  특히 한일전 배구",
          "original_form": "맞아요 저도 딱 그렇게 좋더라구요  특히 한일전 배구",
          "speaker_id": "2",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100001789.1.13",
          "form": "미쳤다라는말밖에..",
          "original_form": "미쳤다라는말밖에..",
          "speaker_id": "2",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100001789.1.14",
          "form": "아.. 진짜 그 감동",
          "original_form": "아.. 진짜 그 감동",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001789.1.15",
          "form": "아직도 생생해요",
          "original_form": "아직도 생생해요",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001789.1.16",
          "form": "진짜 손모으고봤어요",
          "original_form": "진짜 손모으고봤어요",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001789.1.17",
          "form": "저도 ㅋㅋㅋㅋ무릎꿇고 손모으고",
          "original_form": "저도 ㅋㅋㅋㅋ무릎꿇고 손모으고",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001789.1.18",
          "form": "매번 올림픽때되면 저도모르는 스포츠정신이",
          "original_form": "매번 올림픽때되면 저도모르는 스포츠정신이",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001789.1.19",
          "form": "진짜 시작하기전엔 뭔 올림픽 했다가 누구보다 진심이되어버린ㅋㅋ",
          "original_form": "진짜 시작하기전엔 뭔 올림픽 했다가 누구보다 진심이되어버린ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001789.1.20",
          "form": "맞아요 아직도 선수들 구경가요...못보내...",
          "original_form": "맞아요 아직도 선수들 구경가요...못보내...",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001789.1.21",
          "form": "이맘으로 이제 스포츠하나 팔수있을거같은데 코로나가 또 걸리네요ㅜ",
          "original_form": "이맘으로 이제 스포츠하나 팔수있을거같은데 코로나가 또 걸리네요ㅜ",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001789.1.22",
          "form": "하 그래도 1년후면 베이징과 아시안..\r\n그리고 3년후 파리..",
          "original_form": "하 그래도 1년후면 베이징과 아시안..\r\n그리고 3년후 파리..",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001789.1.23",
          "form": "진짜 불붙었을 때 해야하는데 하필이면 코로나가 여러모로 아쉬워요",
          "original_form": "진짜 불붙었을 때 해야하는데 하필이면 코로나가 여러모로 아쉬워요",
          "speaker_id": "1",
          "time": "20210819 21:23"
        }
      ]
    }
