        },
        {
          "id": "MDRW2100004433.1.19",
          "form": "응응 닌텐도 좋지ㅋㅋ",
          "original_form": "응응 닌텐도 좋지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004433.1.20",
          "form": "비싸서 못사겠다 정말 ㅠㅋㅋ",
          "original_form": "비싸서 못사겠다 정말 ㅠㅋㅋ",
