      "utterance": [
        {
          "id": "MDRW2100004414.1.1",
          "form": "최근 강추템 있으신가요",
          "original_form": "최근 강추템 있으신가요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004414.1.2",
          "form": "흠...저는 최근에 산 물건이 없어서...",
          "original_form": "흠...저는 최근에 산 물건이 없어서...",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004414.1.3",
          "form": "아 그 자동 스테이플러 아시나요?",
          "original_form": "아 그 자동 스테이플러 아시나요?",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004414.1.4",
          "form": "오 그게 뭐죠?",
          "original_form": "오 그게 뭐죠?",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004414.1.5",
          "form": "넣으면 자동으로 찝어주나?",
          "original_form": "넣으면 자동으로 찝어주나?",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004414.1.6",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004414.1.7",
          "form": "그냥 갖다대면 알아서 찝어줘요",
          "original_form": "그냥 갖다대면 알아서 찝어줘요",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004414.1.8",
          "form": "엄청 편해요",
          "original_form": "엄청 편해요",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004414.1.9",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004414.1.10",
          "form": "요즘은 진짜 여러가지 물건이 많이 나오는거같아요",
          "original_form": "요즘은 진짜 여러가지 물건이 많이 나오는거같아요",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004414.1.11",
          "form": "맞아요..!",
          "original_form": "맞아요..!",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004414.1.12",
          "form": "그런 생각은 어떻게 하는건지",
          "original_form": "그런 생각은 어떻게 하는건지",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004414.1.13",
          "form": "저는 요즘 당근에 푹 빠져서",
          "original_form": "저는 요즘 당근에 푹 빠져서",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004414.1.14",
          "form": "중고거래 내역만 잔뜩이네요",
          "original_form": "중고거래 내역만 잔뜩이네요",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004414.1.15",
          "form": "사기 당하신 적은 없으세요?",
          "original_form": "사기 당하신 적은 없으세요?",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004414.1.16",
          "form": "다행히 아직은 없어요",
          "original_form": "다행히 아직은 없어요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004414.1.17",
          "form": "다행이네요!",
          "original_form": "다행이네요!",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004414.1.18",
          "form": "다 낮은 가격대의 물건들이라",
          "original_form": "다 낮은 가격대의 물건들이라",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004414.1.19",
          "form": "그럴지도 모르지만요",
          "original_form": "그럴지도 모르지만요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004414.1.20",
          "form": "그리고 저는 대면 거래를 선호해서요",
          "original_form": "그리고 저는 대면 거래를 선호해서요",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004414.1.21",
          "form": "조금 쫄려도 나가서 거래해요 꼭",
          "original_form": "조금 쫄려도 나가서 거래해요 꼭",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004414.1.22",
          "form": "저는 한 번 당해봐서..다시 당근하기가 조금 무섭더라구요..",
          "original_form": "저는 한 번 당해봐서..다시 당근하기가 조금 무섭더라구요..",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004414.1.23",
          "form": "엄마야",
          "original_form": "엄마야",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004414.1.24",
          "form": "무슨일이래요 ㅠㅠ",
          "original_form": "무슨일이래요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004414.1.25",
          "form": "큰 돈이었나요?",
          "original_form": "큰 돈이었나요?",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004414.1.26",
          "form": "저한테는 큰 돈이였지만 엄청 큰 돈은 아니였어요..",
          "original_form": "저한테는 큰 돈이였지만 엄청 큰 돈은 아니였어요..",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004414.1.27",
          "form": "안좋은 기억으로 남을 만 했네요 ㅠㅠ",
          "original_form": "안좋은 기억으로 남을 만 했네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004414.1.28",
          "form": "충격이 크셨겠어요",
          "original_form": "충격이 크셨겠어요",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004414.1.29",
          "form": "경찰서 가서 진술서 쓰고 사기꾼한테 연락해보고 조그 힘들었지만",
          "original_form": "경찰서 가서 진술서 쓰고 사기꾼한테 연락해보고 조그 힘들었지만",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004414.1.30",
          "form": "다시 돈 돌려받았어요!",
          "original_form": "다시 돈 돌려받았어요!",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004414.1.31",
          "form": "정말 다행이도..",
          "original_form": "정말 다행이도..",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004414.1.32",
          "form": "그래도 일이 잘 풀렸었네요 다행이에요",
          "original_form": "그래도 일이 잘 풀렸었네요 다행이에요",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004414.1.33",
          "form": "사기꾼들 다 잡혀가야해 ㅡㅡ",
          "original_form": "사기꾼들 다 잡혀가야해 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004414.1.34",
          "form": "맞아요 진짜...",
          "original_form": "맞아요 진짜...",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004414.1.35",
          "form": "최근 추천템이 있으신가요..?",
          "original_form": "최근 추천템이 있으신가요..?",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004414.1.36",
          "form": "추천템..",
          "original_form": "추천템..",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004414.1.37",
          "form": "흐음",
          "original_form": "흐음",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004414.1.38",
          "form": "이번에 취미를 하나 만들었거든요",
          "original_form": "이번에 취미를 하나 만들었거든요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004414.1.39",
          "form": "그러면서 자수 도구를 싹 샀는데...",
          "original_form": "그러면서 자수 도구를 싹 샀는데...",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004414.1.40",
          "form": "예뻐서 만족스러웠던..",
          "original_form": "예뻐서 만족스러웠던..",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004414.1.41",
          "form": "이런 얘기밖에.. 소비를 잘 안하네요",
          "original_form": "이런 얘기밖에.. 소비를 잘 안하네요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004414.1.42",
          "form": "자수는 바느질이랑 똑같은건가요?",
          "original_form": "자수는 바느질이랑 똑같은건가요?",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004414.1.43",
          "form": "제가하는 건 프랑스 자수라",
          "original_form": "제가하는 건 프랑스 자수라",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004414.1.44",
          "form": "비슷한 것들이 있긴해요",
          "original_form": "비슷한 것들이 있긴해요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004414.1.45",
          "form": "근데 일반 바느질보다 기법이 훨씬 다양하죠",
          "original_form": "근데 일반 바느질보다 기법이 훨씬 다양하죠",
          "speaker_id": "1",
          "time": "20210821 17:15"
        }
      ]
    }
