      "utterance": [
        {
          "id": "MDRW2100044106.1.1",
          "form": "안녕하세요!\r\n최근에 구매한 물건 있나요",
          "original_form": "안녕하세요!\r\n최근에 구매한 물건 있나요",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044106.1.2",
          "form": "네 에어프라이어",
          "original_form": "네 에어프라이어",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044106.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044106.1.4",
          "form": "전 식염수랑 티트리 오일 샀어요 에어프라이어는 어떤걸 사셨나요",
          "original_form": "전 식염수랑 티트리 오일 샀어요 에어프라이어는 어떤걸 사셨나요",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044106.1.5",
          "form": "오호 어디껀지모르겟어요 네이버에서 위에잇는거류..",
          "original_form": "오호 어디껀지모르겟어요 네이버에서 위에잇는거류..",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044106.1.6",
          "form": "티트리오일 어디쓰나요",
          "original_form": "티트리오일 어디쓰나요",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044106.1.7",
          "form": "브랜드는 기억 안 나고 원액으로 샀어요",
          "original_form": "브랜드는 기억 안 나고 원액으로 샀어요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044106.1.8",
          "form": "에어프라이기로 뭘 데우셨나요?",
          "original_form": "에어프라이기로 뭘 데우셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044106.1.9",
          "form": "ㅋㅋㅋㅋ 만두요",
          "original_form": "ㅋㅋㅋㅋ 만두요",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044106.1.10",
          "form": "오 만두 구우셨나요? 군만두처럼요?",
          "original_form": "오 만두 구우셨나요? 군만두처럼요?",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044106.1.11",
          "form": "네!에프잇으시나요",
          "original_form": "네!에프잇으시나요",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044106.1.12",
          "form": "네 작은 거 있어서 많이 못 넣어요 님이 새로 사신건 큰가요?",
          "original_form": "네 작은 거 있어서 많이 못 넣어요 님이 새로 사신건 큰가요?",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044106.1.13",
          "form": "저요 큰거같아요",
          "original_form": "저요 큰거같아요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044106.1.14",
          "form": "아닌가 걍 적당",
          "original_form": "아닌가 걍 적당",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044106.1.15",
          "form": "6-8리턴가??",
          "original_form": "6-8리턴가??",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044106.1.16",
          "form": "만두말고 다른거 넣어봤나요?",
          "original_form": "만두말고 다른거 넣어봤나요?",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044106.1.17",
          "form": "네 고기용 목살",
          "original_form": "네 고기용 목살",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044106.1.18",
          "form": "갠찮던데요",
          "original_form": "갠찮던데요",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044106.1.19",
          "form": "오 잘 구워지나봐요!",
          "original_form": "오 잘 구워지나봐요!",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044106.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 21:48"
        }
      ]
