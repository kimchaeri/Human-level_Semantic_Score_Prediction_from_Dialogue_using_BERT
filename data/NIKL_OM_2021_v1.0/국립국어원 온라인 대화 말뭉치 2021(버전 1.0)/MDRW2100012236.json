      "utterance": [
        {
          "id": "MDRW2100012236.1.1",
          "form": "라면 좋아하시나용",
          "original_form": "라면 좋아하시나용",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012236.1.2",
          "form": "하루에한끼는 무조건 면먹어요..ㅎㅎㅎ\n라면 많이드세요?",
          "original_form": "하루에한끼는 무조건 면먹어요..ㅎㅎㅎ\n라면 많이드세요?",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012236.1.3",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012236.1.4",
          "form": "어떤 라면 좋아하세요?",
          "original_form": "어떤 라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012236.1.5",
          "form": "두꺼운면 좋아요.. \n너구리, 우동 류..",
          "original_form": "두꺼운면 좋아요.. \n너구리, 우동 류..",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012236.1.6",
          "form": "아하 !!!!",
          "original_form": "아하 !!!!",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012236.1.7",
          "form": "전 까르보불닭 좋아해요",
          "original_form": "전 까르보불닭 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012236.1.8",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012236.1.9",
          "form": "아.. 엄청매운거...",
          "original_form": "아.. 엄청매운거...",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012236.1.10",
          "form": "매운거 좋아하시나봐유?",
          "original_form": "매운거 좋아하시나봐유?",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012236.1.11",
          "form": "까르보불닭 전 소스를",
          "original_form": "까르보불닭 전 소스를",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012236.1.12",
          "form": "반에반도 안넣습니다ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "반에반도 안넣습니다ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012236.1.13",
          "form": "ㅋㅋㅋ 아 그럼 맛 싱겁지 않아요?\n비벼지지도 않을꺼 같은딩",
          "original_form": "ㅋㅋㅋ 아 그럼 맛 싱겁지 않아요?\n비벼지지도 않을꺼 같은딩",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012236.1.14",
          "form": "아니에요 물 조금 있고 그러니까",
          "original_form": "아니에요 물 조금 있고 그러니까",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012236.1.15",
          "form": "잘 비벼지고",
          "original_form": "잘 비벼지고",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012236.1.16",
          "form": "맛 좋아요",
          "original_form": "맛 좋아요",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012236.1.17",
          "form": "그래도 매울듯... \n전 신라면도 매워서...",
          "original_form": "그래도 매울듯... \n전 신라면도 매워서...",
          "speaker_id": "1",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012236.1.18",
          "form": "신라면 10미터 밖에서도 땀이 막나요",
          "original_form": "신라면 10미터 밖에서도 땀이 막나요",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012236.1.19",
          "form": "헉 그럼 비빔면도 매우세요?",
          "original_form": "헉 그럼 비빔면도 매우세요?",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012236.1.20",
          "form": "전 비빔면이 매워요...",
          "original_form": "전 비빔면이 매워요...",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012236.1.21",
          "form": "비빔면 먹응때 목에 수건두르고 준비하죠.. ㅋㅋㅋ",
          "original_form": "비빔면 먹응때 목에 수건두르고 준비하죠.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012236.1.22",
          "form": "그래서 집에서밖에 안먹어요.. 밖에선 눈썹 지워져서.. ㅋㅋㅋ",
          "original_form": "그래서 집에서밖에 안먹어요.. 밖에선 눈썹 지워져서.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:18"
        }
      ]
