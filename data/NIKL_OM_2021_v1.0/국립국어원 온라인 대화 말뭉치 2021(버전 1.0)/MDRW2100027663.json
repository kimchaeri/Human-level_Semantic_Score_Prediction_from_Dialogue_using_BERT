      "utterance": [
        {
          "id": "MDRW2100027663.1.1",
          "form": "저는 어제 스크린골프를 쳤어요.",
          "original_form": "저는 어제 스크린골프를 쳤어요.",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027663.1.2",
          "form": "무슨 운동을 하는지요?",
          "original_form": "무슨 운동을 하는지요?",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027663.1.3",
          "form": "저는 요즘 그룹피티 다녀요",
          "original_form": "저는 요즘 그룹피티 다녀요",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027663.1.4",
          "form": "스크린골프는 재밌나요?",
          "original_form": "스크린골프는 재밌나요?",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027663.1.5",
          "form": "네 공이 제대로 맞으면 잼인네요",
          "original_form": "네 공이 제대로 맞으면 잼인네요",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027663.1.6",
          "form": "코로나라 실내에서 할수도 있고요",
          "original_form": "코로나라 실내에서 할수도 있고요",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027663.1.7",
          "form": "그룹피티는 구체저으로 어떠케 하나요?",
          "original_form": "그룹피티는 구체저으로 어떠케 하나요?",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027663.1.8",
          "form": "피티쌤이 짠 프로그램 따라서 다같이 같은 동작을 해요",
          "original_form": "피티쌤이 짠 프로그램 따라서 다같이 같은 동작을 해요",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027663.1.9",
          "form": "쌤이 돌아다니면서 자세교정 해주세요",
          "original_form": "쌤이 돌아다니면서 자세교정 해주세요",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027663.1.10",
          "form": "수강료는 안비싼가요?",
          "original_form": "수강료는 안비싼가요?",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027663.1.11",
          "form": "보통 여성분들이 많이 하시죠?",
          "original_form": "보통 여성분들이 많이 하시죠?",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027663.1.12",
          "form": "그룹이라서 일대일보다는 싸요",
          "original_form": "그룹이라서 일대일보다는 싸요",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027663.1.13",
          "form": "반반인것 같아요",
          "original_form": "반반인것 같아요",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027663.1.14",
          "form": "피티는 프레젠테이션인데..ㅎ",
          "original_form": "피티는 프레젠테이션인데..ㅎ",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100027663.1.15",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027663.1.16",
          "form": "나중에 나이먹고도 칠수있으니까 골프도 한번 도전해보세요",
          "original_form": "나중에 나이먹고도 칠수있으니까 골프도 한번 도전해보세요",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027663.1.17",
          "form": "골프는 시작하려면 장비 필요하지 않나요?",
          "original_form": "골프는 시작하려면 장비 필요하지 않나요?",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027663.1.18",
          "form": "처음에는 스크린골프 레슨 받다가 추후에 클럽 하나씩 구입하면대여",
          "original_form": "처음에는 스크린골프 레슨 받다가 추후에 클럽 하나씩 구입하면대여",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027663.1.19",
          "form": "생각보다 비싸지는 않아요",
          "original_form": "생각보다 비싸지는 않아요",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027663.1.20",
          "form": "아하 레슨할때는 대여인가보네요",
          "original_form": "아하 레슨할때는 대여인가보네요",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027663.1.21",
          "form": "네 거기 장비로 하면대여",
          "original_form": "네 거기 장비로 하면대여",
