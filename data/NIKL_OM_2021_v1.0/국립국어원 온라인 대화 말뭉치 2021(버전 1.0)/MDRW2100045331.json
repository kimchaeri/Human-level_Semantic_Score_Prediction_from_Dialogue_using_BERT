      "utterance": [
        {
          "id": "MDRW2100045331.1.1",
          "form": "뭔가ㅈ대체재가 필요해",
          "original_form": "뭔가ㅈ대체재가 필요해",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045331.1.2",
          "form": "솔직히 물에다도",
          "original_form": "솔직히 물에다도",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045331.1.3",
          "form": "얼음 안넣으면",
          "original_form": "얼음 안넣으면",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045331.1.4",
          "form": "맛없어서 못마셔...",
          "original_form": "맛없어서 못마셔...",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045331.1.5",
          "form": "탄산 자극을 원하는거면",
          "original_form": "탄산 자극을 원하는거면",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045331.1.6",
          "form": "탄산수는 어때?",
          "original_form": "탄산수는 어때?",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045331.1.7",
          "form": "맛없어......",
          "original_form": "맛없어......",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.8",
          "form": "아무리 그래도",
          "original_form": "아무리 그래도",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.9",
          "form": "콜라->탄산수늑",
          "original_form": "콜라->탄산수늑",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.10",
          "form": "는",
          "original_form": "는",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.11",
          "form": "너무 무맛...",
          "original_form": "너무 무맛...",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.12",
          "form": "차라리물을 마실래",
          "original_form": "차라리물을 마실래",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.13",
          "form": "난 맥주 줄이려고",
          "original_form": "난 맥주 줄이려고",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.14",
          "form": "탄산수 마시기 시작했는데",
          "original_form": "탄산수 마시기 시작했는데",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045331.1.15",
          "form": "당이 없으니까 확실히 뒷맛이 깔끔해",
          "original_form": "당이 없으니까 확실히 뒷맛이 깔끔해",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045331.1.16",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045331.1.17",
          "form": "당 대체제 음료 마시는 사람들도",
          "original_form": "당 대체제 음료 마시는 사람들도",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045331.1.18",
          "form": "대체적으로 그렇게 말하던데",
          "original_form": "대체적으로 그렇게 말하던데",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045331.1.19",
          "form": "난 잘 이해가 안가......",
          "original_form": "난 잘 이해가 안가......",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045331.1.20",
          "form": "당 대체제 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "당 대체제 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045331.1.21",
          "form": "난 제로콜라는 좀 끝맛이 한약 느낌이라 별로였고",
          "original_form": "난 제로콜라는 좀 끝맛이 한약 느낌이라 별로였고",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045331.1.22",
          "form": "탄산수는 탄산이 훨씬 강한데",
          "original_form": "탄산수는 탄산이 훨씬 강한데",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.23",
          "form": "깔끔하게 탁 쏘고 끝나는 느낌이라",
          "original_form": "깔끔하게 탁 쏘고 끝나는 느낌이라",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.24",
          "form": "근데 난 청 안탄ㅈ탄산수는",
          "original_form": "근데 난 청 안탄ㅈ탄산수는",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.25",
          "form": "걍 안들어가지더라구",
          "original_form": "걍 안들어가지더라구",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.26",
          "form": "그치만 청타면",
          "original_form": "그치만 청타면",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.27",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.28",
          "form": "사실 나도",
          "original_form": "사실 나도",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.29",
          "form": "액상과당이지",
          "original_form": "액상과당이지",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045331.1.30",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045331.1.31",
          "form": "글고 여행가서 처음 탄산수 먹었었는데",
          "original_form": "글고 여행가서 처음 탄산수 먹었었는데",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045331.1.32",
          "form": "유럽놈들은 생수대신 탄산수 수시로 마시거든",
          "original_form": "유럽놈들은 생수대신 탄산수 수시로 마시거든",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045331.1.33",
          "form": "거긴 물이 진짜 맛없다고 듣긴 했어",
          "original_form": "거긴 물이 진짜 맛없다고 듣긴 했어",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045331.1.34",
          "form": "ㅇㅇ 석회수?",
          "original_form": "ㅇㅇ 석회수?",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045331.1.35",
          "form": "일단 기본적으로 다들 수돗물을 마심...",
          "original_form": "일단 기본적으로 다들 수돗물을 마심...",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045331.1.36",
          "form": "아니 대체 뭘하면",
          "original_form": "아니 대체 뭘하면",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045331.1.37",
          "form": "상수도에 석회질 물이 흐르는걸까",
          "original_form": "상수도에 석회질 물이 흐르는걸까",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045331.1.38",
          "form": "지형이 그렇대 ㅋㅋㅋㅋㅋ",
          "original_form": "지형이 그렇대 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:02"
        }
      ]
