      "utterance": [
        {
          "id": "MDRW2100031329.1.1",
          "form": "전이요? 부침개요?",
          "original_form": "전이요? 부침개요?",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031329.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031329.1.3",
          "form": "좋아하지요 기름에 자글자글",
          "original_form": "좋아하지요 기름에 자글자글",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031329.1.4",
          "form": "무슨 전을 제일 좋아하세요?",
          "original_form": "무슨 전을 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031329.1.5",
          "form": "저는 김치전이요!",
          "original_form": "저는 김치전이요!",
          "speaker_id": "2",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031329.1.6",
          "form": "아 저도 김치전 엄청 좋아해요",
          "original_form": "아 저도 김치전 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031329.1.7",
          "form": "바삭바삭하게 구우면 정말 맛있죠 ㅎㅎ",
          "original_form": "바삭바삭하게 구우면 정말 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031329.1.8",
          "form": "네 근데 바삭하게 만들기 정말 어려워요 은근",
          "original_form": "네 근데 바삭하게 만들기 정말 어려워요 은근",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031329.1.9",
          "form": "기름많이 두르고 구우면 되는거 아닌가요?",
          "original_form": "기름많이 두르고 구우면 되는거 아닌가요?",
          "speaker_id": "2",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031329.1.10",
          "form": "그렇긴 한데 반죽이 뭔가 어렵더라구요 저는 ㅠㅠ",
          "original_form": "그렇긴 한데 반죽이 뭔가 어렵더라구요 저는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100031329.1.11",
          "form": "물을 너무 많이 넣으면 질척해지긴 해요",
          "original_form": "물을 너무 많이 넣으면 질척해지긴 해요",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100031329.1.12",
          "form": "그쵸 맛있는 김치전 먹고싶네요 오징어도 잔뜩 들어간",
          "original_form": "그쵸 맛있는 김치전 먹고싶네요 오징어도 잔뜩 들어간",
          "speaker_id": "1",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100031329.1.13",
          "form": "오징어랑 감자들어가면 맛있죠",
          "original_form": "오징어랑 감자들어가면 맛있죠",
          "speaker_id": "2",
          "time": "20210904 20:30"
        }
