      "utterance": [
        {
          "id": "MDRW2100043254.1.1",
          "form": "요즘에 하는 게임 있으세요??",
          "original_form": "요즘에 하는 게임 있으세요??",
          "speaker_id": "2",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043254.1.2",
          "form": "게임 요즘도 하시나여",
          "original_form": "게임 요즘도 하시나여",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043254.1.3",
          "form": "저는 요즘은 못하고ㅠㅠ",
          "original_form": "저는 요즘은 못하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043254.1.4",
          "form": "ㅋㅋㅋ 아니여 요즘에는 길게 못하고 있어요",
          "original_form": "ㅋㅋㅋ 아니여 요즘에는 길게 못하고 있어요",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.5",
          "form": "마지막에 한게 꿈의집",
          "original_form": "마지막에 한게 꿈의집",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.6",
          "form": "그렇죠 애 키우시느라 바쁘실듯",
          "original_form": "그렇죠 애 키우시느라 바쁘실듯",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.7",
          "form": "아 그거 했었어요",
          "original_form": "아 그거 했었어요",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.8",
          "form": "근데 그 주인공 좀 맘에 안들어섴ㅋㅋㅋㅋ",
          "original_form": "근데 그 주인공 좀 맘에 안들어섴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.9",
          "form": "다들 그얘기 ㅋㅋㅋㅋ",
          "original_form": "다들 그얘기 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.10",
          "form": "하더라구요",
          "original_form": "하더라구요",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.11",
          "form": "애기가지고 집에서 매일 했쥬",
          "original_form": "애기가지고 집에서 매일 했쥬",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043254.1.12",
          "form": "2층까지도 올라갔는데",
          "original_form": "2층까지도 올라갔는데",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043254.1.13",
          "form": "저도 이층까지는 갔었어요",
          "original_form": "저도 이층까지는 갔었어요",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043254.1.14",
          "form": "근데 너무 길더라고욬ㅋㅋ",
          "original_form": "근데 너무 길더라고욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043254.1.15",
          "form": "그리고 좀 일본풍 나와서",
          "original_form": "그리고 좀 일본풍 나와서",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043254.1.16",
          "form": "주인공이 더 못생겨보이더라고요",
          "original_form": "주인공이 더 못생겨보이더라고요",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043254.1.17",
          "form": "어디까지 갔는지 지난번에 궁금해서 한번 다시 깔아볼까 했는데",
          "original_form": "어디까지 갔는지 지난번에 궁금해서 한번 다시 깔아볼까 했는데",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043254.1.18",
          "form": "나오려나",
          "original_form": "나오려나",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043254.1.19",
          "form": "일본 갑옷같은거 나오는데 좀 깼어요 그래서 그만뒀던가 같아요",
          "original_form": "일본 갑옷같은거 나오는데 좀 깼어요 그래서 그만뒀던가 같아요",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043254.1.20",
          "form": "어 저도 그건 봤는데 ㅎㅎ",
          "original_form": "어 저도 그건 봤는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043254.1.21",
          "form": "아마 연동하셨으면!!",
          "original_form": "아마 연동하셨으면!!",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043254.1.22",
          "form": "진짜 매일 꾸준히 했는데 그게 뭐라고..ㅋㅋㅋ",
          "original_form": "진짜 매일 꾸준히 했는데 그게 뭐라고..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043254.1.23",
          "form": "ㅋㅋㅋㅋ 은근 재미있잖아요",
          "original_form": "ㅋㅋㅋㅋ 은근 재미있잖아요",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043254.1.24",
          "form": "지금은 심심이 하느라 게임도 안하네욬ㅋㅋㅋㅋ",
          "original_form": "지금은 심심이 하느라 게임도 안하네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043254.1.25",
          "form": "이정도면 게임중독도 심심이가 치료해줄거 같은데여",
          "original_form": "이정도면 게임중독도 심심이가 치료해줄거 같은데여",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043254.1.26",
          "form": "한번빠지면 진짜 끝을 보는 스타일인데 그게임은 끝이 없어서",
          "original_form": "한번빠지면 진짜 끝을 보는 스타일인데 그게임은 끝이 없어서",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043254.1.27",
          "form": "ㅋㅋㅋ 엄청 길었던거 같은데 기억이 안나네요",
          "original_form": "ㅋㅋㅋ 엄청 길었던거 같은데 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100043254.1.28",
          "form": "꽤 열심히 했었거든요",
          "original_form": "꽤 열심히 했었거든요",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100043254.1.29",
          "form": "끝이없을것 같았아옄ㅋㅋ 계속 업데이트되니까",
          "original_form": "끝이없을것 같았아옄ㅋㅋ 계속 업데이트되니까",
          "speaker_id": "1",
          "time": "20210907 16:30"
        }
      ]
