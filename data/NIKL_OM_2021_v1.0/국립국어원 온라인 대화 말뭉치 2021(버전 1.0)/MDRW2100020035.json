        },
        {
          "id": "MDRW2100020035.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100020035.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020035.1.4",
          "form": "과거에는요?",
          "original_form": "과거에는요?",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020035.1.5",
          "form": "교회1반이요",
          "original_form": "교회1반이요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020035.1.6",
          "form": "저도 교회는 다녔었어요",
          "original_form": "저도 교회는 다녔었어요",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020035.1.7",
          "form": "친구따라요",
          "original_form": "친구따라요",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020035.1.8",
          "form": "저는 그냥 신앙심보단 재밌어서 나갔어요",
          "original_form": "저는 그냥 신앙심보단 재밌어서 나갔어요",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020035.1.9",
          "form": "흥미가 있어요?",
          "original_form": "흥미가 있어요?",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020035.1.10",
          "form": "그냥 친구 따라 나가면",
          "original_form": "그냥 친구 따라 나가면",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020035.1.11",
          "form": "찬송가 부르고 피아노 치고 이러는 게",
          "original_form": "찬송가 부르고 피아노 치고 이러는 게",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020035.1.12",
          "form": "하나의 취미로 보였던 듯요",
          "original_form": "하나의 취미로 보였던 듯요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020035.1.13",
          "form": "저도 따라가봤는데",
          "original_form": "저도 따라가봤는데",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020035.1.14",
          "form": "그래도 신앙심이 없어서 나가기도 좀 그렇더라고요",
          "original_form": "그래도 신앙심이 없어서 나가기도 좀 그렇더라고요",
          "speaker_id": "2",
