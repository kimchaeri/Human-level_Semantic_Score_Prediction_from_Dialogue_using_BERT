      "utterance": [
        {
          "id": "MDRW2100038646.1.1",
          "form": "집에 있는 걸로 먹어야지",
          "original_form": "집에 있는 걸로 먹어야지",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038646.1.2",
          "form": "글쿤 ㅋㅋ 집에 뭐가 많긴함",
          "original_form": "글쿤 ㅋㅋ 집에 뭐가 많긴함",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038646.1.3",
          "form": "뭐 먹고 싶은데",
          "original_form": "뭐 먹고 싶은데",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038646.1.4",
          "form": "맛난거~",
          "original_form": "맛난거~",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038646.1.5",
          "form": "고기 요리 해 줄까",
          "original_form": "고기 요리 해 줄까",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038646.1.6",
          "form": "옹옹 어제 먹은거?",
          "original_form": "옹옹 어제 먹은거?",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.7",
          "form": "그래 고기랑 계란이랑 같이 먹는 거",
          "original_form": "그래 고기랑 계란이랑 같이 먹는 거",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.8",
          "form": "웅 ㅋㅋ 해줘용",
          "original_form": "웅 ㅋㅋ 해줘용",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.9",
          "form": "알았다 고기 먹어 치우자",
          "original_form": "알았다 고기 먹어 치우자",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.10",
          "form": "굿굿 고기는 항상 옳다",
          "original_form": "굿굿 고기는 항상 옳다",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.11",
          "form": "먹을 준비 해라",
          "original_form": "먹을 준비 해라",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.12",
          "form": "넵 ㅋㅋ",
          "original_form": "넵 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.13",
          "form": "채소 좀 있다 씻어 놔라",
          "original_form": "채소 좀 있다 씻어 놔라",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038646.1.14",
          "form": "알았어용 쌈이랑 같이 먹음 꿀맛이지",
          "original_form": "알았어용 쌈이랑 같이 먹음 꿀맛이지",
          "speaker_id": "1",
          "time": "20210902 18:15"
