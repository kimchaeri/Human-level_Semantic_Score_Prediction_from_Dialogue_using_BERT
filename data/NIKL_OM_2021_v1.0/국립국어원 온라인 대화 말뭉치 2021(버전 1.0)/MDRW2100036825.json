      "utterance": [
        {
          "id": "MDRW2100036825.1.1",
          "form": "후식드시나요?",
          "original_form": "후식드시나요?",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100036825.1.2",
          "form": "후식 가끔 먹어요",
          "original_form": "후식 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100036825.1.3",
          "form": "과일이나 떡 같은걸로요",
          "original_form": "과일이나 떡 같은걸로요",
          "speaker_id": "2",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100036825.1.4",
          "form": "후식 드세요?",
          "original_form": "후식 드세요?",
          "speaker_id": "2",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100036825.1.5",
          "form": "네! 수박 먹었어요!!",
          "original_form": "네! 수박 먹었어요!!",
          "speaker_id": "1",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100036825.1.6",
          "form": "수박! 저 일넌넘게 못먹었어요",
          "original_form": "수박! 저 일넌넘게 못먹었어요",
          "speaker_id": "2",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100036825.1.7",
          "form": "너무 양이커서 잘못사요",
          "original_form": "너무 양이커서 잘못사요",
          "speaker_id": "2",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100036825.1.8",
          "form": "앗! 그건 그렇죠ㅜㅠ",
          "original_form": "앗! 그건 그렇죠ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036825.1.9",
          "form": "가족있을때나 먹는거죠 흑",
          "original_form": "가족있을때나 먹는거죠 흑",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036825.1.10",
          "form": "흑흑 애플수박인가 작은거 나오는데",
          "original_form": "흑흑 애플수박인가 작은거 나오는데",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100036825.1.11",
          "form": "매번 고민하다가 복숭아사가요",
          "original_form": "매번 고민하다가 복숭아사가요",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100036825.1.12",
          "form": "복숭아도 최고죠",
          "original_form": "복숭아도 최고죠",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100036825.1.13",
          "form": "올해 복숭아가 맛있었는걸욤",
          "original_form": "올해 복숭아가 맛있었는걸욤",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100036825.1.14",
          "form": "그죠 올해 진짜 맛있었어요 엄청달구요",
          "original_form": "그죠 올해 진짜 맛있었어요 엄청달구요",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100036825.1.15",
          "form": "임실 복숭아 자주사먹었는데 맛있었어요",
          "original_form": "임실 복숭아 자주사먹었는데 맛있었어요",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100036825.1.16",
          "form": "진짜요! 너무 맛있으니 복숭아만 보이더라구욬ㅋㅋ",
          "original_form": "진짜요! 너무 맛있으니 복숭아만 보이더라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100036825.1.17",
          "form": "이제 슬슬 복숭아가 들어가는거 같던데 아쉽네요",
          "original_form": "이제 슬슬 복숭아가 들어가는거 같던데 아쉽네요",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100036825.1.18",
          "form": "맞아요..ㅠㅜ 계속 나와줬으면..",
          "original_form": "맞아요..ㅠㅜ 계속 나와줬으면..",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036825.1.19",
          "form": "좋겠는데 말이죠",
          "original_form": "좋겠는데 말이죠",
          "speaker_id": "1",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100036825.1.20",
          "form": "그죠 그래도 이제 청포도가 슬슬 나와서",
          "original_form": "그죠 그래도 이제 청포도가 슬슬 나와서",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100036825.1.21",
          "form": "아쉬움이 덜해요",
          "original_form": "아쉬움이 덜해요",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100036825.1.22",
          "form": "청포도 좋아하시는군용",
          "original_form": "청포도 좋아하시는군용",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100036825.1.23",
          "form": "다른거 좋아하시는거 있으신가요?",
          "original_form": "다른거 좋아하시는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100036825.1.24",
          "form": "비싸지만 멜론도 좋아해요",
          "original_form": "비싸지만 멜론도 좋아해요",
          "speaker_id": "2",
          "time": "20210831 22:19"
        }
      ]
    }
  ]
