      "utterance": [
        {
          "id": "MDRW2100041319.1.1",
          "form": "운동 가기전에 배고플거 같아서 시리얼 먹었네요 ㅎ",
          "original_form": "운동 가기전에 배고플거 같아서 시리얼 먹었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 08:32"
        },
        {
          "id": "MDRW2100041319.1.2",
          "form": "오 시리얼 어떤거 드시나용",
          "original_form": "오 시리얼 어떤거 드시나용",
          "speaker_id": "1",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041319.1.3",
          "form": "콘푸로스트 컵으로 된거 먹어용ㅎ",
          "original_form": "콘푸로스트 컵으로 된거 먹어용ㅎ",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041319.1.4",
          "form": "매일먹는게 아니라 먹기 편하더라고요",
          "original_form": "매일먹는게 아니라 먹기 편하더라고요",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041319.1.5",
          "form": "아 컵으로 조그맣게 나눠서 나온거요?",
          "original_form": "아 컵으로 조그맣게 나눠서 나온거요?",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041319.1.6",
          "form": "마트에서 본거 같네요",
          "original_form": "마트에서 본거 같네요",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041319.1.7",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041319.1.8",
          "form": "괜찮더라고요 간단히 한끼때우기로 좋아요",
          "original_form": "괜찮더라고요 간단히 한끼때우기로 좋아요",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041319.1.9",
          "form": "그렇겠어요 들고다니기도 편하겠네요",
          "original_form": "그렇겠어요 들고다니기도 편하겠네요",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041319.1.10",
          "form": "전 견과류 들어간거 좋아해요 시리얼은 ㅎㅎ",
          "original_form": "전 견과류 들어간거 좋아해요 시리얼은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100041319.1.11",
          "form": "아하! 그래놀라 이런거 좋아하시나요~?",
          "original_form": "아하! 그래놀라 이런거 좋아하시나요~?",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041319.1.12",
          "form": "네 맞아요 ㅋㅋ 씹는 맛이 있어서.. 근데 가격이 좀 나가더라고요",
          "original_form": "네 맞아요 ㅋㅋ 씹는 맛이 있어서.. 근데 가격이 좀 나가더라고요",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041319.1.13",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041319.1.14",
          "form": "비싸던데요 그래놀라",
          "original_form": "비싸던데요 그래놀라",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041319.1.15",
          "form": "그래도 뭔가 여러가지고 맛은있더만요",
          "original_form": "그래도 뭔가 여러가지고 맛은있더만요",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041319.1.16",
          "form": "요거트에",
          "original_form": "요거트에",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041319.1.17",
          "form": "넣어.서",
          "original_form": "넣어.서",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041319.1.18",
          "form": "먹어도 맛있더라고용",
          "original_form": "먹어도 맛있더라고용",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041319.1.19",
          "form": "맞아요 저도 요거트에 넣어먹는거 좋아해요",
          "original_form": "맞아요 저도 요거트에 넣어먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041319.1.20",
          "form": "과일도 넣어서 냠냠",
          "original_form": "과일도 넣어서 냠냠",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041319.1.21",
          "form": "그쵸 과일ㅇㅣ 빠질수없죠",
          "original_form": "그쵸 과일ㅇㅣ 빠질수없죠",
          "speaker_id": "2",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100041319.1.22",
          "form": "넹 ㅎㅎ 식사는 꼬박꼬박 하시는 편인가요?",
          "original_form": "넹 ㅎㅎ 식사는 꼬박꼬박 하시는 편인가요?",
          "speaker_id": "1",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100041319.1.23",
          "form": "저~ 아침빼곤 잘 챙겨먹어요!",
          "original_form": "저~ 아침빼곤 잘 챙겨먹어요!",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100041319.1.24",
          "form": "너무 잘 챙겨먹어서 탈이죠ㅋㅋㅋ",
          "original_form": "너무 잘 챙겨먹어서 탈이죠ㅋㅋㅋ",
          "speaker_id": "2",
