      "utterance": [
        {
          "id": "MDRW2100021801.1.1",
          "form": "네에?",
          "original_form": "네에?",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021801.1.2",
          "form": "난선픙기돌리고잇는데 ㅋㅋㅋㅋ",
          "original_form": "난선픙기돌리고잇는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021801.1.3",
          "form": "시상에 ㅋㅋㅋㅋ 이렇게 다르구먼 거참",
          "original_form": "시상에 ㅋㅋㅋㅋ 이렇게 다르구먼 거참",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021801.1.4",
          "form": "긍까 ㅋㅋㅋ깝깝해ㅠㅠ ㅋㅋ",
          "original_form": "긍까 ㅋㅋㅋ깝깝해ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021801.1.5",
          "form": "오늘은 또 비가 오는구려!!",
          "original_form": "오늘은 또 비가 오는구려!!",
          "speaker_id": "1",
          "time": "20210829 07:57"
        },
        {
          "id": "MDRW2100021801.1.6",
          "form": "오빠세차안하고오겟네 ㅋㅋㅋㅋ어제세차말하더만",
          "original_form": "오빠세차안하고오겟네 ㅋㅋㅋㅋ어제세차말하더만",
          "speaker_id": "2",
          "time": "20210829 08:55"
        },
        {
          "id": "MDRW2100021801.1.7",
          "form": "ㅋㅋ담주 내내 비올걸?",
          "original_form": "ㅋㅋ담주 내내 비올걸?",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100021801.1.8",
          "form": "그러게 ㅋㅋㅋ긍가봐 잘잣다우",
          "original_form": "그러게 ㅋㅋㅋ긍가봐 잘잣다우",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100021801.1.9",
          "form": "비와서 잠이 잘오쥬 ㅋㅋ",
          "original_form": "비와서 잠이 잘오쥬 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100021801.1.10",
          "form": "이런날엔 칼국수인가유",
          "original_form": "이런날엔 칼국수인가유",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100021801.1.11",
          "form": "날씨가 ㅋㅋㅋ국물이 최고요",
          "original_form": "날씨가 ㅋㅋㅋ국물이 최고요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100021801.1.12",
          "form": "ㅋㅋ맞아 축축하고 추워서 딱이야",
          "original_form": "ㅋㅋ맞아 축축하고 추워서 딱이야",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100021801.1.13",
          "form": "날씨가 그래도 챵문열어뒀능데",
          "original_form": "날씨가 그래도 챵문열어뒀능데",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100021801.1.14",
          "form": "아침부터 아랫집 생선구이냄새들어와서",
          "original_form": "아침부터 아랫집 생선구이냄새들어와서",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021801.1.15",
          "form": "9시부터 ㅋㅋ 생선구이집말도못꺼냇넹...",
          "original_form": "9시부터 ㅋㅋ 생선구이집말도못꺼냇넹...",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021801.1.16",
          "form": "오마이갓 이런 날씨에 생선..",
          "original_form": "오마이갓 이런 날씨에 생선..",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021801.1.17",
          "form": "거실창에서 ㅋㅋㅋ꽤 진하게 올라오던데",
          "original_form": "거실창에서 ㅋㅋㅋ꽤 진하게 올라오던데",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021801.1.18",
