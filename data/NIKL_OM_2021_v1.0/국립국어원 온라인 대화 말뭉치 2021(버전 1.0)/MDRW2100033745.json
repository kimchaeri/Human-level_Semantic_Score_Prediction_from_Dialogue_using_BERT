        },
        {
          "id": "MDRW2100033745.1.12",
          "form": "name3는 역시 추르를 좋아해",
          "original_form": "&name3&는 역시 추르를 좋아해",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033745.1.13",
          "form": "너희집에 추르잇으면 좀 주라",
          "original_form": "너희집에 추르잇으면 좀 주라",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033745.1.14",
          "form": "니가 먹게?",
          "original_form": "니가 먹게?",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033745.1.15",
          "form": "먹겠어?",
          "original_form": "먹겠어?",
