        },
        {
          "id": "MDRW2100025250.1.2",
          "form": "두산팬 친구",
          "original_form": "두산팬 친구",
          "speaker_id": "2",
          "time": "20210901 02:54"
        },
        {
          "id": "MDRW2100025250.1.3",
          "form": "3대1이라서",
          "original_form": "3대1이라서",
          "speaker_id": "2",
          "time": "20210901 02:54"
        },
        {
          "id": "MDRW2100025250.1.4",
          "form": "두산 선수들 돌면서 세레머니할때",
          "original_form": "두산 선수들 돌면서 세레머니할때",
          "speaker_id": "2",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025250.1.5",
          "form": "앗ㅠ",
          "original_form": "앗ㅠ",
          "speaker_id": "1",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025250.1.6",
          "form": "혼자 음식 치웠어요",
          "original_form": "혼자 음식 치웠어요",
          "speaker_id": "2",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025250.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025250.1.8",
          "form": "며칠 전에 김혜성 선수가 주장 됐다고 해서 놀랐는데 생각해보니까 키움 선수단이 되게 젊더라구요",
          "original_form": "며칠 전에 김혜성 선수가 주장 됐다고 해서 놀랐는데 생각해보니까 키움 선수단이 되게 젊더라구요",
          "speaker_id": "1",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025250.1.9",
          "form": "그래도 놀랄만 하긴해요",
          "original_form": "그래도 놀랄만 하긴해요",
          "speaker_id": "2",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025250.1.10",
          "form": "아무리 젊어도 혜성이 22 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아무리 젊어도 혜성이 22 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025250.1.11",
          "form": "ㅋㅋ 사실 젊다못해 어리죠",
          "original_form": "ㅋㅋ 사실 젊다못해 어리죠",
          "speaker_id": "1",
          "time": "20210901 02:58"
        },
        {
          "id": "MDRW2100025250.1.12",
          "form": "다른 팀이면 1군은커녕 2군일텐데ㅋㅋ",
          "original_form": "다른 팀이면 1군은커녕 2군일텐데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:59"
        },
        {
          "id": "MDRW2100025250.1.13",
          "form": "팀이 돈이 없으니까 확실히 물갈이는 빨라서ㅋㅋ",
          "original_form": "팀이 돈이 없으니까 확실히 물갈이는 빨라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:59"
        },
        {
          "id": "MDRW2100025250.1.14",
          "form": "그래도 김혜성 잘하잖아요ㅋㅋ",
          "original_form": "그래도 김혜성 잘하잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:59"
        },
        {
          "id": "MDRW2100025250.1.15",
          "form": "김하성 선수는 미국 가더니 머리도 미국 스타일 됐더라구요 ㅋㅋ",
          "original_form": "김하성 선수는 미국 가더니 머리도 미국 스타일 됐더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100025250.1.16",
          "form": "김혜성 잘하긴하죠ㅋㅋ",
          "original_form": "김혜성 잘하긴하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100025250.1.17",
          "form": "저 김하성 목동에서 봤었는데",
          "original_form": "저 김하성 목동에서 봤었는데",
          "speaker_id": "2",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100025250.1.18",
          "form": "화장실에서 마주쳤었어요ㅋㅋ",
          "original_form": "화장실에서 마주쳤었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:01"
        },
        {
          "id": "MDRW2100025250.1.19",
          "form": "덩치 큰가요?",
          "original_form": "덩치 큰가요?",
          "speaker_id": "1",
          "time": "20210901 03:01"
        },
        {
          "id": "MDRW2100025250.1.20",
          "form": "예전에 삼성 김상수 선수는 가까이서 본 적 있는데 운동선수 치고는 평범한 체격이더라구요",
          "original_form": "예전에 삼성 김상수 선수는 가까이서 본 적 있는데 운동선수 치고는 평범한 체격이더라구요",
          "speaker_id": "1",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025250.1.21",
          "form": "아뇨 작아요",
          "original_form": "아뇨 작아요",
          "speaker_id": "2",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025250.1.22",
          "form": "그리고 되게 무명때라서",
          "original_form": "그리고 되게 무명때라서",
          "speaker_id": "2",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025250.1.23",
          "form": "코너에서 마주쳐서 놀라서 발 쾅했는데",
          "original_form": "코너에서 마주쳐서 놀라서 발 쾅했는데",
          "speaker_id": "2",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025250.1.24",
          "form": "김하성도 놀라서 움찔했었는데",
          "original_form": "김하성도 놀라서 움찔했었는데",
          "speaker_id": "2",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025250.1.25",
          "form": "메이져를 가다니....",
          "original_form": "메이져를 가다니....",
          "speaker_id": "2",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025250.1.26",
          "form": "거기서 잘 했으면 좋겠어요. 오래오래ㅋㅋ",
          "original_form": "거기서 잘 했으면 좋겠어요. 오래오래ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025250.1.27",
          "form": "그쵸 잘해줘야",
          "original_form": "그쵸 잘해줘야",
          "speaker_id": "2",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025250.1.28",
          "form": "다른 친구들도 가고 할텐데",
          "original_form": "다른 친구들도 가고 할텐데",
          "speaker_id": "2",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025250.1.29",
          "form": "그래도 강정호 다음으로",
          "original_form": "그래도 강정호 다음으로",
          "speaker_id": "2",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025250.1.30",
          "form": "입지 탄탄한거 같긴해요",
          "original_form": "입지 탄탄한거 같긴해요",
          "speaker_id": "2",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025250.1.31",
          "form": "출전기회도 더 늘어났으면..",
          "original_form": "출전기회도 더 늘어났으면..",
          "speaker_id": "1",
          "time": "20210901 03:05"
        }
      ]
