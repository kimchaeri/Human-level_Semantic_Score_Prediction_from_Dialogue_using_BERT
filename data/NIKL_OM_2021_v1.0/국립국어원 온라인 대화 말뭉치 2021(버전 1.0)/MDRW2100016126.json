      "utterance": [
        {
          "id": "MDRW2100016126.1.1",
          "form": "오늘 아빠 백신 2차 맞으러 갔다! 떨려",
          "original_form": "오늘 아빠 백신 2차 맞으러 갔다! 떨려",
          "speaker_id": "1",
          "time": "20210826 09:58"
        },
        {
          "id": "MDRW2100016126.1.2",
          "form": "우리집 요번달 수도세 4만원 나왔다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리집 요번달 수도세 4만원 나왔다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016126.1.3",
          "form": "엄마가 어디 누수되는거 아니냐고 ㅋㅋㅋ",
          "original_form": "엄마가 어디 누수되는거 아니냐고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016126.1.4",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016126.1.5",
          "form": "그러게 진짜 찾아봐야하는거아냐?",
          "original_form": "그러게 진짜 찾아봐야하는거아냐?",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016126.1.6",
          "form": "얼마나 더나온가평소보다",
          "original_form": "얼마나 더나온가평소보다",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016126.1.7",
          "form": "ㅋㅋㅋ근데 보니까 저번달도 3만 얼마더라 ㅋㅋ",
          "original_form": "ㅋㅋㅋ근데 보니까 저번달도 3만 얼마더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016126.1.8",
          "form": "세탁을 넘 많이 하는 거 같아 ㅋㅋ 진짜 거의 매일 돌렸으니",
          "original_form": "세탁을 넘 많이 하는 거 같아 ㅋㅋ 진짜 거의 매일 돌렸으니",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016126.1.9",
          "form": "그렇게 세탁물이많아!?",
          "original_form": "그렇게 세탁물이많아!?",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016126.1.10",
          "form": "우린 주말에만하는데 그래도 3번할껄? 하루에 ㅋㅋㅋ",
          "original_form": "우린 주말에만하는데 그래도 3번할껄? 하루에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016126.1.11",
          "form": "난 12200원이다 수도료",
          "original_form": "난 12200원이다 수도료",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016126.1.12",
          "form": "ㅋㅋㅋ진짜 우리집이 좀 심하긴 하네 ㅋㅋㅋ 우린 진짜 금욜부터 주말에는 하루에 2~3번 기본인듯ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ진짜 우리집이 좀 심하긴 하네 ㅋㅋㅋ 우린 진짜 금욜부터 주말에는 하루에 2~3번 기본인듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016126.1.13",
          "form": "아빠 일주일동안 모아서 오지 ㅋㅋ 여름엔 수건도 많이 쓰고! 암튼 넘 많이 돌려",
          "original_form": "아빠 일주일동안 모아서 오지 ㅋㅋ 여름엔 수건도 많이 쓰고! 암튼 넘 많이 돌려",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016126.1.14",
          "form": "세탁이 물이 많이 먹겟지ㅠㅠ",
          "original_form": "세탁이 물이 많이 먹겟지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016126.1.15",
          "form": "난 욕조받는거 덜햇는지 280원줄엇네",
          "original_form": "난 욕조받는거 덜햇는지 280원줄엇네",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016126.1.16",
          "form": "여름엔 샤워를 매일하니",
          "original_form": "여름엔 샤워를 매일하니",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016126.1.17",
          "form": "담달에전기료가궁금하군 ㅋㅋ",
          "original_form": "담달에전기료가궁금하군 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016126.1.18",
          "form": "맞아 거기다 사람도 많고ㅋㅋㅋ 음 많이 나올 수 밖에 없었구만",
          "original_form": "맞아 거기다 사람도 많고ㅋㅋㅋ 음 많이 나올 수 밖에 없었구만",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016126.1.19",
          "form": "그러게ㅜ 뭔가.누스는아닌걸로..",
          "original_form": "그러게ㅜ 뭔가.누스는아닌걸로..",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016126.1.20",
          "form": "ㅋㅋ이번달에 신경써서 쓰고 다시 살펴봐야겠어!",
          "original_form": "ㅋㅋ이번달에 신경써서 쓰고 다시 살펴봐야겠어!",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016126.1.21",
          "form": "여름엔 가스비는 적게나와 ㅋㅋㅋ 온수를.덜쓰니",
          "original_form": "여름엔 가스비는 적게나와 ㅋㅋㅋ 온수를.덜쓰니",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016126.1.22",
          "form": "ㅋㅋ맞아 그래도 난 쪄뜨샤야...",
          "original_form": "ㅋㅋ맞아 그래도 난 쪄뜨샤야...",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016126.1.23",
          "form": "쪄뜨샤 어제 유퀴즈에서 배운 단어지 ㅋㅋㅋ",
          "original_form": "쪄뜨샤 어제 유퀴즈에서 배운 단어지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016126.1.24",
          "form": "풀네임못쓰늨디 ㅡㅡ",
          "original_form": "풀네임못쓰늨디 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100016126.1.25",
          "form": "ㅋㅋㅋㅋ맞혀봐!!!",
          "original_form": "ㅋㅋㅋㅋ맞혀봐!!!",
          "speaker_id": "1",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016126.1.26",
          "form": "아니ㅜ 내가생각한게아닌가 ㅋㅋㅋ",
          "original_form": "아니ㅜ 내가생각한게아닌가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016126.1.27",
          "form": "안올라가유",
          "original_form": "안올라가유",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016126.1.28",
          "form": "ㅋㅋㅋㅋㅋ쪄죽어도 뜨거운물 샤워",
          "original_form": "ㅋㅋㅋㅋㅋ쪄죽어도 뜨거운물 샤워",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016126.1.29",
          "form": "그니깐",
          "original_form": "그니깐",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016126.1.30",
          "form": "쩌..",
          "original_form": "쩌..",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016126.1.31",
          "form": "죽어도는 안되나봐",
          "original_form": "죽어도는 안되나봐",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016126.1.32",
          "form": "쪄 ㅋㅌㅌㅋ",
          "original_form": "쪄 ㅋㅌㅌㅋ",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016126.1.33",
          "form": "암튼 세탁물이 원인인지 궁금하다 ㅋㅋㅋ빨리 다음달 됐으면",
          "original_form": "암튼 세탁물이 원인인지 궁금하다 ㅋㅋㅋ빨리 다음달 됐으면",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016126.1.34",
          "form": "여름엔 빨래를 자주하긴하지ㅜㅜ",
          "original_form": "여름엔 빨래를 자주하긴하지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016126.1.35",
          "form": "ㅋㅋ다음달 물세보고 얘기 해줄게!",
          "original_form": "ㅋㅋ다음달 물세보고 얘기 해줄게!",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016126.1.36",
          "form": "다다음달을 봐야하는건가 ㅋㅋ",
          "original_form": "다다음달을 봐야하는건가 ㅋㅋ",
