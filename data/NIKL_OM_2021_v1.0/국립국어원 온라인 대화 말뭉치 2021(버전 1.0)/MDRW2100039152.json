      "utterance": [
        {
          "id": "MDRW2100039152.1.1",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100039152.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100039152.1.3",
          "form": "혹시 학생이세요?",
          "original_form": "혹시 학생이세요?",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100039152.1.4",
          "form": "넵넵!!",
          "original_form": "넵넵!!",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039152.1.5",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100039152.1.6",
          "form": "대학생인가요?",
          "original_form": "대학생인가요?",
          "speaker_id": "1",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100039152.1.7",
          "form": "네 맞아요 ㅎㅎ",
          "original_form": "네 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100039152.1.8",
          "form": "개강..ㅠㅠ 힘드네요",
          "original_form": "개강..ㅠㅠ 힘드네요",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100039152.1.9",
          "form": "저는 수강정정기간이라 놀고있답니다!!",
          "original_form": "저는 수강정정기간이라 놀고있답니다!!",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100039152.1.10",
          "form": "앗 부러워요 전 오늘도 강의 듣는중이에요ㅠㅠㅋㅋㅋ",
          "original_form": "앗 부러워요 전 오늘도 강의 듣는중이에요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100039152.1.11",
          "form": "헉 무슨과길래",
          "original_form": "헉 무슨과길래",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100039152.1.12",
          "form": "첫주부터 빡세시죠",
          "original_form": "첫주부터 빡세시죠",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100039152.1.13",
          "form": "경제쪽이에요!",
          "original_form": "경제쪽이에요!",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100039152.1.14",
          "form": "헉 그렇구나....",
          "original_form": "헉 그렇구나....",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100039152.1.15",
          "form": "저도 오늘 하나들어야겠어요",
          "original_form": "저도 오늘 하나들어야겠어요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100039152.1.16",
          "form": "뒤에 약속있는데",
          "original_form": "뒤에 약속있는데",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100039152.1.17",
          "form": "일어나기가 싫네요 ..",
          "original_form": "일어나기가 싫네요 ..",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100039152.1.18",
          "form": "무슨과세요?",
          "original_form": "무슨과세요?",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100039152.1.19",
          "form": "화학이요",
          "original_form": "화학이요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100039152.1.20",
          "form": "약속도있다니 알찬 하루 보내시네요",
          "original_form": "약속도있다니 알찬 하루 보내시네요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100039152.1.21",
          "form": "전 침대 지박령이랍니다",
          "original_form": "전 침대 지박령이랍니다",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100039152.1.22",
          "form": "ㅠㅠㅠㅠㅠ 후회돼요",
          "original_form": "ㅠㅠㅠㅠㅠ 후회돼요",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100039152.1.23",
          "form": "저도 지박령이고 싶은데",
          "original_form": "저도 지박령이고 싶은데",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100039152.1.24",
          "form": "화학 너무 멋지시네요..",
          "original_form": "화학 너무 멋지시네요..",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100039152.1.25",
          "form": "사실 침대 지박령하면 안되는데",
          "original_form": "사실 침대 지박령하면 안되는데",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100039152.1.26",
          "form": "제가 게을러서(?)",
          "original_form": "제가 게을러서(?)",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100039152.1.27",
          "form": "그렇답니다",
          "original_form": "그렇답니다",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100039152.1.28",
          "form": "어우 전 진짜 대박 게을러요 ㅋㅋㅋㅋ",
          "original_form": "어우 전 진짜 대박 게을러요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:01"
        },
        {
          "id": "MDRW2100039152.1.29",
          "form": "강의도 안듣다가 시험 일주일전에 벼락치기해요..",
          "original_form": "강의도 안듣다가 시험 일주일전에 벼락치기해요..",
          "speaker_id": "2",
          "time": "20210904 19:02"
        }
      ]
    }
