      "utterance": [
        {
          "id": "MDRW2100000609.1.1",
          "form": "오늘 삼시3끼 봤는데 오리 12마리가 나왔어",
          "original_form": "오늘 삼시3끼 봤는데 오리 12마리가 나왔어",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000609.1.2",
          "form": "나 펫샵 갈까 하는데~",
          "original_form": "나 펫샵 갈까 하는데~",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000609.1.3",
          "form": "오리부화부터 나오는데 정말 귀엽더라",
          "original_form": "오리부화부터 나오는데 정말 귀엽더라",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000609.1.4",
          "form": "고냥이 분양 받을꺼임",
          "original_form": "고냥이 분양 받을꺼임",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000609.1.5",
          "form": "차승원도 새 싫어하는데 오리는 귀엽다고",
          "original_form": "차승원도 새 싫어하는데 오리는 귀엽다고",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000609.1.6",
          "form": "반려오리야",
          "original_form": "반려오리야",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000609.1.7",
          "form": "러시안블루~",
          "original_form": "러시안블루~",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000609.1.8",
          "form": "분양받지말고",
          "original_form": "분양받지말고",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.9",
          "form": "유기동물 데려와",
          "original_form": "유기동물 데려와",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.10",
          "form": "러시안 블루 난 싫은데",
          "original_form": "러시안 블루 난 싫은데",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.11",
          "form": "유기동물 어디서 데려오누?",
          "original_form": "유기동물 어디서 데려오누?",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.12",
          "form": "러시안블루도 있나?",
          "original_form": "러시안블루도 있나?",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.13",
          "form": "잘 모르는데 요즘은 사지말라고 그러더라고",
          "original_form": "잘 모르는데 요즘은 사지말라고 그러더라고",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.14",
          "form": "고냥이는 러시안 블루가 제일 좋음",
          "original_form": "고냥이는 러시안 블루가 제일 좋음",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.15",
          "form": "품종묘 아니어도 불쌍한 고양이들 많아",
          "original_form": "품종묘 아니어도 불쌍한 고양이들 많아",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000609.1.16",
          "form": "흠~ 그럴까?",
          "original_form": "흠~ 그럴까?",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.17",
          "form": "오리어때? 반려오리",
          "original_form": "오리어때? 반려오리",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.18",
          "form": "오리도 엄청 귀여워",
          "original_form": "오리도 엄청 귀여워",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.19",
          "form": "코리안 숏헤어라고 ㅋㅋ 길냥이들도 많긴 함",
          "original_form": "코리안 숏헤어라고 ㅋㅋ 길냥이들도 많긴 함",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.20",
          "form": "오리랑 고양이랑 같이 살아도 되나",
          "original_form": "오리랑 고양이랑 같이 살아도 되나",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.21",
          "form": "고양이가 오리를 괴롭히지는 않겠지?",
          "original_form": "고양이가 오리를 괴롭히지는 않겠지?",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.22",
          "form": "오리는 크면 잡아먹어야 할 것 같아서 ㅋ",
          "original_form": "오리는 크면 잡아먹어야 할 것 같아서 ㅋ",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.23",
          "form": "무슨 소리야 반려오리를 왜 잡아먹어 ㅠㅠ",
          "original_form": "무슨 소리야 반려오리를 왜 잡아먹어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000609.1.24",
          "form": "예전에 키우던 병아리 다커서 결국 잡아먹음",
          "original_form": "예전에 키우던 병아리 다커서 결국 잡아먹음",
          "speaker_id": "2",
          "time": "20210819 17:09"
        }
      ]
    }
  ]
