      "utterance": [
        {
          "id": "MDRW2100038621.1.1",
          "form": "스벅 자주가시나요",
          "original_form": "스벅 자주가시나요",
          "speaker_id": "2",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100038621.1.2",
          "form": "ㅎㅎ 요즘 코로나라 아니오",
          "original_form": "ㅎㅎ 요즘 코로나라 아니오",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100038621.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100038621.1.4",
          "form": "저도 요즘은 별로",
          "original_form": "저도 요즘은 별로",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100038621.1.5",
          "form": "조아하는 메뉴잇으신가요",
          "original_form": "조아하는 메뉴잇으신가요",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100038621.1.6",
          "form": "그러시구나, 저도 그래요, 전 그냥 라떼먹어요",
          "original_form": "그러시구나, 저도 그래요, 전 그냥 라떼먹어요",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100038621.1.7",
          "form": "ㅋㅋㅋ 스벅 바닐라크림콜드브루",
          "original_form": "ㅋㅋㅋ 스벅 바닐라크림콜드브루",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100038621.1.8",
          "form": "드셔보셧나요",
          "original_form": "드셔보셧나요",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100038621.1.9",
          "form": "아니오 ㅠ 콜드브루를 안좋아하는데 추천메뉴심 함 머거볼께요 ㅋㅋ",
          "original_form": "아니오 ㅠ 콜드브루를 안좋아하는데 추천메뉴심 함 머거볼께요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100038621.1.10",
          "form": "ㅋㅋㅋ 맛잇어요 전 칼로리도낮구",
          "original_form": "ㅋㅋㅋ 맛잇어요 전 칼로리도낮구",
          "speaker_id": "2",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100038621.1.11",
          "form": "아 시럽이 라이트시럽인가 보네요 공짜 깊티가 생기면 도전해볼께요",
          "original_form": "아 시럽이 라이트시럽인가 보네요 공짜 깊티가 생기면 도전해볼께요",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100038621.1.12",
          "form": "ㅋㅋㅋ 네네 갑자가배고프내요",
          "original_form": "ㅋㅋㅋ 네네 갑자가배고프내요",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100038621.1.13",
          "form": "저두요 ㅋㅋ 그리고 마침 밥때도 다가오고요",
          "original_form": "저두요 ㅋㅋ 그리고 마침 밥때도 다가오고요",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100038621.1.14",
          "form": "저녁 머드실건가요",
          "original_form": "저녁 머드실건가요",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100038621.1.15",
          "form": "아직 일이남아서 모르겠어요",
          "original_form": "아직 일이남아서 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100038621.1.16",
          "form": "name2님이 머드실지 메뉴가 뭐너지 맛있는지 알려주세요 ㅋㅋ",
          "original_form": "&name2&님이 머드실지 메뉴가 뭐너지 맛있는지 알려주세요 ㅋㅋ",
          "speaker_id": "1",
