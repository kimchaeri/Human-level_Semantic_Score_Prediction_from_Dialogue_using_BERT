      "utterance": [
        {
          "id": "MDRW2100007180.1.1",
          "form": "쇼핑 주로 어디서하세요",
          "original_form": "쇼핑 주로 어디서하세요",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007180.1.2",
          "form": "저는 쿠팡이랑 네이버쇼핑 주로 사용해요",
          "original_form": "저는 쿠팡이랑 네이버쇼핑 주로 사용해요",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007180.1.3",
          "form": "쿠팡 로켓배송이요? 진짜 빨리 오나요?",
          "original_form": "쿠팡 로켓배송이요? 진짜 빨리 오나요?",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007180.1.4",
          "form": "네 밤되기전에 시키면 다음날 아침에 와있어요",
          "original_form": "네 밤되기전에 시키면 다음날 아침에 와있어요",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007180.1.5",
          "form": "당근도 가끔씩 이용하구요",
          "original_form": "당근도 가끔씩 이용하구요",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007180.1.6",
          "form": "당근하세요?",
          "original_form": "당근하세요?",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007180.1.7",
          "form": "네 당근 자주해용",
          "original_form": "네 당근 자주해용",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007180.1.8",
          "form": "근데 진짜 싸게팔아야 사가더라구요",
          "original_form": "근데 진짜 싸게팔아야 사가더라구요",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007180.1.9",
          "form": "맞아요 중고나라보다 시세를 더 낮춰야 팔리더라구요",
          "original_form": "맞아요 중고나라보다 시세를 더 낮춰야 팔리더라구요",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007180.1.10",
          "form": "중고나라는 사기당할까봐 물건 받을때까지 걱정이였는데 당근은 바로 거래되니까 좋은거같아요",
          "original_form": "중고나라는 사기당할까봐 물건 받을때까지 걱정이였는데 당근은 바로 거래되니까 좋은거같아요",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007180.1.11",
          "form": "글고 가끔 필요없는거 나눔할때도 좋은거같아요",
          "original_form": "글고 가끔 필요없는거 나눔할때도 좋은거같아요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007180.1.12",
          "form": "맞아요 저도 돈내고 버려야하는거 나눔하면 바로바로 가져가더라구요",
          "original_form": "맞아요 저도 돈내고 버려야하는거 나눔하면 바로바로 가져가더라구요",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007180.1.13",
          "form": "서로 윈윈인거 같아요",
          "original_form": "서로 윈윈인거 같아요",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007180.1.14",
          "form": "거래할때 조선족 만난적 있는데 진짜 똥매너였어요",
          "original_form": "거래할때 조선족 만난적 있는데 진짜 똥매너였어요",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007180.1.15",
          "form": "헉 왠지 만나기 무서울거같아요",
          "original_form": "헉 왠지 만나기 무서울거같아요",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007180.1.16",
          "form": "젤 최근에 뭐사셨어요?",
          "original_form": "젤 최근에 뭐사셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007180.1.17",
          "form": "저도 한국어 잘해서 채팅할땐 몰랐는데 전화와서 왜빨리안오냐고 화내고",
          "original_form": "저도 한국어 잘해서 채팅할땐 몰랐는데 전화와서 왜빨리안오냐고 화내고",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007180.1.18",
          "form": "자기가 딴데 있으면서 저한테 화냈어요",
          "original_form": "자기가 딴데 있으면서 저한테 화냈어요",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007180.1.19",
          "form": "당근에서 산거요?",
          "original_form": "당근에서 산거요?",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007180.1.20",
          "form": "당근아니여도 젤최근에요ㅎㅎ",
          "original_form": "당근아니여도 젤최근에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007180.1.21",
          "form": "신세계상품권 싸게샀어용",
          "original_form": "신세계상품권 싸게샀어용",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007180.1.22",
          "form": "아 아까 더쿠에 마스크팩 500원이라길래샀어요 열장ㅋㅋ",
          "original_form": "아 아까 더쿠에 마스크팩 500원이라길래샀어요 열장ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007180.1.23",
          "form": "뷰티방가시나봐요",
          "original_form": "뷰티방가시나봐요",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007180.1.24",
          "form": "거기가면 진짜 텅장되죠ㅋㅋㅋㅋ",
          "original_form": "거기가면 진짜 텅장되죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007180.1.25",
          "form": "전 저번에 미샤쟁여둔게 아직도 많아요",
          "original_form": "전 저번에 미샤쟁여둔게 아직도 많아요",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007180.1.26",
          "form": "스퀘어에 올라왔었어요ㅋㅋ 텅장될까봐 븉방은 애써 외면중입니당",
          "original_form": "스퀘어에 올라왔었어요ㅋㅋ 텅장될까봐 븉방은 애써 외면중입니당",
          "speaker_id": "1",
