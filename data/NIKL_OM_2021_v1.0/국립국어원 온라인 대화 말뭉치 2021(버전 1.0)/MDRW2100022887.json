      "utterance": [
        {
          "id": "MDRW2100022887.1.1",
          "form": "네 이번년도는 못가고 좀 됐네요ㅜ",
          "original_form": "네 이번년도는 못가고 좀 됐네요ㅜ",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022887.1.2",
          "form": "제주도 저도매년가는데",
          "original_form": "제주도 저도매년가는데",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022887.1.3",
          "form": "올해못갔어요",
          "original_form": "올해못갔어요",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022887.1.4",
          "form": "제주도 근고기였나?그게 그렇게 맛있더라구요",
          "original_form": "제주도 근고기였나?그게 그렇게 맛있더라구요",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022887.1.5",
          "form": "흑돼지맞죠",
          "original_form": "흑돼지맞죠",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022887.1.6",
          "form": "육즙이살살",
          "original_form": "육즙이살살",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022887.1.7",
          "form": "한라산소주가 쎈편인데 아주 술술넘어가더라고요",
          "original_form": "한라산소주가 쎈편인데 아주 술술넘어가더라고요",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022887.1.8",
          "form": "제주도에서 먹어야 제맛인듯요",
          "original_form": "제주도에서 먹어야 제맛인듯요",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022887.1.9",
          "form": "맞아요 제주도가서먹으면",
          "original_form": "맞아요 제주도가서먹으면",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022887.1.10",
          "form": "뭐든맛잇는듯",
          "original_form": "뭐든맛잇는듯",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022887.1.11",
          "form": "근고기땡기네요",
          "original_form": "근고기땡기네요",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022887.1.12",
          "form": "쌈은사치",
          "original_form": "쌈은사치",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022887.1.13",
          "form": "네 제주도만한데가 없더라고요ㅠ",
          "original_form": "네 제주도만한데가 없더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022887.1.14",
          "form": "시내에서드셧나요?",
          "original_form": "시내에서드셧나요?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022887.1.15",
          "form": "시내는 아니었어요!",
          "original_form": "시내는 아니었어요!",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022887.1.16",
          "form": "근고기도 현지인맛집따로잇대요",
          "original_form": "근고기도 현지인맛집따로잇대요",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022887.1.17",
          "form": "관광객가는곳은별루",
          "original_form": "관광객가는곳은별루",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022887.1.18",
          "form": "맞아요 비싸기만하고ㅠ",
          "original_form": "맞아요 비싸기만하고ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022887.1.19",
          "form": "제주도는 차있으면 편하다던데 운전을 못해서 아쉽네요",
          "original_form": "제주도는 차있으면 편하다던데 운전을 못해서 아쉽네요",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022887.1.20",
          "form": "저도뚜벅여행입니다",
          "original_form": "저도뚜벅여행입니다",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022887.1.21",
          "form": "다음 제주도 여행은 꼭 렌트할거에요",
          "original_form": "다음 제주도 여행은 꼭 렌트할거에요",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022887.1.22",
          "form": "그래도버스잘되어있어서",
          "original_form": "그래도버스잘되어있어서",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022887.1.23",
          "form": "괜찮아용",
          "original_form": "괜찮아용",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022887.1.24",
          "form": "주차걱정없는건 좋을듯요ㅎㅎ",
          "original_form": "주차걱정없는건 좋을듯요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:58"
        }
