      "utterance": [
        {
          "id": "MDRW2100018813.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018813.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018813.1.3",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018813.1.4",
          "form": "아니요ㅋ 숨만겨우쉬어요ㅋㅋ",
          "original_form": "아니요ㅋ 숨만겨우쉬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018813.1.5",
          "form": "ㅋㅋㅋㅋㅋ저도 그랬었는데",
          "original_form": "ㅋㅋㅋㅋㅋ저도 그랬었는데",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018813.1.6",
          "form": "요즘은 억지로 움직이려하고 있어요....",
          "original_form": "요즘은 억지로 움직이려하고 있어요....",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018813.1.7",
          "form": "하시는거 있으신가요",
          "original_form": "하시는거 있으신가요",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018813.1.8",
          "form": "저는 홈트랑 등산을 주로 합니당",
          "original_form": "저는 홈트랑 등산을 주로 합니당",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018813.1.9",
          "form": "등산은 그래도 재밌어요...!",
          "original_form": "등산은 그래도 재밌어요...!",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018813.1.10",
          "form": "등산은 회사 야유회에서나 가봤네요ㅋ\r\n다음날 다리 너무아프지않나요",
          "original_form": "등산은 회사 야유회에서나 가봤네요ㅋ\r\n다음날 다리 너무아프지않나요",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018813.1.11",
          "form": "첨엔 하루갔다가 삼일 누워지냈어요ㅋㅋㅋ",
          "original_form": "첨엔 하루갔다가 삼일 누워지냈어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018813.1.12",
          "form": "요즘엔 그래도 요령이 생겨서 살만하더라구요",
          "original_form": "요즘엔 그래도 요령이 생겨서 살만하더라구요",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018813.1.13",
          "form": "많이 다녀보셧나봐요",
          "original_form": "많이 다녀보셧나봐요",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018813.1.14",
          "form": "어느산에가세요?",
          "original_form": "어느산에가세요?",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018813.1.15",
          "form": "저 초보등산러랍니다 ㅎㅎ 주왕산 비슬산 가야산 황매산 가봤어요",
          "original_form": "저 초보등산러랍니다 ㅎㅎ 주왕산 비슬산 가야산 황매산 가봤어요",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018813.1.16",
          "form": "주왕산이랑 황매산 엄청 이뻐요!",
          "original_form": "주왕산이랑 황매산 엄청 이뻐요!",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018813.1.17",
          "form": "와 다처음 들어보는 산이네요ㅋㅋ\r\n몇시간 걸리시는 거에여?",
          "original_form": "와 다처음 들어보는 산이네요ㅋㅋ\r\n몇시간 걸리시는 거에여?",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018813.1.18",
          "form": "한시간에서 여섯시간까지 산마다 다양해요ㅋㅋㅋ",
          "original_form": "한시간에서 여섯시간까지 산마다 다양해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018813.1.19",
          "form": "첨에 멋모르고 가야산 올랐다가 조난당하는줄 알았어요 ㅠㅠ 넘 길어서",
          "original_form": "첨에 멋모르고 가야산 올랐다가 조난당하는줄 알았어요 ㅠㅠ 넘 길어서",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018813.1.20",
          "form": "전 뒷산 약수터나 가는 정도라ㅋㅋ\r\n대단하시네욤",
          "original_form": "전 뒷산 약수터나 가는 정도라ㅋㅋ\r\n대단하시네욤",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018813.1.21",
          "form": "이게 하다보니까 계속 가게 되더라구요",
          "original_form": "이게 하다보니까 계속 가게 되더라구요",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018813.1.22",
          "form": "등산이 잘 맞으셨나봐요",
          "original_form": "등산이 잘 맞으셨나봐요",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018813.1.23",
          "form": "네네 등산 추천드려요 건강해짐!",
          "original_form": "네네 등산 추천드려요 건강해짐!",
          "speaker_id": "1",
          "time": "20210827 12:43"
        }
      ]
    }
  ]
