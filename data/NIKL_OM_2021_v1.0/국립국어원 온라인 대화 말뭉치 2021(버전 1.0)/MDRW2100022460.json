      "utterance": [
        {
          "id": "MDRW2100022460.1.1",
          "form": "저는 허스키 키우거든요",
          "original_form": "저는 허스키 키우거든요",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022460.1.2",
          "form": "허스키좋아하시나요",
          "original_form": "허스키좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022460.1.3",
          "form": "허스키 완전 좋아해요",
          "original_form": "허스키 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.4",
          "form": "허스키 크죠?",
          "original_form": "허스키 크죠?",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.5",
          "form": "아무래도 대형견이다보니",
          "original_form": "아무래도 대형견이다보니",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.6",
          "form": "혹시 눈이 파래요?",
          "original_form": "혹시 눈이 파래요?",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.7",
          "form": "ㅎㅎ 집에서 키우는건 소형견이시죠",
          "original_form": "ㅎㅎ 집에서 키우는건 소형견이시죠",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.8",
          "form": "음 아니요?",
          "original_form": "음 아니요?",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.9",
          "form": "오즈아이에요?",
          "original_form": "오즈아이에요?",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.10",
          "form": "파란눈은 아니에요",
          "original_form": "파란눈은 아니에요",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.11",
          "form": "오즈아이도 아니고",
          "original_form": "오즈아이도 아니고",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.12",
          "form": "그럼 무슨 색이에요?",
          "original_form": "그럼 무슨 색이에요?",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.13",
          "form": "그냥 갈색이요",
          "original_form": "그냥 갈색이요",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022460.1.14",
          "form": "아 ㅋㅋ 저 파란눈의 허스키 만나봤는데",
          "original_form": "아 ㅋㅋ 저 파란눈의 허스키 만나봤는데",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022460.1.15",
          "form": "진짜 너무 잘생겼더라구요",
          "original_form": "진짜 너무 잘생겼더라구요",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022460.1.16",
          "form": "허스키 종류가 잘생기긴했어요",
          "original_form": "허스키 종류가 잘생기긴했어요",
          "speaker_id": "1",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022460.1.17",
          "form": "ㅠㅠ 부럽다 허스키",
          "original_form": "ㅠㅠ 부럽다 허스키",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022460.1.18",
          "form": "말 잘듣는 편인가요?",
          "original_form": "말 잘듣는 편인가요?",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022460.1.19",
          "form": "아니요 사고뭉치에요ㅋㅋ",
          "original_form": "아니요 사고뭉치에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:27"
        }
