      "utterance": [
        {
          "id": "MDRW2100013131.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100013131.1.2",
          "form": "점심을 늦게 먹어서 복숭아 하나 먹었어요",
          "original_form": "점심을 늦게 먹어서 복숭아 하나 먹었어요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100013131.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100013131.1.4",
          "form": "있는 반찬에 밥 먹었어요.",
          "original_form": "있는 반찬에 밥 먹었어요.",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100013131.1.5",
          "form": "과일 좋아하시나요?",
          "original_form": "과일 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100013131.1.6",
          "form": "네 좋아해서 제철 과일 자주 챙겨먹어요",
          "original_form": "네 좋아해서 제철 과일 자주 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100013131.1.7",
          "form": "과일 좋아하세요?",
          "original_form": "과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100013131.1.8",
          "form": "네. 사과는 매일 먹는거 같네요",
          "original_form": "네. 사과는 매일 먹는거 같네요",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100013131.1.9",
          "form": "초록 사과랑 빨간 사과 중에 어떤 걸 더 좋아하시나요?",
          "original_form": "초록 사과랑 빨간 사과 중에 어떤 걸 더 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100013131.1.10",
          "form": "전 빨간 사과요.",
          "original_form": "전 빨간 사과요.",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100013131.1.11",
          "form": "초록색도 나오는 시기엔 잘 사먹어요.",
          "original_form": "초록색도 나오는 시기엔 잘 사먹어요.",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100013131.1.12",
          "form": "둘 다 맛있죠!",
          "original_form": "둘 다 맛있죠!",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100013131.1.13",
          "form": "배달 음식 자주 시켜 드시나요?",
          "original_form": "배달 음식 자주 시켜 드시나요?",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100013131.1.14",
          "form": "네. 치킨을 자주 먹어요",
          "original_form": "네. 치킨을 자주 먹어요",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100013131.1.15",
          "form": "어느 브랜드에서 시켜드세요?",
          "original_form": "어느 브랜드에서 시켜드세요?",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100013131.1.16",
          "form": "요샌 노랑통닭이요",
          "original_form": "요샌 노랑통닭이요",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013131.1.17",
          "form": "좋아하는 치킨 있으세요?",
          "original_form": "좋아하는 치킨 있으세요?",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013131.1.18",
          "form": "굽네 치킨 좋아해요!",
          "original_form": "굽네 치킨 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013131.1.19",
          "form": "갈비천왕 진짜 맛있어요",
          "original_form": "갈비천왕 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100013131.1.20",
          "form": "안먹어봤는데 한번 먹어봐야겠네요",
          "original_form": "안먹어봤는데 한번 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 20:53"
        }
