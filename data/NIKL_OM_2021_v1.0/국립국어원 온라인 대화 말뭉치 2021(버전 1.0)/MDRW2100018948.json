      "utterance": [
        {
          "id": "MDRW2100018948.1.1",
          "form": "괜찮으시면 저 타이틀 추천해주실 수 있나요?",
          "original_form": "괜찮으시면 저 타이틀 추천해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018948.1.2",
          "form": "저는 툼레이더랑 위쳐3,언챠티드 , 라오어, 갓오브워",
          "original_form": "저는 툼레이더랑 위쳐3,언챠티드 , 라오어, 갓오브워",
          "speaker_id": "2",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018948.1.3",
          "form": "재밌게 했어요",
          "original_form": "재밌게 했어요",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018948.1.4",
          "form": "레데리2는 pc로했는데",
          "original_form": "레데리2는 pc로했는데",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018948.1.5",
          "form": "진짜 꼭한번 해보세요 진짜 명작이예요",
          "original_form": "진짜 꼭한번 해보세요 진짜 명작이예요",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018948.1.6",
          "form": "오오..  다 들어본 이름이네요",
          "original_form": "오오..  다 들어본 이름이네요",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018948.1.7",
          "form": "오늘 퇴근하고 가는 길에",
          "original_form": "오늘 퇴근하고 가는 길에",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018948.1.8",
          "form": "플스대여샵 가려했거든요ㅋㅋ",
          "original_form": "플스대여샵 가려했거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018948.1.9",
          "form": "우와 즐거운 쇼핑이겠네요ㅠㅠ",
          "original_form": "우와 즐거운 쇼핑이겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018948.1.10",
          "form": "어떤거 빌리고 싶으세요?",
          "original_form": "어떤거 빌리고 싶으세요?",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018948.1.11",
          "form": "예전에 용과같이2까지 했었는데",
          "original_form": "예전에 용과같이2까지 했었는데",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018948.1.12",
          "form": "3도 빌려볼까했어요ㅋㅋ",
          "original_form": "3도 빌려볼까했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018948.1.13",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018948.1.14",
          "form": "알아요! 시리즈 중에 하나 해봤어요",
          "original_form": "알아요! 시리즈 중에 하나 해봤어요",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018948.1.15",
          "form": "저는 이상하게 거기서 할수있는 미니카게임이",
          "original_form": "저는 이상하게 거기서 할수있는 미니카게임이",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018948.1.16",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018948.1.17",
          "form": "ㅋㅋㅋㅋㅋ맞아요 저도 재밌게했어요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 저도 재밌게했어요",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018948.1.18",
          "form": "그런데 오늘은 레데리? 빌려보려고요!",
          "original_form": "그런데 오늘은 레데리? 빌려보려고요!",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018948.1.19",
          "form": "2라는건 1도 있죠?",
          "original_form": "2라는건 1도 있죠?",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018948.1.20",
          "form": "1도 있는데 한국어판이 없을거예요",
          "original_form": "1도 있는데 한국어판이 없을거예요",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018948.1.21",
          "form": "스토리가 이어지나요?",
          "original_form": "스토리가 이어지나요?",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100018948.1.22",
          "form": "1이전 스토리가 2예요",
          "original_form": "1이전 스토리가 2예요",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018948.1.23",
          "form": "아무것도 모르고2하셔도 재밌으실거예요",
          "original_form": "아무것도 모르고2하셔도 재밌으실거예요",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018948.1.24",
          "form": "아하 그렇군요ㅎㅎ",
          "original_form": "아하 그렇군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100018948.1.25",
          "form": "name2님은 요즘 게임 뭐 하세요?",
          "original_form": "&name2&님은 요즘 게임 뭐 하세요?",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018948.1.26",
          "form": "저는 스위치로 젤다 야숨해요",
          "original_form": "저는 스위치로 젤다 야숨해요",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018948.1.27",
          "form": "오 젤다",
          "original_form": "오 젤다",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018948.1.28",
          "form": "해본적은 없지만 유명하죵",
          "original_form": "해본적은 없지만 유명하죵",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018948.1.29",
          "form": "야숨은 뭔가요",
          "original_form": "야숨은 뭔가요",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018948.1.30",
          "form": "야생의 숨결 줄임말이요",
          "original_form": "야생의 숨결 줄임말이요",
          "speaker_id": "2",
          "time": "20210827 14:27"
        },
        {
          "id": "MDRW2100018948.1.31",
          "form": "야생의숨결을 야숨이라고 불러요",
          "original_form": "야생의숨결을 야숨이라고 불러요",
