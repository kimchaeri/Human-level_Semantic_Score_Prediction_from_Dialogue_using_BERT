      "utterance": [
        {
          "id": "MDRW2100044359.1.1",
          "form": "사랑합니다",
          "original_form": "사랑합니다",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044359.1.2",
          "form": "없인 못살아요",
          "original_form": "없인 못살아요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044359.1.3",
          "form": "야식 자주 드세요?",
          "original_form": "야식 자주 드세요?",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044359.1.4",
          "form": "아 미치죠",
          "original_form": "아 미치죠",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044359.1.5",
          "form": "주로 뭐 시켜요?",
          "original_form": "주로 뭐 시켜요?",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.6",
          "form": "그래서 역류성식도염이 생긴건 안비밀",
          "original_form": "그래서 역류성식도염이 생긴건 안비밀",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.7",
          "form": "저 다양하죠",
          "original_form": "저 다양하죠",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.8",
          "form": "아니ㅋㅋ 좀 자제하세요",
          "original_form": "아니ㅋㅋ 좀 자제하세요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.9",
          "form": "오늘은 디저트류가 당기네요",
          "original_form": "오늘은 디저트류가 당기네요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.10",
          "form": "흑당밀크티",
          "original_form": "흑당밀크티",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.11",
          "form": "오오 단거 좋아하세요?",
          "original_form": "오오 단거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.12",
          "form": "와플",
          "original_form": "와플",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044359.1.13",
          "form": "충치로 임플란트 할 정도로  좋아합니다",
          "original_form": "충치로 임플란트 할 정도로  좋아합니다",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044359.1.14",
          "form": "name2씨는 뭐 좋아하시나요?",
          "original_form": "&name2&씨는 뭐 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044359.1.15",
          "form": "ㅋㅋ 전 해산물 좋아해요!",
          "original_form": "ㅋㅋ 전 해산물 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044359.1.16",
          "form": "회 멍게 이런거",
          "original_form": "회 멍게 이런거",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044359.1.17",
          "form": "회 좋아하시나요",
          "original_form": "회 좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044359.1.18",
          "form": "건강식이네요",
          "original_form": "건강식이네요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044359.1.19",
          "form": "야식으로는 아니고.. 피부 좋아지고 싶을 때 먹습니다",
          "original_form": "야식으로는 아니고.. 피부 좋아지고 싶을 때 먹습니다",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044359.1.20",
          "form": "소주 안주로 좋아해요",
          "original_form": "소주 안주로 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044359.1.21",
          "form": "아니 회가 건강식이요...?",
          "original_form": "아니 회가 건강식이요...?",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044359.1.22",
          "form": "첨 듣네요ㅋㅋ",
          "original_form": "첨 듣네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044359.1.23",
          "form": "소주 잘드시나요",
          "original_form": "소주 잘드시나요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044359.1.24",
          "form": "회 먹으면 피부가 반질반질",
          "original_form": "회 먹으면 피부가 반질반질",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044359.1.25",
          "form": "네 주량 세잔",
          "original_form": "네 주량 세잔",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044359.1.26",
          "form": "알쓰",
          "original_form": "알쓰",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044359.1.27",
          "form": "그래서 회도 세점?ㅜ",
          "original_form": "그래서 회도 세점?ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044359.1.28",
          "form": "아니ㅋ",
          "original_form": "아니ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044359.1.29",
          "form": "회는 보통 무슨 회를 좋아하시죠?",
          "original_form": "회는 보통 무슨 회를 좋아하시죠?",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044359.1.30",
          "form": "그런데 소주를 입에 올리다니",
          "original_form": "그런데 소주를 입에 올리다니",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044359.1.31",
          "form": "세꼬시류는 다 좋아요",
          "original_form": "세꼬시류는 다 좋아요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044359.1.32",
          "form": "그래서 집에서 많이 비웃어요ㅋ",
          "original_form": "그래서 집에서 많이 비웃어요ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:30"
        }
