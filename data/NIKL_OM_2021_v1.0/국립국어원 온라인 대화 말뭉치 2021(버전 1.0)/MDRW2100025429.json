      "utterance": [
        {
          "id": "MDRW2100025429.1.1",
          "form": "여행 좋아하시나요??",
          "original_form": "여행 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025429.1.2",
          "form": "가끔 기분전환에는 여행이 최고인것같아요",
          "original_form": "가끔 기분전환에는 여행이 최고인것같아요",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025429.1.3",
          "form": "최근에 가신 여행지는 어디셨나요?",
          "original_form": "최근에 가신 여행지는 어디셨나요?",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025429.1.4",
          "form": "강원도요",
          "original_form": "강원도요",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025429.1.5",
          "form": "가깝고 무난하죠",
          "original_form": "가깝고 무난하죠",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025429.1.6",
          "form": "저도 강릉 가봤어요 참좋죠??",
          "original_form": "저도 강릉 가봤어요 참좋죠??",
          "speaker_id": "1",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025429.1.7",
          "form": "그쵸 속초도 좋아요",
          "original_form": "그쵸 속초도 좋아요",
          "speaker_id": "2",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025429.1.8",
          "form": "바다가 짱이에요",
          "original_form": "바다가 짱이에요",
          "speaker_id": "2",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025429.1.9",
          "form": "그죠!!! 맛집도 많고요!! 거기 카페 엄청 크고 좋더라고요",
          "original_form": "그죠!!! 맛집도 많고요!! 거기 카페 엄청 크고 좋더라고요",
          "speaker_id": "1",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025429.1.10",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025429.1.11",
          "form": "차가없어서ㅠㅠ 자주 못가는게 아쉬워요",
          "original_form": "차가없어서ㅠㅠ 자주 못가는게 아쉬워요",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025429.1.12",
          "form": "뚜벅이는 눈물만...ㅜㅠ",
          "original_form": "뚜벅이는 눈물만...ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025429.1.13",
          "form": "해외는 가고싶은 곳 있으세요??",
          "original_form": "해외는 가고싶은 곳 있으세요??",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025429.1.14",
          "form": "지금은 태국이나 터키요",
          "original_form": "지금은 태국이나 터키요",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025429.1.15",
          "form": "한국물가 미쳤잖아요ㅠ",
          "original_form": "한국물가 미쳤잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025429.1.16",
          "form": "해외에서 즐기고 싶네요",
          "original_form": "해외에서 즐기고 싶네요",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025429.1.17",
          "form": "아 저도 터키 너무 가보고싶었는데ㅜㅜ",
          "original_form": "아 저도 터키 너무 가보고싶었는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025429.1.18",
          "form": "요즘 치안이ㅜㅜ",
          "original_form": "요즘 치안이ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025429.1.19",
          "form": "그쵸?ㅠㅠ 제약이 한두가지가 아니에요",
          "original_form": "그쵸?ㅠㅠ 제약이 한두가지가 아니에요",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025429.1.20",
          "form": "증말 아쉬워요",
          "original_form": "증말 아쉬워요",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025429.1.21",
          "form": "전 터키랑 포르투갈이요 여기도 맛나고 물가도 싸대요!",
          "original_form": "전 터키랑 포르투갈이요 여기도 맛나고 물가도 싸대요!",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025429.1.22",
          "form": "유럽ㅠㅠ가보셨어요?",
          "original_form": "유럽ㅠㅠ가보셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025429.1.23",
          "form": "유럽 한번도 못가봤어요ㅜㅠ 가려고할때 딱!!!",
          "original_form": "유럽 한번도 못가봤어요ㅜㅠ 가려고할때 딱!!!",
          "speaker_id": "1",
          "time": "20210901 18:27"
        }
      ]
    }
