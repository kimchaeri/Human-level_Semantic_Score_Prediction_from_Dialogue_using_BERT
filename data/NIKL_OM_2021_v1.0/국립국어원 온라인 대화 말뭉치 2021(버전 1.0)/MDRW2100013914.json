        {
          "id": "MDRW2100013914.1.3",
          "form": "학교다닐때 동아리 뭐였어",
          "original_form": "학교다닐때 동아리 뭐였어",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013914.1.4",
          "form": "나는 타이포그라피 동아리",
          "original_form": "나는 타이포그라피 동아리",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013914.1.5",
          "form": "나는 시 동아시였오",
          "original_form": "나는 시 동아시였오",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013914.1.6",
          "form": "시선이라고 ^^..",
          "original_form": "시선이라고 ^^..",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013914.1.7",
          "form": "오 시 좋지..과제하느라 동아리과제 엉망으로해갔는데..",
          "original_form": "오 시 좋지..과제하느라 동아리과제 엉망으로해갔는데..",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013914.1.8",
          "form": "나는 과제도 동아리도 완벽했지",
          "original_form": "나는 과제도 동아리도 완벽했지",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013914.1.9",
          "form": "시험도 완벽했는데",
          "original_form": "시험도 완벽했는데",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013914.1.10",
          "form": "시험 잘봤니?",
          "original_form": "시험 잘봤니?",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013914.1.11",
          "form": "ㅎㅎ..",
          "original_form": "ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100013914.1.12",
          "form": "나름 평타였어",
          "original_form": "나름 평타였어",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100013914.1.13",
          "form": "나는 학점이 4.41이었단다",
          "original_form": "나는 학점이 4.41이었단다",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100013914.1.14",
          "form": "정말 안궁금하다",
          "original_form": "정말 안궁금하다",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013914.1.15",
          "form": "그런데 너는 무슨 과였니",
          "original_form": "그런데 너는 무슨 과였니",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013914.1.16",
          "form": "3점이상은 다 잘한거야",
          "original_form": "3점이상은 다 잘한거야",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013914.1.17",
          "form": "상대평가였다고",
          "original_form": "상대평가였다고",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013914.1.18",
          "form": "알겠어 그렇다칠게",
          "original_form": "알겠어 그렇다칠게",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013914.1.19",
          "form": "나는 문예창작과",
          "original_form": "나는 문예창작과",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013914.1.20",
          "form": "시와 소설을 배우는 과란다",
          "original_form": "시와 소설을 배우는 과란다",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100013914.1.21",
          "form": "ㅋㅋ...",
          "original_form": "ㅋㅋ...",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100013914.1.22",
          "form": "타이포그라피 너무 어려워서 님이 알까 모르겠어요 ^^",
