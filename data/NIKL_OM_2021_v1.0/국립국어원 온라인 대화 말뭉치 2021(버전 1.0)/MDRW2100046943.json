        },
        {
          "id": "MDRW2100046943.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.3",
          "form": "방가워요",
          "original_form": "방가워요",
          "speaker_id": "1",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.4",
          "form": "불금인데 뭐하세요?",
          "original_form": "불금인데 뭐하세요?",
          "speaker_id": "2",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.5",
          "form": "집어가야죠",
          "original_form": "집어가야죠",
          "speaker_id": "1",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.6",
          "form": "내일단풍구경가요",
          "original_form": "내일단풍구경가요",
          "speaker_id": "1",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.7",
          "form": "헉 저도 내일 단풍구경가요",
          "original_form": "헉 저도 내일 단풍구경가요",
          "speaker_id": "2",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.8",
          "form": "어디로 가셔요?",
          "original_form": "어디로 가셔요?",
          "speaker_id": "2",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.9",
          "form": "북한산으로가요",
          "original_form": "북한산으로가요",
          "speaker_id": "1",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.10",
          "form": "등산 가시는 거예요~?",
          "original_form": "등산 가시는 거예요~?",
          "speaker_id": "2",
          "time": "20211105 18:00"
        },
        {
          "id": "MDRW2100046943.1.11",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20211105 18:01"
        },
        {
          "id": "MDRW2100046943.1.12",
          "form": "초입로에서놀고",
          "original_form": "초입로에서놀고",
          "speaker_id": "1",
          "time": "20211105 18:01"
        },
        {
          "id": "MDRW2100046943.1.13",
          "form": "단풍보면서커피한잔",
          "original_form": "단풍보면서커피한잔",
          "speaker_id": "1",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046943.1.14",
          "form": "아하 ㅎㅎ 그런 스타일!",
          "original_form": "아하 ㅎㅎ 그런 스타일!",
          "speaker_id": "2",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046943.1.15",
          "form": "이게목표에요",
          "original_form": "이게목표에요",
          "speaker_id": "1",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046943.1.16",
          "form": "저도 등산보다 그런게 좋아요",
          "original_form": "저도 등산보다 그런게 좋아요",
          "speaker_id": "2",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046943.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046943.1.18",
          "form": "저랑취향이비슷하네요",
          "original_form": "저랑취향이비슷하네요",
          "speaker_id": "1",
          "time": "20211105 18:03"
        },
        {
          "id": "MDRW2100046943.1.19",
          "form": "네 등산은 너무 힘들어서 단풍 구경할 힘이 없을 것 같아요 ㅋㅋㅋ",
          "original_form": "네 등산은 너무 힘들어서 단풍 구경할 힘이 없을 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 18:03"
        },
        {
          "id": "MDRW2100046943.1.20",
          "form": "즐거운불금보내실계획이신가요",
          "original_form": "즐거운불금보내실계획이신가요",
          "speaker_id": "1",
          "time": "20211105 18:03"
        },
        {
          "id": "MDRW2100046943.1.21",
          "form": "저두등산시러해요",
          "original_form": "저두등산시러해요",
          "speaker_id": "1",
          "time": "20211105 18:04"
        },
        {
          "id": "MDRW2100046943.1.22",
          "form": "아뇨 ㅋㅋㅋ 저도 집이에요!!",
          "original_form": "아뇨 ㅋㅋㅋ 저도 집이에요!!",
          "speaker_id": "2",
          "time": "20211105 18:04"
        },
        {
          "id": "MDRW2100046943.1.23",
          "form": "내일 단풍구경을 위해서 체력을 아껴둬야해요",
          "original_form": "내일 단풍구경을 위해서 체력을 아껴둬야해요",
          "speaker_id": "2",
          "time": "20211105 18:04"
        },
        {
          "id": "MDRW2100046943.1.24",
          "form": "네그럴려구요",
          "original_form": "네그럴려구요",
          "speaker_id": "1",
          "time": "20211105 18:04"
        }
      ]
