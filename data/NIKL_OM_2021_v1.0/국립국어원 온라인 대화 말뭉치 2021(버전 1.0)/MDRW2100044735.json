      "utterance": [
        {
          "id": "MDRW2100044735.1.1",
          "form": "엉 이미숙이 여배우로 나왔어 이 드라마에서",
          "original_form": "엉 이미숙이 여배우로 나왔어 이 드라마에서",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044735.1.2",
          "form": "교통사고 목격자 였는데 모른척하고",
          "original_form": "교통사고 목격자 였는데 모른척하고",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044735.1.3",
          "form": "그다음  조정석의 출연작이",
          "original_form": "그다음  조정석의 출연작이",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044735.1.4",
          "form": "뭐예요?  드라마예요?",
          "original_form": "뭐예요?  드라마예요?",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044735.1.5",
          "form": "오나의귀신님",
          "original_form": "오나의귀신님",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044735.1.6",
          "form": "아 재밌었어요",
          "original_form": "아 재밌었어요",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044735.1.7",
          "form": "이거 인기가 별로 없었네 생각보다",
          "original_form": "이거 인기가 별로 없었네 생각보다",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044735.1.8",
          "form": "난 재밌게 봤는데",
          "original_form": "난 재밌게 봤는데",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044735.1.9",
          "form": "박보영이랑 찍었자나요",
          "original_form": "박보영이랑 찍었자나요",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044735.1.10",
          "form": "16부작 7.3%",
          "original_form": "16부작 7.3%",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044735.1.11",
          "form": "케이블이라 그정도도 잘 나온거래요",
          "original_form": "케이블이라 그정도도 잘 나온거래요",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044735.1.12",
          "form": "엉 박보영이 빙의 되는 주방보조역",
          "original_form": "엉 박보영이 빙의 되는 주방보조역",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044735.1.13",
          "form": "맞아요. 박보영 은근 웃겼는데",
          "original_form": "맞아요. 박보영 은근 웃겼는데",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
