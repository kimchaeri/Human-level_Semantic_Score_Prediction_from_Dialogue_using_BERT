      "utterance": [
        {
          "id": "MDRW2100017536.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017536.1.2",
          "form": "쇼핑 좋아해요?",
          "original_form": "쇼핑 좋아해요?",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017536.1.3",
          "form": "저는 좋아하는데 제 통장은 싫어해요",
          "original_form": "저는 좋아하는데 제 통장은 싫어해요",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017536.1.4",
          "form": "앗 ㅋㅋ 재치 있으시네요.",
          "original_form": "앗 ㅋㅋ 재치 있으시네요.",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017536.1.5",
          "form": "최근에 어떤거 사셨어요?",
          "original_form": "최근에 어떤거 사셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017536.1.6",
          "form": "가을 옷을 미리 좀 샀네요 ㅎㅎㅎ",
          "original_form": "가을 옷을 미리 좀 샀네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017536.1.7",
          "form": "아우터요? 아니면 이너?",
          "original_form": "아우터요? 아니면 이너?",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017536.1.8",
          "form": "아우터요! 코트하나 장만했어요",
          "original_form": "아우터요! 코트하나 장만했어요",
          "speaker_id": "2",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100017536.1.9",
          "form": "오~ 무슨 코트 사셨어요?",
          "original_form": "오~ 무슨 코트 사셨어요?",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100017536.1.10",
          "form": "트렌치 코트요! ㅎㅎ싸개 건졌는대 넘 예뻐요",
          "original_form": "트렌치 코트요! ㅎㅎ싸개 건졌는대 넘 예뻐요",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100017536.1.11",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100017536.1.12",
          "form": "어떤 색깔 사셨어요?ㅎㅎ",
          "original_form": "어떤 색깔 사셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100017536.1.13",
          "form": "베이지색이요",
          "original_form": "베이지색이요",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100017536.1.14",
          "form": "핑크랑 고민하다가.. 무난하게 ㅎㅎ",
          "original_form": "핑크랑 고민하다가.. 무난하게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100017536.1.15",
          "form": "아 베이지만~",
          "original_form": "아 베이지만~",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100017536.1.16",
          "form": "핑크도 예쁠 것 같은데 이물질 묻으면 티날 것 같아요 ㅠ",
          "original_form": "핑크도 예쁠 것 같은데 이물질 묻으면 티날 것 같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100017536.1.17",
          "form": "맞아요 제가 뭘 잘 묻히고다녀서 ㅋㅋㅋ",
          "original_form": "맞아요 제가 뭘 잘 묻히고다녀서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100017536.1.18",
          "form": "최근에 뭐 사신것없어요?",
          "original_form": "최근에 뭐 사신것없어요?",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100017536.1.19",
          "form": "저 옷은 산 건 없구요",
          "original_form": "저 옷은 산 건 없구요",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100017536.1.20",
          "form": "생수는 샀어요~",
          "original_form": "생수는 샀어요~",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100017536.1.21",
          "form": "그리고 제로음료들 쟁여놨어요 ㅎㅎㅎㅎ",
          "original_form": "그리고 제로음료들 쟁여놨어요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100017536.1.22",
          "form": "제로 음료 좋죠!",
          "original_form": "제로 음료 좋죠!",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100017536.1.23",
          "form": "사이다vs콜라",
          "original_form": "사이다vs콜라",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100017536.1.24",
          "form": "콜라요 ㅎㅎ 제로콜라도 다양하더라구요",
          "original_form": "콜라요 ㅎㅎ 제로콜라도 다양하더라구요",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100017536.1.25",
          "form": "오 어떻개 다양한대요?",
          "original_form": "오 어떻개 다양한대요?",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100017536.1.26",
          "form": "저는 제로 음료 가아끔만먹어서",
          "original_form": "저는 제로 음료 가아끔만먹어서",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100017536.1.27",
          "form": "펩시제로 라임향, 코카콜라제로, 815콜라제로 무슨콜라제로 이렇게 있던데요?",
          "original_form": "펩시제로 라임향, 코카콜라제로, 815콜라제로 무슨콜라제로 이렇게 있던데요?",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100017536.1.28",
          "form": "전 코카콜라제로가 젤 나았어요 ㅎㅎ",
          "original_form": "전 코카콜라제로가 젤 나았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100017536.1.29",
          "form": "와 뭐가 진짜 많네요",
          "original_form": "와 뭐가 진짜 많네요",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100017536.1.30",
          "form": "그쵸~ 사이다도 다양하더라구요",
          "original_form": "그쵸~ 사이다도 다양하더라구요",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100017536.1.31",
          "form": "맞아 에이드? 그런것도 있던것같던데",
          "original_form": "맞아 에이드? 그런것도 있던것같던데",
          "speaker_id": "2",
          "time": "20210827 16:54"
        }
      ]
