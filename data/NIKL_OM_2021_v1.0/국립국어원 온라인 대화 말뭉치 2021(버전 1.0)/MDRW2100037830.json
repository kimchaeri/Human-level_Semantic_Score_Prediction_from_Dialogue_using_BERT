      "utterance": [
        {
          "id": "MDRW2100037830.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100037830.1.2",
          "form": "지금 막 먹었어요",
          "original_form": "지금 막 먹었어요",
          "speaker_id": "2",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100037830.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037830.1.4",
          "form": "저두 지금 먹구 복숭아 먹고 있어요 ㅎㅎㅎ",
          "original_form": "저두 지금 먹구 복숭아 먹고 있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100037830.1.5",
          "form": "있는 반찬에 소세지 구워서 케찹찍어먹었는데 오랜만에 먹으니까 맛있어여 ㅋㅋㅋㅋ",
          "original_form": "있는 반찬에 소세지 구워서 케찹찍어먹었는데 오랜만에 먹으니까 맛있어여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100037830.1.6",
          "form": "무슨반찬에 드셨어용?",
          "original_form": "무슨반찬에 드셨어용?",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100037830.1.7",
          "form": "총각김치랑 김치전이요ㅋㅋㅋ",
          "original_form": "총각김치랑 김치전이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100037830.1.8",
          "form": "김치만 먹었네요",
          "original_form": "김치만 먹었네요",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100037830.1.9",
          "form": "엌ㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037830.1.10",
          "form": "김치매니아 등극...ㅋㅋㅋ",
          "original_form": "김치매니아 등극...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037830.1.11",
          "form": "집에 있으면 배추김치 열무김치 파김치 오이소박이...",
          "original_form": "집에 있으면 배추김치 열무김치 파김치 오이소박이...",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037830.1.12",
          "form": "김치로만 가짓수차지하더라구요",
          "original_form": "김치로만 가짓수차지하더라구요",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037830.1.13",
          "form": "우와, 대단한대요",
          "original_form": "우와, 대단한대요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100037830.1.14",
          "form": "다 담그신거에요?",
          "original_form": "다 담그신거에요?",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100037830.1.15",
          "form": "아니옄ㅋㅋ 아... 저희엄마가..? ㅋㅋㅋㅋ",
          "original_form": "아니옄ㅋㅋ 아... 저희엄마가..? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100037830.1.16",
          "form": "김치종류가 늘 많아요.. 저는 먹을게 없어보이는데ㅜㅜ 저희부모님은 너무나 잘 드시는ㅋㅋㅋㅋ",
          "original_form": "김치종류가 늘 많아요.. 저는 먹을게 없어보이는데ㅜㅜ 저희부모님은 너무나 잘 드시는ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037830.1.17",
          "form": "좋아하는 김치종류 있으세요?",
          "original_form": "좋아하는 김치종류 있으세요?",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037830.1.18",
          "form": "음.. 김치 종류는 크게 안가리는데요",
          "original_form": "음.. 김치 종류는 크게 안가리는데요",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037830.1.19",
          "form": "신김치보단 생김치?를 더 좋아해요",
          "original_form": "신김치보단 생김치?를 더 좋아해요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037830.1.20",
          "form": "특히 김장날 막 무친 김치 집어먹는거 최고! ㅋㅋ",
          "original_form": "특히 김장날 막 무친 김치 집어먹는거 최고! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037830.1.21",
          "form": "겉절이!!!!",
          "original_form": "겉절이!!!!",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037830.1.22",
          "form": "샤브샤브에 겉절이 먹는거 정말 좋아해요! ㅋㅋㅋㅋㅋ",
          "original_form": "샤브샤브에 겉절이 먹는거 정말 좋아해요! ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037830.1.23",
          "form": "등촌 샤브 좋아하세요?",
          "original_form": "등촌 샤브 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037830.1.24",
          "form": "와, 최고죠",
          "original_form": "와, 최고죠",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037830.1.25",
          "form": "등촌 샤브는 안가봤어요",
          "original_form": "등촌 샤브는 안가봤어요",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037830.1.26",
          "form": "샤브샤브 좋아하는데",
          "original_form": "샤브샤브 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037830.1.27",
          "form": "저 여기 겉절이랑 샤브먹는거 너무 좋아여... 칼국수랑",
          "original_form": "저 여기 겉절이랑 샤브먹는거 너무 좋아여... 칼국수랑{emoji:♡}",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037830.1.28",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037830.1.29",
          "form": "칼국수에 겉절이 최고죠",
          "original_form": "칼국수에 겉절이 최고죠",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037830.1.30",
          "form": "등촌 샤브라는 곳이 겉절이가 맛있나봐요",
          "original_form": "등촌 샤브라는 곳이 겉절이가 맛있나봐요",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037830.1.31",
          "form": "샤브도 맛있어요 ㅋㅋㅋ",
          "original_form": "샤브도 맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037830.1.32",
          "form": "여기 샤브랑 겉절이 같이먹는걸 좋아해요",
          "original_form": "여기 샤브랑 겉절이 같이먹는걸 좋아해요",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037830.1.33",
          "form": "오오, name1님 맛집리스트인가봐요",
          "original_form": "오오, &name1&님 맛집리스트인가봐요",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037830.1.34",
          "form": "전 학교 근처에 있는 샤브집이",
          "original_form": "전 학교 근처에 있는 샤브집이",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037830.1.35",
          "form": "어바웃샤브라서 거길 젤 많이 가봤어요",
          "original_form": "어바웃샤브라서 거길 젤 많이 가봤어요",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037830.1.36",
          "form": "저희 동네에 있는 곳인데 요기 체인 엄청많아요 ㅎㅎ",
          "original_form": "저희 동네에 있는 곳인데 요기 체인 엄청많아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100037830.1.37",
          "form": "저희엄마 힐링푸드여서 학생때부터 따라갔었는데 저희동네 샤브집 볶음밥이 유달리 일품이에요 ㅎㅎㅎ",
          "original_form": "저희엄마 힐링푸드여서 학생때부터 따라갔었는데 저희동네 샤브집 볶음밥이 유달리 일품이에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037830.1.38",
          "form": "다른지점갈거 거기가서 먹구 그래요!",
