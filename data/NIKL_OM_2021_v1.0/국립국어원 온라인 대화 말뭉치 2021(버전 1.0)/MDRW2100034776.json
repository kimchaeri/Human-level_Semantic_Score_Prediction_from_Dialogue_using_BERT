      "utterance": [
        {
          "id": "MDRW2100034776.1.1",
          "form": "안녕하세용 왓챠나 넷플릭스 보세요?",
          "original_form": "안녕하세용 왓챠나 넷플릭스 보세요?",
          "speaker_id": "1",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100034776.1.2",
          "form": "네 넷플 봐요 ㅎㅎ !",
          "original_form": "네 넷플 봐요 ㅎㅎ !",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034776.1.3",
          "form": "오 최근에 뭐보셨어용?",
          "original_form": "오 최근에 뭐보셨어용?",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100034776.1.4",
          "form": "저 체인지데이즈 봤어요 !",
          "original_form": "저 체인지데이즈 봤어요 !",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034776.1.5",
          "form": "장르가 뭔가용?",
          "original_form": "장르가 뭔가용?",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034776.1.6",
          "form": "드라마는 아니구요 !",
          "original_form": "드라마는 아니구요 !",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034776.1.7",
          "form": "액션인가용?",
          "original_form": "액션인가용?",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034776.1.8",
          "form": "그 연인 바꿔서 데이트하는",
          "original_form": "그 연인 바꿔서 데이트하는",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034776.1.9",
          "form": "내용이에요 ㅋㅋ",
          "original_form": "내용이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034776.1.10",
          "form": "아ㅘ",
          "original_form": "아ㅘ",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034776.1.11",
          "form": "로맨스겠군요ㅋㅋㅋ",
          "original_form": "로맨스겠군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034776.1.12",
          "form": "저는 넷플릭스로 스위트홈 봤어요",
          "original_form": "저는 넷플릭스로 스위트홈 봤어요",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034776.1.13",
          "form": "아 그거 송강 나오는거죠 ?!",
          "original_form": "아 그거 송강 나오는거죠 ?!",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034776.1.14",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034776.1.15",
          "form": "괴물 나오고",
          "original_form": "괴물 나오고",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034776.1.16",
          "form": "송강나오고 그 드라마에용",
          "original_form": "송강나오고 그 드라마에용",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034776.1.17",
          "form": "재밌다고해서 엄청 추천받았어요 !",
          "original_form": "재밌다고해서 엄청 추천받았어요 !",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034776.1.18",
          "form": "괜찮은가요?",
          "original_form": "괜찮은가요?",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034776.1.19",
          "form": "조금 잔인하긴한데",
          "original_form": "조금 잔인하긴한데",
          "speaker_id": "1",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100034776.1.20",
          "form": "스토리는 되게 신박했어요!",
          "original_form": "스토리는 되게 신박했어요!",
          "speaker_id": "1",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100034776.1.21",
          "form": "한번쯤은 볼만한것같아요",
          "original_form": "한번쯤은 볼만한것같아요",
          "speaker_id": "1",
          "time": "20210828 19:39"
        }
      ]
