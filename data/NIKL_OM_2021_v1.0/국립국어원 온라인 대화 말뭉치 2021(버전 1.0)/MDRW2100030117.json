      "utterance": [
        {
          "id": "MDRW2100030117.1.1",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030117.1.2",
          "form": "좋아는 하는데 자주 먹진 않아요",
          "original_form": "좋아는 하는데 자주 먹진 않아요",
          "speaker_id": "2",
          "time": "20210904 07:02"
        },
        {
          "id": "MDRW2100030117.1.3",
          "form": "저는 자주",
          "original_form": "저는 자주",
          "speaker_id": "1",
          "time": "20210904 08:41"
        },
        {
          "id": "MDRW2100030117.1.4",
          "form": "주로 뭐 드시나요",
          "original_form": "주로 뭐 드시나요",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030117.1.5",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030117.1.6",
          "form": "하기야",
          "original_form": "하기야",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100030117.1.7",
          "form": "배달 음식은 치킨이죠",
          "original_form": "배달 음식은 치킨이죠",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100030117.1.8",
          "form": "당연",
          "original_form": "당연",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030117.1.9",
          "form": "집에서 할 수 있을만한 메뉴가 아닌지라",
          "original_form": "집에서 할 수 있을만한 메뉴가 아닌지라",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030117.1.10",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030117.1.11",
          "form": "그러면 브랜드는...?",
          "original_form": "그러면 브랜드는...?",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100030117.1.12",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100030117.1.13",
          "form": "아 네네 좋죠",
          "original_form": "아 네네 좋죠",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100030117.1.14",
          "form": "다만 저는 그",
          "original_form": "다만 저는 그",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100030117.1.15",
          "form": "보드람류 좋아하긴 하는데",
          "original_form": "보드람류 좋아하긴 하는데",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100030117.1.16",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100030117.1.17",
          "form": "양념 있는 거 좋아하시나요",
          "original_form": "양념 있는 거 좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100030117.1.18",
          "form": "저는 양념 보다는 후라이드를 좋아합니다",
          "original_form": "저는 양념 보다는 후라이드를 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 18:06"
        }
