      "utterance": [
        {
          "id": "MDRW2100040012.1.1",
          "form": "일단 타타라 모노가타리 좀 해야겠어",
          "original_form": "일단 타타라 모노가타리 좀 해야겠어",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100040012.1.2",
          "form": "이거 대체 언제 끝나",
          "original_form": "이거 대체 언제 끝나",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100040012.1.3",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100040012.1.4",
          "form": "어 그거 아직도 안끝남?",
          "original_form": "어 그거 아직도 안끝남?",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.5",
          "form": "ㅇㅇ비록 내가",
          "original_form": "ㅇㅇ비록 내가",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.6",
          "form": "하루 이틀정도",
          "original_form": "하루 이틀정도",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.7",
          "form": "잊어버리고 넘어간적이 있다지만",
          "original_form": "잊어버리고 넘어간적이 있다지만",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.8",
          "form": "매일매일 했으면 대충 5일쯤에 끝났던거같은데...",
          "original_form": "매일매일 했으면 대충 5일쯤에 끝났던거같은데...",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.9",
          "form": "왤케 안끝나는거야",
          "original_form": "왤케 안끝나는거야",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.10",
          "form": "일주일 걸리는것같아",
          "original_form": "일주일 걸리는것같아",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.11",
          "form": "그정도였나...?",
          "original_form": "그정도였나...?",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040012.1.12",
          "form": "우인단이랑 전투 했어?",
          "original_form": "우인단이랑 전투 했어?",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100040012.1.13",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100040012.1.14",
          "form": "그 사진찍을때 나오는 애들?",
          "original_form": "그 사진찍을때 나오는 애들?",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100040012.1.15",
          "form": "걔네랑은 싸우긴 했지",
          "original_form": "걔네랑은 싸우긴 했지",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100040012.1.16",
          "form": "이거 최종전투? 하면",
          "original_form": "이거 최종전투? 하면",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040012.1.17",
          "form": "그 지역 전류 해제됨",
          "original_form": "그 지역 전류 해제됨",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040012.1.18",
          "form": "아직 해제 안됐지?",
          "original_form": "아직 해제 안됐지?",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040012.1.19",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040012.1.20",
          "form": "안됐어",
          "original_form": "안됐어",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040012.1.21",
          "form": "번개장판",
          "original_form": "번개장판",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040012.1.22",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040012.1.23",
          "form": "지금 뭐 하는 단계야??",
          "original_form": "지금 뭐 하는 단계야??",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040012.1.24",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040012.1.25",
          "form": "접속 해보면 알겟지.......",
          "original_form": "접속 해보면 알겟지.......",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040012.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040012.1.27",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040012.1.28",
          "form": "멘스를 밀어봐",
          "original_form": "멘스를 밀어봐",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040012.1.29",
          "form": "물음표의 시작은",
          "original_form": "물음표의 시작은",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040012.1.30",
          "form": "지금부터임",
          "original_form": "지금부터임",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040012.1.31",
          "form": "ㅇㅋ....",
          "original_form": "ㅇㅋ....",
          "speaker_id": "2",
          "time": "20210904 00:26"
        }
      ]
    }
  ]
