      "utterance": [
        {
          "id": "MDRW2100003125.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:04"
        },
        {
          "id": "MDRW2100003125.1.2",
          "form": "여행 좋아 하시나봐요 ㅎㅎ",
          "original_form": "여행 좋아 하시나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:04"
        },
        {
          "id": "MDRW2100003125.1.3",
          "form": "40대 첨봤어요 반갑습니다 ㅎㅎ",
          "original_form": "40대 첨봤어요 반갑습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:05"
        },
        {
          "id": "MDRW2100003125.1.4",
          "form": "전 요번에 휴가는 코로나에 남편도 바쁘 고 못갔어요",
          "original_form": "전 요번에 휴가는 코로나에 남편도 바쁘 고 못갔어요",
          "speaker_id": "1",
          "time": "20210820 19:05"
        },
        {
          "id": "MDRW2100003125.1.5",
          "form": "코로나 때문에 어디 못 가죠.ㅜㅜ",
          "original_form": "코로나 때문에 어디 못 가죠.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003125.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100003125.1.7",
          "form": "근데 잘가시는분이 많아요",
          "original_form": "근데 잘가시는분이 많아요",
          "speaker_id": "1",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100003125.1.8",
          "form": "전  요새 겁나서 안가요",
          "original_form": "전  요새 겁나서 안가요",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003125.1.9",
          "form": "저도 웬만허면 안 가요.ㅜㅜ",
          "original_form": "저도 웬만허면 안 가요.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003125.1.10",
          "form": "참 아쉽고 빨리 여행 막가고싶오요",
          "original_form": "참 아쉽고 빨리 여행 막가고싶오요",
          "speaker_id": "1",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003125.1.11",
          "form": "어디 가시고 싶으세요?",
          "original_form": "어디 가시고 싶으세요?",
          "speaker_id": "2",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003125.1.12",
          "form": "저 제주도 가고 싶어요",
          "original_form": "저 제주도 가고 싶어요",
          "speaker_id": "1",
          "time": "20210821 07:47"
        },
        {
          "id": "MDRW2100003125.1.13",
          "form": "작년엔 갔는데 올해 못갔네요 ㅜㅜ아쉬워요",
          "original_form": "작년엔 갔는데 올해 못갔네요 ㅜㅜ아쉬워요",
          "speaker_id": "1",
          "time": "20210821 07:48"
        },
        {
          "id": "MDRW2100003125.1.14",
          "form": "저는 재작년에 갔었어요",
          "original_form": "저는 재작년에 갔었어요",
          "speaker_id": "2",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003125.1.15",
          "form": "요번엔 코로나 확진자가 너무 많아서 못갔네요",
          "original_form": "요번엔 코로나 확진자가 너무 많아서 못갔네요",
          "speaker_id": "1",
          "time": "20210821 09:16"
        },
        {
          "id": "MDRW2100003125.1.16",
          "form": "요새 갔다온곳 있으세요?",
          "original_form": "요새 갔다온곳 있으세요?",
          "speaker_id": "1",
          "time": "20210821 09:18"
        },
        {
          "id": "MDRW2100003125.1.17",
          "form": "없어요.ㅜㅜ집근처도 잘 안 나가니",
          "original_form": "없어요.ㅜㅜ집근처도 잘 안 나가니",
          "speaker_id": "2",
          "time": "20210821 09:40"
        },
        {
          "id": "MDRW2100003125.1.18",
          "form": "네 저도 완전 방콕이예요",
          "original_form": "네 저도 완전 방콕이예요",
          "speaker_id": "1",
          "time": "20210821 09:43"
        },
        {
          "id": "MDRW2100003125.1.19",
          "form": "오늘은 서울가요 서울 투어가 됐네요 ㅋㅋ",
          "original_form": "오늘은 서울가요 서울 투어가 됐네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:46"
        },
        {
          "id": "MDRW2100003125.1.20",
          "form": "치과가는데 겸사겸사 구경가요",
          "original_form": "치과가는데 겸사겸사 구경가요",
          "speaker_id": "1",
          "time": "20210821 09:47"
        },
        {
          "id": "MDRW2100003125.1.21",
          "form": "여행이라 할수도 없죠",
          "original_form": "여행이라 할수도 없죠",
          "speaker_id": "1",
          "time": "20210821 09:47"
        },
        {
          "id": "MDRW2100003125.1.22",
          "form": "기념품 사는 재미로 여행가면 신났는데",
          "original_form": "기념품 사는 재미로 여행가면 신났는데",
          "speaker_id": "1",
          "time": "20210821 10:18"
        },
        {
          "id": "MDRW2100003125.1.23",
          "form": "전 경주빵 안흥찐빵 펑리수 기념 특산품 막사고 좋아했어요",
          "original_form": "전 경주빵 안흥찐빵 펑리수 기념 특산품 막사고 좋아했어요",
          "speaker_id": "1",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100003125.1.24",
          "form": "그래도 집보단 바람도 쐴 수 있으시겠네요. 잘 다녀오세요",
          "original_form": "그래도 집보단 바람도 쐴 수 있으시겠네요. 잘 다녀오세요",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003125.1.25",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 10:26"
        },
        {
          "id": "MDRW2100003125.1.26",
          "form": "가서 아이들 맛있는것도 사주고 오려고요",
          "original_form": "가서 아이들 맛있는것도 사주고 오려고요",
          "speaker_id": "1",
          "time": "20210821 10:27"
        },
        {
          "id": "MDRW2100003125.1.27",
          "form": "비와서 주말엔 꼼짝마인데 이렇게 길을 나서네요 ㅎㅎ",
          "original_form": "비와서 주말엔 꼼짝마인데 이렇게 길을 나서네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 10:27"
        },
        {
          "id": "MDRW2100003125.1.28",
          "form": "조만간 사람없는 계곡이라도 물소리 듣고 오려고요",
          "original_form": "조만간 사람없는 계곡이라도 물소리 듣고 오려고요",
          "speaker_id": "1",
          "time": "20210821 10:27"
        },
        {
          "id": "MDRW2100003125.1.29",
          "form": "지방은 사람 발길 안닿는 곳이 많아요 ㅎㅎ",
          "original_form": "지방은 사람 발길 안닿는 곳이 많아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 10:29"
        },
        {
          "id": "MDRW2100003125.1.30",
          "form": "도전해보세요 ㅎㅎ",
          "original_form": "도전해보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 10:29"
        },
        {
          "id": "MDRW2100003125.1.31",
          "form": "그쵸. 가려면 가는데 코로나 핑계로 집에 있게 되네요.ㅋㅋㅋ",
