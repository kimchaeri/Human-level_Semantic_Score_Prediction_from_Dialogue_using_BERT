      "utterance": [
        {
          "id": "MDRW2100008672.1.1",
          "form": "저는 영드에 빠져있어요",
          "original_form": "저는 영드에 빠져있어요",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008672.1.2",
          "form": "넷플릭스로 보나요?",
          "original_form": "넷플릭스로 보나요?",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008672.1.3",
          "form": "전 웨이브로 봐요",
          "original_form": "전 웨이브로 봐요",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008672.1.4",
          "form": "어떤 드라마예요?",
          "original_form": "어떤 드라마예요?",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008672.1.5",
          "form": "수사물이면 다 좋아해요",
          "original_form": "수사물이면 다 좋아해요",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008672.1.6",
          "form": "파더 브라운이라고 브라운 신부 추천이요 ㅎㅎ",
          "original_form": "파더 브라운이라고 브라운 신부 추천이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008672.1.7",
          "form": "그런거 저도 좋아해요",
          "original_form": "그런거 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008672.1.8",
          "form": "범죄스릴러도 좋고",
          "original_form": "범죄스릴러도 좋고",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008672.1.9",
          "form": "주로 티비방송만 보는데 넷플이나 웨이브로 많이 보나보네요",
          "original_form": "주로 티비방송만 보는데 넷플이나 웨이브로 많이 보나보네요",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008672.1.10",
          "form": "브라운신부는 너무 우울하지 않아서 좋아요 ㅎ",
          "original_form": "브라운신부는 너무 우울하지 않아서 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008672.1.11",
          "form": "뭔지 모르지만 잼나나보네요",
          "original_form": "뭔지 모르지만 잼나나보네요",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008672.1.12",
          "form": "기회되면 함 찾아볼게요",
          "original_form": "기회되면 함 찾아볼게요",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008672.1.13",
          "form": "티비는 잘안보세요?",
          "original_form": "티비는 잘안보세요?",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008672.1.14",
          "form": "티비는 시간 맞춰보기가 쉽지않더라고요 ㅋㅋ",
          "original_form": "티비는 시간 맞춰보기가 쉽지않더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008672.1.15",
          "form": "전 그냥 틀어놓고 재방보고 그래서ㅎ",
          "original_form": "전 그냥 틀어놓고 재방보고 그래서ㅎ",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008672.1.16",
          "form": "유퀴즈 잼있어요",
          "original_form": "유퀴즈 잼있어요",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008672.1.17",
          "form": "유퀴즈 유투브에서 많이봤어요",
          "original_form": "유퀴즈 유투브에서 많이봤어요",
          "speaker_id": "2",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100008672.1.18",
          "form": "생각보다 유익했어요",
          "original_form": "생각보다 유익했어요",
          "speaker_id": "2",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100008672.1.19",
          "form": "뭔가 진로시간? 에도움이될듯한 ㅋㅋ",
          "original_form": "뭔가 진로시간? 에도움이될듯한 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100008672.1.20",
          "form": "나름 생각할시간도 만들어주긴하죠ㅎ",
          "original_form": "나름 생각할시간도 만들어주긴하죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100008672.1.21",
          "form": "전 런닝맨틀어놨어요",
          "original_form": "전 런닝맨틀어놨어요",
          "speaker_id": "1",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100008672.1.22",
          "form": "아직 하나요 런닝맨? 아님 재방?",
          "original_form": "아직 하나요 런닝맨? 아님 재방?",
          "speaker_id": "2",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008672.1.23",
          "form": "본방이요ㅎ",
          "original_form": "본방이요ㅎ",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008672.1.24",
          "form": "올만에 봐요 저도",
          "original_form": "올만에 봐요 저도",
          "speaker_id": "1",
          "time": "20210822 17:55"
        }
      ]
    }
  ]
