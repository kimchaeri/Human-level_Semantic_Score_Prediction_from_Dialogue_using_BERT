      "utterance": [
        {
          "id": "MDRW2100013164.1.1",
          "form": "게임 하세요?",
          "original_form": "게임 하세요?",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013164.1.2",
          "form": "게임 많이 해요 하루에 2시간은 하는듯요",
          "original_form": "게임 많이 해요 하루에 2시간은 하는듯요",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013164.1.3",
          "form": "오 무슨 게임하나요?",
          "original_form": "오 무슨 게임하나요?",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013164.1.4",
          "form": "PC겜은 롤 하고 있고 모바일겜은 동물의숲 해요",
          "original_form": "PC겜은 롤 하고 있고 모바일겜은 동물의숲 해요",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013164.1.5",
          "form": "동물의 숲 폰게임이 있어요?",
          "original_form": "동물의 숲 폰게임이 있어요?",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100013164.1.6",
          "form": "스위치에만 있지 않나요??",
          "original_form": "스위치에만 있지 않나요??",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100013164.1.7",
          "form": "모바일로도 있어요",
          "original_form": "모바일로도 있어요",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100013164.1.8",
          "form": "올해 초에 나왔었어요",
          "original_form": "올해 초에 나왔었어요",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100013164.1.9",
          "form": "포켓캠프 인가여?",
          "original_form": "포켓캠프 인가여?",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100013164.1.10",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100013164.1.11",
          "form": "오호오홍",
          "original_form": "오호오홍",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100013164.1.12",
          "form": "그거 약간 방치형 게임으로 알고있는데 맞나요?",
          "original_form": "그거 약간 방치형 게임으로 알고있는데 맞나요?",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100013164.1.13",
          "form": "동물의 숲은 방치 게임이 될 수 없습니다... 대출금 갚아야죠ㅋㅋ",
          "original_form": "동물의 숲은 방치 게임이 될 수 없습니다... 대출금 갚아야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100013164.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013164.1.15",
          "form": "캠프도 그렇구나 ㅋㅋㅋㅋㅋ",
          "original_form": "캠프도 그렇구나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013164.1.16",
          "form": "사채업자 너굴씨",
          "original_form": "사채업자 너굴씨",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013164.1.17",
          "form": "아 너굴씨는 그만두셨어요... 대신 비둘기 3마리 상대해야해요",
          "original_form": "아 너굴씨는 그만두셨어요... 대신 비둘기 3마리 상대해야해요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100013164.1.18",
          "form": "호오.... 비둘기 유해조류인데",
          "original_form": "호오.... 비둘기 유해조류인데",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100013164.1.19",
          "form": "거기서까지.... 대.단.하.다.",
          "original_form": "거기서까지.... 대.단.하.다.",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100013164.1.20",
          "form": "맞아요 비둘기 유해조류죠... 이번엔 머릿수로도 밀어붙이니 진짜 악마같아요",
          "original_form": "맞아요 비둘기 유해조류죠... 이번엔 머릿수로도 밀어붙이니 진짜 악마같아요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        }
