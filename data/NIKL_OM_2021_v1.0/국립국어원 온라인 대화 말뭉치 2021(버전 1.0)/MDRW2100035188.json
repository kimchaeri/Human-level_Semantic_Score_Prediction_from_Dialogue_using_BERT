      "utterance": [
        {
          "id": "MDRW2100035188.1.1",
          "form": "새벽부터 비가 내리네요ㅡㅠ",
          "original_form": "새벽부터 비가 내리네요ㅡㅠ",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100035188.1.2",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100035188.1.3",
          "form": "저희 동네는 맑은데",
          "original_form": "저희 동네는 맑은데",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100035188.1.4",
          "form": "헉 전국적으로 오는게 아니였군요",
          "original_form": "헉 전국적으로 오는게 아니였군요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100035188.1.5",
          "form": "새벽에 비내리는 소리에 깼거든요",
          "original_form": "새벽에 비내리는 소리에 깼거든요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100035188.1.6",
          "form": "네 지역마다 날씨가 다르네요",
          "original_form": "네 지역마다 날씨가 다르네요",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035188.1.7",
          "form": "이번 주말은 내내 비가 내리지 않았어요",
          "original_form": "이번 주말은 내내 비가 내리지 않았어요",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035188.1.8",
          "form": "앗 그러게요",
          "original_form": "앗 그러게요",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035188.1.9",
          "form": "여기는 어제도 잠깐 내렸었어요",
          "original_form": "여기는 어제도 잠깐 내렸었어요",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035188.1.10",
          "form": "근데 요즘은 갑자기 비가 내리기도 하니",
          "original_form": "근데 요즘은 갑자기 비가 내리기도 하니",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035188.1.11",
          "form": "안심은 못하겠어요",
          "original_form": "안심은 못하겠어요",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035188.1.12",
          "form": "맞아요ㅠ 나가실 예정 있으세요?",
          "original_form": "맞아요ㅠ 나가실 예정 있으세요?",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100035188.1.13",
          "form": "이따가 잠깐 바람 쐴 겸 산책 다녀올까 해요",
          "original_form": "이따가 잠깐 바람 쐴 겸 산책 다녀올까 해요",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100035188.1.14",
          "form": "날 좋으시니 괜찮으시겠어요~",
          "original_form": "날 좋으시니 괜찮으시겠어요~",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100035188.1.15",
          "form": "내일부터는 여기도 꽤 맑더라구요",
          "original_form": "내일부터는 여기도 꽤 맑더라구요",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100035188.1.16",
          "form": "네 맑은 날씨 그냥 보내면 아깝더라고요",
          "original_form": "네 맑은 날씨 그냥 보내면 아깝더라고요",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035188.1.17",
          "form": "곧 추워질수도 있고",
          "original_form": "곧 추워질수도 있고",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035188.1.18",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035188.1.19",
          "form": "이따 좋아지면 저도 산책갈까 생각중이에요~",
          "original_form": "이따 좋아지면 저도 산책갈까 생각중이에요~",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035188.1.20",
          "form": "네네 날이 개서 나가실 수 있음 좋겠네요~",
          "original_form": "네네 날이 개서 나가실 수 있음 좋겠네요~",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035188.1.21",
          "form": "흐흐 감사합니당",
          "original_form": "흐흐 감사합니당",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100035188.1.22",
          "form": "여기는 지금 비가 엄청 많이 내려요",
          "original_form": "여기는 지금 비가 엄청 많이 내려요",
          "speaker_id": "2",
