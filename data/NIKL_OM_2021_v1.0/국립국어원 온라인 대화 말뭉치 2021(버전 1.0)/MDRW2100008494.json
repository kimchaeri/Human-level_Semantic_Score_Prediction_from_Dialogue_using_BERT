      "utterance": [
        {
          "id": "MDRW2100008494.1.1",
          "form": "요리 잘하나?",
          "original_form": "요리 잘하나?",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008494.1.2",
          "form": "나?  잘 못하지",
          "original_form": "나?  잘 못하지",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008494.1.3",
          "form": "나보다는 나을듯",
          "original_form": "나보다는 나을듯",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008494.1.4",
          "form": "그래도 유튜브 보고 쉬운거는 한다",
          "original_form": "그래도 유튜브 보고 쉬운거는 한다",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008494.1.5",
          "form": "그래 대단하네 레시피좀 가르쳐줘",
          "original_form": "그래 대단하네 레시피좀 가르쳐줘",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008494.1.6",
          "form": "아는게 있어야지ㅋ 한번하고 나면 기억이 안나",
          "original_form": "아는게 있어야지ㅋ 한번하고 나면 기억이 안나",
          "speaker_id": "1",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008494.1.7",
          "form": "계속 안하면 그렇지 까먹는다",
          "original_form": "계속 안하면 그렇지 까먹는다",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008494.1.8",
          "form": "다시 유튜브 찾아보고 해야 된다",
          "original_form": "다시 유튜브 찾아보고 해야 된다",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008494.1.9",
          "form": "요린이는 그렇다",
          "original_form": "요린이는 그렇다",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008494.1.10",
          "form": "근데 요새 완전 조리해 놓은거 팔던데",
          "original_form": "근데 요새 완전 조리해 놓은거 팔던데",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008494.1.11",
          "form": "밀키트 같은거? 전문 가게도 있더라",
          "original_form": "밀키트 같은거? 전문 가게도 있더라",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008494.1.12",
          "form": "울동네도 갑자기 두군데 생겼다",
          "original_form": "울동네도 갑자기 두군데 생겼다",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008494.1.13",
          "form": "응 요즘 대세다",
          "original_form": "응 요즘 대세다",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008494.1.14",
          "form": "msg가 들어가서 맛은 있겠다",
          "original_form": "msg가 들어가서 맛은 있겠다",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008494.1.15",
          "form": "ㅋㅋ그래 많이 안먹는 이상 건강에도 이상 없다고 하더라",
          "original_form": "ㅋㅋ그래 많이 안먹는 이상 건강에도 이상 없다고 하더라",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008494.1.16",
          "form": "매번 먹을수도 없지. 가끔 사먹는거지 뭐",
