      "utterance": [
        {
          "id": "MDRW2100031140.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031140.1.2",
          "form": "연말정산으로 세금냈으니...",
          "original_form": "연말정산으로 세금냈으니...",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031140.1.3",
          "form": "봉사죠ㅜㅜ",
          "original_form": "봉사죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031140.1.4",
          "form": "직장인만 봉이지...",
          "original_form": "직장인만 봉이지...",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031140.1.5",
          "form": "그래서 연금펀드하려고요",
          "original_form": "그래서 연금펀드하려고요",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031140.1.6",
          "form": "못잡아먹어서 안달ㅜㅜ",
          "original_form": "못잡아먹어서 안달ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031140.1.7",
          "form": "은퇴까지 못찾는다는 단점이 있지만ㅜ",
          "original_form": "은퇴까지 못찾는다는 단점이 있지만ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031140.1.8",
          "form": "투명한 유리지갑ㅋㅋ",
          "original_form": "투명한 유리지갑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031140.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031140.1.10",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031140.1.11",
          "form": "님은 5년 족쇄 차셨는데 버틸수 있을거 같나여?",
          "original_form": "님은 5년 족쇄 차셨는데 버틸수 있을거 같나여?",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031140.1.12",
          "form": "아... ㅋㅋ 회사가 버텨주면...",
          "original_form": "아... ㅋㅋ 회사가 버텨주면...",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031140.1.13",
          "form": "우선 삼년만 버티자 생각하고있어요ㅋㅋ",
          "original_form": "우선 삼년만 버티자 생각하고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031140.1.14",
          "form": "회사야 버티지 않을까요?",
          "original_form": "회사야 버티지 않을까요?",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031140.1.15",
          "form": "회사 사정이 별로에요?",
          "original_form": "회사 사정이 별로에요?",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031140.1.16",
          "form": "모르겠어요ㅋㅋ 내인생만큼이나 걱정ㅋㅋ",
          "original_form": "모르겠어요ㅋㅋ 내인생만큼이나 걱정ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031140.1.17",
          "form": "도약하냐마냐???",
          "original_form": "도약하냐마냐???",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031140.1.18",
          "form": "관리부시면 대략 알자나요",
          "original_form": "관리부시면 대략 알자나요",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031140.1.19",
          "form": "도약하겠죠 ㅎㅎ",
          "original_form": "도약하겠죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031140.1.20",
          "form": "ㅋㅋ 아 도통 한치앞을ㅋㅋ",
          "original_form": "ㅋㅋ 아 도통 한치앞을ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031140.1.21",
          "form": "이직하실 생각이신가요??",
          "original_form": "이직하실 생각이신가요??",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031140.1.22",
          "form": "앞으로 1년정도는 없는데",
          "original_form": "앞으로 1년정도는 없는데",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031140.1.23",
          "form": "저도 상황 지켜보고",
          "original_form": "저도 상황 지켜보고",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031140.1.24",
          "form": "결정하려구요",
          "original_form": "결정하려구요",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031140.1.25",
          "form": "아까 얘기했듯이",
          "original_form": "아까 얘기했듯이",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031140.1.26",
          "form": "잘못하면 큰일나니깐",
          "original_form": "잘못하면 큰일나니깐",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031140.1.27",
          "form": "ㅜㅜ 진짜 회사는 긴장의 연속이에요 방심하는 순간 사건사고ㅜㅠ",
          "original_form": "ㅜㅜ 진짜 회사는 긴장의 연속이에요 방심하는 순간 사건사고ㅜㅠ",
          "speaker_id": "1",
          "time": "20210904 17:31"
        }
      ]
    }
