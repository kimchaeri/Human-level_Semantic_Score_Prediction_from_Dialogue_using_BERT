      "utterance": [
        {
          "id": "MDRW2100019616.1.1",
          "form": "봉사 하시나요?",
          "original_form": "봉사 하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019616.1.2",
          "form": "학교다닐 땐 교육봉사 자주 했었어요",
          "original_form": "학교다닐 땐 교육봉사 자주 했었어요",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019616.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019616.1.4",
          "form": "저는 지금 하고 있어요",
          "original_form": "저는 지금 하고 있어요",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019616.1.5",
          "form": "오 어떤 봉사하세요?",
          "original_form": "오 어떤 봉사하세요?",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019616.1.6",
          "form": "아름다운 가게에서 봉사해요ㅎㅎ",
          "original_form": "아름다운 가게에서 봉사해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019616.1.7",
          "form": "앗 재밌을거 같아요",
          "original_form": "앗 재밌을거 같아요",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019616.1.8",
          "form": "아름다운 가게 택배로도 기부 받나요?",
          "original_form": "아름다운 가게 택배로도 기부 받나요?",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019616.1.9",
          "form": "요즘은 받아요! 다만 무게 제한이 있는 걸로 알아요ㅠ",
          "original_form": "요즘은 받아요! 다만 무게 제한이 있는 걸로 알아요ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019616.1.10",
          "form": "아하 최소무게 제한인가요 최대무게 제한인가요?",
          "original_form": "아하 최소무게 제한인가요 최대무게 제한인가요?",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019616.1.11",
          "form": "최대무게 제한이요!",
          "original_form": "최대무게 제한이요!",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019616.1.12",
          "form": "헐그렇군요 많이 못하는구나",
          "original_form": "헐그렇군요 많이 못하는구나",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019616.1.13",
          "form": "저희동네에도 아름다운 가게 있었으면 좋겠어요 ㅠㅠ",
          "original_form": "저희동네에도 아름다운 가게 있었으면 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019616.1.14",
          "form": "매장 마다 조금 다를 수 있으니 혹시 하실 계획이시면 그나마 가장 가까운 매장으로 문의를 해보시는 걸 추천드려요!",
          "original_form": "매장 마다 조금 다를 수 있으니 혹시 하실 계획이시면 그나마 가장 가까운 매장으로 문의를 해보시는 걸 추천드려요!",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019616.1.15",
          "form": "앗 네 그럴게요ㅎㅎㅎ",
          "original_form": "앗 네 그럴게요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019616.1.16",
          "form": "정보 감사드려요",
          "original_form": "정보 감사드려요",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019616.1.17",
          "form": "아름다운 가게도 봉사자들이 하시는 줄 몰랐어요!",
          "original_form": "아름다운 가게도 봉사자들이 하시는 줄 몰랐어요!",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019616.1.18",
          "form": "매니저님 제외하고 거의 봉사자로 이루어져있어요ㅎㅎ",
          "original_form": "매니저님 제외하고 거의 봉사자로 이루어져있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019616.1.19",
          "form": "와 진짜 의미있는 가게네요ㅋㅋㅋ",
          "original_form": "와 진짜 의미있는 가게네요ㅋㅋㅋ",
