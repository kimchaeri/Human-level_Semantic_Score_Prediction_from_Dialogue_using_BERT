      "utterance": [
        {
          "id": "MDRW2100023796.1.1",
          "form": "음 종류가 되게 많아서",
          "original_form": "음 종류가 되게 많아서",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023796.1.2",
          "form": "생크림에 오레오 과자 뿌려주는 것도 맛있고",
          "original_form": "생크림에 오레오 과자 뿌려주는 것도 맛있고",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023796.1.3",
          "form": "오 맛있겠다",
          "original_form": "오 맛있겠다",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023796.1.4",
          "form": "미국식 어플의 정석은 생크림에 사과쨈이야",
          "original_form": "미국식 어플의 정석은 생크림에 사과쨈이야",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023796.1.5",
          "form": "그것도 좋을거 같아",
          "original_form": "그것도 좋을거 같아",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023796.1.6",
          "form": "다 먹어봤어?",
          "original_form": "다 먹어봤어?",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023796.1.7",
          "form": "약간 처음 먹는 피자집에 포테이토 피자 시키는 것처럼 처음 먹는 와플 집은 생크림에 사과쨈으로 간을 볼 수 있지",
          "original_form": "약간 처음 먹는 피자집에 포테이토 피자 시키는 것처럼 처음 먹는 와플 집은 생크림에 사과쨈으로 간을 볼 수 있지",
          "speaker_id": "1",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023796.1.8",
          "form": "다 먹어봤어?",
          "original_form": "다 먹어봤어?",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023796.1.9",
          "form": "응 나 전에 살던 동네에 엄청 유명한 와플 집 있었어",
          "original_form": "응 나 전에 살던 동네에 엄청 유명한 와플 집 있었어",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023796.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023796.1.11",
          "form": "바바플이라고 맛있었는데",
          "original_form": "바바플이라고 맛있었는데",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023796.1.12",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023796.1.13",
          "form": "응 거기는 딱 길거리 와플 파는 집이었어",
          "original_form": "응 거기는 딱 길거리 와플 파는 집이었어",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023796.1.14",
          "form": "아 어떤 느낌일지 알거 같아",
          "original_form": "아 어떤 느낌일지 알거 같아",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023796.1.15",
          "form": "카페 디저트 느낌이 아니라 그냥 손에 들고 집에 가면서 먹는 그 느낌",
          "original_form": "카페 디저트 느낌이 아니라 그냥 손에 들고 집에 가면서 먹는 그 느낌",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023796.1.16",
          "form": "시장 부대찌개도 맛있더라",
          "original_form": "시장 부대찌개도 맛있더라",
