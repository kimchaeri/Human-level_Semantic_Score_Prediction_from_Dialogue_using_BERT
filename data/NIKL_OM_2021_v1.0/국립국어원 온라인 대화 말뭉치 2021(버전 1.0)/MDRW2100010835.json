      "utterance": [
        {
          "id": "MDRW2100010835.1.1",
          "form": "최근에 산 물건 있어?",
          "original_form": "최근에 산 물건 있어?",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100010835.1.2",
          "form": "강아지 사료를 구입했어요",
          "original_form": "강아지 사료를 구입했어요",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100010835.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100010835.1.4",
          "form": "저는 옷이요!",
          "original_form": "저는 옷이요!",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100010835.1.5",
          "form": "평소에 쇼핑하는거 좋아하세요?",
          "original_form": "평소에 쇼핑하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100010835.1.6",
          "form": "안 좋아하는데... 정신차리면 지르고 있어요",
          "original_form": "안 좋아하는데... 정신차리면 지르고 있어요",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100010835.1.7",
          "form": "어디서 사셨나요?",
          "original_form": "어디서 사셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100010835.1.8",
          "form": "인터넷이요! 요즘은 거의 인쇼로 사네요",
          "original_form": "인터넷이요! 요즘은 거의 인쇼로 사네요",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100010835.1.9",
          "form": "저도 그래요ㅋㅋㅋ 좋아하는건 아닌데 자꾸 뭘 사요",
          "original_form": "저도 그래요ㅋㅋㅋ 좋아하는건 아닌데 자꾸 뭘 사요",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100010835.1.10",
          "form": "ㅎㅎ 인쇼가 무섭더라고요",
          "original_form": "ㅎㅎ 인쇼가 무섭더라고요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100010835.1.11",
          "form": "너무 구매가 간단해서",
          "original_form": "너무 구매가 간단해서",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100010835.1.12",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100010835.1.13",
          "form": "요즘은 카드등록해서",
          "original_form": "요즘은 카드등록해서",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100010835.1.14",
          "form": "누르면 바로 결제되더라구요",
          "original_form": "누르면 바로 결제되더라구요",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100010835.1.15",
          "form": "ㅎㅎ 고민하다 실수로 누른 적도 있어요",
          "original_form": "ㅎㅎ 고민하다 실수로 누른 적도 있어요",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100010835.1.16",
          "form": "할인쿠폰 적용 알아보다 얼떨결에 구매도 하고",
          "original_form": "할인쿠폰 적용 알아보다 얼떨결에 구매도 하고",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100010835.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100010835.1.18",
          "form": "그리고 카톡으로 할인 홍보도 자주오니까",
          "original_form": "그리고 카톡으로 할인 홍보도 자주오니까",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100010835.1.19",
          "form": "더사게되요ㅎㅎ",
          "original_form": "더사게되요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100010835.1.20",
          "form": "예정에 없던 지출이 느는게 탈이죠ㅎ",
          "original_form": "예정에 없던 지출이 느는게 탈이죠ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100010835.1.21",
          "form": "중고거래도 하시나요?",
          "original_form": "중고거래도 하시나요?",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100010835.1.22",
          "form": "당근마켓 진짜많이써요",
          "original_form": "당근마켓 진짜많이써요",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100010835.1.23",
          "form": "아 전 앱만 깔고 아직 안써봤어요",
          "original_form": "아 전 앱만 깔고 아직 안써봤어요",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100010835.1.24",
          "form": "종류가 다양한가요?",
          "original_form": "종류가 다양한가요?",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100010835.1.25",
          "form": "네 종류도다양하고",
          "original_form": "네 종류도다양하고",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100010835.1.26",
          "form": "직거래위주라 사기가없어서 좋아요ㅎㅎ",
          "original_form": "직거래위주라 사기가없어서 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100010835.1.27",
          "form": "직거래를 잘 안해서 당근을 안했는데",
          "original_form": "직거래를 잘 안해서 당근을 안했는데",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100010835.1.28",
          "form": "사기걱정 없다니 혹하네요",
          "original_form": "사기걱정 없다니 혹하네요",
          "speaker_id": "2",
          "time": "20210826 15:50"
        }
      ]
    }
  ]
