      "utterance": [
        {
          "id": "MDRW2100021940.1.1",
          "form": "밥의 맛으로 드시는 걸까요",
          "original_form": "밥의 맛으로 드시는 걸까요",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021940.1.2",
          "form": "초밥은 활어회 올려진 거 잘 먹는데",
          "original_form": "초밥은 활어회 올려진 거 잘 먹는데",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021940.1.3",
          "form": "영원한 미스터리죠ㅜ",
          "original_form": "영원한 미스터리죠ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021940.1.4",
          "form": "근데 친구들이 초밥은 또 되게 좋아해서 착각했어요",
          "original_form": "근데 친구들이 초밥은 또 되게 좋아해서 착각했어요",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021940.1.5",
          "form": "서로 곤란하셨겠어요",
          "original_form": "서로 곤란하셨겠어요",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021940.1.6",
          "form": "저는 친한친구가 콩국수를 안 좋아해서 넘 아쉬워요",
          "original_form": "저는 친한친구가 콩국수를 안 좋아해서 넘 아쉬워요",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021940.1.7",
          "form": "저는 여름에 콩국수 진짜 자주 먹는데",
          "original_form": "저는 여름에 콩국수 진짜 자주 먹는데",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021940.1.8",
          "form": "물론 그친구 만날 때는 다른 거 먹으면 되긴 하지만...",
          "original_form": "물론 그친구 만날 때는 다른 거 먹으면 되긴 하지만...",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021940.1.9",
          "form": "아ㅜ 제가 콩국수 메이트가 되고 싶네요",
          "original_form": "아ㅜ 제가 콩국수 메이트가 되고 싶네요",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021940.1.10",
          "form": "콩국수 좋아하시는군요!",
          "original_form": "콩국수 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021940.1.11",
          "form": "저는 다행히? 회사 근처에 콩국수 맛집이 많아서 직장 동료들과 종종 가요",
          "original_form": "저는 다행히? 회사 근처에 콩국수 맛집이 많아서 직장 동료들과 종종 가요",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021940.1.12",
          "form": "설탕 넣어 먹으면 맛있는 거 알아주세요",
          "original_form": "설탕 넣어 먹으면 맛있는 거 알아주세요",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021940.1.13",
          "form": "제발요...",
          "original_form": "제발요...",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021940.1.14",
          "form": "앗 저 소금으로만 먹어봤는데",
          "original_form": "앗 저 소금으로만 먹어봤는데",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021940.1.15",
          "form": "저도 그래서 주로 회사 동료들과 먹습니당ㅋㅋㅋ",
          "original_form": "저도 그래서 주로 회사 동료들과 먹습니당ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021940.1.16",
          "form": "저 출신 지역에서는 설탕을 넣어먹거든요ㅋㅋ",
          "original_form": "저 출신 지역에서는 설탕을 넣어먹거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021940.1.17",
          "form": "습관이라 지금도 소금 설탕 다 넣는데",
          "original_form": "습관이라 지금도 소금 설탕 다 넣는데",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021940.1.18",
          "form": "색다른 매력이 있어요",
          "original_form": "색다른 매력이 있어요",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021940.1.19",
          "form": "기회가 된다면 한번 그렇게 드셔보세요",
          "original_form": "기회가 된다면 한번 그렇게 드셔보세요",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021940.1.20",
          "form": "오오 식당 가서 설탕 달라 그러면 주나요?",
          "original_form": "오오 식당 가서 설탕 달라 그러면 주나요?",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021940.1.21",
          "form": "아니면 콩물 사와서 집에서?",
          "original_form": "아니면 콩물 사와서 집에서?",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021940.1.22",
          "form": "맛있을 것 같아요",
          "original_form": "맛있을 것 같아요",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021940.1.23",
          "form": "네 달라면 주시더라구요",
          "original_form": "네 달라면 주시더라구요",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021940.1.24",
          "form": "저같은 사람들이 종종 찾나봐요",
          "original_form": "저같은 사람들이 종종 찾나봐요",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021940.1.25",
          "form": "도전해봐야겠어요",
          "original_form": "도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021940.1.26",
          "form": "살짝 알것 같기도 해요 그맛",
          "original_form": "살짝 알것 같기도 해요 그맛",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021940.1.27",
          "form": "제 회사동료말로는 약간 디저트 같은 느낌들기도 한대요",
          "original_form": "제 회사동료말로는 약간 디저트 같은 느낌들기도 한대요",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021940.1.28",
          "form": "달달해서 그런가봐요ㅋㅋ",
          "original_form": "달달해서 그런가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:11"
        }
      ]
