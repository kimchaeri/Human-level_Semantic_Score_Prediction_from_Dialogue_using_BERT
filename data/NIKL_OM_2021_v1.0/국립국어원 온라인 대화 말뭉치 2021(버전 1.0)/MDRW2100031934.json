      "utterance": [
        {
          "id": "MDRW2100031934.1.1",
          "form": "칭찬해야죠ㅎㅎ",
          "original_form": "칭찬해야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:09"
        },
        {
          "id": "MDRW2100031934.1.2",
          "form": "애한테 매일 칭찬해도ㅎㅎ",
          "original_form": "애한테 매일 칭찬해도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:09"
        },
        {
          "id": "MDRW2100031934.1.3",
          "form": "요즘 말을 잘안듣는 경향이있어서ㅎ 스티커 생각중이에요ㅎ",
          "original_form": "요즘 말을 잘안듣는 경향이있어서ㅎ 스티커 생각중이에요ㅎ",
          "speaker_id": "2",
          "time": "20210905 09:09"
        },
        {
          "id": "MDRW2100031934.1.4",
          "form": "ㅎㅎ 스티커 세트 사지말구요",
          "original_form": "ㅎㅎ 스티커 세트 사지말구요",
          "speaker_id": "1",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100031934.1.5",
          "form": "스탬프 혹시 없으셔요? 집에",
          "original_form": "스탬프 혹시 없으셔요? 집에",
          "speaker_id": "1",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100031934.1.6",
          "form": "아기 도장도 괜찮고 ㅎㅎ",
          "original_form": "아기 도장도 괜찮고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100031934.1.7",
          "form": "같이 동그라미대고 같은거 대고 그리셔요 ㅎㅎ",
          "original_form": "같이 동그라미대고 같은거 대고 그리셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100031934.1.8",
          "form": "포도모양도 좋고 ㅎㅎ",
          "original_form": "포도모양도 좋고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100031934.1.9",
          "form": "그거 자체가 놀이가 됨 ㅎㅎ",
          "original_form": "그거 자체가 놀이가 됨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100031934.1.10",
          "form": "아아아ㅎㅎ",
          "original_form": "아아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031934.1.11",
          "form": "고마워요ㅎㅎ",
          "original_form": "고마워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031934.1.12",
          "form": "티키타카를 해야하는데 죄송해요ㅠ 업무연락이와서ㅎ",
          "original_form": "티키타카를 해야하는데 죄송해요ㅠ 업무연락이와서ㅎ",
          "speaker_id": "2",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031934.1.13",
          "form": "아녀요 ㅋㅋ",
          "original_form": "아녀요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031934.1.14",
          "form": "꼭해볼께요ㅎㅎㅎ",
          "original_form": "꼭해볼께요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031934.1.15",
          "form": "에이 노는 사람들도 아니구 ㅎㅎ",
          "original_form": "에이 노는 사람들도 아니구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031934.1.16",
          "form": "ㅎㅎ 출장을가야하는데",
          "original_form": "ㅎㅎ 출장을가야하는데",
          "speaker_id": "2",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031934.1.17",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031934.1.18",
          "form": "이거뭐... 선박이다보니 스케쥴이 다꼬이네요",
          "original_form": "이거뭐... 선박이다보니 스케쥴이 다꼬이네요",
          "speaker_id": "2",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031934.1.19",
          "form": "계획했던ㄴ대로 흘러가지가않네여ㅎㅎ",
          "original_form": "계획했던ㄴ대로 흘러가지가않네여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100031934.1.20",
          "form": "스케줄 꼬이면 머리 아프죠 ㅜㅁ ㅜ",
          "original_form": "스케줄 꼬이면 머리 아프죠 ㅜㅁ ㅜ",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100031934.1.21",
          "form": "출장비벌어야하는데ㅎㅎ",
          "original_form": "출장비벌어야하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031934.1.22",
          "form": "일도하긴해야지만ㅎㅎㅎ",
          "original_form": "일도하긴해야지만ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031934.1.23",
          "form": "그래서 아이 장난감이나 옷사주려구요ㅎㅎ",
          "original_form": "그래서 아이 장난감이나 옷사주려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031934.1.24",
          "form": "가을이되서 이제 옷도사야해요ㅎ 아이가 커서 일년에 한번씩은 싹다 바꿔줘야해서ㅠㅠ",
          "original_form": "가을이되서 이제 옷도사야해요ㅎ 아이가 커서 일년에 한번씩은 싹다 바꿔줘야해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031934.1.25",
          "form": "ㅎㅎㅎ 아이때문에 더 열심 ㅎ",
          "original_form": "ㅎㅎㅎ 아이때문에 더 열심 ㅎ",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031934.1.26",
          "form": "지금 두척이 경쟁인데ㅎ 하나는제가 갈것같고ㅎ 하나는 제 상사가 갈것같애서ㅎㅎ",
          "original_form": "지금 두척이 경쟁인데ㅎ 하나는제가 갈것같고ㅎ 하나는 제 상사가 갈것같애서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031934.1.27",
          "form": "그래서 꼭 상사보다 하나덜 받아야해요ㅎㅎㅎㅎㅎ",
          "original_form": "그래서 꼭 상사보다 하나덜 받아야해요ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031934.1.28",
          "form": "음 경쟁 ㅡㅁ ㅡ!!",
          "original_form": "음 경쟁 ㅡㅁ ㅡ!!",
          "speaker_id": "1",
