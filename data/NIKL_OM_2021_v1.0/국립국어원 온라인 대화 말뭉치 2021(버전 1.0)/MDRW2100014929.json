      "utterance": [
        {
          "id": "MDRW2100014929.1.1",
          "form": "저는 귀여운 갈색푸들인 강아지를 키워요",
          "original_form": "저는 귀여운 갈색푸들인 강아지를 키워요",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014929.1.2",
          "form": "강아지키워요",
          "original_form": "강아지키워요",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014929.1.3",
          "form": "쓰읍..자랑좀 해죠요",
          "original_form": "쓰읍..자랑좀 해죠요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014929.1.4",
          "form": "이름이 뭔가요",
          "original_form": "이름이 뭔가요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014929.1.5",
          "form": "초코푸들",
          "original_form": "초코푸들",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014929.1.6",
          "form": "name3 입니다",
          "original_form": "&name3& 입니다",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014929.1.7",
          "form": "으음 우선 둥이군요(?0",
          "original_form": "으음 우선 둥이군요(?0",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014929.1.8",
          "form": "이름도 귀엽지 않습니까!!!(주접)",
          "original_form": "이름도 귀엽지 않습니까!!!(주접)",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014929.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014929.1.10",
          "form": "사진 보내는 기능있었으면",
          "original_form": "사진 보내는 기능있었으면",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014929.1.11",
          "form": "저희강아지 사진만 보냈을듯",
          "original_form": "저희강아지 사진만 보냈을듯",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014929.1.12",
          "form": "긍까요 사진 기능 왜 없어!!!!!!",
          "original_form": "긍까요 사진 기능 왜 없어!!!!!!",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014929.1.13",
          "form": "우우!!!! 강아지 보게 해주라!!!!! 우우!!!!",
          "original_form": "우우!!!! 강아지 보게 해주라!!!!! 우우!!!!",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014929.1.14",
          "form": "우리집 강아지가 최고야~",
          "original_form": "우리집 강아지가 최고야~",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014929.1.15",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014929.1.16",
          "form": "강아지가 돼지라",
          "original_form": "강아지가 돼지라",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014929.1.17",
          "form": "너무 무겁네요",
          "original_form": "너무 무겁네요",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014929.1.18",
          "form": "따끈한 강아지",
          "original_form": "따끈한 강아지",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014929.1.19",
          "form": "지금 들고계시나요",
          "original_form": "지금 들고계시나요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014929.1.20",
          "form": "확대하셨군요",
          "original_form": "확대하셨군요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014929.1.21",
          "form": "제 무릎위에서 자고있네요",
          "original_form": "제 무릎위에서 자고있네요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014929.1.22",
          "form": "(다리저려)",
          "original_form": "(다리저려)",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014929.1.23",
          "form": "ㅇㄴ 이렇게 자랑을",
          "original_form": "ㅇㄴ 이렇게 자랑을",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014929.1.24",
          "form": "더해봐요",
          "original_form": "더해봐요",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014929.1.25",
          "form": "(손달달",
          "original_form": "(손달달",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014929.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ손은 왜떠세요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ손은 왜떠세요..",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014929.1.27",
          "form": "아 저희강아지 엄청똑똑해서",
          "original_form": "아 저희강아지 엄청똑똑해서",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014929.1.28",
          "form": "장기자랑하면 10분가능",
          "original_form": "장기자랑하면 10분가능",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014929.1.29",
          "form": "야바위도 할줄알아요",
          "original_form": "야바위도 할줄알아요",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014929.1.30",
          "form": "아 보고싶네요",
          "original_form": "아 보고싶네요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014929.1.31",
          "form": "야바위 강아지라",
          "original_form": "야바위 강아지라",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014929.1.32",
          "form": "야바이..",
          "original_form": "야바이..",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014929.1.33",
          "form": "아 강아지가 야바위를 하는게 아니라 맞추는거죠???",
          "original_form": "아 강아지가 야바위를 하는게 아니라 맞추는거죠???",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014929.1.34",
          "form": "촥촫촥 돌리는 건 사람이 하죠??",
          "original_form": "촥촫촥 돌리는 건 사람이 하죠??",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014929.1.35",
          "form": "그럼요 ㅋㅋㅋ",
          "original_form": "그럼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014929.1.36",
          "form": "강아지는 가만히 맞추는 역할입니다",
          "original_form": "강아지는 가만히 맞추는 역할입니다",
          "speaker_id": "1",
