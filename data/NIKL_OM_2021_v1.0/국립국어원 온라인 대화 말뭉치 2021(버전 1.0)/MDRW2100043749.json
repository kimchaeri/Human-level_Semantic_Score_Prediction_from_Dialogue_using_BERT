      "utterance": [
        {
          "id": "MDRW2100043749.1.1",
          "form": "저도 쿨라타 받음서 사와야겠어요",
          "original_form": "저도 쿨라타 받음서 사와야겠어요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043749.1.2",
          "form": "그럼 모찌가 저녁이신거예요?",
          "original_form": "그럼 모찌가 저녁이신거예요?",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043749.1.3",
          "form": "아니죠ㅋ",
          "original_form": "아니죠ㅋ",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.4",
          "form": "쿨라타 서ㅑ인머스캣으로 받았는데",
          "original_form": "쿨라타 서ㅑ인머스캣으로 받았는데",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.5",
          "form": "맛나더라구요",
          "original_form": "맛나더라구요",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.6",
          "form": "샐러드먹고왔는데 9800원이나해요",
          "original_form": "샐러드먹고왔는데 9800원이나해요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.7",
          "form": "생각보다 비싼거더라구요",
          "original_form": "생각보다 비싼거더라구요",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.8",
          "form": "4500원짜리 ㅎㅎ",
          "original_form": "4500원짜리 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.9",
          "form": "맛괜찮나요?",
          "original_form": "맛괜찮나요?",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.10",
          "form": "맛있어요 시원하이~",
          "original_form": "맛있어요 시원하이~",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.11",
          "form": "달다는사람도 있던데",
          "original_form": "달다는사람도 있던데",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043749.1.12",
          "form": "맞아요 달달해요 ㅎㅎ",
          "original_form": "맞아요 달달해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043749.1.13",
          "form": "샤인머스캣 맛나나요?",
          "original_form": "샤인머스캣 맛나나요?",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043749.1.14",
          "form": "그거 먹었어요 달달하이 저는 괜찮았어요",
          "original_form": "그거 먹었어요 달달하이 저는 괜찮았어요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043749.1.15",
          "form": "샐러드가 야채라서 비싼듯 해요 ㅎㅎ",
          "original_form": "샐러드가 야채라서 비싼듯 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043749.1.16",
          "form": "포케라고 이것저것들었더라구요",
          "original_form": "포케라고 이것저것들었더라구요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043749.1.17",
          "form": "새우랑 병아리콩 렌틸콩",
          "original_form": "새우랑 병아리콩 렌틸콩",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043749.1.18",
          "form": "건강식이네요 ㅎ",
          "original_form": "건강식이네요 ㅎ",
