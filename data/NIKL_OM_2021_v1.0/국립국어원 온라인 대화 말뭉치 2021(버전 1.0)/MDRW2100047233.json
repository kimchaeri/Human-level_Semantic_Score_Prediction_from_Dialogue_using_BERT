      "utterance": [
        {
          "id": "MDRW2100047233.1.1",
          "form": "인체해부 공부해서 외울만큼 반복하고",
          "original_form": "인체해부 공부해서 외울만큼 반복하고",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100047233.1.2",
          "form": "방법이있더라구요",
          "original_form": "방법이있더라구요",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100047233.1.3",
          "form": "와~ 저는 그런걸 몰라서ㅠㅠ",
          "original_form": "와~ 저는 그런걸 몰라서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100047233.1.4",
          "form": "결론은 차근차근 배우면 남들이볼때 와 잘그린다",
          "original_form": "결론은 차근차근 배우면 남들이볼때 와 잘그린다",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100047233.1.5",
          "form": "그정도까지 도달할수있을거 같다 이거였어요",
          "original_form": "그정도까지 도달할수있을거 같다 이거였어요",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100047233.1.6",
          "form": "건물 지을때 뼈대 먼저 세우고 나중에 세세한거 하고 하는거랑 비슷해보였어요",
          "original_form": "건물 지을때 뼈대 먼저 세우고 나중에 세세한거 하고 하는거랑 비슷해보였어요",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100047233.1.7",
          "form": "아 그쵸~ 노력이 중요하네요~ ㅎㅎ 어제는 물건 사신거있으세요?",
          "original_form": "아 그쵸~ 노력이 중요하네요~ ㅎㅎ 어제는 물건 사신거있으세요?",
          "speaker_id": "1",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100047233.1.8",
          "form": "없어요 ㅎㅎㅎ",
          "original_form": "없어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100047233.1.9",
          "form": "오~~~ 저는 오늘은 이것저것 많이샀네요",
          "original_form": "오~~~ 저는 오늘은 이것저것 많이샀네요",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100047233.1.10",
          "form": "마트도가고 고기도사고",
          "original_form": "마트도가고 고기도사고",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100047233.1.11",
          "form": "그렇군요 ㅎㅎ 들으니 기분좋네요",
          "original_form": "그렇군요 ㅎㅎ 들으니 기분좋네요",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100047233.1.12",
          "form": "쇼핑의 즐거움ㅋㅋㅋ",
          "original_form": "쇼핑의 즐거움ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100047233.1.13",
          "form": "ㅎㅎ 저도 쇼핑하고싶어지네요",
          "original_form": "ㅎㅎ 저도 쇼핑하고싶어지네요",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100047233.1.14",
          "form": "구경하다보면 어느새 결제를",
          "original_form": "구경하다보면 어느새 결제를",
          "speaker_id": "1",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100047233.1.15",
          "form": "ㅎㅎ 자꾸 보면 사게되긴해요 ㅎㅎㅎ",
          "original_form": "ㅎㅎ 자꾸 보면 사게되긴해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047233.1.16",
          "form": "중독되는듯 해요ㅋㅋㅋ 쇼핑도",
          "original_form": "중독되는듯 해요ㅋㅋㅋ 쇼핑도",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047233.1.17",
          "form": "그죠 중독시켜요...ㅎㅎㅎ",
          "original_form": "그죠 중독시켜요...ㅎㅎㅎ",
