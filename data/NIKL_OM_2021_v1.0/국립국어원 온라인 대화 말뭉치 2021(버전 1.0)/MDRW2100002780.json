      "utterance": [
        {
          "id": "MDRW2100002780.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002780.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002780.1.3",
          "form": "요새 운동하고계시나요?",
          "original_form": "요새 운동하고계시나요?",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002780.1.4",
          "form": "1만보걷기 운동 하고 있어요",
          "original_form": "1만보걷기 운동 하고 있어요",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002780.1.5",
          "form": "와 대단하시네요 은근어렵던데ㅜㅜ",
          "original_form": "와 대단하시네요 은근어렵던데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002780.1.6",
          "form": "안그래도 운동 제대로 못하고 있는데 그거라도 열심히 하고 있어요ㅠㅠ",
          "original_form": "안그래도 운동 제대로 못하고 있는데 그거라도 열심히 하고 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002780.1.7",
          "form": "코로나 끝나면 등산부터 하고 싶어요",
          "original_form": "코로나 끝나면 등산부터 하고 싶어요",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002780.1.8",
          "form": "등산좋아하세요?",
          "original_form": "등산좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002780.1.9",
          "form": "네 등산 좋아해요",
          "original_form": "네 등산 좋아해요",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002780.1.10",
          "form": "그거라도 해야지 운동한 느낌이 들거든요",
          "original_form": "그거라도 해야지 운동한 느낌이 들거든요",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002780.1.11",
          "form": "마스크쓰고 등산하시는분들 많더라구요",
          "original_form": "마스크쓰고 등산하시는분들 많더라구요",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002780.1.12",
          "form": "맞아요 그렇게 할수도 있는데 숨이 너무 차서ㅜㅜ혹시나 해서 코로나가 걱정되가지구 잘 안가게 되더라구요",
          "original_form": "맞아요 그렇게 할수도 있는데 숨이 너무 차서ㅜㅜ혹시나 해서 코로나가 걱정되가지구 잘 안가게 되더라구요",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002780.1.13",
          "form": "저도 거의 외출삼가고그러는데 너무안움직여서 살이찌더라구요ㅜㅜ 걷기만해도 다이어트될까요?",
          "original_form": "저도 거의 외출삼가고그러는데 너무안움직여서 살이찌더라구요ㅜㅜ 걷기만해도 다이어트될까요?",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002780.1.14",
          "form": "일단 걷기도 유산소 운동이니깐 운동은 될꺼에요",
          "original_form": "일단 걷기도 유산소 운동이니깐 운동은 될꺼에요",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002780.1.15",
          "form": "다이어트로도 가능해요",
          "original_form": "다이어트로도 가능해요",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002780.1.16",
          "form": "식단조절도 해야겠죠?ㅜㅜ",
          "original_form": "식단조절도 해야겠죠?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002780.1.17",
          "form": "다이어트한다면 식단도 조절해야될것같아요ㅠㅠ",
          "original_form": "다이어트한다면 식단도 조절해야될것같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:12"
        }
      ]
