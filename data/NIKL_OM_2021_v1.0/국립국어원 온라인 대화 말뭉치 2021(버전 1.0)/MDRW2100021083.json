        },
        {
          "id": "MDRW2100021083.1.2",
          "form": "그당시에는먹으러가기진심귀찮았는데",
          "original_form": "그당시에는먹으러가기진심귀찮았는데",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021083.1.3",
          "form": "그건맞음",
          "original_form": "그건맞음",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021083.1.4",
          "form": "급식은영양소",
          "original_form": "급식은영양소",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021083.1.5",
          "form": "맞춰서",
          "original_form": "맞춰서",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021083.1.6",
          "form": "나오잖아",
          "original_form": "나오잖아",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021083.1.7",
          "form": "아 맞지맞지",
          "original_form": "아 맞지맞지",
          "speaker_id": "2",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021083.1.8",
          "form": "영양소 맞춰서 딱딱 그게 얼마나 힘든 일이었는지",
          "original_form": "영양소 맞춰서 딱딱 그게 얼마나 힘든 일이었는지",
          "speaker_id": "2",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021083.1.9",
          "form": "지금 깨달음",
          "original_form": "지금 깨달음",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021083.1.10",
          "form": "그영양사쌤?",
          "original_form": "그영양사쌤?",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021083.1.11",
          "form": "뭐라그러지",
          "original_form": "뭐라그러지",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
