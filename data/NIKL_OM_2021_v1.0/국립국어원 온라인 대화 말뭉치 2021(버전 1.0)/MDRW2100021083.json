        },
        {
          "id": "MDRW2100021083.1.13",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021083.1.14",
          "form": "뭐뭐?",
          "original_form": "뭐뭐?",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021083.1.15",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021083.1.16",
          "form": "식단짜는쌤있었잖아",
          "original_form": "식단짜는쌤있었잖아",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021083.1.17",
          "form": "뭐라고하더라",
          "original_form": "뭐라고하더라",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021083.1.18",
          "form": "영양사?",
          "original_form": "영양사?",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021083.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021083.1.20",
          "form": "그분이진심대단하신듯",
          "original_form": "그분이진심대단하신듯",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021083.1.21",
          "form": "그많은식단을다정한다는게",
          "original_form": "그많은식단을다정한다는게",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021083.1.22",
          "form": "그니까 점심 석식해서 2개씩 한달분을 짜는거 아니야",
          "original_form": "그니까 점심 석식해서 2개씩 한달분을 짜는거 아니야",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021083.1.23",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021083.1.24",
          "form": "물론 맛은 없어서 밖으로 나가 먹었지만",
          "original_form": "물론 맛은 없어서 밖으로 나가 먹었지만",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021083.1.25",
          "form": "신기했음",
          "original_form": "신기했음",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100021083.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
