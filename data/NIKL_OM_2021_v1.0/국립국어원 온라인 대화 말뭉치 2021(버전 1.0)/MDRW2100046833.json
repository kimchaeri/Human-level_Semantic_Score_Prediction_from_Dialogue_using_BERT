          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
          "time": "20211105 14:35"
        },
        {
          "id": "MDRW2100046833.1.14",
          "form": "네네 제일 최근에",
          "original_form": "네네 제일 최근에",
          "speaker_id": "2",
          "time": "20211105 14:35"
