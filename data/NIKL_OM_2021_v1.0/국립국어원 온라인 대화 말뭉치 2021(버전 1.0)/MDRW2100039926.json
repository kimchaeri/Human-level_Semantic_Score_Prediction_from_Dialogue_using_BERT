      "utterance": [
        {
          "id": "MDRW2100039926.1.1",
          "form": "오늘 마카롱을 선물받았는데 ㅎㅎ",
          "original_form": "오늘 마카롱을 선물받았는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039926.1.2",
          "form": "전 마카롱의 맛을잘모르겠어요",
          "original_form": "전 마카롱의 맛을잘모르겠어요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039926.1.3",
          "form": "단걸 원래 안 좋아하세요?",
          "original_form": "단걸 원래 안 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039926.1.4",
          "form": "초코는 좋아해요 마카롱만 잘 못먹는것같아요",
          "original_form": "초코는 좋아해요 마카롱만 잘 못먹는것같아요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039926.1.5",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039926.1.6",
          "form": "전 단걸 잘 못 먹는데",
          "original_form": "전 단걸 잘 못 먹는데",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039926.1.7",
          "form": "마카롱 중 솔티카라멜은 좋아해요",
          "original_form": "마카롱 중 솔티카라멜은 좋아해요",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039926.1.8",
          "form": "단짠인가요",
          "original_form": "단짠인가요",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039926.1.9",
          "form": "먹어본 적이 없는데 먹음직스러운 이름이에요 ㅎㅎ",
          "original_form": "먹어본 적이 없는데 먹음직스러운 이름이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039926.1.10",
          "form": "네! 이걸로 도전해보세요ㅎ",
          "original_form": "네! 이걸로 도전해보세요ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039926.1.11",
          "form": "아마 선물받은 구성품 중 있을수도 ㅎㅎ",
          "original_form": "아마 선물받은 구성품 중 있을수도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039926.1.12",
          "form": "선물받은건 검정이랑 핑크색이 있길래 엄마 드렸어요",
          "original_form": "선물받은건 검정이랑 핑크색이 있길래 엄마 드렸어요",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039926.1.13",
          "form": "솔티카라멜 기억해둬야겠어요",
          "original_form": "솔티카라멜 기억해둬야겠어요",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039926.1.14",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100039926.1.15",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100039926.1.16",
          "form": "앗 너무너무 사랑합니다",
          "original_form": "앗 너무너무 사랑합니다",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.17",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.18",
          "form": "요새 많이들 드셔서 저도 먹어봤는데 괜찮더라고요",
          "original_form": "요새 많이들 드셔서 저도 먹어봤는데 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.19",
          "form": "원래 매운것도 좋아하고요 ㅎㅎ",
          "original_form": "원래 매운것도 좋아하고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.20",
          "form": "매운거 잘 드시나요",
          "original_form": "매운거 잘 드시나요",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.21",
          "form": "라고 치고 있었어요ㅎㅎ",
          "original_form": "라고 치고 있었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.22",
          "form": "잘은 못먹어요 ㅎㅎ신전 보통맛정도",
          "original_form": "잘은 못먹어요 ㅎㅎ신전 보통맛정도",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.23",
          "form": "매운맛은 못먹어요 ㄷㄷㄷ",
          "original_form": "매운맛은 못먹어요 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039926.1.24",
          "form": "가게마다 맛이 달라 비교하는 재미가 쏠쏠하더라고요ㅎ",
          "original_form": "가게마다 맛이 달라 비교하는 재미가 쏠쏠하더라고요ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039926.1.25",
          "form": "아ㅎ 불닭볶음면은  드실수 있나요",
          "original_form": "아ㅎ 불닭볶음면은  드실수 있나요",
