      "utterance": [
        {
          "id": "MDRW2100008233.1.1",
          "form": "셋이서 하는데도 힘들겠다",
          "original_form": "셋이서 하는데도 힘들겠다",
          "speaker_id": "1",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008233.1.2",
          "form": "엉 다들 구멍이라서 ㅋㅋ",
          "original_form": "엉 다들 구멍이라서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008233.1.3",
          "form": "놀면뭐하니 스페셜",
          "original_form": "놀면뭐하니 스페셜",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008233.1.4",
          "form": "역시 안되는구만. 하하 자꾸 욕심을ㅋ",
          "original_form": "역시 안되는구만. 하하 자꾸 욕심을ㅋ",
          "speaker_id": "1",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008233.1.5",
          "form": "ㅋㅋ 일부로 하하가 오버할수도 재밌어 보일려고",
          "original_form": "ㅋㅋ 일부로 하하가 오버할수도 재밌어 보일려고",
          "speaker_id": "2",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008233.1.6",
          "form": "아 저 전기드릴채로 어떻게 해? ㅋ",
          "original_form": "아 저 전기드릴채로 어떻게 해? ㅋ",
          "speaker_id": "1",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008233.1.7",
          "form": "ㅋㅋ이제 대형채 드는 것도 일이겠다",
          "original_form": "ㅋㅋ이제 대형채 드는 것도 일이겠다",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008233.1.8",
          "form": "하하 머리 좋다",
          "original_form": "하하 머리 좋다",
          "speaker_id": "1",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008233.1.9",
          "form": "잔머리의 대왕이네 ㅋㅋ",
          "original_form": "잔머리의 대왕이네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008233.1.10",
          "form": "경기가 잘 될라나",
          "original_form": "경기가 잘 될라나",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008233.1.11",
          "form": "그러게 경기는 잘 안되지만 지금이 좀 더 재밌다",
          "original_form": "그러게 경기는 잘 안되지만 지금이 좀 더 재밌다",
          "speaker_id": "2",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008233.1.12",
          "form": "와 하하 대단한데ㅋ",
          "original_form": "와 하하 대단한데ㅋ",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008233.1.13",
          "form": "그러게 대단하네 정말로",
          "original_form": "그러게 대단하네 정말로",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008233.1.14",
          "form": "작가 아이디어가 대단하다",
          "original_form": "작가 아이디어가 대단하다",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008233.1.15",
          "form": "얼음라켓은 들고 힘들겠다 손시려워서",
          "original_form": "얼음라켓은 들고 힘들겠다 손시려워서",
          "speaker_id": "2",
          "time": "20210822 14:26"
