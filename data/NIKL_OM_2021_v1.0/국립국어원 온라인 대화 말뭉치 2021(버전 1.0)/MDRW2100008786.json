      "utterance": [
        {
          "id": "MDRW2100008786.1.1",
          "form": "안녕하세요 요즘 동호회활동 많이하는거 같아요",
          "original_form": "안녕하세요 요즘 동호회활동 많이하는거 같아요",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008786.1.2",
          "form": "활동하시는거 있으신가요?",
          "original_form": "활동하시는거 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008786.1.3",
          "form": "예전에 클래식동호회 가본적 있어요",
          "original_form": "예전에 클래식동호회 가본적 있어요",
          "speaker_id": "1",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008786.1.4",
          "form": "온라인으로도 눈팅하구요",
          "original_form": "온라인으로도 눈팅하구요",
          "speaker_id": "1",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008786.1.5",
          "form": "클래식동호회는 첨들어봐요ㅎ",
          "original_form": "클래식동호회는 첨들어봐요ㅎ",
          "speaker_id": "2",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100008786.1.6",
          "form": "주로 가서 뭐해요?",
          "original_form": "주로 가서 뭐해요?",
          "speaker_id": "2",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100008786.1.7",
          "form": "같이 음악듣던데요 발표회처럼요",
          "original_form": "같이 음악듣던데요 발표회처럼요",
          "speaker_id": "1",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100008786.1.8",
          "form": "아 클래식 공부도 하고가야겠어요",
