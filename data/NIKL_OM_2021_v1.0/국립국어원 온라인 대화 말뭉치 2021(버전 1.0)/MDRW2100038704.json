      "utterance": [
        {
          "id": "MDRW2100038704.1.1",
          "form": "음 위험하신데",
          "original_form": "음 위험하신데",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100038704.1.2",
          "form": "그분은 명성이 자자하여 괜찮으시려나 ㅎ",
          "original_form": "그분은 명성이 자자하여 괜찮으시려나 ㅎ",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100038704.1.3",
          "form": "근데 그럼 영어로 연기하신건가요",
          "original_form": "근데 그럼 영어로 연기하신건가요",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038704.1.4",
          "form": "아님 중국어로",
          "original_form": "아님 중국어로",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038704.1.5",
          "form": "영화봐야알듯요",
          "original_form": "영화봐야알듯요",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038704.1.6",
          "form": "큰스크린에서 봐야징",
          "original_form": "큰스크린에서 봐야징",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038704.1.7",
          "form": "예고편도 아직 안보심?",
          "original_form": "예고편도 아직 안보심?",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038704.1.8",
          "form": "봄 방금",
          "original_form": "봄 방금",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100038704.1.9",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100038704.1.10",
          "form": "예고편  안보고 영화보러가시는편이시군요 ㅋㅋ",
          "original_form": "예고편  안보고 영화보러가시는편이시군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038704.1.11",
          "form": "그래서 연기 어떻게하던가요 ㅋㅋㅋ",
          "original_form": "그래서 연기 어떻게하던가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038704.1.12",
          "form": "마블믿고가유",
          "original_form": "마블믿고가유",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038704.1.13",
          "form": "양조위인데",
          "original_form": "양조위인데",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038704.1.14",
          "form": "연기를 물으시다뇨",
          "original_form": "연기를 물으시다뇨",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038704.1.15",
          "form": "아닝 아닝 영어쓰시나요 중국어쓰시나요 양조위님",
          "original_form": "아닝 아닝 영어쓰시나요 중국어쓰시나요 양조위님",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100038704.1.16",
          "form": "영어쓰실듯해요",
          "original_form": "영어쓰실듯해요",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100038704.1.17",
          "form": "와 대단하셔",
          "original_form": "와 대단하셔",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100038704.1.18",
          "form": "뭐랄까 이미 연기만해도힘든건데",
          "original_form": "뭐랄까 이미 연기만해도힘든건데",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100038704.1.19",
          "form": "그걸 또 외국어로 해야한다는거잖아요",
          "original_form": "그걸 또 외국어로 해야한다는거잖아요",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100038704.1.20",
          "form": "외국어해야쥬",
          "original_form": "외국어해야쥬",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038704.1.21",
          "form": "아 쥔공 김씨네 편의점 주인공 이에요",
          "original_form": "아 쥔공 김씨네 편의점 주인공 이에요",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038704.1.22",
          "form": "미드보셨나요?",
          "original_form": "미드보셨나요?",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038704.1.23",
          "form": "아 아뇨 전 수사물만파요",
          "original_form": "아 아뇨 전 수사물만파요",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100038704.1.24",
          "form": "멜랑꼴리한",
          "original_form": "멜랑꼴리한",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100038704.1.25",
          "form": "꽁냥거리는건ㅋㅋ",
          "original_form": "꽁냥거리는건ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100038704.1.26",
          "form": "그꼴을 제가 못봅니다 ㅋㅋ",
          "original_form": "그꼴을 제가 못봅니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100038704.1.27",
          "form": "미드도",
          "original_form": "미드도",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100038704.1.28",
          "form": "수사물만보시는군요",
          "original_form": "수사물만보시는군요",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100038704.1.29",
          "form": "미드영드일드 수사물 ㅋㅋ",
          "original_form": "미드영드일드 수사물 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100038704.1.30",
          "form": "만봐요",
          "original_form": "만봐요",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100038704.1.31",
          "form": "가끔일드는 음식나오는거봄",
          "original_form": "가끔일드는 음식나오는거봄",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100038704.1.32",
          "form": "고독한 미식가는 명작이에요 ㅋㅋ",
          "original_form": "고독한 미식가는 명작이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100038704.1.33",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100038704.1.34",
          "form": "그거 재밌어요",
          "original_form": "그거 재밌어요",
          "speaker_id": "2",
