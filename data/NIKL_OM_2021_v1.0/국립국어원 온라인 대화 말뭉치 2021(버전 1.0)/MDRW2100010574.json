      "utterance": [
        {
          "id": "MDRW2100010574.1.1",
          "form": "어제는 뭘 사셨나요",
          "original_form": "어제는 뭘 사셨나요",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010574.1.2",
          "form": "생필품이요ㅎㅎ 샴푸 바디워시 같은거요ㅎㅎ",
          "original_form": "생필품이요ㅎㅎ 샴푸 바디워시 같은거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010574.1.3",
          "form": "씻는물건은 중요하죠",
          "original_form": "씻는물건은 중요하죠",
          "speaker_id": "1",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010574.1.4",
          "form": "몇개씩 사둬도 금방 사라지는거같아요",
          "original_form": "몇개씩 사둬도 금방 사라지는거같아요",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010574.1.5",
          "form": "저는 건조피부라서 알로에들어간거 좋아해요",
          "original_form": "저는 건조피부라서 알로에들어간거 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010574.1.6",
          "form": "여름에 많이 쓰셯겠어요",
          "original_form": "여름에 많이 쓰셯겠어요",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010574.1.7",
          "form": "저도 촉촉한 제품위주로 구매해요 뽀득한거보다는ㅎㅎ",
          "original_form": "저도 촉촉한 제품위주로 구매해요 뽀득한거보다는ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010574.1.8",
          "form": "네 아무래도 매일 쓰니까..ㅋㅋㅋ",
          "original_form": "네 아무래도 매일 쓰니까..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010574.1.9",
          "form": "저는 폴메디슨인가 추천받았어요",
          "original_form": "저는 폴메디슨인가 추천받았어요",
          "speaker_id": "1",
          "time": "20210823 19:51"
        },
        {
          "id": "MDRW2100010574.1.10",
          "form": "어린이 아닌데 ㅋㅋ",
          "original_form": "어린이 아닌데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010574.1.11",
          "form": "아기들 쓰는 제품이 순하고 좋아서ㅋㅋ",
          "original_form": "아기들 쓰는 제품이 순하고 좋아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010574.1.12",
          "form": "아 글춍 .. 젤 믿음직해요",
          "original_form": "아 글춍 .. 젤 믿음직해요",
          "speaker_id": "1",
          "time": "20210823 19:54"
        },
        {
          "id": "MDRW2100010574.1.13",
          "form": "아몬드오일도 써보고싶은데",
          "original_form": "아몬드오일도 써보고싶은데",
          "speaker_id": "1",
          "time": "20210823 19:54"
        },
        {
          "id": "MDRW2100010574.1.14",
          "form": "당근마켓을 보고있어요 ㅎㅎ",
          "original_form": "당근마켓을 보고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010574.1.15",
          "form": "오일은 근데 전 못쓰겠어요ㅎㅎ 너무 기름진거같아서",
          "original_form": "오일은 근데 전 못쓰겠어요ㅎㅎ 너무 기름진거같아서",
          "speaker_id": "2",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010574.1.16",
          "form": "아 동백오일은 어떠세요?",
          "original_form": "아 동백오일은 어떠세요?",
          "speaker_id": "1",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010574.1.17",
          "form": "아 그건 괜찮나요?",
          "original_form": "아 그건 괜찮나요?",
          "speaker_id": "2",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010574.1.18",
          "form": "건조피부에 효능이 좋다고들었어요",
          "original_form": "건조피부에 효능이 좋다고들었어요",
          "speaker_id": "1",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010574.1.19",
          "form": "아 검색해봐야겠네요ㅎㅎ",
          "original_form": "아 검색해봐야겠네요ㅎㅎ",
