      "utterance": [
        {
          "id": "MDRW2100041774.1.1",
          "form": "정말 어떻게 해야",
          "original_form": "정말 어떻게 해야",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041774.1.2",
          "form": "취업하시면",
          "original_form": "취업하시면",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041774.1.3",
          "form": "오세요",
          "original_form": "오세요",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041774.1.4",
          "form": "여기로 ㅎㅎ",
          "original_form": "여기로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041774.1.5",
          "form": "잘 먹고 잘 살 수 있을까요",
          "original_form": "잘 먹고 잘 살 수 있을까요",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041774.1.6",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041774.1.7",
          "form": "동네친구긔~~~",
          "original_form": "동네친구긔~~~",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.8",
          "form": "돈 많이 벌어서",
          "original_form": "돈 많이 벌어서",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.9",
          "form": "오피스텔 갭니다",
          "original_form": "오피스텔 {censored}갭니다",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.10",
          "form": "엄빠 데리고 다니고싶어요",
          "original_form": "엄빠 데리고 다니고싶어요",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.11",
          "form": "와우!!!",
          "original_form": "와우!!!",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.12",
          "form": "저 어제 이태원 다녀왔는데",
          "original_form": "저 어제 이태원 다녀왔는데",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.13",
          "form": "이상하게 저는 이태원만 다녀오면",
          "original_form": "이상하게 저는 이태원만 다녀오면",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.15",
          "form": "야망이 마구 샘솟아요?!?!",
          "original_form": "야망이 마구 샘솟아요?!?!",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.16",
          "form": "ㅋㅋㅋㅋ오",
          "original_form": "ㅋㅋㅋㅋ오",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.17",
          "form": "좋져",
          "original_form": "좋져",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.18",
          "form": "하..서울에 내집마련 가능할련지",
          "original_form": "하..서울에 내집마련 가능할련지",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.19",
          "form": "청약만이 답인건지",
          "original_form": "청약만이 답인건지",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.20",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041774.1.21",
          "form": "그리고 친구들 보면",
          "original_form": "그리고 친구들 보면",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041774.1.22",
          "form": "다들 대출 받아서",
          "original_form": "다들 대출 받아서",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041774.1.23",
          "form": "등기치더라구요",
          "original_form": "등기치더라구요",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041774.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041774.1.25",
          "form": "대출을 받을 수 있다는거 자체가 부럽습니다",
          "original_form": "대출을 받을 수 있다는거 자체가 부럽습니다",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041774.1.26",
          "form": "이제는 대출도 능력이라는걸 알아서.",
          "original_form": "이제는 대출도 능력이라는걸 알아서.",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041774.1.27",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041774.1.28",
          "form": "아니그니까요",
          "original_form": "아니그니까요",
          "speaker_id": "1",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041774.1.29",
          "form": "제 말이 그 말입니다...",
          "original_form": "제 말이 그 말입니다...",
          "speaker_id": "1",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041774.1.30",
          "form": "그래서 어제 이태원 다녀왔는데",
          "original_form": "그래서 어제 이태원 다녀왔는데",
          "speaker_id": "1",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041774.1.31",
          "form": "거기는 진짜 걸어다니는 건 노답이잖아요?!",
          "original_form": "거기는 진짜 걸어다니는 건 노답이잖아요?!",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041774.1.32",
          "form": "교통이 좋은 건 아닌데 주택가 이런 데는...",
          "original_form": "교통이 좋은 건 아닌데 주택가 이런 데는...",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041774.1.33",
          "form": "원래 사시던 분들은 시끄럽겠다는 생각이....",
          "original_form": "원래 사시던 분들은 시끄럽겠다는 생각이....",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041774.1.34",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041774.1.35",
          "form": "그래도 그런 힙한 데에 살려면",
          "original_form": "그래도 그런 힙한 데에 살려면",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041774.1.36",
          "form": "한남동 이런 주택 사려면 도대체",
          "original_form": "한남동 이런 주택 사려면 도대체",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041774.1.37",
          "form": "얼마나 있어야하는걸까 생각도 들고...",
          "original_form": "얼마나 있어야하는걸까 생각도 들고...",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041774.1.38",
          "form": "ㅋㅋㅋㅋ진짜 근데 그런데는 복권1등돼도",
          "original_form": "ㅋㅋㅋㅋ진짜 근데 그런데는 복권1등돼도",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100041774.1.39",
          "form": "못사지않나요 ㅠㅠ",
          "original_form": "못사지않나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:44"
        }
