        },
        {
          "id": "MDRW2100018984.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018984.1.12",
          "form": "저도 꽂혀가지구 싸게팔때 한박스사놨어요ㅋㅋㅋㅋ",
          "original_form": "저도 꽂혀가지구 싸게팔때 한박스사놨어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
