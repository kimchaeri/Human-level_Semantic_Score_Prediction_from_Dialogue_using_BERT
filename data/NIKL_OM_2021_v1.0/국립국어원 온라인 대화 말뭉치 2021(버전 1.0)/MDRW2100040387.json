      "utterance": [
        {
          "id": "MDRW2100040387.1.1",
          "form": "오 반려동물 키우시나요",
          "original_form": "오 반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100040387.1.2",
          "form": "아니요 반려동물은 못키우고 있어요",
          "original_form": "아니요 반려동물은 못키우고 있어요",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040387.1.3",
          "form": "가족들이 털 알레르기가 있어서",
          "original_form": "가족들이 털 알레르기가 있어서",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040387.1.4",
          "form": "저두 따로 키우는 건 없는데",
          "original_form": "저두 따로 키우는 건 없는데",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040387.1.5",
          "form": "종종 이웃집에서 강아지 맡기거나 어항을 맡기거나 ㅋㅋㅋ",
          "original_form": "종종 이웃집에서 강아지 맡기거나 어항을 맡기거나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040387.1.6",
          "form": "뭘 자꾸 맡기세요",
          "original_form": "뭘 자꾸 맡기세요",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040387.1.7",
          "form": "아 잘 맡아서 돌보시나봐요",
          "original_form": "아 잘 맡아서 돌보시나봐요",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100040387.1.8",
          "form": "그냥 엄마랑 친하셔서 ㅋㅋㅋ",
          "original_form": "그냥 엄마랑 친하셔서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100040387.1.9",
          "form": "아무한테나 반려동물 맡기진 않잖아요ㅎㅎ",
          "original_form": "아무한테나 반려동물 맡기진 않잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100040387.1.10",
          "form": "근데 진짜 그집강아지 너무 귀엽거든요",
          "original_form": "근데 진짜 그집강아지 너무 귀엽거든요",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040387.1.11",
          "form": "요즘 뭐 시고르자브종이라고 부르던데",
          "original_form": "요즘 뭐 시고르자브종이라고 부르던데",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040387.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040387.1.13",
          "form": "ㅎㅎ귀여운 종이네요",
          "original_form": "ㅎㅎ귀여운 종이네요",
          "speaker_id": "2",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100040387.1.14",
          "form": "원래 시고르자브종이 귀엽잖아요",
          "original_form": "원래 시고르자브종이 귀엽잖아요",
          "speaker_id": "2",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100040387.1.15",
          "form": "마자요 귀염둥이!!",
          "original_form": "마자요 귀염둥이!!",
          "speaker_id": "1",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100040387.1.16",
          "form": "낯선 집이라 좀 경계심 있을법도 한데",
          "original_form": "낯선 집이라 좀 경계심 있을법도 한데",
          "speaker_id": "1",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100040387.1.17",
          "form": "잘 놀고",
          "original_form": "잘 놀고",
          "speaker_id": "1",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100040387.1.18",
          "form": "그 유튜브에 강아지를 위한 힐링음악 모음집 이런거 틀어줬더니",
          "original_form": "그 유튜브에 강아지를 위한 힐링음악 모음집 이런거 틀어줬더니",
          "speaker_id": "1",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100040387.1.19",
          "form": "바로 잠들어버리는 거에요! 너무 신기했어요",
          "original_form": "바로 잠들어버리는 거에요! 너무 신기했어요",
          "speaker_id": "1",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100040387.1.20",
          "form": "오 강아지가 음악 듣고 반응하는군요",
          "original_form": "오 강아지가 음악 듣고 반응하는군요",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100040387.1.21",
          "form": "신기하네요 정말",
          "original_form": "신기하네요 정말",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100040387.1.22",
          "form": "제 친구네 강아지는 좀 예민한 편이라",
          "original_form": "제 친구네 강아지는 좀 예민한 편이라",
          "speaker_id": "1",
          "time": "20210904 19:16"
        },
        {
          "id": "MDRW2100040387.1.23",
          "form": "그런거 틀어줘도 그냥 무반응인데",
          "original_form": "그런거 틀어줘도 그냥 무반응인데",
          "speaker_id": "1",
          "time": "20210904 19:16"
        },
        {
          "id": "MDRW2100040387.1.24",
          "form": "이웃집네 개는 틀자마자 바로 숙면 취하더라구요ㅋㅋㅋ",
          "original_form": "이웃집네 개는 틀자마자 바로 숙면 취하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:16"
        },
        {
          "id": "MDRW2100040387.1.25",
          "form": "아 강아지마다 다르구나",
          "original_form": "아 강아지마다 다르구나",
          "speaker_id": "2",
          "time": "20210904 19:16"
        },
        {
          "id": "MDRW2100040387.1.26",
          "form": "나중에라도 반려동물 키우고 싶으신가요?",
          "original_form": "나중에라도 반려동물 키우고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100040387.1.27",
          "form": "음 아니요 자신이 없어요 ㅋㅋ",
          "original_form": "음 아니요 자신이 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040387.1.28",
          "form": "그쵸 반려동물 키우는 데도 책임감이 많이 필요하죠",
          "original_form": "그쵸 반려동물 키우는 데도 책임감이 많이 필요하죠",
