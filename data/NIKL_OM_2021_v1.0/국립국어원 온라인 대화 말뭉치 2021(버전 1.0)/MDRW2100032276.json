      "utterance": [
        {
          "id": "MDRW2100032276.1.1",
          "form": "오늘 날씨 흐리더라",
          "original_form": "오늘 날씨 흐리더라",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100032276.1.2",
          "form": "나는 너무 더운거보단 이런게 나은거같아",
          "original_form": "나는 너무 더운거보단 이런게 나은거같아",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100032276.1.3",
          "form": "거기도 흐려?",
          "original_form": "거기도 흐려?",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100032276.1.4",
          "form": "막 그렇게 흐린건 아닌데 구름이 많아",
          "original_form": "막 그렇게 흐린건 아닌데 구름이 많아",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032276.1.5",
          "form": "어제 그제는 오전엔 날이 맑다가",
          "original_form": "어제 그제는 오전엔 날이 맑다가",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032276.1.6",
          "form": "오후엔 흐려지고 바람 좀 불더라",
          "original_form": "오후엔 흐려지고 바람 좀 불더라",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032276.1.7",
          "form": "어차피 방에만 있어서 큰 상관은 없지만 ㅋㅋ",
          "original_form": "어차피 방에만 있어서 큰 상관은 없지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:40"
        },
        {
          "id": "MDRW2100032276.1.8",
          "form": "나 날씨 예보 맨날 보는데",
          "original_form": "나 날씨 예보 맨날 보는데",
          "speaker_id": "1",
          "time": "20210905 15:40"
        },
        {
          "id": "MDRW2100032276.1.9",
          "form": "난 나갈일이 없어서 날씨 정보 아예 안봐..크크크",
          "original_form": "난 나갈일이 없어서 날씨 정보 아예 안봐..크크크",
          "speaker_id": "2",
          "time": "20210905 15:41"
        },
        {
          "id": "MDRW2100032276.1.10",
          "form": "근데 요즘들어 좀 쌀쌀하긴 하더라",
          "original_form": "근데 요즘들어 좀 쌀쌀하긴 하더라",
          "speaker_id": "2",
          "time": "20210905 15:41"
        },
        {
          "id": "MDRW2100032276.1.11",
          "form": "난 더운거보단 쌀쌀한게 좋아",
          "original_form": "난 더운거보단 쌀쌀한게 좋아",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.12",
          "form": "슬슬 긴팔 옷들 꺼낼 때가 됐다",
          "original_form": "슬슬 긴팔 옷들 꺼낼 때가 됐다",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.13",
          "form": "내일 조금 비올지도",
          "original_form": "내일 조금 비올지도",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.14",
          "form": "비오는거 좋지",
          "original_form": "비오는거 좋지",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.15",
          "form": "내일 백신맞으러 갈건뎅 ㅋㅋ",
          "original_form": "내일 백신맞으러 갈건뎅 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.16",
          "form": "나갔다와서 집에있을때",
          "original_form": "나갔다와서 집에있을때",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.17",
          "form": "난 추석지나고 갈거야",
          "original_form": "난 추석지나고 갈거야",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.18",
          "form": "비와야좋음",
          "original_form": "비와야좋음",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.19",
          "form": "내일 날씨봐야겠네",
          "original_form": "내일 날씨봐야겠네",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032276.1.21",
          "form": "집에서 빗소리 듣는거 좋짘ㅋㅋ",
          "original_form": "집에서 빗소리 듣는거 좋짘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032276.1.22",
          "form": "거기는 어디날씨라고치면나오지",
          "original_form": "거기는 어디날씨라고치면나오지",
