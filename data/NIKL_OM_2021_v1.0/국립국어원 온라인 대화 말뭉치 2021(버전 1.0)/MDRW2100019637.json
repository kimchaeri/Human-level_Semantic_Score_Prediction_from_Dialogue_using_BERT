        {
          "id": "MDRW2100019637.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ편하고 잘 맞았다니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ편하고 잘 맞았다니",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019637.1.2",
          "form": "다행이에여",
          "original_form": "다행이에여",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019637.1.3",
          "form": "100정도 지분이니까요 호호",
          "original_form": "100정도 지분이니까요 호호",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019637.1.4",
          "form": "아 그럼여",
          "original_form": "아 그럼여",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.5",
          "form": "사실 100/100이에여",
          "original_form": "사실 100/100이에여",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.6",
          "form": "나이가 어떻게 되세요?!",
          "original_form": "나이가 어떻게 되세요?!",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.7",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.8",
          "form": "허 감동이네요...",
          "original_form": "허 감동이네요...",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.9",
          "form": "음 몇살일거같아여??",
          "original_form": "음 몇살일거같아여??",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.10",
          "form": "20대 초반 예상해봅니다 ㅋㅋㅋ",
          "original_form": "20대 초반 예상해봅니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.11",
          "form": "음...24이옵니다!",
          "original_form": "음...24이옵니다!",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.12",
          "form": "하..좋은나이네요..",
          "original_form": "하..좋은나이네요..",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.13",
          "form": "아잌ㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019637.1.14",
          "form": "누님은 올해 춘추가",
          "original_form": "누님은 올해 춘추가",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019637.1.15",
          "form": "어찌되시나요",
          "original_form": "어찌되시나요",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019637.1.16",
          "form": "전 27이랍니다 ㅎ..",
          "original_form": "전 27이랍니다 ㅎ..",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019637.1.17",
          "form": "아 3살이면",
          "original_form": "아 3살이면",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019637.1.18",
          "form": "궁합도 안본다구",
          "original_form": "궁합도 안본다구",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019637.1.19",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019637.1.20",
          "form": "맞아요 원래는 4살이지만 ㅋㅋㅋㅋ",
