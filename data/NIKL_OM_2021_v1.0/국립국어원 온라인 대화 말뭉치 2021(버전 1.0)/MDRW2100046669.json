      "utterance": [
        {
          "id": "MDRW2100046669.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 13:03"
        },
        {
          "id": "MDRW2100046669.1.2",
          "form": "안녕하세요! 오늘은 조금 추운 가을 날씨네요~!",
          "original_form": "안녕하세요! 오늘은 조금 추운 가을 날씨네요~!",
          "speaker_id": "2",
          "time": "20211104 13:08"
        },
        {
          "id": "MDRW2100046669.1.3",
          "form": "추워서 감기 걸렸어요",
          "original_form": "추워서 감기 걸렸어요",
          "speaker_id": "1",
          "time": "20211104 13:09"
        },
        {
          "id": "MDRW2100046669.1.4",
          "form": "아유ㅠ 환절기엔 감기 조심해야하는데!",
          "original_form": "아유ㅠ 환절기엔 감기 조심해야하는데!",
          "speaker_id": "2",
          "time": "20211104 13:10"
        },
        {
          "id": "MDRW2100046669.1.5",
          "form": "따듯한거 많이 마시면 조금 나아질거에요",
          "original_form": "따듯한거 많이 마시면 조금 나아질거에요",
          "speaker_id": "2",
          "time": "20211104 13:10"
        },
        {
          "id": "MDRW2100046669.1.6",
          "form": "name2님도 감기 조심하세요~ 미리미리 생강차 타 드세요!",
          "original_form": "&name2&님도 감기 조심하세요~ 미리미리 생강차 타 드세요!",
          "speaker_id": "1",
          "time": "20211104 13:10"
        },
        {
          "id": "MDRW2100046669.1.7",
          "form": "감사합니다ㅠ 사실 코가 좀 간질간질한게 낌새가 보이고 있긴 했어요 ㅋㅋㅋ",
          "original_form": "감사합니다ㅠ 사실 코가 좀 간질간질한게 낌새가 보이고 있긴 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:12"
        },
        {
          "id": "MDRW2100046669.1.8",
          "form": "요새 감기 대유행이예요",
          "original_form": "요새 감기 대유행이예요",
          "speaker_id": "1",
          "time": "20211104 13:16"
        },
        {
          "id": "MDRW2100046669.1.9",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "1",
          "time": "20211104 13:16"
        },
        {
          "id": "MDRW2100046669.1.10",
          "form": "우리집은 아이가 콧물이 주룩",
          "original_form": "우리집은 아이가 콧물이 주룩",
          "speaker_id": "1",
          "time": "20211104 13:17"
        },
        {
          "id": "MDRW2100046669.1.11",
          "form": "아뇨 저는 결혼은 하지 않았습니다!",
          "original_form": "아뇨 저는 결혼은 하지 않았습니다!",
          "speaker_id": "2",
          "time": "20211104 13:17"
        },
        {
          "id": "MDRW2100046669.1.12",
          "form": "애기는 감기 잘 걸리잖아요 ㅠㅠ",
          "original_form": "애기는 감기 잘 걸리잖아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 13:17"
        },
        {
          "id": "MDRW2100046669.1.13",
          "form": "집에 애들이랑 같이 살면",
          "original_form": "집에 애들이랑 같이 살면",
          "speaker_id": "1",
          "time": "20211104 13:17"
        },
        {
          "id": "MDRW2100046669.1.14",
          "form": "감기가 쉽게 옮아요",
          "original_form": "감기가 쉽게 옮아요",
          "speaker_id": "1",
          "time": "20211104 13:18"
        },
        {
          "id": "MDRW2100046669.1.15",
          "form": "그래서 저도 콜록콜록",
          "original_form": "그래서 저도 콜록콜록",
          "speaker_id": "1",
          "time": "20211104 13:19"
        },
        {
          "id": "MDRW2100046669.1.16",
          "form": "ㅠㅠㅠ아이들은 감기도 자주걸리는 것 같아요",
          "original_form": "ㅠㅠㅠ아이들은 감기도 자주걸리는 것 같아요",
          "speaker_id": "2",
          "time": "20211104 13:20"
        },
        {
          "id": "MDRW2100046669.1.17",
          "form": "네! ",
          "original_form": "네! ",
          "speaker_id": "1",
          "time": "20211104 13:20"
        },
        {
          "id": "MDRW2100046669.1.18",
          "form": "따듯한거 많이 마시구 푹 쉬세요!!",
          "original_form": "따듯한거 많이 마시구 푹 쉬세요!!",
