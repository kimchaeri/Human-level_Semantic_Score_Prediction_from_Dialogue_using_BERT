      "utterance": [
        {
          "id": "MDRW2100028765.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그러고보니 선바도 개망하고...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그러고보니 선바도 개망하고...",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028765.1.2",
          "form": "선바 여친 진짜 꺠더라",
          "original_form": "선바 여친 진짜 꺠더라",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028765.1.3",
          "form": "내동생이 선바 봐서 가끔봤는데",
          "original_form": "내동생이 선바 봐서 가끔봤는데",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028765.1.4",
          "form": "진짜 극여초라 이번에 완전 망한듯...",
          "original_form": "진짜 극여초라 이번에 완전 망한듯...",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028765.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ선바도 깨더라.... 대충 알고 만났다며",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ선바도 깨더라.... 대충 알고 만났다며",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028765.1.6",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028765.1.7",
          "form": "글고 애도..",
          "original_form": "글고 애도..",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028765.1.8",
          "form": "라방 봤는데",
          "original_form": "라방 봤는데",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028765.1.9",
          "form": "먼지도 모르고봤다가",
          "original_form": "먼지도 모르고봤다가",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028765.1.10",
          "form": "대뜸 욕부터 먹음",
          "original_form": "대뜸 욕부터 먹음",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028765.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 맞아",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.12",
          "form": "여친이 인라로 시원하게",
          "original_form": "여친이 인라로 시원하게",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.13",
          "form": "욕했다며",
          "original_form": "욕했다며",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네..",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.15",
          "form": "저는 몰랐거든여 무슨일이 있었는지",
          "original_form": "저는 몰랐거든여 무슨일이 있었는지",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.16",
          "form": "그냥 누가 아무말없이 여친영상이라고 올려서",
          "original_form": "그냥 누가 아무말없이 여친영상이라고 올려서",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.17",
          "form": "진짜 그렇게 버는 돈 하나도 안부러운디....",
          "original_form": "진짜 그렇게 버는 돈 하나도 안부러운디....",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.18",
          "form": "머지 하고 켰다가",
          "original_form": "머지 하고 켰다가",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.19",
          "form": "?????????",
          "original_form": "?????????",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028765.1.20",
          "form": "이렇게됨",
          "original_form": "이렇게됨",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.21",
          "form": "나도 모르는 사이에 열폭러되고...",
          "original_form": "나도 모르는 사이에 열폭러되고...",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 예쁜여자로 살기히들다는거보고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 예쁜여자로 살기히들다는거보고",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.23",
          "form": "ㅋㅋㅋㅋㅋ앗네.... ㅎ.... (그래서 머어쩌라는거지)",
          "original_form": "ㅋㅋㅋㅋㅋ앗네.... ㅎ.... (그래서 머어쩌라는거지)",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.24",
          "form": "이런기분",
          "original_form": "이런기분",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.25",
          "form": "과거사진도 다털렸던데...",
          "original_form": "과거사진도 다털렸던데...",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.26",
          "form": "근데 그 사이에도 선바 여치한테",
          "original_form": "근데 그 사이에도 선바 여치한테",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028765.1.28",
          "form": "시녀생겼더라",
          "original_form": "시녀생겼더라",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028765.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028765.1.30",
          "form": "어이가 아리마셍...",
          "original_form": "어이가 아리마셍...",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028765.1.31",
          "form": "진짜 인간은 알수없는 생물이다",
          "original_form": "진짜 인간은 알수없는 생물이다",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028765.1.32",
          "form": "어떻게 그런 비제이한테도",
          "original_form": "어떻게 그런 비제이한테도",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028765.1.33",
          "form": "딸랑이가붙지?",
          "original_form": "딸랑이가붙지?",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028765.1.34",
          "form": "애초에 선바한테 유사 먹은것도.....",
          "original_form": "애초에 선바한테 유사 먹은것도.....",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028765.1.35",
          "form": "걔가 팬이어서 꼬신건지 뭔지 모르겠지만",
          "original_form": "걔가 팬이어서 꼬신건지 뭔지 모르겠지만",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028765.1.36",
          "form": "걍 멀쩡한 팬 빼고",
          "original_form": "걍 멀쩡한 팬 빼고",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028765.1.37",
          "form": "다 이상해",
          "original_form": "다 이상해",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028765.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028765.1.39",
          "form": "트위터 친목 못하게 막았다며",
          "original_form": "트위터 친목 못하게 막았다며",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028765.1.40",
          "form": "그래서 멀쩡한 팬계정도 팬덤이 못하게 고나리했대",
          "original_form": "그래서 멀쩡한 팬계정도 팬덤이 못하게 고나리했대",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028765.1.41",
          "form": "그리고 친목하지말라고그래놓고",
          "original_form": "그리고 친목하지말라고그래놓고",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028765.1.42",
          "form": "자기가 ㅍㄴ이랑 친목해서 그여친사귄거잖아",
          "original_form": "자기가 ㅍㄴ이랑 친목해서 그여친사귄거잖아",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028765.1.43",
          "form": "팬들 친목고나리하면서\r\n정작자기가하는...",
          "original_form": "팬들 친목고나리하면서\r\n정작자기가하는...",
