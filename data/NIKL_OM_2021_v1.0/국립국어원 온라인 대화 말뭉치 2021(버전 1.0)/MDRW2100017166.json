      "utterance": [
        {
          "id": "MDRW2100017166.1.1",
          "form": "저녁에 뭐드실거에요??",
          "original_form": "저녁에 뭐드실거에요??",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017166.1.2",
          "form": "그냥 집밥먹을거같아요 ㅋㅋㅋ",
          "original_form": "그냥 집밥먹을거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017166.1.3",
          "form": "저는 말나온김에 유가네닭갈비 볶음밥을 시켜먹을까해요ㅋㅋ",
          "original_form": "저는 말나온김에 유가네닭갈비 볶음밥을 시켜먹을까해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017166.1.4",
          "form": "저는 거기 같이 주는 양배추 샐러드도 맛있더라구요",
          "original_form": "저는 거기 같이 주는 양배추 샐러드도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017166.1.5",
          "form": "갑자기 부럽네요전 다이어트중이라..ㅠㅠ",
          "original_form": "갑자기 부럽네요전 다이어트중이라..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017166.1.6",
          "form": "다이어트라니 ㅠ",
          "original_form": "다이어트라니 ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017166.1.7",
          "form": "그럼 닭가슴설 드시는건가요?",
          "original_form": "그럼 닭가슴설 드시는건가요?",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017166.1.8",
          "form": "ㅠㅠ살맛안나요",
          "original_form": "ㅠㅠ살맛안나요",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017166.1.9",
          "form": "아뇨 그렇게까진 안하고 그냥 칼로리만 제한해서 먹고있어요",
          "original_form": "아뇨 그렇게까진 안하고 그냥 칼로리만 제한해서 먹고있어요",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017166.1.10",
          "form": "ㅠㅠ 일반식으로 칼로리 제한이면",
          "original_form": "ㅠㅠ 일반식으로 칼로리 제한이면",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017166.1.11",
          "form": "양이 너무 적어지지 않나요?ㅠ",
          "original_form": "양이 너무 적어지지 않나요?ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017166.1.12",
          "form": "맞아요 식사가아니라 시식한 느낌이에여..",
          "original_form": "맞아요 식사가아니라 시식한 느낌이에여..",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017166.1.13",
          "form": "그 양은 익숙해지지가 않더라구요.... ㅠ",
          "original_form": "그 양은 익숙해지지가 않더라구요.... ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017166.1.14",
          "form": "저는 6개월 닭가슴살만 먹었는데",
          "original_form": "저는 6개월 닭가슴살만 먹었는데",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017166.1.15",
          "form": "두번은 못할것같아여 ㅎ",
          "original_form": "두번은 못할것같아여 ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017166.1.16",
          "form": "맞아요 ㅠㅠ 제위는 또 더럽게 커서..",
          "original_form": "맞아요 ㅠㅠ 제위는 또 더럽게 커서..",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017166.1.17",
          "form": "와 대단하세요",
          "original_form": "와 대단하세요",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017166.1.18",
          "form": "name1님도 대단하세요",
          "original_form": "&name1&님도 대단하세요",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017166.1.19",
          "form": "전 작심이틀이에여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 작심이틀이에여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017166.1.20",
          "form": "이틀마다 마음을 먹으면 되죠!ㅋㅋㅋㅋ",
