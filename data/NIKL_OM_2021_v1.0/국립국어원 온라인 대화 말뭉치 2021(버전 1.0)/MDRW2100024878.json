      "utterance": [
        {
          "id": "MDRW2100024878.1.1",
          "form": "계속 아프네",
          "original_form": "계속 아프네",
          "speaker_id": "2",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024878.1.2",
          "form": "병원은 다녀왔어요?",
          "original_form": "병원은 다녀왔어요?",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024878.1.3",
          "form": "한의원다녀왔어",
          "original_form": "한의원다녀왔어",
          "speaker_id": "2",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024878.1.4",
          "form": "치료는 받았어요?",
          "original_form": "치료는 받았어요?",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024878.1.5",
          "form": "어. 침 맞았지",
          "original_form": "어. 침 맞았지",
          "speaker_id": "2",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024878.1.6",
          "form": "침 맞은지 꽤 된 거 같은데.. 아직도 아픈 거 보면",
          "original_form": "침 맞은지 꽤 된 거 같은데.. 아직도 아픈 거 보면",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024878.1.7",
          "form": "그 한의원 실력 별로인 거 같은데...",
          "original_form": "그 한의원 실력 별로인 거 같은데...",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024878.1.8",
          "form": "침은 꾸준히 맞아야지~",
          "original_form": "침은 꾸준히 맞아야지~",
          "speaker_id": "2",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024878.1.9",
          "form": "그래도 전혀 나아지지를 않고 있는 거 같은데..ㅠㅠ",
          "original_form": "그래도 전혀 나아지지를 않고 있는 거 같은데..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024878.1.10",
          "form": "계속 다니면 낫겠지~",
          "original_form": "계속 다니면 낫겠지~",
          "speaker_id": "2",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024878.1.11",
          "form": "약은 따로 먹는 거 아니에요/",
          "original_form": "약은 따로 먹는 거 아니에요/",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024878.1.12",
          "form": "처음에 근육풀어지는 약 주긴 했었어",
          "original_form": "처음에 근육풀어지는 약 주긴 했었어",
          "speaker_id": "2",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024878.1.13",
          "form": "그거 먹고도 별 차도가 없었던 거에요?",
          "original_form": "그거 먹고도 별 차도가 없었던 거에요?",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024878.1.14",
          "form": "그러게 먹고도 별 소용이 없는 거 같네",
          "original_form": "그러게 먹고도 별 소용이 없는 거 같네",
          "speaker_id": "2",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024878.1.15",
          "form": "흠.. 병원을 옮기는게 좋을 거 같은데..",
          "original_form": "흠.. 병원을 옮기는게 좋을 거 같은데..",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024878.1.16",
          "form": "내일까지 가보고 좋아지지 않으면 다름데 가봐야겠다~",
          "original_form": "내일까지 가보고 좋아지지 않으면 다름데 가봐야겠다~",
          "speaker_id": "2",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024878.1.17",
          "form": "저번에 갔던 한의원이 더 잘하는 듯!!!",
          "original_form": "저번에 갔던 한의원이 더 잘하는 듯!!!",
          "speaker_id": "1",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100024878.1.18",
