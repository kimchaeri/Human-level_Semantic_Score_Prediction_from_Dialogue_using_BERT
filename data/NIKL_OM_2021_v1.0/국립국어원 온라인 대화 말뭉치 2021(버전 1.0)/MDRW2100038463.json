      "utterance": [
        {
          "id": "MDRW2100038463.1.1",
          "form": "근데 사람들은 민초파로 나누더라구요 ㅠㅠ",
          "original_form": "근데 사람들은 민초파로 나누더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038463.1.2",
          "form": "민초파 반민초파 이런거 재밌으니까요ㅋㅋㅋ",
          "original_form": "민초파 반민초파 이런거 재밌으니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100038463.1.3",
          "form": "물복이세요? 딱복이세요?",
          "original_form": "물복이세요? 딱복이세요?",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100038463.1.4",
          "form": "물복파입니당",
          "original_form": "물복파입니당",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038463.1.5",
          "form": "저도 물복이예요",
          "original_form": "저도 물복이예요",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100038463.1.6",
          "form": "이렇게된거 호불호 해봐요",
          "original_form": "이렇게된거 호불호 해봐요",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100038463.1.7",
          "form": "하와이안피자는 좋아하세요?",
          "original_form": "하와이안피자는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038463.1.8",
          "form": "그거근데한번도",
          "original_form": "그거근데한번도",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038463.1.9",
          "form": "안먹어봤는데",
          "original_form": "안먹어봤는데",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038463.1.10",
          "form": "따뜻한 파인에플 좋아하긴해요",
          "original_form": "따뜻한 파인에플 좋아하긴해요",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038463.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038463.1.12",
          "form": "우와 좋아하시는구나",
          "original_form": "우와 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100038463.1.13",
          "form": "저는 맛있다고생각은 하는데 식사로 안어울려서",
          "original_form": "저는 맛있다고생각은 하는데 식사로 안어울려서",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100038463.1.14",
          "form": "싫어해요",
          "original_form": "싫어해요",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100038463.1.15",
          "form": "그러쿤요 안먹어봐서 모르겠네요 ㅋㅋ",
          "original_form": "그러쿤요 안먹어봐서 모르겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100038463.1.16",
          "form": "그럼 또 뭐있을까요",
          "original_form": "그럼 또 뭐있을까요",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100038463.1.17",
          "form": "짜장면이좋으세요? 짬뽕이좋으세요",
          "original_form": "짜장면이좋으세요? 짬뽕이좋으세요",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100038463.1.18",
          "form": "전짜장면에 짬뽕국물좋아해요",
          "original_form": "전짜장면에 짬뽕국물좋아해요",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100038463.1.19",
          "form": "와...조화로우시네요",
          "original_form": "와...조화로우시네요",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100038463.1.20",
          "form": "짬뽕국물 몇숟갈 넣고 비비면 더 맛있더라구요",
          "original_form": "짬뽕국물 몇숟갈 넣고 비비면 더 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038463.1.21",
          "form": "맞죠 ㅋㅋㅋㅋㅋㅋ저런먹는거는고를수없어요 ㅠㅠ",
          "original_form": "맞죠 ㅋㅋㅋㅋㅋㅋ저런먹는거는고를수없어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100038463.1.22",
          "form": "저는 항상고민하는데 매번 짬뽕을 골라요",
          "original_form": "저는 항상고민하는데 매번 짬뽕을 골라요",
          "speaker_id": "1",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100038463.1.23",
          "form": "짬뽕도 조춍",
          "original_form": "짬뽕도 조춍",
          "speaker_id": "2",
          "time": "20210902 16:32"
        }
      ]
    }
