      "utterance": [
        {
          "id": "MDRW2100018381.1.1",
          "form": "자몽이 제겐 쓰더라구요 ㅎㅎㅎ",
          "original_form": "자몽이 제겐 쓰더라구요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:46"
        },
        {
          "id": "MDRW2100018381.1.2",
          "form": "맞아요 저도 사실 그래서 별로 안좋아했는데 언제가부터 좋아지더라고요",
          "original_form": "맞아요 저도 사실 그래서 별로 안좋아했는데 언제가부터 좋아지더라고요",
          "speaker_id": "1",
          "time": "20210827 00:48"
        },
        {
          "id": "MDRW2100018381.1.3",
          "form": "그럼 좋아하는 과일 있으신가요?",
          "original_form": "그럼 좋아하는 과일 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 00:48"
        },
        {
          "id": "MDRW2100018381.1.4",
          "form": "저는 복숭아가 좋아요",
          "original_form": "저는 복숭아가 좋아요",
          "speaker_id": "2",
          "time": "20210827 00:49"
        },
        {
          "id": "MDRW2100018381.1.5",
          "form": "달달한 과일 좋아하시나봐요",
          "original_form": "달달한 과일 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018381.1.6",
          "form": "맞아요 특히 말랑말랑한 복숭아 좋아요",
          "original_form": "맞아요 특히 말랑말랑한 복숭아 좋아요",
          "speaker_id": "2",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018381.1.7",
          "form": "맞아요 딱복보다는 말복이 좀 더 달콤하죠",
          "original_form": "맞아요 딱복보다는 말복이 좀 더 달콤하죠",
          "speaker_id": "1",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018381.1.8",
          "form": "딱복은 약간 무 같다는 생각을 해요",
          "original_form": "딱복은 약간 무 같다는 생각을 해요",
          "speaker_id": "2",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018381.1.9",
          "form": "ㅋㅋㅋㅋㅋ맞아요 잘 목 먹으면 단 맛은 없는데 딱딱한 식감만 느껴지니까 가끔 그렇죠",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 잘 목 먹으면 단 맛은 없는데 딱딱한 식감만 느껴지니까 가끔 그렇죠",
          "speaker_id": "1",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018381.1.10",
          "form": "귤도 좋아해요",
          "original_form": "귤도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018381.1.11",
          "form": "그럼 여름엔 복숭아먹고 겨울엔 귤 드시나요?",
          "original_form": "그럼 여름엔 복숭아먹고 겨울엔 귤 드시나요?",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100018381.1.12",
          "form": "정확하십니다 ㅎㅎㅎㅎ 가을엔 뭐가좋을까요",
          "original_form": "정확하십니다 ㅎㅎㅎㅎ 가을엔 뭐가좋을까요",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018381.1.13",
          "form": "흠...가을에 나는 과일이 뭐가 있을까요?",
          "original_form": "흠...가을에 나는 과일이 뭐가 있을까요?",
          "speaker_id": "1",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018381.1.14",
          "form": "무화과가 지금이려나요",
          "original_form": "무화과가 지금이려나요",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018381.1.15",
          "form": "그런것 같기도 해요",
          "original_form": "그런것 같기도 해요",
