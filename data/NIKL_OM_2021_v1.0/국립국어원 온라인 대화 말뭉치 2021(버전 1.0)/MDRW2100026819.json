      "utterance": [
        {
          "id": "MDRW2100026819.1.1",
          "form": "뭐??",
          "original_form": "뭐??",
          "speaker_id": "2",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026819.1.2",
          "form": "근디 그거 그림체는 귀여운데",
          "original_form": "근디 그거 그림체는 귀여운데",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.3",
          "form": "내용은 무서운거라",
          "original_form": "내용은 무서운거라",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.4",
          "form": "더 무서움",
          "original_form": "더 무서움",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.5",
          "form": "맞아ㅋㅋㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.6",
          "form": "그그",
          "original_form": "그그",
          "speaker_id": "1",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.7",
          "form": "사신 나오는편인데",
          "original_form": "사신 나오는편인데",
          "speaker_id": "1",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.8",
          "form": "어른은 안죽이고",
          "original_form": "어른은 안죽이고",
          "speaker_id": "1",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.9",
          "form": "어린이들만 따라다니는..",
          "original_form": "어린이들만 따라다니는..",
          "speaker_id": "1",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.11",
          "form": "듣기만해도 무섭다",
          "original_form": "듣기만해도 무섭다",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.12",
          "form": "난 그 터널에서",
          "original_form": "난 그 터널에서",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026819.1.13",
          "form": "내가 그때 초딩이었어서ㅋㅋㅋㅋ개무서웠어",
          "original_form": "내가 그때 초딩이었어서ㅋㅋㅋㅋ개무서웠어",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.14",
          "form": "귀신이 운전자 다리잡고",
          "original_form": "귀신이 운전자 다리잡고",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.15",
          "form": "으으으억",
          "original_form": "으으으억",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.16",
          "form": "뭐 그런편 무서웠음",
          "original_form": "뭐 그런편 무서웠음",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.17",
          "form": "근데 무서운게 딱 좋아 알제",
          "original_form": "근데 무서운게 딱 좋아 알제",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.18",
          "form": "ㅇㅇㅇㅇㅇㅇㅇㅇ당연하지",
          "original_form": "ㅇㅇㅇㅇㅇㅇㅇㅇ당연하지",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.19",
          "form": "그거 웹툰으로 나왔잖아",
          "original_form": "그거 웹툰으로 나왔잖아",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.21",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.22",
          "form": "나 별로 안무서울줄 알았는데",
          "original_form": "나 별로 안무서울줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026819.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.24",
          "form": "너무 무섭더라",
          "original_form": "너무 무섭더라",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.25",
          "form": "개무서움",
          "original_form": "개무서움",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.26",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.27",
          "form": "내가 그거때문에 초딩때",
          "original_form": "내가 그거때문에 초딩때",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.28",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.29",
          "form": "거울을 제대로못봤음",
          "original_form": "거울을 제대로못봤음",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.30",
          "form": "응응ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "응응ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.31",
          "form": "거울 관련해서 무서운 에피소드들이 너무많아서..",
          "original_form": "거울 관련해서 무서운 에피소드들이 너무많아서..",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.32",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무무서웠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무무서웠다",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026819.1.34",
          "form": "막 거울로 귀신 들어왔다 나갔다 하고",
          "original_form": "막 거울로 귀신 들어왔다 나갔다 하고",
