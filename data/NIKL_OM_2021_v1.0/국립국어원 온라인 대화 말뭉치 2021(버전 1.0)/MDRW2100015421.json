      "utterance": [
        {
          "id": "MDRW2100015421.1.1",
          "form": "난 오늘 하루종일 김볶밥 먹었지롱",
          "original_form": "난 오늘 하루종일 김볶밥 먹었지롱",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015421.1.2",
          "form": "김치볶음밥... 맛있어...",
          "original_form": "김치볶음밥... 맛있어...",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015421.1.3",
          "form": "안질려... 최고야...",
          "original_form": "안질려... 최고야...",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015421.1.4",
          "form": "와 맛있겠당",
          "original_form": "와 맛있겠당",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015421.1.5",
          "form": "ㅇ0ㅇ",
          "original_form": "ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015421.1.6",
          "form": "전 오징어젓갈요 ㅎ",
          "original_form": "전 오징어젓갈요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015421.1.7",
          "form": "며칠째야? ㅋㅋㅋㅋㅋ",
          "original_form": "며칠째야? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015421.1.8",
          "form": "일주일은 넘은거같은데",
          "original_form": "일주일은 넘은거같은데",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015421.1.9",
          "form": "헤아려보지 않았어요...",
          "original_form": "헤아려보지 않았어요...",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015421.1.10",
          "form": "다른게 먹고싶음",
          "original_form": "다른게 먹고싶음",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015421.1.11",
          "form": "맛없진않지만...",
          "original_form": "맛없진않지만...",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015421.1.12",
          "form": "매워..",
          "original_form": "매워..",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015421.1.13",
          "form": "음 너 매운거 잘 못먹지 않아?",
          "original_form": "음 너 매운거 잘 못먹지 않아?",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015421.1.14",
          "form": "혀는 버티지만",
          "original_form": "혀는 버티지만",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015421.1.15",
          "form": "장기적인 의미로...",
          "original_form": "장기적인 의미로...",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015421.1.16",
          "form": "넹...그래서",
          "original_form": "넹...그래서",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015421.1.17",
          "form": "괴로운것 ㅎㅎ...",
          "original_form": "괴로운것 ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015421.1.18",
          "form": "계란찜 같은거랑 같이 먹으면 좋은데",
          "original_form": "계란찜 같은거랑 같이 먹으면 좋은데",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015421.1.19",
          "form": "요즘 계란값 너무 비싸",
          "original_form": "요즘 계란값 너무 비싸",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015421.1.20",
          "form": "ㄱㅖ란은 그냥 계란만 먹을래두",
          "original_form": "ㄱㅖ란은 그냥 계란만 먹을래두",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015421.1.21",
          "form": "비싸서...",
          "original_form": "비싸서...",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015421.1.22",
          "form": "ㅋㅋ큐ㅠ 두부라도... 먹어야...하나...",
          "original_form": "ㅋㅋ큐ㅠ 두부라도... 먹어야...하나...",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015421.1.23",
          "form": "원래 가장 만만한 식재료는 계란이었는데",
          "original_form": "원래 가장 만만한 식재료는 계란이었는데",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015421.1.24",
          "form": "이렇게 되어서 곤란해",
          "original_form": "이렇게 되어서 곤란해",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015421.1.25",
          "form": "계란 이제 만만하지않아요...ㅠㅠ",
          "original_form": "계란 이제 만만하지않아요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015421.1.26",
          "form": "편의점 계란샌드위치도 점점 양이 줄고있음여",
          "original_form": "편의점 계란샌드위치도 점점 양이 줄고있음여",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015421.1.27",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015421.1.28",
          "form": "올라가는건 순식간인데",
          "original_form": "올라가는건 순식간인데",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.29",
          "form": "ㅎㅎ...",
          "original_form": "ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.30",
          "form": "내려오지를 않아",
          "original_form": "내려오지를 않아",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.31",
          "form": "내 계란 장...조...림..",
          "original_form": "내 계란 장...조...림..",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.32",
          "form": "장조림 저",
          "original_form": "장조림 저",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.33",
          "form": "짱좋아하는데",
          "original_form": "짱좋아하는데",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.34",
          "form": "고급음식됨",
          "original_form": "고급음식됨",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.35",
          "form": "고오급...음식",
          "original_form": "고오급...음식",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015421.1.36",
          "form": "전 메추리알 장조림이 더좋아요",
          "original_form": "전 메추리알 장조림이 더좋아요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015421.1.37",
          "form": "간이 잘배서",
          "original_form": "간이 잘배서",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015421.1.38",
          "form": "와 찾아보니까 한판에 9천원 하네 계란이",
          "original_form": "와 찾아보니까 한판에 9천원 하네 계란이",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015421.1.39",
          "form": "어쩌다 두배가 됐냐",
          "original_form": "어쩌다 두배가 됐냐",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015421.1.40",
          "form": "이쯤이면 집에서 암탉 키워야할듯",
          "original_form": "이쯤이면 집에서 암탉 키워야할듯",
          "speaker_id": "2",
          "time": "20210825 23:08"
        }
      ]
    }
