      "utterance": [
        {
          "id": "MDRW2100029655.1.1",
          "form": "확률이너무낮아요",
          "original_form": "확률이너무낮아요",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029655.1.2",
          "form": "퍼즐앤드레곤이라는",
          "original_form": "퍼즐앤드레곤이라는",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029655.1.3",
          "form": "게임이 있는데",
          "original_form": "게임이 있는데",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029655.1.4",
          "form": "퍼즐게임이군요",
          "original_form": "퍼즐게임이군요",
          "speaker_id": "1",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029655.1.5",
          "form": "그것도 캐릭터 뽑기 게임이거든요",
          "original_form": "그것도 캐릭터 뽑기 게임이거든요",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029655.1.6",
          "form": "좋은캐릭뽑으면 퍼즐이 쉬워져요",
          "original_form": "좋은캐릭뽑으면 퍼즐이 쉬워져요",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029655.1.7",
          "form": "그렇죠 현질의 시작",
          "original_form": "그렇죠 현질의 시작",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029655.1.8",
          "form": "어떨땐 두번만에 좋은거 나오고",
          "original_form": "어떨땐 두번만에 좋은거 나오고",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029655.1.9",
          "form": "어떨땐 수십개를 뽑아도 꽝이에요",
          "original_form": "어떨땐 수십개를 뽑아도 꽝이에요",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029655.1.10",
          "form": "그래도 매일하죠",
          "original_form": "그래도 매일하죠",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029655.1.11",
          "form": "알고도 당하죠",
          "original_form": "알고도 당하죠",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029655.1.12",
          "form": "실력으로만 하기 힘드네요",
          "original_form": "실력으로만 하기 힘드네요",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.13",
          "form": "돈이 최고에요",
          "original_form": "돈이 최고에요",
          "speaker_id": "1",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.14",
          "form": "쿠키런킹덤?",
          "original_form": "쿠키런킹덤?",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.15",
          "form": "그런거처럼요",
          "original_form": "그런거처럼요",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.16",
          "form": "하지만 대충하겧읍니다",
          "original_form": "하지만 대충하겧읍니다",
          "speaker_id": "1",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.17",
          "form": "공짜만하고있습니다",
          "original_form": "공짜만하고있습니다",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.18",
          "form": "광고도 자주 보구요",
          "original_form": "광고도 자주 보구요",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.19",
          "form": "네 재미로만하죠",
          "original_form": "네 재미로만하죠",
          "speaker_id": "1",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029655.1.20",
          "form": "광고보는거 개이득이죠",
          "original_form": "광고보는거 개이득이죠",
          "speaker_id": "1",
          "time": "20210903 11:26"
        }
