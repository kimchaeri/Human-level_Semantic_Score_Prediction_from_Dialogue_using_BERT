      "utterance": [
        {
          "id": "MDRW2100020744.1.1",
          "form": "오 미세먼지 수치가 점점 떨어지고 있어 ㅎㅎ",
          "original_form": "오 미세먼지 수치가 점점 떨어지고 있어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020744.1.2",
          "form": "그러네~ 멀리까지 잘 보이네ㅎㅎ",
          "original_form": "그러네~ 멀리까지 잘 보이네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020744.1.3",
          "form": "오후에는 산책 가도 되겠다",
          "original_form": "오후에는 산책 가도 되겠다",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020744.1.4",
          "form": "ㅇㅇ ㅎㅎ 동네 한바퀴나 어제 말한 푸른 수목원 가자",
          "original_form": "ㅇㅇ ㅎㅎ 동네 한바퀴나 어제 말한 푸른 수목원 가자",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020744.1.5",
          "form": "비온 뒤라 시원하고 쾌적하겠네",
          "original_form": "비온 뒤라 시원하고 쾌적하겠네",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020744.1.6",
          "form": "미세먼지 보통일 때 얼른 돌고 와야지 ㅎㅎ",
          "original_form": "미세먼지 보통일 때 얼른 돌고 와야지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020744.1.7",
          "form": "일단 점심 먹고 커피 한 잔 한 다음 ㅋㅋ",
          "original_form": "일단 점심 먹고 커피 한 잔 한 다음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020744.1.8",
          "form": "오후 늦게나 갈 수 있겠는걸? ㅋㅋ",
          "original_form": "오후 늦게나 갈 수 있겠는걸? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020744.1.9",
          "form": "언제든 좋아 ㅎㅎ",
          "original_form": "언제든 좋아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020744.1.10",
          "form": "요즘 단풍 든 나무들도 간혹 보이더라고",
          "original_form": "요즘 단풍 든 나무들도 간혹 보이더라고",
          "speaker_id": "1",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020744.1.11",
          "form": "곧 알록달록 예뻐지겠당",
          "original_form": "곧 알록달록 예뻐지겠당",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020744.1.12",
          "form": "몇 주 지나면 아주 볼만할듯",
          "original_form": "몇 주 지나면 아주 볼만할듯",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020744.1.13",
          "form": "ㅎㅎ 기대만땅",
          "original_form": "ㅎㅎ 기대만땅",
