      "utterance": [
        {
          "id": "MDRW2100037019.1.1",
          "form": "지금 키우는 동물 있으신가요? 전 물고기 키우고 있어요",
          "original_form": "지금 키우는 동물 있으신가요? 전 물고기 키우고 있어요",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100037019.1.2",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100037019.1.3",
          "form": "저는 머 키우는거에는 소질이 없나봐요",
          "original_form": "저는 머 키우는거에는 소질이 없나봐요",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037019.1.4",
          "form": "식물도 제손에만오면 죽어서나가네요 ㅎ",
          "original_form": "식물도 제손에만오면 죽어서나가네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037019.1.5",
          "form": "물고기 많으신가요?",
          "original_form": "물고기 많으신가요?",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037019.1.6",
          "form": "구피 키우고 있는데.. 초보자용이라 키우기 쉬워요",
          "original_form": "구피 키우고 있는데.. 초보자용이라 키우기 쉬워요",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100037019.1.7",
          "form": "그럼 동물이고 식물이고 집에서 키우는 건 없는건가요?",
          "original_form": "그럼 동물이고 식물이고 집에서 키우는 건 없는건가요?",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100037019.1.8",
          "form": "네 ㅋㅋ\r\n아이둘만 키워요 ㅋ",
          "original_form": "네 ㅋㅋ\r\n아이둘만 키워요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037019.1.9",
          "form": "아이들 키우면 된거죠..ㅎㅎㅎ",
          "original_form": "아이들 키우면 된거죠..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100037019.1.10",
          "form": "저도 아이들 키울때는 동물이랑 식물 많이 못 키웠어요",
          "original_form": "저도 아이들 키울때는 동물이랑 식물 많이 못 키웠어요",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100037019.1.11",
          "form": "네 정신없어요",
          "original_form": "네 정신없어요",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100037019.1.12",
          "form": "하루 눈뜸과 동시에 할일투성이예요 ㅎ",
          "original_form": "하루 눈뜸과 동시에 할일투성이예요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100037019.1.13",
          "form": "지금은 좀 여유가 생겨서.. 구피도 키우고.. 식물들도 많이 키우고 있죠..ㅎㅎ",
          "original_form": "지금은 좀 여유가 생겨서.. 구피도 키우고.. 식물들도 많이 키우고 있죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 06:44"
        },
        {
          "id": "MDRW2100037019.1.14",
          "form": "그 여유 저도 느끼고싶네요 ㅎ",
          "original_form": "그 여유 저도 느끼고싶네요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037019.1.15",
          "form": "곧 느낄거에요.. 아이들은 금방 크거든요..ㅎㅎ",
          "original_form": "곧 느낄거에요.. 아이들은 금방 크거든요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037019.1.16",
          "form": "그러게요 지나고나면 후딱일듯한데 ㅎㅎ",
          "original_form": "그러게요 지나고나면 후딱일듯한데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100037019.1.17",
          "form": "그래요.. 전 사실 아이들 키울때 아이들이 원해서 강아지 키운적 있어요",
          "original_form": "그래요.. 전 사실 아이들 키울때 아이들이 원해서 강아지 키운적 있어요",
          "speaker_id": "1",
          "time": "20210902 06:16"
        },
        {
          "id": "MDRW2100037019.1.18",
          "form": "근데.. 결국 뒷치닥거리는 엄마인 내가 해야해서",
          "original_form": "근데.. 결국 뒷치닥거리는 엄마인 내가 해야해서",
          "speaker_id": "1",
          "time": "20210902 06:16"
        },
        {
          "id": "MDRW2100037019.1.19",
          "form": "아이들은 그저 이뻐만 해주죠",
          "original_form": "아이들은 그저 이뻐만 해주죠",
          "speaker_id": "1",
          "time": "20210902 06:16"
        },
        {
          "id": "MDRW2100037019.1.20",
          "form": "지금도 아이들이 고양이 강아지 키우고싶다고",
          "original_form": "지금도 아이들이 고양이 강아지 키우고싶다고",
          "speaker_id": "2",
          "time": "20210902 06:56"
        },
        {
          "id": "MDRW2100037019.1.21",
          "form": "결국 제가해야할것을 알기에",
          "original_form": "결국 제가해야할것을 알기에",
          "speaker_id": "2",
          "time": "20210902 06:57"
        },
        {
          "id": "MDRW2100037019.1.22",
          "form": "절대 안된다고 하고있어요 ㅎ",
          "original_form": "절대 안된다고 하고있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 06:57"
