      "utterance": [
        {
          "id": "MDRW2100030933.1.1",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100030933.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100030933.1.3",
          "form": "중고거랜 안해요",
          "original_form": "중고거랜 안해요",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030933.1.4",
          "form": "아 그러시군요 최근 쇼핑하신 물건은요?",
          "original_form": "아 그러시군요 최근 쇼핑하신 물건은요?",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030933.1.5",
          "form": "거의 먹는것만사요",
          "original_form": "거의 먹는것만사요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030933.1.6",
          "form": "청정원에서 소스류나 냉동사고 핫딜있음 그런거하구요",
          "original_form": "청정원에서 소스류나 냉동사고 핫딜있음 그런거하구요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030933.1.7",
          "form": "청정원 스파게티 소스요? 다른 소스도 있난ㅅ?",
          "original_form": "청정원 스파게티 소스요? 다른 소스도 있난ㅅ?",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030933.1.8",
          "form": "있나요",
          "original_form": "있나요",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030933.1.9",
          "form": "돈가스소스. 케찹. 마요네즈. 머스타드 등등 다있어요~~",
          "original_form": "돈가스소스. 케찹. 마요네즈. 머스타드 등등 다있어요~~",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100030933.1.10",
          "form": "신한이나 삼성에서 40퍼쿠폰받아서 사요",
          "original_form": "신한이나 삼성에서 40퍼쿠폰받아서 사요",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030933.1.11",
          "form": "아 종류별로 사시는군요 청정원이 맛있나요?",
          "original_form": "아 종류별로 사시는군요 청정원이 맛있나요?",
          "speaker_id": "1",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100030933.1.12",
          "form": "네 맛도 괜찮아요. 원래 먹던거라그런지 전 거부감없더라구요",
          "original_form": "네 맛도 괜찮아요. 원래 먹던거라그런지 전 거부감없더라구요",
          "speaker_id": "2",
          "time": "20210904 14:40"
        },
        {
          "id": "MDRW2100030933.1.13",
          "form": "40퍼 쿠폰은 카드홈페이지나 앱인가요?",
          "original_form": "40퍼 쿠폰은 카드홈페이지나 앱인가요?",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100030933.1.14",
          "form": "네 카드요.",
          "original_form": "네 카드요.",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030933.1.15",
          "form": "알뜰하시네요",
          "original_form": "알뜰하시네요",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100030933.1.16",
          "form": "ㅎ 요즘에 관심이많아서~~~ 인터넷폐지줍기? 엄청하고있어요",
          "original_form": "ㅎ 요즘에 관심이많아서~~~ 인터넷폐지줍기? 엄청하고있어요",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100030933.1.17",
          "form": "소비가 많이줄더라구요",
          "original_form": "소비가 많이줄더라구요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030933.1.18",
          "form": "그건 뭔가요?처음듣네요",
          "original_form": "그건 뭔가요?처음듣네요",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030933.1.19",
          "form": "출석같은거해서 10원씩 버는거요",
          "original_form": "출석같은거해서 10원씩 버는거요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
