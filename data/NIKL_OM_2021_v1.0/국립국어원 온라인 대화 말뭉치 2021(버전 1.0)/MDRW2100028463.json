      "utterance": [
        {
          "id": "MDRW2100028463.1.1",
          "form": "연애와 결혼에 관심 있으신가요",
          "original_form": "연애와 결혼에 관심 있으신가요",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028463.1.2",
          "form": "연애는 관심 있는데 결혼은 아직요ㅋㅋ",
          "original_form": "연애는 관심 있는데 결혼은 아직요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028463.1.3",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028463.1.4",
          "form": "아니요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028463.1.5",
          "form": "연애도 결혼도 관심이 없어요",
          "original_form": "연애도 결혼도 관심이 없어요",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028463.1.6",
          "form": "어떤분과 연애하고 싶으신가요",
          "original_form": "어떤분과 연애하고 싶으신가요",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028463.1.7",
          "form": "일단 대화가 잘 통하는 사람요ㅎㅎ",
          "original_form": "일단 대화가 잘 통하는 사람요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028463.1.8",
          "form": "아 그럼 혹시 비혼주의자신가요?",
          "original_form": "아 그럼 혹시 비혼주의자신가요?",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028463.1.9",
          "form": "맞아요 일단 대화가 잘 통해야죠 ㅎㅎ",
          "original_form": "맞아요 일단 대화가 잘 통해야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028463.1.10",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028463.1.11",
          "form": "비혼주의자예요",
          "original_form": "비혼주의자예요",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028463.1.12",
          "form": "오 저두요 결혼은 할 생각이 없어요",
          "original_form": "오 저두요 결혼은 할 생각이 없어요",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028463.1.13",
          "form": "제 주위도 몇 명 빼고는 다 결혼 생각이 없더라구요.",
          "original_form": "제 주위도 몇 명 빼고는 다 결혼 생각이 없더라구요.",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028463.1.14",
          "form": "요즘 이십대 여성들이 많이들 그러는거 같아요",
          "original_form": "요즘 이십대 여성들이 많이들 그러는거 같아요",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028463.1.15",
          "form": "제 주변도 그래요",
          "original_form": "제 주변도 그래요",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028463.1.16",
          "form": "아예 비연애주의자들도 꽤 있구요",
          "original_form": "아예 비연애주의자들도 꽤 있구요",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028463.1.17",
          "form": "오 진짜요? 아직 비연애주의자는 못봤어요",
          "original_form": "오 진짜요? 아직 비연애주의자는 못봤어요",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028463.1.18",
          "form": "혹시 연애에 관심이 없는 이유는 뭘까요?",
          "original_form": "혹시 연애에 관심이 없는 이유는 뭘까요?",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028463.1.19",
          "form": "친구들이랑 시간 보내는게 더 즐거운 것 같아요",
          "original_form": "친구들이랑 시간 보내는게 더 즐거운 것 같아요",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028463.1.20",
          "form": "물론 남자친구도 친구이긴 하지만",
          "original_form": "물론 남자친구도 친구이긴 하지만",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028463.1.21",
          "form": "아 그렇죠 친구들이 더 편하니까ㅋㅋ",
          "original_form": "아 그렇죠 친구들이 더 편하니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028463.1.22",
          "form": "요새는 친구들도 바빠서 다 맞는 시간 잡기가 힘들더라구요..코로나 때문도 있고ㅠ",
          "original_form": "요새는 친구들도 바빠서 다 맞는 시간 잡기가 힘들더라구요..코로나 때문도 있고ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028463.1.23",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028463.1.24",
          "form": "저는 취준하는 친구들이 많아서 요즘 좀 외로울 때 있긴 하네요 ㅋ큐ㅠ",
          "original_form": "저는 취준하는 친구들이 많아서 요즘 좀 외로울 때 있긴 하네요 ㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028463.1.25",
          "form": "헐 저두요. 제 친구들도 취준중이거나 일하는 중이라 단톡방 조용해질때 좀 외롭더라구요ㅋㅋ..",
          "original_form": "헐 저두요. 제 친구들도 취준중이거나 일하는 중이라 단톡방 조용해질때 좀 외롭더라구요ㅋㅋ..",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028463.1.26",
          "form": "ㅋㅋㅋ 저희 나이또래가 다 겪는 일인가보네요ㅠ",
          "original_form": "ㅋㅋㅋ 저희 나이또래가 다 겪는 일인가보네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:41"
        }
