      "utterance": [
        {
          "id": "MDRW2100039855.1.1",
          "form": "아ㅠㅠ또 바삐 바로 편입 준비중이시군요",
          "original_form": "아ㅠㅠ또 바삐 바로 편입 준비중이시군요",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039855.1.2",
          "form": "어디로 가고싶으세용",
          "original_form": "어디로 가고싶으세용",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039855.1.3",
          "form": "넵ㅠㅠ 학교가 마음에 안들어서요",
          "original_form": "넵ㅠㅠ 학교가 마음에 안들어서요",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039855.1.4",
          "form": "서울과기대요!",
          "original_form": "서울과기대요!",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039855.1.5",
          "form": "아직 영어가ㅋㅋㅋㅠㅜㅜ",
          "original_form": "아직 영어가ㅋㅋㅋㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039855.1.6",
          "form": "오 서울과기대 가고싶으시구나!",
          "original_form": "오 서울과기대 가고싶으시구나!",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039855.1.7",
          "form": "영어 공부 중이신가요",
          "original_form": "영어 공부 중이신가요",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039855.1.8",
          "form": "저도 편입 준비하다가 ㅠㅠ 교통사고당해서",
          "original_form": "저도 편입 준비하다가 ㅠㅠ 교통사고당해서",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.9",
          "form": "학부 졸업하고 대학원을 가고싶던 학교로 진학했어요",
          "original_form": "학부 졸업하고 대학원을 가고싶던 학교로 진학했어요",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.10",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.11",
          "form": "토익 공부중인데 정말",
          "original_form": "토익 공부중인데 정말",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.12",
          "form": "안되더라고요",
          "original_form": "안되더라고요",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.13",
          "form": "어렵죠 토익 ㅠㅠ",
          "original_form": "어렵죠 토익 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.14",
          "form": "교통사고ㅠㅠㅠ 아고...",
          "original_form": "교통사고ㅠㅠㅠ 아고...",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.15",
          "form": "편입 하려고 교류로 다니기도했어요",
          "original_form": "편입 하려고 교류로 다니기도했어요",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039855.1.16",
          "form": "대학원 진짜 멋있네요",
          "original_form": "대학원 진짜 멋있네요",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.17",
          "form": "그때 기억으로 만족합니다ㅠㅠ",
          "original_form": "그때 기억으로 만족합니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.18",
          "form": "헉 멋있어요",
          "original_form": "헉 멋있어요",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.19",
          "form": "다녀는 봤기땜에 후회는 없어요 ㅋㅋ",
          "original_form": "다녀는 봤기땜에 후회는 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.21",
          "form": "ㅋㅋㅋㅋㅋ대리만족? 합리화",
          "original_form": "ㅋㅋㅋㅋㅋ대리만족? 합리화",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.22",
          "form": "꼭 편입 성공하세용!!!",
          "original_form": "꼭 편입 성공하세용!!!",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.23",
          "form": "와 진짜... 멋있으세요",
          "original_form": "와 진짜... 멋있으세요",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.24",
          "form": "고럼 이제 2학년 이세요?",
          "original_form": "고럼 이제 2학년 이세요?",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.25",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039855.1.26",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "2",
