      "utterance": [
        {
          "id": "MDRW2100003716.1.1",
          "form": "저 어제 제주도에서 돌아왔어요",
          "original_form": "저 어제 제주도에서 돌아왔어요",
          "speaker_id": "2",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003716.1.2",
          "form": "우와~~ 어떠셨나요?",
          "original_form": "우와~~ 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003716.1.3",
          "form": "4단계여도 자유롭게 볼수있나요~?",
          "original_form": "4단계여도 자유롭게 볼수있나요~?",
          "speaker_id": "1",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003716.1.4",
          "form": "날이 좋아서 다행이었어요",
          "original_form": "날이 좋아서 다행이었어요",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003716.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003716.1.6",
          "form": "관광지에 사람 바글바글해요...",
          "original_form": "관광지에 사람 바글바글해요...",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003716.1.7",
          "form": "에고ㅠㅠ 조금 걱정되긴했겠네여ㅠ",
          "original_form": "에고ㅠㅠ 조금 걱정되긴했겠네여ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003716.1.8",
          "form": "얼마나 있다오신거에요?",
          "original_form": "얼마나 있다오신거에요?",
          "speaker_id": "1",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003716.1.9",
          "form": "2박 3일이요",
          "original_form": "2박 3일이요",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003716.1.10",
          "form": "충동적으로 간거였어요",
          "original_form": "충동적으로 간거였어요",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003716.1.11",
          "form": "티켓도 새벽에 공항가면서 끊고",
          "original_form": "티켓도 새벽에 공항가면서 끊고",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003716.1.12",
          "form": "숙소는 그날그날 잡았어요",
          "original_form": "숙소는 그날그날 잡았어요",
          "speaker_id": "2",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003716.1.13",
          "form": "아하 진짜 갑자기긴하네요",
          "original_form": "아하 진짜 갑자기긴하네요",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003716.1.14",
          "form": "그래도 재밌을것같아요~",
          "original_form": "그래도 재밌을것같아요~",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003716.1.15",
          "form": "친구나 가족?! 이랑 다녀오신거에요??",
          "original_form": "친구나 가족?! 이랑 다녀오신거에요??",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003716.1.16",
          "form": "남자친구랑 다녀왔어요ㅎㅎ",
          "original_form": "남자친구랑 다녀왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003716.1.17",
          "form": "오오~~ 제주도는 진짜 안간지 오래됬는데 요즘에는 제주도 가면 어디로 가나요~?",
          "original_form": "오오~~ 제주도는 진짜 안간지 오래됬는데 요즘에는 제주도 가면 어디로 가나요~?",
          "speaker_id": "1",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003716.1.18",
          "form": "전 그냥 해안도로 일주하고왔는데 인스타보면 힙한곳 많더라구요",
          "original_form": "전 그냥 해안도로 일주하고왔는데 인스타보면 힙한곳 많더라구요",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003716.1.19",
          "form": "사람 많은곳은 무서워서 피했어요ㅎㅎ",
          "original_form": "사람 많은곳은 무서워서 피했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003716.1.20",
          "form": "관광지도 폭포만보고...",
          "original_form": "관광지도 폭포만보고...",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003716.1.21",
          "form": "아하 가보고싶은곳들은 사람들이 많긴 하겠네용ㅠ",
          "original_form": "아하 가보고싶은곳들은 사람들이 많긴 하겠네용ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:17"
        },
        {
          "id": "MDRW2100003716.1.22",
          "form": "맛있는것도 많이 드셨나요?!",
          "original_form": "맛있는것도 많이 드셨나요?!",
          "speaker_id": "1",
          "time": "20210821 11:17"
        },
        {
          "id": "MDRW2100003716.1.23",
          "form": "네ㅋㅋ한끼에 생선구이 6종류 먹어보고...",
          "original_form": "네ㅋㅋ한끼에 생선구이 6종류 먹어보고...",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003716.1.24",
          "form": "아주 끝장을 보고왔어요",
          "original_form": "아주 끝장을 보고왔어요",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003716.1.25",
          "form": "스트레스 받아서 충동적으로 간것치고 잘 하고온거같아요",
          "original_form": "스트레스 받아서 충동적으로 간것치고 잘 하고온거같아요",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003716.1.26",
          "form": "무계획이 베스트가 맞나봥ᆢㄷ",
          "original_form": "무계획이 베스트가 맞나봥ᆢㄷ",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003716.1.27",
          "form": "무계획이 베스트가 맞나봐요",
          "original_form": "무계획이 베스트가 맞나봐요",
          "speaker_id": "2",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003716.1.28",
          "form": "우왕ㅋㅋㅋㅋ 오히려 계획대로하면 그걸지키려교 스트레스받기도한데",
          "original_form": "우왕ㅋㅋㅋㅋ 오히려 계획대로하면 그걸지키려교 스트레스받기도한데",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003716.1.29",
          "form": "그냥 무계획이 어찌보묜 나은것같네용ㅋ",
          "original_form": "그냥 무계획이 어찌보묜 나은것같네용ㅋ",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003716.1.30",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003716.1.31",
          "form": "진짜 계획없이갔는데",
          "original_form": "진짜 계획없이갔는데",
          "speaker_id": "2",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003716.1.32",
          "form": "계획해서 간것보다 만족도 최고였어요",
          "original_form": "계획해서 간것보다 만족도 최고였어요",
