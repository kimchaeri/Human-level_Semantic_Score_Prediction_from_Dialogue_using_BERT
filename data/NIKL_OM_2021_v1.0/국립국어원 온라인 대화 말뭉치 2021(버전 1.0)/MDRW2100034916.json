      "utterance": [
        {
          "id": "MDRW2100034916.1.1",
          "form": "전시회는 좋아하세요?",
          "original_form": "전시회는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100034916.1.2",
          "form": "사진전하고 미술전은 꽤 좋아해요!",
          "original_form": "사진전하고 미술전은 꽤 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034916.1.3",
          "form": "오오 ㅎㅎ 저도 좋아하는데 볼 기회가 잘 없네요 ㅠㅠ",
          "original_form": "오오 ㅎㅎ 저도 좋아하는데 볼 기회가 잘 없네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100034916.1.4",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100034916.1.5",
          "form": "ㅅ요즘은 특히 더 그러네요",
          "original_form": "ㅅ요즘은 특히 더 그러네요",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100034916.1.6",
          "form": "보통 작가님 보고 가시나요?",
          "original_form": "보통 작가님 보고 가시나요?",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100034916.1.7",
          "form": "음 주제를 보고 가는경우가 많아요!",
          "original_form": "음 주제를 보고 가는경우가 많아요!",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100034916.1.8",
          "form": "오오 보통 어떤 주제로 가세요?",
          "original_form": "오오 보통 어떤 주제로 가세요?",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100034916.1.9",
          "form": "인물관련된게 좋아서 그쪽으로 더 가게되더라구요!",
          "original_form": "인물관련된게 좋아서 그쪽으로 더 가게되더라구요!",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100034916.1.10",
          "form": "오오 ! 전 인물관련은 한번도 안가봤는뎌",
          "original_form": "오오 ! 전 인물관련은 한번도 안가봤는뎌",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100034916.1.11",
          "form": "어떤 느낌이에요?",
          "original_form": "어떤 느낌이에요?",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100034916.1.12",
          "form": "표정에 나타나는 감정이나 분위기가 상상력을 자극하는 느낌이에요!",
          "original_form": "표정에 나타나는 감정이나 분위기가 상상력을 자극하는 느낌이에요!",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100034916.1.13",
          "form": "오오 ! 하긴 전시회는 상상하는게 또 좋은거같아요 ㅎㅎ",
          "original_form": "오오 ! 하긴 전시회는 상상하는게 또 좋은거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100034916.1.14",
          "form": "사람마다 다르게 상상하는게 신기하기도 하구요 ㅋㅋ",
          "original_form": "사람마다 다르게 상상하는게 신기하기도 하구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100034916.1.15",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100034916.1.16",
          "form": "그런의미에서 진짜 좋은거같아요ㅎㅎ",
          "original_form": "그런의미에서 진짜 좋은거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100034916.1.17",
          "form": "저도 사진찍는걸 좋아해서 사진전을 좋아하는데",
          "original_form": "저도 사진찍는걸 좋아해서 사진전을 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100034916.1.18",
          "form": "언젠가 한번 열어보고싶네요 ㅋㅋ",
          "original_form": "언젠가 한번 열어보고싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:20"
        }
      ]
