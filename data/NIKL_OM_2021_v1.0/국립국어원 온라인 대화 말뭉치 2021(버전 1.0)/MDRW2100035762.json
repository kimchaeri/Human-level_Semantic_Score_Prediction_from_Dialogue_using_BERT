      "utterance": [
        {
          "id": "MDRW2100035762.1.1",
          "form": "커피는 좋아하세요?",
          "original_form": "커피는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.2",
          "form": "name1님은 커피좋아하세요? 전 카페인에 민감해서",
          "original_form": "&name1&님은 커피좋아하세요? 전 카페인에 민감해서",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.3",
          "form": "아주 가~~~~끔 마시는데 잘안먹어요",
          "original_form": "아주 가~~~~끔 마시는데 잘안먹어요",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.4",
          "form": "저도 카페인때문에 못마시구",
          "original_form": "저도 카페인때문에 못마시구",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.5",
          "form": "심장뛰고 잠못자고",
          "original_form": "심장뛰고 잠못자고",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.6",
          "form": "디카페인이나 스무디 위주로 마셔요 ㅡㅋㅋㅋ",
          "original_form": "디카페인이나 스무디 위주로 마셔요 ㅡㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.7",
          "form": "엇 저두 그래요ㅋㅋㅋ",
          "original_form": "엇 저두 그래요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.8",
          "form": "커피향만 즐겨요",
          "original_form": "커피향만 즐겨요",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035762.1.9",
          "form": "ㅎㅎㅎ오 신기하네용",
          "original_form": "ㅎㅎㅎ오 신기하네용",
          "speaker_id": "2",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.10",
          "form": "모카향은 진짜 좋죠ㅎㅎ",
          "original_form": "모카향은 진짜 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.11",
          "form": "민트초코 좋아하세요?",
          "original_form": "민트초코 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.12",
          "form": "네 ㅎㅎㅎ 넘좋아요 향기~",
          "original_form": "네 ㅎㅎㅎ 넘좋아요 향기~",
          "speaker_id": "2",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.13",
          "form": "완전요ㅠㅠ",
          "original_form": "완전요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.14",
          "form": "민초단입니다!!ㅋ",
          "original_form": "민초단입니다!!ㅋ",
          "speaker_id": "2",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.15",
          "form": "저도 민초단이에요!ㅋㅋ",
          "original_form": "저도 민초단이에요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.16",
          "form": "입안이 상쾌하고 달달하고 너무 맛있어요",
          "original_form": "입안이 상쾌하고 달달하고 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100035762.1.17",
          "form": "저 카페가면 매번 민트초코 마시거든요ㅎㅎㅎ",
          "original_form": "저 카페가면 매번 민트초코 마시거든요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100035762.1.18",
          "form": "앗 그러시구낭ㅎㅎ",
          "original_form": "앗 그러시구낭ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100035762.1.19",
          "form": "근데 스벅 민초는 비추에요ㅠㅠ",
          "original_form": "근데 스벅 민초는 비추에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100035762.1.20",
          "form": "밍밍해요",
          "original_form": "밍밍해요",
          "speaker_id": "1",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100035762.1.21",
          "form": "오 다행히도 스벅은 집에서 멀리있어서 잘안가네요ㅋㅋㅋ",
          "original_form": "오 다행히도 스벅은 집에서 멀리있어서 잘안가네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100035762.1.22",
          "form": "다행이네요ㅋㅋㅋ",
          "original_form": "다행이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100035762.1.23",
          "form": "민초는 이디야꺼 추천드려용!",
          "original_form": "민초는 이디야꺼 추천드려용!",
          "speaker_id": "1",
          "time": "20210830 01:00"
        }
