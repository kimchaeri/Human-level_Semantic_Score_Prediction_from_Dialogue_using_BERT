      "utterance": [
        {
          "id": "MDRW2100036620.1.1",
          "form": "네 귀찮을땐 안올리는데",
          "original_form": "네 귀찮을땐 안올리는데",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.2",
          "form": "거의 올려먹는것같아요",
          "original_form": "거의 올려먹는것같아요",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.3",
          "form": "저도 노른자 안익은 계란후라이 꼭 올려먹거든요ㅋㅋㅋㅋ",
          "original_form": "저도 노른자 안익은 계란후라이 꼭 올려먹거든요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.4",
          "form": "진짜 각잡고 할땐 양파랑돼지고기볶아서 ㅎㅎ",
          "original_form": "진짜 각잡고 할땐 양파랑돼지고기볶아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.5",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.6",
          "form": "우와 한끼 가볍게 때우는정도가 아니라",
          "original_form": "우와 한끼 가볍게 때우는정도가 아니라",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.7",
          "form": "요리네요!",
          "original_form": "요리네요!",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.8",
          "form": "노른자 톡 터뚜려서",
          "original_form": "노른자 톡 터뚜려서",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100036620.1.9",
          "form": "계란 드실때",
          "original_form": "계란 드실때",
          "speaker_id": "1",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100036620.1.10",
          "form": "네 근데 잘안해요 진짜 진짜 먹고싶을때만 ㅎㅎ",
          "original_form": "네 근데 잘안해요 진짜 진짜 먹고싶을때만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100036620.1.11",
          "form": "반숙 완숙 중에 뭘 더 좋아하세요?",
          "original_form": "반숙 완숙 중에 뭘 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100036620.1.12",
          "form": "계란후라이 단독으로먹을땐 완숙이여",
          "original_form": "계란후라이 단독으로먹을땐 완숙이여",
          "speaker_id": "2",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100036620.1.13",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100036620.1.14",
          "form": "근데 비벼먹거나 할땐 반숙으로먹어요",
          "original_form": "근데 비벼먹거나 할땐 반숙으로먹어요",
          "speaker_id": "2",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100036620.1.15",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100036620.1.16",
          "form": "그냥 삶은 계란은요?",
          "original_form": "그냥 삶은 계란은요?",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100036620.1.17",
          "form": "전 둘 다 반숙이요!",
          "original_form": "전 둘 다 반숙이요!",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100036620.1.18",
          "form": "삶은계란은 완숙으로먹어요",
          "original_form": "삶은계란은 완숙으로먹어요",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100036620.1.19",
          "form": "목멕히는 맛이좋아서 ㅎㅎ",
          "original_form": "목멕히는 맛이좋아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100036620.1.20",
          "form": "오호 계란 취향이 확고하시네요ㅋㅋㅋ",
          "original_form": "오호 계란 취향이 확고하시네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:29"
        }
      ]
