      "utterance": [
        {
          "id": "MDRW2100002238.1.1",
          "form": "전 둘째도 못울리고 안고있었네요",
          "original_form": "전 둘째도 못울리고 안고있었네요",
          "speaker_id": "1",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002238.1.2",
          "form": "저희애들 특히 첫째 운적이 별로없어요",
          "original_form": "저희애들 특히 첫째 운적이 별로없어요",
          "speaker_id": "1",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002238.1.3",
          "form": "저도요ㅎㅎ첫째는 거의 안울고",
          "original_form": "저도요ㅎㅎ첫째는 거의 안울고",
          "speaker_id": "2",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002238.1.4",
          "form": "아플때만 울엇어요",
          "original_form": "아플때만 울엇어요",
          "speaker_id": "2",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002238.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002238.1.6",
          "form": "하도 입원해서 그때 절 잡아먹으려고 했어요 ㅡㅡ",
          "original_form": "하도 입원해서 그때 절 잡아먹으려고 했어요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002238.1.7",
          "form": "저는 낳고서 한달만에 입원햇어서 애기가",
          "original_form": "저는 낳고서 한달만에 입원햇어서 애기가",
          "speaker_id": "2",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002238.1.8",
          "form": "멘붕이엿어요",
          "original_form": "멘붕이엿어요",
          "speaker_id": "2",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002238.1.9",
          "form": "전 인큐베이터에 있었어요",
          "original_form": "전 인큐베이터에 있었어요",
          "speaker_id": "1",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002238.1.10",
          "form": "무슨 바이러스에 옮앗다고해서",
          "original_form": "무슨 바이러스에 옮앗다고해서",
          "speaker_id": "2",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002238.1.11",
          "form": "힘드셨겠어요 아 애들이 그때 그러더라구요",
          "original_form": "힘드셨겠어요 아 애들이 그때 그러더라구요",
          "speaker_id": "1",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002238.1.12",
          "form": "저희애는 미성숙하게 태어나서",
          "original_form": "저희애는 미성숙하게 태어나서",
          "speaker_id": "1",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002238.1.13",
          "form": "코로나까지 겹쳐서",
          "original_form": "코로나까지 겹쳐서",
          "speaker_id": "2",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002238.1.14",
          "form": "좀 키우고 나왔네요",
          "original_form": "좀 키우고 나왔네요",
          "speaker_id": "1",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002238.1.15",
          "form": "보호자 한명만되고 검사받고 난리엿어요",
          "original_form": "보호자 한명만되고 검사받고 난리엿어요",
          "speaker_id": "2",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002238.1.16",
          "form": "병원가고 아이 낳기 힘들때죠",
          "original_form": "병원가고 아이 낳기 힘들때죠",
          "speaker_id": "1",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002238.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002238.1.18",
          "form": "요새 산모들 넘 불쌍해요",
          "original_form": "요새 산모들 넘 불쌍해요",
          "speaker_id": "1",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002238.1.19",
          "form": "진짜 고생 많으세요",
          "original_form": "진짜 고생 많으세요",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002238.1.20",
          "form": "마스크쓰고 낳는데요ㅋ요즘",
          "original_form": "마스크쓰고 낳는데요ㅋ요즘",
          "speaker_id": "2",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002238.1.21",
          "form": "코로나가 모든걸 바꿔놓고 스톱시켰어요",
          "original_form": "코로나가 모든걸 바꿔놓고 스톱시켰어요",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002238.1.22",
          "form": "세상에 숨막하게",
          "original_form": "세상에 숨막하게",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002238.1.23",
          "form": "그러게요 삶의질이 ..",
          "original_form": "그러게요 삶의질이 ..",
          "speaker_id": "2",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002238.1.24",
          "form": "떨어졌죠",
          "original_form": "떨어졌죠",
          "speaker_id": "1",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002238.1.25",
          "form": "어쩐데요 ㅜㅜ큰일이네요",
          "original_form": "어쩐데요 ㅜㅜ큰일이네요",
          "speaker_id": "1",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002238.1.26",
          "form": "지나가겟죠..",
          "original_form": "지나가겟죠..",
          "speaker_id": "2",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002238.1.27",
          "form": "낳는것도 힘든데",
          "original_form": "낳는것도 힘든데",
          "speaker_id": "1",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002238.1.28",
          "form": "네 잘 낳을거예요",
          "original_form": "네 잘 낳을거예요",
          "speaker_id": "1",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002238.1.29",
          "form": "순산 하실거예요",
          "original_form": "순산 하실거예요",
          "speaker_id": "1",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002238.1.30",
          "form": "전 수술해서",
          "original_form": "전 수술해서",
          "speaker_id": "1",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002238.1.31",
          "form": "네 빨리 키워야죠",
          "original_form": "네 빨리 키워야죠",
          "speaker_id": "2",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002238.1.32",
          "form": "네 금방 키우고 금방 크고요",
          "original_form": "네 금방 키우고 금방 크고요",
          "speaker_id": "1",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002238.1.33",
          "form": "감사해요 힘이됩니당",
          "original_form": "감사해요 힘이됩니당",
          "speaker_id": "2",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002238.1.34",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002238.1.35",
          "form": "전 진통제 알레르기라 진통제를 수술후 못맞았는데",
          "original_form": "전 진통제 알레르기라 진통제를 수술후 못맞았는데",
          "speaker_id": "1",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002238.1.36",
          "form": "어찌어찌 지나가더라구요",
          "original_form": "어찌어찌 지나가더라구요",
          "speaker_id": "1",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002238.1.37",
          "form": "세상일이 또 그렇게 잘 지나가요",
          "original_form": "세상일이 또 그렇게 잘 지나가요",
          "speaker_id": "1",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002238.1.38",
          "form": "조리원은 묯주 하셨어요?",
          "original_form": "조리원은 묯주 하셨어요?",
          "speaker_id": "1",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002238.1.39",
          "form": "저 2주요",
          "original_form": "저 2주요",
          "speaker_id": "2",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002238.1.40",
          "form": "어케든 버티려구요",
          "original_form": "어케든 버티려구요",
          "speaker_id": "2",
          "time": "20210820 01:10"
        }
      ]
    }
