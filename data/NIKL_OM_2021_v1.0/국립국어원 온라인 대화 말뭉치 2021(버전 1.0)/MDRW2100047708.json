      "utterance": [
        {
          "id": "MDRW2100047708.1.1",
          "form": "안녕하세요\n최근데 쇼핑 하셨나요?",
          "original_form": "안녕하세요\n최근데 쇼핑 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100047708.1.2",
          "form": "앗 넹넹",
          "original_form": "앗 넹넹",
          "speaker_id": "2",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100047708.1.3",
          "form": "어떤 물품 구매하셨나용?",
          "original_form": "어떤 물품 구매하셨나용?",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100047708.1.4",
          "form": "쿠팡에서 식재료 샀어요 ㅋㅋㅋㅋㅋ",
          "original_form": "쿠팡에서 식재료 샀어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100047708.1.5",
          "form": "뭐 사셨나요 ㅋㅋㅋㅋㅋ",
          "original_form": "뭐 사셨나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100047708.1.6",
          "form": "김이랑 무말랭이욬ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "김이랑 무말랭이욬ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100047708.1.7",
          "form": "감자도 사려고 보고있어요",
          "original_form": "감자도 사려고 보고있어요",
          "speaker_id": "2",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100047708.1.8",
          "form": "감자 채 썰어서 감자전 해먹으면 맛있는데 저도 감자 살까봐요 ㅎㅎ",
          "original_form": "감자 채 썰어서 감자전 해먹으면 맛있는데 저도 감자 살까봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100047708.1.9",
          "form": "홍감자라고 겉에 껍질빨갈고 속은 노란 감자가 있던데",
          "original_form": "홍감자라고 겉에 껍질빨갈고 속은 노란 감자가 있던데",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100047708.1.10",
          "form": "먹어본적이 없어서 도전해볼까말까 고민돼서 고민만하고 있어요 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "먹어본적이 없어서 도전해볼까말까 고민돼서 고민만하고 있어요 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100047708.1.11",
          "form": "홍감자요?? 이름도 처음 들어봤어요 ㅎㅎ",
          "original_form": "홍감자요?? 이름도 처음 들어봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100047708.1.12",
          "form": "오 찾아보니 생긴거 뭉툭한 고구마 같네요 ㅎㅎ",
          "original_form": "오 찾아보니 생긴거 뭉툭한 고구마 같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100047708.1.13",
          "form": "저도 이번에 감자사려다가 처음 알았어요 ㅋㅋㅋㅋ",
          "original_form": "저도 이번에 감자사려다가 처음 알았어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100047708.1.14",
          "form": "찐거보니까 속이 노래서 맛있어보이긴 하더라구요",
          "original_form": "찐거보니까 속이 노래서 맛있어보이긴 하더라구요",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100047708.1.15",
          "form": "그러게요! 왠지 맛도 더 달달할 것 같아요 ㅎㅎ",
          "original_form": "그러게요! 왠지 맛도 더 달달할 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100047708.1.16",
          "form": "궁금하니까 저는 주문해보겠습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "궁금하니까 저는 주문해보겠습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100047708.1.17",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100047708.1.18",
          "form": "맛있었으면 좋겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "맛있었으면 좋겠네요 ㅋㅋㅋㅋㅋ",
