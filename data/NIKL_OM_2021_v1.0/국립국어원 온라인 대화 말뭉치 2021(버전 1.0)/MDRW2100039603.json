      "utterance": [
        {
          "id": "MDRW2100039603.1.1",
          "form": "그런가바요 유투브에는 고수들이 많네요",
          "original_form": "그런가바요 유투브에는 고수들이 많네요",
          "speaker_id": "2",
          "time": "20210903 19:31"
        },
        {
          "id": "MDRW2100039603.1.2",
          "form": "변기 뚫는 용액이면",
          "original_form": "변기 뚫는 용액이면",
          "speaker_id": "2",
          "time": "20210903 19:31"
        },
        {
          "id": "MDRW2100039603.1.3",
          "form": "다될거같은데요",
          "original_form": "다될거같은데요",
          "speaker_id": "2",
          "time": "20210903 19:31"
        },
        {
          "id": "MDRW2100039603.1.4",
          "form": "변기 뚫는 용액으로 화장실 청소를 하는거죠?",
          "original_form": "변기 뚫는 용액으로 화장실 청소를 하는거죠?",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.5",
          "form": "주방도 닦고요?",
          "original_form": "주방도 닦고요?",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.6",
          "form": "닦는건 모르겠고 후드에 기름때 담그면 바로 새거되더라구요 ㄷㄷ",
          "original_form": "닦는건 모르겠고 후드에 기름때 담그면 바로 새거되더라구요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.7",
          "form": "그게 독해요",
          "original_form": "그게 독해요",
          "speaker_id": "2",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.8",
          "form": "그럴것 같아요",
          "original_form": "그럴것 같아요",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.9",
          "form": "몸에는 안좋겠넹ㅅ",
          "original_form": "몸에는 안좋겠넹ㅅ",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.10",
          "form": "머리카락 녹이는거라",
          "original_form": "머리카락 녹이는거라",
          "speaker_id": "2",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.11",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039603.1.12",
          "form": "머리카락도 녹아요?",
          "original_form": "머리카락도 녹아요?",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.13",
          "form": "살에 닿으면 살도 녹는거 아니예요?",
          "original_form": "살에 닿으면 살도 녹는거 아니예요?",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.14",
          "form": "네 하수구 뚫는거자나요",
          "original_form": "네 하수구 뚫는거자나요",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.15",
          "form": "아.... 위험한거네요",
          "original_form": "아.... 위험한거네요",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.16",
          "form": "세면대 막히거나할때 부으래요",
          "original_form": "세면대 막히거나할때 부으래요",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.17",
          "form": "고무장갑 필수겠어요",
          "original_form": "고무장갑 필수겠어요",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.18",
          "form": "마자요 그래서",
          "original_form": "마자요 그래서",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.19",
          "form": "일주이에 한번정도 하시나요?",
          "original_form": "일주이에 한번정도 하시나요?",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.20",
          "form": "화장실 줄눈도 깨끗해지나바요",
          "original_form": "화장실 줄눈도 깨끗해지나바요",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.21",
          "form": "시꺼매지면 쫌 뿌려 문대놔요",
          "original_form": "시꺼매지면 쫌 뿌려 문대놔요",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039603.1.22",
          "form": "오!! 눈줄도 깨끗해지면 한번 해보고 싶네요",
          "original_form": "오!! 눈줄도 깨끗해지면 한번 해보고 싶네요",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039603.1.23",
          "form": "그죠 하얘지더라구요",
          "original_form": "그죠 하얘지더라구요",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039603.1.24",
          "form": "환기는 필수",
          "original_form": "환기는 필수",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039603.1.25",
          "form": "당장 도전해보겠어요 여기서 꿀팁을 얻다니 ㅋㅋ",
          "original_form": "당장 도전해보겠어요 여기서 꿀팁을 얻다니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039603.1.26",
          "form": "마스크도 쓰고 후다닥 하고 환기 해야겠네요",
          "original_form": "마스크도 쓰고 후다닥 하고 환기 해야겠네요",
          "speaker_id": "1",
          "time": "20210903 19:34"
        }
      ]
    }
  ]
