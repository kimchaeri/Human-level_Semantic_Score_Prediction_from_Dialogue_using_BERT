      "utterance": [
        {
          "id": "MDRW2100012822.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210825 02:02"
        },
        {
          "id": "MDRW2100012822.1.2",
          "form": "아뇨 저는 무교입니다. name2님은요?",
          "original_form": "아뇨 저는 무교입니다. &name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012822.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012822.1.4",
          "form": "무교입니다!",
          "original_form": "무교입니다!",
          "speaker_id": "2",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012822.1.5",
          "form": "가족들도 다 무교인가요?",
          "original_form": "가족들도 다 무교인가요?",
          "speaker_id": "1",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012822.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012822.1.7",
          "form": "님도요?",
          "original_form": "님도요?",
          "speaker_id": "2",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012822.1.8",
          "form": "저희 가족은 천주교에요. 저만 무교",
          "original_form": "저희 가족은 천주교에요. 저만 무교",
          "speaker_id": "1",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012822.1.9",
          "form": "와우 그렇군요",
          "original_form": "와우 그렇군요",
          "speaker_id": "2",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012822.1.10",
          "form": "왜 무교세요?",
          "original_form": "왜 무교세요?",
          "speaker_id": "2",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012822.1.11",
          "form": "별로 믿음이 안 생기더라구요 ㅋㅋ",
          "original_form": "별로 믿음이 안 생기더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012822.1.12",
          "form": "공감도 안 가고",
          "original_form": "공감도 안 가고",
          "speaker_id": "1",
          "time": "20210825 02:17"
        },
        {
          "id": "MDRW2100012822.1.13",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "2",
          "time": "20210825 02:18"
        },
        {
          "id": "MDRW2100012822.1.14",
          "form": "저도 신앙심이 안생겨서",
          "original_form": "저도 신앙심이 안생겨서",
          "speaker_id": "2",
          "time": "20210825 02:18"
        },
        {
          "id": "MDRW2100012822.1.15",
          "form": "무교랍니다",
          "original_form": "무교랍니다",
          "speaker_id": "2",
          "time": "20210825 02:18"
        },
        {
          "id": "MDRW2100012822.1.16",
          "form": "모태신앙이었으면 어땠을지 모르겠는데 안 받아들여지더라구요",
          "original_form": "모태신앙이었으면 어땠을지 모르겠는데 안 받아들여지더라구요",
          "speaker_id": "1",
          "time": "20210825 02:20"
        },
        {
          "id": "MDRW2100012822.1.17",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 02:23"
        },
        {
          "id": "MDRW2100012822.1.18",
          "form": "종교는 참 그런것같아요",
          "original_form": "종교는 참 그런것같아요",
          "speaker_id": "2",
          "time": "20210825 02:23"
        },
        {
          "id": "MDRW2100012822.1.19",
          "form": "종교 권유하면 정말 부담스럽죠",
          "original_form": "종교 권유하면 정말 부담스럽죠",
          "speaker_id": "1",
          "time": "20210825 02:29"
        }
      ]
