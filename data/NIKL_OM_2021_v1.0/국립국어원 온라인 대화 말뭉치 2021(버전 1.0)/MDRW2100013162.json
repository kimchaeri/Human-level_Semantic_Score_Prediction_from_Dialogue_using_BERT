        },
        {
          "id": "MDRW2100013162.1.2",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013162.1.3",
          "form": "저는 김밥에 라면 ㅋㅋ",
          "original_form": "저는 김밥에 라면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013162.1.4",
          "form": "근데 3시에 친구들 마난서",
          "original_form": "근데 3시에 친구들 마난서",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013162.1.5",
          "form": "또 먹을 것 같아요",
          "original_form": "또 먹을 것 같아요",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013162.1.6",
          "form": "뭐 드시게요? ㅎ",
          "original_form": "뭐 드시게요? ㅎ",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013162.1.7",
          "form": "등촌칼국수요",
          "original_form": "등촌칼국수요",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013162.1.8",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013162.1.9",
          "form": "네 근데 솔직히 양이 넘 적어요",
          "original_form": "네 근데 솔직히 양이 넘 적어요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013162.1.10",
          "form": "그런 면이 있죠 ㅎㅎ",
          "original_form": "그런 면이 있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013162.1.11",
          "form": "근데 지금 배불러서",
          "original_form": "근데 지금 배불러서",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013162.1.12",
          "form": "딱 맞을 것 같아요 ㅋㅋㅋ",
