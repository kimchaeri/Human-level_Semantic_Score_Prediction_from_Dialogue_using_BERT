      "utterance": [
        {
          "id": "MDRW2100047500.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100047500.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100047500.1.3",
          "form": "앗ㅎㅎ",
          "original_form": "앗ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100047500.1.4",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100047500.1.5",
          "form": "네 보쌈 먹었어요",
          "original_form": "네 보쌈 먹었어요",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100047500.1.6",
          "form": "저는 아직이요!",
          "original_form": "저는 아직이요!",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100047500.1.7",
          "form": "보쌈! 든든하게 드셨네요",
          "original_form": "보쌈! 든든하게 드셨네요",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100047500.1.8",
          "form": "저는 새우찜 시켜서 기다리고있어요",
          "original_form": "저는 새우찜 시켜서 기다리고있어요",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100047500.1.9",
          "form": "네 집에 왔더니 보쌈이 있더라고요",
          "original_form": "네 집에 왔더니 보쌈이 있더라고요",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100047500.1.10",
          "form": "오호 새우찜도 맛있겠네요",
          "original_form": "오호 새우찜도 맛있겠네요",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100047500.1.11",
          "form": "룸메가 오늘 백신을 맞아서",
          "original_form": "룸메가 오늘 백신을 맞아서",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100047500.1.12",
          "form": "든든하게 먹으려구요!",
          "original_form": "든든하게 먹으려구요!",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100047500.1.13",
          "form": "아 그랬구나",
          "original_form": "아 그랬구나",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100047500.1.14",
          "form": "백신 맞은 날은 식사 든든히 해야죠!!",
          "original_form": "백신 맞은 날은 식사 든든히 해야죠!!",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100047500.1.15",
          "form": "간식도 먹고",
          "original_form": "간식도 먹고",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100047500.1.16",
          "form": "그쵸 방금 밥도 먹고 간식도 사왔어요",
          "original_form": "그쵸 방금 밥도 먹고 간식도 사왔어요",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047500.1.17",
          "form": "디저트류 좋아하세요?",
          "original_form": "디저트류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100047500.1.18",
          "form": "오 잘하셨어요",
          "original_form": "오 잘하셨어요",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047500.1.19",
          "form": "네 디저트류 좋아해요",
          "original_form": "네 디저트류 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047500.1.20",
          "form": "하나 꼽자면 무엇인가요?",
          "original_form": "하나 꼽자면 무엇인가요?",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047500.1.21",
          "form": "한동안 마카롱에 빠져있다가",
          "original_form": "한동안 마카롱에 빠져있다가",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047500.1.22",
          "form": "요즘은 마들렌이요ㅎㅎ",
          "original_form": "요즘은 마들렌이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047500.1.23",
          "form": "마들렌! 기본 마들렌 좋아하세요?",
          "original_form": "마들렌! 기본 마들렌 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100047500.1.24",
          "form": "네 기본도 좋아하고",
          "original_form": "네 기본도 좋아하고",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100047500.1.25",
          "form": "얼그레이 마들렌도 좋아해요",
          "original_form": "얼그레이 마들렌도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100047500.1.26",
          "form": "단것 당길땐 초코도",
          "original_form": "단것 당길땐 초코도",
          "speaker_id": "2",
          "time": "20210826 20:35"
        }
      ]
    }
