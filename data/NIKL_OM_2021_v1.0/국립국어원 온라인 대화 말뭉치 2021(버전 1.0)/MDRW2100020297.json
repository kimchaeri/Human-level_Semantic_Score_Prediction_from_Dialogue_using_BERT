      "utterance": [
        {
          "id": "MDRW2100020297.1.1",
          "form": "강경후라이드파입니다 ㅋㅋㅋㅋㅋ",
          "original_form": "강경후라이드파입니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020297.1.2",
          "form": "그렇다고 양념을 아예 입도 안대는건 아니에요 ㅋㅋㅋ",
          "original_form": "그렇다고 양념을 아예 입도 안대는건 아니에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020297.1.3",
          "form": "ㅋㅋㅋㅋㅋ 전 후라이드 시켜봐서 맛있으면 2~3번째 까진 후라이드 먹고",
          "original_form": "ㅋㅋㅋㅋㅋ 전 후라이드 시켜봐서 맛있으면 2~3번째 까진 후라이드 먹고",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020297.1.4",
          "form": "다음에 이것저것 시켜봐요",
          "original_form": "다음에 이것저것 시켜봐요",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020297.1.5",
          "form": "헛 먹는예기 하니 치킨이 ㅜㅡ",
          "original_form": "헛 먹는예기 하니 치킨이 ㅜㅡ",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020297.1.6",
          "form": "저녁은 뭐 드셨어요?",
          "original_form": "저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020297.1.7",
          "form": "ㅋㅋㅋㅋㅋ저는 호기심에 다른 양념 한두번 먹어보고 다시 후라이드로 돌아가요",
          "original_form": "ㅋㅋㅋㅋㅋ저는 호기심에 다른 양념 한두번 먹어보고 다시 후라이드로 돌아가요",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020297.1.8",
          "form": "후라이드가 진리긴 하죠",
          "original_form": "후라이드가 진리긴 하죠",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020297.1.9",
          "form": "자꾸 나쁜 말이라고 안보내지넹 ㅋㅋㅋ",
          "original_form": "자꾸 나쁜 말이라고 안보내지넹 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020297.1.10",
          "form": "암튼 치킨 먹고 싶네요",
          "original_form": "암튼 치킨 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020297.1.11",
          "form": "ㅋㅋㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋㅋㅋ저도요",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020297.1.12",
          "form": "치킨말고는 뭐 시켜드세요?",
          "original_form": "치킨말고는 뭐 시켜드세요?",
          "speaker_id": "2",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020297.1.13",
          "form": "저는 치킨 말고는 거의 잘 안시켜요 ㅎ",
          "original_form": "저는 치킨 말고는 거의 잘 안시켜요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020297.1.14",
          "form": "와 진짜 치킨 좋아하시는구나",
          "original_form": "와 진짜 치킨 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020297.1.15",
          "form": "넵 ㅋㅋㅋㅋㅋㅋ ㅜㅡ 그래서 살이",
          "original_form": "넵 ㅋㅋㅋㅋㅋㅋ ㅜㅡ 그래서 살이",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020297.1.16",
          "form": "아 고기도 좋아해요",
          "original_form": "아 고기도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020297.1.17",
          "form": "ㅋㅋㅋㅋㅋ 전 다양하게 시켜먹어서... 살이... ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 전 다양하게 시켜먹어서... 살이... ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020297.1.18",
          "form": "치킨과 고긴 진리죠 ㅎ",
          "original_form": "치킨과 고긴 진리죠 ㅎ",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020297.1.19",
          "form": "아 저도 고기좋아해요",
          "original_form": "아 저도 고기좋아해요",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020297.1.20",
          "form": "무슨고기 좋아하세요?",
          "original_form": "무슨고기 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020297.1.21",
          "form": "흠 나가서 구워먹는건 대부분다 좋아하긴 하는데",
          "original_form": "흠 나가서 구워먹는건 대부분다 좋아하긴 하는데",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020297.1.22",
          "form": "삽겹살 항정살 그리고 곱창이요",
          "original_form": "삽겹살 항정살 그리고 곱창이요",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020297.1.23",
          "form": "name2은 치킨 말곤 뭐 시켜드세요?",
          "original_form": "&name2&은 치킨 말곤 뭐 시켜드세요?",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020297.1.24",
          "form": "저는 피자도 좋아하고 중국집도 좋아해요",
          "original_form": "저는 피자도 좋아하고 중국집도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020297.1.25",
          "form": "항정살... 침고이네요",
          "original_form": "항정살... 침고이네요",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020297.1.26",
          "form": "헛 피잔 도미노데이에만",
          "original_form": "헛 피잔 도미노데이에만",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020297.1.27",
          "form": "딱 1번만요",
          "original_form": "딱 1번만요",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020297.1.28",
          "form": "중국음식도 ㅜㅡ",
          "original_form": "중국음식도 ㅜㅡ",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020297.1.29",
          "form": "도미노데이 놓칠수없죠 ㅋㅋㅋㅋ",
          "original_form": "도미노데이 놓칠수없죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020297.1.30",
          "form": "그러고 보니 저도 많이 시키네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러고 보니 저도 많이 시키네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020297.1.31",
          "form": "전 피자알볼로도 좋아해요",
          "original_form": "전 피자알볼로도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020297.1.32",
          "form": "헐 ㅜㅡ",
          "original_form": "헐 ㅜㅡ",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020297.1.33",
          "form": "자주 시켜먹다보니 알볼로 넘비싸서",
          "original_form": "자주 시켜먹다보니 알볼로 넘비싸서",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020297.1.34",
          "form": "달에 1번정도만 시켜요 ㅜㅡ",
          "original_form": "달에 1번정도만 시켜요 ㅜㅡ",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020297.1.35",
          "form": "맞아요 자주 못시켜먹음 ㅠㅠ",
          "original_form": "맞아요 자주 못시켜먹음 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020297.1.36",
