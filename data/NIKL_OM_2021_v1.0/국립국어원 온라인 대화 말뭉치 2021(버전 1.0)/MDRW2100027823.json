      "utterance": [
        {
          "id": "MDRW2100027823.1.1",
          "form": "다른 게임스트리머들은 욕이 흔한 편인가요?",
          "original_form": "다른 게임스트리머들은 욕이 흔한 편인가요?",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027823.1.2",
          "form": "그게 진짜 재밌었어요  무인도이야기 게임",
          "original_form": "그게 진짜 재밌었어요  무인도이야기 게임",
          "speaker_id": "1",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027823.1.3",
          "form": "시청자수 많아도 욕 쫌 쓰더라구요",
          "original_form": "시청자수 많아도 욕 쫌 쓰더라구요",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027823.1.4",
          "form": "진정한 생존게임  무인도이야기 ㅎㅎ",
          "original_form": "진정한 생존게임  무인도이야기 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027823.1.5",
          "form": "무인도이야기 검색해보니",
          "original_form": "무인도이야기 검색해보니",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027823.1.6",
          "form": "미소녀들과 love love 탈출 대작전이라는데 이게 맞나요ㅋㅋㅋ",
          "original_form": "미소녀들과 love love 탈출 대작전이라는데 이게 맞나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027823.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027823.1.8",
          "form": "약간 그런게 있었던 느낌",
          "original_form": "약간 그런게 있었던 느낌",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027823.1.9",
          "form": "4가 재밌었어요",
          "original_form": "4가 재밌었어요",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027823.1.10",
          "form": "그걸 어렸을때 했다니 ㅋㅋㅋㅋ",
          "original_form": "그걸 어렸을때 했다니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027823.1.11",
          "form": "그림이 프린세스메이커 생각나고 추억돋네요!",
          "original_form": "그림이 프린세스메이커 생각나고 추억돋네요!",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027823.1.12",
          "form": "나무랑 음식 찾으러다니고",
          "original_form": "나무랑 음식 찾으러다니고",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027823.1.13",
          "form": "비행기 부품줏으러다니고",
          "original_form": "비행기 부품줏으러다니고",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027823.1.14",
          "form": "혹시 굶지마도 해보셨나요!",
          "original_form": "혹시 굶지마도 해보셨나요!",
          "speaker_id": "2",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027823.1.15",
          "form": "헐 그거 좋아해요",
          "original_form": "헐 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027823.1.16",
          "form": "우와앙",
          "original_form": "우와앙",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027823.1.17",
          "form": "주로 어떤 캐릭터로 하세요?",
          "original_form": "주로 어떤 캐릭터로 하세요?",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027823.1.18",
          "form": "저는 웬디 좋아해요",
          "original_form": "저는 웬디 좋아해요",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027823.1.19",
          "form": "저 캐릭터는 잘몰라요",
          "original_form": "저 캐릭터는 잘몰라요",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027823.1.20",
          "form": "방송만 봐서 ㅋㅋ",
          "original_form": "방송만 봐서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027823.1.21",
          "form": "웬디는 유령 언니? 여동생? 달고다니는 캐릭터예요",
          "original_form": "웬디는 유령 언니? 여동생? 달고다니는 캐릭터예요",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027823.1.22",
          "form": "그것도 생존게임맞죠",
          "original_form": "그것도 생존게임맞죠",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027823.1.23",
          "form": "제가 컨트롤이 좀 안좋아서ㅋㅋ 유령 언니가 대신 때려주고 맞아주니까 하기 수월하더라고요",
          "original_form": "제가 컨트롤이 좀 안좋아서ㅋㅋ 유령 언니가 대신 때려주고 맞아주니까 하기 수월하더라고요",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027823.1.24",
          "form": "넵넵 이것저것 주워서 살아남고",
          "original_form": "넵넵 이것저것 주워서 살아남고",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027823.1.25",
          "form": "궁극적으로 탈출이요!",
          "original_form": "궁극적으로 탈출이요!",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027823.1.26",
          "form": "근데 한번도 못깨봤어요ㅋㅋ",
          "original_form": "근데 한번도 못깨봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027823.1.27",
          "form": "스위치에서",
          "original_form": "스위치에서",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027823.1.28",
          "form": "살까말까 했었거든요",
          "original_form": "살까말까 했었거든요",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027823.1.29",
          "form": "할인할때",
          "original_form": "할인할때",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027823.1.30",
          "form": "스위치에서 하기 괜찮을지 모르겠네요",
          "original_form": "스위치에서 하기 괜찮을지 모르겠네요",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027823.1.31",
          "form": "폰버전으로 나왔길래 사봤는데 컨이 영 어렵더라고요",
          "original_form": "폰버전으로 나왔길래 사봤는데 컨이 영 어렵더라고요",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027823.1.32",
          "form": "같이 하는건 같이 하는 재미가 있고 혼자하는것도 나름의 재미가 있어요",
          "original_form": "같이 하는건 같이 하는 재미가 있고 혼자하는것도 나름의 재미가 있어요",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027823.1.33",
          "form": "진짜 잘 만든게임이라고 생각해요",
          "original_form": "진짜 잘 만든게임이라고 생각해요",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027823.1.34",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027823.1.35",
          "form": "스타듀벨리",
          "original_form": "스타듀벨리",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027823.1.36",
          "form": "그것도 스위치로 하니 너무 빡새요",
          "original_form": "그것도 스위치로 하니 너무 빡새요",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027823.1.37",
          "form": "스타듀벨리 스위치로 하니 너무 힘들어오",
          "original_form": "스타듀벨리 스위치로 하니 너무 힘들어오",
