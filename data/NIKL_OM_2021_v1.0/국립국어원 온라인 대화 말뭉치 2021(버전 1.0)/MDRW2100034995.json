      "utterance": [
        {
          "id": "MDRW2100034995.1.1",
          "form": "최근 잘산템있나요",
          "original_form": "최근 잘산템있나요",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100034995.1.2",
          "form": "최근 원피스를 하나 샀었는데 다들 잘 어울린다고 칭찬해줘서",
          "original_form": "최근 원피스를 하나 샀었는데 다들 잘 어울린다고 칭찬해줘서",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100034995.1.3",
          "form": "몹시 만족했었죵",
          "original_form": "몹시 만족했었죵",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100034995.1.4",
          "form": "와 어떤원피스인가용",
          "original_form": "와 어떤원피스인가용",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100034995.1.5",
          "form": "빅카라 머메이드 원피스에용",
          "original_form": "빅카라 머메이드 원피스에용",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100034995.1.6",
          "form": "원피스라 편한데 갖춰입은 느낌나여",
          "original_form": "원피스라 편한데 갖춰입은 느낌나여",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100034995.1.7",
          "form": "와 이름 어렵네요",
          "original_form": "와 이름 어렵네요",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100034995.1.8",
          "form": "원피스 좋아용",
          "original_form": "원피스 좋아용",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100034995.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100034995.1.10",
          "form": "이름은 저게 아닌데 암튼 소개해봤어용",
          "original_form": "이름은 저게 아닌데 암튼 소개해봤어용",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100034995.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100034995.1.12",
          "form": "곧 가을옷을 사볼까해요",
          "original_form": "곧 가을옷을 사볼까해요",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100034995.1.13",
          "form": "단발 엄청 어울리시던데 이제",
          "original_form": "단발 엄청 어울리시던데 이제",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100034995.1.14",
          "form": "시크한",
          "original_form": "시크한",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100034995.1.15",
          "form": "옷을 사시나요",
          "original_form": "옷을 사시나요",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100034995.1.16",
          "form": "네네 시크한 가죽 잠바 살거에요",
          "original_form": "네네 시크한 가죽 잠바 살거에요",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100034995.1.17",
          "form": "아앗 너무 멋있다",
          "original_form": "아앗 너무 멋있다",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100034995.1.18",
          "form": "이제 가죽바지입ㄱㅎ 춤출거에요",
          "original_form": "이제 가죽바지입ㄱㅎ 춤출거에요",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100034995.1.19",
          "form": "또 뭐 사고싶으세여",
          "original_form": "또 뭐 사고싶으세여",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100034995.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100034995.1.21",
          "form": "와 완전 아이돌이네요",
          "original_form": "와 완전 아이돌이네요",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100034995.1.22",
          "form": "음 장바구니 다 털었더니 이제 언박싱할 일만 남아서",
          "original_form": "음 장바구니 다 털었더니 이제 언박싱할 일만 남아서",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100034995.1.23",
          "form": "자제하려고요 ㅎㅎㅎㅎㅎㅎ",
          "original_form": "자제하려고요 ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100034995.1.24",
          "form": "이러다 파산해요ㅎㅎㅎㅎㅎ",
          "original_form": "이러다 파산해요ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100034995.1.25",
          "form": "와 언박싱 또 뭐샀나요",
          "original_form": "와 언박싱 또 뭐샀나요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100034995.1.26",
          "form": "그냥 전부 가을 옷이엥ㅆ",
          "original_form": "그냥 전부 가을 옷이엥ㅆ",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100034995.1.27",
          "form": "쇼핑이 좀 귀차나서",
          "original_form": "쇼핑이 좀 귀차나서",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100034995.1.28",
          "form": "계절바뀔때쯤 한꺼번에 몰아서 사고",
          "original_form": "계절바뀔때쯤 한꺼번에 몰아서 사고",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100034995.1.29",
          "form": "그걸로 내내 계절 보내요",
          "original_form": "그걸로 내내 계절 보내요",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100034995.1.30",
          "form": "아 한꺼번에 하시는구나",
          "original_form": "아 한꺼번에 하시는구나",
          "speaker_id": "1",
          "time": "20210902 20:46"
        }
      ]
