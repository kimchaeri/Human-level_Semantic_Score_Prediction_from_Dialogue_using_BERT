      "utterance": [
        {
          "id": "MDRW2100022808.1.1",
          "form": "근데 키차이 때문에",
          "original_form": "근데 키차이 때문에",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022808.1.2",
          "form": "엄마랑 둘이 밥먹을땐 보는데",
          "original_form": "엄마랑 둘이 밥먹을땐 보는데",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022808.1.3",
          "form": "아빠랑 같이 먹으면 안봐",
          "original_form": "아빠랑 같이 먹으면 안봐",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022808.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022808.1.5",
          "form": "암튼 키차이가 막 설레는 키차이 이러면서",
          "original_form": "암튼 키차이가 막 설레는 키차이 이러면서",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022808.1.6",
          "form": "사람들이 좋아함",
          "original_form": "사람들이 좋아함",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022808.1.7",
          "form": "아빠랑 밥먹으면 원래",
          "original_form": "아빠랑 밥먹으면 원래",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022808.1.8",
          "form": "오옭..",
          "original_form": "오옭..",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022808.1.9",
          "form": "백종원의 골목식당 봤는데",
          "original_form": "백종원의 골목식당 봤는데",
          "speaker_id": "2",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022808.1.10",
          "form": "이제 유퀴즈로 갈아탔음ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이제 유퀴즈로 갈아탔음ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022808.1.11",
          "form": "넌 엄마랑 같이 보는거 없어?",
          "original_form": "넌 엄마랑 같이 보는거 없어?",
          "speaker_id": "2",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022808.1.12",
          "form": "응 우리는 다들 밥먹는시간이 달라서ㅌㅋㅋㅋㅋ",
          "original_form": "응 우리는 다들 밥먹는시간이 달라서ㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022808.1.13",
          "form": "각자 알아서 밥 챙겨먹다보니까",
          "original_form": "각자 알아서 밥 챙겨먹다보니까",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022808.1.14",
          "form": "딱히 같이보는게 없는듯",
          "original_form": "딱히 같이보는게 없는듯",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022808.1.15",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022808.1.16",
          "form": "다 다르다고?",
          "original_form": "다 다르다고?",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022808.1.17",
          "form": "ㅇㅇ..아빠는 시도때도없이 먹고 엄마는 새벽일찍 일어나고 난 늦게일어나고 ㅋㅋㅋ",
          "original_form": "ㅇㅇ..아빠는 시도때도없이 먹고 엄마는 새벽일찍 일어나고 난 늦게일어나고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100022808.1.18",
          "form": "엄마가 볼거없다고 심심해할때마다",
          "original_form": "엄마가 볼거없다고 심심해할때마다",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022808.1.19",
          "form": "자빱컨텐츠 영업하는데",
          "original_form": "자빱컨텐츠 영업하는데",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022808.1.20",
          "form": "번번히 실패중임ㅋㅋㅋㅋ",
          "original_form": "번번히 실패중임ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022808.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022808.1.22",
          "form": "어머니 동감합니다...",
          "original_form": "어머니 동감합니다...",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022808.1.23",
          "form": "저도 자빱에 입덕하기가 쉽지 않더라구요",
          "original_form": "저도 자빱에 입덕하기가 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022808.1.24",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100022808.1.25",
          "form": "다음에 다시 도전...",
          "original_form": "다음에 다시 도전...",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022808.1.26",
          "form": "그래 언젠간 너도 꼭..재밌게볼수 있을거야",
          "original_form": "그래 언젠간 너도 꼭..재밌게볼수 있을거야",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022808.1.27",
          "form": "그랰ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그랰ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022808.1.28",
          "form": "근데 나 진짜 입덕하면",
          "original_form": "근데 나 진짜 입덕하면",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022808.1.29",
          "form": "너보다 더 난리치고 다닐듯",
          "original_form": "너보다 더 난리치고 다닐듯",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022808.1.30",
          "form": "일단 굿즈란 굿즈 다 사고",
          "original_form": "일단 굿즈란 굿즈 다 사고",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022808.1.31",
          "form": "그럴것 같다..",
          "original_form": "그럴것 같다..",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022808.1.32",
          "form": "괴담레스토랑",
          "original_form": "괴담레스토랑",
