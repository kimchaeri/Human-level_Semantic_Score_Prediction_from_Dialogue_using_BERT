      "utterance": [
        {
          "id": "MDRW2100002506.1.1",
          "form": "좋아하는 과자있어요?",
          "original_form": "좋아하는 과자있어요?",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002506.1.2",
          "form": "딱히 좋아하는건 없는데",
          "original_form": "딱히 좋아하는건 없는데",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.3",
          "form": "누가 사다놓으면 잘먹어요ㅋㅋ",
          "original_form": "누가 사다놓으면 잘먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.4",
          "form": "저는 약간 비스캣같은건 안좋아하고",
          "original_form": "저는 약간 비스캣같은건 안좋아하고",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.5",
          "form": "봉지과자 좋아하는편이거든요",
          "original_form": "봉지과자 좋아하는편이거든요",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.6",
          "form": "꼬깔콘 새우깡 포스틱 이런거",
          "original_form": "꼬깔콘 새우깡 포스틱 이런거",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.7",
          "form": "앗 저두 뭔가 그런것같은데",
          "original_form": "앗 저두 뭔가 그런것같은데",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.8",
          "form": "비스켓은 너무 무거운느낌?",
          "original_form": "비스켓은 너무 무거운느낌?",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.9",
          "form": "약간 비스캣은 맞아요",
          "original_form": "약간 비스캣은 맞아요",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002506.1.10",
          "form": "과자라기엔 쿠키같은 느낌이라",
          "original_form": "과자라기엔 쿠키같은 느낌이라",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.11",
          "form": "맛은 있는데 부담스럽고",
          "original_form": "맛은 있는데 부담스럽고",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.12",
          "form": "식사대용같아서",
          "original_form": "식사대용같아서",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.13",
          "form": "감자칩같은 얇은게",
          "original_form": "감자칩같은 얇은게",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.14",
          "form": "봉지과자가 짭짤해서 좋은 것도 있고",
          "original_form": "봉지과자가 짭짤해서 좋은 것도 있고",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.16",
          "form": "딱 간식느낌이라 더 좋은듯해여",
          "original_form": "딱 간식느낌이라 더 좋은듯해여",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.17",
          "form": "감자과자 너무좋아",
          "original_form": "감자과자 너무좋아",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.18",
          "form": "가볍게 먹는게 넘 메리트",
          "original_form": "가볍게 먹는게 넘 메리트",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.19",
          "form": "자고로 과자는",
          "original_form": "자고로 과자는",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.20",
          "form": "과자는?",
          "original_form": "과자는?",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.21",
          "form": "얇아야 맛있는듯해여ㅋㅋㅋ",
          "original_form": "얇아야 맛있는듯해여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.22",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002506.1.23",
          "form": "식감이 좋아서ㅎㅎㅎ",
          "original_form": "식감이 좋아서ㅎㅎㅎ",
          "speaker_id": "2",
