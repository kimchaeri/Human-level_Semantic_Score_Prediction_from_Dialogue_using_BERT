      "utterance": [
        {
          "id": "MDRW2100003818.1.1",
          "form": "치킨을? 가게하셔?",
          "original_form": "치킨을? 가게하셔?",
          "speaker_id": "2",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003818.1.2",
          "form": "아니아니 ㅋㅋ 집 근처에 사셔서",
          "original_form": "아니아니 ㅋㅋ 집 근처에 사셔서",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003818.1.3",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003818.1.4",
          "form": "포장하시는 김에 우리도 가져다 주셨어",
          "original_form": "포장하시는 김에 우리도 가져다 주셨어",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003818.1.5",
          "form": "좋은 분이시구나",
          "original_form": "좋은 분이시구나",
          "speaker_id": "2",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003818.1.6",
          "form": "ㅋㅋㅋ 응",
          "original_form": "ㅋㅋㅋ 응",
          "speaker_id": "1",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003818.1.7",
          "form": "그 때 60계 간지치킨 처음 먹어봤는데",
          "original_form": "그 때 60계 간지치킨 처음 먹어봤는데",
          "speaker_id": "1",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003818.1.8",
          "form": "오 60계",
          "original_form": "오 60계",
          "speaker_id": "2",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003818.1.9",
          "form": "닭 날개로만 있더라고",
          "original_form": "닭 날개로만 있더라고",
          "speaker_id": "1",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003818.1.10",
          "form": "윙 맛있지",
          "original_form": "윙 맛있지",
          "speaker_id": "2",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003818.1.11",
          "form": "진짜 맛있더라",
          "original_form": "진짜 맛있더라",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003818.1.12",
          "form": "먹고 싶다",
          "original_form": "먹고 싶다",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003818.1.13",
          "form": "약간 교촌 오리지날 느낌도 났어",
          "original_form": "약간 교촌 오리지날 느낌도 났어",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003818.1.14",
          "form": "맞아 비슷한듯",
          "original_form": "맞아 비슷한듯",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003818.1.15",
          "form": "생각해보니 그렇네",
          "original_form": "생각해보니 그렇네",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003818.1.16",
          "form": "60계 젤 유명한 그 치킨만 먹어봤는데",
          "original_form": "60계 젤 유명한 그 치킨만 먹어봤는데",
