        },
        {
          "id": "MDRW2100000326.1.2",
          "form": "일은 할만한데",
          "original_form": "일은 할만한데",
          "speaker_id": "1",
          "time": "20210818 15:32"
        },
        {
          "id": "MDRW2100000326.1.3",
          "form": "급여로는 답이 없을것 같아서",
          "original_form": "급여로는 답이 없을것 같아서",
          "speaker_id": "1",
          "time": "20210818 15:32"
        },
        {
          "id": "MDRW2100000326.1.4",
          "form": "대기업에 비하면 그래도 중소에 비하면",
          "original_form": "대기업에 비하면 그래도 중소에 비하면",
          "speaker_id": "2",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000326.1.5",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "1",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000326.1.6",
          "form": "난 박봉~",
          "original_form": "난 박봉~",
          "speaker_id": "2",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000326.1.7",
          "form": "오늘 일이 없네",
          "original_form": "오늘 일이 없네",
          "speaker_id": "2",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000326.1.8",
          "form": "그래서 재테크를 해야해",
          "original_form": "그래서 재테크를 해야해",
          "speaker_id": "1",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000326.1.9",
          "form": "돈 삭제?",
          "original_form": "돈 삭제?",
          "speaker_id": "2",
          "time": "20210818 15:34"
        },
        {
