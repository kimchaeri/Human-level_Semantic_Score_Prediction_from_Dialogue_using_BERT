      "utterance": [
        {
          "id": "MDRW2100026585.1.1",
          "form": "무슨 음료 젤 좋아하시나여",
          "original_form": "무슨 음료 젤 좋아하시나여",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026585.1.2",
          "form": "와 완전 황금레시피네요!!",
          "original_form": "와 완전 황금레시피네요!!",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026585.1.3",
          "form": "저는 초코음료는 거의 다 먹어요",
          "original_form": "저는 초코음료는 거의 다 먹어요",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026585.1.4",
          "form": "오 카페가면 모카 마시나요?",
          "original_form": "오 카페가면 모카 마시나요?",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026585.1.5",
          "form": "네 어느정도냐면",
          "original_form": "네 어느정도냐면",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026585.1.6",
          "form": "예전에  쥬시에서 맨날 초코바나나만 마셨어요",
          "original_form": "예전에  쥬시에서 맨날 초코바나나만 마셨어요",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026585.1.7",
          "form": "초코바나나...! 한번도 안먹어봤어요...",
          "original_form": "초코바나나...! 한번도 안먹어봤어요...",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026585.1.8",
          "form": "초코들어간건 다 좋아하시는 거에요?",
          "original_form": "초코들어간건 다 좋아하시는 거에요?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026585.1.9",
          "form": "네 일단 초콜릿이면!",
          "original_form": "네 일단 초콜릿이면!",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026585.1.10",
          "form": "민초 드시나요?",
          "original_form": "민초 드시나요?",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026585.1.11",
          "form": "네! 민트는 신성한 허브",
          "original_form": "네! 민트는 신성한 허브",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026585.1.12",
          "form": "민초가 세상을 구한다",
          "original_form": "민초가 세상을 구한다",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026585.1.13",
          "form": "오 그럼 민초라떼도 드시나요?",
          "original_form": "오 그럼 민초라떼도 드시나요?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026585.1.14",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026585.1.15",
          "form": "민초 과자는 안먹어요 근데",
          "original_form": "민초 과자는 안먹어요 근데",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026585.1.16",
          "form": "시원한 맛이 느껴져야 맛있어요",
          "original_form": "시원한 맛이 느껴져야 맛있어요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026585.1.17",
          "form": "민초 어떠세요",
          "original_form": "민초 어떠세요",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026585.1.18",
          "form": "민초 과자는 안먹는데",
          "original_form": "민초 과자는 안먹는데",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026585.1.19",
          "form": "음료는 먹어요 시원해서 끝맛이 좋더라고요",
          "original_form": "음료는 먹어요 시원해서 끝맛이 좋더라고요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026585.1.20",
          "form": "그쵸 전 베라 시킬때 꼭 민초 시켜요 입가심(?)용으로 ㅋㅋㅋ",
          "original_form": "그쵸 전 베라 시킬때 꼭 민초 시켜요 입가심(?)용으로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026585.1.21",
          "form": "그건 어때요 탕수육 부먹찍먹",
          "original_form": "그건 어때요 탕수육 부먹찍먹",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026585.1.22",
          "form": "찍먹이죠",
          "original_form": "찍먹이죠",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026585.1.23",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026585.1.24",
          "form": "엇..전 부먹인디요",
          "original_form": "엇..전 부먹인디요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026585.1.25",
          "form": "부먹인데! 하지만 찍먹 친구들을 위해",
          "original_form": "부먹인데! 하지만 찍먹 친구들을 위해",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026585.1.26",
          "form": "제 접시에 부어먹습니다",
          "original_form": "제 접시에 부어먹습니다",
          "speaker_id": "1",
