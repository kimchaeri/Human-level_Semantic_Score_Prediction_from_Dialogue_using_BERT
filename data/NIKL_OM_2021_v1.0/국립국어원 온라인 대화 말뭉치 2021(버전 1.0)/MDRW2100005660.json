      "utterance": [
        {
          "id": "MDRW2100005660.1.1",
          "form": "코로나 끝나면 페스티벌 다 가야됨",
          "original_form": "코로나 끝나면 페스티벌 다 가야됨",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005660.1.2",
          "form": "대학축제도 다 즐길 자신 있는데ㅠ",
          "original_form": "대학축제도 다 즐길 자신 있는데ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005660.1.3",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005660.1.4",
          "form": "이게 뭐야",
          "original_form": "이게 뭐야",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005660.1.5",
          "form": "오히려 어릴 때 더 많이 다닌 듯",
          "original_form": "오히려 어릴 때 더 많이 다닌 듯",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005660.1.6",
          "form": "광운대 인덕대 ㅋㅋㅋㅋ",
          "original_form": "광운대 인덕대 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005660.1.7",
          "form": "난 한 번도 안 감",
          "original_form": "난 한 번도 안 감",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005660.1.8",
          "form": "태생부터 집순이",
          "original_form": "태생부터 집순이",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005660.1.9",
          "form": "그냥 근처에서 불꽃 터지면",
          "original_form": "그냥 근처에서 불꽃 터지면",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005660.1.10",
          "form": "오늘 축젠가...?",
          "original_form": "오늘 축젠가...?",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005660.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앜",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005660.1.12",
          "form": "나 8살 때부터 매년 광운대 축제 감",
          "original_form": "나 8살 때부터 매년 광운대 축제 감",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005660.1.13",
          "form": "나 진짜 즐길 자신 있음",
          "original_form": "나 진짜 즐길 자신 있음",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005660.1.14",
          "form": "솔직히 축제는 자기가 즐기는 만큼 재밌는 거라고 생각해",
          "original_form": "솔직히 축제는 자기가 즐기는 만큼 재밌는 거라고 생각해",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005660.1.15",
          "form": "맞지",
          "original_form": "맞지",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005660.1.16",
          "form": "주점 사라짐 게 아쉬움",
          "original_form": "주점 사라짐 게 아쉬움",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005660.1.17",
          "form": "맞아ㅜ",
          "original_form": "맞아ㅜ",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005660.1.18",
          "form": "그게 축제 감성인데",
          "original_form": "그게 축제 감성인데",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005660.1.19",
          "form": "어른들만 즐기고",
          "original_form": "어른들만 즐기고",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005660.1.20",
          "form": "우린 못 즐기는 거 너ㅜㅁ 억울",
          "original_form": "우린 못 즐기는 거 너ㅜㅁ 억울",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005660.1.21",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005660.1.22",
          "form": "콘서트도 가고 싶다",
          "original_form": "콘서트도 가고 싶다",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005660.1.23",
          "form": "그 에너지 다 쏟아서 즐기는 게 너무 하고 싶어",
          "original_form": "그 에너지 다 쏟아서 즐기는 게 너무 하고 싶어",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005660.1.24",
          "form": "너무 집에만 있어서 놀고 싶어...",
          "original_form": "너무 집에만 있어서 놀고 싶어...",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005660.1.25",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005660.1.26",
          "form": "원래 집에 가고 싶어하는데",
          "original_form": "원래 집에 가고 싶어하는데",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005660.1.27",
          "form": "지금은 반대로 집에만 있으미까",
          "original_form": "지금은 반대로 집에만 있으미까",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005660.1.28",
          "form": "밖에서 즐기는 게 하고 싶응ㅁ",
          "original_form": "밖에서 즐기는 게 하고 싶응ㅁ",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005660.1.29",
          "form": "진짜로~~",
          "original_form": "진짜로~~",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005660.1.30",
          "form": "가을이 딱 콘서트나 축제하기 좋은데ㅎㅎ",
          "original_form": "가을이 딱 콘서트나 축제하기 좋은데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005660.1.31",
          "form": "ㅜ어림도 없지",
          "original_form": "ㅜ어림도 없지",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005660.1.32",
          "form": "내년에는 되려나",
          "original_form": "내년에는 되려나",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005660.1.33",
          "form": "제발...",
          "original_form": "제발...",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005660.1.34",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:51"
        }
      ]
    }
