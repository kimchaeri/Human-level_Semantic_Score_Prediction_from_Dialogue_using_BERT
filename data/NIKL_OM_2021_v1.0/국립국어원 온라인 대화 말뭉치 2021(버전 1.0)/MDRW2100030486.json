      "utterance": [
        {
          "id": "MDRW2100030486.1.1",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030486.1.2",
          "form": "집사는 무엇이든 할 수 있어",
          "original_form": "집사는 무엇이든 할 수 있어",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030486.1.3",
          "form": "너무 사고 싶은 게 많다",
          "original_form": "너무 사고 싶은 게 많다",
          "speaker_id": "1",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030486.1.4",
          "form": "거기서 뭐뭐 봐",
          "original_form": "거기서 뭐뭐 봐",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030486.1.5",
          "form": "이것저것...",
          "original_form": "이것저것...",
          "speaker_id": "1",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030486.1.6",
          "form": "요즘 키워드 알림 해놓은 건",
          "original_form": "요즘 키워드 알림 해놓은 건",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030486.1.7",
          "form": "역방쿠...",
          "original_form": "역방쿠...",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030486.1.8",
          "form": "그거 말고는 그냥 구경하는듯",
          "original_form": "그거 말고는 그냥 구경하는듯",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030486.1.9",
          "form": "해먹이나 스크래쳐 이런 거",
          "original_form": "해먹이나 스크래쳐 이런 거",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030486.1.10",
          "form": "고양이... 쿠션은 아직 없구나",
          "original_form": "고양이... 쿠션은 아직 없구나",
          "speaker_id": "2",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030486.1.11",
          "form": "해먹 귀엽겠다",
          "original_form": "해먹 귀엽겠다",
          "speaker_id": "2",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030486.1.12",
          "form": "창문에 붙이는 해먹... 사고 싶긴 한데",
          "original_form": "창문에 붙이는 해먹... 사고 싶긴 한데",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030486.1.13",
          "form": "여전히 고민 중",
          "original_form": "여전히 고민 중",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030486.1.14",
          "form": "습식도 기호성 테스트 실패한 분들이 가끔 파시는데",
          "original_form": "습식도 기호성 테스트 실패한 분들이 가끔 파시는데",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030486.1.15",
          "form": "요즘은 별로 없네",
          "original_form": "요즘은 별로 없네",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030486.1.16",
          "form": "글쿠만",
          "original_form": "글쿠만",
          "speaker_id": "2",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030486.1.17",
          "form": "너희 집 고영은 습식 기호성테스트",
          "original_form": "너희 집 고영은 습식 기호성테스트",
          "speaker_id": "2",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030486.1.18",
          "form": "대충 됐니",
          "original_form": "대충 됐니",
          "speaker_id": "2",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030486.1.19",
          "form": "생각보다 이것저것 잘 먹긴 하는데",
          "original_form": "생각보다 이것저것 잘 먹긴 하는데",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030486.1.20",
          "form": "카루 제일 잘 먹어서 걍 그것만 맛별로 다 사먹이는 중",
          "original_form": "카루 제일 잘 먹어서 걍 그것만 맛별로 다 사먹이는 중",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030486.1.21",
          "form": "쿠폰 이것저것 먹이면 싸",
          "original_form": "쿠폰 이것저것 먹이면 싸",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030486.1.22",
          "form": "고양이 때문에 페이코도 쓰기 시작한",
          "original_form": "고양이 때문에 페이코도 쓰기 시작한",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030486.1.23",
          "form": "집사는 무엇이든2",
          "original_form": "집사는 무엇이든2",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030486.1.24",
          "form": "좋은 조건의 역방쿠를 구했으면 좋겠다",
          "original_form": "좋은 조건의 역방쿠를 구했으면 좋겠다",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030486.1.25",
          "form": "그러게...",
          "original_form": "그러게...",
          "speaker_id": "1",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030486.1.26",
          "form": "제발 가까운 데",
          "original_form": "제발 가까운 데",
          "speaker_id": "1",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030486.1.27",
          "form": "그거 바리바리 들고 버스까지 타고 싶지 않아",
          "original_form": "그거 바리바리 들고 버스까지 타고 싶지 않아",
          "speaker_id": "1",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030486.1.28",
          "form": "으 최악",
          "original_form": "으 최악",
          "speaker_id": "2",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030486.1.29",
          "form": "같은 단지면 좋을 텐데",
          "original_form": "같은 단지면 좋을 텐데",
          "speaker_id": "2",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030486.1.30",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030486.1.31",
          "form": "얼마 전에 겜 타이틀 당근에서 샀는데",
          "original_form": "얼마 전에 겜 타이틀 당근에서 샀는데",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030486.1.32",
          "form": "전에 근처에서 파는 거 있었는데",
          "original_form": "전에 근처에서 파는 거 있었는데",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030486.1.33",
          "form": "같은 단지여서 짱 좋았음",
          "original_form": "같은 단지여서 짱 좋았음",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030486.1.34",
          "form": "망설이는 사이 사라져서 슬픈",
          "original_form": "망설이는 사이 사라져서 슬픈",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030486.1.35",
          "form": "같은 단지면 좋지...",
          "original_form": "같은 단지면 좋지...",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030486.1.36",
          "form": "난 거의 버스 타고 갔던 듯",
          "original_form": "난 거의 버스 타고 갔던 듯",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030486.1.37",
          "form": "헐... 난 버스 탈 정성까진 없음...",
          "original_form": "헐... 난 버스 탈 정성까진 없음...",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030486.1.38",
          "form": "무조건 가까운 거.",
          "original_form": "무조건 가까운 거.",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030486.1.39",
          "form": "그래도 배송비보단 싸기도 하고...",
          "original_form": "그래도 배송비보단 싸기도 하고...",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030486.1.40",
          "form": "금방 받아볼 수 있는 게 좋은",
          "original_form": "금방 받아볼 수 있는 게 좋은",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030486.1.41",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030486.1.42",
          "form": "서브 모니터가 생겼어",
          "original_form": "서브 모니터가 생겼어",
          "speaker_id": "1",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100030486.1.43",
          "form": "너무 좋아",
          "original_form": "너무 좋아",
          "speaker_id": "1",
          "time": "20210906 20:04"
        }
      ]
    }
  ]
