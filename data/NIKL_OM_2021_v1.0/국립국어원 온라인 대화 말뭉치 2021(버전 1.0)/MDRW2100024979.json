      "utterance": [
        {
          "id": "MDRW2100024979.1.1",
          "form": "최근에? 어제인가",
          "original_form": "최근에? 어제인가",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024979.1.2",
          "form": "카카오에서 새로나온 게임 받아서 해봤어요",
          "original_form": "카카오에서 새로나온 게임 받아서 해봤어요",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024979.1.3",
          "form": "어떤 게임인가요?",
          "original_form": "어떤 게임인가요?",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024979.1.4",
          "form": "나이스샷",
          "original_form": "나이스샷",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024979.1.5",
          "form": "아 아니다",
          "original_form": "아 아니다",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024979.1.6",
          "form": "프렌즈샷이요 ㅋㅋㅋ",
          "original_form": "프렌즈샷이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024979.1.7",
          "form": "골프게임 저도 알림 받았던 거 같아요 ㅋㅋ",
          "original_form": "골프게임 저도 알림 받았던 거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024979.1.8",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024979.1.9",
          "form": "ㅋㅋㅋㅋㅋ맞아요 골프게임",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 골프게임",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024979.1.10",
          "form": "음 몇번해봤는데 아직 감을 못잡겠어요",
          "original_form": "음 몇번해봤는데 아직 감을 못잡겠어요",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024979.1.11",
          "form": "저도 한 번 깔아봐야겠어요 ㅎㅎ",
          "original_form": "저도 한 번 깔아봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024979.1.12",
          "form": "ㅋㅋ 괜찮은거같아요",
          "original_form": "ㅋㅋ 괜찮은거같아요",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024979.1.13",
          "form": "골프에 대해서 몰라서 약간 그렇지만",
          "original_form": "골프에 대해서 몰라서 약간 그렇지만",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024979.1.14",
          "form": "게임 하면 뭐 준다고 하면 좀 하게 되더라구요ㅋㅋㅋ 이모티콘이라던가",
          "original_form": "게임 하면 뭐 준다고 하면 좀 하게 되더라구요ㅋㅋㅋ 이모티콘이라던가",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024979.1.15",
          "form": "맞아요ㄲㅋㅋㅋㅋ저도 이모티콘 받으려고 해보는거같아요",
          "original_form": "맞아요ㄲㅋㅋㅋㅋ저도 이모티콘 받으려고 해보는거같아요",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024979.1.16",
          "form": "이번에도 뭐땜에 해본거같은데 기억이안나네요",
          "original_form": "이번에도 뭐땜에 해본거같은데 기억이안나네요",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024979.1.17",
          "form": "한번씩 게임이 땡기긴해요",
          "original_form": "한번씩 게임이 땡기긴해요",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024979.1.18",
          "form": "프렌즈팝도 몇년동안 안했는데 이모티콘 준대서 좀 했어요ㅋㅋ",
          "original_form": "프렌즈팝도 몇년동안 안했는데 이모티콘 준대서 좀 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024979.1.19",
          "form": "약간 희안하게 생긴 이모티콘이긴 한데ㅋㅋ",
          "original_form": "약간 희안하게 생긴 이모티콘이긴 한데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024979.1.20",
          "form": "ㅋㅋㅋㅋ 공짜로 주면 이상한게 있긴해요",
          "original_form": "ㅋㅋㅋㅋ 공짜로 주면 이상한게 있긴해요",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100024979.1.21",
          "form": "최근에 광고로 봤는데 비행기(?) 전투기? 암튼 그거 해보고 싶더라고요",
          "original_form": "최근에 광고로 봤는데 비행기(?) 전투기? 암튼 그거 해보고 싶더라고요",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100024979.1.22",
          "form": "전투기 시뮬레이션 뭐 그런 건가요?",
          "original_form": "전투기 시뮬레이션 뭐 그런 건가요?",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024979.1.23",
          "form": "약간 옛날 오락실에서 하던 그런 게임 같았어요",
          "original_form": "약간 옛날 오락실에서 하던 그런 게임 같았어요",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100024979.1.24",
          "form": "요즘 기억력이 왜이렇게 떨어지는지...ㅜ",
          "original_form": "요즘 기억력이 왜이렇게 떨어지는지...ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100024979.1.25",
          "form": "갤러그?ㅋㅋ",
          "original_form": "갤러그?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100024979.1.26",
          "form": "오!!!! 맞아요",
          "original_form": "오!!!! 맞아요",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100024979.1.27",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100024979.1.28",
          "form": "ㅋㅋ 추억의 게임이네요",
          "original_form": "ㅋㅋ 추억의 게임이네요",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100024979.1.29",
          "form": "ㅋㅋㅋㅋ 그쵸",
          "original_form": "ㅋㅋㅋㅋ 그쵸",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100024979.1.30",
          "form": "그거 다시 해볼까해요",
          "original_form": "그거 다시 해볼까해요",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100024979.1.31",
          "form": "옛날에 버블버블도 재밌었는데",
          "original_form": "옛날에 버블버블도 재밌었는데",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100024979.1.32",
