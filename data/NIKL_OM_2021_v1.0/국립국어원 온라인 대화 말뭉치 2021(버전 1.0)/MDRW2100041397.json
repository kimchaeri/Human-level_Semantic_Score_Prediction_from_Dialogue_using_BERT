      "utterance": [
        {
          "id": "MDRW2100041397.1.1",
          "form": "웅 치즈빵 먹었어",
          "original_form": "웅 치즈빵 먹었어",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041397.1.2",
          "form": "엄마는?",
          "original_form": "엄마는?",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041397.1.3",
          "form": "두부조림해서 먹었어",
          "original_form": "두부조림해서 먹었어",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041397.1.4",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041397.1.5",
          "form": "엄마 두부조림 짱 맛있는데ㅜㅜ",
          "original_form": "엄마 두부조림 짱 맛있는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041397.1.6",
          "form": "다음에 해줄게",
          "original_form": "다음에 해줄게",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041397.1.7",
          "form": "감자탕은 먹었어?",
          "original_form": "감자탕은 먹었어?",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041397.1.8",
          "form": "아니 어제 너무 많이 먹어서 일단 냉동실에 넣어둠",
          "original_form": "아니 어제 너무 많이 먹어서 일단 냉동실에 넣어둠",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041397.1.9",
          "form": "어제 고기 진짜 많이 먹었다...ㅋㅋㅋㅋ",
          "original_form": "어제 고기 진짜 많이 먹었다...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041397.1.10",
          "form": "다음에 먹고 싶을 때 꺼내먹어",
          "original_form": "다음에 먹고 싶을 때 꺼내먹어",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041397.1.11",
          "form": "감자탕 좋아하는지 몰랐어. 알았으면 자주해줄걸",
          "original_form": "감자탕 좋아하는지 몰랐어. 알았으면 자주해줄걸",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041397.1.12",
          "form": "ㅋㅋㅋㅋ 집에서 하기 번거로우니가 해달라고 말 안 했지",
          "original_form": "ㅋㅋㅋㅋ 집에서 하기 번거로우니가 해달라고 말 안 했지",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041397.1.13",
          "form": "그래도 집에서 해먹으니까 완전 푸짐하고 좋더라",
          "original_form": "그래도 집에서 해먹으니까 완전 푸짐하고 좋더라",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041397.1.14",
          "form": "다음에 생각나면 또 해줄게",
          "original_form": "다음에 생각나면 또 해줄게",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041397.1.15",
          "form": "다음주에는 또 뭐 먹을까",
          "original_form": "다음주에는 또 뭐 먹을까",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041397.1.16",
          "form": "다음주 메뉴를 벌써 고민해?ㅋㅋㅋ",
          "original_form": "다음주 메뉴를 벌써 고민해?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041397.1.17",
          "form": "이번주 고기 먹었으니까 다음주는 좀 간단하게 먹자",
          "original_form": "이번주 고기 먹었으니까 다음주는 좀 간단하게 먹자",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041397.1.18",
          "form": "미리 생각해놔야지.",
          "original_form": "미리 생각해놔야지.",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041397.1.19",
          "form": "그럼 잔치국수할까",
          "original_form": "그럼 잔치국수할까",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041397.1.20",
          "form": "오 잔치국수 좋다",
          "original_form": "오 잔치국수 좋다",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041397.1.21",
          "form": "고명은 뭐 뭐 할 거야?",
          "original_form": "고명은 뭐 뭐 할 거야?",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041397.1.22",
          "form": "애호박, 당근, 지단,, 김치 정도",
          "original_form": "애호박, 당근, 지단,, 김치 정도",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041397.1.23",
          "form": "더 넣고 싶은 거 있음 말하고",
          "original_form": "더 넣고 싶은 거 있음 말하고",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041397.1.24",
          "form": "아냐 그정도면 충분해",
          "original_form": "아냐 그정도면 충분해",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041397.1.25",
          "form": "김가루만 좀 올리면 돼",
          "original_form": "김가루만 좀 올리면 돼",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041397.1.26",
          "form": "그래. 국물은 따듯한 게 좋지?",
          "original_form": "그래. 국물은 따듯한 게 좋지?",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041397.1.27",
          "form": "응! 잔치국수 오랜만에 먹겠다~ㅎㅎㅎ",
          "original_form": "응! 잔치국수 오랜만에 먹겠다~ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:07"
        }
      ]
    }
  ]
