      "utterance": [
        {
          "id": "MDRW2100000927.1.1",
          "form": "나는 버즈2가 새로나온건 줄 몰랐어",
          "original_form": "나는 버즈2가 새로나온건 줄 몰랐어",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000927.1.2",
          "form": "버즈2 살 생각 있나?",
          "original_form": "버즈2 살 생각 있나?",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.3",
          "form": "나?",
          "original_form": "나?",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.4",
          "form": "노티드랑 콜라보 한 케이스 예쁘더라",
          "original_form": "노티드랑 콜라보 한 케이스 예쁘더라",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.5",
          "form": "음 3만원이면 살게",
          "original_form": "음 3만원이면 살게",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.6",
          "form": "어. 이어폰 바꿀 때 안됐나?",
          "original_form": "어. 이어폰 바꿀 때 안됐나?",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.7",
          "form": "아니 나는 안팔거고 다른 판매자한테 알아봐야지",
          "original_form": "아니 나는 안팔거고 다른 판매자한테 알아봐야지",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.8",
          "form": "내껀 아직 멀쩡해",
          "original_form": "내껀 아직 멀쩡해",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.9",
          "form": "새로 나온 버즈2 성능 좋더라",
          "original_form": "새로 나온 버즈2 성능 좋더라",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.10",
          "form": "근데 니 귀에 안맞지않나",
          "original_form": "근데 니 귀에 안맞지않나",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.11",
          "form": "노이즈캔슬링도 되고 외부소음듣기..? 그 기능도 있고...",
          "original_form": "노이즈캔슬링도 되고 외부소음듣기..? 그 기능도 있고...",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.12",
          "form": "최근에 버즈라이브샀는데ㅜ",
          "original_form": "최근에 버즈라이브샀는데ㅜ",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000927.1.13",
          "form": "어 내 귀에 안맞아서 너무 아깝다",
          "original_form": "어 내 귀에 안맞아서 너무 아깝다",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000927.1.14",
          "form": "아무래도 꽉 끼는거니까",
          "original_form": "아무래도 꽉 끼는거니까",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000927.1.15",
          "form": "기능은 버즈2가 제일 좋은 거 같은데",
          "original_form": "기능은 버즈2가 제일 좋은 거 같은데",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000927.1.16",
          "form": "귀가 꽉 막혀",
          "original_form": "귀가 꽉 막혀",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000927.1.17",
          "form": "외부소리듣기는 내 버즈도된다!",
          "original_form": "외부소리듣기는 내 버즈도된다!",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000927.1.18",
          "form": "멀미나고 머리아파.",
          "original_form": "멀미나고 머리아파.",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000927.1.19",
          "form": "그거는 외부소리 그냥 들리는 거 아닝교.",
          "original_form": "그거는 외부소리 그냥 들리는 거 아닝교.",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000927.1.20",
          "form": "근데 그 기능 별로 쓸모없어",
          "original_form": "근데 그 기능 별로 쓸모없어",
          "speaker_id": "1",
          "time": "20210819 20:40"
        }
      ]
