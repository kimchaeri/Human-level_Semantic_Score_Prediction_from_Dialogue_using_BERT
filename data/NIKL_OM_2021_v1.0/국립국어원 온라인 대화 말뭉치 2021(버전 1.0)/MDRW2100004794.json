      "utterance": [
        {
          "id": "MDRW2100004794.1.1",
          "form": "오늘도 세 끼 잘먹었다!",
          "original_form": "오늘도 세 끼 잘먹었다!",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004794.1.2",
          "form": "아 깐풍육 맛있네",
          "original_form": "아 깐풍육 맛있네",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004794.1.3",
          "form": "깐풍소스.. 매력적이야",
          "original_form": "깐풍소스.. 매력적이야",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004794.1.4",
          "form": "맞아 매력적",
          "original_form": "맞아 매력적",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004794.1.5",
          "form": "깐풍육은 우리가족 다 먹을 수 있는 메뉴니까",
          "original_form": "깐풍육은 우리가족 다 먹을 수 있는 메뉴니까",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004794.1.6",
          "form": "오늘 아침으로 먹은 돼지국밥도 좋았어",
          "original_form": "오늘 아침으로 먹은 돼지국밥도 좋았어",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004794.1.7",
          "form": "밥은 없었지만",
          "original_form": "밥은 없었지만",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004794.1.8",
          "form": "돼지국인가",
          "original_form": "돼지국인가",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004794.1.9",
          "form": "밥이없으면",
          "original_form": "밥이없으면",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004794.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004794.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004794.1.12",
          "form": "돼지부속국",
          "original_form": "돼지부속국",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004794.1.13",
          "form": "내일은 무슨 맛있는 음식을 먹을까 기대가 됩니다",
          "original_form": "내일은 무슨 맛있는 음식을 먹을까 기대가 됩니다",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004794.1.14",
          "form": "돼지부속국 좋네",
          "original_form": "돼지부속국 좋네",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004794.1.15",
          "form": "아님 돼지내장탕",
          "original_form": "아님 돼지내장탕",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004794.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004794.1.17",
          "form": "ㅋㅋㅋ맛있어 맛있어",
          "original_form": "ㅋㅋㅋ맛있어 맛있어",
          "speaker_id": "1",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004794.1.18",
          "form": "돼지부속이랑 다대기랑 육수랑",
          "original_form": "돼지부속이랑 다대기랑 육수랑",
          "speaker_id": "2",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004794.1.19",
          "form": "정말 너무 만족스럽다",
          "original_form": "정말 너무 만족스럽다",
          "speaker_id": "2",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004794.1.20",
          "form": "응 진짜 만족!!",
          "original_form": "응 진짜 만족!!",
          "speaker_id": "1",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004794.1.21",
          "form": "좋은 음식 구매해주셔서 감사합니다~",
          "original_form": "좋은 음식 구매해주셔서 감사합니다~",
          "speaker_id": "1",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004794.1.22",
          "form": "담주에도 냉장고 털면서 맛있게 잘 먹고 지내자!",
          "original_form": "담주에도 냉장고 털면서 맛있게 잘 먹고 지내자!",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004794.1.23",
          "form": "그러자 냉털해서 이거저거 먹자",
          "original_form": "그러자 냉털해서 이거저거 먹자",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004794.1.24",
          "form": "냉장고도 비우고",
          "original_form": "냉장고도 비우고",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004794.1.25",
          "form": "맛있는 것도 먹고",
          "original_form": "맛있는 것도 먹고",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004794.1.26",
          "form": "어 당근라페도 만들어야겠네",
          "original_form": "어 당근라페도 만들어야겠네",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004794.1.27",
          "form": "오 맞다 당근라페",
          "original_form": "오 맞다 당근라페",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004794.1.28",
          "form": "만들어두면 든든한 양식이 되겠죠!!",
          "original_form": "만들어두면 든든한 양식이 되겠죠!!",
          "speaker_id": "1",
          "time": "20210821 19:22"
        }
      ]
    }
