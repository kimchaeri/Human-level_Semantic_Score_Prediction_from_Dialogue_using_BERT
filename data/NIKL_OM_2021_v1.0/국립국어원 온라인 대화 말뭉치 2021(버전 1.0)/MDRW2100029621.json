      "utterance": [
        {
          "id": "MDRW2100029621.1.1",
          "form": "퇴근하고싶네요",
          "original_form": "퇴근하고싶네요",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029621.1.2",
          "form": "저도요...",
          "original_form": "저도요...",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029621.1.3",
          "form": "퇴근마렵다...",
          "original_form": "퇴근마렵다...",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029621.1.4",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029621.1.5",
          "form": "불금이네야 ㅎㅎ",
          "original_form": "불금이네야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029621.1.6",
          "form": "신나요 ㅋㅋ 본가가야지",
          "original_form": "신나요 ㅋㅋ 본가가야지",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029621.1.7",
          "form": "마감이라 넘 바빴어요",
          "original_form": "마감이라 넘 바빴어요",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029621.1.8",
          "form": "하..마감....",
          "original_form": "하..마감....",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029621.1.9",
          "form": "본가는 가까우세여?ㅎㅎ",
          "original_form": "본가는 가까우세여?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029621.1.10",
          "form": "차로 1시간거리에요ㅋㅋㅋ",
          "original_form": "차로 1시간거리에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029621.1.11",
          "form": "오 마감의 힘듦을 아시는군요",
          "original_form": "오 마감의 힘듦을 아시는군요",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029621.1.12",
          "form": "저도 마감...",
          "original_form": "저도 마감...",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029621.1.13",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029621.1.14",
          "form": "생각만 해도 지옥이에요",
          "original_form": "생각만 해도 지옥이에요",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029621.1.15",
          "form": "본가가시면",
          "original_form": "본가가시면",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029621.1.16",
          "form": "좀 편히 쉬시겠네요",
          "original_form": "좀 편히 쉬시겠네요",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029621.1.17",
          "form": "맛난것도 드시고 ㅎㅎ",
          "original_form": "맛난것도 드시고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029621.1.18",
          "form": "완전 여유로워요 ㅋㅋ 늦잠자고",
          "original_form": "완전 여유로워요 ㅋㅋ 늦잠자고",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100029621.1.19",
          "form": "하지만 또 출근이라니",
          "original_form": "하지만 또 출근이라니",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100029621.1.20",
          "form": "곧",
          "original_form": "곧",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100029621.1.21",
          "form": "아 주말근무도 하시는군요 ㅠㅠ",
          "original_form": "아 주말근무도 하시는군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100029621.1.22",
          "form": "아뇨 헤헤..월요일출근인데",
          "original_form": "아뇨 헤헤..월요일출근인데",
          "speaker_id": "1",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100029621.1.23",
          "form": "알잖아요 직장은은 금요일 저녁만 행복..",
          "original_form": "알잖아요 직장은은 금요일 저녁만 행복..",
          "speaker_id": "1",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100029621.1.24",
          "form": "그건맞아요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "그건맞아요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029621.1.25",
          "form": "눈감았다뜨면",
          "original_form": "눈감았다뜨면",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029621.1.26",
          "form": "출근..준비...ㅜㅠㅠ",
          "original_form": "출근..준비...ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029621.1.27",
          "form": "흑 주 4일제 실시해야해요 진짜",
          "original_form": "흑 주 4일제 실시해야해요 진짜",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100029621.1.28",
          "form": "정말좋네요....상상만해도ㅠ.ㅡ",
          "original_form": "정말좋네요....상상만해도ㅠ.ㅡ",
          "speaker_id": "2",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100029621.1.29",
          "form": "진짜 너무 힘들어용..ㅜㅠ",
          "original_form": "진짜 너무 힘들어용..ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 12:55"
        },
        {
          "id": "MDRW2100029621.1.30",
          "form": "최저도 이만원정도 ^^",
          "original_form": "최저도 이만원정도 ^^",
          "speaker_id": "1",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100029621.1.31",
          "form": "너무좋네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무좋네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100029621.1.32",
          "form": "진짜 인생 즐기면서 살아야하는데",
          "original_form": "진짜 인생 즐기면서 살아야하는데",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100029621.1.33",
          "form": "일하느라 바쁘니까 뭘하지못하겠네여 ㅋㅋ",
          "original_form": "일하느라 바쁘니까 뭘하지못하겠네여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100029621.1.34",
