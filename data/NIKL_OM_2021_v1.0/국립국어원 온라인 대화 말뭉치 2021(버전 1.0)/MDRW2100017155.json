      "utterance": [
        {
          "id": "MDRW2100017155.1.1",
          "form": "ㅎㅎ오늘 저녁메뉴 바꾸시나요?",
          "original_form": "ㅎㅎ오늘 저녁메뉴 바꾸시나요?",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017155.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ잠시 흔들렸지만 한냄비 남아있는 고등어를 생각합니다.... ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ잠시 흔들렸지만 한냄비 남아있는 고등어를 생각합니다.... ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017155.1.3",
          "form": "고기먹구싶네요 저도ㅠ",
          "original_form": "고기먹구싶네요 저도ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017155.1.4",
          "form": "아 일단 고등어찌개부터",
          "original_form": "아 일단 고등어찌개부터",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017155.1.5",
          "form": "약간 잔반처리 뉘앙스이긴한데 ㅎㅎ 맛있을때 언넝먹어야 맛있게 먹으니까요!",
          "original_form": "약간 잔반처리 뉘앙스이긴한데 ㅎㅎ 맛있을때 언넝먹어야 맛있게 먹으니까요!",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017155.1.6",
          "form": "네네 고등어찌개는 했을때 맛있게 많이 먹어야죠",
          "original_form": "네네 고등어찌개는 했을때 맛있게 많이 먹어야죠",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017155.1.7",
          "form": "밥도둑이잖아요ㅎㅎ",
          "original_form": "밥도둑이잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017155.1.8",
          "form": "맞아여 ㅋㅋㅋㅋ 그런의미로 지금 불 올립니다 보글보글 데워야겠어요.",
          "original_form": "맞아여 ㅋㅋㅋㅋ 그런의미로 지금 불 올립니다 보글보글 데워야겠어요.",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017155.1.9",
          "form": "아아 저는 이거 먹으면서 보쌈이 생각날것 같아요 ㅋㅋㅋ",
          "original_form": "아아 저는 이거 먹으면서 보쌈이 생각날것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017155.1.10",
          "form": "보글보글 맛있게 드시고",
          "original_form": "보글보글 맛있게 드시고",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017155.1.11",
          "form": "조만간 보쌈 드셔요ㅎㅎ",
          "original_form": "조만간 보쌈 드셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017155.1.12",
          "form": "그래야겠어요 ㅋㅋㅋㅋ 앞으로 먹을게 많네요 ㅋㅋㅋ 쌀국수도 너무 먹고 싶고 보쌈도 먹어야하고..",
          "original_form": "그래야겠어요 ㅋㅋㅋㅋ 앞으로 먹을게 많네요 ㅋㅋㅋ 쌀국수도 너무 먹고 싶고 보쌈도 먹어야하고..",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017155.1.13",
          "form": "ㅎㅎ가을엔 식욕이 왕성해지죠~",
          "original_form": "ㅎㅎ가을엔 식욕이 왕성해지죠~",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017155.1.14",
          "form": "아아 그래서 그런가봐요 ㅋㅋㅋㅋ",
          "original_form": "아아 그래서 그런가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017155.1.15",
          "form": "계절이 바뀌어서 그렇구나",
          "original_form": "계절이 바뀌어서 그렇구나",
          "speaker_id": "1",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017155.1.16",
          "form": "요즘 먹고싶은게 너무 많아요 ㅎㅎ",
          "original_form": "요즘 먹고싶은게 너무 많아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017155.1.17",
          "form": "여름엔 더워서 입맛이 없더니",
          "original_form": "여름엔 더워서 입맛이 없더니",
          "speaker_id": "2",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017155.1.18",
          "form": "저도 요즘 식욕 폭발이예요",
          "original_form": "저도 요즘 식욕 폭발이예요",
          "speaker_id": "2",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017155.1.19",
          "form": "ㅋㅋㅋㅋ 다 비슷한가봥",
          "original_form": "ㅋㅋㅋㅋ 다 비슷한가봥",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017155.1.20",
          "form": "비슷한가봐요 ㅋㅋ",
          "original_form": "비슷한가봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017155.1.21",
          "form": "그런가봐요ㅎㅎ",
          "original_form": "그런가봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:55"
        }
      ]
    }
  ]
