        },
        {
          "id": "MDRW2100034049.1.2",
          "form": "아니요 ㅎ",
          "original_form": "아니요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100034049.1.3",
          "form": "직장인입니다",
          "original_form": "직장인입니다",
          "speaker_id": "2",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100034049.1.4",
          "form": "학생인가요",
          "original_form": "학생인가요",
          "speaker_id": "2",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100034049.1.5",
          "form": "아뇨 저도 직장인이요 ㅎ",
          "original_form": "아뇨 저도 직장인이요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100034049.1.6",
          "form": "아하 ㅎ ㅎㅎ",
          "original_form": "아하 ㅎ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100034049.1.7",
          "form": "전공과로 취업하셨나요?",
          "original_form": "전공과로 취업하셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100034049.1.8",
          "form": "아니요...",
          "original_form": "아니요...",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100034049.1.9",
          "form": "그러지 못했어요 흑",
          "original_form": "그러지 못했어요 흑",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100034049.1.10",
          "form": "전공으로 가셨어요?",
          "original_form": "전공으로 가셨어요?",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100034049.1.11",
          "form": "네네 저는 치위생이욥 ㅎ치과에서 일해욥",
          "original_form": "네네 저는 치위생이욥 ㅎ치과에서 일해욥",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100034049.1.12",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100034049.1.13",
          "form": "저 전공 고를 때",
          "original_form": "저 전공 고를 때",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100034049.1.14",
          "form": "무슨고ㅏ 나오셨어요?",
          "original_form": "무슨고ㅏ 나오셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100034049.1.15",
          "form": "치위생도 고민이였는데",
          "original_form": "치위생도 고민이였는데",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100034049.1.16",
          "form": "저 패션쪽이요 ~~~~~",
          "original_form": "저 패션쪽이요 ~~~~~",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100034049.1.17",
          "form": "오 ... 지금은 어떤회사 다니세용?!",
          "original_form": "오 ... 지금은 어떤회사 다니세용?!",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100034049.1.18",
          "form": "그냥 사무직이요",
          "original_form": "그냥 사무직이요",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100034049.1.19",
          "form": "등록금이",
          "original_form": "등록금이",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100034049.1.20",
          "form": "아깝네요ㅋㅋ",
