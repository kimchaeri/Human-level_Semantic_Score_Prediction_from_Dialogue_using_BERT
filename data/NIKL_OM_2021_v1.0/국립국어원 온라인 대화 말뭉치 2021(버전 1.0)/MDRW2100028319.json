        {
          "id": "MDRW2100028319.1.2",
          "form": "같이 봤잖니",
          "original_form": "같이 봤잖니",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100028319.1.3",
          "form": "ㅎㅎ 혹시 기억 안나실까봐... 어떤게 제일 재밌으셨어요?",
          "original_form": "ㅎㅎ 혹시 기억 안나실까봐... 어떤게 제일 재밌으셨어요?",
          "speaker_id": "1",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028319.1.4",
          "form": "엄마는 배구가 좋더라",
          "original_form": "엄마는 배구가 좋더라",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028319.1.5",
          "form": "배구 재밌었죠!!",
          "original_form": "배구 재밌었죠!!",
          "speaker_id": "1",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028319.1.6",
          "form": "김연경 선수 멋있었어요 ㅎㅎ",
          "original_form": "김연경 선수 멋있었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028319.1.7",
          "form": "다들 열심히 해서 멋있더라",
          "original_form": "다들 열심히 해서 멋있더라",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028319.1.8",
          "form": "메달은 못땄지만",
          "original_form": "메달은 못땄지만",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028319.1.9",
          "form": "메달 색보다는 열심히 하는게 중요하니까요 ㅋㅋㅋ",
          "original_form": "메달 색보다는 열심히 하는게 중요하니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028319.1.10",
          "form": "그건 그래",
          "original_form": "그건 그래",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028319.1.11",
          "form": "너는 뭐가 좋았니",
          "original_form": "너는 뭐가 좋았니",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028319.1.12",
          "form": "저는 도마요!!",
          "original_form": "저는 도마요!!",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028319.1.13",
          "form": "여서정 선수랑 여홍진 해설?? 그 얘기 많이 하더라구요",
          "original_form": "여서정 선수랑 여홍진 해설?? 그 얘기 많이 하더라구요",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028319.1.14",
          "form": "대세였지",
          "original_form": "대세였지",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028319.1.15",
          "form": "사람 몸이 어떻게 그렇게 움직이니",
          "original_form": "사람 몸이 어떻게 그렇게 움직이니",
