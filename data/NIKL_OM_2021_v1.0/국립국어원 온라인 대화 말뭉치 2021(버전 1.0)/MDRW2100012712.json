      "utterance": [
        {
          "id": "MDRW2100012712.1.1",
          "form": "게임이군요ㅎㅎ",
          "original_form": "게임이군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012712.1.2",
          "form": "예전에 많이했었는데ㅎㅎ 지금은 모바일만해요ㅎ",
          "original_form": "예전에 많이했었는데ㅎㅎ 지금은 모바일만해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012712.1.3",
          "form": "모바일 게임 어떤거 하시는데요?",
          "original_form": "모바일 게임 어떤거 하시는데요?",
          "speaker_id": "1",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012712.1.4",
          "form": "카트라이더했어요ㅎㅎㅎ",
          "original_form": "카트라이더했어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012712.1.5",
          "form": "무슨게임하세요?",
          "original_form": "무슨게임하세요?",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012712.1.6",
          "form": "몇년전까지만 해도.. 게임에 중독되는데 요즘에는.. 재밌는거 없더라고요",
          "original_form": "몇년전까지만 해도.. 게임에 중독되는데 요즘에는.. 재밌는거 없더라고요",
          "speaker_id": "1",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100012712.1.7",
          "form": "어떤 게임이 흥미있을려나..",
          "original_form": "어떤 게임이 흥미있을려나..",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012712.1.8",
          "form": "아ㅎㅎ 중독이요?ㅎㅎ",
          "original_form": "아ㅎㅎ 중독이요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012712.1.9",
          "form": "무슨게임 하셨는데요?",
          "original_form": "무슨게임 하셨는데요?",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012712.1.10",
          "form": "저는 단계별로 미션 클리어하는걸 좋아해요",
          "original_form": "저는 단계별로 미션 클리어하는걸 좋아해요",
          "speaker_id": "1",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012712.1.11",
          "form": "게임 가리지 않고 했던거같아요",
          "original_form": "게임 가리지 않고 했던거같아요",
          "speaker_id": "1",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012712.1.12",
          "form": "인기게임으로 된 게임도 몇분하다가 재미없으면 삭제하네요 ㅋㅋ",
          "original_form": "인기게임으로 된 게임도 몇분하다가 재미없으면 삭제하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:56"
        },
        {
          "id": "MDRW2100012712.1.13",
          "form": "아 근데 pc게임으로 피파는 합니다^^",
          "original_form": "아 근데 pc게임으로 피파는 합니다^^",
          "speaker_id": "1",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012712.1.14",
          "form": "미션이 좋죠ㅎ 보상도 좋고ㅎㅎ",
          "original_form": "미션이 좋죠ㅎ 보상도 좋고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012712.1.15",
          "form": "미션하나만해도 시간금방가잖아요ㅎㅎ",
          "original_form": "미션하나만해도 시간금방가잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012712.1.16",
          "form": "전 피파는 안했었어요ㅎ 위닝쪽이라ㅎㅎ",
          "original_form": "전 피파는 안했었어요ㅎ 위닝쪽이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012712.1.17",
          "form": "온라인 게임위주로 하셨나보네요ㅎㅎ",
          "original_form": "온라인 게임위주로 하셨나보네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012712.1.18",
          "form": "피파는 오늘도 플레이 했습니다",
          "original_form": "피파는 오늘도 플레이 했습니다",
          "speaker_id": "1",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100012712.1.19",
          "form": "미션이 경쟁게임이라서 ㅎㅎ 순위도 있고 재밌어요",
          "original_form": "미션이 경쟁게임이라서 ㅎㅎ 순위도 있고 재밌어요",
          "speaker_id": "1",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100012712.1.20",
          "form": "pc게임으로는 뭐해보셨어요?",
          "original_form": "pc게임으로는 뭐해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012712.1.21",
          "form": "전 나이가 있어서ㅎㅎ 디아블로2 많이했어요ㅎ",
          "original_form": "전 나이가 있어서ㅎㅎ 디아블로2 많이했어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100012712.1.22",
          "form": "잠깐잠깐 그판에 끝나는 게임보다는 지속적인 rpg가 좋더라구요ㅎㅎ",
          "original_form": "잠깐잠깐 그판에 끝나는 게임보다는 지속적인 rpg가 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100012712.1.23",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
