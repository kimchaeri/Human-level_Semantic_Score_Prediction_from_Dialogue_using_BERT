        {
          "id": "MDRW2100020160.1.2",
          "form": "쓰읍",
          "original_form": "쓰읍",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020160.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020160.1.4",
          "form": "나름 뭐 재밌었어요!",
          "original_form": "나름 뭐 재밌었어요!",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020160.1.5",
          "form": "affiliation라는곳에서 근무했는데",
          "original_form": "&affiliation&라는곳에서 근무했는데",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020160.1.6",
          "form": "38선 바로앞가지도가보고",
          "original_form": "38선 바로앞가지도가보고",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020160.1.7",
          "form": "앞까지",
          "original_form": "앞까지",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020160.1.8",
          "form": "dmz도 들어가보고",
          "original_form": "dmz도 들어가보고",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020160.1.9",
          "form": "재밌었어요!",
          "original_form": "재밌었어요!",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020160.1.10",
          "form": "오!! 잊지못할 추억 쌓고오셨네요~ 부러워요",
          "original_form": "오!! 잊지못할 추억 쌓고오셨네요~ 부러워요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020160.1.11",
          "form": "아니다 부럽진않아요 ㅋㅋㅋㅋ죄송",
          "original_form": "아니다 부럽진않아요 ㅋㅋㅋㅋ죄송",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020160.1.12",
          "form": "아잌ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020160.1.13",
          "form": "부러워해도 괜찬아요",
          "original_form": "부러워해도 괜찬아요",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020160.1.14",
          "form": "괜찮아욤",
          "original_form": "괜찮아욤",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020160.1.15",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 근데혹시 북한군인도 마주친적잇으신가요?",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 근데혹시 북한군인도 마주친적잇으신가요?",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020160.1.16",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020160.1.17",
          "form": "우오옹 ! 서로 대화는 못하죠?",
          "original_form": "우오옹 ! 서로 대화는 못하죠?",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020160.1.18",
          "form": "네네 그건못해여",
          "original_form": "네네 그건못해여",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020160.1.19",
          "form": "대화했으면",
          "original_form": "대화했으면",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020160.1.20",
          "form": "저 여기 없을걸요",
          "original_form": "저 여기 없을걸요",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020160.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020160.1.22",
          "form": "앗 그렇구나~ ㅋㅋㅋㅋㅋ 전사실 북한사람이랑 이야기해본적있어요 ㅋㅋㅋ",
          "original_form": "앗 그렇구나~ ㅋㅋㅋㅋㅋ 전사실 북한사람이랑 이야기해본적있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020160.1.23",
          "form": "이런거 여기서 말해도되나모르겠네요 ㅋㅋㅋ",
          "original_form": "이런거 여기서 말해도되나모르겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020160.1.24",
          "form": "???!!!!",
          "original_form": "???!!!!",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020160.1.25",
          "form": "어떻게요??!!",
          "original_form": "어떻게요??!!",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020160.1.26",
          "form": "ㅋㅋㅋ 중국에서요~ 한국인인줄알았는데 알고보니 북한인이시더라구요! ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 중국에서요~ 한국인인줄알았는데 알고보니 북한인이시더라구요! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020160.1.27",
          "form": "여기서 이런말하기 그러면",
          "original_form": "여기서 이런말하기 그러면",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020160.1.28",
          "form": "카톡으로하자고하기엔 속보이고 쿨럭",
          "original_form": "카톡으로하자고하기엔 속보이고 쿨럭",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020160.1.29",
          "form": "으아아 중국!!",
          "original_form": "으아아 중국!!",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020160.1.30",
          "form": "ㅋㅋㅋㅋㅋ 앗 저 카톡 삭제했어요ㅠ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 앗 저 카톡 삭제했어요ㅠ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020160.1.31",
          "form": "아녜요 ㅋㅋㅋ 속보이지않아욬ㅋㅋㅋㅋ",
          "original_form": "아녜요 ㅋㅋㅋ 속보이지않아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020160.1.32",
          "form": "아앙 다행이다",
          "original_form": "아앙 다행이다",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020160.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020160.1.34",
          "form": "중국이나 러시아아니면 북한사람들하고 만날곳이없긴해요",
          "original_form": "중국이나 러시아아니면 북한사람들하고 만날곳이없긴해요",
