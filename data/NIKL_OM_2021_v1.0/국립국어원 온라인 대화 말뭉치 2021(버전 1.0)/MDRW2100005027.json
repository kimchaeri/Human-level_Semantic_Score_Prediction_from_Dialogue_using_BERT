      "utterance": [
        {
          "id": "MDRW2100005027.1.1",
          "form": "대학생이세요?!",
          "original_form": "대학생이세요?!",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005027.1.2",
          "form": "네네! 샌애긔,, 랍니다",
          "original_form": "네네! 샌애긔,, 랍니다",
          "speaker_id": "1",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005027.1.3",
          "form": "근데 이제 곧 미개봉 중고가 될...",
          "original_form": "근데 이제 곧 미개봉 중고가 될...",
          "speaker_id": "1",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005027.1.4",
          "form": "2학기에도 수업을 비대면으로 하는 건가요?ㅠ",
          "original_form": "2학기에도 수업을 비대면으로 하는 건가요?ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005027.1.5",
          "form": "저는 올해 졸업했어요 ..ㅎㅎ!",
          "original_form": "저는 올해 졸업했어요 ..ㅎㅎ!",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005027.1.6",
          "form": "헐 졸업 축하드려요!!",
          "original_form": "헐 졸업 축하드려요!!",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005027.1.7",
          "form": "2학기 4단계 내려가면 대면할 것 같더라고요ㅠㅠ (싫은데,,,",
          "original_form": "2학기 4단계 내려가면 대면할 것 같더라고요ㅠㅠ (싫은데,,,",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005027.1.8",
          "form": "대면이 장단점이 확실히 있더라구요ㅠㅠ",
          "original_form": "대면이 장단점이 확실히 있더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005027.1.9",
          "form": "이번에 수강신청은 잘 하셨나요?!",
          "original_form": "이번에 수강신청은 잘 하셨나요?!",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005027.1.10",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100005027.1.11",
          "form": "너무 화납니다",
          "original_form": "너무 화납니다",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100005027.1.12",
          "form": "몇백씩 돈 내고 학교 다니는데",
          "original_form": "몇백씩 돈 내고 학교 다니는데",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100005027.1.13",
          "form": "원하는 강의도 못 듣고",
          "original_form": "원하는 강의도 못 듣고",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100005027.1.14",
          "form": "잘안되셨나봐요ㅠㅠ",
          "original_form": "잘안되셨나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005027.1.15",
          "form": "똑같은 등록금을 내는데 왜 수업을 못듣는지",
          "original_form": "똑같은 등록금을 내는데 왜 수업을 못듣는지",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005027.1.16",
          "form": "정말 매번 이해가 안돼요 정말 ..",
          "original_form": "정말 매번 이해가 안돼요 정말 ..",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005027.1.17",
          "form": "저는 1학기도 그렇고 이번도 그렇고",
          "original_form": "저는 1학기도 그렇고 이번도 그렇고",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005027.1.18",
          "form": "10시에 딱 했을 땐",
          "original_form": "10시에 딱 했을 땐",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005027.1.19",
          "form": "세상 누구보다 망하고",
          "original_form": "세상 누구보다 망하고",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005027.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005027.1.21",
          "form": "그다음에 끝나기 전에 하나씩 주워서 교체하고",
          "original_form": "그다음에 끝나기 전에 하나씩 주워서 교체하고",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005027.1.22",
          "form": "정정 때 최상의 시간표가 되더라고요",
          "original_form": "정정 때 최상의 시간표가 되더라고요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005027.1.23",
          "form": "그래서 이번 정정 때도 교양 잡길 기대하고 있어요",
          "original_form": "그래서 이번 정정 때도 교양 잡길 기대하고 있어요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005027.1.24",
          "form": "정정때 교양 꼭 잡으시길 기를 드리겠습니다 ..!",
          "original_form": "정정때 교양 꼭 잡으시길 기를 드리겠습니다 ..!",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005027.1.25",
          "form": "과제는 비대면이라 괜찮으신가요?",
          "original_form": "과제는 비대면이라 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005027.1.26",
          "form": "저는 비대면일때 과제가 엄청 많이 늘어서 고생한 기억이 있어요 ㅠㅠ",
          "original_form": "저는 비대면일때 과제가 엄청 많이 늘어서 고생한 기억이 있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005027.1.27",
          "form": "네! 과제가 많긴 하지만",
          "original_form": "네! 과제가 많긴 하지만",
          "speaker_id": "1",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005027.1.28",
          "form": "거의 다 녹화강의니까",
          "original_form": "거의 다 녹화강의니까",
          "speaker_id": "1",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005027.1.29",
          "form": "신나게 놀다가 몰아서 강의 보고 과제하고",
          "original_form": "신나게 놀다가 몰아서 강의 보고 과제하고",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005027.1.30",
          "form": "나름 꿀인 것 같아요...ㅎㅎ",
          "original_form": "나름 꿀인 것 같아요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005027.1.31",
          "form": "맞아요..! 녹화강의 너무 좋아요",
          "original_form": "맞아요..! 녹화강의 너무 좋아요",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005027.1.32",
          "form": "가끔 과제와 강의가 엄청나게 밀릴때가 있지만..",
          "original_form": "가끔 과제와 강의가 엄청나게 밀릴때가 있지만..",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005027.1.33",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005027.1.34",
          "form": "강의 막 몇주차씩 밀리면",
          "original_form": "강의 막 몇주차씩 밀리면",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005027.1.35",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005027.1.36",
          "form": "그럴때 후회하지만 후회만 할뿐 .. 미리 수업을 듣게 되진 않더라구요..ㅎㅎ",
          "original_form": "그럴때 후회하지만 후회만 할뿐 .. 미리 수업을 듣게 되진 않더라구요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005027.1.37",
