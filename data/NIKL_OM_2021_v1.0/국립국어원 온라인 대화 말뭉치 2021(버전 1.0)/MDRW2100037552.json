      "utterance": [
        {
          "id": "MDRW2100037552.1.1",
          "form": "연애 하시나요",
          "original_form": "연애 하시나요",
          "speaker_id": "2",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100037552.1.2",
          "form": "아뇨~~~~",
          "original_form": "아뇨~~~~",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.3",
          "form": "듣고싶어서ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "듣고싶어서ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.5",
          "form": "뭐예요~~~ 만난지 얼마 안됐어요?",
          "original_form": "뭐예요~~~ 만난지 얼마 안됐어요?",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.6",
          "form": "풋풋할때다",
          "original_form": "풋풋할때다",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.7",
          "form": "이제 10일된 새내기",
          "original_form": "이제 10일된 새내기",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.9",
          "form": "어마마",
          "original_form": "어마마",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.10",
          "form": "얘기 좀 해줘봐요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "얘기 좀 해줘봐요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.11",
          "form": "소개 받았어요?",
          "original_form": "소개 받았어요?",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100037552.1.12",
          "form": "오랜 공백기를 끝내고",
          "original_form": "오랜 공백기를 끝내고",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100037552.1.13",
          "form": "소개팅을 겁나하다가",
          "original_form": "소개팅을 겁나하다가",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100037552.1.14",
          "form": "좀 포기할 무렵에",
          "original_form": "좀 포기할 무렵에",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100037552.1.15",
          "form": "드디어 만났어요",
          "original_form": "드디어 만났어요",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100037552.1.16",
          "form": "부러워요~",
          "original_form": "부러워요~",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100037552.1.17",
          "form": "주변 지인이 많이 없어서",
          "original_form": "주변 지인이 많이 없어서",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100037552.1.18",
          "form": "소개팅 받을 곳이 마땅히 ㅜ",
          "original_form": "소개팅 받을 곳이 마땅히 ㅜ",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100037552.1.19",
          "form": "저도 지인 소개팅은 아니었고",
          "original_form": "저도 지인 소개팅은 아니었고",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037552.1.20",
          "form": "엄마가 친구분 딸 결혼식을 갔었는데",
          "original_form": "엄마가 친구분 딸 결혼식을 갔었는데",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037552.1.21",
          "form": "거기서 급 말걸어오는 아주머니가 있으셨오요",
          "original_form": "거기서 급 말걸어오는 아주머니가 있으셨오요",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037552.1.22",
          "form": "딸 있으시냐 소개해줄 남자가 많다",
          "original_form": "딸 있으시냐 소개해줄 남자가 많다",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037552.1.23",
          "form": "중매해주는 분을 만나버린 것이죠",
          "original_form": "중매해주는 분을 만나버린 것이죠",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100037552.1.24",
          "form": "어머ㅋㅋㅋㅋ",
          "original_form": "어머ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100037552.1.25",
          "form": "나이차는 어떻게 돼요?",
          "original_form": "나이차는 어떻게 돼요?",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100037552.1.26",
          "form": "두살 연상을 만났어요",
          "original_form": "두살 연상을 만났어요",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100037552.1.27",
          "form": "근데 맨날 자만추만 해오다가 소개팅으로 남친 만난건 처음인데",
          "original_form": "근데 맨날 자만추만 해오다가 소개팅으로 남친 만난건 처음인데",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100037552.1.28",
          "form": "이것도 나름 매력있네요",
          "original_form": "이것도 나름 매력있네요",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100037552.1.29",
          "form": "제가 다 달달해지네요ㅜ",
          "original_form": "제가 다 달달해지네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 11:11"
        }
      ]
    }
