        },
        {
          "id": "MDRW2100026764.1.2",
          "form": "name4랑 또 일본갈래",
          "original_form": "&name4&랑 또 일본갈래",
          "speaker_id": "1",
          "time": "20210902 12:46"
        },
        {
          "id": "MDRW2100026764.1.3",
          "form": "이거 알림 잘 안와",
          "original_form": "이거 알림 잘 안와",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100026764.1.4",
          "form": "name4랑 또 일본가자",
          "original_form": "&name4&랑 또 일본가자",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100026764.1.5",
          "form": "담에는 도쿄가자",
          "original_form": "담에는 도쿄가자",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100026764.1.6",
          "form": "ㅠㅠ도쿄 가고싶어",
          "original_form": "ㅠㅠ도쿄 가고싶어",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100026764.1.7",
          "form": "삿포로도 가고싶더",
          "original_form": "삿포로도 가고싶더",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100026764.1.8",
          "form": "돈 많이 벌어둬야지",
          "original_form": "돈 많이 벌어둬야지",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100026764.1.9",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100026764.1.10",
          "form": "도쿄는 name3가 잘 안내할 수 잇어!(아마",
          "original_form": "도쿄는 &name3&가 잘 안내할 수 잇어!(아마",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100026764.1.11",
          "form": "삿포로는 name3도 안가봤는데 꼭 가보고싶다...",
          "original_form": "삿포로는 &name3&도 안가봤는데 꼭 가보고싶다...",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100026764.1.12",
          "form": "삿포로 눈 축제? 거기 가보고싶어",
          "original_form": "삿포로 눈 축제? 거기 가보고싶어",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100026764.1.13",
          "form": "마자ㅠㅠ",
          "original_form": "마자ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100026764.1.14",
          "form": "코로나 풀리면 나랑 꼭 도쿄가조ㅠ",
          "original_form": "코로나 풀리면 나랑 꼭 도쿄가조ㅠ",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100026764.1.15",
          "form": "조아! 도쿄 가서 또 마싯는 팬케이크 먹고",
          "original_form": "조아! 도쿄 가서 또 마싯는 팬케이크 먹고",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100026764.1.16",
          "form": "사진도 찍구",
          "original_form": "사진도 찍구",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100026764.1.17",
          "form": "기여운거 보러가자",
          "original_form": "기여운거 보러가자",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100026764.1.18",
          "form": "넘 조와....ㅠㅠ",
          "original_form": "넘 조와....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100026764.1.19",
          "form": "팬케이크 맛있겠다,,,,,,",
          "original_form": "팬케이크 맛있겠다,,,,,,",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100026764.1.20",
          "form": "폭신폭신 팬케이크",
          "original_form": "폭신폭신 팬케이크",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100026764.1.21",
          "form": "그리구 스페인도 가고싶어",
          "original_form": "그리구 스페인도 가고싶어",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100026764.1.22",
          "form": "마자......",
          "original_form": "마자......",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100026764.1.23",
          "form": "name4랑 스페인 함 가야하는데ㅠㅠ",
          "original_form": "&name4&랑 스페인 함 가야하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100026764.1.24",
