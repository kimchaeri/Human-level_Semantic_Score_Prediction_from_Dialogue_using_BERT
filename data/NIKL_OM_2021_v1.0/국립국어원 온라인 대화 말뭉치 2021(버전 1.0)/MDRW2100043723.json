      "utterance": [
        {
          "id": "MDRW2100043723.1.1",
          "form": "제가 지금 먹고싶은 음식 맞춰보실래요?",
          "original_form": "제가 지금 먹고싶은 음식 맞춰보실래요?",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043723.1.2",
          "form": "좋아요!",
          "original_form": "좋아요!",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043723.1.3",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043723.1.4",
          "form": "어...네..(?) 한국산 입니다",
          "original_form": "어...네..(?) 한국산 입니다",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043723.1.5",
          "form": "한끼 식사는 아니예요",
          "original_form": "한끼 식사는 아니예요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043723.1.6",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043723.1.7",
          "form": "디저트로 먹는 건가요!",
          "original_form": "디저트로 먹는 건가요!",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043723.1.8",
          "form": "네 맞아요! 척하면 척이시군요!!!",
          "original_form": "네 맞아요! 척하면 척이시군요!!!",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043723.1.9",
          "form": "칭찬봇이세요ㅋㅋㅋㅋㅋ",
          "original_form": "칭찬봇이세요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043723.1.10",
          "form": "과일인가요??",
          "original_form": "과일인가요??",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043723.1.11",
          "form": "과일은 아니랍니다~~~~ 하하",
          "original_form": "과일은 아니랍니다~~~~ 하하",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043723.1.12",
          "form": "밀가루가 들어가나요!!",
          "original_form": "밀가루가 들어가나요!!",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043723.1.13",
          "form": "이번 건 좀 어렵겠어요ㅋㅋㅋㅋ",
          "original_form": "이번 건 좀 어렵겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043723.1.14",
          "form": "헉 네! 밀가루 팍팍 들어간답니다",
          "original_form": "헉 네! 밀가루 팍팍 들어간답니다",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043723.1.15",
          "form": "ㅋㅋㅋㅋ후후 정확히 맞춰야 인정하겠어요",
          "original_form": "ㅋㅋㅋㅋ후후 정확히 맞춰야 인정하겠어요",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043723.1.16",
          "form": "빵인가요???",
          "original_form": "빵인가요???",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043723.1.17",
          "form": "과자인가요!!!",
          "original_form": "과자인가요!!!",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043723.1.18",
          "form": "과자입니다!!! 바삭바삭",
          "original_form": "과자입니다!!! 바삭바삭",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043723.1.19",
          "form": "음 초코맛인가요??",
          "original_form": "음 초코맛인가요??",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043723.1.20",
          "form": "아니요! 초코맛 아닙니다",
          "original_form": "아니요! 초코맛 아닙니다",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043723.1.21",
          "form": "으으으음 포장지 색깔이 어떤 색인가요??",
          "original_form": "으으으음 포장지 색깔이 어떤 색인가요??",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043723.1.22",
          "form": "헉 빨간색이요 예리하시군요",
          "original_form": "헉 빨간색이요 예리하시군요",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043723.1.23",
          "form": "와 감이 안 오네요",
          "original_form": "와 감이 안 오네요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043723.1.24",
          "form": "빨간 봉지에 바삭바삭한 밀가루가",
          "original_form": "빨간 봉지에 바삭바삭한 밀가루가",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043723.1.25",
          "form": "담겨있습니다 후후",
          "original_form": "담겨있습니다 후후",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043723.1.26",
          "form": "빨간색..으음",
          "original_form": "빨간색..으음",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043723.1.27",
          "form": "꼬깔콘 스윙칩 선칩 눈을감자 쿠크다스 치토스 중에 있나요?",
          "original_form": "꼬깔콘 스윙칩 선칩 눈을감자 쿠크다스 치토스 중에 있나요?",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043723.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ우르르르 다 나오냐구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ우르르르 다 나오냐구요",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043723.1.29",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043723.1.30",
          "form": "아니요! 아깝게 저 중에는 없습니다",
          "original_form": "아니요! 아깝게 저 중에는 없습니다",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043723.1.31",
          "form": "아니면 무슨 닭다리??",
          "original_form": "아니면 무슨 닭다리??",
