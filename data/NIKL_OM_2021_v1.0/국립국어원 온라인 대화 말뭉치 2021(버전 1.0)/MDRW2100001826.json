      "utterance": [
        {
          "id": "MDRW2100001826.1.1",
          "form": "아르바이트 해보신 적 있으세요?",
          "original_form": "아르바이트 해보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001826.1.2",
          "form": "네! 아르바이트를 오래 하지는 못했는데",
          "original_form": "네! 아르바이트를 오래 하지는 못했는데",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001826.1.3",
          "form": "꽤 여러가지 해본 것 같아요",
          "original_form": "꽤 여러가지 해본 것 같아요",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001826.1.4",
          "form": "우와 어떤거 해보셨어요?",
          "original_form": "우와 어떤거 해보셨어요?",
          "speaker_id": "2",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001826.1.5",
          "form": "전 여러개는 못해봤어요ㅠㅠ",
          "original_form": "전 여러개는 못해봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001826.1.6",
          "form": "전 전단지랑 호텔알바, 택배, 급식알바, 유치원알바, 서빙, 카페, 피씨방, 판매 등등?.. 해봤어요",
          "original_form": "전 전단지랑 호텔알바, 택배, 급식알바, 유치원알바, 서빙, 카페, 피씨방, 판매 등등?.. 해봤어요",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001826.1.7",
          "form": "중학생때부터 한거라 그냥 하다보니 이것저것 많이 건들여 본거같아요",
          "original_form": "중학생때부터 한거라 그냥 하다보니 이것저것 많이 건들여 본거같아요",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001826.1.8",
          "form": "볶음밥님은 어떤거 해보셨어요?!",
          "original_form": "볶음밥님은 어떤거 해보셨어요?!",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001826.1.9",
          "form": "우와 진짜 많이 하셨네요 대박",
          "original_form": "우와 진짜 많이 하셨네요 대박",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001826.1.10",
          "form": "저는 그냥 애기들 가르치는 거랑 티켓알바 이런거 했었어요!",
          "original_form": "저는 그냥 애기들 가르치는 거랑 티켓알바 이런거 했었어요!",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001826.1.11",
          "form": "ㅋㅌㅋㅋㅋ다 단기로 했어요",
          "original_form": "ㅋㅌㅋㅋㅋ다 단기로 했어요",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001826.1.12",
          "form": "티켓알바 완전 꿀알바라고하던데 어떠셨어요?! 궁금해요",
          "original_form": "티켓알바 완전 꿀알바라고하던데 어떠셨어요?! 궁금해요",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001826.1.13",
          "form": "저도 티켓알바는 단기였는데",
          "original_form": "저도 티켓알바는 단기였는데",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001826.1.14",
          "form": "일단 실내니까 시원해서 좋았어요ㅎㅎ그리고 대극장 아니었어서 사람도 별로 없었어요",
          "original_form": "일단 실내니까 시원해서 좋았어요ㅎㅎ그리고 대극장 아니었어서 사람도 별로 없었어요",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001826.1.15",
          "form": "아하 ㅜㅜ저도 그런알바 구하고싶네요",
          "original_form": "아하 ㅜㅜ저도 그런알바 구하고싶네요",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001826.1.16",
          "form": "요즘엔 코로나라서 알바도 잘 안구해지고",
          "original_form": "요즘엔 코로나라서 알바도 잘 안구해지고",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001826.1.17",
          "form": "과장일수도있는데 면접만 100명넘게 봤다는 사장님도 봤어요",
          "original_form": "과장일수도있는데 면접만 100명넘게 봤다는 사장님도 봤어요",
          "speaker_id": "1",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001826.1.18",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001826.1.19",
          "form": "저 지금 사는 곳은 대학생이나 젊은 사람들이 많이 없어서",
          "original_form": "저 지금 사는 곳은 대학생이나 젊은 사람들이 많이 없어서",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001826.1.20",
          "form": "여긴 서빙알바 같은 거 구하기 진짜 힘들대요 사장님들이",
          "original_form": "여긴 서빙알바 같은 거 구하기 진짜 힘들대요 사장님들이",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001826.1.21",
          "form": "헉 정말요? ㅜㅜ 모아니면 도네요",
          "original_form": "헉 정말요? ㅜㅜ 모아니면 도네요",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001826.1.22",
          "form": "전 번화가쪽만 지원했어서 더 경쟁이 치열했나봐요",
          "original_form": "전 번화가쪽만 지원했어서 더 경쟁이 치열했나봐요",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001826.1.23",
          "form": "근데 여기가 특이한거 같긴 하더라구요ㅠㅠ",
          "original_form": "근데 여기가 특이한거 같긴 하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001826.1.24",
          "form": "어우 근데 진짜 코로나때문에 일 구하기도 쉽지 않아서ㅠ",
          "original_form": "어우 근데 진짜 코로나때문에 일 구하기도 쉽지 않아서ㅠ",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001826.1.25",
          "form": "정말요 .. ㅜㅜ 일자리가 너무 없어서 큰일이에요 취업은 잘 할지 모르겠네요",
          "original_form": "정말요 .. ㅜㅜ 일자리가 너무 없어서 큰일이에요 취업은 잘 할지 모르겠네요",
          "speaker_id": "1",
          "time": "20210819 21:58"
        }
      ]
