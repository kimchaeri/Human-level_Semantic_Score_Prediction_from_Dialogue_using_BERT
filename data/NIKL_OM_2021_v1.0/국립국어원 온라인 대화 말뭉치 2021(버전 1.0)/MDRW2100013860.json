      "utterance": [
        {
          "id": "MDRW2100013860.1.1",
          "form": "지금 학교 다니시나요??",
          "original_form": "지금 학교 다니시나요??",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013860.1.2",
          "form": "네 대학교 방학이 곧 끝나네요 ㅠㅠ",
          "original_form": "네 대학교 방학이 곧 끝나네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013860.1.3",
          "form": "글게요ㅠㅠ 혹시 전공 여쭤봐도 될까요??",
          "original_form": "글게요ㅠㅠ 혹시 전공 여쭤봐도 될까요??",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013860.1.4",
          "form": "저는 공대쪽이에요  화학공학",
          "original_form": "저는 공대쪽이에요  화학공학",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013860.1.5",
          "form": "오 전 통걔 전공이에요",
          "original_form": "오 전 통걔 전공이에요",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013860.1.6",
          "form": "우와!! 듣기만해도 어렵네요",
          "original_form": "우와!! 듣기만해도 어렵네요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013860.1.7",
          "form": "저랑 되게 안맞긴 해요ㅠㅠ",
          "original_form": "저랑 되게 안맞긴 해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013860.1.8",
          "form": "전공은 적성에 잘 맞으신가요??",
          "original_form": "전공은 적성에 잘 맞으신가요??",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013860.1.9",
          "form": "저는 사실 원전공이 잘안맞아서 복수전공을 해서 화학공학 전공이된거에요 ㅠㅠ",
          "original_form": "저는 사실 원전공이 잘안맞아서 복수전공을 해서 화학공학 전공이된거에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013860.1.10",
          "form": "앗 저도 통계학이 잘 안맞아서 국문 복전하고있어요ㅋㅋ",
          "original_form": "앗 저도 통계학이 잘 안맞아서 국문 복전하고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013860.1.11",
          "form": "우와 ㅋㅋㅋㅋ비슷하게 통하는점이많네요  전공은좀다르지만",
          "original_form": "우와 ㅋㅋㅋㅋ비슷하게 통하는점이많네요  전공은좀다르지만",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013860.1.12",
          "form": "그러게요ㅋㅋㅋ 신기하네요",
          "original_form": "그러게요ㅋㅋㅋ 신기하네요",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013860.1.13",
          "form": "혹시 원전공은 어떤거였나요??",
          "original_form": "혹시 원전공은 어떤거였나요??",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013860.1.14",
          "form": "원전공은 건축이었어요",
          "original_form": "원전공은 건축이었어요",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013860.1.15",
          "form": "오 되게 어려운 거였네요...",
          "original_form": "오 되게 어려운 거였네요...",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013860.1.16",
          "form": "큽 ㅠㅠ 그래서 잘안맞았나봐요 국문학과는어때요?",
          "original_form": "큽 ㅠㅠ 그래서 잘안맞았나봐요 국문학과는어때요?",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013860.1.17",
          "form": "아 훨씬 나아요ㅋㅋㅋ",
          "original_form": "아 훨씬 나아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013860.1.18",
          "form": "전공을 잘못 정했던 거 같아요ㅋㅋㅋ",
          "original_form": "전공을 잘못 정했던 거 같아요ㅋㅋㅋ",
