      "utterance": [
        {
          "id": "MDRW2100006419.1.1",
          "form": "낼도 비올까요?",
          "original_form": "낼도 비올까요?",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006419.1.2",
          "form": "비가 언제까지 올까요?",
          "original_form": "비가 언제까지 올까요?",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006419.1.3",
          "form": "그만좀 오면 좋겠어요",
          "original_form": "그만좀 오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006419.1.4",
          "form": "이머니 쓰러 이마트가야되는데 이번주 계속오는거아니겠죠?",
          "original_form": "이머니 쓰러 이마트가야되는데 이번주 계속오는거아니겠죠?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006419.1.5",
          "form": "그만올꺼같긴해요",
          "original_form": "그만올꺼같긴해요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006419.1.6",
          "form": "축축해서 비오는거 넘싫은데",
          "original_form": "축축해서 비오는거 넘싫은데",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006419.1.7",
          "form": "지긋지긋하네요",
          "original_form": "지긋지긋하네요",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006419.1.8",
          "form": "넘 집이 습해요",
          "original_form": "넘 집이 습해요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006419.1.9",
          "form": "여름엔 안오고 가을에 비가 많이오네요",
          "original_form": "여름엔 안오고 가을에 비가 많이오네요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006419.1.10",
          "form": "지금 일기예보보니 낼 오전까지만 비뜨고 담주까진 괜찮네요",
          "original_form": "지금 일기예보보니 낼 오전까지만 비뜨고 담주까진 괜찮네요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006419.1.11",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006419.1.12",
          "form": "기상이 이상해요",
          "original_form": "기상이 이상해요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006419.1.13",
          "form": "네~  쫌 뽀송하게 말랐으면 좋겠어요",
          "original_form": "네~  쫌 뽀송하게 말랐으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006419.1.14",
          "form": "저도요 파란하늘보고싶네요",
          "original_form": "저도요 파란하늘보고싶네요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006419.1.15",
          "form": "낼지남 가을하늘 볼수있겠죠.",
          "original_form": "낼지남 가을하늘 볼수있겠죠.",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006419.1.16",
          "form": "비그침 좋을거같아요",
          "original_form": "비그침 좋을거같아요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006419.1.17",
          "form": "주말엔 좋았는데",
          "original_form": "주말엔 좋았는데",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006419.1.18",
          "form": "이제 좋아질꺼에요.",
          "original_form": "이제 좋아질꺼에요.",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006419.1.19",
          "form": "어디갈곳은없지만 날씨라도 좋음좋겠네요",
          "original_form": "어디갈곳은없지만 날씨라도 좋음좋겠네요",
          "speaker_id": "2",
          "time": "20210908 00:03"
        }
      ]
