      "utterance": [
        {
          "id": "MDRW2100017560.1.1",
          "form": "최근에 사신거 있나요?",
          "original_form": "최근에 사신거 있나요?",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100017560.1.2",
          "form": "최근에 맨투맨 샀어요",
          "original_form": "최근에 맨투맨 샀어요",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017560.1.3",
          "form": "가을옷 사야죠!",
          "original_form": "가을옷 사야죠!",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017560.1.4",
          "form": "오오 맨투맨 ! 어디꺼 사셨어요 ㅎㅎ",
          "original_form": "오오 맨투맨 ! 어디꺼 사셨어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017560.1.5",
          "form": "저도 슬슬 사려구요 ㅎㅎ",
          "original_form": "저도 슬슬 사려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017560.1.6",
          "form": "저는 배드블러드에서 샀어요!",
          "original_form": "저는 배드블러드에서 샀어요!",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017560.1.7",
          "form": "마침 세일하길래요ㅎㅎ",
          "original_form": "마침 세일하길래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017560.1.8",
          "form": "오 거기 브랜드 괜찮은가요 ??",
          "original_form": "오 거기 브랜드 괜찮은가요 ??",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017560.1.9",
          "form": "저번에 무신사에서 본거같아요 ㅎㅎ",
          "original_form": "저번에 무신사에서 본거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017560.1.10",
          "form": "네! 저는 디자인이 취향이기도 한데",
          "original_form": "네! 저는 디자인이 취향이기도 한데",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017560.1.11",
          "form": "질도 좋아요!",
          "original_form": "질도 좋아요!",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017560.1.12",
          "form": "가격대가 있는데 세일 때 사시면 돼요ㅎㅎ",
          "original_form": "가격대가 있는데 세일 때 사시면 돼요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017560.1.13",
          "form": "오오 한번 사봐야겠어요 !",
          "original_form": "오오 한번 사봐야겠어요 !",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017560.1.14",
          "form": "저는 질 좋은 맨투맨을 선호해서 ㅎㅎ",
          "original_form": "저는 질 좋은 맨투맨을 선호해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017560.1.15",
          "form": "저두요ㅠㅠ 얘기하니까 하나 더 사고 싶네요ㅎㅎ",
          "original_form": "저두요ㅠㅠ 얘기하니까 하나 더 사고 싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017560.1.16",
          "form": "옷은 사도사도 사고싶어요 ㅋㅋㅋ",
          "original_form": "옷은 사도사도 사고싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017560.1.17",
          "form": "특히 저는 가을옷을 좋아해서",
          "original_form": "특히 저는 가을옷을 좋아해서",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017560.1.18",
          "form": "많이 사요 ㅎㅎ",
          "original_form": "많이 사요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017560.1.19",
          "form": "저두요ㅠㅠㅠ",
          "original_form": "저두요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017560.1.20",
          "form": "저도 가을 아우터나 맨투맨 후드티 좋아해서",
          "original_form": "저도 가을 아우터나 맨투맨 후드티 좋아해서",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017560.1.21",
          "form": "제일 많이 사요",
          "original_form": "제일 많이 사요",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017560.1.22",
          "form": "맞아요 ㅎㅎ 제 최애 패션이 후드티에 트레이닝 바지 입는거에요 ㅋㅋ",
          "original_form": "맞아요 ㅎㅎ 제 최애 패션이 후드티에 트레이닝 바지 입는거에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017560.1.23",
          "form": "가을에만 입을 수 있는 ㅎㅎ",
          "original_form": "가을에만 입을 수 있는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017560.1.24",
          "form": "저두요!!! 그래서 후드랑 맨투맨은 매년 사요 진짜ㅋㅋㅋ",
          "original_form": "저두요!!! 그래서 후드랑 맨투맨은 매년 사요 진짜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017560.1.25",
          "form": "겨울엔 그 위에 롱패딩 입고.. ㅋㅋㅋ",
          "original_form": "겨울엔 그 위에 롱패딩 입고.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017560.1.26",
          "form": "맞아요 ㅋㅋㅋ 롱패딩 겨울 필수템이죠 !",
          "original_form": "맞아요 ㅋㅋㅋ 롱패딩 겨울 필수템이죠 !",
          "speaker_id": "1",
          "time": "20210826 20:38"
        }
      ]
