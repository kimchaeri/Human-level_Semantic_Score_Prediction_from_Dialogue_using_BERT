      "utterance": [
        {
          "id": "MDRW2100008306.1.1",
          "form": "지금 날씨 어때요?",
          "original_form": "지금 날씨 어때요?",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.2",
          "form": "오늘은 맑아요",
          "original_form": "오늘은 맑아요",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.3",
          "form": "어제는 비가 엄청왔거든요",
          "original_form": "어제는 비가 엄청왔거든요",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.5",
          "form": "저도 비슷해요",
          "original_form": "저도 비슷해요",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.6",
          "form": "댜신 좀 더운거같은?",
          "original_form": "댜신 좀 더운거같은?",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.7",
          "form": "습해서 그런걸까요",
          "original_form": "습해서 그런걸까요",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.8",
          "form": "그런가봐요ㅠ 에어컨 트셨나요",
          "original_form": "그런가봐요ㅠ 에어컨 트셨나요",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008306.1.9",
          "form": "에어컨 틀기에는 조금 애매한것같아서 ㅠㅠ",
          "original_form": "에어컨 틀기에는 조금 애매한것같아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008306.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008306.1.11",
          "form": "저는 오전까지 틀다가 지금은 창문열고 선풍기 틀어놨어요",
          "original_form": "저는 오전까지 틀다가 지금은 창문열고 선풍기 틀어놨어요",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008306.1.12",
          "form": "제가 사는 곳은 바람이 선선하니 불어서",
          "original_form": "제가 사는 곳은 바람이 선선하니 불어서",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008306.1.13",
          "form": "덥지는 않은 것 같아요",
          "original_form": "덥지는 않은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008306.1.14",
          "form": "오 부러워요!",
          "original_form": "오 부러워요!",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008306.1.15",
          "form": "산책하기 좋겠엉ㅅ",
          "original_form": "산책하기 좋겠엉ㅅ",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008306.1.16",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008306.1.17",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008306.1.18",
          "form": "맞아요 살랑살랑 나갔다오기 좋을 거 같아요",
          "original_form": "맞아요 살랑살랑 나갔다오기 좋을 거 같아요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008306.1.19",
          "form": "저는 늦여름을 좋아해요",
          "original_form": "저는 늦여름을 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008306.1.20",
          "form": "더위가 한풀꺾이는 가을이 오기 전 날씨가 좋아요",
          "original_form": "더위가 한풀꺾이는 가을이 오기 전 날씨가 좋아요",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008306.1.21",
          "form": "name2님은 어떠세요",
          "original_form": "&name2&님은 어떠세요",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008306.1.22",
          "form": "표현이 너무 예쁘네요",
          "original_form": "표현이 너무 예쁘네요",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008306.1.23",
          "form": "저는 가을이 좋아요",
          "original_form": "저는 가을이 좋아요",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008306.1.24",
          "form": "산책할때 가장 즐거운것같아요",
          "original_form": "산책할때 가장 즐거운것같아요",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008306.1.25",
          "form": "어떤점이 즐겁나요",
          "original_form": "어떤점이 즐겁나요",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008306.1.26",
          "form": "덥지않은 공기 맑은 하늘?",
          "original_form": "덥지않은 공기 맑은 하늘?",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008306.1.27",
          "form": "이런점이요?",
          "original_form": "이런점이요?",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008306.1.28",
          "form": "많아요!",
          "original_form": "많아요!",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008306.1.29",
          "form": "단풍 풍경도 너무 아름답고요",
          "original_form": "단풍 풍경도 너무 아름답고요",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008306.1.30",
          "form": "그리고 더운 걸 너무 싫어해서",
          "original_form": "그리고 더운 걸 너무 싫어해서",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008306.1.31",
          "form": "살았다..하고 느끼는 것같아요",
          "original_form": "살았다..하고 느끼는 것같아요",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008306.1.32",
          "form": "정말 가을 그 자체를 사랑하시는군요",
          "original_form": "정말 가을 그 자체를 사랑하시는군요",
          "speaker_id": "1",
          "time": "20210822 15:47"
        }
      ]
    }
