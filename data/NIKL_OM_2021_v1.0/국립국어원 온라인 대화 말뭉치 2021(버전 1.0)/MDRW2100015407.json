      "utterance": [
        {
          "id": "MDRW2100015407.1.1",
          "form": "치킨 생각나는 밤이예요",
          "original_form": "치킨 생각나는 밤이예요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015407.1.2",
          "form": "앗 저 치킨 오늘시킴",
          "original_form": "앗 저 치킨 오늘시킴",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100015407.1.3",
          "form": "꺄",
          "original_form": "꺄",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100015407.1.4",
          "form": "어디꺼요",
          "original_form": "어디꺼요",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100015407.1.5",
          "form": "비에이치",
          "original_form": "비에이치",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100015407.1.6",
          "form": "치킨배달 이조",
          "original_form": "치킨배달 이조",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100015407.1.7",
          "form": "저녁 모드셨나요??",
          "original_form": "저녁 모드셨나요??",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100015407.1.8",
          "form": "비에이치씨는 뿌링클밖에 생각이 안나요",
          "original_form": "비에이치씨는 뿌링클밖에 생각이 안나요",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100015407.1.9",
          "form": "^^네",
          "original_form": "^^네",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100015407.1.10",
          "form": "저는 양념게장에 밥먹었어요ㅎㅎㅎ",
          "original_form": "저는 양념게장에 밥먹었어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100015407.1.11",
          "form": "와 밥도둑드셨네요",
          "original_form": "와 밥도둑드셨네요",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100015407.1.12",
          "form": "야식은 안드시죠?",
          "original_form": "야식은 안드시죠?",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100015407.1.13",
          "form": "야식먹으면 늦게자야해서 금요일 말고는 잘 안먹어요",
          "original_form": "야식먹으면 늦게자야해서 금요일 말고는 잘 안먹어요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100015407.1.14",
          "form": "오늘 야식 드시나요?",
          "original_form": "오늘 야식 드시나요?",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100015407.1.15",
          "form": "네 치킨이요",
          "original_form": "네 치킨이요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100015407.1.16",
          "form": "자주는 매일 먹는듯 야식요",
          "original_form": "자주는 매일 먹는듯 야식요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100015407.1.17",
          "form": "아 맞다 치킨시켰다 하셨지",
          "original_form": "아 맞다 치킨시켰다 하셨지",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100015407.1.18",
          "form": "정신이 없네요ㅋㅋㅋㅋ",
          "original_form": "정신이 없네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100015407.1.19",
          "form": "어제는 쉬었어요",
          "original_form": "어제는 쉬었어요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100015407.1.20",
          "form": "원래 며칠 쉬다가 먹어줘야 더 맛있는걸요~~",
          "original_form": "원래 며칠 쉬다가 먹어줘야 더 맛있는걸요~~",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100015407.1.21",
          "form": "이제 도착했어요",
          "original_form": "이제 도착했어요",
          "speaker_id": "2",
