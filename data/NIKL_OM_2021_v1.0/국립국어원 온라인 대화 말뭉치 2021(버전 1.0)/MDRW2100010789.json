      "utterance": [
        {
          "id": "MDRW2100010789.1.1",
          "form": "오랜만에 떠올렸어요 ㅋㅋ",
          "original_form": "오랜만에 떠올렸어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:52"
        },
        {
          "id": "MDRW2100010789.1.2",
          "form": "맞아요 진짜 새록새록!!",
          "original_form": "맞아요 진짜 새록새록!!",
          "speaker_id": "2",
          "time": "20210824 08:47"
        },
        {
          "id": "MDRW2100010789.1.3",
          "form": "저는 겨울왕국이 떠오르네요 보셨나요",
          "original_form": "저는 겨울왕국이 떠오르네요 보셨나요",
          "speaker_id": "2",
          "time": "20210824 08:47"
        },
        {
          "id": "MDRW2100010789.1.4",
          "form": "제가 바로 그 귀하다는 겨울왕국2안본 사람이요 ㅋㅋ",
          "original_form": "제가 바로 그 귀하다는 겨울왕국2안본 사람이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:56"
        },
        {
          "id": "MDRW2100010789.1.5",
          "form": "1도 봤던가 말았던가",
          "original_form": "1도 봤던가 말았던가",
          "speaker_id": "1",
          "time": "20210824 08:56"
        },
        {
          "id": "MDRW2100010789.1.6",
          "form": "헐 저도요!!",
          "original_form": "헐 저도요!!",
          "speaker_id": "2",
          "time": "20210824 09:00"
        },
        {
          "id": "MDRW2100010789.1.7",
          "form": "1만 보고 2는 안봤어요 ㅋㅋㅋㅋ",
          "original_form": "1만 보고 2는 안봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:00"
        },
        {
          "id": "MDRW2100010789.1.8",
          "form": "인투디언노운 노래만 알아요 ㅋㅋㅋㅋ",
          "original_form": "인투디언노운 노래만 알아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:00"
        },
        {
          "id": "MDRW2100010789.1.9",
          "form": "전 일부러 내용도 안찾아봤어요 ㅋㅋ",
          "original_form": "전 일부러 내용도 안찾아봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:15"
        },
        {
          "id": "MDRW2100010789.1.10",
          "form": "ㅋㅋㅋㅋ헉 저는 스포는 다 당해서",
          "original_form": "ㅋㅋㅋㅋ헉 저는 스포는 다 당해서",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100010789.1.11",
          "form": "내용만 알아요 ㅋㅋㅋㅋ",
          "original_form": "내용만 알아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100010789.1.12",
          "form": "좀만 더있다가 공짜로 볼거라며ㅋㅋ",
          "original_form": "좀만 더있다가 공짜로 볼거라며ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100010789.1.13",
          "form": "아 이제 슬슬 티비에서 해주겠네요 ㅋㅋㅋ",
          "original_form": "아 이제 슬슬 티비에서 해주겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100010789.1.14",
          "form": "요샌 또 금방금방 해주더라고요 명절에 ㅋㅋ",
          "original_form": "요샌 또 금방금방 해주더라고요 명절에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100010789.1.15",
          "form": "맞아욬ㅋㅋㅋㅋ 특히 ㄱ가족 애니메이션은 잘 해줄거같아요",
          "original_form": "맞아욬ㅋㅋㅋㅋ 특히 ㄱ가족 애니메이션은 잘 해줄거같아요",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100010789.1.16",
          "form": "이번 추석에 당장 해줄지도요 ㅋㅋ",
          "original_form": "이번 추석에 당장 해줄지도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100010789.1.17",
          "form": "ㅋㅋㅋㅋㅋ 저번에 코코? 해줫던거같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 저번에 코코? 해줫던거같아요",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100010789.1.18",
          "form": "그러니까요 ㅋㅋ 넷플릭스 같은건 구독 안하세요?",
          "original_form": "그러니까요 ㅋㅋ 넷플릭스 같은건 구독 안하세요?",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100010789.1.19",
          "form": "저 넷플은 안보고 ip tv 보는데",
          "original_form": "저 넷플은 안보고 ip tv 보는데",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100010789.1.20",
          "form": "요새 꼬리에 꼬리를 무는 그날이야기도 열심히 보고있아ㅓ요 ㅋㅋㅋㅋ",
          "original_form": "요새 꼬리에 꼬리를 무는 그날이야기도 열심히 보고있아ㅓ요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100010789.1.21",
          "form": "오 그건 개별구매 아닌가요 ㅋㅋ",
          "original_form": "오 그건 개별구매 아닌가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100010789.1.22",
          "form": "그것도 구독 서비스 있나요",
          "original_form": "그것도 구독 서비스 있나요",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100010789.1.23",
          "form": "아 맞아요 걍 월마다 요금내면 무료로 제공되는거 보고잇어요",
          "original_form": "아 맞아요 걍 월마다 요금내면 무료로 제공되는거 보고잇어요",
          "speaker_id": "2",
