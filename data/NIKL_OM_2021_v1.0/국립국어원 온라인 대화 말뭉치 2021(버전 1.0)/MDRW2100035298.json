      "utterance": [
        {
          "id": "MDRW2100035298.1.1",
          "form": "명절이후엔 떡국떡으로 떡볶이가 국룰 아닌가요?ㅋㅋ",
          "original_form": "명절이후엔 떡국떡으로 떡볶이가 국룰 아닌가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035298.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035298.1.3",
          "form": "항상 함께하조",
          "original_form": "항상 함께하조",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035298.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035298.1.5",
          "form": "그걸로 처리해야 떡이 빨리 줄더라구요",
          "original_form": "그걸로 처리해야 떡이 빨리 줄더라구요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035298.1.6",
          "form": "떡볶이 끓일때 어묵 넣어드시나요?",
          "original_form": "떡볶이 끓일때 어묵 넣어드시나요?",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035298.1.7",
          "form": "맞아요 떡국만 먹다가 질릴때 먹기좋아요",
          "original_form": "맞아요 떡국만 먹다가 질릴때 먹기좋아요",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035298.1.8",
          "form": "전 좀 넣긴하는데요 전 국물파라서",
          "original_form": "전 좀 넣긴하는데요 전 국물파라서",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100035298.1.9",
          "form": "멸치액젓 설탕 이런 맛이 들어가니 어묵이 필수는 아니에요",
          "original_form": "멸치액젓 설탕 이런 맛이 들어가니 어묵이 필수는 아니에요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100035298.1.10",
          "form": "더 좋아해는건 대파 양배추요",
          "original_form": "더 좋아해는건 대파 양배추요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100035298.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100035298.1.12",
          "form": "대파 양배추 필수죠",
          "original_form": "대파 양배추 필수죠",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.13",
          "form": "이상하게 빠지면 그 맛이 안나더라구요",
          "original_form": "이상하게 빠지면 그 맛이 안나더라구요",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.14",
          "form": "어묵은 있음 넣구 없음 말구",
          "original_form": "어묵은 있음 넣구 없음 말구",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.15",
          "form": "매콤한 그 맛이 있어야해요",
          "original_form": "매콤한 그 맛이 있어야해요",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.16",
          "form": "전 엄마랑 먹을때",
          "original_form": "전 엄마랑 먹을때",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.17",
          "form": "엄마는 어묵만 먹고 전 떡만 먹어서",
          "original_form": "엄마는 어묵만 먹고 전 떡만 먹어서",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.18",
          "form": "어묵이 필수에요",
          "original_form": "어묵이 필수에요",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035298.1.20",
          "form": "그래도 서루 나눠먹으니 좋네요 ㅋㅋㅋㅋ",
          "original_form": "그래도 서루 나눠먹으니 좋네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.21",
          "form": "요즘은 떡반 어묵반씩 고를수도 있더라구요",
          "original_form": "요즘은 떡반 어묵반씩 고를수도 있더라구요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.22",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.24",
          "form": "세상이 참 많이 다양해졌어요",
          "original_form": "세상이 참 많이 다양해졌어요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.25",
          "form": "어묵 반이요? 좀 비싼 배달 떡볶이겠네요",
          "original_form": "어묵 반이요? 좀 비싼 배달 떡볶이겠네요",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.26",
          "form": "네 엄마랑 먹을땐",
          "original_form": "네 엄마랑 먹을땐",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.27",
          "form": "학교앞 분식점은 어묵은 쬐꼼 들어가는데",
          "original_form": "학교앞 분식점은 어묵은 쬐꼼 들어가는데",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.28",
          "form": "엄마가 사줘서",
          "original_form": "엄마가 사줘서",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.29",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035298.1.30",
          "form": "ㅠ이제 시대는 엽떡 배달 로제 떡볶이 배달인데",
          "original_form": "ㅠ이제 시대는 엽떡 배달 로제 떡볶이 배달인데",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100035298.1.31",
          "form": "전 초등학생때 떡볶이 스타일을 고집하니깐ㅋ",
          "original_form": "전 초등학생때 떡볶이 스타일을 고집하니깐ㅋ",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100035298.1.32",
          "form": "확실히 뒤쳐지긴 하네요",
          "original_form": "확실히 뒤쳐지긴 하네요",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100035298.1.33",
          "form": "제가 먹은 최신 유행 떡볶이는",
          "original_form": "제가 먹은 최신 유행 떡볶이는",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100035298.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100035298.1.35",
          "form": "근데 먹다보면",
          "original_form": "근데 먹다보면",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100035298.1.36",
          "form": "꼭 그 기본떡볶이가 땡기더라구요",
          "original_form": "꼭 그 기본떡볶이가 땡기더라구요",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100035298.1.37",
          "form": "또보겠지 떡볶이에요",
          "original_form": "또보겠지 떡볶이에요",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100035298.1.38",
          "form": "그죠~ 아는 맛이맛있는 맛에에욬",
          "original_form": "그죠~ 아는 맛이맛있는 맛에에욬",
          "speaker_id": "2",
          "time": "20210829 14:14"
        }
      ]
    }
