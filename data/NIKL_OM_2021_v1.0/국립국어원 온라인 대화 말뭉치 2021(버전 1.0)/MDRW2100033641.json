      "utterance": [
        {
          "id": "MDRW2100033641.1.1",
          "form": "요리좋아하시나요",
          "original_form": "요리좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100033641.1.2",
          "form": "좋아하는데",
          "original_form": "좋아하는데",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100033641.1.3",
          "form": "잘 못만들어요ㅋㅋㅋㅋㅋ",
          "original_form": "잘 못만들어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100033641.1.4",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100033641.1.5",
          "form": "네 저도요ㅋㅋ",
          "original_form": "네 저도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100033641.1.6",
          "form": "매일해요",
          "original_form": "매일해요",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100033641.1.7",
          "form": "어떤 요리 자신있으세요?",
          "original_form": "어떤 요리 자신있으세요?",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100033641.1.8",
          "form": "우와 대단해요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우와 대단해요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100033641.1.9",
          "form": "자신 있는건 딱히 흑",
          "original_form": "자신 있는건 딱히 흑",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100033641.1.10",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100033641.1.11",
          "form": "저 다 좋아해요",
          "original_form": "저 다 좋아해요",
          "speaker_id": "2",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100033641.1.12",
          "form": "떡볶이 마라탕 닭볶음탕 등",
          "original_form": "떡볶이 마라탕 닭볶음탕 등",
          "speaker_id": "2",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100033641.1.13",
          "form": "어떤 요리 잘하세요?",
          "original_form": "어떤 요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100033641.1.14",
          "form": "냄비밥이요",
          "original_form": "냄비밥이요",
          "speaker_id": "1",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100033641.1.15",
          "form": "냄비밥 어렵지 않나요??",
          "original_form": "냄비밥 어렵지 않나요??",
          "speaker_id": "2",
          "time": "20210824 19:48"
        },
        {
          "id": "MDRW2100033641.1.16",
          "form": "여러번 해보면 쉬워요",
          "original_form": "여러번 해보면 쉬워요",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100033641.1.17",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100033641.1.18",
          "form": "네 떡볶이 먹으려다",
          "original_form": "네 떡볶이 먹으려다",
          "speaker_id": "2",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100033641.1.19",
          "form": "치킨 먹었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "치킨 먹었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100033641.1.20",
          "form": "어떤 치킨 드셨나요?",
          "original_form": "어떤 치킨 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100033641.1.21",
          "form": "저 후참이요 !!",
          "original_form": "저 후참이요 !!",
          "speaker_id": "2",
          "time": "20210824 19:56"
        },
        {
          "id": "MDRW2100033641.1.22",
          "form": "근데 맛은 그냥 그래요 ㄹ",
          "original_form": "근데 맛은 그냥 그래요 ㄹ",
