      "utterance": [
        {
          "id": "MDRW2100039134.1.1",
          "form": "걔를 화단에 보내세요",
          "original_form": "걔를 화단에 보내세요",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100039134.1.2",
          "form": "다시 학생으로 돌아가고 싶으세요?",
          "original_form": "다시 학생으로 돌아가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100039134.1.3",
          "form": "오 영어좀 열심히하라고",
          "original_form": "오 영어좀 열심히하라고",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100039134.1.4",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.5",
          "form": "말해주야죠",
          "original_form": "말해주야죠",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.6",
          "form": "아.'.",
          "original_form": "아.'.",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.7",
          "form": "안할꺼같은데",
          "original_form": "안할꺼같은데",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.8",
          "form": "한문장씩 외웟는디",
          "original_form": "한문장씩 외웟는디",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.9",
          "form": "많아져서",
          "original_form": "많아져서",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.10",
          "form": "구몬수학j만 엄청 푸셨던거 같은데",
          "original_form": "구몬수학j만 엄청 푸셨던거 같은데",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.11",
          "form": "안할꺼같네요 제가봐도",
          "original_form": "안할꺼같네요 제가봐도",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.12",
          "form": "것도 같은 페이지..",
          "original_form": "것도 같은 페이지..",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.13",
          "form": "지금은 다 까묵엇소",
          "original_form": "지금은 다 까묵엇소",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100039134.1.14",
          "form": "영어는 어제 배운것도 까먹어서ㅋ",
          "original_form": "영어는 어제 배운것도 까먹어서ㅋ",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039134.1.15",
          "form": "name1님은 잘 아시겠군요 수학",
          "original_form": "&name1&님은 잘 아시겠군요 수학",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039134.1.16",
          "form": "많이 외워야 치매가 안오는데ㅜ",
          "original_form": "많이 외워야 치매가 안오는데ㅜ",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039134.1.17",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039134.1.18",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039134.1.19",
          "form": "고 1연산이 한계에요ㅋ",
          "original_form": "고 1연산이 한계에요ㅋ",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039134.1.20",
          "form": "그것도 대단하신데요",
