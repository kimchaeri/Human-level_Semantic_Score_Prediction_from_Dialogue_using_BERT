      "utterance": [
        {
          "id": "MDRW2100034276.1.1",
          "form": "아니요 저 백수일때",
          "original_form": "아니요 저 백수일때",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100034276.1.2",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100034276.1.3",
          "form": "맨날 누워만 있었더니",
          "original_form": "맨날 누워만 있었더니",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100034276.1.4",
          "form": "저두요 ㅋㅋㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.5",
          "form": "허리도 아프고 엉치도 너무  아픈거에요",
          "original_form": "허리도 아프고 엉치도 너무  아픈거에요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.6",
          "form": "허리공감이요..",
          "original_form": "허리공감이요..",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.7",
          "form": "그래서 도수치료  받으러갔었는데",
          "original_form": "그래서 도수치료  받으러갔었는데",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.8",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.9",
          "form": "근육이 너무 없다고 그러셔서",
          "original_form": "근육이 너무 없다고 그러셔서",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.10",
          "form": "저랑 비슷하시네요 ㅋㅋㅋㅋ",
          "original_form": "저랑 비슷하시네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.11",
          "form": "운동해야된다그러더라구요",
          "original_form": "운동해야된다그러더라구요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.12",
          "form": "아..ㅠㅠ",
          "original_form": "아..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100034276.1.13",
          "form": "근데 진짜 운동하니까 안아파지더라구요",
          "original_form": "근데 진짜 운동하니까 안아파지더라구요",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100034276.1.14",
          "form": "진짜 신기했어요",
          "original_form": "진짜 신기했어요",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100034276.1.15",
          "form": "그래서  피티 받았던건데",
          "original_form": "그래서  피티 받았던건데",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100034276.1.16",
          "form": "와 효과가있나봐요",
          "original_form": "와 효과가있나봐요",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100034276.1.17",
          "form": "근력운동하니까 확실히 몸에 근육 생기는 느낌이 들더라구요",
          "original_form": "근력운동하니까 확실히 몸에 근육 생기는 느낌이 들더라구요",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100034276.1.18",
          "form": "오 저도 근육좀 만들고싶은데",
          "original_form": "오 저도 근육좀 만들고싶은데",
          "speaker_id": "1",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100034276.1.19",
          "form": "어떤 느낌이 드는거죠..?! 저도 느껴보고 싶어요 ㅌㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "어떤 느낌이 드는거죠..?! 저도 느껴보고 싶어요 ㅌㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:39"
        }
