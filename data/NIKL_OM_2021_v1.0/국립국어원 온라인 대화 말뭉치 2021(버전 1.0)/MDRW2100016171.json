      "utterance": [
        {
          "id": "MDRW2100016171.1.1",
          "form": "아침 안먹었나요?",
          "original_form": "아침 안먹었나요?",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100016171.1.2",
          "form": "네 어제 새벽에2시에먹고잣어요..ㅋㅋ",
          "original_form": "네 어제 새벽에2시에먹고잣어요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100016171.1.3",
          "form": "ㅋㅋㅋ뭘드신거죠? 2시에?",
          "original_form": "ㅋㅋㅋ뭘드신거죠? 2시에?",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016171.1.4",
          "form": "그때 밥을먹었음",
          "original_form": "그때 밥을먹었음",
          "speaker_id": "1",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016171.1.5",
          "form": "군것질해서 저녁이 엉터리엿네요 ㅠㅠㅋㅋㅋ",
          "original_form": "군것질해서 저녁이 엉터리엿네요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016171.1.6",
          "form": "결국배는안고픈데 ㅋㅋ2시반에먹음 ㅡㅡ",
          "original_form": "결국배는안고픈데 ㅋㅋ2시반에먹음 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016171.1.7",
          "form": "저런 ㅠㅠ 이래서 밥먹고 군것질 해야하는것이야!!",
          "original_form": "저런 ㅠㅠ 이래서 밥먹고 군것질 해야하는것이야!!",
          "speaker_id": "2",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016171.1.8",
          "form": "그래서 군것질은 뭐했대 ㅋㅋ",
          "original_form": "그래서 군것질은 뭐했대 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016171.1.9",
          "form": "ㅋㅋㅋ그냥 빵먹고 아수쿠림먹고 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그냥 빵먹고 아수쿠림먹고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016171.1.10",
          "form": "요기요가 나이트쿠폰도배신해서 이제 파바안녕이다",
          "original_form": "요기요가 나이트쿠폰도배신해서 이제 파바안녕이다",
          "speaker_id": "1",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016171.1.11",
          "form": "ㅋㅋ아이스크림 얘기하니까 곧 31일이궁",
          "original_form": "ㅋㅋ아이스크림 얘기하니까 곧 31일이궁",
          "speaker_id": "2",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016171.1.12",
          "form": "왜?! 나이트쿠폰 안줘?",
          "original_form": "왜?! 나이트쿠폰 안줘?",
          "speaker_id": "2",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016171.1.13",
          "form": "뭐 그래도 필요할때 할인하면 좋긴하겟지만",
          "original_form": "뭐 그래도 필요할때 할인하면 좋긴하겟지만",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016171.1.14",
          "form": "15처너으로오룸 ㅋㅋㅋ구래서 파바 0원결제안되",
          "original_form": "15처너으로오룸 ㅋㅋㅋ구래서 파바 0원결제안되",
