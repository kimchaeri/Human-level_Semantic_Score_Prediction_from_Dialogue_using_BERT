      "utterance": [
        {
          "id": "MDRW2100036695.1.1",
          "form": "네 그런것 같아요 첫 펄은 거기서 경험했는데 ㅎㅎ",
          "original_form": "네 그런것 같아요 첫 펄은 거기서 경험했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100036695.1.2",
          "form": "식빵을 두장이나 먹어도 ..성이 안차네요 ㅎㅎ 하아ㅠㅠ",
          "original_form": "식빵을 두장이나 먹어도 ..성이 안차네요 ㅎㅎ 하아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100036695.1.3",
          "form": "아이고ㅎ",
          "original_form": "아이고ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:20"
        },
        {
          "id": "MDRW2100036695.1.4",
          "form": "아침은 황제같이!",
          "original_form": "아침은 황제같이!",
          "speaker_id": "2",
          "time": "20210831 09:20"
        },
        {
          "id": "MDRW2100036695.1.5",
          "form": "세장 드시는 겁니다",
          "original_form": "세장 드시는 겁니다",
          "speaker_id": "2",
          "time": "20210831 09:20"
        },
        {
          "id": "MDRW2100036695.1.6",
          "form": "딱 3장 있는데 ㅎㅎ 저한장까지 ㅎㅎ",
          "original_form": "딱 3장 있는데 ㅎㅎ 저한장까지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:20"
        },
        {
          "id": "MDRW2100036695.1.7",
          "form": "ㅎㅎ 커피는 다 드셨나요",
          "original_form": "ㅎㅎ 커피는 다 드셨나요",
          "speaker_id": "2",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036695.1.8",
          "form": "커피 쪼오금 남았어요 이제 그한장과 마무으리",
          "original_form": "커피 쪼오금 남았어요 이제 그한장과 마무으리",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036695.1.9",
          "form": "ㅎ 커피가 남았으니 고고!",
          "original_form": "ㅎ 커피가 남았으니 고고!",
          "speaker_id": "2",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036695.1.10",
          "form": "바닐라라떼와 조합은 좋은가요",
          "original_form": "바닐라라떼와 조합은 좋은가요",
          "speaker_id": "2",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036695.1.11",
          "form": "네 좋아요 ㅎㅎ 원래는 매일 아메마시는데",
          "original_form": "네 좋아요 ㅎㅎ 원래는 매일 아메마시는데",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036695.1.12",
          "form": "아침에는",
          "original_form": "아침에는",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036695.1.13",
          "form": "비오고 추우면 라떼도 좋더라구요 ㅎㅎ",
          "original_form": "비오고 추우면 라떼도 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036695.1.14",
          "form": "ㅎㅎ 부드러운 우유가 생각날 때니까요",
          "original_form": "ㅎㅎ 부드러운 우유가 생각날 때니까요",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036695.1.15",
          "form": "회사 근처 검색하니 일리터가 있기는한데 흑당밀크티를 안 파네요 왜죠",
          "original_form": "회사 근처 검색하니 일리터가 있기는한데 흑당밀크티를 안 파네요 왜죠",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036695.1.16",
          "form": "그럴리가요!",
          "original_form": "그럴리가요!",
          "speaker_id": "1",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036695.1.17",
          "form": "회사다니시면 점심은 어디서 드세요?? 구내식당??",
          "original_form": "회사다니시면 점심은 어디서 드세요?? 구내식당??",
          "speaker_id": "1",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036695.1.18",
          "form": "메뉴판이 업뎃 안된거 아닐까요ㅠㅠㅠ",
          "original_form": "메뉴판이 업뎃 안된거 아닐까요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036695.1.19",
          "form": "저라면 전화를 해보겠어요 ㅎㅎ",
          "original_form": "저라면 전화를 해보겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100036695.1.20",
          "form": "지점마다 메뉴가 좀 다른가봐요ㅜ",
          "original_form": "지점마다 메뉴가 좀 다른가봐요ㅜ",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100036695.1.21",
          "form": "점심은 밖에서 사 먹어요",
          "original_form": "점심은 밖에서 사 먹어요",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100036695.1.22",
          "form": "지역 상생 웅 하며 안 만들어주더라고요ㅜ",
          "original_form": "지역 상생 {censored}웅 하며 안 만들어주더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100036695.1.23",
          "form": "이런~ 맙소사 직원복지는요ㅠ",
          "original_form": "이런~ 맙소사 직원복지는요ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:30"
        },
        {
          "id": "MDRW2100036695.1.24",
          "form": "ㅎㅎ 그리고 건물 다른층은 임대장사하는 거 보면 정이 뚝 떨어지네요",
          "original_form": "ㅎㅎ 그리고 건물 다른층은 임대장사하는 거 보면 정이 뚝 떨어지네요",
          "speaker_id": "2",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100036695.1.25",
          "form": "임대료..때문인거같네요",
          "original_form": "임대료..때문인거같네요",
          "speaker_id": "1",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100036695.1.26",
          "form": "식비 자부담이져?",
          "original_form": "식비 자부담이져?",
          "speaker_id": "1",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100036695.1.27",
          "form": "식대 십만원 나오는데",
          "original_form": "식대 십만원 나오는데",
          "speaker_id": "2",
          "time": "20210831 09:32"
        },
        {
          "id": "MDRW2100036695.1.28",
          "form": "누구코에 붙이란 건지 모르겠어요ㅜ",
          "original_form": "누구코에 붙이란 건지 모르겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 09:32"
        },
        {
          "id": "MDRW2100036695.1.29",
          "form": "오천원짜리 집이 어디있다고",
          "original_form": "오천원짜리 집이 어디있다고",
          "speaker_id": "2",
          "time": "20210831 09:32"
        },
        {
          "id": "MDRW2100036695.1.30",
          "form": "창사 이래 오른적이 없대요ㅎㅎ",
          "original_form": "창사 이래 오른적이 없대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:32"
        }
      ]
