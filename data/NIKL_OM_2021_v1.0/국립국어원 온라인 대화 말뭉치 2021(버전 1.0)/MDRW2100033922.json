      "utterance": [
        {
          "id": "MDRW2100033922.1.1",
          "form": "칼국수vs수제비는 어떠세요?",
          "original_form": "칼국수vs수제비는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100033922.1.2",
          "form": "어머 둘다좋아하는데.. 음..... 칼국수영!",
          "original_form": "어머 둘다좋아하는데.. 음..... 칼국수영!",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033922.1.3",
          "form": "짜장vs짬뽕!",
          "original_form": "짜장vs짬뽕!",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033922.1.4",
          "form": "짬뽕이죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "짬뽕이죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033922.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033922.1.6",
          "form": "짜장요ㅋㅋ",
          "original_form": "짜장요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100033922.1.7",
          "form": "좋아하는 음식이 뭐에요?",
          "original_form": "좋아하는 음식이 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100033922.1.8",
          "form": "초밥이요!!!!",
          "original_form": "초밥이요!!!!",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100033922.1.9",
          "form": "오저두요! 어떤게 올라간거 좋아하세요?,",
          "original_form": "오저두요! 어떤게 올라간거 좋아하세요?,",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100033922.1.10",
          "form": "전 다좋아해요ㅋㅋㅋ",
          "original_form": "전 다좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100033922.1.11",
          "form": "진짜로요",
          "original_form": "진짜로요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100033922.1.12",
          "form": "가리는거 없으시군요 이야기하니까 초밥먹고싶네요",
          "original_form": "가리는거 없으시군요 이야기하니까 초밥먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 08:29"
        },
        {
          "id": "MDRW2100033922.1.13",
          "form": "ㅋㅋㅋㅋ초밥이",
          "original_form": "ㅋㅋㅋㅋ초밥이",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100033922.1.14",
          "form": "너무 맛있긴하죠!",
          "original_form": "너무 맛있긴하죠!",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100033922.1.15",
          "form": "네 배부르게 먹기엔 좀비싸서... 특별한날 시켜먹게되요ㅋ",
          "original_form": "네 배부르게 먹기엔 좀비싸서... 특별한날 시켜먹게되요ㅋ",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100033922.1.16",
          "form": "비싸긴해요 회가ㅠㅠ",
          "original_form": "비싸긴해요 회가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:01"
