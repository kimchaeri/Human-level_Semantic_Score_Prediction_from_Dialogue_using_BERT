      "utterance": [
        {
          "id": "MDRW2100039673.1.1",
          "form": "목소리가 그렇데",
          "original_form": "목소리가 그렇데",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039673.1.2",
          "form": "밥 1끼먹었단다ㅜㅜ",
          "original_form": "밥 1끼먹었단다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039673.1.3",
          "form": "참...우울하네",
          "original_form": "참...우울하네",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039673.1.4",
          "form": "응 통화하니 우울하다ㅜ",
          "original_form": "응 통화하니 우울하다ㅜ",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039673.1.5",
          "form": "어제답이지워졌네 ㅜ",
          "original_form": "어제답이지워졌네 ㅜ",
          "speaker_id": "2",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100039673.1.6",
          "form": "나 모더나 접종왔다",
          "original_form": "나 모더나 접종왔다",
          "speaker_id": "2",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100039673.1.7",
          "form": "사람 많나???",
          "original_form": "사람 많나???",
          "speaker_id": "1",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100039673.1.8",
          "form": "엄청많네",
          "original_form": "엄청많네",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100039673.1.9",
          "form": "겁나네",
          "original_form": "겁나네",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100039673.1.10",
          "form": "맞고 30분 앉아있어라",
          "original_form": "맞고 30분 앉아있어라",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100039673.1.11",
          "form": "어지러우면 바로 얘기하고",
          "original_form": "어지러우면 바로 얘기하고",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100039673.1.12",
          "form": "ㅇㅇ바로가라고 안한다데",
          "original_form": "ㅇㅇ바로가라고 안한다데",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100039673.1.13",
          "form": "갈때 김밥포장해가야게따",
          "original_form": "갈때 김밥포장해가야게따",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100039673.1.14",
          "form": "응 난 어지럽더라",
          "original_form": "응 난 어지럽더라",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100039673.1.15",
          "form": "한 일주일 조심해야한다",
          "original_form": "한 일주일 조심해야한다",
          "speaker_id": "1",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100039673.1.16",
          "form": "아 ㅜ 그래\r\n난그래도 집에있으니",
          "original_form": "아 ㅜ 그래\r\n난그래도 집에있으니",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100039673.1.17",
          "form": "난 계속 누워있었다",
          "original_form": "난 계속 누워있었다",
          "speaker_id": "1",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100039673.1.18",
          "form": "그래 월욜이 예약인데",
          "original_form": "그래 월욜이 예약인데",
          "speaker_id": "2",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100039673.1.19",
          "form": "주말이나을거같고...모더나일지아닐지도 모르고",
          "original_form": "주말이나을거같고...모더나일지아닐지도 모르고",
          "speaker_id": "2",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100039673.1.20",
          "form": "주말에 쉬는게 낫지",
          "original_form": "주말에 쉬는게 낫지",
          "speaker_id": "1",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100039673.1.21",
          "form": "맞았어 금방 정말잠깐맞네 약이적네",
          "original_form": "맞았어 금방 정말잠깐맞네 약이적네",
          "speaker_id": "2",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100039673.1.22",
          "form": "응 괜찮나?",
          "original_form": "응 괜찮나?",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100039673.1.23",
          "form": "팔이스물아파오네",
          "original_form": "팔이스물아파오네",
          "speaker_id": "2",
          "time": "20210904 11:33"
        }
      ]
