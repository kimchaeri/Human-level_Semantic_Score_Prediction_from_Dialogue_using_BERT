      "utterance": [
        {
          "id": "MDRW2100031532.1.1",
          "form": "가장 좋아하는 집안일 있으세요?",
          "original_form": "가장 좋아하는 집안일 있으세요?",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100031532.1.2",
          "form": "좋아하는 건 없지만 스팀 청소기로 바닥이 깨끗해지는 걸 좋아합니다.",
          "original_form": "좋아하는 건 없지만 스팀 청소기로 바닥이 깨끗해지는 걸 좋아합니다.",
          "speaker_id": "2",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100031532.1.3",
          "form": "화장실 청소도 결과물에 기분이 좋아지는 것 같아요.",
          "original_form": "화장실 청소도 결과물에 기분이 좋아지는 것 같아요.",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031532.1.4",
          "form": "저도 샤워하면서 화장실 청소할 때 물 때 벗기는 기 기분이 좋더라고요",
          "original_form": "저도 샤워하면서 화장실 청소할 때 물 때 벗기는 기 기분이 좋더라고요",
          "speaker_id": "1",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100031532.1.5",
          "form": "ㅎㅎ 맞아요. 물 때 사라지는 거 좋죠.",
          "original_form": "ㅎㅎ 맞아요. 물 때 사라지는 거 좋죠.",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100031532.1.6",
          "form": "마르면 반짝반짝하고",
          "original_form": "마르면 반짝반짝하고",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100031532.1.7",
          "form": "거울도 유리밀대로 싹싹 밀거든요",
          "original_form": "거울도 유리밀대로 싹싹 밀거든요",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100031532.1.8",
          "form": "정말 깨끗하시네요!",
          "original_form": "정말 깨끗하시네요!",
          "speaker_id": "1",
          "time": "20210905 12:55"
        },
        {
          "id": "MDRW2100031532.1.9",
          "form": "싫어하는 집안일은요?",
          "original_form": "싫어하는 집안일은요?",
          "speaker_id": "1",
          "time": "20210905 12:55"
        },
        {
          "id": "MDRW2100031532.1.10",
          "form": "진공청소기로 매일 미는 거요 ㅜ ㅎㅎ",
          "original_form": "진공청소기로 매일 미는 거요 ㅜ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100031532.1.11",
          "form": "저는 설거지가 제일 싫습니다...",
          "original_form": "저는 설거지가 제일 싫습니다...",
          "speaker_id": "1",
          "time": "20210906 10:36"
        },
        {
          "id": "MDRW2100031532.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 진리죠 으",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 진리죠 으",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100031532.1.13",
          "form": "그래서 동생이랑 번갈아가면서 하고 있어요",
          "original_form": "그래서 동생이랑 번갈아가면서 하고 있어요",
          "speaker_id": "1",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100031532.1.14",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "1",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100031532.1.15",
          "form": "그릇이 깨끗해지는 건 좋은데 .. 정말 인간인 게 번거롭다는 걸 체감하는 작업인 것 같아요.",
          "original_form": "그릇이 깨끗해지는 건 좋은데 .. 정말 인간인 게 번거롭다는 걸 체감하는 작업인 것 같아요.",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100031532.1.16",
          "form": "아뇨, 가족(부모님)이랑 사는데",
          "original_form": "아뇨, 가족(부모님)이랑 사는데",
          "speaker_id": "2",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100031532.1.17",
          "form": "주거공간이 분리되다시피해서",
          "original_form": "주거공간이 분리되다시피해서",
          "speaker_id": "2",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100031532.1.18",
          "form": "제 구역은 제가 다 관리하거든요.",
          "original_form": "제 구역은 제가 다 관리하거든요.",
          "speaker_id": "2",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100031532.1.19",
          "form": "우와 구역도 나뉘어져있다니 신기해요",
          "original_form": "우와 구역도 나뉘어져있다니 신기해요",
          "speaker_id": "1",
          "time": "20210906 10:40"
        },
        {
          "id": "MDRW2100031532.1.20",
          "form": "요리도 하시나요?",
          "original_form": "요리도 하시나요?",
          "speaker_id": "1",
          "time": "20210906 10:40"
        },
        {
          "id": "MDRW2100031532.1.21",
          "form": "집 구조가 좀 특이해서 거실과 주방만 공유하는 셈이예요",
          "original_form": "집 구조가 좀 특이해서 거실과 주방만 공유하는 셈이예요",
          "speaker_id": "2",
          "time": "20210906 10:40"
        },
        {
          "id": "MDRW2100031532.1.22",
          "form": "간단한 요리는 해요 ㅎㅎ",
          "original_form": "간단한 요리는 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 10:40"
        }
      ]
    }
