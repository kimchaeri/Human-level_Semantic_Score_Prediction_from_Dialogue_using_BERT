      "utterance": [
        {
          "id": "MDRW2100038191.1.1",
          "form": "일일드라마 저녁은 역시 kbs지",
          "original_form": "일일드라마 저녁은 역시 kbs지",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038191.1.2",
          "form": "그래 계속 시청률 좋음",
          "original_form": "그래 계속 시청률 좋음",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038191.1.3",
          "form": "엉 지금도 17%대 빨강구두랑 비슷하네",
          "original_form": "엉 지금도 17%대 빨강구두랑 비슷하네",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100038191.1.4",
          "form": "엠비씨가 분발했네",
          "original_form": "엠비씨가 분발했네",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100038191.1.5",
          "form": "소이현이 나와서 재미잇는듯 거기다 최명길이니",
          "original_form": "소이현이 나와서 재미잇는듯 거기다 최명길이니",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100038191.1.6",
          "form": "그렇겠다 드라마는 뭐야",
          "original_form": "그렇겠다 드라마는 뭐야",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100038191.1.7",
          "form": "속아도 꿈결",
          "original_form": "속아도 꿈결",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038191.1.8",
          "form": "제목이 참 ~ 배우는",
          "original_form": "제목이 참 ~ 배우는",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038191.1.9",
          "form": "이게 부모의 황혼 재혼으로 만나 하나의 가족이 되어가는 이야기래",
          "original_form": "이게 부모의 황혼 재혼으로 만나 하나의 가족이 되어가는 이야기래",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038191.1.10",
          "form": "내용은 교훈적이다",
          "original_form": "내용은 교훈적이다",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038191.1.11",
          "form": "엉 kbs1저녁 드라마는 그렇게 막장은 아니더라",
          "original_form": "엉 kbs1저녁 드라마는 그렇게 막장은 아니더라",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038191.1.12",
          "form": "엉 맞어 좀 그런듯",
          "original_form": "엉 맞어 좀 그런듯",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038191.1.13",
          "form": "어제 종영방송 했네",
          "original_form": "어제 종영방송 했네",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038191.1.14",
          "form": "ㅋㅋ웃긴다",
          "original_form": "ㅋㅋ웃긴다",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038191.1.15",
          "form": "최정우 이분이랑 박준금이 재혼인갑다",
          "original_form": "최정우 이분이랑 박준금이 재혼인갑다",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038191.1.16",
          "form": "괜찮겠다",
          "original_form": "괜찮겠다",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038191.1.17",
          "form": "자녀들에 류진 박탐희 임형준 윤해영 왕지혜  함은정",
          "original_form": "자녀들에 류진 박탐희 임형준 윤해영 왕지혜  함은정",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038191.1.18",
          "form": "다 괜찮네",
          "original_form": "다 괜찮네",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038191.1.19",
          "form": "그러게 한때 다 주조연했던 사람들이 나오네",
          "original_form": "그러게 한때 다 주조연했던 사람들이 나오네",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038191.1.20",
          "form": "박군은 덥다고 빙수 만드네",
