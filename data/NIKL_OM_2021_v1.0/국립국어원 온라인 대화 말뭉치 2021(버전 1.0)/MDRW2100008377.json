        {
          "id": "MDRW2100008377.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008377.1.2",
          "form": "예전에 개 몇번 키웠었어요",
          "original_form": "예전에 개 몇번 키웠었어요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008377.1.3",
          "form": "근데 헤어질 때 너무 괴롭더라고요",
          "original_form": "근데 헤어질 때 너무 괴롭더라고요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008377.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008377.1.5",
          "form": "슬프죠",
          "original_form": "슬프죠",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008377.1.6",
          "form": "name1님도 동물 키워본 적 있어요?",
          "original_form": "&name1&님도 동물 키워본 적 있어요?",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008377.1.7",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008377.1.8",
          "form": "키워보고 싶은 동물 있어요?",
          "original_form": "키워보고 싶은 동물 있어요?",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008377.1.9",
          "form": "강아지여",
          "original_form": "강아지여",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008377.1.10",
          "form": "역시 강아지가 제일 귀엽고 친근한 것같아요",
          "original_form": "역시 강아지가 제일 귀엽고 친근한 것같아요",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008377.1.11",
          "form": "잘 따르죠",
          "original_form": "잘 따르죠",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008377.1.12",
          "form": "털 알러지같은거 있으면 키우기 어려운데 없어서 다행이에요",
          "original_form": "털 알러지같은거 있으면 키우기 어려운데 없어서 다행이에요",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008377.1.13",
          "form": "고양이는 털때문에 안되겠도라구요",
          "original_form": "고양이는 털때문에 안되겠도라구요",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008377.1.14",
          "form": "아 고양이가 털이 많이 빠지나요?",
          "original_form": "아 고양이가 털이 많이 빠지나요?",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008377.1.15",
          "form": "네 돌돌이 필수요",
