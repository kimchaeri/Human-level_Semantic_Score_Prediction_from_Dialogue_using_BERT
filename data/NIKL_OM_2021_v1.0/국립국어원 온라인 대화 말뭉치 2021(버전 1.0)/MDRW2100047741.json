      "utterance": [
        {
          "id": "MDRW2100047741.1.1",
          "form": "맞아요 그래서 집에 책이 쌓였는데 팔은건 십분의 일도 안돼요",
          "original_form": "맞아요 그래서 집에 책이 쌓였는데 팔은건 십분의 일도 안돼요",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100047741.1.2",
          "form": "책은 정말 제값받기힘든거 같아요",
          "original_form": "책은 정말 제값받기힘든거 같아요",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100047741.1.3",
          "form": "음반도요!",
          "original_form": "음반도요!",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100047741.1.4",
          "form": "그러고보면 책은 삼사천원으로 팔기도 했네요",
          "original_form": "그러고보면 책은 삼사천원으로 팔기도 했네요",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100047741.1.5",
          "form": "사신 중고물품은 뭐가 있나요?",
          "original_form": "사신 중고물품은 뭐가 있나요?",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100047741.1.6",
          "form": "학교 교재나 그림 관련  책이요",
          "original_form": "학교 교재나 그림 관련  책이요",
          "speaker_id": "2",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100047741.1.7",
          "form": "거의 책이네요 ㅎㅎ",
          "original_form": "거의 책이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100047741.1.8",
          "form": "학교교재...다 쓰지도 않으면서 비싸죠...",
          "original_form": "학교교재...다 쓰지도 않으면서 비싸죠...",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100047741.1.9",
          "form": "책 주로 사셨구나",
          "original_form": "책 주로 사셨구나",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100047741.1.10",
          "form": "당근이용하셨나요?",
          "original_form": "당근이용하셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100047741.1.11",
          "form": "교재 수업때만 쓰는데 제값주고 사기엔 아까워요",
          "original_form": "교재 수업때만 쓰는데 제값주고 사기엔 아까워요",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100047741.1.12",
          "form": "중고나라랑 당근 써요",
          "original_form": "중고나라랑 당근 써요",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100047741.1.13",
          "form": "그죠... 저는 선배들 책 받아쓰거나 학교내에서 중고거래했었어요",
          "original_form": "그죠... 저는 선배들 책 받아쓰거나 학교내에서 중고거래했었어요",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100047741.1.14",
          "form": "아 그러고보니 에타로도 중고거래 되죠?",
          "original_form": "아 그러고보니 에타로도 중고거래 되죠?",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100047741.1.15",
          "form": "저희땐 에타가 없었어가지구...그건 잘모르겠어요",
          "original_form": "저희땐 에타가 없었어가지구...그건 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100047741.1.16",
          "form": "저흰 그냥 학교사이트에서 거래약속잡았어요",
          "original_form": "저흰 그냥 학교사이트에서 거래약속잡았어요",
          "speaker_id": "1",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100047741.1.17",
          "form": "아 그래요?? 학교 사이트에서 교재 거래하기도 했군요",
          "original_form": "아 그래요?? 학교 사이트에서 교재 거래하기도 했군요",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100047741.1.18",
          "form": "신기하네요 ㅎㅎ",
          "original_form": "신기하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100047741.1.19",
          "form": "학교어플이 따로있기도했어요",
          "original_form": "학교어플이 따로있기도했어요",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100047741.1.20",
          "form": "에타는 중고거래 게시판이 따로있나요?",
          "original_form": "에타는 중고거래 게시판이 따로있나요?",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100047741.1.21",
          "form": "네! 에타에 거래게시판이 따로 있어요",
          "original_form": "네! 에타에 거래게시판이 따로 있어요",
          "speaker_id": "2",
          "time": "20210827 13:48"
        }
      ]
