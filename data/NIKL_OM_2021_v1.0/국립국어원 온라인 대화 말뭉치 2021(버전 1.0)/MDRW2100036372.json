      "utterance": [
        {
          "id": "MDRW2100036372.1.1",
          "form": "네넵 !! 케익도 매번 끌리게 달라요 ㅋㅋ",
          "original_form": "네넵 !! 케익도 매번 끌리게 달라요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036372.1.2",
          "form": "케익 좋죠",
          "original_form": "케익 좋죠",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036372.1.3",
          "form": "투썸에서 케이크 뭐 드세요?",
          "original_form": "투썸에서 케이크 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036372.1.4",
          "form": "초코먹울때고잇고",
          "original_form": "초코먹울때고잇고",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.5",
          "form": "저는 생크림딸기초코? 초코딸기? 이거 원픽이에요",
          "original_form": "저는 생크림딸기초코? 초코딸기? 이거 원픽이에요",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.6",
          "form": "아님 슈먹을때도잇고 !",
          "original_form": "아님 슈먹을때도잇고 !",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.7",
          "form": "티라미스먹을때도잇고",
          "original_form": "티라미스먹을때도잇고",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.8",
          "form": "헐 슈도 팔아요?",
          "original_form": "헐 슈도 팔아요?",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.9",
          "form": "저희 동네 투썸엔 안팔아요ㅠㅠㅠ",
          "original_form": "저희 동네 투썸엔 안팔아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.10",
          "form": "왕슈잇어요 ㅋㅋ안에 크림가득 ㅋㅋㅋㅋ",
          "original_form": "왕슈잇어요 ㅋㅋ안에 크림가득 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.11",
          "form": "헐.... 아아랑 딱이다",
          "original_form": "헐.... 아아랑 딱이다",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.12",
          "form": "막상 스벅가면 음료만 마시는데 투썸은 꼭 케익먹어요 ㅋㅋㅋ",
          "original_form": "막상 스벅가면 음료만 마시는데 투썸은 꼭 케익먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036372.1.13",
          "form": "나중에 슈 파는 지점가면 아아에 먹어야겠어요",
          "original_form": "나중에 슈 파는 지점가면 아아에 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.14",
          "form": "투썸은 케익 맛집ㅋㅋㅌ",
          "original_form": "투썸은 케익 맛집ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.15",
          "form": "커피는",
          "original_form": "커피는",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.16",
          "form": "아아파세요!?",
          "original_form": "아아파세요!?",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.17",
          "form": "아주 가아끔",
          "original_form": "아주 가아끔",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.18",
          "form": "디카페인으로 마셔용",
          "original_form": "디카페인으로 마셔용",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.19",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.20",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036372.1.21",
          "form": "전 커피 러버에요 ㅠㅠㅋㅋㅋ",
          "original_form": "전 커피 러버에요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
