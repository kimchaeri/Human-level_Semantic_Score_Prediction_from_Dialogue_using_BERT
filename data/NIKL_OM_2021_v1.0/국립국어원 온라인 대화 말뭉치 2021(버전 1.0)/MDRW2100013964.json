        {
          "id": "MDRW2100013964.1.2",
          "form": "여행가자",
          "original_form": "여행가자",
          "speaker_id": "1",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100013964.1.3",
          "form": "어디로?",
          "original_form": "어디로?",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100013964.1.4",
          "form": "갈수있어.?",
          "original_form": "갈수있어.?",
          "speaker_id": "1",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100013964.1.5",
          "form": "들어보고",
          "original_form": "들어보고",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100013964.1.6",
          "form": "제주도가자",
          "original_form": "제주도가자",
          "speaker_id": "1",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100013964.1.7",
          "form": "뜬금없이?",
          "original_form": "뜬금없이?",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100013964.1.8",
          "form": "특가떴단 말이야",
          "original_form": "특가떴단 말이야",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100013964.1.9",
          "form": "혹하긴 하네",
          "original_form": "혹하긴 하네",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100013964.1.10",
          "form": "너 어차피 다음주 한가하잖아",
          "original_form": "너 어차피 다음주 한가하잖아",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100013964.1.11",
          "form": "그렇긴하지만",
          "original_form": "그렇긴하지만",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100013964.1.12",
          "form": "렌트는 내가할게",
          "original_form": "렌트는 내가할게",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100013964.1.13",
          "form": "비행기표부터 알아봐야지",
          "original_form": "비행기표부터 알아봐야지",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100013964.1.14",
