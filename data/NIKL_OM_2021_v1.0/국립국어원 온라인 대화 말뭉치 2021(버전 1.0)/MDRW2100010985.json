      "utterance": [
        {
          "id": "MDRW2100010985.1.1",
          "form": "저는 베트남이요",
          "original_form": "저는 베트남이요",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010985.1.2",
          "form": "베트남이랑 대만이랑 좀 비슷한 기후죠?",
          "original_form": "베트남이랑 대만이랑 좀 비슷한 기후죠?",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010985.1.3",
          "form": "음.. 둘다 후덥지근하죠",
          "original_form": "음.. 둘다 후덥지근하죠",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010985.1.4",
          "form": "저번에 대만 가봤는데 바퀴벌레가 진짜 많았어요",
          "original_form": "저번에 대만 가봤는데 바퀴벌레가 진짜 많았어요",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010985.1.5",
          "form": "으... 괴로운여행이셨겠어요",
          "original_form": "으... 괴로운여행이셨겠어요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010985.1.6",
          "form": "그거만 빼면 먹거리도 맛있고 재밌었어요",
          "original_form": "그거만 빼면 먹거리도 맛있고 재밌었어요",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010985.1.7",
          "form": "대만에서 뭐가 제일 맛있었어요?",
          "original_form": "대만에서 뭐가 제일 맛있었어요?",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010985.1.8",
          "form": "맥도날드...?",
          "original_form": "맥도날드...?",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010985.1.9",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010985.1.10",
          "form": "소시지류가 맛있었어요",
          "original_form": "소시지류가 맛있었어요",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010985.1.11",
          "form": "만두랑 샤오룽바오",
          "original_form": "만두랑 샤오룽바오",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010985.1.12",
          "form": "샤오룽바오 너무 맛있겠다...",
          "original_form": "샤오룽바오 너무 맛있겠다...",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010985.1.13",
          "form": "대만 샤오룽바오 진짜 맛있어요",
          "original_form": "대만 샤오룽바오 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010985.1.14",
          "form": "여행 가고싶은데 있어요?",
          "original_form": "여행 가고싶은데 있어요?",
          "speaker_id": "1",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010985.1.15",
          "form": "저는 일본에 가고 싶어요",
          "original_form": "저는 일본에 가고 싶어요",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010985.1.16",
          "form": "일본 어디요?",
          "original_form": "일본 어디요?",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010985.1.17",
          "form": "오키나와요 가보셨나요",
          "original_form": "오키나와요 가보셨나요",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010985.1.18",
          "form": "저 오키나와여행 자주 갔었어요",
          "original_form": "저 오키나와여행 자주 갔었어요",
          "speaker_id": "1",
          "time": "20210823 23:35"
        }
