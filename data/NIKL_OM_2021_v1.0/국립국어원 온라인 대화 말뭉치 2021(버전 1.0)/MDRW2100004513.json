      "utterance": [
        {
          "id": "MDRW2100004513.1.1",
          "form": "지금 비오나요?",
          "original_form": "지금 비오나요?",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004513.1.2",
          "form": "여긴 안오다 다시 비오네요",
          "original_form": "여긴 안오다 다시 비오네요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004513.1.3",
          "form": "여긴 아까오더니 지금은 또 안오네요",
          "original_form": "여긴 아까오더니 지금은 또 안오네요",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004513.1.4",
          "form": "일본에 천미리 왔다고 하던데 그비인가봐요",
          "original_form": "일본에 천미리 왔다고 하던데 그비인가봐요",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004513.1.5",
          "form": "헐~~~ 일본은 난리네요",
          "original_form": "헐~~~ 일본은 난리네요",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004513.1.6",
          "form": "뉴스대강봤는데 태풍온다는거같기도하고요",
          "original_form": "뉴스대강봤는데 태풍온다는거같기도하고요",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004513.1.7",
          "form": "담주까진 비 많이 오려나봐요",
          "original_form": "담주까진 비 많이 오려나봐요",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004513.1.8",
          "form": "네 지반도 약해져서 지진에 더 취약해졓다고 하더라구요",
          "original_form": "네 지반도 약해져서 지진에 더 취약해졓다고 하더라구요",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004513.1.9",
          "form": "늦은 장마래요",
          "original_form": "늦은 장마래요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004513.1.10",
          "form": "그래서 담주내내 비소식인거같아요",
          "original_form": "그래서 담주내내 비소식인거같아요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004513.1.11",
          "form": "피해없이 지나갔음 좋겠어요",
          "original_form": "피해없이 지나갔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004513.1.12",
          "form": "안그래도 코로나때매 힘든데...",
          "original_form": "안그래도 코로나때매 힘든데...",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004513.1.13",
          "form": "본사에서 장마대비하라고 공문도 오더라구요",
          "original_form": "본사에서 장마대비하라고 공문도 오더라구요",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004513.1.14",
          "form": "공문올정도면 진짜 많이 오려나보네요",
          "original_form": "공문올정도면 진짜 많이 오려나보네요",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004513.1.15",
          "form": "일본이랑 중국은 난리래요",
          "original_form": "일본이랑 중국은 난리래요",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004513.1.16",
          "form": "비와서 시원할줄알았는데 습해서 자꾸 에어컨을 켜게되네요",
          "original_form": "비와서 시원할줄알았는데 습해서 자꾸 에어컨을 켜게되네요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004513.1.17",
          "form": "에고~~~ 살기힘든데 비까지 난리네요",
          "original_form": "에고~~~ 살기힘든데 비까지 난리네요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004513.1.18",
          "form": "아깐 바람 시원하게불더니 지금은 끈적거리네요",
          "original_form": "아깐 바람 시원하게불더니 지금은 끈적거리네요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004513.1.19",
          "form": "정말 재앙이 오려는지 이것저것 다오는거같아요",
          "original_form": "정말 재앙이 오려는지 이것저것 다오는거같아요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004513.1.20",
          "form": "그러게요.",
          "original_form": "그러게요.",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004513.1.21",
          "form": "곧 옷정리도 해야겠네요 너무 귀찮아요 계절바뀔때마다",
          "original_form": "곧 옷정리도 해야겠네요 너무 귀찮아요 계절바뀔때마다",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004513.1.22",
          "form": "오늘은 날씨 너무좋네요",
          "original_form": "오늘은 날씨 너무좋네요",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100004513.1.23",
          "form": "선선하니 좋아요",
          "original_form": "선선하니 좋아요",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100004513.1.24",
          "form": "해가나오긴했는데 그렇게 덥진않은거같아요",
          "original_form": "해가나오긴했는데 그렇게 덥진않은거같아요",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100004513.1.25",
          "form": "오늘 하늘 보셨었나요? 진짜 가을인거같아요",
          "original_form": "오늘 하늘 보셨었나요? 진짜 가을인거같아요",
          "speaker_id": "2",
          "time": "20210905 20:52"
        }
      ]
    }
