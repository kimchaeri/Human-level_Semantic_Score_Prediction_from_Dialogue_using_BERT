      "utterance": [
        {
          "id": "MDRW2100024939.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024939.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024939.1.3",
          "form": "최근에 쇼핑하신거 있으신가여",
          "original_form": "최근에 쇼핑하신거 있으신가여",
          "speaker_id": "1",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024939.1.4",
          "form": "음 저는 원두 구매했어요!",
          "original_form": "음 저는 원두 구매했어요!",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024939.1.5",
          "form": "오 우와",
          "original_form": "오 우와",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024939.1.6",
          "form": "커피 잘 아시나봐야",
          "original_form": "커피 잘 아시나봐야",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024939.1.7",
          "form": "멋있당",
          "original_form": "멋있당",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024939.1.8",
          "form": "ㅋㅋㅋㅋㅋ커피좋아해요!",
          "original_form": "ㅋㅋㅋㅋㅋ커피좋아해요!",
          "speaker_id": "2",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100024939.1.9",
          "form": "name1님은 뭐 사셧어요?",
          "original_form": "&name1&님은 뭐 사셧어요?",
          "speaker_id": "2",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100024939.1.10",
          "form": "저는 아까 집 오면서 장봤어요 ㅋㅋ",
          "original_form": "저는 아까 집 오면서 장봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024939.1.11",
          "form": "건강하게 먹을라고 과자를 생략했지만...",
          "original_form": "건강하게 먹을라고 과자를 생략했지만...",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024939.1.12",
          "form": "아른거려요",
          "original_form": "아른거려요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024939.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 저두 과자 좋아하는데...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 저두 과자 좋아하는데...",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024939.1.14",
          "form": "살찔까봐 잘 안사요 ㅠㅠ",
          "original_form": "살찔까봐 잘 안사요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024939.1.15",
          "form": "저는 최근에 라면을 10일정도 먹었더니",
          "original_form": "저는 최근에 라면을 10일정도 먹었더니",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024939.1.16",
          "form": "배가 금새 나오더라구여?",
          "original_form": "배가 금새 나오더라구여?",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024939.1.17",
          "form": "깜짝 놀라서 이번에는 라면도 안샀어요",
          "original_form": "깜짝 놀라서 이번에는 라면도 안샀어요",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024939.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ라면 살 많이쪄요ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ라면 살 많이쪄요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024939.1.19",
          "form": "어쩌다가 그렇게 많이드신가에요 ㅋㅋㅋㅋ",
          "original_form": "어쩌다가 그렇게 많이드신가에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024939.1.20",
          "form": "야근하고 그러니깐 먹게 되더라구요... 도르륵",
          "original_form": "야근하고 그러니깐 먹게 되더라구요... 도르륵",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024939.1.21",
          "form": "이번에 체감 제대로 했습니다",
          "original_form": "이번에 체감 제대로 했습니다",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024939.1.22",
          "form": "그래서 오늘은 샐러드를 샀답니다",
          "original_form": "그래서 오늘은 샐러드를 샀답니다",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024939.1.23",
          "form": "아 야식으로 드셨군요 ㅋㅋㅋㅋ",
          "original_form": "아 야식으로 드셨군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024939.1.24",
          "form": "샐러드 재료를 사신거에요?",
          "original_form": "샐러드 재료를 사신거에요?",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024939.1.25",
          "form": "그냥 팩으로 된거 샀어요",
          "original_form": "그냥 팩으로 된거 샀어요",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024939.1.26",
          "form": "비싸긴 하지만 저의 건강을 위해서!",
          "original_form": "비싸긴 하지만 저의 건강을 위해서!",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024939.1.27",
          "form": "팩으로 사는게 더 먹기 편하긴해요 ㅋㅋㅋㅋ",
          "original_form": "팩으로 사는게 더 먹기 편하긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024939.1.28",
          "form": "샐러드말구 또 뭐사셧어요?",
          "original_form": "샐러드말구 또 뭐사셧어요?",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024939.1.29",
          "form": "그거랑 계란이랑 요거트요!",
          "original_form": "그거랑 계란이랑 요거트요!",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024939.1.30",
          "form": "햇반도 샀네여",
          "original_form": "햇반도 샀네여",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024939.1.31",
          "form": "오 요거트 좋아하시나봐요",
          "original_form": "오 요거트 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210831 21:19"
        }
      ]
    }
