      "utterance": [
        {
          "id": "MDRW2100017275.1.1",
          "form": "걔는 사람은 엄청 좋아하는데",
          "original_form": "걔는 사람은 엄청 좋아하는데",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017275.1.2",
          "form": "말을 아예 안들었어요",
          "original_form": "말을 아예 안들었어요",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.3",
          "form": "ㅋㅋㅋ 개는",
          "original_form": "ㅋㅋㅋ 개는",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.4",
          "form": "하긴 개도 내성적인 애들",
          "original_form": "하긴 개도 내성적인 애들",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.5",
          "form": "인싸 스타일",
          "original_form": "인싸 스타일",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.6",
          "form": "여러 성격이 있겠어요",
          "original_form": "여러 성격이 있겠어요",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.7",
          "form": "name3 성격은 어때요?",
          "original_form": "&name3& 성격은 어때요?",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.8",
          "form": "사고쳐놓고 해맑게 웃는 스타일",
          "original_form": "사고쳐놓고 해맑게 웃는 스타일",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.9",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.10",
          "form": "사고치는건 똑같은데",
          "original_form": "사고치는건 똑같은데",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.11",
          "form": "귀엽네",
          "original_form": "귀엽네",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.12",
          "form": "개는 눈치보거는요",
          "original_form": "개는 눈치보거는요",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017275.1.13",
          "form": "지가 잘못한거알아서",
          "original_form": "지가 잘못한거알아서",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017275.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017275.1.15",
          "form": "고양이는 안그러더라구요",
          "original_form": "고양이는 안그러더라구요",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017275.1.16",
          "form": "그 특유의 눈이 있어요",
          "original_form": "그 특유의 눈이 있어요",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017275.1.17",
          "form": "지가 더 당당해서 나한테 성질내는데",
          "original_form": "지가 더 당당해서 나한테 성질내는데",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017275.1.18",
          "form": "어이가없어가지고",
          "original_form": "어이가없어가지고",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017275.1.19",
          "form": "ㅋㅋㅋ 반성따위는 없다",
          "original_form": "ㅋㅋㅋ 반성따위는 없다",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017275.1.20",
          "form": "그냥 좀 더 사회적이냐 아니냐의 차이같아요ㅋㅋ",
          "original_form": "그냥 좀 더 사회적이냐 아니냐의 차이같아요ㅋㅋ",
