      "utterance": [
        {
          "id": "MDRW2100044373.1.1",
          "form": "무슨맛좋아하세요",
          "original_form": "무슨맛좋아하세요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044373.1.2",
          "form": "저는 상큼한 것도 좋아하고 민초도 좋아해요!",
          "original_form": "저는 상큼한 것도 좋아하고 민초도 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044373.1.3",
          "form": "민트초코 좋아하시나요?",
          "original_form": "민트초코 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044373.1.4",
          "form": "민초좋죵",
          "original_form": "민초좋죵",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044373.1.5",
          "form": "아이스크림은 무슨 맛 좋아하세요??",
          "original_form": "아이스크림은 무슨 맛 좋아하세요??",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044373.1.6",
          "form": "저는바닐라요",
          "original_form": "저는바닐라요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044373.1.7",
          "form": "요거트나~",
          "original_form": "요거트나~",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044373.1.8",
          "form": "아 바닐라 같은 기본도 넘 맛있죠",
          "original_form": "아 바닐라 같은 기본도 넘 맛있죠",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044373.1.9",
          "form": "아포카토 해먹으면 진짜 좋기도 하고 후식으로 깔끔하게 입가심 하기도 좋고!",
          "original_form": "아포카토 해먹으면 진짜 좋기도 하고 후식으로 깔끔하게 입가심 하기도 좋고!",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044373.1.10",
          "form": "맞아용ㅎㅇㅎ",
          "original_form": "맞아용ㅎㅇㅎ",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044373.1.11",
          "form": "아포카토맛있어오",
          "original_form": "아포카토맛있어오",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044373.1.12",
          "form": "맞아요!! 카페인 부담되지만 커피맛 느끼고 싶을때 너무 좋더라구요!",
          "original_form": "맞아요!! 카페인 부담되지만 커피맛 느끼고 싶을때 너무 좋더라구요!",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044373.1.13",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044373.1.14",
          "form": "배라도 자주 가시나요??",
          "original_form": "배라도 자주 가시나요??",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044373.1.15",
          "form": "저이것도베라하려고해용",
          "original_form": "저이것도베라하려고해용",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044373.1.16",
          "form": "아 ㅎㅎㅎ 배라 좋아하시군요!!",
          "original_form": "아 ㅎㅎㅎ 배라 좋아하시군요!!",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044373.1.17",
          "form": "배라 가시면 뭐 드시나요??",
          "original_form": "배라 가시면 뭐 드시나요??",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044373.1.18",
          "form": "엄마는외계인이용ㅎㅈㅎ",
          "original_form": "엄마는외계인이용ㅎㅈㅎ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044373.1.19",
          "form": "뉴욕치즈케이크나 녹차도좋아요",
