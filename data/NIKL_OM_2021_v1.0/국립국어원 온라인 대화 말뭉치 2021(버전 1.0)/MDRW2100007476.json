      "utterance": [
        {
          "id": "MDRW2100007476.1.1",
          "form": "안녕하세요 게임 좋아하세요?",
          "original_form": "안녕하세요 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007476.1.2",
          "form": "게임 좋아해요! 여러 게임 하고 있어요",
          "original_form": "게임 좋아해요! 여러 게임 하고 있어요",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007476.1.3",
          "form": "와 어떤 게임 하세요???",
          "original_form": "와 어떤 게임 하세요???",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007476.1.4",
          "form": "살짝 망한...? 게임도 하고요 파이널판타지 요새 하구 있고 주로 하는 건 스팀게임이에요!",
          "original_form": "살짝 망한...? 게임도 하고요 파이널판타지 요새 하구 있고 주로 하는 건 스팀게임이에요!",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007476.1.5",
          "form": "스타듀밸리!",
          "original_form": "스타듀밸리!",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007476.1.6",
          "form": "살짝 망한 게임은 뭐예요???ㅋㅋㅋ",
          "original_form": "살짝 망한 게임은 뭐예요???ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007476.1.7",
          "form": "스타듀밸리 유명하더라구요!",
          "original_form": "스타듀밸리 유명하더라구요!",
          "speaker_id": "1",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007476.1.8",
          "form": "저는 지금 돈스타브 하고있는데 스타듀밸리가 유명해서 나중에 한번 해보려구요",
          "original_form": "저는 지금 돈스타브 하고있는데 스타듀밸리가 유명해서 나중에 한번 해보려구요",
          "speaker_id": "1",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007476.1.9",
          "form": "스타듀밸리 재미있어요! 돈스타브 하시면 스듀밸도 즐기실 수 있을 거예요",
          "original_form": "스타듀밸리 재미있어요! 돈스타브 하시면 스듀밸도 즐기실 수 있을 거예요",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007476.1.10",
          "form": "전 돈스타브 살짝 담궜다가 어려워서 묵히고 있어요ㅠㅠ",
          "original_form": "전 돈스타브 살짝 담궜다가 어려워서 묵히고 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007476.1.11",
          "form": "앗 어렵죠ㅜㅜ 저 지금 계속 죽으면서 계임하고 있어요ㅋㅋ",
          "original_form": "앗 어렵죠ㅜㅜ 저 지금 계속 죽으면서 계임하고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007476.1.12",
          "form": "망한게임 그... 오로나민 c같은 게임인데....",
          "original_form": "망한게임 그... 오로나민 c같은 게임인데....",
          "speaker_id": "2",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007476.1.13",
          "form": "돈스타브는 아무래도 친구들이랑 해야겠더라고요ㅎㅎ",
          "original_form": "돈스타브는 아무래도 친구들이랑 해야겠더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007476.1.14",
          "form": "ㅋㅋㅋ저도 친구가 돈스타브투게더 추천해서 같이 하게 됐어요.",
          "original_form": "ㅋㅋㅋ저도 친구가 돈스타브투게더 추천해서 같이 하게 됐어요.",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007476.1.15",
          "form": "오로나민 씨같은 게임이요??",
          "original_form": "오로나민 씨같은 게임이요??",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007476.1.16",
          "form": "흑ㅎ흑 사이퍼즈라는 게임이에요",
          "original_form": "흑ㅎ흑 사이퍼즈라는 게임이에요",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007476.1.17",
          "form": "부끄러워서 말 못하겠더라고요",
          "original_form": "부끄러워서 말 못하겠더라고요",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007476.1.18",
          "form": "뱆님 또 하시는 게임 있으세요?",
          "original_form": "뱆님 또 하시는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007476.1.19",
          "form": "아 이거 들어본 것 같아요",
          "original_form": "아 이거 들어본 것 같아요",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007476.1.20",
          "form": "저 오버워치 가끔씩 해요!",
          "original_form": "저 오버워치 가끔씩 해요!",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007476.1.21",
          "form": "옵치도 했었어요!",
          "original_form": "옵치도 했었어요!",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007476.1.22",
          "form": "옵치랑 비슷한 게임이에요",
          "original_form": "옵치랑 비슷한 게임이에요",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007476.1.23",
          "form": "옵치 주로 하는 캐릭터 있으셨나요!",
          "original_form": "옵치 주로 하는 캐릭터 있으셨나요!",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007476.1.24",
          "form": "그래요???? 헐 해봐야겠어요!",
          "original_form": "그래요???? 헐 해봐야겠어요!",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007476.1.25",
          "form": "저는.... 메르시ㅋㅋㅠㅠ 랑 시그마요...",
          "original_form": "저는.... 메르시ㅋㅋㅠㅠ 랑 시그마요...",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007476.1.26",
          "form": "헐 귀한 분이셨네요",
          "original_form": "헐 귀한 분이셨네요",
          "speaker_id": "2",
          "time": "20210822 02:58"
        },
        {
          "id": "MDRW2100007476.1.27",
          "form": "탱힐 유저셨구나",
          "original_form": "탱힐 유저셨구나",
          "speaker_id": "2",
          "time": "20210822 02:58"
        },
        {
          "id": "MDRW2100007476.1.28",
          "form": "네... 잘 못해요ㅋㅋㅋ",
          "original_form": "네... 잘 못해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007476.1.29",
          "form": "재밌자고 게임 하는 건데요 뭐!",
          "original_form": "재밌자고 게임 하는 건데요 뭐!",
          "speaker_id": "2",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007476.1.30",
          "form": "실력 떠나서 탱힐 유저분은 멋진 걸요!",
          "original_form": "실력 떠나서 탱힐 유저분은 멋진 걸요!",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007476.1.31",
          "form": "탱힐이 워낙 어렵기도 하구요",
          "original_form": "탱힐이 워낙 어렵기도 하구요",
          "speaker_id": "2",
          "time": "20210822 03:00"
        }
      ]
