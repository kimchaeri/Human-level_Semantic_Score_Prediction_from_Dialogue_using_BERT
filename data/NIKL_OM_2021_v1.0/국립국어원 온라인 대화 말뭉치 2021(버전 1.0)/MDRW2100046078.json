      "utterance": [
        {
          "id": "MDRW2100046078.1.1",
          "form": "전 놀이기구도 잘 못 타겠더라고요",
          "original_form": "전 놀이기구도 잘 못 타겠더라고요",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.2",
          "form": "그러시구나 ㅋㅋㅋㅈ",
          "original_form": "그러시구나 ㅋㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.3",
          "form": "걍 기념상품만 사고ㅋㅋㅋ",
          "original_form": "걍 기념상품만 사고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.4",
          "form": "전놀이기구는잘타요",
          "original_form": "전놀이기구는잘타요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.6",
          "form": "막 자이로드롭같은 것도요?",
          "original_form": "막 자이로드롭같은 것도요?",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.7",
          "form": "안전만보장된다면 ㅋㅋㅋ",
          "original_form": "안전만보장된다면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.9",
          "form": "자이로드롭은 연속 10번도타요",
          "original_form": "자이로드롭은 연속 10번도타요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.10",
          "form": "와...멋있어요",
          "original_form": "와...멋있어요",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046078.1.12",
          "form": "유일한자랑거리",
          "original_form": "유일한자랑거리",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.13",
          "form": "전 걍 먹거리만 자꾸 사고",
          "original_form": "전 걍 먹거리만 자꾸 사고",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.14",
          "form": "그 버터구이옥수수ㅋㅋㅋ열심히 먹어요",
          "original_form": "그 버터구이옥수수ㅋㅋㅋ열심히 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.15",
          "form": "ㅋㅋㅋ맛있죠 ㅠㅜ",
          "original_form": "ㅋㅋㅋ맛있죠 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.16",
          "form": "놀이공원안긴지도",
          "original_form": "놀이공원안긴지도",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.17",
          "form": "오래됐네요 ㅠ",
          "original_form": "오래됐네요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.18",
          "form": "그죠ㅜㅜㅜ 코로나 때매 더더욱",
          "original_form": "그죠ㅜㅜㅜ 코로나 때매 더더욱",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.19",
          "form": "그래도 기념상품 사는 재미가 있죠",
          "original_form": "그래도 기념상품 사는 재미가 있죠",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046078.1.20",
          "form": "글게요",
          "original_form": "글게요",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046078.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046078.1.22",
          "form": "캐릭터가 그래도 귀여워요",
          "original_form": "캐릭터가 그래도 귀여워요",
