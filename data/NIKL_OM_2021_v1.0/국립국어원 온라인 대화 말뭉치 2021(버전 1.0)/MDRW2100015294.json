        {
          "id": "MDRW2100015294.1.1",
          "form": "말해줄래요",
          "original_form": "말해줄래요",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015294.1.2",
          "form": "예?!",
          "original_form": "예?!",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.3",
          "form": "앞에 질문을 못봐써요",
          "original_form": "앞에 질문을 못봐써요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.4",
          "form": "앗 오늘 뭐드셨었다 했죠?!",
          "original_form": "앗 오늘 뭐드셨었다 했죠?!",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.5",
          "form": "아 닭갈비요",
          "original_form": "아 닭갈비요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.6",
          "form": "맞다맞다 유가네",
          "original_form": "맞다맞다 유가네",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.7",
          "form": "뭐에 감명을 받으셨나요",
          "original_form": "뭐에 감명을 받으셨나요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.8",
          "form": "나왔얶다",
          "original_form": "나왔얶다",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.9",
          "form": "먹는거에서요???",
          "original_form": "먹는거에서요???",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015294.1.10",
          "form": "방금 쩝쩝박사..",
          "original_form": "방금 쩝쩝박사..",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015294.1.11",
          "form": "ㅋㅋㅋㅋㅋ 우리 지금 뭐해요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 우리 지금 뭐해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015294.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ서로 오해중",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ서로 오해중",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015294.1.13",
          "form": "아 생수 뭐마셔요",
          "original_form": "아 생수 뭐마셔요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.14",
          "form": "생수 안마시나",
          "original_form": "생수 안마시나",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.15",
          "form": "아 저는 쩝쩝석사라 박사는 못따라가겠다 했어요",
          "original_form": "아 저는 쩝쩝석사라 박사는 못따라가겠다 했어요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.16",
          "form": "생수 저 하..",
          "original_form": "생수 저 하..",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.17",
          "form": "동원 F&B거 얼마전에 샀거든요",
          "original_form": "동원 F&B거 얼마전에 샀거든요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.18",
          "form": "헐 나도",
          "original_form": "헐 나도",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.19",
          "form": "오늘 불검출기사봄",
          "original_form": "오늘 불검출기사봄",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.20",
          "form": "근데 그거 식수로 부적합하다는 판정을 받았대요",
          "original_form": "근데 그거 식수로 부적합하다는 판정을 받았대요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.21",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.22",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.23",
          "form": "배신감 개오짐",
          "original_form": "배신감 개오짐",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.24",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.25",
          "form": "깨끗하다며",
          "original_form": "깨끗하다며",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.26",
          "form": "아 불검출아니고 부적합이구나",
          "original_form": "아 불검출아니고 부적합이구나",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.27",
          "form": "이자식들아",
          "original_form": "이자식들아",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015294.1.28",
          "form": "그니까 믿고마신 나만 바보됐어요",
          "original_form": "그니까 믿고마신 나만 바보됐어요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.29",
          "form": "동원이라매",
          "original_form": "동원이라매",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.30",
          "form": "막 엄청 싼값도 아니었던거같은디",
          "original_form": "막 엄청 싼값도 아니었던거같은디",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.31",
          "form": "참치를 믿고 구매한건데",
          "original_form": "참치를 믿고 구매한건데",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.32",
          "form": "쿠팡 탐사수 이런거는 최저가잖어요",
          "original_form": "쿠팡 탐사수 이런거는 최저가잖어요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.33",
          "form": "근데 동원 최저가도 아니면서",
          "original_form": "근데 동원 최저가도 아니면서",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.34",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.35",
          "form": "삼다수만 1도 안나왔대요",
          "original_form": "삼다수만 1도 안나왔대요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.36",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015294.1.37",
