      "utterance": [
        {
          "id": "MDRW2100043479.1.1",
          "form": "고딩때로 돌아간다면",
          "original_form": "고딩때로 돌아간다면",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043479.1.2",
          "form": "어쩌겠어요",
          "original_form": "어쩌겠어요",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043479.1.3",
          "form": "공부를 엄청 열심히해서",
          "original_form": "공부를 엄청 열심히해서",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043479.1.4",
          "form": "서울대를 갈거에요 ㅋㅋㅋ",
          "original_form": "서울대를 갈거에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043479.1.5",
          "form": "ㅋㅋㅋㅋㅋsky도전할거임",
          "original_form": "ㅋㅋㅋㅋㅋsky도전할거임",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043479.1.6",
          "form": "학력이 중요하긴 한것같아요",
          "original_form": "학력이 중요하긴 한것같아요",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043479.1.7",
          "form": "여기 확실히 인문계아닌애들",
          "original_form": "여기 확실히 인문계아닌애들",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043479.1.8",
          "form": "급여높음ㅋㅋ",
          "original_form": "급여높음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043479.1.9",
          "form": "열심히 했겠죠...",
          "original_form": "열심히 했겠죠...",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043479.1.10",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043479.1.11",
          "form": "그리구 인문대 안 갈거에요",
          "original_form": "그리구 인문대 안 갈거에요",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043479.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043479.1.13",
          "form": "ㅋㅋㅋ미투",
          "original_form": "ㅋㅋㅋ미투",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043479.1.14",
          "form": "그리고 좀놀고싶음",
          "original_form": "그리고 좀놀고싶음",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043479.1.15",
          "form": "다양하게",
          "original_form": "다양하게",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043479.1.16",
          "form": "맞아요 저는 공부도 못하는 주제에 너무너무 범생이기만 했음ㅋㅋㅋ",
          "original_form": "맞아요 저는 공부도 못하는 주제에 너무너무 범생이기만 했음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043479.1.17",
          "form": "나도ㅋㅋㅋㅋ진짜 근데 여전히 범생이 인생스타일못버림",
          "original_form": "나도ㅋㅋㅋㅋ진짜 근데 여전히 범생이 인생스타일못버림",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043479.1.18",
          "form": "그때 나름 열심히 한것같은뎅",
          "original_form": "그때 나름 열심히 한것같은뎅",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043479.1.19",
          "form": "ㅋㅋㅋㅋㅋ 어쩌겠어요 타고난 걸",
          "original_form": "ㅋㅋㅋㅋㅋ 어쩌겠어요 타고난 걸",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043479.1.20",
          "form": "성적이 별로였네",
          "original_form": "성적이 별로였네",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043479.1.21",
          "form": "친구들도 다 범생이 스타일림",
          "original_form": "친구들도 다 범생이 스타일림",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043479.1.22",
          "form": "맞아옄ㅋㅋㅋㅋ",
          "original_form": "맞아옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043479.1.23",
          "form": "학생때 진짜 죙일 학교에 어떻게 있었는지 대단함",
          "original_form": "학생때 진짜 죙일 학교에 어떻게 있었는지 대단함",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043479.1.24",
          "form": "새삼스럽게",
          "original_form": "새삼스럽게",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043479.1.25",
          "form": "그쳐",
          "original_form": "그쳐",
