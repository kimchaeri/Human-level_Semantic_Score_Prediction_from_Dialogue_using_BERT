      "utterance": [
        {
          "id": "MDRW2100020719.1.1",
          "form": "혹시 마라드시나요",
          "original_form": "혹시 마라드시나요",
          "speaker_id": "1",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100020719.1.2",
          "form": "네 마라탕은 또 좋아합니다",
          "original_form": "네 마라탕은 또 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100020719.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.4",
          "form": "아.. 안타깝게도",
          "original_form": "아.. 안타깝게도",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.5",
          "form": "못먹습니다",
          "original_form": "못먹습니다",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.6",
          "form": "앗 저런..",
          "original_form": "앗 저런..",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.7",
          "form": "향나는걸 좀 못먹는듯",
          "original_form": "향나는걸 좀 못먹는듯",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.8",
          "form": "향신료에 약하신가요",
          "original_form": "향신료에 약하신가요",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.9",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.10",
          "form": "저 그 쯔란인가",
          "original_form": "저 그 쯔란인가",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.11",
          "form": "해외가면 힘드시겠어요",
          "original_form": "해외가면 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.12",
          "form": "그거 바른 김 먹어봤는데",
          "original_form": "그거 바른 김 먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.13",
          "form": "양꼬치 찍어먹는?",
          "original_form": "양꼬치 찍어먹는?",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.14",
          "form": "오..우...",
          "original_form": "오..우...",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.15",
          "form": "네 그거라더라구요",
          "original_form": "네 그거라더라구요",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.16",
          "form": "와ㅋㅋㅋ 전 쯔란 좋아해서",
          "original_form": "와ㅋㅋㅋ 전 쯔란 좋아해서",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.17",
          "form": "아.. 안맞아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아.. 안맞아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020719.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020719.1.19",
          "form": "집에 소스도 따로 샀어요",
          "original_form": "집에 소스도 따로 샀어요",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020719.1.20",
          "form": "가루!",
          "original_form": "가루!",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020719.1.21",
          "form": "쯔란을 좋아하시다니",
          "original_form": "쯔란을 좋아하시다니",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020719.1.22",
          "form": "약간 맛이 애매하던데",
          "original_form": "약간 맛이 애매하던데",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020719.1.23",
          "form": "무슨맛이 느껴지시던가요",
          "original_form": "무슨맛이 느껴지시던가요",
          "speaker_id": "1",
          "time": "20210828 11:37"
        }
      ]
