      "utterance": [
        {
          "id": "MDRW2100028297.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028297.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028297.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028297.1.4",
          "form": "분업하는 게 정말 좋은 것 같아요!",
          "original_form": "분업하는 게 정말 좋은 것 같아요!",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028297.1.5",
          "form": "전 자취하고 있어서 저 혼자 다 하지만요ㅎ",
          "original_form": "전 자취하고 있어서 저 혼자 다 하지만요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028297.1.6",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100028297.1.7",
          "form": "아 공감해요",
          "original_form": "아 공감해요",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100028297.1.8",
          "form": "저는 귀찮아서 그냥 자기 전에 곰팡이 제거제 뿌리고 일어나서 물로 다 씻어내요",
          "original_form": "저는 귀찮아서 그냥 자기 전에 곰팡이 제거제 뿌리고 일어나서 물로 다 씻어내요",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100028297.1.9",
          "form": "세면대는 안 쓰는 치약 칫솔로 닦구요",
          "original_form": "세면대는 안 쓰는 치약 칫솔로 닦구요",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100028297.1.10",
          "form": "화장실 청소 팁 있으신가요?",
          "original_form": "화장실 청소 팁 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100028297.1.11",
          "form": "락스조",
          "original_form": "락스조",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100028297.1.12",
          "form": "뭐 락스가 최고긴 하죠",
          "original_form": "뭐 락스가 최고긴 하죠",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100028297.1.13",
          "form": "곰팡이안 생기게 자주햐죠",
          "original_form": "곰팡이안 생기게 자주햐죠",
          "speaker_id": "1",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100028297.1.14",
          "form": "곰팡이 청소가 중요하죠",
          "original_form": "곰팡이 청소가 중요하죠",
          "speaker_id": "2",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100028297.1.15",
          "form": "청소해도 계속 생기는 게 짜증나요ㅋㅋ",
          "original_form": "청소해도 계속 생기는 게 짜증나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100028297.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100028297.1.17",
          "form": "씻고나면 물기 다 닦아주고 해줘도 생기더라구요..ㅠ",
          "original_form": "씻고나면 물기 다 닦아주고 해줘도 생기더라구요..ㅠ",
