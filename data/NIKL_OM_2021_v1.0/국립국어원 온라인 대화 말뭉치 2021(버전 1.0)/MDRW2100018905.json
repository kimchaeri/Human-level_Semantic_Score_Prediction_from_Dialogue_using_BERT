      "utterance": [
        {
          "id": "MDRW2100018905.1.1",
          "form": "울산 여행 해보셨어요?",
          "original_form": "울산 여행 해보셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018905.1.2",
          "form": "안녕하세요. 아뇨 울산이 고향이신가요?",
          "original_form": "안녕하세요. 아뇨 울산이 고향이신가요?",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018905.1.3",
          "form": "아뇨 고향은 다른곳인데",
          "original_form": "아뇨 고향은 다른곳인데",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018905.1.4",
          "form": "울산으로 여행가시려구요?",
          "original_form": "울산으로 여행가시려구요?",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018905.1.5",
          "form": "아뇨 살고 있어요",
          "original_form": "아뇨 살고 있어요",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018905.1.6",
          "form": "여행지로 울산 좋아요",
          "original_form": "여행지로 울산 좋아요",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018905.1.7",
          "form": "아 그렇군요! 울산 어디로 여행가면 좋을까요? 추천해주실수있나요?",
          "original_form": "아 그렇군요! 울산 어디로 여행가면 좋을까요? 추천해주실수있나요?",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018905.1.8",
          "form": "태화강 국가 정원 좋고",
          "original_form": "태화강 국가 정원 좋고",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018905.1.9",
          "form": "대왕암공원도 좋고",
          "original_form": "대왕암공원도 좋고",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018905.1.10",
          "form": "울산대공원도 좋아요",
          "original_form": "울산대공원도 좋아요",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018905.1.11",
          "form": "태화강 많이 들어봤어요!",
          "original_form": "태화강 많이 들어봤어요!",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018905.1.12",
          "form": "아름다운 강입니다",
          "original_form": "아름다운 강입니다",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018905.1.13",
          "form": "추천 감사합니다!ㅎㅎ",
          "original_form": "추천 감사합니다!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018905.1.14",
          "form": "초겨울에 오시면 엄청난 까마귀때를 보실수도 있어요",
          "original_form": "초겨울에 오시면 엄청난 까마귀때를 보실수도 있어요",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018905.1.15",
          "form": "장관이에요",
          "original_form": "장관이에요",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018905.1.16",
          "form": "오! 참고하겠습니다 ㅎㅎ 울산에 가면 꼭 먹어야 할 음식이 있나요?",
          "original_form": "오! 참고하겠습니다 ㅎㅎ 울산에 가면 꼭 먹어야 할 음식이 있나요?",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018905.1.17",
          "form": "정자대게랑",
          "original_form": "정자대게랑",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018905.1.18",
          "form": "회랑 해산물",
          "original_form": "회랑 해산물",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018905.1.19",
          "form": "음 그리고",
          "original_form": "음 그리고",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018905.1.20",
          "form": "말만 들어도 맛있겠네요!",
          "original_form": "말만 들어도 맛있겠네요!",
          "speaker_id": "2",
          "time": "20210827 13:50"
        }
      ]
