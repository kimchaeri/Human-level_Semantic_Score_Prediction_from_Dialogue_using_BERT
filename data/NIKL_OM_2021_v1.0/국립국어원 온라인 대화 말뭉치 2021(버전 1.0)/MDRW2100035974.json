      "utterance": [
        {
          "id": "MDRW2100035974.1.1",
          "form": "습해서 근가 반팔입어도 땀이나더라고요",
          "original_form": "습해서 근가 반팔입어도 땀이나더라고요",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035974.1.2",
          "form": "앗 저는 긴팔입었는데",
          "original_form": "앗 저는 긴팔입었는데",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035974.1.3",
          "form": "온도감이 적당하니 괜찮네용",
          "original_form": "온도감이 적당하니 괜찮네용",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035974.1.4",
          "form": "곧 긴팔 입어야 될 것 같긴해요 밤 온도는 많이 떨어지더라구요",
          "original_form": "곧 긴팔 입어야 될 것 같긴해요 밤 온도는 많이 떨어지더라구요",
          "speaker_id": "2",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035974.1.5",
          "form": "맞아요ㅠㅠ 제법 쌀쌀해서 이젠 에어컨도 안틀구요ㅋㅋ",
          "original_form": "맞아요ㅠㅠ 제법 쌀쌀해서 이젠 에어컨도 안틀구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035974.1.6",
          "form": "요즘 날씨보면 죽어라 더웠던 때가 먼 옛날 같죠ㅋㅋㅋ",
          "original_form": "요즘 날씨보면 죽어라 더웠던 때가 먼 옛날 같죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035974.1.7",
          "form": "맞아요~ㅋㅋㅋㅋ 이불은 바꾸셨나용?",
          "original_form": "맞아요~ㅋㅋㅋㅋ 이불은 바꾸셨나용?",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100035974.1.8",
          "form": "아직이요 ㅋㅋ 대신 창문은 닫고 자요",
          "original_form": "아직이요 ㅋㅋ 대신 창문은 닫고 자요",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035974.1.9",
          "form": "이러고 자는데",
          "original_form": "이러고 자는데",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035974.1.10",
          "form": "창문도 닫고",
          "original_form": "창문도 닫고",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035974.1.11",
          "form": "온도 딱 맞아요ㅋㅋㅋ",
          "original_form": "온도 딱 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035974.1.12",
          "form": "갑자기 더워질까봐 무섭네요",
          "original_form": "갑자기 더워질까봐 무섭네요",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035974.1.13",
          "form": "계속 비온다니까 더워지진 않을 것 같은데",
          "original_form": "계속 비온다니까 더워지진 않을 것 같은데",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035974.1.14",
          "form": "계속 비오는게 문제네요",
          "original_form": "계속 비오는게 문제네요",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035974.1.15",
          "form": "지금은 저희동네는 해쨍쨍 맑은데",
          "original_form": "지금은 저희동네는 해쨍쨍 맑은데",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035974.1.16",
          "form": "name2님 동네는 어떠세요?",
          "original_form": "&name2&님 동네는 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035974.1.17",
          "form": "구름이 좀 껴서 흐려요",
          "original_form": "구름이 좀 껴서 흐려요",
          "speaker_id": "2",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035974.1.18",
          "form": "곧 비가 오겠네요ㅠㅠ",
          "original_form": "곧 비가 오겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035974.1.19",
          "form": "ㅠㅠ 요즘은 우산이 필수에요",
          "original_form": "ㅠㅠ 요즘은 우산이 필수에요",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100035974.1.20",
          "form": "맞아요ㅜㅜㅠ 소나기때문에 급하게 산 우산이",
          "original_form": "맞아요ㅜㅜㅠ 소나기때문에 급하게 산 우산이",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100035974.1.21",
          "form": "한가득이랍니다...ㅠ",
          "original_form": "한가득이랍니다...ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100035974.1.22",
          "form": "점점 양산 쓰는 사람이 늘어나더라구요",
          "original_form": "점점 양산 쓰는 사람이 늘어나더라구요",
