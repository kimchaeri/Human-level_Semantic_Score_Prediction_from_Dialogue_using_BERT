      "utterance": [
        {
          "id": "MDRW2100033106.1.1",
          "form": "연애하십니까",
          "original_form": "연애하십니까",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100033106.1.2",
          "form": "아니오 결혼하셨어요?",
          "original_form": "아니오 결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100033106.1.3",
          "form": "아니요 연애합니다",
          "original_form": "아니요 연애합니다",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100033106.1.4",
          "form": "좋아하는 사람은 있어요?",
          "original_form": "좋아하는 사람은 있어요?",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100033106.1.5",
          "form": "아니요ㅋㅋ지금은 쉬는중이요",
          "original_form": "아니요ㅋㅋ지금은 쉬는중이요",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033106.1.6",
          "form": "연애중이라니 행복하실거같아오ㅡ",
          "original_form": "연애중이라니 행복하실거같아오ㅡ",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100033106.1.7",
          "form": "글쎄요...",
          "original_form": "글쎄요...",
          "speaker_id": "1",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100033106.1.8",
          "form": "혼자가 편한거같기도하고..",
          "original_form": "혼자가 편한거같기도하고..",
          "speaker_id": "1",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100033106.1.9",
          "form": "혼자가 편할때도 있고 쓸쓸할 때도있죠",
          "original_form": "혼자가 편할때도 있고 쓸쓸할 때도있죠",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100033106.1.10",
          "form": "여친분이랑 맛집 자주다니시겠네요",
          "original_form": "여친분이랑 맛집 자주다니시겠네요",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100033106.1.11",
          "form": "코로나라 맛집은 많이 못가고 시켜먹죠",
          "original_form": "코로나라 맛집은 많이 못가고 시켜먹죠",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100033106.1.12",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100033106.1.13",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100033106.1.14",
          "form": "아 집데이트 많이하시는군요",
          "original_form": "아 집데이트 많이하시는군요",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100033106.1.15",
          "form": "오늘도 데이트하시나요?",
          "original_form": "오늘도 데이트하시나요?",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100033106.1.16",
          "form": "아니요 요즘 이래저래 바빠서",
          "original_form": "아니요 요즘 이래저래 바빠서",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100033106.1.17",
          "form": "아휴 바쁜하루네요",
          "original_form": "아휴 바쁜하루네요",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100033106.1.18",
          "form": "아이고 서운하시겠네요",
          "original_form": "아이고 서운하시겠네요",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100033106.1.19",
          "form": "어떻게 만나셨나요?",
          "original_form": "어떻게 만나셨나요?",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100033106.1.20",
          "form": "사연이 좀길어서 그건 패스!",
          "original_form": "사연이 좀길어서 그건 패스!",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100033106.1.21",
          "form": "그냥저냥 자연스럽게 만나게 되더라구요",
          "original_form": "그냥저냥 자연스럽게 만나게 되더라구요",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100033106.1.22",
          "form": "마지막 연애는 언제에요",
          "original_form": "마지막 연애는 언제에요",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100033106.1.23",
          "form": "저는 작년말이요",
          "original_form": "저는 작년말이요",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100033106.1.24",
          "form": "자연스러운 만남좋네요",
          "original_form": "자연스러운 만남좋네요",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100033106.1.25",
          "form": "얼마나 만났었어요?",
          "original_form": "얼마나 만났었어요?",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100033106.1.26",
          "form": "한 6개월 정도요",
          "original_form": "한 6개월 정도요",
          "speaker_id": "2",
          "time": "20210907 18:27"
        }
      ]
    }
  ]
