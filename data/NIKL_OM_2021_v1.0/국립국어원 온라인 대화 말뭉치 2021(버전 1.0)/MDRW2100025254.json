      "utterance": [
        {
          "id": "MDRW2100025254.1.1",
          "form": "어떤 사진 위주로 퍼즐게임 했었어?",
          "original_form": "어떤 사진 위주로 퍼즐게임 했었어?",
          "speaker_id": "2",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025254.1.2",
          "form": "당연 연예인",
          "original_form": "당연 연예인",
          "speaker_id": "1",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025254.1.3",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025254.1.4",
          "form": "나도 당연히 연예인!",
          "original_form": "나도 당연히 연예인!",
          "speaker_id": "2",
          "time": "20210901 03:11"
        },
        {
          "id": "MDRW2100025254.1.5",
          "form": "영화 포스터 같은 것도 하고, 연예인 셀카로도 하고!",
          "original_form": "영화 포스터 같은 것도 하고, 연예인 셀카로도 하고!",
          "speaker_id": "2",
          "time": "20210901 03:11"
        },
        {
          "id": "MDRW2100025254.1.6",
          "form": "중독 심했지?",
          "original_form": "중독 심했지?",
          "speaker_id": "1",
          "time": "20210901 03:13"
        },
        {
          "id": "MDRW2100025254.1.7",
          "form": "완전 심했지ㅋㅋㅋ",
          "original_form": "완전 심했지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:14"
        },
        {
          "id": "MDRW2100025254.1.8",
          "form": "연달아서 몇판은 기본이고 아주.. 시간 가는줄 모르고 했네",
          "original_form": "연달아서 몇판은 기본이고 아주.. 시간 가는줄 모르고 했네",
          "speaker_id": "2",
          "time": "20210901 03:14"
        },
        {
          "id": "MDRW2100025254.1.9",
          "form": "혹시 그럼 수박게임도 알아?",
          "original_form": "혹시 그럼 수박게임도 알아?",
          "speaker_id": "2",
          "time": "20210901 03:14"
        },
        {
          "id": "MDRW2100025254.1.10",
          "form": "당연하지ㅋㅋㅋ 정신놓고 한듯",
          "original_form": "당연하지ㅋㅋㅋ 정신놓고 한듯",
          "speaker_id": "1",
          "time": "20210901 03:15"
        },
        {
          "id": "MDRW2100025254.1.11",
          "form": "역시.... 다들 그런거였어ㅋㅋㅋ 나만 게임에 진심인줄 알았넼ㅋㅋ",
          "original_form": "역시.... 다들 그런거였어ㅋㅋㅋ 나만 게임에 진심인줄 알았넼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025254.1.12",
          "form": "난 그 얼굴사진 다똑같은 수박게임도 했어ㅋㅋ",
          "original_form": "난 그 얼굴사진 다똑같은 수박게임도 했어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025254.1.13",
          "form": "왘ㅋㅋㅋㅋ 엄청 헷갈리지는 않았어?",
          "original_form": "왘ㅋㅋㅋㅋ 엄청 헷갈리지는 않았어?",
          "speaker_id": "2",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025254.1.14",
          "form": "완전 헷갈렸는데 하다보면 존잼이야ㅋㅋ",
          "original_form": "완전 헷갈렸는데 하다보면 존잼이야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025254.1.15",
          "form": "수박게임이고 퍼즐이고 너무 덕잘알 게임들인 것 같어!",
          "original_form": "수박게임이고 퍼즐이고 너무 덕잘알 게임들인 것 같어!",
          "speaker_id": "2",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025254.1.16",
          "form": "그치 너 덕맞지?",
          "original_form": "그치 너 덕맞지?",
          "speaker_id": "1",
          "time": "20210901 03:29"
        },
        {
          "id": "MDRW2100025254.1.17",
          "form": "맞지ㅋㅋㅋㅋㅋㅋ ",
          "original_form": "맞지ㅋㅋㅋㅋㅋㅋ ",
          "speaker_id": "2",
          "time": "20210901 03:30"
        }
      ]
