        },
        {
          "id": "MDRW2100001485.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100001485.1.3",
          "form": "방갑습니다",
          "original_form": "방갑습니다",
          "speaker_id": "1",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100001485.1.4",
          "form": "술 좋아하시나용",
          "original_form": "술 좋아하시나용",
          "speaker_id": "2",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100001485.1.5",
          "form": "조금 좋아합니다",
          "original_form": "조금 좋아합니다",
          "speaker_id": "1",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100001485.1.6",
          "form": "선호하는 주종이 따로 있으신가융",
          "original_form": "선호하는 주종이 따로 있으신가융",
          "speaker_id": "2",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100001485.1.7",
          "form": "테라 맥주입니다",
          "original_form": "테라 맥주입니다",
          "speaker_id": "1",
          "time": "20211015 13:11"
        },
        {
          "id": "MDRW2100001485.1.8",
          "form": "요즈음은 막거리도 가끔 마십니다",
          "original_form": "요즈음은 막거리도 가끔 마십니다",
          "speaker_id": "1",
          "time": "20211015 13:14"
        },
        {
          "id": "MDRW2100001485.1.9",
          "form": "오 저도 맥주는 테라만 ㅎㅎ",
          "original_form": "오 저도 맥주는 테라만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:16"
        },
        {
          "id": "MDRW2100001485.1.10",
          "form": "막걸리는 지평..!!",
          "original_form": "막걸리는 지평..!!",
          "speaker_id": "2",
          "time": "20211015 13:17"
        },
        {
          "id": "MDRW2100001485.1.11",
          "form": "저두 테라만~ 지평두 마셔봤어요!!",
          "original_form": "저두 테라만~ 지평두 마셔봤어요!!",
          "speaker_id": "1",
          "time": "20211015 13:17"
        },
        {
          "id": "MDRW2100001485.1.12",
          "form": "장수는 제 입맛에 뭔가 애매하더라고요 ㅋㅋㅋ",
          "original_form": "장수는 제 입맛에 뭔가 애매하더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 13:20"
        },
        {
          "id": "MDRW2100001485.1.13",
          "form": "호랑이 막걸리라고 있는데",
          "original_form": "호랑이 막걸리라고 있는데",
          "speaker_id": "2",
          "time": "20211015 13:20"
        },
        {
          "id": "MDRW2100001485.1.14",
          "form": "존맛이에요",
          "original_form": "존맛이에요",
          "speaker_id": "2",
          "time": "20211015 13:21"
        },
        {
          "id": "MDRW2100001485.1.15",
          "form": "파는데 찾으시면 한 번 드셔보세요!!",
          "original_form": "파는데 찾으시면 한 번 드셔보세요!!",
          "speaker_id": "2",
          "time": "20211015 13:21"
        },
        {
          "id": "MDRW2100001485.1.16",
          "form": "호랑이 처음 들어보는데요\r\n마셔볼께요~!!",
          "original_form": "호랑이 처음 들어보는데요\r\n마셔볼께요~!!",
          "speaker_id": "1",
          "time": "20211015 13:22"
        },
        {
          "id": "MDRW2100001485.1.17",
          "form": "가까운 편의점에서 판매할지 모르겠네요..",
          "original_form": "가까운 편의점에서 판매할지 모르겠네요..",
          "speaker_id": "1",
          "time": "20211015 13:23"
        },
        {
          "id": "MDRW2100001485.1.18",
          "form": "편의점..에 있을런지는 모르겠어요 ㅠ 저도",
          "original_form": "편의점..에 있을런지는 모르겠어요 ㅠ 저도",
          "speaker_id": "2",
          "time": "20211015 13:23"
        },
        {
          "id": "MDRW2100001485.1.19",
          "form": "전집 가서 먹어본거라..ㅎㅎ",
          "original_form": "전집 가서 먹어본거라..ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:23"
        },
        {
          "id": "MDRW2100001485.1.20",
          "form": "아..ㅎ 편의점엔 항상 있는것만 있더라구요..",
          "original_form": "아..ㅎ 편의점엔 항상 있는것만 있더라구요..",
          "speaker_id": "1",
          "time": "20211015 13:23"
        },
        {
          "id": "MDRW2100001485.1.21",
          "form": "맞아요.... 맥주처럼 막걸리도 다양한 맛을 달라...!!\r\n",
