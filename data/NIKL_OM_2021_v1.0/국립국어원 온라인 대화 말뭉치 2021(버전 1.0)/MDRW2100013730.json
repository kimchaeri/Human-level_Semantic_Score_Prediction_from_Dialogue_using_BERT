      "utterance": [
        {
          "id": "MDRW2100013730.1.1",
          "form": "소소엿어요",
          "original_form": "소소엿어요",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013730.1.2",
          "form": "저는 공부안해도되서 좋앗어요",
          "original_form": "저는 공부안해도되서 좋앗어요",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013730.1.3",
          "form": "평범했다는 뜻인거 알죠?",
          "original_form": "평범했다는 뜻인거 알죠?",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013730.1.4",
          "form": "네 알고있어요",
          "original_form": "네 알고있어요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013730.1.5",
          "form": "저는 공부 너무 열심히 해야됐어서 싫었네요",
          "original_form": "저는 공부 너무 열심히 해야됐어서 싫었네요",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013730.1.6",
          "form": "오 그렇군여",
          "original_form": "오 그렇군여",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013730.1.7",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013730.1.8",
          "form": "부럽게해서 죄송해요",
          "original_form": "부럽게해서 죄송해요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013730.1.9",
          "form": "공부 별로 안해도 됐어서",
          "original_form": "공부 별로 안해도 됐어서",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013730.1.10",
          "form": "가끔 과거로 돌아가고싶어져요",
          "original_form": "가끔 과거로 돌아가고싶어져요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013730.1.11",
          "form": "저는 안그렇네요",
          "original_form": "저는 안그렇네요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013730.1.12",
          "form": "초등학생때부터 공부 차근차근히 하거싶어서요",
          "original_form": "초등학생때부터 공부 차근차근히 하거싶어서요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013730.1.13",
          "form": "저는 아니요",
          "original_form": "저는 아니요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013730.1.14",
          "form": "그렇군여",
          "original_form": "그렇군여",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013730.1.15",
          "form": "초등학생때 공부하지 말걸 후회해요",
