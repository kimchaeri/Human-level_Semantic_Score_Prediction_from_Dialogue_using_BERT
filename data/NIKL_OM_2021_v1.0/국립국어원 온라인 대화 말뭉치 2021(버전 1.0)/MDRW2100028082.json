      "utterance": [
        {
          "id": "MDRW2100028082.1.1",
          "form": "안녕하세요 ",
          "original_form": "안녕하세요 ",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028082.1.2",
          "form": "dp 혹시 보셨나요 군대에 관련된 넷플릭스 드라마요 ㅎㅎ",
          "original_form": "dp 혹시 보셨나요 군대에 관련된 넷플릭스 드라마요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028082.1.3",
          "form": "어 그거 요세 말만든데 재밌음?",
          "original_form": "어 그거 요세 말만든데 재밌음?",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028082.1.4",
          "form": "꿀잼여 진짜 재밋어요",
          "original_form": "꿀잼여 진짜 재밋어요",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028082.1.5",
          "form": "정해인 연기 잘함여",
          "original_form": "정해인 연기 잘함여",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100028082.1.6",
          "form": "예전에 가짜사나이 재밌게봤는데",
          "original_form": "예전에 가짜사나이 재밌게봤는데",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028082.1.7",
          "form": "dp도 그런쪽인가여",
          "original_form": "dp도 그런쪽인가여",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028082.1.8",
          "form": "군대 잘 몰라서 찐 군대 얘긴 좀 지루할것같은디",
          "original_form": "군대 잘 몰라서 찐 군대 얘긴 좀 지루할것같은디",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028082.1.9",
          "form": "ㅋㅋㅋㅋㅋ저도 글케생각해요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 글케생각해요",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028082.1.10",
          "form": "근데 dp는 탈영병 잡는얘기라",
          "original_form": "근데 dp는 탈영병 잡는얘기라",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028082.1.11",
          "form": "스릴러같고 잼써요 ㅋㅋ 후반에 넘 다크하긴 하지만",
          "original_form": "스릴러같고 잼써요 ㅋㅋ 후반에 넘 다크하긴 하지만",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028082.1.12",
          "form": "아 재밌겠다",
          "original_form": "아 재밌겠다",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028082.1.13",
          "form": "왓챠엔 없나??",
          "original_form": "왓챠엔 없나??",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028082.1.14",
          "form": "넷플 오리지널일걸여 꿀잼쓰",
          "original_form": "넷플 오리지널일걸여 꿀잼쓰",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028082.1.15",
          "form": "군대... 전남친 군대보냇던거 생각나네염",
          "original_form": "군대... 전남친 군대보냇던거 생각나네염",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028082.1.16",
          "form": "휴가때 대박 싸우고 헤어짐",
          "original_form": "휴가때 대박 싸우고 헤어짐",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028082.1.17",
          "form": "젠장...왓챠 기생충인데.... 어쩔 수 없나. 넷플 기생충도 되어봐야겠군 이번엔...",
          "original_form": "젠장...왓챠 기생충인데.... 어쩔 수 없나. 넷플 기생충도 되어봐야겠군 이번엔...",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028082.1.18",
          "form": "원래 다들 휴가때 싸우고 헤어지잖음",
          "original_form": "원래 다들 휴가때 싸우고 헤어지잖음",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028082.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 솔직히 영화는 왓챠죠 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 솔직히 영화는 왓챠죠 ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028082.1.20",
          "form": "왜지? 왜 휴가때 싸우지?",
          "original_form": "왜지? 왜 휴가때 싸우지?",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028082.1.21",
          "form": "구남침은 군대에서 자유시간 없고 힘들고 그러고",
          "original_form": "구남침은 군대에서 자유시간 없고 힘들고 그러고",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028082.1.22",
          "form": "근데 늘 재밌는 건 죄다 넷플이었음... 가짜사나이도 그렇고.. dp도 넷플인거보면...",
          "original_form": "근데 늘 재밌는 건 죄다 넷플이었음... 가짜사나이도 그렇고.. dp도 넷플인거보면...",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028082.1.23",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028082.1.24",
          "form": "전 걔 휴가 일정 맞춰서 맨날 약속못잡고",
          "original_form": "전 걔 휴가 일정 맞춰서 맨날 약속못잡고",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028082.1.25",
          "form": "서터레스 ㅡㅡ",
          "original_form": "서터레스 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028082.1.26",
          "form": "그래도 다행이네 하마터면 끝까지 군대 기다려주고 헤어질뻔 했네",
          "original_form": "그래도 다행이네 하마터면 끝까지 군대 기다려주고 헤어질뻔 했네",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028082.1.27",
          "form": "군인들도 이젠 불쌍해보여요 이제 군인아저씨아니고 군인동생들임여",
          "original_form": "군인들도 이젠 불쌍해보여요 이제 군인아저씨아니고 군인동생들임여",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028082.1.28",
          "form": "맞네 어렸을땐 그냥 무서운 아저씨들처럼 보였는데",
          "original_form": "맞네 어렸을땐 그냥 무서운 아저씨들처럼 보였는데",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028082.1.29",
          "form": "근데 여전히 아재같긴해",
          "original_form": "근데 여전히 아재같긴해",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028082.1.30",
          "form": "찌들어있어서... ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "찌들어있어서... ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028082.1.31",
          "form": "군대에 대해 생각나는건 그런거밖에 없네용 복학생이랑..",
          "original_form": "군대에 대해 생각나는건 그런거밖에 없네용 복학생이랑..",
          "speaker_id": "1",
          "time": "20210902 17:51"
        }
      ]
