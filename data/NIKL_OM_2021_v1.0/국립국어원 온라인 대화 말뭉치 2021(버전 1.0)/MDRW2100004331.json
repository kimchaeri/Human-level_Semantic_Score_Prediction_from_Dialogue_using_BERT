      "utterance": [
        {
          "id": "MDRW2100004331.1.1",
          "form": "저녁 뭐 드실 계획인가요?",
          "original_form": "저녁 뭐 드실 계획인가요?",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004331.1.2",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004331.1.3",
          "form": "아마 고기요",
          "original_form": "아마 고기요",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004331.1.4",
          "form": "오옷..집에서 구워드시는건가요?",
          "original_form": "오옷..집에서 구워드시는건가요?",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004331.1.5",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004331.1.6",
          "form": "불판이 구려서 기름이 사방에 튀어요 ㅋㅋ",
          "original_form": "불판이 구려서 기름이 사방에 튀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004331.1.7",
          "form": "불판이 좋으면 덜튈까요??",
          "original_form": "불판이 좋으면 덜튈까요??",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004331.1.8",
          "form": "그런것 같아요",
          "original_form": "그런것 같아요",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004331.1.9",
          "form": "집에서 먹음 항상 기름때문에 힘들었돈기억이..",
          "original_form": "집에서 먹음 항상 기름때문에 힘들었돈기억이..",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004331.1.10",
          "form": "연기도 덜나고요",
          "original_form": "연기도 덜나고요",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004331.1.11",
          "form": "하나 사려고요 ㅜㅜ",
          "original_form": "하나 사려고요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004331.1.12",
          "form": "전 에프 접한뒤로는 간단히 에프에 구워먹거나",
          "original_form": "전 에프 접한뒤로는 간단히 에프에 구워먹거나",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004331.1.13",
          "form": "외식으로만 먹는거 같아요",
          "original_form": "외식으로만 먹는거 같아요",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004331.1.14",
          "form": "에프 좋아요",
          "original_form": "에프 좋아요",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.15",
          "form": "활용도 좋아요",
          "original_form": "활용도 좋아요",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.16",
          "form": "쓰임새가 좋아요ㅋㅋ배달음식 남으면",
          "original_form": "쓰임새가 좋아요ㅋㅋ배달음식 남으면",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.17",
          "form": "전 요새 배달을 넘시켜서ㅜㅜ",
          "original_form": "전 요새 배달을 넘시켜서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.18",
          "form": "고거 돌려먹기도 좋구요",
          "original_form": "고거 돌려먹기도 좋구요",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.19",
          "form": "큰일이예요",
          "original_form": "큰일이예요",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.20",
          "form": "헛..저도요 ㅎㅎㅎㅎ",
          "original_form": "헛..저도요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.22",
          "form": "전 살짝 기름쳐서 해요",
          "original_form": "전 살짝 기름쳐서 해요",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.23",
          "form": "배달음식 줄이려 이번에 장보면서 식재료를 좀 샀어요~",
          "original_form": "배달음식 줄이려 이번에 장보면서 식재료를 좀 샀어요~",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004331.1.24",
          "form": "맞아요 기름 좀 발라 돌리는게 맛이 더 좋더라고요",
          "original_form": "맞아요 기름 좀 발라 돌리는게 맛이 더 좋더라고요",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004331.1.25",
          "form": "네 ㅋㅋ기름이 진리",
          "original_form": "네 ㅋㅋ기름이 진리",
          "speaker_id": "2",
          "time": "20210821 16:08"
        }
      ]
    }
