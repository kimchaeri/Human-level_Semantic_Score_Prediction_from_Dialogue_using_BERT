      "utterance": [
        {
          "id": "MDRW2100020188.1.1",
          "form": "여행가면 기념품 사오시나여???",
          "original_form": "여행가면 기념품 사오시나여???",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020188.1.2",
          "form": "저는 어린왕자 책을사와요!",
          "original_form": "저는 어린왕자 책을사와요!",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020188.1.3",
          "form": "여행간나라의 언어로 출판된",
          "original_form": "여행간나라의 언어로 출판된",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020188.1.4",
          "form": "우와!!! 이런분 첨봣어요 되게 몽글몽글하네여~",
          "original_form": "우와!!! 이런분 첨봣어요 되게 몽글몽글하네여~",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020188.1.5",
          "form": "앗...그런가여!ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗...그런가여!ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020188.1.6",
          "form": "몇권정도모으셨나요??",
          "original_form": "몇권정도모으셨나요??",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020188.1.7",
          "form": "음 지금은 2권이요!",
          "original_form": "음 지금은 2권이요!",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020188.1.8",
          "form": "프랑스랑 일본!",
          "original_form": "프랑스랑 일본!",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020188.1.9",
          "form": "원래는 대학생되서 방학마다 여행가려고했는데",
          "original_form": "원래는 대학생되서 방학마다 여행가려고했는데",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020188.1.10",
          "form": "오!! 으아닛 한국책은 없는건가요??",
          "original_form": "오!! 으아닛 한국책은 없는건가요??",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020188.1.11",
          "form": "코시국,,,,",
          "original_form": "코시국,,,,",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020188.1.12",
          "form": "여행지기준이요!ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "여행지기준이요!ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020188.1.13",
          "form": "한국책도있죠!!!",
          "original_form": "한국책도있죠!!!",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020188.1.14",
          "form": "아핰ㅋㅋㅋㅋㅋㅋㅋ 제가또 맥락파악이안되었네요 핫",
          "original_form": "아핰ㅋㅋㅋㅋㅋㅋㅋ 제가또 맥락파악이안되었네요 핫",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
