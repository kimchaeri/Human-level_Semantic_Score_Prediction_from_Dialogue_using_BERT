        {
          "id": "MDRW2100033806.1.1",
          "form": "염색해보셨나요",
          "original_form": "염색해보셨나요",
          "speaker_id": "2",
          "time": "20210825 03:28"
        },
        {
          "id": "MDRW2100033806.1.2",
          "form": "네 초딩때요ㅋㅋ",
          "original_form": "네 초딩때요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 03:28"
        },
        {
          "id": "MDRW2100033806.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 03:28"
        },
        {
          "id": "MDRW2100033806.1.4",
          "form": "저는 자주하는 편이에요ㅎ",
          "original_form": "저는 자주하는 편이에요ㅎ",
          "speaker_id": "2",
          "time": "20210825 03:28"
        },
        {
          "id": "MDRW2100033806.1.5",
          "form": "탈색은 해보셨나요",
          "original_form": "탈색은 해보셨나요",
          "speaker_id": "2",
          "time": "20210825 03:28"
        },
        {
          "id": "MDRW2100033806.1.6",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "1",
          "time": "20210825 03:29"
        },
        {
          "id": "MDRW2100033806.1.7",
          "form": "염색만 해 보셨구나 어떤 색 해보셨나요",
          "original_form": "염색만 해 보셨구나 어떤 색 해보셨나요",
          "speaker_id": "2",
          "time": "20210825 03:29"
        },
        {
          "id": "MDRW2100033806.1.8",
          "form": "건강 챙기는 편이신가요?",
          "original_form": "건강 챙기는 편이신가요?",
          "speaker_id": "1",
          "time": "20210825 03:29"
        },
        {
          "id": "MDRW2100033806.1.9",
          "form": "어  특별히 챙기는 건 없어요",
          "original_form": "어  특별히 챙기는 건 없어요",
          "speaker_id": "2",
          "time": "20210825 03:29"
        },
        {
          "id": "MDRW2100033806.1.10",
          "form": "건강 챙기시는 편인가요",
          "original_form": "건강 챙기시는 편인가요",
          "speaker_id": "2",
          "time": "20210825 03:29"
        },
        {
          "id": "MDRW2100033806.1.11",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "1",
          "time": "20210825 03:29"
        },
        {
          "id": "MDRW2100033806.1.12",
          "form": "어 ... 운동을 그 딱 좋아하지 않아서",
          "original_form": "어 ... 운동을 그 딱 좋아하지 않아서",
          "speaker_id": "2",
          "time": "20210825 03:30"
        },
        {
          "id": "MDRW2100033806.1.13",
          "form": "네 오래살고 싶어서요~",
          "original_form": "네 오래살고 싶어서요~",
          "speaker_id": "1",
          "time": "20210825 03:30"
        },
        {
          "id": "MDRW2100033806.1.14",
          "form": "운동 좋아하시나요",
          "original_form": "운동 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 03:30"
        },
        {
          "id": "MDRW2100033806.1.15",
          "form": "좋아하는 편은 아니예요",
