        },
        {
          "id": "MDRW2100012978.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012978.1.3",
          "form": "근데 지금 정신이 없어서",
          "original_form": "근데 지금 정신이 없어서",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100012978.1.4",
          "form": "유튜브 티비에 틀러놨네요 ㅋㅋ",
          "original_form": "유튜브 티비에 틀러놨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100012978.1.5",
          "form": "요샌 바로 연결되고 좋은 세상이예요",
          "original_form": "요샌 바로 연결되고 좋은 세상이예요",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100012978.1.6",
          "form": "들으면 딴거 해도되고",
          "original_form": "들으면 딴거 해도되고",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100012978.1.7",
          "form": "ㅋㅋ스마트 티비 좋죠",
          "original_form": "ㅋㅋ스마트 티비 좋죠",
          "speaker_id": "2",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012978.1.8",
          "form": "진짜 세상이~~",
          "original_form": "진짜 세상이~~",
          "speaker_id": "2",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012978.1.9",
          "form": "네 그때 사길 잘했어요",
          "original_form": "네 그때 사길 잘했어요",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012978.1.10",
          "form": "넷플도 바로 연결되고",
          "original_form": "넷플도 바로 연결되고",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012978.1.11",
          "form": "넘편해요",
          "original_form": "넘편해요",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012978.1.12",
          "form": "넷플 킹덤 보셨나요ㅋㅋ",
          "original_form": "넷플 킹덤 보셨나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100012978.1.13",
          "form": "네 ㅎㅎ 다봤어요",
          "original_form": "네 ㅎㅎ 다봤어요",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012978.1.14",
          "form": "스위트홈도보고",
          "original_form": "스위트홈도보고",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012978.1.15",
          "form": "아신전도보고",
          "original_form": "아신전도보고",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012978.1.16",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012978.1.17",
          "form": "스위트홈은 빼고요ㅎㅎ",
          "original_form": "스위트홈은 빼고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100012978.1.18",
          "form": "킹덤 재밌죠",
          "original_form": "킹덤 재밌죠",
          "speaker_id": "2",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100012978.1.19",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100012978.1.20",
          "form": "네 전 좀비 좋아해요 ㅋㅋ",
          "original_form": "네 전 좀비 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100012978.1.21",
          "form": "세자 멋있죠ㅋㅋ",
          "original_form": "세자 멋있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100012978.1.22",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100012978.1.23",
          "form": "에일리어니스트 보셨어요?",
          "original_form": "에일리어니스트 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100012978.1.24",
          "form": "살인사건 추적하는 프로파일러얘기인데 잼나요",
          "original_form": "살인사건 추적하는 프로파일러얘기인데 잼나요",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100012978.1.25",
          "form": "오 한번 봐야겠어요 넷플인가요",
          "original_form": "오 한번 봐야겠어요 넷플인가요",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100012978.1.26",
          "form": "네 좀 잔인해요",
          "original_form": "네 좀 잔인해요",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012978.1.27",
          "form": "추리과정이 넘잼나요",
          "original_form": "추리과정이 넘잼나요",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012978.1.28",
          "form": "반전도 있고",
          "original_form": "반전도 있고",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012978.1.29",
          "form": "뭐 또 재밌게 보셨어요?",
          "original_form": "뭐 또 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012978.1.30",
          "form": "사실 이런게 젤 어려운 질문이네요 ㅋㅋ",
          "original_form": "사실 이런게 젤 어려운 질문이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012978.1.31",
          "form": "뭐 요새는 대탈출이?ㅋㅋ",
          "original_form": "뭐 요새는 대탈출이?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012978.1.32",
          "form": "아 전 저번에 유튜브",
          "original_form": "아 전 저번에 유튜브",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100012978.1.33",
          "form": "그 돈뺏는거요",
          "original_form": "그 돈뺏는거요",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100012978.1.34",
          "form": "막싸우고",
          "original_form": "막싸우고",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100012978.1.35",
          "form": "아 제목이 ㅜㅜ생각이 안나네요",
          "original_form": "아 제목이 ㅜㅜ생각이 안나네요",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100012978.1.36",
          "form": "여자팀 남자팀 나눠서 서로 눈치싸움 하는거",
          "original_form": "여자팀 남자팀 나눠서 서로 눈치싸움 하는거",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100012978.1.37",
          "form": "그거 좀 너무 마장이지만 잘봤어요 ㅋㅋ",
          "original_form": "그거 좀 너무 마장이지만 잘봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100012978.1.38",
          "form": "머니게임 ㅡㅡ",
          "original_form": "머니게임 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100012978.1.39",
          "form": "대탈출은 얘기만 들었네요",
          "original_form": "대탈출은 얘기만 들었네요",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100012978.1.40",
          "form": "강철부대 ㅋㅋ 본방사수",
          "original_form": "강철부대 ㅋㅋ 본방사수",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100012978.1.41",
          "form": "유튜브 보고 결정하셨어요?",
          "original_form": "유튜브 보고 결정하셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012978.1.42",
          "form": "전 현대차로 결정중이요",
          "original_form": "전 현대차로 결정중이요",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012978.1.43",
          "form": "아 2시까지 유튜브 볼까하다 확 하려고요",
          "original_form": "아 2시까지 유튜브 볼까하다 확 하려고요",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100012978.1.44",
          "form": "저도 보긴 할건데 삼성 갈듯 해요 ㅎㅎ",
          "original_form": "저도 보긴 할건데 삼성 갈듯 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.45",
          "form": "머니게임 진짜 막장이여서",
          "original_form": "머니게임 진짜 막장이여서",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.46",
          "form": "웹툰보다 별로였어요",
          "original_form": "웹툰보다 별로였어요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.47",
          "form": "아 네",
          "original_form": "아 네",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.48",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.49",
          "form": "이건뭐",
          "original_form": "이건뭐",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.50",
          "form": "끝이 그냥 궁금해서 봤어요",
          "original_form": "끝이 그냥 궁금해서 봤어요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.51",
          "form": "인성이 이건뭐",
          "original_form": "인성이 이건뭐",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012978.1.52",
          "form": "말도 못하게 엉망이더라구요",
          "original_form": "말도 못하게 엉망이더라구요",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100012978.1.53",
          "form": "담엔 유튜버 말고 좀 일반 참가자 나옴좋겠어요 이상해요",
          "original_form": "담엔 유튜버 말고 좀 일반 참가자 나옴좋겠어요 이상해요",
