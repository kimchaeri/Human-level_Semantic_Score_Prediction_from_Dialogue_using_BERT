      "utterance": [
        {
          "id": "MDRW2100034824.1.1",
          "form": "그 꼬꼬무 같은거도 보세요?",
          "original_form": "그 꼬꼬무 같은거도 보세요?",
          "speaker_id": "2",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100034824.1.2",
          "form": "엇 네네! 이것도 자주 봐요ㅋㅋㅋ",
          "original_form": "엇 네네! 이것도 자주 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034824.1.3",
          "form": "무슨 실화인데 영화같은 이야기가 많더라구요",
          "original_form": "무슨 실화인데 영화같은 이야기가 많더라구요",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034824.1.4",
          "form": "맞죠ㅋㄱㅋ 설명을 되게 잘해줘서",
          "original_form": "맞죠ㅋㄱㅋ 설명을 되게 잘해줘서",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100034824.1.5",
          "form": "좋아해요 ㅋㅋㅋ",
          "original_form": "좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100034824.1.6",
          "form": "약간 아는 사람이 얘기해주는 느낌?",
          "original_form": "약간 아는 사람이 얘기해주는 느낌?",
          "speaker_id": "1",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100034824.1.7",
          "form": "그래서 오오~ 하면서 보게되는ㅋㅋ",
          "original_form": "그래서 오오~ 하면서 보게되는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100034824.1.8",
          "form": "맞아요 ㅋㅋㅋ 반응 보는것도 재밌어요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 반응 보는것도 재밌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100034824.1.9",
          "form": "그래도 결말은 시원한 사이다로 끝나지않을까 싶어요",
          "original_form": "그래도 결말은 시원한 사이다로 끝나지않을까 싶어요",
          "speaker_id": "1",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100034824.1.10",
          "form": "좀 답답하게 끝나면 시청자게시판에 글이 한가득ㅋㅋㅋ",
          "original_form": "좀 답답하게 끝나면 시청자게시판에 글이 한가득ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100034824.1.11",
          "form": "ㅋㄱㅋㅋ혹시 열린 결말도 좋아하세요?",
          "original_form": "ㅋㄱㅋㅋ혹시 열린 결말도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034824.1.12",
          "form": "네네! 차라리 새드앤딩인것보다는",
          "original_form": "네네! 차라리 새드앤딩인것보다는",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034824.1.13",
          "form": "열린결말이 훨 좋아요",
          "original_form": "열린결말이 훨 좋아요",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034824.1.14",
          "form": "아 저두요 ㅋㅋ 새드앤딩 극혐해욬ㅋㅋ",
          "original_form": "아 저두요 ㅋㅋ 새드앤딩 극혐해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100034824.1.15",
          "form": "뭔가 새드앤딩은 찝찝해요ㅠㅠㅋㅋ",
          "original_form": "뭔가 새드앤딩은 찝찝해요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100034824.1.16",
          "form": "그리고 다시 복습할 마음이 사라져요 ㅋㅋ",
          "original_form": "그리고 다시 복습할 마음이 사라져요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100034824.1.17",
          "form": "아 맞아요ㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034824.1.18",
          "form": "제일 싫었던게",
          "original_form": "제일 싫었던게",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034824.1.19",
          "form": "하이킥 결말 ㅣ.....ㅠㅠ",
          "original_form": "하이킥 결말 ㅣ.....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034824.1.20",
          "form": "아 진짜 하 .. 충격받았어요",
          "original_form": "아 진짜 하 .. 충격받았어요",
          "speaker_id": "2",
