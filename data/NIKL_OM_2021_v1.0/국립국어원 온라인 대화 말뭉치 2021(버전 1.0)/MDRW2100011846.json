      "utterance": [
        {
          "id": "MDRW2100011846.1.1",
          "form": "다른 동물 좋아하는거 있어?",
          "original_form": "다른 동물 좋아하는거 있어?",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011846.1.2",
          "form": "고슴도치 엄청 귀엽더라",
          "original_form": "고슴도치 엄청 귀엽더라",
          "speaker_id": "1",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011846.1.3",
          "form": "근데 냄새 많이 나잖아",
          "original_form": "근데 냄새 많이 나잖아",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011846.1.4",
          "form": "고슴도치 냄새 많이 나?",
          "original_form": "고슴도치 냄새 많이 나?",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011846.1.5",
          "form": "나도 안키워봐서 잘모르는데 냄새 좀 날걸?",
          "original_form": "나도 안키워봐서 잘모르는데 냄새 좀 날걸?",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011846.1.6",
          "form": "난 강아지 냄새만 맡아봤어",
          "original_form": "난 강아지 냄새만 맡아봤어",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011846.1.7",
          "form": "난 강아지도 안맡아봤어",
          "original_form": "난 강아지도 안맡아봤어",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011846.1.8",
          "form": "개 키우는 친구네 집 가 본 적 없어?",
          "original_form": "개 키우는 친구네 집 가 본 적 없어?",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011846.1.9",
          "form": "있긴 한데 안만져봐서",
          "original_form": "있긴 한데 안만져봐서",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011846.1.10",
          "form": "냄새는 나던데",
          "original_form": "냄새는 나던데",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011846.1.11",
          "form": "냄새는 많이 나던데",
          "original_form": "냄새는 많이 나던데",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011846.1.12",
          "form": "그니까 나는 냄새 때문에라도 못 키우겠더라",
          "original_form": "그니까 나는 냄새 때문에라도 못 키우겠더라",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011846.1.13",
          "form": "그니까 그래도 고양이는 키워보고 싶어",
          "original_form": "그니까 그래도 고양이는 키워보고 싶어",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011846.1.14",
          "form": "고양이 냄새는 맡아본 적 없는 거 같애",
          "original_form": "고양이 냄새는 맡아본 적 없는 거 같애",
          "speaker_id": "1",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100011846.1.15",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100011846.1.16",
          "form": "난 고양이 카페 가봤어",
          "original_form": "난 고양이 카페 가봤어",
          "speaker_id": "1",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100011846.1.17",
          "form": "만화 카펜데 고양이가 있었어",
          "original_form": "만화 카펜데 고양이가 있었어",
          "speaker_id": "1",
