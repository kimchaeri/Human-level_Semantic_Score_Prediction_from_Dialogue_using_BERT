      "utterance": [
        {
          "id": "MDRW2100039989.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039989.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039989.1.3",
          "form": "공연보는거 좋아하세요?",
          "original_form": "공연보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039989.1.4",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039989.1.5",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039989.1.6",
          "form": "네 최근에 본거있나요",
          "original_form": "네 최근에 본거있나요",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100039989.1.7",
          "form": "최근에는 유튜브로 무한도전 옛날에 공연한거 봤어요",
          "original_form": "최근에는 유튜브로 무한도전 옛날에 공연한거 봤어요",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100039989.1.8",
          "form": "직접가본거는 ㅠ코시국이라 오래됐네요",
          "original_form": "직접가본거는 ㅠ코시국이라 오래됐네요",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100039989.1.9",
          "form": "공연 최근에 본거있으신가요?",
          "original_form": "공연 최근에 본거있으신가요?",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100039989.1.10",
          "form": "무한도전 저도 맨날봐요ㅋㅋ",
          "original_form": "무한도전 저도 맨날봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100039989.1.11",
          "form": "공연은 저도 올해는 못봤네요ㅠ",
          "original_form": "공연은 저도 올해는 못봤네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100039989.1.12",
          "form": "맞아요.ㅋㅋ저두요.ㅋㅋ옛날꺼가 재미있어서요ㅋㅋ",
          "original_form": "맞아요.ㅋㅋ저두요.ㅋㅋ옛날꺼가 재미있어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100039989.1.13",
          "form": "코로나때문에 공연도 거의없구 있다해도",
          "original_form": "코로나때문에 공연도 거의없구 있다해도",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100039989.1.14",
          "form": "찝찝해서 못가겠더라구요",
          "original_form": "찝찝해서 못가겠더라구요",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100039989.1.15",
          "form": "전 식당같은곳보단 공연장이나 영화관이 더 맘편하더라구요",
          "original_form": "전 식당같은곳보단 공연장이나 영화관이 더 맘편하더라구요",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100039989.1.16",
          "form": "공연장은 못가도 영화관은 자주갔어요",
          "original_form": "공연장은 못가도 영화관은 자주갔어요",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100039989.1.17",
          "form": "사람도없고 있어봤자 다들 입다물고 가만있으니ㅋㅋ",
          "original_form": "사람도없고 있어봤자 다들 입다물고 가만있으니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100039989.1.18",
          "form": "네 맞아요ㅋ",
          "original_form": "네 맞아요ㅋ",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100039989.1.19",
          "form": "영화관은 갈만하더라구요ㅋ",
          "original_form": "영화관은 갈만하더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100039989.1.20",
          "form": "영화는 뭐 보셨나요?",
          "original_form": "영화는 뭐 보셨나요?",
          "speaker_id": "1",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100039989.1.21",
          "form": "모가디슈요ㅋㅋ",
          "original_form": "모가디슈요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100039989.1.22",
          "form": "사람들이 없어서ㅋ",
          "original_form": "사람들이 없어서ㅋ",
          "speaker_id": "2",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100039989.1.23",
          "form": "한번 더 가려구요ㅋ",
          "original_form": "한번 더 가려구요ㅋ",
          "speaker_id": "2",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100039989.1.24",
          "form": "진짜 사람없어서 좋아요ㅋ",
          "original_form": "진짜 사람없어서 좋아요ㅋ",
          "speaker_id": "1",
          "time": "20210904 13:53"
        }
      ]
    }
