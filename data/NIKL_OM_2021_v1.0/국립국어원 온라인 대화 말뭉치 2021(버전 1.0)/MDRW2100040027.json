      "utterance": [
        {
          "id": "MDRW2100040027.1.1",
          "form": "어떤 다큐멘터리 볼 건데?",
          "original_form": "어떤 다큐멘터리 볼 건데?",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100040027.1.2",
          "form": "몰라 보다가 중간에 하차한 건 엄청 많은데..",
          "original_form": "몰라 보다가 중간에 하차한 건 엄청 많은데..",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100040027.1.3",
          "form": "케이틀린 제너 다큐랑 f1 다큐도 보다가 말았어",
          "original_form": "케이틀린 제너 다큐랑 f1 다큐도 보다가 말았어",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100040027.1.4",
          "form": "F1 다큐멘터리는 재밌더라",
          "original_form": "F1 다큐멘터리는 재밌더라",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100040027.1.5",
          "form": "그치? 근데 시즌이 너무 길고.. f1 자체에 관심이 안 생겨서 끝까지 못 보겠더라구",
          "original_form": "그치? 근데 시즌이 너무 길고.. f1 자체에 관심이 안 생겨서 끝까지 못 보겠더라구",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100040027.1.6",
          "form": "911 테러 관련 된 다큐도 떴네",
          "original_form": "911 테러 관련 된 다큐도 떴네",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100040027.1.7",
          "form": "신작인가봐",
          "original_form": "신작인가봐",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100040027.1.8",
          "form": "제목이 뭐야??",
          "original_form": "제목이 뭐야??",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100040027.1.9",
          "form": "터닝포인트 - 9/11 그리고 테러와의 전쟁",
          "original_form": "터닝포인트 - 9/11 그리고 테러와의 전쟁",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100040027.1.10",
          "form": "이거야",
          "original_form": "이거야",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100040027.1.11",
          "form": "찜해놓고 나중에 봐야겠다..",
          "original_form": "찜해놓고 나중에 봐야겠다..",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100040027.1.12",
          "form": "이렇게 찜만 해놓은 작품이 수십개...ㅎ",
          "original_form": "이렇게 찜만 해놓은 작품이 수십개...ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100040027.1.13",
          "form": "원래 뭘 처음 보겠다고 고르는 게 제일 어려워.",
          "original_form": "원래 뭘 처음 보겠다고 고르는 게 제일 어려워.",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100040027.1.14",
          "form": "한번 보기만 하면 끝까지 금방 다 보는데 말야.",
          "original_form": "한번 보기만 하면 끝까지 금방 다 보는데 말야.",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100040027.1.15",
          "form": "응ㅠㅠ 그래서 맨날 본 거 또 보고 또 보고..",
          "original_form": "응ㅠㅠ 그래서 맨날 본 거 또 보고 또 보고..",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100040027.1.16",
          "form": "빅뱅이론이랑 모팸 재탕만 수십번 한 듯ㅋㅋㅋ",
          "original_form": "빅뱅이론이랑 모팸 재탕만 수십번 한 듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040027.1.17",
          "form": "D.P. 도 재밌대",
          "original_form": "D.P. 도 재밌대",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040027.1.18",
          "form": "정해인 나오는 드라마",
          "original_form": "정해인 나오는 드라마",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040027.1.19",
          "form": "아 그 군대 드라마?",
          "original_form": "아 그 군대 드라마?",
          "speaker_id": "1",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040027.1.20",
          "form": "욕이 너무 많이 나온대서 안 봣는데..",
          "original_form": "욕이 너무 많이 나온대서 안 봣는데..",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040027.1.21",
          "form": "그래도 15세 관람가던데.",
          "original_form": "그래도 15세 관람가던데.",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040027.1.22",
          "form": "나중에 봐봐",
          "original_form": "나중에 봐봐",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040027.1.23",
          "form": "넷플에 호텔 델루나도 업데이트 됐네",
