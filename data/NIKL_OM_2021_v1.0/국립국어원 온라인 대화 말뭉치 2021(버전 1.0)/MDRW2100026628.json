      "utterance": [
        {
          "id": "MDRW2100026628.1.1",
          "form": "홈베이킹을 하면 주변인이 좋아요",
          "original_form": "홈베이킹을 하면 주변인이 좋아요",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026628.1.2",
          "form": "맞아요 완전",
          "original_form": "맞아요 완전",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026628.1.3",
          "form": "갬동받아요",
          "original_form": "갬동받아요",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026628.1.4",
          "form": "ㅋㅋ죄송 제가 지금 베이킹하며 이거하느라",
          "original_form": "ㅋㅋ죄송 제가 지금 베이킹하며 이거하느라",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026628.1.5",
          "form": "지금 수플레 케이크를 꺼냈는데 껍데기가 딱딱하네요",
          "original_form": "지금 수플레 케이크를 꺼냈는데 껍데기가 딱딱하네요",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026628.1.6",
          "form": "이럼 안되는데,,,",
          "original_form": "이럼 안되는데,,,",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026628.1.7",
          "form": "헐대박",
          "original_form": "헐대박",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026628.1.8",
          "form": "요리에 진심이시군여",
          "original_form": "요리에 진심이시군여",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026628.1.9",
          "form": "name1 지인들은 너무좋겠어요",
          "original_form": "&name1& 지인들은 너무좋겠어요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026628.1.10",
          "form": "망한 요리를 들이밀댄",
          "original_form": "망한 요리를 들이밀댄",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026628.1.11",
          "form": "다들 싫어해서",
          "original_form": "다들 싫어해서",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026628.1.12",
          "form": "망한 요리는 제가 다 먹고",
          "original_form": "망한 요리는 제가 다 먹고",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026628.1.13",
          "form": "ㅠ 맛있는건 잘 받아가더군요",
          "original_form": "ㅠ 맛있는건 잘 받아가더군요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026628.1.14",
          "form": "힝구네요",
          "original_form": "힝구네요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026628.1.15",
          "form": "그래도 만들어주는 정성이 너무 고마울것같아요",
          "original_form": "그래도 만들어주는 정성이 너무 고마울것같아요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026628.1.16",
          "form": "음,, 만드는 사람의 입장에서 말씀드리면",
          "original_form": "음,, 만드는 사람의 입장에서 말씀드리면",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026628.1.17",
          "form": "저의 만드는 쾌락을 위해 주변인이 이용당하는 면도 있죠ㅋㅋ",
          "original_form": "저의 만드는 쾌락을 위해 주변인이 이용당하는 면도 있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026628.1.18",
          "form": "케켘켘",
          "original_form": "케켘켘",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026628.1.19",
          "form": "윈윈인데 너무좋죠",
          "original_form": "윈윈인데 너무좋죠",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026628.1.20",
          "form": "베이킹할땐 둘째치고 설거지가 힘들어서",
          "original_form": "베이킹할땐 둘째치고 설거지가 힘들어서",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026628.1.21",
          "form": "시작하는게 고민되기도 하는것같아요ㅋㅋ",
          "original_form": "시작하는게 고민되기도 하는것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026628.1.22",
          "form": "전 도구구매가 좀 망설여졌어요",
          "original_form": "전 도구구매가 좀 망설여졌어요",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026628.1.23",
          "form": "그래서 아직도 오븐이 없어요",
          "original_form": "그래서 아직도 오븐이 없어요",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026628.1.24",
          "form": "오븐... 꽤 맘을 크게먹어야 살것같아요!!!",
          "original_form": "오븐... 꽤 맘을 크게먹어야 살것같아요!!!",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026628.1.25",
          "form": "에프로도 아직 빵은 구워지는데,",
          "original_form": "에프로도 아직 빵은 구워지는데,",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026628.1.26",
          "form": "비록 좀 오븐보다 망하고",
          "original_form": "비록 좀 오븐보다 망하고",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026628.1.27",
          "form": "울수 있는 양도 병아리 눈곱만하지만",
          "original_form": "울수 있는 양도 병아리 눈곱만하지만",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026628.1.28",
          "form": "구울",
          "original_form": "구울",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026628.1.29",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026628.1.30",
          "form": "딱딱한 수플레케이크",
          "original_form": "딱딱한 수플레케이크",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026628.1.31",
          "form": "시식해보셨나요??",
          "original_form": "시식해보셨나요??",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026628.1.32",
          "form": "식어야 틀에서 빼서",
          "original_form": "식어야 틀에서 빼서",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026628.1.33",
          "form": "아지 식히고 있습니다 ㅋㅋ",
          "original_form": "아지 식히고 있습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026628.1.34",
          "form": "궁금하당",
          "original_form": "궁금하당",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026628.1.35",
