      "utterance": [
        {
          "id": "MDRW2100013301.1.1",
          "form": "그쵸 그건 좋아여^^",
          "original_form": "그쵸 그건 좋아여^^",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013301.1.2",
          "form": "한끼는 안차려도 되니깐~",
          "original_form": "한끼는 안차려도 되니깐~",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013301.1.3",
          "form": "나도과제없이 살고싶네",
          "original_form": "나도과제없이 살고싶네",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013301.1.4",
          "form": "레포트 쓰기 빡신데",
          "original_form": "레포트 쓰기 빡신데",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013301.1.5",
          "form": "레포트 돈주고 살라해도 없던데;;",
          "original_form": "레포트 돈주고 살라해도 없던데;;",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013301.1.6",
          "form": "레포트, 시험 둘다 싫네요ㅠ",
          "original_form": "레포트, 시험 둘다 싫네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013301.1.7",
          "form": "그자 대충해야지",
          "original_form": "그자 대충해야지",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013301.1.8",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013301.1.9",
          "form": "엉망진창  이다",
          "original_form": "엉망진창  이다",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013301.1.10",
          "form": "60점만 넘음 된데요 ㅋㅋㅋㅋ지도 대충햤으요~",
          "original_form": "60점만 넘음 된데요 ㅋㅋㅋㅋ지도 대충햤으요~",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013301.1.11",
          "form": "시험도 진짜 어렵게 내는 교수있데요..정말 짜증남..",
          "original_form": "시험도 진짜 어렵게 내는 교수있데요..정말 짜증남..",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013301.1.12",
          "form": "우린80 ㅋ",
          "original_form": "우린80 ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013301.1.13",
          "form": "아...왜그런데요??",
          "original_form": "아...왜그런데요??",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013301.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ느무 하네ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ느무 하네ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013301.1.15",
          "form": "그자? 겁나빡심",
          "original_form": "그자? 겁나빡심",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013301.1.16",
          "form": "왜그래요??교육기관이 달라서 카나??",
          "original_form": "왜그래요??교육기관이 달라서 카나??",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013301.1.17",
          "form": "20점 차이면 큰데ㅠㅠ",
          "original_form": "20점 차이면 큰데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013301.1.18",
          "form": "ㅋㅋ 다시 시작이넷",
          "original_form": "ㅋㅋ 다시 시작이넷",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013301.1.19",
          "form": "학번도까먹음",
          "original_form": "학번도까먹음",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013301.1.20",
          "form": "난도 기억안나요 ㅋㅋㅋㅋㅋ",
          "original_form": "난도 기억안나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013301.1.21",
          "form": "진짜 일하면서 공부하기란..ㅠㅠ 살림도 살아야하니ㅠ",
          "original_form": "진짜 일하면서 공부하기란..ㅠㅠ 살림도 살아야하니ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013301.1.22",
          "form": "거기다 주식까지",
          "original_form": "거기다 주식까지",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100013301.1.23",
          "form": "많다많다",
          "original_form": "많다많다",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100013301.1.24",
          "form": "공부",
          "original_form": "공부",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100013301.1.25",
          "form": "그렇네요~언닌.배움의.연속이구만요~",
          "original_form": "그렇네요~언닌.배움의.연속이구만요~",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100013301.1.26",
          "form": "잘할수 있어요 화이팅^^",
          "original_form": "잘할수 있어요 화이팅^^",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100013301.1.27",
          "form": "엉ㅋ 학점 잘받아보쟈ㅋ",
          "original_form": "엉ㅋ 학점 잘받아보쟈ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100013301.1.28",
          "form": "지금도  공부중ㅋ",
          "original_form": "지금도  공부중ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100013301.1.29",
          "form": "언니 멋지네여~진짜 하루가ㅜ늠 짧다ㅠ",
          "original_form": "언니 멋지네여~진짜 하루가ㅜ늠 짧다ㅠ",
