      "utterance": [
        {
          "id": "MDRW2100009484.1.1",
          "form": "마지막에잘려서 못봤는데ㅋㅋ",
          "original_form": "마지막에잘려서 못봤는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009484.1.2",
          "form": "어디까지 보셨어요?",
          "original_form": "어디까지 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009484.1.3",
          "form": "name2님 드라마 좋아하신다고ㅋ",
          "original_form": "&name2&님 드라마 좋아하신다고ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009484.1.4",
          "form": "저 3회정도 보다 내용넘 이상하길래 보다말다했거든요ㅎ",
          "original_form": "저 3회정도 보다 내용넘 이상하길래 보다말다했거든요ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009484.1.5",
          "form": "로건리 살아있는거보고",
          "original_form": "로건리 살아있는거보고",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009484.1.6",
          "form": "지지난주까지 저도 코미디인가",
          "original_form": "지지난주까지 저도 코미디인가",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009484.1.7",
          "form": "이건머지 하면서 보다가",
          "original_form": "이건머지 하면서 보다가",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009484.1.8",
          "form": "지난주는 그래도 괜찮더라구요",
          "original_form": "지난주는 그래도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009484.1.9",
          "form": "온주완은 대체 누구편이예요?",
          "original_form": "온주완은 대체 누구편이예요?",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009484.1.10",
          "form": "그냥 돈밝히는 도박꾼",
          "original_form": "그냥 돈밝히는 도박꾼",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009484.1.11",
          "form": "알고보니 천서진이 매수했더라구요",
          "original_form": "알고보니 천서진이 매수했더라구요",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009484.1.12",
          "form": "돈주는사람편ㅋㅋ",
          "original_form": "돈주는사람편ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009484.1.13",
          "form": "그 제니아빠는요?",
          "original_form": "그 제니아빠는요?",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009484.1.14",
          "form": "제니아빠는 약점잡혀서 주단태밑에 있다가 복수한다고 주단태 잡는데 일조하고 자수하고 감옥갔어요",
          "original_form": "제니아빠는 약점잡혀서 주단태밑에 있다가 복수한다고 주단태 잡는데 일조하고 자수하고 감옥갔어요",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009484.1.15",
          "form": "아 또감옥갔군요",
          "original_form": "아 또감옥갔군요",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009484.1.16",
          "form": "자기가 오윤희 시체를 다시 갖다놓은걸",
          "original_form": "자기가 오윤희 시체를 다시 갖다놓은걸",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009484.1.17",
          "form": "했으니 그걸자수하고간것같아요",
          "original_form": "했으니 그걸자수하고간것같아요",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009484.1.18",
          "form": "그게 오윤희가 아닐수 있단거죠?",
          "original_form": "그게 오윤희가 아닐수 있단거죠?",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009484.1.19",
          "form": "추측이죠ㅋㅋㅋ",
          "original_form": "추측이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009484.1.20",
          "form": "그 펜트하우스에 벽에 시체가",
          "original_form": "그 펜트하우스에 벽에 시체가",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009484.1.21",
          "form": "하나더있긴했거든요",
          "original_form": "하나더있긴했거든요",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009484.1.22",
          "form": "그거까진 아시나요?ㅋ",
          "original_form": "그거까진 아시나요?ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009484.1.23",
          "form": "까도까도 계속나오네요",
          "original_form": "까도까도 계속나오네요",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009484.1.24",
          "form": "그것도 못봤어요ㅎㅎ",
          "original_form": "그것도 못봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009484.1.25",
          "form": "아 그시체는 젤첨에 심수련이 자기앤지 알고 병간호 하던애 엄마요",
          "original_form": "아 그시체는 젤첨에 심수련이 자기앤지 알고 병간호 하던애 엄마요",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009484.1.26",
          "form": "애바꿔주고 주단태 약점잡아서 돈 매번빼먹다가",
          "original_form": "애바꿔주고 주단태 약점잡아서 돈 매번빼먹다가",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009484.1.27",
          "form": "아 그아줌마 죽은거아니예요?",
          "original_form": "아 그아줌마 죽은거아니예요?",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009484.1.28",
          "form": "벽에 있던시체죠?",
          "original_form": "벽에 있던시체죠?",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009484.1.29",
          "form": "석경이가 심수련딸인거 알고 크게한탕 할라다가 주단태가 죽이고",
          "original_form": "석경이가 심수련딸인거 알고 크게한탕 할라다가 주단태가 죽이고",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009484.1.30",
          "form": "네그시체",
          "original_form": "네그시체",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009484.1.31",
          "form": "그시체랑 바꿔치기 된거아닐까라는",
          "original_form": "그시체랑 바꿔치기 된거아닐까라는",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009484.1.32",
          "form": "그러기엔 너무썩었나.",
          "original_form": "그러기엔 너무썩었나.",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009484.1.33",
          "form": "아ㅎ 담주부턴 좀 챙겨봐야겠네요ㅋ",
          "original_form": "아ㅎ 담주부턴 좀 챙겨봐야겠네요ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009484.1.34",
          "form": "그건 제생각이었는데",
          "original_form": "그건 제생각이었는데",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009484.1.35",
          "form": "아닐수도있겠네요 시간이 많이지났으니ㅋㅋㅋ",
          "original_form": "아닐수도있겠네요 시간이 많이지났으니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009484.1.36",
          "form": "작가가 상상이상의 장면을 만드니",
          "original_form": "작가가 상상이상의 장면을 만드니",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009484.1.37",
          "form": "그럼 그시체는어디간건가",
          "original_form": "그럼 그시체는어디간건가",
          "speaker_id": "2",
          "time": "20210823 00:00"
        }
      ]
    }
