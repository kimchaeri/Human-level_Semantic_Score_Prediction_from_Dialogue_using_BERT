      "utterance": [
        {
          "id": "MDRW2100031323.1.1",
          "form": "픽사영화는 어떤거 좋아하시나용",
          "original_form": "픽사영화는 어떤거 좋아하시나용",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031323.1.2",
          "form": "저 픽사영화 다 좋아하는데 소울이 가장 최근에 본 영화라서",
          "original_form": "저 픽사영화 다 좋아하는데 소울이 가장 최근에 본 영화라서",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031323.1.3",
          "form": "가장 기억과 감정이 생생하네요 ㅋㅋㅋㅋ",
          "original_form": "가장 기억과 감정이 생생하네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031323.1.4",
          "form": "허걱 소울 좋죠",
          "original_form": "허걱 소울 좋죠",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031323.1.5",
          "form": "뭔가 그 푸른곰팡이같은애들이",
          "original_form": "뭔가 그 푸른곰팡이같은애들이",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031323.1.6",
          "form": "너무 몽글몽글하게생겨서",
          "original_form": "너무 몽글몽글하게생겨서",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031323.1.7",
          "form": "귀엽더라구요ㅠ",
          "original_form": "귀엽더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031323.1.8",
          "form": "특히 고양이가",
          "original_form": "특히 고양이가",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031323.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ푸른곰팡이 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ푸른곰팡이 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031323.1.10",
          "form": "맞아요 고양이도 귀여웠어요",
          "original_form": "맞아요 고양이도 귀여웠어요",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031323.1.11",
          "form": "그 천국(?) 선생님이랑 악역도 너무 귀여웠어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 천국(?) 선생님이랑 악역도 너무 귀여웠어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031323.1.12",
          "form": "ㅋㅋㅋㅋㅋ그니깐요",
          "original_form": "ㅋㅋㅋㅋㅋ그니깐요",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031323.1.13",
          "form": "픽사는 ost 노래들도 다 너무좋아요ㅠㅠ",
          "original_form": "픽사는 ost 노래들도 다 너무좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100031323.1.14",
          "form": "좋죠ㅠㅠㅠ",
          "original_form": "좋죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031323.1.15",
          "form": "눈물나는듯한 그런기분",
          "original_form": "눈물나는듯한 그런기분",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031323.1.16",
          "form": "그쵸 ㅠㅠㅠㅠㅠㅠ영화 보고 나오면 ost 돌려듣구 ㅜㅜㅜㅜ코코도 ost 좋았어요 리멤버미ㅠㅠㅠ",
          "original_form": "그쵸 ㅠㅠㅠㅠㅠㅠ영화 보고 나오면 ost 돌려듣구 ㅜㅜㅜㅜ코코도 ost 좋았어요 리멤버미ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031323.1.17",
          "form": "으악 코코 너무좋아요",
          "original_form": "으악 코코 너무좋아요",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031323.1.18",
          "form": "코코도 진짜.. 어떻게 그런 상상을 했지 너무 신기해요",
          "original_form": "코코도 진짜.. 어떻게 그런 상상을 했지 너무 신기해요",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031323.1.19",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031323.1.20",
          "form": "빨리 디즈니 플러스 들어와서 다시보고싶네요 ㅠㅠ",
          "original_form": "빨리 디즈니 플러스 들어와서 다시보고싶네요 ㅠㅠ",
