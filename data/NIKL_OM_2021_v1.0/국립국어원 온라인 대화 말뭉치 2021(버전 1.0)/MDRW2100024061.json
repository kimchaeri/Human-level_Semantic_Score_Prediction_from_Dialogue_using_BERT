      "utterance": [
        {
          "id": "MDRW2100024061.1.1",
          "form": "여행 자주 다니셨나요?",
          "original_form": "여행 자주 다니셨나요?",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024061.1.2",
          "form": "인직 못가본곳이 많아서",
          "original_form": "인직 못가본곳이 많아서",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024061.1.3",
          "form": "많다고는 말을 못하겠어요",
          "original_form": "많다고는 말을 못하겠어요",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024061.1.4",
          "form": "name2님은 어떠세요?",
          "original_form": "&name2&님은 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024061.1.5",
          "form": "저도 가볼데가 백만배많죠 ㅋㅋㅋㅋ",
          "original_form": "저도 가볼데가 백만배많죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024061.1.6",
          "form": "최근 어디 다니셨나요....?",
          "original_form": "최근 어디 다니셨나요....?",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100024061.1.7",
          "form": "이번 휴가때 부산 해운대에서 호캉스했어요",
          "original_form": "이번 휴가때 부산 해운대에서 호캉스했어요",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100024061.1.8",
          "form": "먹으면서 보내는 여행",
          "original_form": "먹으면서 보내는 여행",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100024061.1.9",
          "form": "name2님은 가장 최근 여행지가 어디신가요?",
          "original_form": "&name2&님은 가장 최근 여행지가 어디신가요?",
          "speaker_id": "1",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100024061.1.10",
          "form": "ㅇ 좋네요 저희 간지 오래 ㅠ 마지막 간건 경주요 봄쯤",
          "original_form": "ㅇ 좋네요 저희 간지 오래 ㅠ 마지막 간건 경주요 봄쯤",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100024061.1.11",
          "form": "저 오늘 경주 다녀왔는데",
          "original_form": "저 오늘 경주 다녀왔는데",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100024061.1.12",
          "form": "황리단길",
          "original_form": "황리단길",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100024061.1.13",
          "form": "오늘 황리단길에서 오스테리아 밀즈라는 식당에서",
          "original_form": "오늘 황리단길에서 오스테리아 밀즈라는 식당에서",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100024061.1.14",
          "form": "오호 그쪽 사세요?",
          "original_form": "오호 그쪽 사세요?",
          "speaker_id": "2",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100024061.1.15",
          "form": "리조또랑 파스타 먹었어요",
          "original_form": "리조또랑 파스타 먹었어요",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100024061.1.16",
          "form": "울산 살거든요",
          "original_form": "울산 살거든요",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100024061.1.17",
          "form": "우오ㅏ 그립네요 경주",
          "original_form": "우오ㅏ 그립네요 경주",
          "speaker_id": "2",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100024061.1.18",
          "form": "와이프랑 데이트 다녀왔어요",
          "original_form": "와이프랑 데이트 다녀왔어요",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100024061.1.19",
          "form": "경주 좋죠 해갈 갈수록 더 좋아지는 것 같아요",
          "original_form": "경주 좋죠 해갈 갈수록 더 좋아지는 것 같아요",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100024061.1.20",
          "form": "도시도 계속 정비하고",
          "original_form": "도시도 계속 정비하고",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100024061.1.21",
          "form": "가까운가봐요",
          "original_form": "가까운가봐요",
          "speaker_id": "2",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100024061.1.22",
          "form": "저희 집에서 황리단길까지",
          "original_form": "저희 집에서 황리단길까지",
          "speaker_id": "1",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100024061.1.23",
          "form": "차로 한시간 정도 걸려요",
          "original_form": "차로 한시간 정도 걸려요",
          "speaker_id": "1",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100024061.1.24",
          "form": "아 딱 적당하네요",
          "original_form": "아 딱 적당하네요",
          "speaker_id": "2",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100024061.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100024061.1.26",
          "form": "오늘 날씨도 제법 선선해서 좋았어요",
          "original_form": "오늘 날씨도 제법 선선해서 좋았어요",
          "speaker_id": "1",
          "time": "20210904 20:19"
        }
      ]
    }
