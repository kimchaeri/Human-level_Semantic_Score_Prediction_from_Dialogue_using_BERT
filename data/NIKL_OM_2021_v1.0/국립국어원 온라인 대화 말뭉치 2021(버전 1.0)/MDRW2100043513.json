      "utterance": [
        {
          "id": "MDRW2100043513.1.1",
          "form": "치킨중에 깐풍기 너무맛잇어",
          "original_form": "치킨중에 깐풍기 너무맛잇어",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.2",
          "form": "아 바삭바삭하쥬",
          "original_form": "아 바삭바삭하쥬",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.3",
          "form": "글고 양념도 너무 맛잇더라",
          "original_form": "글고 양념도 너무 맛잇더라",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.4",
          "form": "먼가 탕수만두같지?",
          "original_form": "먼가 탕수만두같지?",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.5",
          "form": "헐 마쥬",
          "original_form": "헐 마쥬",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.6",
          "form": "탕수만두 좋아해 나두",
          "original_form": "탕수만두 좋아해 나두",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.7",
          "form": "비슷한데 또 달라",
          "original_form": "비슷한데 또 달라",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.8",
          "form": "마저마저",
          "original_form": "마저마저",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043513.1.9",
          "form": "글고 깐풍기는 튀김이잖아",
          "original_form": "글고 깐풍기는 튀김이잖아",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043513.1.10",
          "form": "맞아 그니깐 바삭하지!",
          "original_form": "맞아 그니깐 바삭하지!",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043513.1.11",
          "form": "진짜 튀김이 꿀맛",
          "original_form": "진짜 튀김이 꿀맛",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043513.1.12",
          "form": "살찌지만 어쩔수없어ㅠ",
          "original_form": "살찌지만 어쩔수없어ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043513.1.13",
          "form": "그러니깐말야",
          "original_form": "그러니깐말야",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043513.1.14",
          "form": "포기가 안돼",
          "original_form": "포기가 안돼",
          "speaker_id": "1",
          "time": "20210907 19:23"
        }
      ]
