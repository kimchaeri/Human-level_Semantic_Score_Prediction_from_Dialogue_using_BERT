      "utterance": [
        {
          "id": "MDRW2100032590.1.1",
          "form": "검진가나?\r\n아빠랑?",
          "original_form": "검진가나?\r\n아빠랑?",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032590.1.2",
          "form": "부산백병원 진료보러 엄마아빠 다 간당",
          "original_form": "부산백병원 진료보러 엄마아빠 다 간당",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032590.1.3",
          "form": "나 태워주는김에 드라이브나 하고오게",
          "original_form": "나 태워주는김에 드라이브나 하고오게",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032590.1.4",
          "form": "가족 다같이 즐겁겠당~",
          "original_form": "가족 다같이 즐겁겠당~",
          "speaker_id": "2",
          "time": "20210906 00:21"
        },
        {
          "id": "MDRW2100032590.1.5",
          "form": "가봐야알듯 가도 가볍게 드라이브하기 좋은곳 별로 없더라구",
          "original_form": "가봐야알듯 가도 가볍게 드라이브하기 좋은곳 별로 없더라구",
          "speaker_id": "1",
          "time": "20210906 00:22"
        },
        {
          "id": "MDRW2100032590.1.6",
          "form": "부산이니까 바다옆도로로 달림 좋겠다ㅎ",
          "original_form": "부산이니까 바다옆도로로 달림 좋겠다ㅎ",
          "speaker_id": "2",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100032590.1.7",
          "form": "ㅇㅇ그렇게할라고",
          "original_form": "ㅇㅇ그렇게할라고",
          "speaker_id": "1",
          "time": "20210906 08:21"
        },
        {
          "id": "MDRW2100032590.1.8",
          "form": "응 잘다녀와",
          "original_form": "응 잘다녀와",
          "speaker_id": "2",
          "time": "20210906 08:33"
        },
        {
          "id": "MDRW2100032590.1.9",
          "form": "몇시 출발해?",
          "original_form": "몇시 출발해?",
          "speaker_id": "2",
          "time": "20210906 09:56"
        },
        {
          "id": "MDRW2100032590.1.10",
          "form": "아홉시 오십분쯤 출발해서 지금 도착했어",
          "original_form": "아홉시 오십분쯤 출발해서 지금 도착했어",
          "speaker_id": "1",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100032590.1.11",
          "form": "빠르게 도착했네ㅎ",
          "original_form": "빠르게 도착했네ㅎ",
          "speaker_id": "2",
          "time": "20210906 11:35"
        },
        {
          "id": "MDRW2100032590.1.12",
          "form": "ㅜㅜ 대기하는시간이 넘 기네 오늘따라 빨리 밥먹고 드라이브 하고싶은데",
          "original_form": "ㅜㅜ 대기하는시간이 넘 기네 오늘따라 빨리 밥먹고 드라이브 하고싶은데",
          "speaker_id": "1",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100032590.1.13",
          "form": "무한대기가?",
          "original_form": "무한대기가?",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100032590.1.14",
          "form": "이제나왔어! 이제부산 여기저기 다녀야지",
          "original_form": "이제나왔어! 이제부산 여기저기 다녀야지",
          "speaker_id": "1",
          "time": "20210906 12:31"
        },
        {
          "id": "MDRW2100032590.1.15",
          "form": "ㅎㅎㅎ그래도 오래 걸리진않았네",
          "original_form": "ㅎㅎㅎ그래도 오래 걸리진않았네",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100032590.1.16",
          "form": "맛난점심 재미난 시간보내겠네~",
          "original_form": "맛난점심 재미난 시간보내겠네~",
          "speaker_id": "2",
          "time": "20210906 12:48"
        }
      ]
