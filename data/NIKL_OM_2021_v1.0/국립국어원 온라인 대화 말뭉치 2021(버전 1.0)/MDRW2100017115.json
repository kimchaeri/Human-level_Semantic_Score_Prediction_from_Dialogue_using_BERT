        {
          "id": "MDRW2100017115.1.1",
          "form": "음 name2 강아지는 지금 무엇을 하고 있는 중이야",
          "original_form": "음 &name2& 강아지는 지금 무엇을 하고 있는 중이야",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017115.1.2",
          "form": "퍼질러 누워있을거야",
          "original_form": "퍼질러 누워있을거야",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017115.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ귀여워ㅠㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ귀여워ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017115.1.4",
          "form": "강아지는 좋겠다",
          "original_form": "강아지는 좋겠다",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017115.1.5",
          "form": "집에 갈때쯤에는 미리 발코니에서 기다리더라",
          "original_form": "집에 갈때쯤에는 미리 발코니에서 기다리더라",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017115.1.6",
          "form": "똑똑한 name2 강아지이",
          "original_form": "똑똑한 &name2& 강아지이",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017115.1.7",
          "form": "마중도 나오고",
          "original_form": "마중도 나오고",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017115.1.8",
          "form": "귀여운 애기 강아지",
          "original_form": "귀여운 애기 강아지",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017115.1.9",
          "form": "꼬리도 흔들어줘요",
          "original_form": "꼬리도 흔들어줘요",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017115.1.10",
          "form": "웰시코기가 꼬리 흔드는 생각을 하니",
          "original_form": "웰시코기가 꼬리 흔드는 생각을 하니",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017115.1.11",
          "form": "꿍디만 생각나네",
          "original_form": "꿍디만 생각나네",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017115.1.12",
          "form": "나름 작은 꼬리 흔들다가",
          "original_form": "나름 작은 꼬리 흔들다가",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017115.1.13",
          "form": "귀여옹",
          "original_form": "귀여옹",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017115.1.14",
          "form": "넘 반가우면 꿍디 흔들어",
          "original_form": "넘 반가우면 꿍디 흔들어",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017115.1.15",
          "form": "ㅋㅋㅋㅋㅋ꿍디는 나름 2단계구나",
          "original_form": "ㅋㅋㅋㅋㅋ꿍디는 나름 2단계구나",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017115.1.16",
          "form": "그렇지 나름 체계적이야ㅋㅋ",
          "original_form": "그렇지 나름 체계적이야ㅋㅋ",
