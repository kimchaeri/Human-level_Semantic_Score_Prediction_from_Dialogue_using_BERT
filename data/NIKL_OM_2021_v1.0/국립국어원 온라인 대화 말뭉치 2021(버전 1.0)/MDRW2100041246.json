      "utterance": [
        {
          "id": "MDRW2100041246.1.1",
          "form": "오케이광자매 보고있어요",
          "original_form": "오케이광자매 보고있어요",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041246.1.2",
          "form": "오늘도 광자매네요ㅋㅋㅋ",
          "original_form": "오늘도 광자매네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041246.1.3",
          "form": "볼만해요?",
          "original_form": "볼만해요?",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041246.1.4",
          "form": "이거보고 갯마을 볼거에요",
          "original_form": "이거보고 갯마을 볼거에요",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041246.1.5",
          "form": "엄마가 보셔서 같이 봐요",
          "original_form": "엄마가 보셔서 같이 봐요",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041246.1.6",
          "form": "name1은 머봐요",
          "original_form": "&name1&은 머봐요",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041246.1.7",
          "form": "name1는 코빅보고있져여",
          "original_form": "&name1&는 코빅보고있져여",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041246.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041246.1.9",
          "form": "아하 코빅이군요",
          "original_form": "아하 코빅이군요",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041246.1.10",
          "form": "갯차는 보세여?",
          "original_form": "갯차는 보세여?",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041246.1.11",
          "form": "갯차는안봤는데\r\n어제 게시판에서 김선호짤보고",
          "original_form": "갯차는안봤는데\r\n어제 게시판에서 김선호짤보고",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041246.1.12",
          "form": "반했자나여.ㅈ",
          "original_form": "반했자나여.ㅈ",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041246.1.13",
          "form": "김선호 핵존잘",
          "original_form": "김선호 핵존잘",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100041246.1.14",
          "form": "진짜 대존잘",
          "original_form": "진짜 대존잘",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100041246.1.15",
          "form": "보조개에 빠지고싶어여",
          "original_form": "보조개에 빠지고싶어여",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100041246.1.16",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041246.1.17",
          "form": "맞아요ㅠㅠ 허우적",
          "original_form": "맞아요ㅠㅠ 허우적",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041246.1.18",
          "form": "세상엔 왜케 잘생긴 사람 많을가",
          "original_form": "세상엔 왜케 잘생긴 사람 많을가",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041246.1.19",
          "form": "그러게..주위에ㅣㄴ없어",
          "original_form": "그러게..주위에ㅣㄴ없어",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041246.1.20",
          "form": "유니콘",
