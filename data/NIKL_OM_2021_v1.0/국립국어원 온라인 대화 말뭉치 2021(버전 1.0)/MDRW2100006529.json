      "utterance": [
        {
          "id": "MDRW2100006529.1.1",
          "form": "미용은 좀 관심 있어?",
          "original_form": "미용은 좀 관심 있어?",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100006529.1.2",
          "form": "ㅋㅋㅋㅋ나 잘 모르잖아 미용 같은거",
          "original_form": "ㅋㅋㅋㅋ나 잘 모르잖아 미용 같은거",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100006529.1.3",
          "form": "관심은 있는데 잘 몰라",
          "original_form": "관심은 있는데 잘 몰라",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100006529.1.4",
          "form": "넌 미용 쪽에 관심 많잖아",
          "original_form": "넌 미용 쪽에 관심 많잖아",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006529.1.5",
          "form": "머리 같은거 아직도 기르는거?그럼",
          "original_form": "머리 같은거 아직도 기르는거?그럼",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006529.1.6",
          "form": "아니야 자르고 다녀",
          "original_form": "아니야 자르고 다녀",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006529.1.7",
          "form": "기르는거 별로였구나 ㅋㅋㅋㅋㅋ",
          "original_form": "기르는거 별로였구나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006529.1.8",
          "form": "ㅋㅋㅋㅋㅋ아닠ㅋㅋㅋㅋㅋ 왜 저렇게 기르지 했었지",
          "original_form": "ㅋㅋㅋㅋㅋ아닠ㅋㅋㅋㅋㅋ 왜 저렇게 기르지 했었지",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006529.1.9",
          "form": "근데 너말고도 기르는 동기들 꽤 있더라구",
          "original_form": "근데 너말고도 기르는 동기들 꽤 있더라구",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006529.1.10",
          "form": "ㅋㅋㅋㅋㅋ말하지 바로 잘랐을텐데",
          "original_form": "ㅋㅋㅋㅋㅋ말하지 바로 잘랐을텐데",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006529.1.11",
          "form": "군대 가기 전이었으니까 아무래도",
          "original_form": "군대 가기 전이었으니까 아무래도",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006529.1.12",
          "form": "아는척은 안했지맠ㅋㅋ 지나가다 보면서 오 ?",
          "original_form": "아는척은 안했지맠ㅋㅋ 지나가다 보면서 오 ?",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006529.1.13",
          "form": "싶었음 우와..!",
          "original_form": "싶었음 우와..!",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006529.1.14",
          "form": "내가 유행시켰어!!ㅋㅋㅋㅋㅋ그렇다고 하자",
          "original_form": "내가 유행시켰어!!ㅋㅋㅋㅋㅋ그렇다고 하자",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006529.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ그게 좋아?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그게 좋아?",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100006529.1.16",
          "form": "그럼 그렇게햌ㅋㅋ너가 유행시킨 긴머맄ㅋㅋㅋ",
          "original_form": "그럼 그렇게햌ㅋㅋ너가 유행시킨 긴머맄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006529.1.17",
          "form": "응응 뭔가 아이콘이 된 느낌이잖아 ㅋㅋㅋ",
          "original_form": "응응 뭔가 아이콘이 된 느낌이잖아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006529.1.18",
          "form": "지금도 긴건진 모르겠네",
          "original_form": "지금도 긴건진 모르겠네",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006529.1.19",
          "form": "딱 한번봐가지구",
          "original_form": "딱 한번봐가지구",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006529.1.20",
          "form": "지금?나??",
          "original_form": "지금?나??",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006529.1.21",
          "form": "와서 봐 ㅋㅋㅋㅋㅋ",
          "original_form": "와서 봐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006529.1.22",
          "form": "ㅋㅋㅋ아니 동기들",
          "original_form": "ㅋㅋㅋ아니 동기들",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006529.1.23",
          "form": "동기들 머리는 나도 모르겠다",
          "original_form": "동기들 머리는 나도 모르겠다",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006529.1.24",
          "form": "난 그 당시 과 홍일점이 꿈이었어",
          "original_form": "난 그 당시 과 홍일점이 꿈이었어",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006529.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006529.1.26",
          "form": "ㅋㅋㅋㅋ뭐랰ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ뭐랰ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006529.1.27",
          "form": "안타깝다.. 청일점두 아니고",
          "original_form": "안타깝다.. 청일점두 아니고",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006529.1.28",
          "form": "ㅋㅋㅋㅋ제일 예뻐지려했지ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ제일 예뻐지려했지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006529.1.29",
          "form": "청일점은 살짝 힘들지 않을까 싶어서",
          "original_form": "청일점은 살짝 힘들지 않을까 싶어서",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100006529.1.30",
          "form": "홍일점이라도!ㅋㅋㅋㅋㅋ",
          "original_form": "홍일점이라도!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100006529.1.31",
          "form": "경쟁자가 적잖아",
          "original_form": "경쟁자가 적잖아",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100006529.1.32",
          "form": "너 핑크 머리나 봤어야하는데",
          "original_form": "너 핑크 머리나 봤어야하는데",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100006529.1.33",
