        },
        {
          "id": "MDRW2100013631.1.3",
          "form": "응 이번 휴가에 여행 다녀왔지",
          "original_form": "응 이번 휴가에 여행 다녀왔지",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013631.1.4",
          "form": "우리 그때 갔던 부산 좋았지",
          "original_form": "우리 그때 갔던 부산 좋았지",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013631.1.5",
          "form": "맞아 근데 비가 많이와서 ㅜㅜ",
          "original_form": "맞아 근데 비가 많이와서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013631.1.6",
          "form": "맞아 비가 많이 와서 흐렸어",
          "original_form": "맞아 비가 많이 와서 흐렸어",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013631.1.7",
          "form": "아쉬웠지만 또 가고싶네",
          "original_form": "아쉬웠지만 또 가고싶네",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013631.1.8",
          "form": "여행 가면 주로 기념품으로 어떤걸사?",
          "original_form": "여행 가면 주로 기념품으로 어떤걸사?",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013631.1.9",
          "form": "자석이나 엽서 너는?",
          "original_form": "자석이나 엽서 너는?",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013631.1.10",
          "form": "나도 자석이 좋아  엽서는 왠지 1회용같아서",
          "original_form": "나도 자석이 좋아  엽서는 왠지 1회용같아서",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013631.1.11",
          "form": "사진이라서 걸어놓으면 더 좋은데",
          "original_form": "사진이라서 걸어놓으면 더 좋은데",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013631.1.12",
          "form": "아 그렇게 쓸 수도 있구나? 꿀팁이네",
          "original_form": "아 그렇게 쓸 수도 있구나? 꿀팁이네",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013631.1.13",
          "form": "난 기념품으로 펜도좋아",
          "original_form": "난 기념품으로 펜도좋아",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013631.1.14",
          "form": "펜은 안사봐서 모르겠네",
          "original_form": "펜은 안사봐서 모르겠네",
