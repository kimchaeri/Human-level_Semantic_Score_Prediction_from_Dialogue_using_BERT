      "utterance": [
        {
          "id": "MDRW2100017940.1.1",
          "form": "동호회나 봉사하러 다니세요?",
          "original_form": "동호회나 봉사하러 다니세요?",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017940.1.2",
          "form": "음 지금은 하고있는 것은 없어요.\n혹시 하시는거 있으신가요",
          "original_form": "음 지금은 하고있는 것은 없어요.\n혹시 하시는거 있으신가요",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017940.1.3",
          "form": "아니요 없어요! 예전엔 어떤거 하셧어요?",
          "original_form": "아니요 없어요! 예전엔 어떤거 하셧어요?",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100017940.1.4",
          "form": "저 예전에 사물놀이 했었어요ㅋㅋㄲ",
          "original_form": "저 예전에 사물놀이 했었어요ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100017940.1.5",
          "form": "아ㅋㅋㅋㅋㅋ아주어릴때한건가요?",
          "original_form": "아ㅋㅋㅋㅋㅋ아주어릴때한건가요?",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017940.1.6",
          "form": "너무 어릴 때는 아니구요ㅋㅋ",
          "original_form": "너무 어릴 때는 아니구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100017940.1.7",
          "form": "재밌는거 하재서 따라갔더니 사물놀이더라구요",
          "original_form": "재밌는거 하재서 따라갔더니 사물놀이더라구요",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100017940.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ아 누구따라간거에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 누구따라간거에요",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100017940.1.9",
          "form": "아는 분 따라갔더니 징을 쥐어주시더라구요",
          "original_form": "아는 분 따라갔더니 징을 쥐어주시더라구요",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017940.1.10",
          "form": "보통 북이나 장구 주잖아요",
          "original_form": "보통 북이나 장구 주잖아요",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017940.1.11",
          "form": "아니면 꾕과리나..",
          "original_form": "아니면 꾕과리나..",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100017940.1.12",
          "form": "그렇죠 두개가 메인이긴한데ㅋㅋㅋㅋ",
          "original_form": "그렇죠 두개가 메인이긴한데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017940.1.13",
          "form": "얼떨결에 사물놀이하셧네요ㅋㅋㅋ",
          "original_form": "얼떨결에 사물놀이하셧네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100017940.1.14",
          "form": "얼마나하셨어요?",
          "original_form": "얼마나하셨어요?",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100017940.1.15",
          "form": "한 3년?",
          "original_form": "한 3년?",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100017940.1.16",
          "form": "했던거 같아요",
          "original_form": "했던거 같아요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100017940.1.17",
          "form": "네..?ㅋㅋㅋㅋㅋ생각보다 오래하셨네요",
          "original_form": "네..?ㅋㅋㅋㅋㅋ생각보다 오래하셨네요",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017940.1.18",
          "form": "ㅋㅋㅋ 그쵸 저도 그렇게 오래하게될 줄 몰랐어요",
          "original_form": "ㅋㅋㅋ 그쵸 저도 그렇게 오래하게될 줄 몰랐어요",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100017940.1.19",
          "form": "대회나 이런데도 나가셨어요?",
          "original_form": "대회나 이런데도 나가셨어요?",
