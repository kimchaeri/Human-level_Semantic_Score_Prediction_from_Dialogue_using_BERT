      "utterance": [
        {
          "id": "MDRW2100042364.1.1",
          "form": "난 바깥쪽 허벅지 할때 너무 아프더라",
          "original_form": "난 바깥쪽 허벅지 할때 너무 아프더라",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042364.1.2",
          "form": "아아아아ㅏㅏㅏㅏㅏㅏ ㅇㅈㅇㅈㅇㅈㅇㅈ",
          "original_form": "아아아아ㅏㅏㅏㅏㅏㅏ ㅇㅈㅇㅈㅇㅈㅇㅈ",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042364.1.3",
          "form": "맞제",
          "original_form": "맞제",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042364.1.4",
          "form": "진짜 지옥임 그거",
          "original_form": "진짜 지옥임 그거",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042364.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.6",
          "form": "앞쪽 뒤쪽은 시원하고 좋은데",
          "original_form": "앞쪽 뒤쪽은 시원하고 좋은데",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.7",
          "form": "근데 진짜 몸한테 미안해짐",
          "original_form": "근데 진짜 몸한테 미안해짐",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.8",
          "form": "옆벅지 진짜....",
          "original_form": "옆벅지 진짜....",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.9",
          "form": "맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.10",
          "form": "그거 목도 너무 아파",
          "original_form": "그거 목도 너무 아파",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.11",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.12",
          "form": "목도 폼롤러해??",
          "original_form": "목도 폼롤러해??",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.13",
          "form": "응 목도 하면 좋대",
          "original_form": "응 목도 하면 좋대",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.15",
          "form": "우리 목이 약간 곡선이잖아",
          "original_form": "우리 목이 약간 곡선이잖아",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.16",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042364.1.17",
          "form": "그래서 목에 수건 두르고",
          "original_form": "그래서 목에 수건 두르고",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042364.1.18",
          "form": "끄덕끄덕 하는거랑",
          "original_form": "끄덕끄덕 하는거랑",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042364.1.19",
          "form": "폼롤러 목 뒤에 대고 누워서",
          "original_form": "폼롤러 목 뒤에 대고 누워서",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042364.1.20",
          "form": "옆으로 도리도리 하는게 좋대",
          "original_form": "옆으로 도리도리 하는게 좋대",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042364.1.21",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042364.1.22",
          "form": "특히 나는 거북목이라서",
          "original_form": "특히 나는 거북목이라서",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042364.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042364.1.24",
          "form": "목에 힘이 들어간거 풀어준다 그랬던것 같아",
          "original_form": "목에 힘이 들어간거 풀어준다 그랬던것 같아",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042364.1.25",
          "form": "그것도 엄청 아파 ㅠ",
          "original_form": "그것도 엄청 아파 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042364.1.26",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042364.1.27",
          "form": "지금 해보겟습니다",
          "original_form": "지금 해보겟습니다",
          "speaker_id": "1",
          "time": "20210906 21:53"
        }
