      "utterance": [
        {
          "id": "MDRW2100036843.1.1",
          "form": "네네. 추석때요??",
          "original_form": "네네. 추석때요??",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036843.1.2",
          "form": "남들 전먹을때",
          "original_form": "남들 전먹을때",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036843.1.3",
          "form": "ㅋㅋㅋㅋㅋ네네",
          "original_form": "ㅋㅋㅋㅋㅋ네네",
          "speaker_id": "1",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.4",
          "form": "저는 서양전 먹으러갈거에요!",
          "original_form": "저는 서양전 먹으러갈거에요!",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.5",
          "form": "저 어느 요리커뮤니티에서 찐 열정가진분들을봣어요.",
          "original_form": "저 어느 요리커뮤니티에서 찐 열정가진분들을봣어요.",
          "speaker_id": "1",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.6",
          "form": "진짜 딱 3주만 식단조절 하자며",
          "original_form": "진짜 딱 3주만 식단조절 하자며",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.7",
          "form": "오오 어떤분이요",
          "original_form": "오오 어떤분이요",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.8",
          "form": "막 화덕같은것도 직구하더라고요",
          "original_form": "막 화덕같은것도 직구하더라고요",
          "speaker_id": "1",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.9",
          "form": "세상에 그거 어디두죠?ㅋㅋ",
          "original_form": "세상에 그거 어디두죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.10",
          "form": "집에서 해먹겟다며..\r\n분명히 피자매니아임에 틀림엊ㅅ겟죠",
          "original_form": "집에서 해먹겟다며..\r\n분명히 피자매니아임에 틀림엊ㅅ겟죠",
          "speaker_id": "1",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036843.1.11",
          "form": "진짜대단해!",
          "original_form": "진짜대단해!",
          "speaker_id": "2",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100036843.1.12",
          "form": "마당에두려나",
          "original_form": "마당에두려나",
          "speaker_id": "2",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100036843.1.13",
          "form": "그게 원하는모양이아니라 그냥 팔기로햇대요 ㅋㅋ",
          "original_form": "그게 원하는모양이아니라 그냥 팔기로햇대요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100036843.1.14",
          "form": "아파트에서 가스설치 못한다더라고요 ㅋㅋㅋ",
          "original_form": "아파트에서 가스설치 못한다더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100036843.1.15",
          "form": "아니 원하는 모양도 있어 ㄷㄷㄷ",
          "original_form": "아니 원하는 모양도 있어 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100036843.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 그 막 화덕오븐도 사시고..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그 막 화덕오븐도 사시고..",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100036843.1.17",
          "form": "사실 진짜 마니아면 해먹는게 싸죠",
          "original_form": "사실 진짜 마니아면 해먹는게 싸죠",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100036843.1.18",
          "form": "몇분만에 확 익는게 큰 장점인것같고ㅋㅋㅋ",
          "original_form": "몇분만에 확 익는게 큰 장점인것같고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100036843.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 무슨메뉴가 최애세여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 무슨메뉴가 최애세여",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100036843.1.20",
          "form": "정말 궁금해지네여",
          "original_form": "정말 궁금해지네여",
          "speaker_id": "1",
          "time": "20210831 13:34"
        }
      ]
