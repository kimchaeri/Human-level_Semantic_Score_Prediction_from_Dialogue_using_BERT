      "utterance": [
        {
          "id": "MDRW2100009041.1.1",
          "form": "이번주 내내 비온대",
          "original_form": "이번주 내내 비온대",
          "speaker_id": "1",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009041.1.2",
          "form": "나 비올 때마다",
          "original_form": "나 비올 때마다",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009041.1.3",
          "form": "내 방 베란다에서 물새는데",
          "original_form": "내 방 베란다에서 물새는데",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009041.1.4",
          "form": "망함",
          "original_form": "망함",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009041.1.5",
          "form": "이사한 집?",
          "original_form": "이사한 집?",
          "speaker_id": "1",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100009041.1.6",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009041.1.7",
          "form": "근데 비 안와야 베란다 손 볼 수 있다고 해서",
          "original_form": "근데 비 안와야 베란다 손 볼 수 있다고 해서",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009041.1.8",
          "form": "걍 이번주는..",
          "original_form": "걍 이번주는..",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009041.1.9",
          "form": "ㅠㅠ 견적 뽑아봤나",
          "original_form": "ㅠㅠ 견적 뽑아봤나",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009041.1.10",
          "form": "모르겠음",
          "original_form": "모르겠음",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009041.1.11",
          "form": "근데 여기 집 주인이.. 와서 고쳐준다고 하긴 하는데",
          "original_form": "근데 여기 집 주인이.. 와서 고쳐준다고 하긴 하는데",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009041.1.12",
          "form": "우리집은 집 안에 물새는데",
          "original_form": "우리집은 집 안에 물새는데",
          "speaker_id": "1",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009041.1.13",
          "form": "그거.. 견적이 백~천단위임..",
          "original_form": "그거.. 견적이 백~천단위임..",
          "speaker_id": "1",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009041.1.14",
          "form": "엄두를 못 내는 중",
          "original_form": "엄두를 못 내는 중",
          "speaker_id": "1",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009041.1.15",
          "form": "그냥 천장 뚫린채로 바가지 받아놓고 있다",
          "original_form": "그냥 천장 뚫린채로 바가지 받아놓고 있다",
          "speaker_id": "1",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009041.1.16",
          "form": "백-천..?",
          "original_form": "백-천..?",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009041.1.17",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009041.1.18",
          "form": "이번주 비..",
          "original_form": "이번주 비..",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009041.1.19",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009041.1.20",
          "form": "진짜 임시방편으로 제일 싼 게 600이고",
          "original_form": "진짜 임시방편으로 제일 싼 게 600이고",
          "speaker_id": "1",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009041.1.21",
          "form": "물이 새는 걸 다 잡아야 돼서..",
          "original_form": "물이 새는 걸 다 잡아야 돼서..",
          "speaker_id": "1",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009041.1.22",
          "form": "600...",
          "original_form": "600...",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009041.1.23",
          "form": "결국 그 이후로 그 집 사람들",
          "original_form": "결국 그 이후로 그 집 사람들",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009041.1.24",
          "form": "모이지 않은거니..",
          "original_form": "모이지 않은거니..",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009041.1.25",
          "form": "응 8가구인데 1집 왔음",
          "original_form": "응 8가구인데 1집 왔음",
          "speaker_id": "1",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009041.1.26",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009041.1.27",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009041.1.28",
          "form": "어쩔려고 그러는건지..",
          "original_form": "어쩔려고 그러는건지..",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009041.1.29",
          "form": "자기집은 아직 괜찮다 그거지",
          "original_form": "자기집은 아직 괜찮다 그거지",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009041.1.30",
          "form": "태풍오면 어찌될줄 알고",
          "original_form": "태풍오면 어찌될줄 알고",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009041.1.31",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009041.1.32",
          "form": "빨리.. 이사를 가야 되는데",
          "original_form": "빨리.. 이사를 가야 되는데",
