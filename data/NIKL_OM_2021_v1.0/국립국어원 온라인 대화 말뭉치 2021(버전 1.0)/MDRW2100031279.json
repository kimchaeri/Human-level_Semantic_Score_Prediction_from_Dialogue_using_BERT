          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031279.1.9",
          "form": "ㅇㅇ어제찾아봄ㅋㅋ",
          "original_form": "ㅇㅇ어제찾아봄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100031279.1.10",
          "form": "ㅋㅋㅋ 밥 먹음??",
          "original_form": "ㅋㅋㅋ 밥 먹음??",
          "speaker_id": "2",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100031279.1.11",
          "form": "볼민힌게 읍네 요즘",
          "original_form": "볼민힌게 읍네 요즘",
          "speaker_id": "2",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100031279.1.12",
          "form": "지금먹엇서!",
          "original_form": "지금먹엇서!",
          "speaker_id": "1",
          "time": "20210905 13:04"
        },
        {
          "id": "MDRW2100031279.1.13",
          "form": "나는 화이트칙스보는중",
          "original_form": "나는 화이트칙스보는중",
          "speaker_id": "1",
          "time": "20210905 13:04"
        },
        {
          "id": "MDRW2100031279.1.14",
          "form": "밀키야",
          "original_form": "밀키야",
          "speaker_id": "2",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100031279.1.15",
          "form": "위글위글 텀블러 뭔데 이쁘냐",
          "original_form": "위글위글 텀블러 뭔데 이쁘냐",
          "speaker_id": "2",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100031279.1.16",
          "form": "그거 화이트 칙스 여장한 남자들아니덩가",
          "original_form": "그거 화이트 칙스 여장한 남자들아니덩가",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100031279.1.17",
          "form": "ㅇㅇ맞음ㅋㅋㅋㅋ여장경찰",
          "original_form": "ㅇㅇ맞음ㅋㅋㅋㅋ여장경찰",
          "speaker_id": "1",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100031279.1.18",
          "form": "위글위글이쁘지",
          "original_form": "위글위글이쁘지",
          "speaker_id": "1",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100031279.1.19",
          "form": "내 패드파우치도 위글위글",
          "original_form": "내 패드파우치도 위글위글",
          "speaker_id": "1",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100031279.1.20",
          "form": "텀블러 너무 귀여워서결제 할뻔",
