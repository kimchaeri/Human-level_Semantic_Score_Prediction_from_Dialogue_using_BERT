      "utterance": [
        {
          "id": "MDRW2100011571.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011571.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011571.1.3",
          "form": "메타버스에 대해 어떻게 생각하세요?",
          "original_form": "메타버스에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011571.1.4",
          "form": "재밋을거같애요",
          "original_form": "재밋을거같애요",
          "speaker_id": "2",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011571.1.5",
          "form": "전 신기했어요",
          "original_form": "전 신기했어요",
          "speaker_id": "1",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011571.1.6",
          "form": "가상의 세계잖아요",
          "original_form": "가상의 세계잖아요",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011571.1.7",
          "form": "가상세계에서는 현실에서 못한걸 할수있으니까..",
          "original_form": "가상세계에서는 현실에서 못한걸 할수있으니까..",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011571.1.8",
          "form": "근데 이 세계가 너무 커지면 현실과 구분하지 못하는 사람들이 생길까봐 조금 무서워요",
          "original_form": "근데 이 세계가 너무 커지면 현실과 구분하지 못하는 사람들이 생길까봐 조금 무서워요",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011571.1.9",
          "form": "그러니까요 게임중독처럼요...",
          "original_form": "그러니까요 게임중독처럼요...",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011571.1.10",
          "form": "현재 몇몇 아이돌들이 메타버스를 이용해서 세계관을 만들었잖아요",
          "original_form": "현재 몇몇 아이돌들이 메타버스를 이용해서 세계관을 만들었잖아요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011571.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011571.1.12",
          "form": "그렇게하면 무슨 이득이 있을까요?",
          "original_form": "그렇게하면 무슨 이득이 있을까요?",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011571.1.13",
          "form": "음.....",
          "original_form": "음.....",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011571.1.14",
          "form": "요즘에는 세계관 이용해서 굿즈도 내고 그래서 그런거 때문에 하는거 아닐까요?",
          "original_form": "요즘에는 세계관 이용해서 굿즈도 내고 그래서 그런거 때문에 하는거 아닐까요?",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011571.1.15",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011571.1.16",
          "form": "그렇네요!",
          "original_form": "그렇네요!",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011571.1.17",
          "form": "온라인으로 더 쉽게 연결되니까요!",
          "original_form": "온라인으로 더 쉽게 연결되니까요!",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011571.1.18",
          "form": "엑소는 초능력 그런거로 무늬 만들어서 굿즈도 많이내고 그러니까요!",
          "original_form": "엑소는 초능력 그런거로 무늬 만들어서 굿즈도 많이내고 그러니까요!",
          "speaker_id": "2",
          "time": "20210824 14:26"
        }
      ]
    }
