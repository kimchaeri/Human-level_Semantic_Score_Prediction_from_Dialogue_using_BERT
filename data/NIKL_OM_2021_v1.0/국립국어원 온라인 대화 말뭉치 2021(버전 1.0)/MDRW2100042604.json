      "utterance": [
        {
          "id": "MDRW2100042604.1.1",
          "form": "카레우동?",
          "original_form": "카레우동?",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042604.1.2",
          "form": "그건 망했어",
          "original_form": "그건 망했어",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042604.1.3",
          "form": "? 카레우동 먹을거 아녓어?",
          "original_form": "? 카레우동 먹을거 아녓어?",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042604.1.4",
          "form": "그냥 스파게티 소스에",
          "original_form": "그냥 스파게티 소스에",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042604.1.5",
          "form": "아닝왴ㅋㅋ",
          "original_form": "아닝왴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042604.1.6",
          "form": "감자에서",
          "original_form": "감자에서",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042604.1.7",
          "form": "쉰내가 나",
          "original_form": "쉰내가 나",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042604.1.8",
          "form": "하루지낫는데...",
          "original_form": "하루지낫는데...",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042604.1.9",
          "form": "여튼 그래서 스파게티로",
          "original_form": "여튼 그래서 스파게티로",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042604.1.10",
          "form": "대체하기로 했어",
          "original_form": "대체하기로 했어",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042604.1.11",
          "form": "헐 싹나옴?",
          "original_form": "헐 싹나옴?",
          "speaker_id": "1",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042604.1.12",
          "form": "고기를 볶고 소스를 부어서",
          "original_form": "고기를 볶고 소스를 부어서",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042604.1.13",
          "form": "소고기 파스타를",
          "original_form": "소고기 파스타를",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042604.1.14",
          "form": "먹을거야",
          "original_form": "먹을거야",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042604.1.15",
          "form": "감자가 싹나서",
          "original_form": "감자가 싹나서",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042604.1.16",
          "form": "스파게티하는거??",
          "original_form": "스파게티하는거??",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042604.1.17",
          "form": "싹은 안났고",
          "original_form": "싹은 안났고",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042604.1.18",
          "form": "해둔 카레였는데",
          "original_form": "해둔 카레였는데",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042604.1.19",
          "form": "쉰내 나",
          "original_form": "쉰내 나",
          "speaker_id": "2",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100042604.1.20",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042604.1.21",
          "form": "요즘 날도 글케 안더웟는데",
          "original_form": "요즘 날도 글케 안더웟는데",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042604.1.22",
          "form": "왜지...",
          "original_form": "왜지...",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042604.1.23",
          "form": "스파게티에 고기 넣엇니",
          "original_form": "스파게티에 고기 넣엇니",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100042604.1.24",
          "form": "원래는 카레 우동 하려고 했는데",
          "original_form": "원래는 카레 우동 하려고 했는데",
          "speaker_id": "2",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042604.1.25",
          "form": "카레가 쉬어서 스파게티 소스에 우동면인ㄷ니ㅣ",
          "original_form": "카레가 쉬어서 스파게티 소스에 우동면인ㄷ니ㅣ",
          "speaker_id": "2",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042604.1.26",
          "form": "심지어 스파게티 소스도 모자라서",
          "original_form": "심지어 스파게티 소스도 모자라서",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.27",
          "form": "아 우동면이야?",
          "original_form": "아 우동면이야?",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.28",
          "form": "물 좀 붓고 굴소스 섞어서",
          "original_form": "물 좀 붓고 굴소스 섞어서",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.29",
          "form": "어쩐지 통통해보이더라 ㅋㅋㅋㅋ",
          "original_form": "어쩐지 통통해보이더라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.30",
          "form": "뭔가의..... 볶음우동이",
          "original_form": "뭔가의..... 볶음우동이",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.31",
          "form": "되었어",
          "original_form": "되었어",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.32",
          "form": "맛은 있을것같은데",
          "original_form": "맛은 있을것같은데",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.33",
          "form": "그렇게 잡탕이 되엇을땐",
          "original_form": "그렇게 잡탕이 되엇을땐",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.34",
          "form": "ㅇㅇ우동면이야",
          "original_form": "ㅇㅇ우동면이야",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.36",
          "form": "고춧가루 한스푼 넣으면",
          "original_form": "고춧가루 한스푼 넣으면",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042604.1.37",
          "form": "시판 토마토소스와 굴소스가",
          "original_form": "시판 토마토소스와 굴소스가",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.38",
          "form": "아니면 보통 땡초",
          "original_form": "아니면 보통 땡초",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.39",
          "form": "일을 해줄거야",
          "original_form": "일을 해줄거야",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.40",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.41",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.42",
          "form": "넣으면 묘하게 어디서 먹어본거같은",
          "original_form": "넣으면 묘하게 어디서 먹어본거같은",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.43",
          "form": "지금 집에",
          "original_form": "지금 집에",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.44",
          "form": "있는게 없음",
          "original_form": "있는게 없음",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.45",
          "form": "맛이 탄생함 ㅋㅋㅋㅋ",
          "original_form": "맛이 탄생함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042604.1.46",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210906 23:49"
        }
      ]
