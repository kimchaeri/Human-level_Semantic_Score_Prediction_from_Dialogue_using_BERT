      "utterance": [
        {
          "id": "MDRW2100041968.1.1",
          "form": "언니 오늘은 일찍 설거지 끝냈어요??",
          "original_form": "언니 오늘은 일찍 설거지 끝냈어요??",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100041968.1.2",
          "form": "나는 일찍 마무리하고 애들 재울라고 했는데",
          "original_form": "나는 일찍 마무리하고 애들 재울라고 했는데",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100041968.1.3",
          "form": "원은 엄마랑 같이 자자 하고",
          "original_form": "원은 엄마랑 같이 자자 하고",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100041968.1.4",
          "form": "투는 오빠랑 둘이 자고 싶다고 나가라고 하고",
          "original_form": "투는 오빠랑 둘이 자고 싶다고 나가라고 하고",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100041968.1.5",
          "form": "성질 남",
          "original_form": "성질 남",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100041968.1.6",
          "form": "그럼 어떻게해야되노?ㅋㅋ나",
          "original_form": "그럼 어떻게해야되노?ㅋㅋ나",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041968.1.7",
          "form": "난 육퇴했다만",
          "original_form": "난 육퇴했다만",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041968.1.8",
          "form": "이제 널린 집안일해야지",
          "original_form": "이제 널린 집안일해야지",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041968.1.9",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041968.1.10",
          "form": "그래서 신랑 이재운다고 들어갔는데",
          "original_form": "그래서 신랑 이재운다고 들어갔는데",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041968.1.11",
          "form": "원은 좋아하고",
          "original_form": "원은 좋아하고",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041968.1.12",
          "form": "투는 아빠 나가라고 울고",
          "original_form": "투는 아빠 나가라고 울고",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041968.1.13",
