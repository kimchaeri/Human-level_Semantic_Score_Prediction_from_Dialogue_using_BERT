      "utterance": [
        {
          "id": "MDRW2100016518.1.1",
          "form": "여행 자주 다니시나요?",
          "original_form": "여행 자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016518.1.2",
          "form": "아니요 님은요?",
          "original_form": "아니요 님은요?",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016518.1.3",
          "form": "코로나 전엔 여행가는거 좋아했는데 요즘은 못가네요",
          "original_form": "코로나 전엔 여행가는거 좋아했는데 요즘은 못가네요",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016518.1.4",
          "form": "코로나 끝나면 어디가고 싶으세요?",
          "original_form": "코로나 끝나면 어디가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016518.1.5",
          "form": "혼자 대만가거나 친구들이랑 동남아 휴양지 가고싶네요ㅎㅎ",
          "original_form": "혼자 대만가거나 친구들이랑 동남아 휴양지 가고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016518.1.6",
          "form": "대만 가보고싶어요",
          "original_form": "대만 가보고싶어요",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016518.1.7",
          "form": "전 패키지로 가족들이랑 한 번 갑핬는데 아쉬워서 자유여행으로 또 가고 싶어요",
          "original_form": "전 패키지로 가족들이랑 한 번 갑핬는데 아쉬워서 자유여행으로 또 가고 싶어요",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016518.1.8",
          "form": "야시장도 다니고 구경도 더 하고",
          "original_form": "야시장도 다니고 구경도 더 하고",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016518.1.9",
          "form": "패키지는 별로인가요?",
          "original_form": "패키지는 별로인가요?",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016518.1.10",
          "form": "자유여행이 나름 헤매는 재미도 있고ㅋㅋ",
          "original_form": "자유여행이 나름 헤매는 재미도 있고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016518.1.11",
          "form": "기념품 구매하는 편이세요?",
          "original_form": "기념품 구매하는 편이세요?",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016518.1.12",
          "form": "주로 먹을걸 사요ㅋㅋ",
          "original_form": "주로 먹을걸 사요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016518.1.13",
          "form": "대만갔을땐 펑리수 사왔었어요",
          "original_form": "대만갔을땐 펑리수 사왔었어요",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016518.1.14",
          "form": "대만에 뭐가 맛있나요?",
          "original_form": "대만에 뭐가 맛있나요?",
          "speaker_id": "2",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016518.1.15",
          "form": "우육면도 괜찮고 샤브샤브류도 괜찮았어요",
          "original_form": "우육면도 괜찮고 샤브샤브류도 괜찮았어요",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016518.1.16",
          "form": "근데 망고빙수가 제일 맛있더라고요ㅋㅋ",
          "original_form": "근데 망고빙수가 제일 맛있더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:21"
        }
      ]
