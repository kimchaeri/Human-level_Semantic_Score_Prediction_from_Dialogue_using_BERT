      "utterance": [
        {
          "id": "MDRW2100014228.1.1",
          "form": "애들은 뭐하고 있어?",
          "original_form": "애들은 뭐하고 있어?",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014228.1.2",
          "form": "스토리 한국사 봐",
          "original_form": "스토리 한국사 봐",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014228.1.3",
          "form": "ㅎㅎ 고생이 많네",
          "original_form": "ㅎㅎ 고생이 많네",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014228.1.4",
          "form": "오늘의 마지막 미션이야",
          "original_form": "오늘의 마지막 미션이야",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014228.1.5",
          "form": "많이 봐두면 좋지~",
          "original_form": "많이 봐두면 좋지~",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014228.1.6",
          "form": "역사가 꽤 어려워서 미리 봐두면 용어라도 도움 되겠지 ㅎㅎ",
          "original_form": "역사가 꽤 어려워서 미리 봐두면 용어라도 도움 되겠지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014228.1.7",
          "form": "ㅇㅇ 책도 같이 보게 해~",
          "original_form": "ㅇㅇ 책도 같이 보게 해~",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014228.1.8",
          "form": "큰아들은 문제까지 풀게 하고 있어 ㅎㅎ",
          "original_form": "큰아들은 문제까지 풀게 하고 있어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014228.1.9",
          "form": "굿 잘하고 있네",
          "original_form": "굿 잘하고 있네",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014228.1.10",
          "form": "스토리 한국사는 2편 선생님이 더 재미있는 듯 해",
          "original_form": "스토리 한국사는 2편 선생님이 더 재미있는 듯 해",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014228.1.11",
          "form": "선생님 가리지 말고 1,2편 다 봐야지 ㅎㅎ",
          "original_form": "선생님 가리지 말고 1,2편 다 봐야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014228.1.12",
          "form": "오늘은 너무 힘들어서 같이 못봤네",
          "original_form": "오늘은 너무 힘들어서 같이 못봤네",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014228.1.13",
          "form": "애들만 보면 되지 뭐",
          "original_form": "애들만 보면 되지 뭐",
