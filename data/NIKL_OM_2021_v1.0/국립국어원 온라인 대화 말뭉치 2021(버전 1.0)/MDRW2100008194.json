        {
          "id": "MDRW2100008194.1.1",
          "form": "여행은 다녀오셨어요?",
          "original_form": "여행은 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008194.1.2",
          "form": "국내 조금씩 다녀왔어요",
          "original_form": "국내 조금씩 다녀왔어요",
          "speaker_id": "2",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008194.1.3",
          "form": "올해 어디다녔오셨어요?",
          "original_form": "올해 어디다녔오셨어요?",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008194.1.4",
          "form": "울릉도요ㅎㅎ",
          "original_form": "울릉도요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008194.1.5",
          "form": "오 울릉도 가보고싶던데 어때요?",
          "original_form": "오 울릉도 가보고싶던데 어때요?",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008194.1.6",
          "form": "좋아요ㅋㅋ 편안하고",
          "original_form": "좋아요ㅋㅋ 편안하고",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008194.1.7",
          "form": "지금 시국에서 베스트입니다",
          "original_form": "지금 시국에서 베스트입니다",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008194.1.8",
          "form": "사람없던가요?ㅎ",
          "original_form": "사람없던가요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008194.1.9",
          "form": "코로나라 암데도 못가고있어요ㅠㅠ",
          "original_form": "코로나라 암데도 못가고있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008194.1.10",
          "form": "아뇨 핵많던데요ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 핵많던데요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008194.1.11",
          "form": "그럼 베스트는 아닌거 아니예요?ㅎ",
          "original_form": "그럼 베스트는 아닌거 아니예요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008194.1.12",
          "form": "ㅋㅋ다른데 보다는 그래도. . .",
          "original_form": "ㅋㅋ다른데 보다는 그래도. . .",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008194.1.13",
          "form": "가족여행가신거예요?",
          "original_form": "가족여행가신거예요?",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008194.1.14",
          "form": "가는길 험난하지않아요?",
          "original_form": "가는길 험난하지않아요?",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008194.1.15",
          "form": "와이프랑 ㅎㅎ 배타고 한참 가요",
          "original_form": "와이프랑 ㅎㅎ 배타고 한참 가요",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008194.1.16",
          "form": "얼마나 타고 가요?",
          "original_form": "얼마나 타고 가요?",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008194.1.17",
          "form": "강릉에서 세시간정도 걸렸어요",
          "original_form": "강릉에서 세시간정도 걸렸어요",
          "speaker_id": "2",
