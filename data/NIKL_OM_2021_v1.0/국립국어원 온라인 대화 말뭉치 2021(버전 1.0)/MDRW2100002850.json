      "utterance": [
        {
          "id": "MDRW2100002850.1.1",
          "form": "뮤지컬 좋아하세요?",
          "original_form": "뮤지컬 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002850.1.2",
          "form": "네ㅋㅋㅋㅋ아이비 좋아해서 몇번봤어요ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ아이비 좋아해서 몇번봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002850.1.3",
          "form": "오 아이비 진짜 잘하더라구요",
          "original_form": "오 아이비 진짜 잘하더라구요",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002850.1.4",
          "form": "영상으로만 봤는데..",
          "original_form": "영상으로만 봤는데..",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002850.1.5",
          "form": "그쵸ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002850.1.6",
          "form": "레드북도 보러 가려구요!ㅋㅋㅋ",
          "original_form": "레드북도 보러 가려구요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002850.1.7",
          "form": "언제 보러가세요?? 티켓팅 안해본지도 오래됐네요 ㅎㅎㅎ",
          "original_form": "언제 보러가세요?? 티켓팅 안해본지도 오래됐네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100002850.1.8",
          "form": "다음주에 보러가요ㅋㅋㅋ",
          "original_form": "다음주에 보러가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:23"
        },
        {
          "id": "MDRW2100002850.1.9",
          "form": "레드북은 처음 보는거라서 엄청 기대되네요ㅋㅋㅋ",
          "original_form": "레드북은 처음 보는거라서 엄청 기대되네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100002850.1.10",
          "form": "콘서트는 많이 가보셨나요??",
          "original_form": "콘서트는 많이 가보셨나요??",
          "speaker_id": "2",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100002850.1.11",
          "form": "많이는 아니고 몇번요 ㅎㅎ",
          "original_form": "많이는 아니고 몇번요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100002850.1.12",
          "form": "마지막 콘서트는 앤마리!!",
          "original_form": "마지막 콘서트는 앤마리!!",
          "speaker_id": "1",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100002850.1.13",
          "form": "우와ㅋㅋㅋㅋ부러워요ㅠㅜㅜ",
          "original_form": "우와ㅋㅋㅋㅋ부러워요ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100002850.1.14",
          "form": "앤마리라니ㅋㅋㅋ전 콘서트는 딱 한 번 가봤어요",
          "original_form": "앤마리라니ㅋㅋㅋ전 콘서트는 딱 한 번 가봤어요",
          "speaker_id": "2",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100002850.1.15",
          "form": "ft아일랜드꺼ㅋㅋㅋㅋㅋ",
          "original_form": "ft아일랜드꺼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100002850.1.16",
          "form": "앤마리 라이브 진짜 잘해요 ㅋㅋㅋㅋㅋ",
          "original_form": "앤마리 라이브 진짜 잘해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:36"
        },
        {
          "id": "MDRW2100002850.1.17",
          "form": "에프티아일랜드 ... 추억이네요..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "에프티아일랜드 ... 추억이네요..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:36"
        },
        {
          "id": "MDRW2100002850.1.18",
          "form": "ㅋㅋㅋ친구가 엄청 팬이어서 따라 가본 게 다예요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ친구가 엄청 팬이어서 따라 가본 게 다예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100002850.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 앤마리 공연에 친구 끌고갔는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 앤마리 공연에 친구 끌고갔는데",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002850.1.20",
          "form": "친구가 입덕해서 뿌듯했어요..",
          "original_form": "친구가 입덕해서 뿌듯했어요..",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002850.1.21",
          "form": "공연가고싶네요ㅠㅠ",
          "original_form": "공연가고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002850.1.22",
          "form": "락페스티벌같은거..",
          "original_form": "락페스티벌같은거..",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002850.1.23",
          "form": "와 저 한번도 안 가봤어요ㅋㅋㄱ유튜버들 간 것만 영상으로 봤어요ㅋㅋ",
          "original_form": "와 저 한번도 안 가봤어요ㅋㅋㄱ유튜버들 간 것만 영상으로 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:12"
        },
        {
          "id": "MDRW2100002850.1.24",
          "form": "락을 안좋아하셔도 락페스티벌은 꼭 가보세요!! 야외공연만의 즐거움이 잇답니다..",
          "original_form": "락을 안좋아하셔도 락페스티벌은 꼭 가보세요!! 야외공연만의 즐거움이 잇답니다..",
