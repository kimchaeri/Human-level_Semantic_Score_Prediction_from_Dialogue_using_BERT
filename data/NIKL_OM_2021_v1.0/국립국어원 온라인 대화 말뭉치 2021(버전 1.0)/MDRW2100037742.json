      "utterance": [
        {
          "id": "MDRW2100037742.1.1",
          "form": "이제 설거지 한다",
          "original_form": "이제 설거지 한다",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037742.1.2",
          "form": "벌써먹였나 좋다ㅋ",
          "original_form": "벌써먹였나 좋다ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037742.1.3",
          "form": "나는 김밥해줬다ㅋㅋ",
          "original_form": "나는 김밥해줬다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037742.1.4",
          "form": "거의 다먹어감ㅋ",
          "original_form": "거의 다먹어감ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037742.1.5",
          "form": "카시트 커버 분리세탁하고",
          "original_form": "카시트 커버 분리세탁하고",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037742.1.6",
          "form": "신랑 이불세탁했다ㅋㅋ",
          "original_form": "신랑 이불세탁했다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037742.1.7",
          "form": "어후 카시트는 장난 아닌데",
          "original_form": "어후 카시트는 장난 아닌데",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037742.1.8",
          "form": "오늘 큰일 했네",
          "original_form": "오늘 큰일 했네",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037742.1.9",
          "form": "난 이제 마지막 관문",
          "original_form": "난 이제 마지막 관문",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037742.1.10",
          "form": "양치하러 간다",
          "original_form": "양치하러 간다",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037742.1.11",
          "form": "응 언니 양치하고와ㅋ",
          "original_form": "응 언니 양치하고와ㅋ",
          "speaker_id": "1",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100037742.1.12",
          "form": "나는 잘준비끝ㅋ",
          "original_form": "나는 잘준비끝ㅋ",
          "speaker_id": "1",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100037742.1.13",
          "form": "둘이 자라고 하고 나왔다",
          "original_form": "둘이 자라고 하고 나왔다",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037742.1.14",
          "form": "둘이 잠든지 이틀째",
          "original_form": "둘이 잠든지 이틀째",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037742.1.15",
          "form": "다행히 투가 오빠랑 자는 건 너무 좋아해서",
          "original_form": "다행히 투가 오빠랑 자는 건 너무 좋아해서",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037742.1.16",
          "form": "원이 맨날 무섭다고 질질 짜는데",
          "original_form": "원이 맨날 무섭다고 질질 짜는데",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037742.1.17",
          "form": "원더 투가 좋아하니 그냥 자는 모양",
          "original_form": "원더 투가 좋아하니 그냥 자는 모양",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037742.1.18",
          "form": "오 좋다좋다ㅋㅋ",
          "original_form": "오 좋다좋다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037742.1.19",
          "form": "나는 진짜 열받는다. 1시간째 장난치고",
          "original_form": "나는 진짜 열받는다. 1시간째 장난치고",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037742.1.20",
          "form": "아놔ㅡ",
          "original_form": "아놔ㅡ",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037742.1.21",
          "form": "지가 눕다가 머리 바닥에  부딪혔는데 모르겠다카고 안방옴.  씅난다.",
          "original_form": "지가 눕다가 머리 바닥에  부딪혔는데 모르겠다카고 안방옴.  씅난다.",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037742.1.22",
          "form": "하아.............",
          "original_form": "하아.............",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037742.1.23",
          "form": "성질나나....",
          "original_form": "성질나나....",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037742.1.24",
          "form": "성질난다",
          "original_form": "성질난다",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037742.1.25",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037742.1.26",
          "form": "진짜 계속 웃으며 장난치다가 지가 부딪혀서운다. 아빠찾고",
          "original_form": "진짜 계속 웃으며 장난치다가 지가 부딪혀서운다. 아빠찾고",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037742.1.27",
          "form": "두찌랑 같이 못자것다",
          "original_form": "두찌랑 같이 못자것다",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037742.1.28",
          "form": "스트레스받아서",
          "original_form": "스트레스받아서",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037742.1.29",
          "form": "어후..아직\r\n잘 시간이 아닌가...",
          "original_form": "어후..아직\r\n잘 시간이 아닌가...",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037742.1.30",
          "form": "주말에는 낮잠자나??",
          "original_form": "주말에는 낮잠자나??",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037742.1.31",
          "form": "낮잠안자면 몇시에 자노??",
          "original_form": "낮잠안자면 몇시에 자노??",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037742.1.32",
          "form": "주말에 낮잠안자고",
          "original_form": "주말에 낮잠안자고",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037742.1.33",
          "form": "보통6~7시에 잠들지ㅜ",
          "original_form": "보통6~7시에 잠들지ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037742.1.34",
          "form": "근데 낮잠자도5~6시에 잠들면",
          "original_form": "근데 낮잠자도5~6시에 잠들면",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037742.1.35",
          "form": "12시에자고",
          "original_form": "12시에자고",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037742.1.36",
          "form": "그래 낮잠시간이 늦어지면 또 늦지",
          "original_form": "그래 낮잠시간이 늦어지면 또 늦지",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037742.1.37",
          "form": "응 그라이",
          "original_form": "응 그라이",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037742.1.38",
          "form": "4살에는 낮잠 재우면 안되겠다",
          "original_form": "4살에는 낮잠 재우면 안되겠다",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037742.1.39",
          "form": "안잔다,  누워있어두ㅜ미쳐ㅜ",
          "original_form": "안잔다,  누워있어두ㅜ미쳐ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037742.1.40",
          "form": "화가나서 달래기도싫네",
          "original_form": "화가나서 달래기도싫네",
          "speaker_id": "1",
          "time": "20210901 21:31"
        }
      ]
    }
  ]
