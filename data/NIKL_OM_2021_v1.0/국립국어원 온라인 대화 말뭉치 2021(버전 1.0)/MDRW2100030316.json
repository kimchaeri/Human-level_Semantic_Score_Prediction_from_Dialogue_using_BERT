      "utterance": [
        {
          "id": "MDRW2100030316.1.1",
          "form": "아 학굥/ㅐ기네",
          "original_form": "아 학굥/ㅐ기네",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030316.1.2",
          "form": "거..학교생활은 잘 하고 있니",
          "original_form": "거..학교생활은 잘 하고 있니",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030316.1.3",
          "form": "원서접수는 잘 준비하고 있고?",
          "original_form": "원서접수는 잘 준비하고 있고?",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030316.1.4",
          "form": "이제 자기소개서 써야함",
          "original_form": "이제 자기소개서 써야함",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030316.1.5",
          "form": "9월 10일부터 수시 원서 쓴다",
          "original_form": "9월 10일부터 수시 원서 쓴다",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030316.1.6",
          "form": "잘 써봐라... 난 대학 갈 때 자소서 안 써서 잘 모름 ㅎㅎ",
          "original_form": "잘 써봐라... 난 대학 갈 때 자소서 안 써서 잘 모름 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030316.1.7",
          "form": "와 진짜 얼마 안 남았네 고기 먹고 힘내서 써",
          "original_form": "와 진짜 얼마 안 남았네 고기 먹고 힘내서 써",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030316.1.8",
          "form": "오예ㅎㅎ",
          "original_form": "오예ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030316.1.9",
          "form": "누난 복학 내년에 하나?",
          "original_form": "누난 복학 내년에 하나?",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.10",
          "form": "웅 그때까지 미친듯이 놀아야 돼",
          "original_form": "웅 그때까지 미친듯이 놀아야 돼",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.11",
          "form": "복학하면..,, 놀기 힘들다..",
          "original_form": "복학하면..,, 놀기 힘들다..",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.12",
          "form": "ㅠㅠ 3학년 때부터 졸작 준비 시작이야",
          "original_form": "ㅠㅠ 3학년 때부터 졸작 준비 시작이야",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.13",
          "form": "글치",
          "original_form": "글치",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.14",
          "form": "나중에 나 졸작 하면 졸작 구경 와",
          "original_form": "나중에 나 졸작 하면 졸작 구경 와",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.15",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.16",
          "form": "아 잘못하면 너 군대 가있겠다",
          "original_form": "아 잘못하면 너 군대 가있겠다",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030316.1.17",
          "form": "졸작은 어케 함?",
          "original_form": "졸작은 어케 함?",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030316.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030316.1.19",
          "form": "ㅋㅋ 나 웹툰 몇 화 그리거나 아니면 영상 촬영하고 특수효과 넣고..쓸디 손대기?? 그거 해",
          "original_form": "ㅋㅋ 나 웹툰 몇 화 그리거나 아니면 영상 촬영하고 특수효과 넣고..쓸디 손대기?? 그거 해",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030316.1.20",
          "form": "오 볼만 하겠다",
          "original_form": "오 볼만 하겠다",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030316.1.21",
          "form": "근데 졸업 못할까봐 좀 쫄림..,",
          "original_form": "근데 졸업 못할까봐 좀 쫄림..,",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030316.1.22",
          "form": "선배들 보니가 이번에 웹툰 졸업도 스무몇? 열몇? 중 여덟밖에 못하고",
          "original_form": "선배들 보니가 이번에 웹툰 졸업도 스무몇? 열몇? 중 여덟밖에 못하고",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030316.1.23",
          "form": "영상 졸업도 4명 중 2명 했더라",
          "original_form": "영상 졸업도 4명 중 2명 했더라",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030316.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030316.1.25",
          "form": "졸업이 힘드나보네",
          "original_form": "졸업이 힘드나보네",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030316.1.26",
          "form": "웅 그래서 다들 담학기에 졸업한다고 하더라고",
          "original_form": "웅 그래서 다들 담학기에 졸업한다고 하더라고",
