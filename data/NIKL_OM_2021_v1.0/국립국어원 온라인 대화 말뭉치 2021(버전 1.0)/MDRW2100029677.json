      "utterance": [
        {
          "id": "MDRW2100029677.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029677.1.2",
          "form": "네 가끔 해요",
          "original_form": "네 가끔 해요",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029677.1.3",
          "form": "안쓰는 가방이나 가구를 팔아요 ㅋㅋㅋ",
          "original_form": "안쓰는 가방이나 가구를 팔아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029677.1.4",
          "form": "종종 하시나요?",
          "original_form": "종종 하시나요?",
          "speaker_id": "2",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029677.1.5",
          "form": "가끔요",
          "original_form": "가끔요",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029677.1.6",
          "form": "판매하시는 편이신가요? 구매하시는 편이신가요?",
          "original_form": "판매하시는 편이신가요? 구매하시는 편이신가요?",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029677.1.7",
          "form": "팔기만요",
          "original_form": "팔기만요",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029677.1.8",
          "form": "앗 저도 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 저도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029677.1.9",
          "form": "당근 많이 이용하시나요??",
          "original_form": "당근 많이 이용하시나요??",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029677.1.10",
          "form": "필요할 때만요",
          "original_form": "필요할 때만요",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029677.1.11",
          "form": "구경은 안하세요??",
          "original_form": "구경은 안하세요??",
          "speaker_id": "2",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029677.1.12",
          "form": "많이 보죠",
          "original_form": "많이 보죠",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029677.1.13",
          "form": "특이한 거 있었어요???",
          "original_form": "특이한 거 있었어요???",
          "speaker_id": "2",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029677.1.14",
          "form": "유물 같은거요",
          "original_form": "유물 같은거요",
          "speaker_id": "1",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029677.1.15",
          "form": "와 유물도 팔아요??",
          "original_form": "와 유물도 팔아요??",
          "speaker_id": "2",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029677.1.16",
          "form": "안사는 주의인데 유물은 혹하네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "안사는 주의인데 유물은 혹하네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029677.1.17",
          "form": "막 조선시대 유물 이런건가요???",
          "original_form": "막 조선시대 유물 이런건가요???",
          "speaker_id": "2",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029677.1.18",
          "form": "그정도까진 ㅋㅋ",
          "original_form": "그정도까진 ㅋㅋ",
