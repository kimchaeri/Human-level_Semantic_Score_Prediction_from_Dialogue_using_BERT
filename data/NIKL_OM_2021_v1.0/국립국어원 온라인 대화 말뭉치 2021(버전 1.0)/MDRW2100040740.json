      "utterance": [
        {
          "id": "MDRW2100040740.1.1",
          "form": "네 방에 있는 거 아니야",
          "original_form": "네 방에 있는 거 아니야",
          "speaker_id": "2",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040740.1.2",
          "form": "방금 들어왔다 ㅎㅎ",
          "original_form": "방금 들어왔다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040740.1.3",
          "form": "왜 아직 안 자",
          "original_form": "왜 아직 안 자",
          "speaker_id": "2",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040740.1.4",
          "form": "그러게..내가 안자고있어서 그런가",
          "original_form": "그러게..내가 안자고있어서 그런가",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040740.1.5",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040740.1.6",
          "form": "울 강아지 자게 빨리 자야겠다",
          "original_form": "울 강아지 자게 빨리 자야겠다",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040740.1.7",
          "form": "그래 안고 자라",
          "original_form": "그래 안고 자라",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040740.1.8",
          "form": "웅웅 같이 누워서 자야겠다",
          "original_form": "웅웅 같이 누워서 자야겠다",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040740.1.9",
          "form": "강아지들 잠을 많이 자야 한다",
          "original_form": "강아지들 잠을 많이 자야 한다",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040740.1.10",
          "form": "웅웅 이제 나이도 꽤 있으니 ㅠ",
          "original_form": "웅웅 이제 나이도 꽤 있으니 ㅠ",
          "speaker_id": "1",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100040740.1.11",
          "form": "그래 잠이 보약이다",
          "original_form": "그래 잠이 보약이다",
          "speaker_id": "2",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100040740.1.12",
          "form": "웅웅ㅠㅠ",
          "original_form": "웅웅ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100040740.1.13",
          "form": "울 강아지들을 위해 빨리 자라",
          "original_form": "울 강아지들을 위해 빨리 자라",
          "speaker_id": "2",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100040740.1.14",
          "form": "알았어용",
          "original_form": "알았어용",
          "speaker_id": "1",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100040740.1.15",
          "form": "울 강아지들 사료 살때 되었는데",
