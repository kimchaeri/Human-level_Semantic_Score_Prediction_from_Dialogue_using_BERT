      "utterance": [
        {
          "id": "MDRW2100002401.1.1",
          "form": "점심 뭐 드실건가요?",
          "original_form": "점심 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002401.1.2",
          "form": "매일 메뉴고르는것도 일이예요",
          "original_form": "매일 메뉴고르는것도 일이예요",
          "speaker_id": "1",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002401.1.3",
          "form": "맞아요 매일 뭘 먹나 고민하는거",
          "original_form": "맞아요 매일 뭘 먹나 고민하는거",
          "speaker_id": "2",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002401.1.4",
          "form": "생각보다 어렵더라구요",
          "original_form": "생각보다 어렵더라구요",
          "speaker_id": "2",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002401.1.5",
          "form": "메뉴 고르셨어요?",
          "original_form": "메뉴 고르셨어요?",
          "speaker_id": "1",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002401.1.6",
          "form": "저는 초밥 먹을까 생각중이에요. 초밥 좋아하세요?",
          "original_form": "저는 초밥 먹을까 생각중이에요. 초밥 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002401.1.7",
          "form": "초밥좋아하죠ㅎ 맛있겠어요",
          "original_form": "초밥좋아하죠ㅎ 맛있겠어요",
          "speaker_id": "1",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002401.1.8",
          "form": "어떤 초밥 좋아하세요? 저는 오로지 새우만 공략해요",
          "original_form": "어떤 초밥 좋아하세요? 저는 오로지 새우만 공략해요",
          "speaker_id": "2",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002401.1.9",
          "form": "여름이라 괜히 배탈날까봐 한동안 안먹었는데  먹고싶네요",
          "original_form": "여름이라 괜히 배탈날까봐 한동안 안먹었는데  먹고싶네요",
          "speaker_id": "1",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002401.1.10",
          "form": "전 주로 활어쪽이요ㅋ",
          "original_form": "전 주로 활어쪽이요ㅋ",
          "speaker_id": "1",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002401.1.11",
          "form": "활어요? 활어면 광어같은거요?",
          "original_form": "활어요? 활어면 광어같은거요?",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002401.1.12",
          "form": "네 광어 우럭 이런거요ㅎ 쫄깃한거좋아해요",
          "original_form": "네 광어 우럭 이런거요ㅎ 쫄깃한거좋아해요",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002401.1.13",
          "form": "갑자기 먹고싶네요",
          "original_form": "갑자기 먹고싶네요",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002401.1.14",
          "form": "들으니까 저도 먹고싶어졌어요",
          "original_form": "들으니까 저도 먹고싶어졌어요",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002401.1.15",
          "form": "초밥 먹을 때 와사비 드시나요?",
          "original_form": "초밥 먹을 때 와사비 드시나요?",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002401.1.16",
          "form": "제 친구는 초밥은 먹는데 와사비를 다 빼고 먹더라구요",
          "original_form": "제 친구는 초밥은 먹는데 와사비를 다 빼고 먹더라구요",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002401.1.17",
          "form": "네 와사비 좋아해요 코끗찡한데도 먹어줘야죠",
          "original_form": "네 와사비 좋아해요 코끗찡한데도 먹어줘야죠",
          "speaker_id": "1",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002401.1.18",
          "form": "저는 좋아하는 편은 아닌데 초밥에는 와사비가 있는게 좋더라구요ㅎㅎ",
          "original_form": "저는 좋아하는 편은 아닌데 초밥에는 와사비가 있는게 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:19"
        }
      ]
