      "utterance": [
        {
          "id": "MDRW2100002412.1.1",
          "form": "아침 내내 집안일 했더니 지친다",
          "original_form": "아침 내내 집안일 했더니 지친다",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002412.1.2",
          "form": "일 많이 했어?",
          "original_form": "일 많이 했어?",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002412.1.3",
          "form": "웅 날씨 좋아서 빨래하고 청소기 돌리고 설거지 하고 ㅠㅠ",
          "original_form": "웅 날씨 좋아서 빨래하고 청소기 돌리고 설거지 하고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002412.1.4",
          "form": "아궁 쉬엄쉬엄해 너무 많아 ㅠ",
          "original_form": "아궁 쉬엄쉬엄해 너무 많아 ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002412.1.5",
          "form": "하나하나 손대다 보니 안할 수가 없네",
          "original_form": "하나하나 손대다 보니 안할 수가 없네",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002412.1.6",
          "form": "그냥 둬~ 내가 집에 가서 할게",
          "original_form": "그냥 둬~ 내가 집에 가서 할게",
          "speaker_id": "2",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002412.1.7",
          "form": "신랑은 일하느라 힘든데 내가 해야지",
          "original_form": "신랑은 일하느라 힘든데 내가 해야지",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002412.1.8",
          "form": "그런 게 어딨어~ 조금이라도 힘센 내가 해야지",
          "original_form": "그런 게 어딨어~ 조금이라도 힘센 내가 해야지",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002412.1.9",
          "form": "ㅎㅎ 나보다는 훨씬 힘이 세지",
          "original_form": "ㅎㅎ 나보다는 훨씬 힘이 세지",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002412.1.10",
          "form": "그래도 내가 할께",
          "original_form": "그래도 내가 할께",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002412.1.11",
          "form": "쉬엄쉬엄 해",
          "original_form": "쉬엄쉬엄 해",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002412.1.12",
          "form": "집안일 하다가 병날라",
          "original_form": "집안일 하다가 병날라",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002412.1.13",
          "form": "걱정마요 ㅎㅎ",
          "original_form": "걱정마요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002412.1.14",
          "form": "내가 반찬이랑 몇 가지는 해줄게",
          "original_form": "내가 반찬이랑 몇 가지는 해줄게",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002412.1.15",
          "form": "집에 가면",
          "original_form": "집에 가면",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002412.1.16",
          "form": "ㅎㅎ 말이라도 고마워",
          "original_form": "ㅎㅎ 말이라도 고마워",
          "speaker_id": "1",
