        },
        {
          "id": "MDRW2100032676.1.2",
          "form": "실패햇어 ㅋㅋㅋㅋㅋ",
          "original_form": "실패햇어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032676.1.3",
          "form": "다이어트 계획어케댐",
          "original_form": "다이어트 계획어케댐",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032676.1.4",
          "form": "일단 내일부터구여",
          "original_form": "일단 내일부터구여",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032676.1.5",
          "form": "피티를 주3회 가는게 목표예여",
          "original_form": "피티를 주3회 가는게 목표예여",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032676.1.6",
          "form": "오오 피티는한번에 몇시간하나여",
          "original_form": "오오 피티는한번에 몇시간하나여",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032676.1.7",
          "form": "50분해여",
          "original_form": "50분해여",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032676.1.8",
          "form": "오오 그렇군",
          "original_form": "오오 그렇군",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032676.1.9",
          "form": "피티말고는 또 운동머하나여",
          "original_form": "피티말고는 또 운동머하나여",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032676.1.10",
          "form": "하고 나서 노잼 런닝머신 조금 걸어여",
          "original_form": "하고 나서 노잼 런닝머신 조금 걸어여",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032676.1.11",
          "form": "안하면 근육통 생긴다고 해서...",
          "original_form": "안하면 근육통 생긴다고 해서...",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032676.1.12",
          "form": "아 그럼 해줘야지",
          "original_form": "아 그럼 해줘야지",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032676.1.13",
          "form": "출퇴근 걸어서 잘하구잇심까",
          "original_form": "출퇴근 걸어서 잘하구잇심까",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032676.1.14",
          "form": "아니요...ㅋㅋㅋㅋ",
          "original_form": "아니요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032676.1.15",
          "form": "비와서 계속 타고다녕ㅅ어요",
          "original_form": "비와서 계속 타고다녕ㅅ어요",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032676.1.16",
          "form": "앗 그렇군 하긴 요즘날씨엉망",
          "original_form": "앗 그렇군 하긴 요즘날씨엉망",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032676.1.17",
          "form": "식단은어케하나여",
          "original_form": "식단은어케하나여",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032676.1.18",
          "form": "ㅋㅋ식단 요즘 거의 포기했었는데",
          "original_form": "ㅋㅋ식단 요즘 거의 포기했었는데",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032676.1.19",
          "form": "다시 고삐를 잡아야쥬ㅠ",
          "original_form": "다시 고삐를 잡아야쥬ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032676.1.20",
          "form": "너는 식단 어캐해",
          "original_form": "너는 식단 어캐해",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032676.1.21",
          "form": "나..? 안한지오래되긴햇어..",
          "original_form": "나..? 안한지오래되긴햇어..",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032676.1.22",
          "form": "일단 이번주는 간헐적단식은 지키는중",
          "original_form": "일단 이번주는 간헐적단식은 지키는중",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032676.1.23",
          "form": "오 16 : 8 요런거?",
          "original_form": "오 16 : 8 요런거?",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032676.1.24",
          "form": "마따마따",
          "original_form": "마따마따",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032676.1.25",
          "form": "아직 몸무게는 변화모르겟지만",
          "original_form": "아직 몸무게는 변화모르겟지만",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032676.1.26",
          "form": "습관을들여볼라고..",
          "original_form": "습관을들여볼라고..",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032676.1.27",
          "form": "간헐적 단식은 어떤 효과가 있는고야?",
          "original_form": "간헐적 단식은 어떤 효과가 있는고야?",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032676.1.28",
          "form": "공복기간이 길면 인슐린수치??이런거 낮아지고 살빠진데",
          "original_form": "공복기간이 길면 인슐린수치??이런거 낮아지고 살빠진데",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032676.1.29",
          "form": "그 보통 저녁6시이후로 먹지말라 이러는게",
          "original_form": "그 보통 저녁6시이후로 먹지말라 이러는게",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032676.1.30",
          "form": "간헝적단식과같은그런이윸ㅋㅋㅋ",
          "original_form": "간헝적단식과같은그런이윸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032676.1.31",
          "form": "아하...ㅋㅋㅋㅋㅋ 저녁6시 이후 금식 너무 힘든것..",
          "original_form": "아하...ㅋㅋㅋㅋㅋ 저녁6시 이후 금식 너무 힘든것..",
