      "utterance": [
        {
          "id": "MDRW2100037505.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ제동생 맥콜도 먹었던듯..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ제동생 맥콜도 먹었던듯..",
          "speaker_id": "2",
          "time": "20210901 02:10"
        },
        {
          "id": "MDRW2100037505.1.2",
          "form": "신기하네요~",
          "original_form": "신기하네요~",
          "speaker_id": "2",
          "time": "20210901 02:10"
        },
        {
          "id": "MDRW2100037505.1.3",
          "form": "그 약간 비주류의 맛들이 일맥상통하는게 있나봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 약간 비주류의 맛들이 일맥상통하는게 있나봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:10"
        },
        {
          "id": "MDRW2100037505.1.4",
          "form": "그 와중에 파인애플 피자는 거부해서 맘 상했잖아요ㅎㅎㅎ",
          "original_form": "그 와중에 파인애플 피자는 거부해서 맘 상했잖아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 02:11"
        },
        {
          "id": "MDRW2100037505.1.5",
          "form": "그게 화룡점정인데ㅎ",
          "original_form": "그게 화룡점정인데ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:11"
        },
        {
          "id": "MDRW2100037505.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:13"
        },
        {
          "id": "MDRW2100037505.1.7",
          "form": "아 그건 쫌...",
          "original_form": "아 그건 쫌...",
          "speaker_id": "2",
          "time": "20210901 02:13"
        },
        {
          "id": "MDRW2100037505.1.8",
          "form": "제동생취향은 저도 모르겠지만 그건 안먹을고 같아욬ㅋㅋㅋ",
          "original_form": "제동생취향은 저도 모르겠지만 그건 안먹을고 같아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:13"
        },
        {
          "id": "MDRW2100037505.1.9",
          "form": "엇... 파인애플 취향이신가요..!!",
          "original_form": "엇... 파인애플 취향이신가요..!!",
          "speaker_id": "2",
          "time": "20210901 02:13"
        },
        {
          "id": "MDRW2100037505.1.10",
          "form": "아닠 두번씩 강조할 일인가요",
          "original_form": "아닠 두번씩 강조할 일인가요",
          "speaker_id": "1",
          "time": "20210901 02:13"
        },
        {
          "id": "MDRW2100037505.1.11",
          "form": "전 페퍼로니 피자 일편단심...",
          "original_form": "전 페퍼로니 피자 일편단심...",
          "speaker_id": "1",
          "time": "20210901 02:13"
        },
        {
          "id": "MDRW2100037505.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:13"
        },
        {
          "id": "MDRW2100037505.1.13",
          "form": "저는 고구마피자!",
          "original_form": "저는 고구마피자!",
          "speaker_id": "2",
          "time": "20210901 02:14"
        },
        {
          "id": "MDRW2100037505.1.14",
          "form": "이었으나 가끔 파인애플 토핑하면 특식 먹는 기분이더이다ㅎㅅ",
          "original_form": "이었으나 가끔 파인애플 토핑하면 특식 먹는 기분이더이다ㅎㅅ",
          "speaker_id": "1",
          "time": "20210901 02:14"
        },
        {
          "id": "MDRW2100037505.1.15",
          "form": "여기서 갈리네요 ㅋㅋㅋ짠맛과 단맛!",
          "original_form": "여기서 갈리네요 ㅋㅋㅋ짠맛과 단맛!",
          "speaker_id": "2",
          "time": "20210901 02:14"
        },
        {
          "id": "MDRW2100037505.1.16",
          "form": "앗정말 초지일관 단맛이군요!",
          "original_form": "앗정말 초지일관 단맛이군요!",
          "speaker_id": "1",
          "time": "20210901 02:14"
        },
        {
          "id": "MDRW2100037505.1.17",
          "form": "그럼 파인애플도 드셔야죠!!ㅎㅎ",
          "original_form": "그럼 파인애플도 드셔야죠!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 02:14"
        },
        {
          "id": "MDRW2100037505.1.18",
          "form": "먹으면 먹을것 같긴한데... 파인애플 피자는 파인애플만 올라가나요??",
          "original_form": "먹으면 먹을것 같긴한데... 파인애플 피자는 파인애플만 올라가나요??",
          "speaker_id": "2",
          "time": "20210901 02:15"
        },
        {
          "id": "MDRW2100037505.1.19",
          "form": "그럼 맛없을것 같은데ㅠㅠ",
          "original_form": "그럼 맛없을것 같은데ㅠㅠ",
