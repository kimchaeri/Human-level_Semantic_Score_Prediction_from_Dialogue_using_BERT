      "utterance": [
        {
          "id": "MDRW2100016431.1.1",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016431.1.2",
          "form": "된장국에 달걀 한식으로요",
          "original_form": "된장국에 달걀 한식으로요",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016431.1.3",
          "form": "식사 뭐 하셨어요?",
          "original_form": "식사 뭐 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016431.1.4",
          "form": "저는 근무중이라 순대국 포장해와서ㅋㅋ",
          "original_form": "저는 근무중이라 순대국 포장해와서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016431.1.5",
          "form": "아 포장해서 드셨어요",
          "original_form": "아 포장해서 드셨어요",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016431.1.6",
          "form": "먹는곳은 자리가 편한가요",
          "original_form": "먹는곳은 자리가 편한가요",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016431.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016431.1.8",
          "form": "먹는곳이 편해야 맛나요",
          "original_form": "먹는곳이 편해야 맛나요",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016431.1.9",
          "form": "대부분 편하게 먹게 순대국 많이 드시더라구요",
          "original_form": "대부분 편하게 먹게 순대국 많이 드시더라구요",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016431.1.10",
          "form": "저희 남편도 뭐먹었어 하면 순대국 먹었데요 ㅎㅎ",
          "original_form": "저희 남편도 뭐먹었어 하면 순대국 먹었데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016431.1.11",
          "form": "음 글케 편하지는 않아서ㅋㅋㄱ",
          "original_form": "음 글케 편하지는 않아서ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100016431.1.12",
          "form": "순대국 좋아합니다",
          "original_form": "순대국 좋아합니다",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100016431.1.13",
          "form": "아 점심먹느라 고생하셨어요",
          "original_form": "아 점심먹느라 고생하셨어요",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016431.1.14",
          "form": "순대국 다 거의 좋아 하는거 같아요 ㅎㅎ",
          "original_form": "순대국 다 거의 좋아 하는거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016431.1.15",
          "form": "한끼 해결했죠 ㅎㅎ",
          "original_form": "한끼 해결했죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016431.1.16",
          "form": "저희 아들이 엄청 즐겨요",
          "original_form": "저희 아들이 엄청 즐겨요",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016431.1.17",
          "form": "아들이 벌써",
          "original_form": "아들이 벌써",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016431.1.18",
          "form": "네 간단히 좋죠 ㅎㅎ",
          "original_form": "네 간단히 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016431.1.19",
          "form": "순대국 먹을 나이인가요ㅋㅋ",
          "original_form": "순대국 먹을 나이인가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016431.1.20",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016431.1.21",
          "form": "어릴때부터 애가 한식 잘먹고",
          "original_form": "어릴때부터 애가 한식 잘먹고",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016431.1.22",
          "form": "순대국 좋아했어요",
          "original_form": "순대국 좋아했어요",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016431.1.23",
          "form": "애들도 사실 순대국이 먹기 편하죠 ㅎㅎ",
          "original_form": "애들도 사실 순대국이 먹기 편하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016431.1.24",
          "form": "국에 말아서",
          "original_form": "국에 말아서",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016431.1.25",
          "form": "와 귀엽겠어요ㅋㅋ",
          "original_form": "와 귀엽겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100016431.1.26",
          "form": "ㅋㅋ어렸을땐 귀여웠죠",
          "original_form": "ㅋㅋ어렸을땐 귀여웠죠",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100016431.1.27",
          "form": "어서 일끝내고 저녁은 편하니 맥주한잔도 하고 맛난거 드세요 ㅎㅎ",
