      "utterance": [
        {
          "id": "MDRW2100015009.1.1",
          "form": "안녕하세요! 저녁 드셨나요?",
          "original_form": "안녕하세요! 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015009.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015009.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015009.1.4",
          "form": "저는 오늘 좀 일찍 먹어서 좀 있다가 야식 땡기려구요",
          "original_form": "저는 오늘 좀 일찍 먹어서 좀 있다가 야식 땡기려구요",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015009.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015009.1.6",
          "form": "머 땡기실건가요ㅣ",
          "original_form": "머 땡기실건가요ㅣ",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015009.1.7",
          "form": "식빵 한조각 버터에 구워먹었어요",
          "original_form": "식빵 한조각 버터에 구워먹었어요",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015009.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015009.1.9",
          "form": "소식가시네요",
          "original_form": "소식가시네요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015009.1.10",
          "form": "아무래도 야식이니까 의식하고 적게 먹게 되는거 같아요",
          "original_form": "아무래도 야식이니까 의식하고 적게 먹게 되는거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015009.1.11",
          "form": "그렇군요ㅠ",
          "original_form": "그렇군요ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015009.1.12",
          "form": "매우배부른상태랍니다",
          "original_form": "매우배부른상태랍니다",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015009.1.13",
          "form": "배부른 상태가 제일 행복하죠",
          "original_form": "배부른 상태가 제일 행복하죠",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015009.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015009.1.15",
          "form": "맥주vs소주 어떠세요?",
          "original_form": "맥주vs소주 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015009.1.16",
          "form": "저 술을 아예 못해서 소주는 맛도 몰라요ㅠ",
          "original_form": "저 술을 아예 못해서 소주는 맛도 몰라요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015009.1.17",
          "form": "알쓰시군요?ㅋㅋㅋㅋ",
          "original_form": "알쓰시군요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015009.1.18",
          "form": "알쓰중에서도 최강알쓰입니다",
          "original_form": "알쓰중에서도 최강알쓰입니다",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015009.1.19",
          "form": "한모금만 마셔도 두통이 심하게 와요ㅠ",
          "original_form": "한모금만 마셔도 두통이 심하게 와요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015009.1.20",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015009.1.21",
          "form": "한모금만 마셔도인가요?!",
          "original_form": "한모금만 마셔도인가요?!",
