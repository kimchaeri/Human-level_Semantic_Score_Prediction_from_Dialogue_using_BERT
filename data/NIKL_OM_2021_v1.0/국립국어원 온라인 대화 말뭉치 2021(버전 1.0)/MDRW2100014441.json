      "utterance": [
        {
          "id": "MDRW2100014441.1.1",
          "form": "혹시 반려동물 기르시나요?",
          "original_form": "혹시 반려동물 기르시나요?",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014441.1.2",
          "form": "아뇨ㅠㅠ키우고 계세요?",
          "original_form": "아뇨ㅠㅠ키우고 계세요?",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014441.1.3",
          "form": "아 반려동물이라고하긴 뭐하지만 금붕어 길러요ㅋㅋㅋ",
          "original_form": "아 반려동물이라고하긴 뭐하지만 금붕어 길러요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014441.1.4",
          "form": "동생이 털알레르기가있어서 기르고싶어도 못길어요 ㅠ",
          "original_form": "동생이 털알레르기가있어서 기르고싶어도 못길어요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014441.1.5",
          "form": "아...그럼 힘들죠 ㅠㅠ저도 붕어 길렀었는데ㅎㅎ",
          "original_form": "아...그럼 힘들죠 ㅠㅠ저도 붕어 길렀었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014441.1.6",
          "form": "아하! 저는 어항이작아서 붕어는 못길러보고 구피만 기르는중이에요 ㅋㅋ",
          "original_form": "아하! 저는 어항이작아서 붕어는 못길러보고 구피만 기르는중이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014441.1.7",
          "form": "오 저도 그 미니구피 길렀었어요ㅎㅎ 귀여워요ㅋㅋ",
          "original_form": "오 저도 그 미니구피 길렀었어요ㅎㅎ 귀여워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014441.1.8",
          "form": "구피귀엽죠",
          "original_form": "구피귀엽죠",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014441.1.9",
          "form": "알도많이낳아요 ㅋㅋ",
          "original_form": "알도많이낳아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014441.1.10",
          "form": "네 맞아요 ㅋㅋㅋ 점점 감당이 안됐어요ㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋㅋ 점점 감당이 안됐어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014441.1.11",
          "form": "그래도 다 죽기전까진 계속 길러보려구요",
          "original_form": "그래도 다 죽기전까진 계속 길러보려구요",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014441.1.12",
          "form": "보고있으면 힐링되더라구요ㅎㅎ평화롭구",
          "original_form": "보고있으면 힐링되더라구요ㅎㅎ평화롭구",
          "speaker_id": "2",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014441.1.13",
          "form": "맞아요,, 물가는게좀귀찮아도 키우다보면 정이가요 ㅋㅋ",
          "original_form": "맞아요,, 물가는게좀귀찮아도 키우다보면 정이가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014441.1.14",
          "form": "뭐든 기르다보면 그런거같아요ㅎㅎ마음이 가는거..ㅋㅋㅋ",
          "original_form": "뭐든 기르다보면 그런거같아요ㅎㅎ마음이 가는거..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014441.1.15",
          "form": "맞아요,, 그래서 금방죽는동물 함부로 기르면 안된다더라구요 ㅠ",
          "original_form": "맞아요,, 그래서 금방죽는동물 함부로 기르면 안된다더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014441.1.16",
          "form": "ㅠㅠㅠ마음아플거같아요ㅠㅠ",
