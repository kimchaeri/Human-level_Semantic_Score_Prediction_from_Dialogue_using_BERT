      "utterance": [
        {
          "id": "MDRW2100013768.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013768.1.2",
          "form": "안녕하세요!ㅋㅋ군대다녀오셨나요",
          "original_form": "안녕하세요!ㅋㅋ군대다녀오셨나요",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013768.1.3",
          "form": "ㅋㅋ 제가 갔다오기는 쉽진 않죠",
          "original_form": "ㅋㅋ 제가 갔다오기는 쉽진 않죠",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013768.1.4",
          "form": "여성분이시군용",
          "original_form": "여성분이시군용",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013768.1.5",
          "form": "넹 name2님도 여성분 아니신가요?",
          "original_form": "넹 &name2&님도 여성분 아니신가요?",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013768.1.6",
          "form": "그렇게 써져있길래...",
          "original_form": "그렇게 써져있길래...",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013768.1.7",
          "form": "맞아요!ㅋㅋㅋ 여기서정보가 안보여서요",
          "original_form": "맞아요!ㅋㅋㅋ 여기서정보가 안보여서요",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013768.1.8",
          "form": "군대.... 남자였으면 어땠을지 모르겠네요 ㅋㅋ",
          "original_form": "군대.... 남자였으면 어땠을지 모르겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013768.1.9",
          "form": "아하!ㅎㅎ 보이는 줄 알았네요",
          "original_form": "아하!ㅎㅎ 보이는 줄 알았네요",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013768.1.10",
          "form": "그러게요...ㅎㅎ ㅠㅠ 주변에도 아직 제대한 친구들은 없어서....",
          "original_form": "그러게요...ㅎㅎ ㅠㅠ 주변에도 아직 제대한 친구들은 없어서....",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013768.1.11",
          "form": "아 !아직 20초반쯤신가봐요",
          "original_form": "아 !아직 20초반쯤신가봐요",
          "speaker_id": "2",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100013768.1.12",
          "form": "넹ㅎㅎ name2님은 30대시죠?",
          "original_form": "넹ㅎㅎ &name2&님은 30대시죠?",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013768.1.13",
          "form": "넵 친구들 휴가나오면 보고 그러시나요",
          "original_form": "넵 친구들 휴가나오면 보고 그러시나요",
          "speaker_id": "2",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013768.1.14",
          "form": "그렇게까지 친한 남자애들은 없구요ㅋㅋ",
          "original_form": "그렇게까지 친한 남자애들은 없구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013768.1.15",
          "form": "그냥 건너건너 소식 듣거나 하는 정도라 ㅠㅋ",
          "original_form": "그냥 건너건너 소식 듣거나 하는 정도라 ㅠㅋ",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013768.1.16",
          "form": "아하..남자들은 군다갓다오면 진짜 군대얘기 질리도록 하더라구요",
          "original_form": "아하..남자들은 군다갓다오면 진짜 군대얘기 질리도록 하더라구요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013768.1.17",
          "form": "ㅋㅋㅋㅋㅋ그만큼 힘들다는 거겠죠",
          "original_form": "ㅋㅋㅋㅋㅋ그만큼 힘들다는 거겠죠",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013768.1.18",
          "form": "저희 사촌오빠도.. 한창 탄핵시위 있을때 의경이었는데",
          "original_form": "저희 사촌오빠도.. 한창 탄핵시위 있을때 의경이었는데",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013768.1.19",
          "form": "그때얘기 아직까지 해요ㅋㅋ",
          "original_form": "그때얘기 아직까지 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013768.1.20",
          "form": "아이고 ㅋㅋㅋㅋ 그건 진짜 함들만했겠네요",
          "original_form": "아이고 ㅋㅋㅋㅋ 그건 진짜 함들만했겠네요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013768.1.21",
          "form": "ㅋㅋㅋ 그래서 저는 남자들이 군대얘기 하면 어느정도는 맞장구쳐주면서 잠자코 들어줘요ㅋㅋ",
          "original_form": "ㅋㅋㅋ 그래서 저는 남자들이 군대얘기 하면 어느정도는 맞장구쳐주면서 잠자코 들어줘요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013768.1.22",
          "form": "ㅋㅋㅋㅋ 잘 받아주시네요",
          "original_form": "ㅋㅋㅋㅋ 잘 받아주시네요",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013768.1.23",
          "form": "근데 저도 진짜 1일체험 그런건 해보고싶어요",
          "original_form": "근데 저도 진짜 1일체험 그런건 해보고싶어요",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013768.1.24",
          "form": "특수부대들 하는거",
          "original_form": "특수부대들 하는거",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.25",
          "form": "헬기에서 내려오기",
          "original_form": "헬기에서 내려오기",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.27",
          "form": "헐 저두요",
          "original_form": "헐 저두요",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.28",
          "form": "ㅋㅋㅋㅋㅋ 솔직히 제대로된 군대는 아니더라도",
          "original_form": "ㅋㅋㅋㅋㅋ 솔직히 제대로된 군대는 아니더라도",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.29",
          "form": "훈련소 생활은 한번쯤",
          "original_form": "훈련소 생활은 한번쯤",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.30",
          "form": "경험해보고싶어요",
          "original_form": "경험해보고싶어요",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.31",
          "form": "총기조작법도 배우고",
          "original_form": "총기조작법도 배우고",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013768.1.32",
          "form": "오 맞아요 총 ㅋㅋㅋ사격 좋아해셔",
          "original_form": "오 맞아요 총 ㅋㅋㅋ사격 좋아해셔",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013768.1.33",
          "form": "사격장가면 20발에 몇마넌인데",
          "original_form": "사격장가면 20발에 몇마넌인데",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013768.1.34",
          "form": "군대가면 공짜체험 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "군대가면 공짜체험 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013768.1.35",
          "form": "남자들이 들으면 싫어할듯 그럼 가라고 ㅋㅋㅋ",
          "original_form": "남자들이 들으면 싫어할듯 그럼 가라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013768.1.36",
          "form": "ㅋㅋㅋ ㅠㅠㅠㅠㅠ 그리구 사격장은 실탄사격장가도",
          "original_form": "ㅋㅋㅋ ㅠㅠㅠㅠㅠ 그리구 사격장은 실탄사격장가도",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013768.1.37",
          "form": "정말 체험용으로만 알려주잖아여!",
          "original_form": "정말 체험용으로만 알려주잖아여!",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013768.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 남자들이 들으면 경기하긴할듯요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 남자들이 들으면 경기하긴할듯요",
