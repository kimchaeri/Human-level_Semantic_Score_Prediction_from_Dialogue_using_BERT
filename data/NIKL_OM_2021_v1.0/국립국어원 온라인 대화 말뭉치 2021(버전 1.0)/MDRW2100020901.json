      "utterance": [
        {
          "id": "MDRW2100020901.1.1",
          "form": "오늘 날씨 어떤 거 같아요?",
          "original_form": "오늘 날씨 어떤 거 같아요?",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020901.1.2",
          "form": "오늘 날 좋네요!",
          "original_form": "오늘 날 좋네요!",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020901.1.3",
          "form": "날씨 어때요?",
          "original_form": "날씨 어때요?",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020901.1.4",
          "form": "오늘 하늘이 정말 맑은 것 같아요",
          "original_form": "오늘 하늘이 정말 맑은 것 같아요",
          "speaker_id": "1",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020901.1.5",
          "form": "미세먼지도 없어서 창문 열어 뒀어요 ㅎ",
          "original_form": "미세먼지도 없어서 창문 열어 뒀어요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020901.1.6",
          "form": "맞죠 ㅎㅎ",
          "original_form": "맞죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020901.1.7",
          "form": "근데 또 담주부턴 가을장마래요 ㅜㅜ",
          "original_form": "근데 또 담주부턴 가을장마래요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020901.1.8",
          "form": "가을장마요...?",
          "original_form": "가을장마요...?",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020901.1.9",
          "form": "늦장마에요 또..?",
          "original_form": "늦장마에요 또..?",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020901.1.10",
          "form": "네 ㅠㅠ 저도 첨 들었는데",
          "original_form": "네 ㅠㅠ 저도 첨 들었는데",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020901.1.11",
          "form": "가을장마가 있대요!",
          "original_form": "가을장마가 있대요!",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020901.1.12",
          "form": "아니 최근에 그렇게 소나기 내리더니 또 장마라니",
          "original_form": "아니 최근에 그렇게 소나기 내리더니 또 장마라니",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020901.1.13",
          "form": "비오는 날씨 그만 보고 싶네요 ㅜㅜ 안그래요?",
          "original_form": "비오는 날씨 그만 보고 싶네요 ㅜㅜ 안그래요?",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020901.1.14",
          "form": "저도 비오는거 젤 싫어해요 ㅠㅠ",
          "original_form": "저도 비오는거 젤 싫어해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020901.1.15",
          "form": "특히 습한게 너무 싫어요!",
          "original_form": "특히 습한게 너무 싫어요!",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020901.1.16",
          "form": "그니까요ㅜ 가을장마 끝나면 갑자기 겨울이 훅 오는 건 아닐지..",
          "original_form": "그니까요ㅜ 가을장마 끝나면 갑자기 겨울이 훅 오는 건 아닐지..",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020901.1.17",
          "form": "그럴거같지않나요?",
          "original_form": "그럴거같지않나요?",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020901.1.18",
          "form": "요즘 가을이 젤 짧은 느낌이에요!",
          "original_form": "요즘 가을이 젤 짧은 느낌이에요!",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020901.1.19",
          "form": "해가 지날 수록 더더욱 그런 느낌이죠?ㅠ",
          "original_form": "해가 지날 수록 더더욱 그런 느낌이죠?ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020901.1.20",
          "form": "네ㅠㅠ 지구온난화가 갈수록 심해지니까 ㅜㅜ",
          "original_form": "네ㅠㅠ 지구온난화가 갈수록 심해지니까 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020901.1.21",
          "form": "전 여름이 짧은게 좋은데 ..",
          "original_form": "전 여름이 짧은게 좋은데 ..",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020901.1.22",
          "form": "이러다가 우리나라 열대기후에 속해서 바나나 열리는 거 아닌지 모르겠어요ㅠㅠ",
          "original_form": "이러다가 우리나라 열대기후에 속해서 바나나 열리는 거 아닌지 모르겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:32"
        }
      ]
    }
