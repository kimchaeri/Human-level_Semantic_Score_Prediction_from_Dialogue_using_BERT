      "utterance": [
        {
          "id": "MDRW2100022486.1.1",
          "form": "슈비버거먹었어요",
          "original_form": "슈비버거먹었어요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100022486.1.2",
          "form": "되게 맛있었어요",
          "original_form": "되게 맛있었어요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100022486.1.3",
          "form": "사이드 메뉴도 드셨나요?",
          "original_form": "사이드 메뉴도 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 08:31"
        },
        {
          "id": "MDRW2100022486.1.4",
          "form": "세트메뉴로 먹었어요",
          "original_form": "세트메뉴로 먹었어요",
          "speaker_id": "2",
          "time": "20210830 09:12"
        },
        {
          "id": "MDRW2100022486.1.5",
          "form": "후식으로 스벅가서 음료도 먹었어요",
          "original_form": "후식으로 스벅가서 음료도 먹었어요",
          "speaker_id": "2",
          "time": "20210830 09:12"
        },
        {
          "id": "MDRW2100022486.1.6",
          "form": "스벅 음료는 뭐 드셨나요?!",
          "original_form": "스벅 음료는 뭐 드셨나요?!",
          "speaker_id": "1",
          "time": "20210830 09:14"
        },
        {
          "id": "MDRW2100022486.1.7",
          "form": "딸기음료 6100원 항상 마시는 음료에요",
          "original_form": "딸기음료 6100원 항상 마시는 음료에요",
          "speaker_id": "2",
          "time": "20210830 09:15"
        },
        {
          "id": "MDRW2100022486.1.8",
          "form": "아 그 딸기요거트블렌디드? 그건가요?",
          "original_form": "아 그 딸기요거트블렌디드? 그건가요?",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100022486.1.9",
          "form": "맞아요!!!그게제단골음료에요",
          "original_form": "맞아요!!!그게제단골음료에요",
          "speaker_id": "2",
          "time": "20210830 09:58"
        },
        {
          "id": "MDRW2100022486.1.10",
          "form": "요거트 맛 많이 나나요?",
          "original_form": "요거트 맛 많이 나나요?",
          "speaker_id": "1",
          "time": "20210830 10:05"
        },
        {
          "id": "MDRW2100022486.1.11",
          "form": "그냥 딸기맛이 나던데요",
          "original_form": "그냥 딸기맛이 나던데요",
          "speaker_id": "2",
          "time": "20210830 10:17"
        },
        {
          "id": "MDRW2100022486.1.12",
          "form": "음 그렇담 다음에 한 번 먹어봐야겠어요",
          "original_form": "음 그렇담 다음에 한 번 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210830 10:18"
        },
        {
          "id": "MDRW2100022486.1.13",
          "form": "넼ㅋㅋㅋㄱㄱ",
          "original_form": "넼ㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210830 10:50"
        },
        {
          "id": "MDRW2100022486.1.14",
          "form": "맛있어요 진짜",
          "original_form": "맛있어요 진짜",
          "speaker_id": "2",
          "time": "20210830 10:50"
        },
        {
          "id": "MDRW2100022486.1.15",
          "form": "스벅에서 돼지바 커스텀 이런거 드셔보셨나요?",
          "original_form": "스벅에서 돼지바 커스텀 이런거 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210830 10:52"
        },
        {
          "id": "MDRW2100022486.1.16",
          "form": "돼지바요? 그건 또 먼가요?",
          "original_form": "돼지바요? 그건 또 먼가요?",
          "speaker_id": "2",
          "time": "20210830 11:15"
        },
