      "utterance": [
        {
          "id": "MDRW2100000477.1.1",
          "form": "점심 뭐 먹었어?",
          "original_form": "점심 뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000477.1.2",
          "form": "나 점심 그냥 백반먹었어",
          "original_form": "나 점심 그냥 백반먹었어",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000477.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000477.1.4",
          "form": "난 그냥 라면ㅋㅋㅋㅋㅋㅋㅋㅋ \r\n라면에 계란이랑 파 넣어먹었어",
          "original_form": "난 그냥 라면ㅋㅋㅋㅋㅋㅋㅋㅋ \r\n라면에 계란이랑 파 넣어먹었어",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000477.1.5",
          "form": "어제 백반먹을까하다가 안먹었는데",
          "original_form": "어제 백반먹을까하다가 안먹었는데",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000477.1.6",
          "form": "제육볶음 먹고싶네",
          "original_form": "제육볶음 먹고싶네",
          "speaker_id": "1",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000477.1.7",
          "form": "오  라면 맛있겠다",
          "original_form": "오  라면 맛있겠다",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000477.1.8",
          "form": "제육볶음은 저녁에 먹으면 되겠네",
          "original_form": "제육볶음은 저녁에 먹으면 되겠네",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000477.1.9",
          "form": "그 기사식당느낌 제육 좋아",
          "original_form": "그 기사식당느낌 제육 좋아",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000477.1.10",
          "form": "마자!",
          "original_form": "마자!",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000477.1.11",
          "form": "난 좀 국물있게 한게 좋더라",
          "original_form": "난 좀 국물있게 한게 좋더라",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000477.1.12",
          "form": "나도ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000477.1.13",
          "form": "약간 걸쭉한 느낌으로",
          "original_form": "약간 걸쭉한 느낌으로",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000477.1.14",
          "form": "마자 국물에 밥비벼서 먹으면 최고지",
          "original_form": "마자 국물에 밥비벼서 먹으면 최고지",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000477.1.15",
          "form": "내 저녁 메뉴는 정해졌네",
          "original_form": "내 저녁 메뉴는 정해졌네",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000477.1.16",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000477.1.17",
          "form": "나는 로제떡볶이 먹고싶은데",
          "original_form": "나는 로제떡볶이 먹고싶은데",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000477.1.18",
          "form": "집에 먹어치워야 할게많아서 그냥 밥에 반찬 먹어야돼",
          "original_form": "집에 먹어치워야 할게많아서 그냥 밥에 반찬 먹어야돼",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000477.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그맘알지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그맘알지",
          "speaker_id": "1",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000477.1.20",
          "form": "반찬은 직접 해먹어?",
          "original_form": "반찬은 직접 해먹어?",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000477.1.21",
          "form": "응 보통 해먹는편이야 \r\n사먹으면 비싸기도하고",
          "original_form": "응 보통 해먹는편이야 \r\n사먹으면 비싸기도하고",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000477.1.22",
          "form": "근데 배달도 많이 시켜먹어ㅋㅋㄱ",
          "original_form": "근데 배달도 많이 시켜먹어ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000477.1.23",
          "form": "오 요리잘하나보다 부러워",
          "original_form": "오 요리잘하나보다 부러워",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000477.1.24",
          "form": "그냥 먹을만큼만해ㅋㅋ",
          "original_form": "그냥 먹을만큼만해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000477.1.25",
          "form": "너는 요리 자주 안해?",
          "original_form": "너는 요리 자주 안해?",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000477.1.26",
          "form": "에어프라이기 돌리는건 요리가 아니겠지? ...",
          "original_form": "에어프라이기 돌리는건 요리가 아니겠지? ...",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000477.1.27",
          "form": "ㅠㅠㅠ 난 조리 수준밖에 할줄몰라",
          "original_form": "ㅠㅠㅠ 난 조리 수준밖에 할줄몰라",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000477.1.28",
          "form": "아니야 조리도 못하는 사람 많아",
          "original_form": "아니야 조리도 못하는 사람 많아",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000477.1.29",
          "form": "안태우는것도 기술이라구",
          "original_form": "안태우는것도 기술이라구",
          "speaker_id": "2",
          "time": "20210819 17:26"
        }
      ]
    }
