      "utterance": [
        {
          "id": "MDRW2100008873.1.1",
          "form": "둘째가 식성이 좋아요",
          "original_form": "둘째가 식성이 좋아요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008873.1.2",
          "form": "첫째는 입이 짧고...둘째덕에 첫째가 좀 먹긴해요",
          "original_form": "첫째는 입이 짧고...둘째덕에 첫째가 좀 먹긴해요",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008873.1.3",
          "form": "야채도 잘먹고 고기도 잘먹고 안가려서 좋아요 ㅎ",
          "original_form": "야채도 잘먹고 고기도 잘먹고 안가려서 좋아요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008873.1.4",
          "form": "저희애는 김치만 잘 안먹어요",
          "original_form": "저희애는 김치만 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008873.1.5",
          "form": "아주 수를 세고있어요",
          "original_form": "아주 수를 세고있어요",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008873.1.6",
          "form": "한조각을 나노 단위로 찢어서 먹어요 참나",
          "original_form": "한조각을 나노 단위로 찢어서 먹어요 참나",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008873.1.7",
          "form": "첫째랑 비슷해요 ㅎㅎ",
          "original_form": "첫째랑 비슷해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008873.1.8",
          "form": "3조각주고 한조각만 먹으라고 해요 ㅎㅎ",
          "original_form": "3조각주고 한조각만 먹으라고 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008873.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008873.1.10",
          "form": "그럼 아기 김치만먹겠다며 고르고 골라먹어요",
          "original_form": "그럼 아기 김치만먹겠다며 고르고 골라먹어요",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008873.1.11",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008873.1.12",
          "form": "양념 다 걷어내고 ㅋㅋ",
          "original_form": "양념 다 걷어내고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008873.1.13",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008873.1.14",
          "form": "애들이 다 비슷하군요 ㅎ",
          "original_form": "애들이 다 비슷하군요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008873.1.15",
          "form": "그러게요 우리애만 안그러네요 ㅋㅋ",
          "original_form": "그러게요 우리애만 안그러네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008873.1.16",
          "form": "다똑같아요 ㅋㅋ",
          "original_form": "다똑같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008873.1.17",
          "form": "첫째는 유치원에서 집에왔는데 입에 멀물고있더라구요",
          "original_form": "첫째는 유치원에서 집에왔는데 입에 멀물고있더라구요",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008873.1.18",
          "form": "숙주나물이 반찬으로 나온거같은데",
          "original_form": "숙주나물이 반찬으로 나온거같은데",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008873.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008873.1.20",
          "form": "ㅋㅋ애들 먹기싫음 그렇죠",
          "original_form": "ㅋㅋ애들 먹기싫음 그렇죠",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008873.1.21",
          "form": "뱉으면 혼날까봐 그랬는지 입안 볼에 저장해왔더라구요 ㅎ",
          "original_form": "뱉으면 혼날까봐 그랬는지 입안 볼에 저장해왔더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008873.1.22",
          "form": "커도 잘 안먹어요 ㅜㅜ",
          "original_form": "커도 잘 안먹어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008873.1.23",
          "form": "마스크를 쓰니 선생님도 전혀 못알아채시고요 ㅎ",
          "original_form": "마스크를 쓰니 선생님도 전혀 못알아채시고요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008873.1.24",
          "form": "식성은 어쩔수 없나봐요 ㅎ",
          "original_form": "식성은 어쩔수 없나봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008873.1.25",
          "form": "모르죠 ㅡㅡ",
          "original_form": "모르죠 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008873.1.26",
          "form": "저희는 첫째둘째 연년생인데",
          "original_form": "저희는 첫째둘째 연년생인데",
