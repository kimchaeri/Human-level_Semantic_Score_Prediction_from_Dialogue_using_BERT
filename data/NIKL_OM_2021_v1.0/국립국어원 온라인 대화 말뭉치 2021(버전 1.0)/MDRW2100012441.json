      "utterance": [
        {
          "id": "MDRW2100012441.1.1",
          "form": "운동하시는건 있으세요?",
          "original_form": "운동하시는건 있으세요?",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012441.1.2",
          "form": "운동하시는건 있으신가요?",
          "original_form": "운동하시는건 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012441.1.3",
          "form": "그냥 산책 꾸준히하고\n집에서 플랭크 잠깐해요",
          "original_form": "그냥 산책 꾸준히하고\n집에서 플랭크 잠깐해요",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012441.1.4",
          "form": "플랭크 힘든데 몇세트하시는거예요?",
          "original_form": "플랭크 힘든데 몇세트하시는거예요?",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012441.1.5",
          "form": "한 1분정도 생각날때마다요",
          "original_form": "한 1분정도 생각날때마다요",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012441.1.6",
          "form": "가끔씩 한발로 서있기도 하고요",
          "original_form": "가끔씩 한발로 서있기도 하고요",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012441.1.7",
          "form": "전 코로나전엔 스피닝  줌바이런거했는데",
          "original_form": "전 코로나전엔 스피닝  줌바이런거했는데",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012441.1.8",
          "form": "요즘 숨쉬기운동만해요^^",
          "original_form": "요즘 숨쉬기운동만해요^^",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012441.1.9",
          "form": "굉장히 활동적이시구나",
          "original_form": "굉장히 활동적이시구나",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012441.1.10",
          "form": "코로나가 참",
          "original_form": "코로나가 참",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012441.1.11",
          "form": "그건아닌데 ㅎ 운동은 해야겠고 지루한건 재미없더라구요",
          "original_form": "그건아닌데 ㅎ 운동은 해야겠고 지루한건 재미없더라구요",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012441.1.12",
          "form": "헬스 요가 이런거요",
          "original_form": "헬스 요가 이런거요",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012441.1.13",
          "form": "그나마 음악들으면서 운동되는걸 찾으니 그런거더라구요",
          "original_form": "그나마 음악들으면서 운동되는걸 찾으니 그런거더라구요",
          "speaker_id": "1",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012441.1.14",
          "form": "스피닝은 혹시 멋진 화면보면서\n하는 건줄 알았어요",
          "original_form": "스피닝은 혹시 멋진 화면보면서\n하는 건줄 알았어요",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012441.1.15",
          "form": "그냥 고정된사이클 타고 음악들으면서 엄청달리는거예요ㅎ",
          "original_form": "그냥 고정된사이클 타고 음악들으면서 엄청달리는거예요ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012441.1.16",
          "form": "상체도 율동좀 섞어서",
          "original_form": "상체도 율동좀 섞어서",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012441.1.17",
          "form": "정신 집중되고\n스트레스 풀릴거 같아요",
          "original_form": "정신 집중되고\n스트레스 풀릴거 같아요",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012441.1.18",
          "form": "신나더라구요 혼자하는게 아니고 강사님이계시거든요",
          "original_form": "신나더라구요 혼자하는게 아니고 강사님이계시거든요",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012441.1.19",
          "form": "그렇죠\n여럿이 모여서 하면 뭔가\n승부욕도 생기고요",
          "original_form": "그렇죠\n여럿이 모여서 하면 뭔가\n승부욕도 생기고요",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012441.1.20",
          "form": "땀흘리고 씻고나오면 상쾌해지고~",
          "original_form": "땀흘리고 씻고나오면 상쾌해지고~",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012441.1.21",
          "form": "줌바가 근데 더 잼있더라구요 전신운동돼서",
          "original_form": "줌바가 근데 더 잼있더라구요 전신운동돼서",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012441.1.22",
          "form": "다시 그런때가 오겠죠",
          "original_form": "다시 그런때가 오겠죠",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012441.1.23",
          "form": "그러게여   코로나 지긋지긋하네요",
          "original_form": "그러게여   코로나 지긋지긋하네요",
          "speaker_id": "1",
          "time": "20210824 22:15"
        }
      ]
