        {
          "id": "MDRW2100029994.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029994.1.2",
          "form": "동물은 예전에 키웠었어요!",
          "original_form": "동물은 예전에 키웠었어요!",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029994.1.3",
          "form": "전 구피요",
          "original_form": "전 구피요",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029994.1.4",
          "form": "구피 키우고 있나요?",
          "original_form": "구피 키우고 있나요?",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029994.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100029994.1.6",
          "form": "몇마리 키우시나요?",
          "original_form": "몇마리 키우시나요?",
          "speaker_id": "2",
          "time": "20210903 19:00"
        },
        {
          "id": "MDRW2100029994.1.7",
          "form": "열마리요",
          "original_form": "열마리요",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100029994.1.8",
          "form": "와 많이 키우시네요",
          "original_form": "와 많이 키우시네요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100029994.1.9",
          "form": "물고기 키우는거 쉽나요?",
          "original_form": "물고기 키우는거 쉽나요?",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100029994.1.10",
          "form": "어렵죠",
          "original_form": "어렵죠",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100029994.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100029994.1.12",
          "form": "구피말고 키우고 싶은 동물 있나요?",
          "original_form": "구피말고 키우고 싶은 동물 있나요?",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100029994.1.13",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210904 08:28"
        },
        {
          "id": "MDRW2100029994.1.14",
          "form": "강아지 좋져ㅎㅎ 힘이 되는 존재이긴 해요",
