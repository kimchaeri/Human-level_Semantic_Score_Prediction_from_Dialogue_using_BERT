      "utterance": [
        {
          "id": "MDRW2100011566.1.1",
          "form": "최근에 구매한 물건있으세요",
          "original_form": "최근에 구매한 물건있으세요",
          "speaker_id": "1",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100011566.1.2",
          "form": "책이랑 액정타블렛이요",
          "original_form": "책이랑 액정타블렛이요",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011566.1.3",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011566.1.4",
          "form": "네 쇼핑 좋아해요~",
          "original_form": "네 쇼핑 좋아해요~",
          "speaker_id": "1",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011566.1.5",
          "form": "어떤책 구매하셨어요?",
          "original_form": "어떤책 구매하셨어요?",
          "speaker_id": "1",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011566.1.6",
          "form": "일러스트레이터 책이요",
          "original_form": "일러스트레이터 책이요",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011566.1.7",
          "form": "배우시는 중인가요? 멋지세요",
          "original_form": "배우시는 중인가요? 멋지세요",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011566.1.8",
          "form": "감사합니다 ㅎㅎ 근데 잘못해요",
          "original_form": "감사합니다 ㅎㅎ 근데 잘못해요",
          "speaker_id": "2",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011566.1.9",
          "form": "중고거래해 보신적있나요",
          "original_form": "중고거래해 보신적있나요",
          "speaker_id": "1",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011566.1.10",
          "form": "네 핸드폰사보고",
          "original_form": "네 핸드폰사보고",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011566.1.11",
          "form": "요즘엔 기프티콘을 팔았어요",
          "original_form": "요즘엔 기프티콘을 팔았어요",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011566.1.12",
          "form": "저는 갤럭시 버즈 팔았어요",
          "original_form": "저는 갤럭시 버즈 팔았어요",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011566.1.13",
          "form": "중고나라랑 당근중에 어떤걸 이용하세요?",
          "original_form": "중고나라랑 당근중에 어떤걸 이용하세요?",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011566.1.14",
          "form": "가격은 원하시는만큼 받으셨어요?",
          "original_form": "가격은 원하시는만큼 받으셨어요?",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011566.1.15",
          "form": "둘다 안해요",
          "original_form": "둘다 안해요",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011566.1.16",
          "form": "올려놓은가격대로 팔았어요",
          "original_form": "올려놓은가격대로 팔았어요",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011566.1.17",
          "form": "금방 나가더라고요",
          "original_form": "금방 나가더라고요",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011566.1.18",
          "form": "잘됐네요",
          "original_form": "잘됐네요",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011566.1.19",
          "form": "어떤 플랫폼 이용하세요?",
          "original_form": "어떤 플랫폼 이용하세요?",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011566.1.20",
          "form": "중고거래요?",
          "original_form": "중고거래요?",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011566.1.21",
          "form": "저는 예전에 한두번해본게 다여서요",
          "original_form": "저는 예전에 한두번해본게 다여서요",
          "speaker_id": "2",
          "time": "20210824 14:13"
        }
      ]
