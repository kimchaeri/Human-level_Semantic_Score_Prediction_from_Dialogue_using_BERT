      "utterance": [
        {
          "id": "MDRW2100005641.1.1",
          "form": "오! 그럼 name2님은",
          "original_form": "오! 그럼 &name2&님은",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.2",
          "form": "자녀가 몇 명이에요?",
          "original_form": "자녀가 몇 명이에요?",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.3",
          "form": "둘다 수술요",
          "original_form": "둘다 수술요",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.4",
          "form": "둘이요",
          "original_form": "둘이요",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.5",
          "form": "저도 둘다 수술했어요ㅠ",
          "original_form": "저도 둘다 수술했어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.6",
          "form": "막 죽는다니까 간호사 언니가 시작도 안했다고 ㅋㅋ",
          "original_form": "막 죽는다니까 간호사 언니가 시작도 안했다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.7",
          "form": "성별은 어떻게 되나용?",
          "original_form": "성별은 어떻게 되나용?",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.8",
          "form": "아들 딸 요",
          "original_form": "아들 딸 요",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005641.1.9",
          "form": "와 다 가지셨네요 부롭당",
          "original_form": "와 다 가지셨네요 부롭당",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005641.1.10",
          "form": "노노",
          "original_form": "노노",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005641.1.11",
          "form": "애들이 맨날 싸워요",
          "original_form": "애들이 맨날 싸워요",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005641.1.12",
          "form": "남인거죠",
          "original_form": "남인거죠",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005641.1.13",
          "form": "ㅋㅋ저도 오빠가 있는데",
          "original_form": "ㅋㅋ저도 오빠가 있는데",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005641.1.14",
          "form": "어릴때도 그렇게 싸웠는데",
          "original_form": "어릴때도 그렇게 싸웠는데",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005641.1.15",
          "form": "지금 각자 결혼해서 아이들도 있는데도",
          "original_form": "지금 각자 결혼해서 아이들도 있는데도",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005641.1.16",
          "form": "싸웁니다.ㅋㅋㅋㅋㅋ",
          "original_form": "싸웁니다.ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005641.1.17",
          "form": "맞아요 서로 성졀이 다르면 연락도 잘안해요",
          "original_form": "맞아요 서로 성졀이 다르면 연락도 잘안해요",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005641.1.18",
          "form": "저도 연락안해요 ㅜㅜ",
          "original_form": "저도 연락안해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005641.1.19",
          "form": "힝ㅠ 가족이여도 관계라는건",
          "original_form": "힝ㅠ 가족이여도 관계라는건",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005641.1.20",
          "form": "참 어렵쥬 ㅜㅜ",
          "original_form": "참 어렵쥬 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005641.1.21",
          "form": "네 어려워요",
          "original_form": "네 어려워요",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005641.1.22",
          "form": "언니랑은 연락해도 남동생은 음 연락안해요",
          "original_form": "언니랑은 연락해도 남동생은 음 연락안해요",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005641.1.23",
          "form": "우리애들 그럴것 같아 슬퍼요",
          "original_form": "우리애들 그럴것 같아 슬퍼요",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005641.1.24",
          "form": "이게 또 부모 입장에서 생각하면",
          "original_form": "이게 또 부모 입장에서 생각하면",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005641.1.25",
          "form": "세상천지에 딱 둘인데",
          "original_form": "세상천지에 딱 둘인데",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005641.1.26",
          "form": "이런 생각이 들어서 그래지죠ㅠㅠ",
          "original_form": "이런 생각이 들어서 그래지죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005641.1.27",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005641.1.28",
          "form": "전 항상 딸딸 아들아들 부러워요",
          "original_form": "전 항상 딸딸 아들아들 부러워요",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005641.1.29",
          "form": "아.... 아들둘맘은 웁니다ㅠㅠㅋㅋ",
          "original_form": "아.... 아들둘맘은 웁니다ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005641.1.30",
          "form": "저는 제 안에 헐크가 있다는걸",
          "original_form": "저는 제 안에 헐크가 있다는걸",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005641.1.31",
          "form": "아들둘 낳고 알게 되었어여",
          "original_form": "아들둘 낳고 알게 되었어여",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005641.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005641.1.33",
          "form": "저도요 막 아들한테 삐비빅 해요",
          "original_form": "저도요 막 아들한테 삐비빅 해요",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005641.1.34",
          "form": "저도 용을 키워요",
          "original_form": "저도 용을 키워요",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005641.1.35",
          "form": "와.... 제가 졌네요... 용도 있군용",
          "original_form": "와.... 제가 졌네요... 용도 있군용",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005641.1.36",
          "form": "네 윈 ㅋㅋ",
          "original_form": "네 윈 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005641.1.37",
          "form": "you win!!!",
          "original_form": "you win!!!",
          "speaker_id": "1",
          "time": "20210821 23:43"
        }
      ]
