      "utterance": [
        {
          "id": "MDRW2100037207.1.1",
          "form": "웅웅~ 선한영향력 짱",
          "original_form": "웅웅~ 선한영향력 짱",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100037207.1.2",
          "form": "영상도 꾸준히 올리시는 거 같고~",
          "original_form": "영상도 꾸준히 올리시는 거 같고~",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100037207.1.3",
          "form": "우웅~~ 넘 보기좋아",
          "original_form": "우웅~~ 넘 보기좋아",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100037207.1.4",
          "form": "운동도 넘 깔끔하고",
          "original_form": "운동도 넘 깔끔하고",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100037207.1.5",
          "form": "웅웅~ 어려운 동작도 별로 없고~",
          "original_form": "웅웅~ 어려운 동작도 별로 없고~",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100037207.1.6",
          "form": "따라서 열시미해봐야겠어 ㅋㅋ",
          "original_form": "따라서 열시미해봐야겠어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037207.1.7",
          "form": "영상이 길지도 않아서~",
          "original_form": "영상이 길지도 않아서~",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037207.1.8",
          "form": "화이팅//",
          "original_form": "화이팅//",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037207.1.9",
          "form": "시작하기에는 부담 없다!!",
          "original_form": "시작하기에는 부담 없다!!",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037207.1.10",
          "form": "마자마자 ㅋㅋ 적당한 길이~",
          "original_form": "마자마자 ㅋㅋ 적당한 길이~",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037207.1.11",
          "form": "빠르지도 않아서 무리도 덜가는듯~",
          "original_form": "빠르지도 않아서 무리도 덜가는듯~",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037207.1.12",
          "form": "좋았어.. 파이팅! 오늘부터 다시 홈트 재개!",
          "original_form": "좋았어.. 파이팅! 오늘부터 다시 홈트 재개!",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037207.1.13",
          "form": "예얍예얍// 홈트고고~~",
          "original_form": "예얍예얍// 홈트고고~~",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100037207.1.14",
          "form": "웅웅 ㅋㅋ 관절은 소중하니까 무리안가게!",
          "original_form": "웅웅 ㅋㅋ 관절은 소중하니까 무리안가게!",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100037207.1.15",
          "form": "넵넵// 무릎에 무리 안 가게~//",
          "original_form": "넵넵// 무릎에 무리 안 가게~//",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
