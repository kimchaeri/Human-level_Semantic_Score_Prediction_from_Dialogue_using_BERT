        {
          "id": "MDRW2100034765.1.1",
          "form": "카페가면 뭐드시나요",
          "original_form": "카페가면 뭐드시나요",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100034765.1.2",
          "form": "저는 케익류요 ㅋㅋㅋ",
          "original_form": "저는 케익류요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100034765.1.3",
          "form": "디저트를 좋아해서요",
          "original_form": "디저트를 좋아해서요",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100034765.1.4",
          "form": "오 글쿤요",
          "original_form": "오 글쿤요",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100034765.1.5",
          "form": "디저트는 당근케잌파? 치즈파? 초코?",
          "original_form": "디저트는 당근케잌파? 치즈파? 초코?",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.6",
          "form": "당근케익!!너무좋죠",
          "original_form": "당근케익!!너무좋죠",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.7",
          "form": "근데 초코랑 치즈도 좋아요",
          "original_form": "근데 초코랑 치즈도 좋아요",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.8",
          "form": "다좋네요",
          "original_form": "다좋네요",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.9",
          "form": "ㅋㅋ저도..",
          "original_form": "ㅋㅋ저도..",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.10",
          "form": "고를수없네요",
          "original_form": "고를수없네요",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.11",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.12",
          "form": "당근은 별론데",
          "original_form": "당근은 별론데",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.13",
          "form": "당케는 왤케 맛있나요",
          "original_form": "당케는 왤케 맛있나요",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034765.1.14",
          "form": "ㅋㅋㅋ 맛있죠",
          "original_form": "ㅋㅋㅋ 맛있죠",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100034765.1.15",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100034765.1.16",
          "form": "음료는 뭐좋아하세요?",
          "original_form": "음료는 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100034765.1.17",
          "form": "말차 요",
          "original_form": "말차 요",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034765.1.18",
          "form": "오 저는 안 먹어봤어요",
          "original_form": "오 저는 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 19:10"
