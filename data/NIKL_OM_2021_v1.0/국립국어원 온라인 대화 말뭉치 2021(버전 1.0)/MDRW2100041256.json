        },
        {
          "id": "MDRW2100041256.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041256.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041256.1.4",
          "form": "네 두부김치 랑 무국 만들어 먹었죠",
          "original_form": "네 두부김치 랑 무국 만들어 먹었죠",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041256.1.5",
          "form": "저녁 메뉴는 무얼 드셨나요",
          "original_form": "저녁 메뉴는 무얼 드셨나요",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041256.1.6",
          "form": "저는 찜닭이요",
          "original_form": "저는 찜닭이요",
          "speaker_id": "1",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041256.1.7",
          "form": "요리 잘하시나봐요??",
          "original_form": "요리 잘하시나봐요??",
          "speaker_id": "1",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041256.1.8",
          "form": "배달한 찜닭 인가요",
          "original_form": "배달한 찜닭 인가요",
          "speaker_id": "2",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100041256.1.9",
          "form": "잘하지는 못해도 먹을 만큼은 해요",
          "original_form": "잘하지는 못해도 먹을 만큼은 해요",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100041256.1.10",
          "form": "요리는 좀 하시나요",
          "original_form": "요리는 좀 하시나요",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100041256.1.11",
          "form": "저는 거의 못해요 찜닭도 마트 찜닭이었어요ㅋㅋㅋ",
          "original_form": "저는 거의 못해요 찜닭도 마트 찜닭이었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041256.1.12",
          "form": "좋아하시는 음식 있으신가요?",
          "original_form": "좋아하시는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041256.1.13",
          "form": "전 개인적으로 비지장을 좋아해요",
          "original_form": "전 개인적으로 비지장을 좋아해요",
          "speaker_id": "2",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041256.1.14",
          "form": "어떤 음식 좋아 하나요",
          "original_form": "어떤 음식 좋아 하나요",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041256.1.15",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041256.1.16",
          "form": "비지장이 그 콩으로 만든 음식 맞아요?",
          "original_form": "비지장이 그 콩으로 만든 음식 맞아요?",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041256.1.17",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100041256.1.18",
          "form": "두부 만들면서 나온 자투리라고 볼수 있죠",
          "original_form": "두부 만들면서 나온 자투리라고 볼수 있죠",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041256.1.19",
          "form": "매운 음식 좋아하나요",
          "original_form": "매운 음식 좋아하나요",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041256.1.20",
          "form": "매운 건 잘 못먹지만 매운 음식은 좋아해요",
          "original_form": "매운 건 잘 못먹지만 매운 음식은 좋아해요",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041256.1.21",
          "form": "name2님은 매운 음식 잘 드시나요?",
          "original_form": "&name2&님은 매운 음식 잘 드시나요?",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041256.1.22",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041256.1.23",
          "form": "전 매운음식 못 먹어요\r\n먹으면 속이 많이 아퍼요",
          "original_form": "전 매운음식 못 먹어요\r\n먹으면 속이 많이 아퍼요",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041256.1.24",
          "form": "헐...그러시군요",
          "original_form": "헐...그러시군요",
