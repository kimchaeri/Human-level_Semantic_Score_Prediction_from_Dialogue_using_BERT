      "utterance": [
        {
          "id": "MDRW2100016605.1.1",
          "form": "언니 뭔가 정상적인거 말규",
          "original_form": "언니 뭔가 정상적인거 말규",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016605.1.2",
          "form": "내가 가던 피부샵에 열 명 이상을 소개시켜줬어",
          "original_form": "내가 가던 피부샵에 열 명 이상을 소개시켜줬어",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016605.1.3",
          "form": "옥장판같은거 ㅋㅋㅋㅋㅋ",
          "original_form": "옥장판같은거 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016605.1.4",
          "form": "이야~멋진데여??",
          "original_form": "이야~멋진데여??",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016605.1.5",
          "form": "어 다단계 또 잘하지",
          "original_form": "어 다단계 또 잘하지",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016605.1.6",
          "form": "그래서 다른 사람들보다 서비스도 많이 받고 돈도 싸게 받음",
          "original_form": "그래서 다른 사람들보다 서비스도 많이 받고 돈도 싸게 받음",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016605.1.7",
          "form": "ㅋㅋㅋㅋㅋ그건 타고 나야된다며~",
          "original_form": "ㅋㅋㅋㅋㅋ그건 타고 나야된다며~",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016605.1.8",
          "form": "언니오면 손님 많이 옴 ㅋㅋㅋㅋㅋ",
          "original_form": "언니오면 손님 많이 옴 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016605.1.9",
          "form": "일끝나면 어데 매장가서",
          "original_form": "일끝나면 어데 매장가서",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016605.1.10",
          "form": "문앞에 서있는 알바해여 ㅋㅋㅋ",
          "original_form": "문앞에 서있는 알바해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016605.1.11",
          "form": "투잡으로 ㅋㅋㅋ",
          "original_form": "투잡으로 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016605.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 진짜 화장품 다단계 할까싶다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 진짜 화장품 다단계 할까싶다",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016605.1.13",
          "form": "피부관리 잘해가지고",
          "original_form": "피부관리 잘해가지고",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016605.1.14",
          "form": "내가 여태 그거썼다고",
          "original_form": "내가 여태 그거썼다고",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016605.1.15",
          "form": "환갑 아줌마들 모아놓고",
          "original_form": "환갑 아줌마들 모아놓고",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016605.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그런 아줌들 또 사람말에 잘 넘어감 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그런 아줌들 또 사람말에 잘 넘어감 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016605.1.17",
          "form": "난제 퇴직하면 알바해여 ㅋㅋㅋㅋㅋ",
          "original_form": "난제 퇴직하면 알바해여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016605.1.18",
          "form": "ㅋㅋㅋㅋㄱ 같이할래??",
          "original_form": "ㅋㅋㅋㅋㄱ 같이할래??",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016605.1.19",
          "form": "기미빼고 관리해서 기다려라",
          "original_form": "기미빼고 관리해서 기다려라",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016605.1.20",
          "form": "같이 팔러다니자",
          "original_form": "같이 팔러다니자",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016605.1.21",
          "form": "내가 운전할께 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "내가 운전할께 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016605.1.22",
          "form": "좋아여 온니 ㅋㅋㅋㅋ크게 한탕해서 뜹시다 ㅋㅋㅋㅋㅋ",
          "original_form": "좋아여 온니 ㅋㅋㅋㅋ크게 한탕해서 뜹시다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016605.1.23",
          "form": "뜨긴 뭘  뜨노 임마 ㅋㅋㅋㅋㄱ",
          "original_form": "뜨긴 뭘  뜨노 임마 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016605.1.24",
          "form": "평생 봉리 살아야지",
          "original_form": "평생 봉리 살아야지",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016605.1.25",
          "form": "수배자 되서 쫓겨 다니는 거 아니야??",
          "original_form": "수배자 되서 쫓겨 다니는 거 아니야??",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016605.1.26",
          "form": "아녀 온니 봉리는 못뜨지 ㅋㅋㅋㅋ",
          "original_form": "아녀 온니 봉리는 못뜨지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016605.1.27",
          "form": "그 바닥을 떠야져 ㅋㅋㅋㅋ",
          "original_form": "그 바닥을 떠야져 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016605.1.28",
          "form": "다단계 ㅋㅋㅋㅋㅋ",
          "original_form": "다단계 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016605.1.29",
          "form": "저쭈 서울 발령이나도 봉리살아여함 ㅋㅋㅋㅋ",
          "original_form": "저쭈 서울 발령이나도 봉리살아여함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016605.1.30",
          "form": "ㅋㅋㅋ 영원히 봉리 인생",
          "original_form": "ㅋㅋㅋ 영원히 봉리 인생",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100016605.1.31",
          "form": "나 전에 이 직업 갖기 전에 아르바이트를 한 번 했는데",
          "original_form": "나 전에 이 직업 갖기 전에 아르바이트를 한 번 했는데",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100016605.1.32",
          "form": "휴대폰을 팔았거든",
          "original_form": "휴대폰을 팔았거든",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100016605.1.33",
          "form": "거기서 실적이 너무 좋아서",
          "original_form": "거기서 실적이 너무 좋아서",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100016605.1.34",
          "form": "알바를 세 달했는데 사장님이 우리 엄마 아빠 고깃집에 계속 놀러왔음",
          "original_form": "알바를 세 달했는데 사장님이 우리 엄마 아빠 고깃집에 계속 놀러왔음",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100016605.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진미언니 뭘해도 할사람 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진미언니 뭘해도 할사람 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100016605.1.36",
          "form": "텍폴에도 하나 내까여??ㅋㅋㅋ",
          "original_form": "텍폴에도 하나 내까여??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100016605.1.37",
          "form": "ㅋㅋㅋㅋㄱ 그래 잘 꼬실수 있다",
          "original_form": "ㅋㅋㅋㅋㄱ 그래 잘 꼬실수 있다",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016605.1.38",
          "form": "나중에 퇴직하고 뭐라도 해보자",
          "original_form": "나중에 퇴직하고 뭐라도 해보자",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016605.1.39",
          "form": "봉리에서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "봉리에서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016605.1.40",
          "form": "까페에 나온데이",
          "original_form": "까페에 나온데이",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016605.1.41",
          "form": "링가링 가야 뻗었니??",
          "original_form": "링가링 가야 뻗었니??",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100016605.1.42",
          "form": "사기꾼이라 하는거 아님여??",
          "original_form": "사기꾼이라 하는거 아님여??",
          "speaker_id": "2",
          "time": "20210827 08:19"
        },
        {
          "id": "MDRW2100016605.1.43",
          "form": "어제 늠 돌어댕겼디 기절요 ㅋㅋㅋ",
          "original_form": "어제 늠 돌어댕겼디 기절요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:19"
        },
        {
          "id": "MDRW2100016605.1.44",
          "form": "그래 어제 업무 보느라고 고생했네",
          "original_form": "그래 어제 업무 보느라고 고생했네",
          "speaker_id": "1",
          "time": "20210827 08:34"
        },
        {
          "id": "MDRW2100016605.1.45",
          "form": "이제 출근 준비해야겠구만",
          "original_form": "이제 출근 준비해야겠구만",
          "speaker_id": "1",
          "time": "20210827 08:34"
        },
        {
          "id": "MDRW2100016605.1.46",
