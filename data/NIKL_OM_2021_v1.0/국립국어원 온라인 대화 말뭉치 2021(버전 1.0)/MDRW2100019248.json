      "utterance": [
        {
          "id": "MDRW2100019248.1.1",
          "form": "탑 꼭대기에서",
          "original_form": "탑 꼭대기에서",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019248.1.2",
          "form": "하강하고싶어...",
          "original_form": "하강하고싶어...",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019248.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019248.1.4",
          "form": "바위 높게 쌓아올려서 하강하는 컨텐츠는 있어",
          "original_form": "바위 높게 쌓아올려서 하강하는 컨텐츠는 있어",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019248.1.5",
          "form": "바위는",
          "original_form": "바위는",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019248.1.6",
          "form": "예쁘지 않아...",
          "original_form": "예쁘지 않아...",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019248.1.7",
          "form": "그리고 집 주변에 넝쿨같은것도",
          "original_form": "그리고 집 주변에 넝쿨같은것도",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019248.1.8",
          "form": "두르고싶었다구",
          "original_form": "두르고싶었다구",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019248.1.9",
          "form": "멋알못들같으니",
          "original_form": "멋알못들같으니",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019248.1.10",
          "form": "반성해라 미호요",
          "original_form": "반성해라 미호요",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019248.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.12",
          "form": "근데 넝쿨은",
          "original_form": "근데 넝쿨은",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.13",
          "form": "이나즈마에서 집에다가 얽어놓은거 보니까",
          "original_form": "이나즈마에서 집에다가 얽어놓은거 보니까",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.14",
          "form": "운치있는 담쟁이가 아니라",
          "original_form": "운치있는 담쟁이가 아니라",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.15",
          "form": "폐가될듯 ㅋㅋㅋㅋ",
          "original_form": "폐가될듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ그렇긴 해.....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ그렇긴 해.....",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.17",
          "form": "티바트 진짜",
          "original_form": "티바트 진짜",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.18",
          "form": "폐가 너무 많아",
          "original_form": "폐가 너무 많아",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019248.1.19",
          "form": "돌아다니다보면",
          "original_form": "돌아다니다보면",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019248.1.20",
          "form": "게임인데도 너무 음산해",
          "original_form": "게임인데도 너무 음산해",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019248.1.21",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019248.1.22",
          "form": "리월이랑 몬드는",
          "original_form": "리월이랑 몬드는",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019248.1.23",
          "form": "아니 그리고 자꾸 진짜 귀신을 넣어",
          "original_form": "아니 그리고 자꾸 진짜 귀신을 넣어",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019248.1.24",
          "form": "그나마 날씨라도 좋고",
          "original_form": "그나마 날씨라도 좋고",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019248.1.25",
          "form": "배경 색이 노란색/하늘색이라",
          "original_form": "배경 색이 노란색/하늘색이라",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.26",
          "form": "그런 분위기 좀 덜했는데",
          "original_form": "그런 분위기 좀 덜했는데",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.27",
          "form": "이나즈마는 걍... 귀곡산장임",
          "original_form": "이나즈마는 걍... 귀곡산장임",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.28",
          "form": "ㅇㅇ....",
          "original_form": "ㅇㅇ....",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.29",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.30",
          "form": "사람도 다도망가고",
          "original_form": "사람도 다도망가고",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.31",
          "form": "리월은 그나마",
          "original_form": "리월은 그나마",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.32",
          "form": "세월에 바래진 흔적같은건데",
          "original_form": "세월에 바래진 흔적같은건데",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.33",
          "form": "이나즈마는 ㄹㅇ 사람 다죽은 귀곡산장이야",
          "original_form": "이나즈마는 ㄹㅇ 사람 다죽은 귀곡산장이야",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019248.1.34",
          "form": "몬드 너무 소중해...",
          "original_form": "몬드 너무 소중해...",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019248.1.35",
          "form": "어딜가든 평화롭고",
          "original_form": "어딜가든 평화롭고",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019248.1.36",
          "form": "폐가도 없어....",
          "original_form": "폐가도 없어....",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019248.1.37",
          "form": "집은 마을에만 잇고 마을에는 사람이 잇어......",
          "original_form": "집은 마을에만 잇고 마을에는 사람이 잇어......",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019248.1.38",
          "form": "아니 그리고 그",
          "original_form": "아니 그리고 그",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019248.1.39",
          "form": "우인단 치친술사도 너무",
          "original_form": "우인단 치친술사도 너무",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019248.1.40",
          "form": "귀신같이 돌아다녀",
          "original_form": "귀신같이 돌아다녀",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019248.1.41",
          "form": "잡는건 어렵지 않은데",
          "original_form": "잡는건 어렵지 않은데",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019248.1.42",
          "form": "보이면 걍 너무 음침해",
          "original_form": "보이면 걍 너무 음침해",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019248.1.43",
          "form": "누가 분석해놨던데",
          "original_form": "누가 분석해놨던데",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019248.1.44",
          "form": "주인공이 리월이랑 몬드에서는",
          "original_form": "주인공이 리월이랑 몬드에서는",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019248.1.45",
          "form": "적이랑 싸워서 이겼을때",
          "original_form": "적이랑 싸워서 이겼을때",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019248.1.46",
          "form": "적이 도망가는걸로 표현됐는데",
          "original_form": "적이 도망가는걸로 표현됐는데",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019248.1.47",
          "form": "이나즈마는 그런거 없고 걍 죽이는걸로 나온다고",
          "original_form": "이나즈마는 그런거 없고 걍 죽이는걸로 나온다고",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.48",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.49",
          "form": "ㄹㅇ...?",
          "original_form": "ㄹㅇ...?",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.50",
          "form": "으 소름끼쳐....",
          "original_form": "으 소름끼쳐....",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.51",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.52",
          "form": "까마귀상단 걔네는",
          "original_form": "까마귀상단 걔네는",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.53",
          "form": "무슨 섬광탄같은걸로 튀는 묘사구나",
          "original_form": "무슨 섬광탄같은걸로 튀는 묘사구나",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.54",
          "form": "헐.....",
          "original_form": "헐.....",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019248.1.55",
          "form": "아냐 그래도",
          "original_form": "아냐 그래도",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019248.1.56",
          "form": "츄츄족이랑 슬ㄹㄹ라임은",
          "original_form": "츄츄족이랑 슬ㄹㄹ라임은",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019248.1.57",
          "form": "다 죽지 않나",
          "original_form": "다 죽지 않나",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019248.1.58",
          "form": "츄츄족은",
          "original_form": "츄츄족은",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019248.1.59",
          "form": "사람이라기보다는 몬스터잖아",
          "original_form": "사람이라기보다는 몬스터잖아",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019248.1.60",
          "form": "심연메이지도",
          "original_form": "심연메이지도",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019248.1.61",
          "form": "죽지 않나...?",
          "original_form": "죽지 않나...?",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019248.1.62",
          "form": "후 마음이",
          "original_form": "후 마음이",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019248.1.63",
          "form": "좀 놓인다",
          "original_form": "좀 놓인다",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019248.1.64",
          "form": "약간 과몰입햇서",
          "original_form": "약간 과몰입햇서",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019248.1.65",
          "form": "걔네 다",
          "original_form": "걔네 다",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019248.1.66",
          "form": "사람 아니구",
          "original_form": "사람 아니구",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019248.1.67",
          "form": "몬스터잖아 ㅋㅋㅋㅋㅋ",
          "original_form": "몬스터잖아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019248.1.68",
          "form": "엄밀히 따지면 켄리아 주민이라고도 볼 수 잇지만",
          "original_form": "엄밀히 따지면 켄리아 주민이라고도 볼 수 잇지만",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019248.1.69",
          "form": "심연메이지 나름 설정상 원래 인간이잔아",
