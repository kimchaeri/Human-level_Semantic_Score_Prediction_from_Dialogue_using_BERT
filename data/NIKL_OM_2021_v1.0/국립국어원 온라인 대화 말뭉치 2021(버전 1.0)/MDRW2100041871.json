      "utterance": [
        {
          "id": "MDRW2100041871.1.1",
          "form": "그래도 항생제는 안주듸라 ㅇ",
          "original_form": "그래도 항생제는 안주듸라 ㅇ",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041871.1.2",
          "form": "그러게유 진짜 난감",
          "original_form": "그러게유 진짜 난감",
          "speaker_id": "1",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041871.1.3",
          "form": "name3이도 그때 비염알러지였더라구요",
          "original_form": "&name3&이도 그때 비염알러지였더라구요",
          "speaker_id": "1",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041871.1.4",
          "form": "아 심하지 않은가봐요",
          "original_form": "아 심하지 않은가봐요",
          "speaker_id": "1",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041871.1.5",
          "form": "어 약도 먹다안먹다가 해도된데",
          "original_form": "어 약도 먹다안먹다가 해도된데",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041871.1.6",
          "form": "심할때만 먹이라네",
          "original_form": "심할때만 먹이라네",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041871.1.7",
          "form": "아~ 그정도면",
          "original_form": "아~ 그정도면",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041871.1.8",
          "form": "약하네요 완전 ㅋㅋ",
          "original_form": "약하네요 완전 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041871.1.9",
          "form": "다행",
          "original_form": "다행",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041871.1.10",
          "form": "니 진짜 대단하다ㅋㅋ",
          "original_form": "니 진짜 대단하다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041871.1.11",
          "form": "그게가능하군ㅋㅋ",
          "original_form": "그게가능하군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041871.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ얍얍",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ얍얍",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041871.1.13",
          "form": "내일 언니 몇시쯤 와요?",
          "original_form": "내일 언니 몇시쯤 와요?",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041871.1.14",
          "form": "일단 2시안에는 가도록노력하께ㅋㅋ",
          "original_form": "일단 2시안에는 가도록노력하께ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041871.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041871.1.16",
          "form": "지금 밥하고있다 오늘 분식",
          "original_form": "지금 밥하고있다 오늘 분식",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041871.1.17",
          "form": "2시안에는 와요 ㅋㅋ",
          "original_form": "2시안에는 와요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041871.1.18",
          "form": "2시안에는 커피 다 마셨을것같아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "2시안에는 커피 다 마셨을것같아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041871.1.19",
          "form": "분식 좋아요 ㅋ",
          "original_form": "분식 좋아요 ㅋ",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041871.1.20",
          "form": "헉 그래?ㅋㅋ 난 이제육퇴했다",
          "original_form": "헉 그래?ㅋㅋ 난 이제육퇴했다",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100041871.1.21",
          "form": "씻으러가야겧으",
          "original_form": "씻으러가야겧으",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100041871.1.22",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100041871.1.23",
          "form": "낼 일안하는것으로좋다",
          "original_form": "낼 일안하는것으로좋다",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100041871.1.24",
          "form": "와 대박 너무좋네요ㅋ",
          "original_form": "와 대박 너무좋네요ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041871.1.25",
          "form": "저는 지금 책읽어주고",
          "original_form": "저는 지금 책읽어주고",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041871.1.26",
