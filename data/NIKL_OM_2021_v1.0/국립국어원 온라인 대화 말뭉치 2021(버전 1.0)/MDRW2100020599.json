      "utterance": [
        {
          "id": "MDRW2100020599.1.1",
          "form": "유러피안 프런코도 트러플 머쉬룸도",
          "original_form": "유러피안 프런코도 트러플 머쉬룸도",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020599.1.2",
          "form": "애플민트도..",
          "original_form": "애플민트도..",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020599.1.3",
          "form": "애플민트는",
          "original_form": "애플민트는",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.4",
          "form": "애플민트는 그래도 가끔 살아나더라구요",
          "original_form": "애플민트는 그래도 가끔 살아나더라구요",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.5",
          "form": "시즌메뉴라 여름에 나와요",
          "original_form": "시즌메뉴라 여름에 나와요",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.6",
          "form": "6월쯤부터 나왔다가",
          "original_form": "6월쯤부터 나왔다가",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.7",
          "form": "9월쯤 들어가요",
          "original_form": "9월쯤 들어가요",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.8",
          "form": "맞아요 여름메뉴였군요",
          "original_form": "맞아요 여름메뉴였군요",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.9",
          "form": "ㅋㅋㅋㅋㅋ아쉬워하지말아요",
          "original_form": "ㅋㅋㅋㅋㅋ아쉬워하지말아요",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.10",
          "form": "와 한가지만 담으시는거 치고",
          "original_form": "와 한가지만 담으시는거 치고",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.11",
          "form": "되게 정보력이 좋으신데요?",
          "original_form": "되게 정보력이 좋으신데요?",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.12",
          "form": "요거트 먹기전에 그것만 먹었거든요ㅋㄱㅋㅋㅋ",
          "original_form": "요거트 먹기전에 그것만 먹었거든요ㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020599.1.13",
          "form": "먹으려면 알아야해욬ㅋㅋㅋㅋ",
          "original_form": "먹으려면 알아야해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020599.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 정말",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 정말",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020599.1.15",
          "form": "반민초파에서 한걸음 멀어졌다가",
          "original_form": "반민초파에서 한걸음 멀어졌다가",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020599.1.16",
          "form": "전 먹는것에 진심이라",
          "original_form": "전 먹는것에 진심이라",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020599.1.17",
          "form": "애플민트로 다시 가까워졌어요",
          "original_form": "애플민트로 다시 가까워졌어요",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020599.1.18",
          "form": "우리 한 25m가까워졌나요",
          "original_form": "우리 한 25m가까워졌나요",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020599.1.19",
          "form": "네 딱 적정한 거리네요",
          "original_form": "네 딱 적정한 거리네요",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020599.1.20",
          "form": "나쁘지않네요",
          "original_form": "나쁘지않네요",
          "speaker_id": "1",
          "time": "20210828 10:21"
        }
      ]
