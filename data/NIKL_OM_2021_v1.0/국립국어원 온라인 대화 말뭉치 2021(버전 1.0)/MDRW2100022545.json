      "utterance": [
        {
          "id": "MDRW2100022545.1.1",
          "form": "저녁 뭐드시나요",
          "original_form": "저녁 뭐드시나요",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022545.1.2",
          "form": "글쎄요~ㅎㅎㅎ",
          "original_form": "글쎄요~ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022545.1.3",
          "form": "점심을 너무 거하게, 너무 늦게 먹어서~ㅎㅎ",
          "original_form": "점심을 너무 거하게, 너무 늦게 먹어서~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022545.1.4",
          "form": "간단하게 먹거나 패쓰할 거 같아요~~ㅎ",
          "original_form": "간단하게 먹거나 패쓰할 거 같아요~~ㅎ",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022545.1.5",
          "form": "점심 맛있게 드셨어요?",
          "original_form": "점심 맛있게 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022545.1.6",
          "form": "아침 점심 안먹고 뒹굴거리고 있어요",
          "original_form": "아침 점심 안먹고 뒹굴거리고 있어요",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022545.1.7",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022545.1.8",
          "form": "도미노 피자 시켜먹었어요~ㅎ",
          "original_form": "도미노 피자 시켜먹었어요~ㅎ",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022545.1.9",
          "form": "스파게티까지 같이 있는 세트로 시켜서 아주 배부르네요~ㅎㅎ",
          "original_form": "스파게티까지 같이 있는 세트로 시켜서 아주 배부르네요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022545.1.10",
          "form": "오 진짜 푸짐하게 드셨네요!!",
          "original_form": "오 진짜 푸짐하게 드셨네요!!",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022545.1.11",
          "form": "그래서 오늘은 조금.. 소식하려고요..ㅠ",
          "original_form": "그래서 오늘은 조금.. 소식하려고요..ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100022545.1.12",
          "form": "또 점심 돌아왔네요. 맨날 뭐 먹을지 고민되요",
          "original_form": "또 점심 돌아왔네요. 맨날 뭐 먹을지 고민되요",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100022545.1.13",
          "form": "맞아요~ 뭔가 색다른 거 먹고 싶기도 하고~",
          "original_form": "맞아요~ 뭔가 색다른 거 먹고 싶기도 하고~",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100022545.1.14",
          "form": "점심메뉴가 젤 큰 고민인거 같아요~",
          "original_form": "점심메뉴가 젤 큰 고민인거 같아요~",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100022545.1.15",
          "form": "전 만두국 먹었어요. \r\n점심 드셨나요",
          "original_form": "전 만두국 먹었어요. \r\n점심 드셨나요",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100022545.1.16",
          "form": "저는 어제 과식해가지고..",
          "original_form": "저는 어제 과식해가지고..",
          "speaker_id": "1",
          "time": "20210830 15:23"
        },
        {
          "id": "MDRW2100022545.1.17",
          "form": "속이 불편해서 죽 먹었어요~~",
          "original_form": "속이 불편해서 죽 먹었어요~~",
          "speaker_id": "1",
          "time": "20210830 15:23"
        },
        {
          "id": "MDRW2100022545.1.18",
          "form": "속 안 좋을 땐 죽이 좋죠\r\n요즘 죽마을 같은데 다양한 죽있어서 골라먹기 편한거 같아요",
          "original_form": "속 안 좋을 땐 죽이 좋죠\r\n요즘 죽마을 같은데 다양한 죽있어서 골라먹기 편한거 같아요",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100022545.1.19",
          "form": "전 팥죽에 설탕뿌려 먹는 거 좋아해요",
          "original_form": "전 팥죽에 설탕뿌려 먹는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100022545.1.20",
          "form": "오~~팥죽맛나죠~~",
          "original_form": "오~~팥죽맛나죠~~",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100022545.1.21",
          "form": "팥칼국수도 좋아하세요??",
          "original_form": "팥칼국수도 좋아하세요??",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100022545.1.22",
          "form": "네 팥칼숙수도 좋아요. 설탕솔솔 해서 달달하게 먹음 맛나요",
          "original_form": "네 팥칼숙수도 좋아요. 설탕솔솔 해서 달달하게 먹음 맛나요",
          "speaker_id": "2",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100022545.1.23",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100022545.1.24",
          "form": "근데 은근 호불호가 있더라고요~",
          "original_form": "근데 은근 호불호가 있더라고요~",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100022545.1.25",
          "form": "진한 팥 국물이 좋은데 최근엔 먹은 적이 없네요",
          "original_form": "진한 팥 국물이 좋은데 최근엔 먹은 적이 없네요",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100022545.1.26",
          "form": "요즘엔 시중에서도 진한 팥국물 찾기가 어려운 거 같아요..ㅠ",
          "original_form": "요즘엔 시중에서도 진한 팥국물 찾기가 어려운 거 같아요..ㅠ",
          "speaker_id": "1",
