      "utterance": [
        {
          "id": "MDRW2100012188.1.1",
          "form": "그건 또 뭐야? ㅎㅎ",
          "original_form": "그건 또 뭐야? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012188.1.2",
          "form": "현금이랑 같은거야~",
          "original_form": "현금이랑 같은거야~",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012188.1.3",
          "form": "예전엔 더 많이 살 수 있었는데 요즘엔 매달 50만원까지밖에 등록이 안돼 ㅠ",
          "original_form": "예전엔 더 많이 살 수 있었는데 요즘엔 매달 50만원까지밖에 등록이 안돼 ㅠ",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012188.1.4",
          "form": "현금이랑 같은건데 왜 굳이 상품권을 사서 써?",
          "original_form": "현금이랑 같은건데 왜 굳이 상품권을 사서 써?",
          "speaker_id": "2",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100012188.1.5",
          "form": "카드 실적이 인정되더라고.. 포인트 많이 모이니깐",
          "original_form": "카드 실적이 인정되더라고.. 포인트 많이 모이니깐",
          "speaker_id": "1",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100012188.1.6",
          "form": "아 ㅎㅎ",
          "original_form": "아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100012188.1.7",
          "form": "50만원어치 사면 포인트 만점이라 주 꿀이야",
          "original_form": "50만원어치 사면 포인트 만점이라 주 꿀이야",
          "speaker_id": "1",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100012188.1.8",
          "form": "주식이야 뭐 매달 사서 모으는데 상품권으로 하면 좋긴 하겠네 ㅎㅎ",
          "original_form": "주식이야 뭐 매달 사서 모으는데 상품권으로 하면 좋긴 하겠네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100012188.1.9",
          "form": "글치~ 좀 귀찮긴 해도 포인트가 팍팍 모이니 ㅎㅎ",
          "original_form": "글치~ 좀 귀찮긴 해도 포인트가 팍팍 모이니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012188.1.10",
          "form": "증권사가 거기 하나야?",
          "original_form": "증권사가 거기 하나야?",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012188.1.11",
          "form": "아니~ kb증권도 금융상품권 있어",
          "original_form": "아니~ kb증권도 금융상품권 있어",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012188.1.12",
          "form": "그것도 한도 50이야?",
          "original_form": "그것도 한도 50이야?",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012188.1.13",
          "form": "아니 그건 한도가 극악이라 아예 안사 ㅎㅎ",
          "original_form": "아니 그건 한도가 극악이라 아예 안사 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012188.1.14",
          "form": "금융상품권이라는 게 있는지도 몰랐네~",
          "original_form": "금융상품권이라는 게 있는지도 몰랐네~",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012188.1.15",
          "form": "ㅎㅎ 관심 없으면 모를 수도 있지 뭐",
          "original_form": "ㅎㅎ 관심 없으면 모를 수도 있지 뭐",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012188.1.16",
          "form": "머지포인트 때문에 짜증나 죽겠어",
