      "utterance": [
        {
          "id": "MDRW2100029265.1.1",
          "form": "아무래도 허리가 보일거 같기도 한 옷들이었어요",
          "original_form": "아무래도 허리가 보일거 같기도 한 옷들이었어요",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029265.1.2",
          "form": "아 최악이네요",
          "original_form": "아 최악이네요",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029265.1.3",
          "form": "너무짧아요 요즘옷",
          "original_form": "너무짧아요 요즘옷",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029265.1.4",
          "form": "손을 올리면",
          "original_form": "손을 올리면",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029265.1.5",
          "form": "저는 펑퍼짐햐 옷 구매희망해요",
          "original_form": "저는 펑퍼짐햐 옷 구매희망해요",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029265.1.6",
          "form": "그래서 옷감이 좀 아까운가 했지요...",
          "original_form": "그래서 옷감이 좀 아까운가 했지요...",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029265.1.7",
          "form": "바람막이도 사고싶고,,",
          "original_form": "바람막이도 사고싶고,,",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029265.1.8",
          "form": "아직 옷이 많이 나온게 아니라 그럴수도 있는데",
          "original_form": "아직 옷이 많이 나온게 아니라 그럴수도 있는데",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029265.1.9",
          "form": "겉옷도 별로 없더라고요",
          "original_form": "겉옷도 별로 없더라고요",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029265.1.10",
          "form": "아니 지금 안나오면 언제나오려고 그러는걸까요",
          "original_form": "아니 지금 안나오면 언제나오려고 그러는걸까요",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029265.1.11",
          "form": "지금이 적기인데 올해는 글렀네요,,",
          "original_form": "지금이 적기인데 올해는 글렀네요,,",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029265.1.12",
          "form": "가을은 짧긴 하니까... 아예 겨울옷이 나올거 같더라고요",
          "original_form": "가을은 짧긴 하니까... 아예 겨울옷이 나올거 같더라고요",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029265.1.13",
          "form": "자라만 봐도 코로나때 디자이너들이 파업한거 같아요...",
          "original_form": "자라만 봐도 코로나때 디자이너들이 파업한거 같아요...",
          "speaker_id": "2",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029265.1.14",
          "form": "옷말고는 어떤거사고싶으세요?!",
          "original_form": "옷말고는 어떤거사고싶으세요?!",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029265.1.15",
          "form": "지금 당장은 생각나는게 없는데 뭐 있으세요?",
          "original_form": "지금 당장은 생각나는게 없는데 뭐 있으세요?",
          "speaker_id": "2",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029265.1.16",
          "form": "저는 복숭아요",
          "original_form": "저는 복숭아요",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029265.1.17",
          "form": "올해 복숭아가 맛있대요",
          "original_form": "올해 복숭아가 맛있대요",
          "speaker_id": "1",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029265.1.18",
          "form": "철 끝나기 전에 드셔야죠",
          "original_form": "철 끝나기 전에 드셔야죠",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029265.1.19",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029265.1.20",
          "form": "오늘 사려고했는데 깜빡했어요ㅠ",
          "original_form": "오늘 사려고했는데 깜빡했어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 00:56"
        }
