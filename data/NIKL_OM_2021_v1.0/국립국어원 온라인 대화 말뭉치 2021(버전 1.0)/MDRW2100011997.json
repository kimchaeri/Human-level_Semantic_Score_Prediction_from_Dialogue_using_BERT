        {
          "id": "MDRW2100011997.1.1",
          "form": "최근에 여행 다녀오셨나요?",
          "original_form": "최근에 여행 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011997.1.2",
          "form": "요즘은 코로나라 가지못하네요ㅠ",
          "original_form": "요즘은 코로나라 가지못하네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100011997.1.3",
          "form": "맞아요ㅠ 진짜 마지막으로 놀러간적이 언젠지 모르겠어요",
          "original_form": "맞아요ㅠ 진짜 마지막으로 놀러간적이 언젠지 모르겠어요",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100011997.1.4",
          "form": "해외여행도 가고싶고 그렇네요ㅠ",
          "original_form": "해외여행도 가고싶고 그렇네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100011997.1.5",
          "form": "해외 너무나가고싶어요ㅠ",
          "original_form": "해외 너무나가고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100011997.1.6",
          "form": "그쵸! 어디가고싶으세요?",
          "original_form": "그쵸! 어디가고싶으세요?",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100011997.1.7",
          "form": "저는 미국이요!",
          "original_form": "저는 미국이요!",
          "speaker_id": "1",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100011997.1.8",
          "form": "와 미국ㅠㅠ완전 가고싶어요",
          "original_form": "와 미국ㅠㅠ완전 가고싶어요",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100011997.1.9",
          "form": "미국에서 어디가고싶으세용?",
          "original_form": "미국에서 어디가고싶으세용?",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100011997.1.10",
          "form": "뉴욕 가고싶어요!",
          "original_form": "뉴욕 가고싶어요!",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100011997.1.11",
          "form": "와 뉴욕ㅠㅠ",
          "original_form": "와 뉴욕ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100011997.1.12",
          "form": "뉴욕에서 핫도그도 먹고 스테이크도 먹고 센트럴파크도 가고 그러고싶어요~~",
          "original_form": "뉴욕에서 핫도그도 먹고 스테이크도 먹고 센트럴파크도 가고 그러고싶어요~~",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100011997.1.13",
          "form": "와 타임스퀘어도 가보고싶어요",
          "original_form": "와 타임스퀘어도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011997.1.14",
          "form": "헉 맞네요 타임스퀘어도ㅠㅠ",
          "original_form": "헉 맞네요 타임스퀘어도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011997.1.15",
          "form": "ㅋㅋㄱㅋ일단 해외에 가고싶어요ㅎㅎ",
