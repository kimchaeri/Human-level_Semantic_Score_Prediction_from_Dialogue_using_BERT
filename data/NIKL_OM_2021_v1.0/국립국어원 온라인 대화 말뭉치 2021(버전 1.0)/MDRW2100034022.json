      "utterance": [
        {
          "id": "MDRW2100034022.1.1",
          "form": "저는 샤브샤브 먹으려구요! 좋아하세요?",
          "original_form": "저는 샤브샤브 먹으려구요! 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100034022.1.2",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100034022.1.3",
          "form": "샤브샤브 코스로 드시나요!",
          "original_form": "샤브샤브 코스로 드시나요!",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100034022.1.4",
          "form": "무한리필로 가요ㅎㅅㅎ 무슨 국밥 드세요?",
          "original_form": "무한리필로 가요ㅎㅅㅎ 무슨 국밥 드세요?",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100034022.1.5",
          "form": "저는 돼지국밥이요!",
          "original_form": "저는 돼지국밥이요!",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100034022.1.6",
          "form": "무한리필! 고기 든든하게 드시겠네요",
          "original_form": "무한리필! 고기 든든하게 드시겠네요",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100034022.1.7",
          "form": "맛있겠다~ 깍두기 좋아하세요?",
          "original_form": "맛있겠다~ 깍두기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100034022.1.8",
          "form": "네! 깍두기 필수죠",
          "original_form": "네! 깍두기 필수죠",
          "speaker_id": "2",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100034022.1.9",
          "form": "묵은지 좋아하세요 겉절이 좋아하세요?",
          "original_form": "묵은지 좋아하세요 겉절이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100034022.1.10",
          "form": "저는 아직 겉절이요",
          "original_form": "저는 아직 겉절이요",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100034022.1.11",
          "form": "밥님은요?",
          "original_form": "밥님은요?",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100034022.1.12",
          "form": "저두 겉절이요ㅎㅅㅎ 김치 직접 담가드세요?",
          "original_form": "저두 겉절이요ㅎㅅㅎ 김치 직접 담가드세요?",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100034022.1.13",
          "form": "아니요 김치 집에서는 잘안먹게되어서 아주가끔 한포기씩 사먹어요",
          "original_form": "아니요 김치 집에서는 잘안먹게되어서 아주가끔 한포기씩 사먹어요",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100034022.1.14",
          "form": "오 어디김치가 맛있나요?",
          "original_form": "오 어디김치가 맛있나요?",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100034022.1.15",
          "form": "종갓집이요!",
          "original_form": "종갓집이요!",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100034022.1.16",
          "form": "추천 감사합니다..",
          "original_form": "추천 감사합니다..",
          "speaker_id": "1",
          "time": "20210827 12:23"
        }
      ]
