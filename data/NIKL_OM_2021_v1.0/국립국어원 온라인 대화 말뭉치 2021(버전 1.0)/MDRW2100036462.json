      "utterance": [
        {
          "id": "MDRW2100036462.1.1",
          "form": "참외장아찌 이런건들어봣는데",
          "original_form": "참외장아찌 이런건들어봣는데",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036462.1.2",
          "form": "편견을가지면안되겠어요",
          "original_form": "편견을가지면안되겠어요",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036462.1.3",
          "form": "ㅋㅋㄱㅋㅈㅋ",
          "original_form": "ㅋㅋㄱㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036462.1.4",
          "form": "무궁무진하네요",
          "original_form": "무궁무진하네요",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036462.1.5",
          "form": "전 참외장아찌 첨들어요!",
          "original_form": "전 참외장아찌 첨들어요!",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036462.1.6",
          "form": "과일의 세계란ㅎ",
          "original_form": "과일의 세계란ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036462.1.7",
          "form": "아진짜요?! ㅋㅋㅋ복숭아깍두기느낌일거같아요ㅋㅋㅋㅋㅋ \r\n수박김치 이런거ㅋㅋㅋㅋ",
          "original_form": "아진짜요?! ㅋㅋㅋ복숭아깍두기느낌일거같아요ㅋㅋㅋㅋㅋ \r\n수박김치 이런거ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036462.1.8",
          "form": "ㅎㅎ 저도 첨 먹어봐서 어디서 파는지 못 권한는 이 안타까움",
          "original_form": "ㅎㅎ 저도 첨 먹어봐서 어디서 파는지 못 권한는 이 안타까움",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036462.1.9",
          "form": "수박김치는 또 뭔가용",
          "original_form": "수박김치는 또 뭔가용",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036462.1.10",
          "form": "그 수박 흰부분으로 김치를 담근대요!  저도들어만봤습니다..ㅋㅋㅋ",
          "original_form": "그 수박 흰부분으로 김치를 담근대요!  저도들어만봤습니다..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100036462.1.11",
          "form": "오오 궁금하네요  무슨맛일지!!!",
          "original_form": "오오 궁금하네요  무슨맛일지!!!",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100036462.1.12",
          "form": "저는복숭아깍두기가젤궁금한데용ㅋㅋㅋㅋ",
          "original_form": "저는복숭아깍두기가젤궁금한데용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036462.1.13",
          "form": "ㅎㅎ 딱볶+깍두기 소스 찐하게",
          "original_form": "ㅎㅎ 딱볶+깍두기 소스 찐하게",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100036462.1.14",
          "form": "딱 이거예요ㅎㅎ",
          "original_form": "딱 이거예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100036462.1.15",
          "form": "비싼게 흠이네요",
          "original_form": "비싼게 흠이네요",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100036462.1.16",
          "form": "앗 비쌌어용?!",
          "original_form": "앗 비쌌어용?!",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100036462.1.17",
          "form": "그야 무보다는 복숭아가 비쌀테니까?",
          "original_form": "그야 무보다는 복숭아가 비쌀테니까?",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100036462.1.18",
          "form": "그 투다리 사장님이 별거별거를 다 만드시는데",
          "original_form": "그 투다리 사장님이 별거별거를 다 만드시는데",
          "speaker_id": "1",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100036462.1.19",
          "form": "가끔 신기해요ㅎ 그런건 돈받고 파는것도 아니라ㅎ",
          "original_form": "가끔 신기해요ㅎ 그런건 돈받고 파는것도 아니라ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100036462.1.20",
          "form": "아하ㅋㅋ순간 메뉴가 따로있나했어요ㅋㅋ\r\n그쵸 비쌀텐데 그걸 반찬으로 ㅇㅁㅇ..!",
          "original_form": "아하ㅋㅋ순간 메뉴가 따로있나했어요ㅋㅋ\r\n그쵸 비쌀텐데 그걸 반찬으로 ㅇㅁㅇ..!",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036462.1.21",
          "form": "투다리 예전에 자주가던곳있었는듸ㅣ",
          "original_form": "투다리 예전에 자주가던곳있었는듸ㅣ",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036462.1.22",
          "form": "이젠추억이된곳인데 아직있군요!!",
          "original_form": "이젠추억이된곳인데 아직있군요!!",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036462.1.23",
          "form": "ㅎㅎ투다리도 사장 하기 나름이더라구요",
          "original_form": "ㅎㅎ투다리도 사장 하기 나름이더라구요",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036462.1.24",
          "form": "ㅠㅡㅠ 진짜맛있었는데...진짜사장님나름인듯",
          "original_form": "ㅠㅡㅠ 진짜맛있었는데...진짜사장님나름인듯",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036462.1.25",
          "form": "오옷 거기도 맛있었군요!",
          "original_form": "오옷 거기도 맛있었군요!",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036462.1.26",
          "form": "저 원래 가던 투다리는 시커먹 튀김 밍밍한 탕이었는데",
          "original_form": "저 원래 가던 투다리는 시커먹 튀김 밍밍한 탕이었는데",
          "speaker_id": "1",
