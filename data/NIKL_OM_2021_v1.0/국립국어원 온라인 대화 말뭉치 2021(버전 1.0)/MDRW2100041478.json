      "utterance": [
        {
          "id": "MDRW2100041478.1.1",
          "form": "나 키우고 싶은 동물 생겼어",
          "original_form": "나 키우고 싶은 동물 생겼어",
          "speaker_id": "2",
          "time": "20210906 01:08"
        },
        {
          "id": "MDRW2100041478.1.2",
          "form": "너 원래 고양이 키우고 싶다며",
          "original_form": "너 원래 고양이 키우고 싶다며",
          "speaker_id": "1",
          "time": "20210906 01:08"
        },
        {
          "id": "MDRW2100041478.1.3",
          "form": "못보다 훨씬 귀여운 동물을 찾았어",
          "original_form": "못보다 훨씬 귀여운 동물을 찾았어",
          "speaker_id": "2",
          "time": "20210906 01:08"
        },
        {
          "id": "MDRW2100041478.1.4",
          "form": "무슨 동물인데",
          "original_form": "무슨 동물인데",
          "speaker_id": "1",
          "time": "20210906 01:08"
        },
        {
          "id": "MDRW2100041478.1.5",
          "form": "바로 래서팬더",
          "original_form": "바로 래서팬더",
          "speaker_id": "2",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100041478.1.6",
          "form": "지금 레서팬더를 키우고 싶다고 한 거니",
          "original_form": "지금 레서팬더를 키우고 싶다고 한 거니",
          "speaker_id": "1",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.7",
          "form": "유튜브에 추천 떠서 봤는데 진짜 귀엽던데",
          "original_form": "유튜브에 추천 떠서 봤는데 진짜 귀엽던데",
          "speaker_id": "2",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.8",
          "form": "너 그 애들이 얼마나 비싼지 모르는구나",
          "original_form": "너 그 애들이 얼마나 비싼지 모르는구나",
          "speaker_id": "1",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.9",
          "form": "아 귀한 동물인가 보네",
          "original_form": "아 귀한 동물인가 보네",
          "speaker_id": "2",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.10",
          "form": "그 애들 개체수가 많지 않아서 보호종이라고",
          "original_form": "그 애들 개체수가 많지 않아서 보호종이라고",
          "speaker_id": "1",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.11",
          "form": "아 아쉽다 키우고 싶었는데",
          "original_form": "아 아쉽다 키우고 싶었는데",
          "speaker_id": "2",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.12",
          "form": "아 뭔 이해해",
          "original_form": "아 뭔 이해해",
          "speaker_id": "1",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.13",
          "form": "진짜 귀엽게 생겼더라",
          "original_form": "진짜 귀엽게 생겼더라",
          "speaker_id": "2",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041478.1.14",
          "form": "우리도 너무 이쁘지",
          "original_form": "우리도 너무 이쁘지",
          "speaker_id": "1",
