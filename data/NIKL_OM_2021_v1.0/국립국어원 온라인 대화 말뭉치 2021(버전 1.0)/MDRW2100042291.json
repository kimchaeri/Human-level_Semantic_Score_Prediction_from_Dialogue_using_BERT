        },
        {
          "id": "MDRW2100042291.1.2",
          "form": "요새 순두부열라면 맛있드라구요",
          "original_form": "요새 순두부열라면 맛있드라구요",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042291.1.3",
          "form": "아 그 레시피!!!",
          "original_form": "아 그 레시피!!!",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042291.1.4",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042291.1.5",
          "form": "네 토욜마다먹어요^^",
          "original_form": "네 토욜마다먹어요^^",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042291.1.6",
          "form": "그렇게 맛있나요??",
          "original_form": "그렇게 맛있나요??",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042291.1.7",
          "form": "뭔가 더 요리같은 느낌이였어요~~",
          "original_form": "뭔가 더 요리같은 느낌이였어요~~",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042291.1.8",
          "form": "무슨라면 좋아하세요?",
          "original_form": "무슨라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042291.1.9",
          "form": "저 진매 좋아해요!",
          "original_form": "저 진매 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042291.1.10",
          "form": "그리고 짜장라면이랑 불닭 합쳐서",
          "original_form": "그리고 짜장라면이랑 불닭 합쳐서",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042291.1.11",
          "form": "아 그렇게도먹을수있군요",
          "original_form": "아 그렇게도먹을수있군요",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042291.1.12",
          "form": "자장라면에 고.  추..",
          "original_form": "자장라면에 고.  추..",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042291.1.13",
          "form": "추천이요",
          "original_form": "추천이요",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042291.1.14",
          "form": "이번주말에 해먹어야겠어요",
          "original_form": "이번주말에 해먹어야겠어요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042291.1.15",
          "form": "평일엔 다욧이라서ㅋㅋㅋ",
          "original_form": "평일엔 다욧이라서ㅋㅋㅋ",
