        {
          "id": "MDRW2100028212.1.1",
          "form": "저는 천주교요",
          "original_form": "저는 천주교요",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028212.1.2",
          "form": "네 독실하시나여",
          "original_form": "네 독실하시나여",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028212.1.3",
          "form": "김태희랑 세례명이 깉아요",
          "original_form": "김태희랑 세례명이 깉아요",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028212.1.4",
          "form": "음 김태희 세례명이 모소리인가여",
          "original_form": "음 김태희 세례명이 모소리인가여",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028212.1.5",
          "form": "독실하죠",
          "original_form": "독실하죠",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028212.1.6",
          "form": "제가 잘 몰.라여...ㅜㅜ 종교",
          "original_form": "제가 잘 몰.라여...ㅜㅜ 종교",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028212.1.7",
          "form": "동네에 성당이 있어 다녀요",
          "original_form": "동네에 성당이 있어 다녀요",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028212.1.8",
          "form": "저도 제 자신에게 독실함",
          "original_form": "저도 제 자신에게 독실함",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028212.1.9",
          "form": "봉사도 좀 하구요",
          "original_form": "봉사도 좀 하구요",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028212.1.10",
          "form": "성당\u0000다니시면 수녀님?",
          "original_form": "성당\u0000다니시면 수녀님?",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028212.1.11",
          "form": "자신에게 진실해야죠",
          "original_form": "자신에게 진실해야죠",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028212.1.12",
          "form": "네 저는 차라리 불교가조아요",
          "original_form": "네 저는 차라리 불교가조아요",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
