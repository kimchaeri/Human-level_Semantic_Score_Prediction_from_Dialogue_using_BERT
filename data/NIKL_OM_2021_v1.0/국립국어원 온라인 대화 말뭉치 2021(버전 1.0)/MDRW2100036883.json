        },
        {
          "id": "MDRW2100036883.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036883.1.3",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036883.1.4",
          "form": "넹 조아합니당",
          "original_form": "넹 조아합니당",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036883.1.5",
          "form": "어디어디가셨어요",
          "original_form": "어디어디가셨어요",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036883.1.6",
          "form": "음..국내는 올해 제주도 부안 ?",
          "original_form": "음..국내는 올해 제주도 부안 ?",
          "speaker_id": "2",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100036883.1.7",
          "form": "해외는 간지 오래됐네용 ㅜ",
          "original_form": "해외는 간지 오래됐네용 ㅜ",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100036883.1.8",
          "form": "제주도 넘 좋죠",
          "original_form": "제주도 넘 좋죠",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100036883.1.9",
          "form": "저도 여행은코로나전 베트남이마지막..",
          "original_form": "저도 여행은코로나전 베트남이마지막..",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100036883.1.10",
          "form": "제주도 어디가 좋으셨어요",
          "original_form": "제주도 어디가 좋으셨어요",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100036883.1.11",
          "form": "제주도 올해는 짧게가서요.. ㅠ 그냥 오른쪽 위쪽 바다만 봣어용 ㅋㅋ 가고싶은데가 더 많은데ㅠ",
          "original_form": "제주도 올해는 짧게가서요.. ㅠ 그냥 오른쪽 위쪽 바다만 봣어용 ㅋㅋ 가고싶은데가 더 많은데ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100036883.1.12",
          "form": "작년말쯤 성산 쪽 갓구요 ㅋㅋㅋ 그쪽을 좋아하는것같아요",
          "original_form": "작년말쯤 성산 쪽 갓구요 ㅋㅋㅋ 그쪽을 좋아하는것같아요",
          "speaker_id": "2",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100036883.1.13",
          "form": "성산넘좋져 일출이나일몰때 크으",
          "original_form": "성산넘좋져 일출이나일몰때 크으",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100036883.1.14",
          "form": "맛집은어디다녀오셨어요",
          "original_form": "맛집은어디다녀오셨어요",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100036883.1.15",
          "form": "근데 날씨가 거의 태풍급이라 아쉬웟슴다ㅜㅜㅜ",
          "original_form": "근데 날씨가 거의 태풍급이라 아쉬웟슴다ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036883.1.16",
          "form": "맛집 해녀의집 이런데서 전복죽 먹고 ㅋㅋㅋ",
          "original_form": "맛집 해녀의집 이런데서 전복죽 먹고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036883.1.17",
          "form": "흙돼지먹구",
          "original_form": "흙돼지먹구",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036883.1.18",
          "form": "근데 꼭다시가야지 하는덴 없엇네용 ㅠㅠ",
          "original_form": "근데 꼭다시가야지 하는덴 없엇네용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036883.1.19",
          "form": "해녀의집ㅜㅜㅜ",
          "original_form": "해녀의집ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036883.1.20",
          "form": "흙돼지도 넘생각나네요ㅜㅜㅜ",
          "original_form": "흙돼지도 넘생각나네요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036883.1.21",
          "form": "카페들도 넘좋았는데 바다 쫙보이구여",
          "original_form": "카페들도 넘좋았는데 바다 쫙보이구여",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036883.1.22",
          "form": "바다보이는 카페 !ㅠㅠㅠ",
          "original_form": "바다보이는 카페 !ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100036883.1.23",
          "form": "여유롭게 오래오래 있다오고싶어요",
          "original_form": "여유롭게 오래오래 있다오고싶어요",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100036883.1.24",
          "form": "네 ㅜㅜ코로나끝나면제주부터..",
          "original_form": "네 ㅜㅜ코로나끝나면제주부터..",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100036883.1.25",
          "form": "해외는어디어디가셨었어요",
          "original_form": "해외는어디어디가셨었어요",
          "speaker_id": "1",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100036883.1.26",
          "form": "음.. 일본 이탈리아 스페인 프랑스 베트남 발리 미국 이집트,,?",
          "original_form": "음.. 일본 이탈리아 스페인 프랑스 베트남 발리 미국 이집트,,?",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036883.1.27",
          "form": "비행기타고싶네요ㅠㅠ",
          "original_form": "비행기타고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100036883.1.28",
          "form": "여행많이다니셨네요ㅜㅜ",
          "original_form": "여행많이다니셨네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100036883.1.29",
          "form": "스페인 음식 그렇게맛있다든데 진짠가여",
          "original_form": "스페인 음식 그렇게맛있다든데 진짠가여",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100036883.1.30",
          "form": "스페인 음식얘기해주세요",
          "original_form": "스페인 음식얘기해주세요",
          "speaker_id": "1",
          "time": "20210831 17:09"
        }
      ]
