      "utterance": [
        {
          "id": "MDRW2100022702.1.1",
          "form": "그리고 최종결제할대 다시  체크해보셈",
          "original_form": "그리고 최종결제할대 다시  체크해보셈",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022702.1.2",
          "form": "아니 뱌달 되는 곳중에 에그드랍도 있네",
          "original_form": "아니 뱌달 되는 곳중에 에그드랍도 있네",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022702.1.3",
          "form": "돌았다",
          "original_form": "돌았다",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022702.1.4",
          "form": "하배고파",
          "original_form": "하배고파",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022702.1.5",
          "form": "대박이다",
          "original_form": "대박이다",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022702.1.6",
          "form": "걍 뇌에힘줘서 돈아껴야게싿",
          "original_form": "걍 뇌에힘줘서 돈아껴야게싿",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022702.1.7",
          "form": "좋은생각임",
          "original_form": "좋은생각임",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022702.1.8",
          "form": "나중에 진짜 너무너무먹고싶을때",
          "original_form": "나중에 진짜 너무너무먹고싶을때",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022702.1.9",
          "form": "먁으셈",
          "original_form": "먁으셈",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022702.1.10",
          "form": "겹살이 아직많아",
          "original_form": "겹살이 아직많아",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022702.1.11",
          "form": "배달음식 한달에 한번이면 족하다",
          "original_form": "배달음식 한달에 한번이면 족하다",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022702.1.12",
          "form": "ㄱㄹ",
          "original_form": "ㄱㄹ",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022702.1.13",
          "form": "바른 생활이다",
          "original_form": "바른 생활이다",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022702.1.14",
          "form": "오늘ㅇ",
          "original_form": "오늘ㅇ",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022702.1.15",
          "form": "겹살로 뭐할거임",
          "original_form": "겹살로 뭐할거임",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022702.1.16",
          "form": "저 걍..",
          "original_form": "저 걍..",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022702.1.17",
          "form": "할수잇는 거 없어서 평범하게목음",
          "original_form": "할수잇는 거 없어서 평범하게목음",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022702.1.18",
          "form": "걍~",
          "original_form": "걍~",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022702.1.19",
          "form": "평범하게 모할건데",
          "original_form": "평범하게 모할건데",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022702.1.20",
          "form": "후라이팬에구워머겅야지",
          "original_form": "후라이팬에구워머겅야지",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022702.1.21",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022702.1.22",
          "form": "나 오늘",
          "original_form": "나 오늘",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022702.1.23",
          "form": "짜장몀",
          "original_form": "짜장몀",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022702.1.24",
          "form": "맛없는걸로 먹어서",
          "original_form": "맛없는걸로 먹어서",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022702.1.25",
          "form": "맘이슬퍼요",
          "original_form": "맘이슬퍼요",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022702.1.26",
          "form": "헐..맛이업엇구나.",
          "original_form": "헐..맛이업엇구나.",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022702.1.27",
          "form": "지금은 배안고픔?",
          "original_form": "지금은 배안고픔?",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022702.1.28",
          "form": "지금..",
          "original_form": "지금..",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022702.1.29",
          "form": "배고플 시간인데",
          "original_form": "배고플 시간인데",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022702.1.30",
          "form": "입맛도없고",
          "original_form": "입맛도없고",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022702.1.31",
          "form": "배도불러요",
          "original_form": "배도불러요",
          "speaker_id": "2",
          "time": "20210829 17:20"
        }
      ]
    }
  ]
