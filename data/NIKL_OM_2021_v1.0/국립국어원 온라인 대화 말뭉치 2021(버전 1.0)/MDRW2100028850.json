      "utterance": [
        {
          "id": "MDRW2100028850.1.1",
          "form": "수강신청은 어땠나요",
          "original_form": "수강신청은 어땠나요",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028850.1.2",
          "form": "수강신청을 하지 않았어요",
          "original_form": "수강신청을 하지 않았어요",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028850.1.3",
          "form": "이번 학기는 잠시 휴식을 취할 예정이기 때문이에요.",
          "original_form": "이번 학기는 잠시 휴식을 취할 예정이기 때문이에요.",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028850.1.4",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028850.1.5",
          "form": "..님께서는 어떠셨나요?",
          "original_form": "..님께서는 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028850.1.6",
          "form": "저는 아슬아슬하게 성공했어요.",
          "original_form": "저는 아슬아슬하게 성공했어요.",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028850.1.7",
          "form": "아슬아슬하게 성공하셨나요?",
          "original_form": "아슬아슬하게 성공하셨나요?",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028850.1.8",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028850.1.9",
          "form": "앞으로 남은 학기도 화이팅이에요!",
          "original_form": "앞으로 남은 학기도 화이팅이에요!",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028850.1.10",
          "form": "네. 교수님이 추가로 자리를 열어주셨거든요",
          "original_form": "네. 교수님이 추가로 자리를 열어주셨거든요",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028850.1.11",
          "form": "name2님은 휴학을 하게 된 이유가 있나요?",
          "original_form": "&name2&님은 휴학을 하게 된 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028850.1.12",
          "form": "교수님께서 도와주셨군요!",
          "original_form": "교수님께서 도와주셨군요!",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028850.1.13",
          "form": "여러 이유가 있지만, 가장 큰 이유는..",
          "original_form": "여러 이유가 있지만, 가장 큰 이유는..",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028850.1.14",
          "form": "가장 큰 이유는?",
          "original_form": "가장 큰 이유는?",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028850.1.15",
          "form": "몸이 너무 좋지 않아서 쉬고 싶다는 생각을 했어요",
          "original_form": "몸이 너무 좋지 않아서 쉬고 싶다는 생각을 했어요",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028850.1.16",
          "form": "이럴수가 유감이에요",
          "original_form": "이럴수가 유감이에요",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028850.1.17",
          "form": "그리고 정신건강을 위해 실컷 낮잠을 자는거죠",
          "original_form": "그리고 정신건강을 위해 실컷 낮잠을 자는거죠",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028850.1.18",
          "form": "고양이가 있는 침대에서",
          "original_form": "고양이가 있는 침대에서",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028850.1.19",
          "form": "와! 그거 좋은생각이네요!",
          "original_form": "와! 그거 좋은생각이네요!",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028850.1.20",
          "form": "고양이... 부러워요",
          "original_form": "고양이... 부러워요",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028850.1.21",
          "form": "그렇게 말씀해주셔서 기뻐요!",
          "original_form": "그렇게 말씀해주셔서 기뻐요!",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028850.1.22",
          "form": "name2님이 키우시는 고양이는 무슨 색인가요?",
          "original_form": "&name2&님이 키우시는 고양이는 무슨 색인가요?",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028850.1.23",
          "form": "고등어와 닮았어요. 갈색과 검정색 무늬가 어우러져있고, 하얀 양말을 신었어요",
          "original_form": "고등어와 닮았어요. 갈색과 검정색 무늬가 어우러져있고, 하얀 양말을 신었어요",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028850.1.24",
          "form": "귀여울 것 같아요!",
          "original_form": "귀여울 것 같아요!",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028850.1.25",
          "form": "뒷발은 겨울양말, 앞발은 여름양말이에요",
          "original_form": "뒷발은 겨울양말, 앞발은 여름양말이에요",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028850.1.26",
          "form": "진짜 귀엽겠네요",
          "original_form": "진짜 귀엽겠네요",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028850.1.27",
          "form": "젤리는 무슨 색인가요?",
          "original_form": "젤리는 무슨 색인가요?",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028850.1.28",
          "form": "검정색과 분홍색이 섞여 있어요. 초코우유와 딸기우유 같아요",
          "original_form": "검정색과 분홍색이 섞여 있어요. 초코우유와 딸기우유 같아요",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028850.1.29",
          "form": "좋네요.. 여기에도 사진첨부가 됐으면 좋을텐데요",
          "original_form": "좋네요.. 여기에도 사진첨부가 됐으면 좋을텐데요",
          "speaker_id": "1",
          "time": "20210902 22:28"
        }
      ]
    }
