      "utterance": [
        {
          "id": "MDRW2100031975.1.1",
          "form": "여행좋아하시죠?",
          "original_form": "여행좋아하시죠?",
          "speaker_id": "2",
          "time": "20210905 10:10"
        },
        {
          "id": "MDRW2100031975.1.2",
          "form": "여행 많이 다녀봤나요?",
          "original_form": "여행 많이 다녀봤나요?",
          "speaker_id": "1",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100031975.1.3",
          "form": "네..좋아합니다",
          "original_form": "네..좋아합니다",
          "speaker_id": "2",
          "time": "20210905 10:14"
        },
        {
          "id": "MDRW2100031975.1.4",
          "form": "작년부터 여행이 쉽지 않아 속상하시지요?",
          "original_form": "작년부터 여행이 쉽지 않아 속상하시지요?",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100031975.1.5",
          "form": "네..많이요",
          "original_form": "네..많이요",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100031975.1.6",
          "form": "저희 가족도 해마다 가족여행을 다녔는데",
          "original_form": "저희 가족도 해마다 가족여행을 다녔는데",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100031975.1.7",
          "form": "작년부터 그러지 못해 꽤 아쉽네요",
          "original_form": "작년부터 그러지 못해 꽤 아쉽네요",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100031975.1.8",
          "form": "저희두요..가까운곳만다니고있네요",
          "original_form": "저희두요..가까운곳만다니고있네요",
          "speaker_id": "2",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100031975.1.9",
          "form": "사람없고",
          "original_form": "사람없고",
          "speaker_id": "2",
          "time": "20210905 17:24"
        },
        {
          "id": "MDRW2100031975.1.10",
          "form": "가족들이 코로나에 많이 민감해서",
          "original_form": "가족들이 코로나에 많이 민감해서",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100031975.1.11",
          "form": "작년부터는 동네만 있는 중입니다",
          "original_form": "작년부터는 동네만 있는 중입니다",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100031975.1.12",
          "form": "차로 이동할 일이 없었네요",
          "original_form": "차로 이동할 일이 없었네요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100031975.1.13",
          "form": "저는사람없는곳 찾아다녀요",
          "original_form": "저는사람없는곳 찾아다녀요",
          "speaker_id": "2",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100031975.1.14",
          "form": "애들이아직주사를안맞아서",
          "original_form": "애들이아직주사를안맞아서",
          "speaker_id": "2",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100031975.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100031975.1.16",
          "form": "아이들과 함께하는 여행 좋죠",
          "original_form": "아이들과 함께하는 여행 좋죠",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100031975.1.17",
          "form": "코로나로 제약이 없었다면 더 많이 다녔을텐데 아쉬웠겠습니다",
          "original_form": "코로나로 제약이 없었다면 더 많이 다녔을텐데 아쉬웠겠습니다",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100031975.1.18",
          "form": "체험위주로요",
          "original_form": "체험위주로요",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100031975.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100031975.1.20",
          "form": "코로나가빨리없어지길",
          "original_form": "코로나가빨리없어지길",
          "speaker_id": "2",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100031975.1.21",
          "form": "없어지면 실컷돌아다니려구요",
          "original_form": "없어지면 실컷돌아다니려구요",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100031975.1.22",
          "form": "아이에겐 체험이 참 중요한데 말이죠",
          "original_form": "아이에겐 체험이 참 중요한데 말이죠",
          "speaker_id": "1",
          "time": "20210905 22:27"
        }
