      "utterance": [
        {
          "id": "MDRW2100025953.1.1",
          "form": "종교잇으세요?",
          "original_form": "종교잇으세요?",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025953.1.2",
          "form": "무교에요!",
          "original_form": "무교에요!",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025953.1.3",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025953.1.4",
          "form": "저도 무교에요!",
          "original_form": "저도 무교에요!",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025953.1.5",
          "form": "아직 종교를 왜 믿는지부터 이해를 못하고 있어요 저는 ㅎㅎ",
          "original_form": "아직 종교를 왜 믿는지부터 이해를 못하고 있어요 저는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025953.1.6",
          "form": "저도 가족들중에서 종교있는 사람이어뵤러서",
          "original_form": "저도 가족들중에서 종교있는 사람이어뵤러서",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025953.1.7",
          "form": "없어서",
          "original_form": "없어서",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025953.1.8",
          "form": "진짜 빠진사람을 보면 이해가 안되요ㅠ",
          "original_form": "진짜 빠진사람을 보면 이해가 안되요ㅠ",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025953.1.9",
          "form": "뉴스 보면 더 무섭죠 ㅎㅎ",
          "original_form": "뉴스 보면 더 무섭죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025953.1.10",
          "form": "그러니 뉴스나오겠지만",
          "original_form": "그러니 뉴스나오겠지만",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025953.1.11",
          "form": "꼬꼬무나 이런프로에서 재연하는거 보면",
          "original_form": "꼬꼬무나 이런프로에서 재연하는거 보면",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025953.1.12",
          "form": "정말 놀라요",
          "original_form": "정말 놀라요",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025953.1.13",
          "form": "그정도는 병인 것 같아요..",
          "original_form": "그정도는 병인 것 같아요..",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025953.1.14",
          "form": "태평양이었나 그 편 너무 무서웠어요",
          "original_form": "태평양이었나 그 편 너무 무서웠어요",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025953.1.15",
          "form": "사이비교주편 이요!",
          "original_form": "사이비교주편 이요!",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025953.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025953.1.17",
          "form": "저도 기억나요",
          "original_form": "저도 기억나요",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025953.1.18",
          "form": "와;;; 그거빠져서 부모도 때리고",
          "original_form": "와;;; 그거빠져서 부모도 때리고",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025953.1.19",
          "form": "돈도 다갖다 바치고",
          "original_form": "돈도 다갖다 바치고",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025953.1.20",
          "form": "아 오대양이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 오대양이네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025953.1.21",
          "form": "저는 그런 사람들이면 다같이 그렇게 가고 싶은 곳으로 가시는 것도... 좋은 세상 만들기에 도움 된다고 봐요",
          "original_form": "저는 그런 사람들이면 다같이 그렇게 가고 싶은 곳으로 가시는 것도... 좋은 세상 만들기에 도움 된다고 봐요",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025953.1.22",
          "form": "더 퍼지기는전에...",
          "original_form": "더 퍼지기는전에...",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025953.1.23",
          "form": "저도 태평양에 그냥 오대양얘기하고있었네요",
          "original_form": "저도 태평양에 그냥 오대양얘기하고있었네요",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025953.1.24",
          "form": "휴거 편도 무서웠어요 ㅋㅋㅋㅋ 그 시대라서 그게 믿겼던 걸까요..",
          "original_form": "휴거 편도 무서웠어요 ㅋㅋㅋㅋ 그 시대라서 그게 믿겼던 걸까요..",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100025953.1.25",
          "form": "맞아요 언능 시즌다시하면좋겠어요",
          "original_form": "맞아요 언능 시즌다시하면좋겠어요",
          "speaker_id": "1",
          "time": "20210901 23:01"
        }
      ]
    }
  ]
