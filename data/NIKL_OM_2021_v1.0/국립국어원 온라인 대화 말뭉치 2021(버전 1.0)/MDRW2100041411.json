      "utterance": [
        {
          "id": "MDRW2100041411.1.1",
          "form": "중식은 좋아하세요?",
          "original_form": "중식은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041411.1.2",
          "form": "음~ 어떤게 있을까요?",
          "original_form": "음~ 어떤게 있을까요?",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041411.1.3",
          "form": "짜장면도 중국음식이아니라던뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "짜장면도 중국음식이아니라던뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041411.1.4",
          "form": "뭐..짜장밥! 좋아해요",
          "original_form": "뭐..짜장밥! 좋아해요",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041411.1.5",
          "form": "아 그래용? 저는 마라탕 좋아해용",
          "original_form": "아 그래용? 저는 마라탕 좋아해용",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041411.1.6",
          "form": "아 마라탕.. ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 마라탕.. ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041411.1.7",
          "form": "보통 불호잘없으시던데",
          "original_form": "보통 불호잘없으시던데",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041411.1.8",
          "form": "별론가용 ?",
          "original_form": "별론가용 ?",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041411.1.9",
          "form": "전 불호입니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 불호입니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041411.1.10",
          "form": "ㅋㅋㅋㅋㅋ 어떤점에서요?",
          "original_form": "ㅋㅋㅋㅋㅋ 어떤점에서요?",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041411.1.11",
          "form": "그냥 맛이...?",
          "original_form": "그냥 맛이...?",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041411.1.12",
          "form": "썩...",
          "original_form": "썩...",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041411.1.13",
          "form": "더이상먹고싶지않은맛..?",
          "original_form": "더이상먹고싶지않은맛..?",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041411.1.14",
          "form": "ㅋㅋㅋㅋㅋ그런거 있죠ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그런거 있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041411.1.15",
          "form": "꿔바로우는 좋아하세요? 깐쇼새우 이런건 ?",
          "original_form": "꿔바로우는 좋아하세요? 깐쇼새우 이런건 ?",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100041411.1.16",
          "form": "맛있조 꿔바로우",
          "original_form": "맛있조 꿔바로우",
          "speaker_id": "1",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100041411.1.17",
          "form": "근데 뭔가 탕수육파 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 뭔가 탕수육파 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100041411.1.18",
          "form": "저도 ㅋㅋㅋ꿔바로우보단 일반 탕수육이 좋아요",
          "original_form": "저도 ㅋㅋㅋ꿔바로우보단 일반 탕수육이 좋아요",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100041411.1.19",
          "form": "맞아요 탕수육이 얼마나 맛있는데요 ㅎㅎ",
          "original_form": "맞아요 탕수육이 얼마나 맛있는데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100041411.1.20",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:15"
        }
      ]
