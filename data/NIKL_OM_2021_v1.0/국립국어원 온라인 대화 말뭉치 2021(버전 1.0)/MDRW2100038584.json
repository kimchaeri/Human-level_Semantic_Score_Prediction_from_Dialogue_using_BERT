      "utterance": [
        {
          "id": "MDRW2100038584.1.1",
          "form": "오늘 저녁은 집밥입니당",
          "original_form": "오늘 저녁은 집밥입니당",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100038584.1.2",
          "form": "콩나물국에 계란찜!",
          "original_form": "콩나물국에 계란찜!",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100038584.1.3",
          "form": "직접",
          "original_form": "직접",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100038584.1.4",
          "form": "해드시나요?",
          "original_form": "해드시나요?",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100038584.1.5",
          "form": "넿ㅎ",
          "original_form": "넿ㅎ",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038584.1.6",
          "form": "오 요리 잘하시나봐요",
          "original_form": "오 요리 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038584.1.7",
          "form": "기본적인 것만 해요ㅋㅋ",
          "original_form": "기본적인 것만 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038584.1.8",
          "form": "name1님은 저녁메뉴 고르셨나요?",
          "original_form": "&name1&님은 저녁메뉴 고르셨나요?",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038584.1.9",
          "form": "아직요 ㅠ ㅠ",
          "original_form": "아직요 ㅠ ㅠ",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038584.1.10",
          "form": "저녁고르는게",
          "original_form": "저녁고르는게",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038584.1.11",
          "form": "은근 어렵더라구요",
          "original_form": "은근 어렵더라구요",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038584.1.12",
          "form": "맞아요ㅜㅠ 음식 고르기 표를 봐도 고르기어려워요...",
          "original_form": "맞아요ㅜㅠ 음식 고르기 표를 봐도 고르기어려워요...",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038584.1.13",
          "form": "오 그런표도 있어요ㅋㅋ?",
          "original_form": "오 그런표도 있어요ㅋㅋ?",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038584.1.14",
          "form": "세상 좋네요",
          "original_form": "세상 좋네요",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038584.1.15",
          "form": "결정못하는 사람들을 위한...",
          "original_form": "결정못하는 사람들을 위한...",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038584.1.16",
          "form": "그런 거 있잖아요 채식vs고기",
          "original_form": "그런 거 있잖아요 채식vs고기",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038584.1.17",
          "form": "고기로 넘어가면 닭고기 소고기 돼지고기 양고기 등등ㅋㅋㅋ",
          "original_form": "고기로 넘어가면 닭고기 소고기 돼지고기 양고기 등등ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038584.1.18",
          "form": "아 좋네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 좋네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038584.1.19",
          "form": "고기 뭐 좋아하세용",
          "original_form": "고기 뭐 좋아하세용",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038584.1.20",
          "form": "저는 다 좋아해요ㅋㅋㅋ치킨을 자주 접하니 닭고기를 가장 많이 먹긴하지만요ㅋㅋ",
          "original_form": "저는 다 좋아해요ㅋㅋㅋ치킨을 자주 접하니 닭고기를 가장 많이 먹긴하지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100038584.1.21",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
