      "utterance": [
        {
          "id": "MDRW2100039597.1.1",
          "form": "먼지봉투 은근 비싸죠",
          "original_form": "먼지봉투 은근 비싸죠",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039597.1.2",
          "form": "네 다 채우기도 전에",
          "original_form": "네 다 채우기도 전에",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039597.1.3",
          "form": "냄새나구요",
          "original_form": "냄새나구요",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039597.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039597.1.5",
          "form": "물청소가 편하도라구요",
          "original_form": "물청소가 편하도라구요",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039597.1.6",
          "form": "네 먼지봉투는 안이 잘 보이지도 않고요",
          "original_form": "네 먼지봉투는 안이 잘 보이지도 않고요",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.7",
          "form": "네 이번건 투명이라",
          "original_form": "네 이번건 투명이라",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.8",
          "form": "먼지가 잘 보여서",
          "original_form": "먼지가 잘 보여서",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.9",
          "form": "뿌듯 ㅎㅎ",
          "original_form": "뿌듯 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.11",
          "form": "뿌듯하죠 ㅎㅎㅎ",
          "original_form": "뿌듯하죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.12",
          "form": "네 먼지 모인거 보면 ㅋㅋ",
          "original_form": "네 먼지 모인거 보면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.13",
          "form": "기분 좋아요",
          "original_form": "기분 좋아요",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.14",
          "form": "힘은 어때요?",
          "original_form": "힘은 어때요?",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.15",
          "form": "센데 2단은 넘 세고",
          "original_form": "센데 2단은 넘 세고",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.16",
          "form": "1단은 약해요 ㅠㅍ",
          "original_form": "1단은 약해요 ㅠㅍ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039597.1.17",
          "form": "1.5단은요?",
          "original_form": "1.5단은요?",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039597.1.18",
          "form": "그렇게가 안돼요",
          "original_form": "그렇게가 안돼요",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039597.1.19",
          "form": "삼성 비추!",
          "original_form": "삼성 비추!",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039597.1.20",
          "form": "네 별로네요",
          "original_form": "네 별로네요",
          "speaker_id": "2",
          "time": "20210903 19:26"
        }
      ]
