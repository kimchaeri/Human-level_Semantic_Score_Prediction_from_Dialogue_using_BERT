        },
        {
          "id": "MDRW2100011181.1.3",
          "form": "공연 관람해보신적 있으세요?",
          "original_form": "공연 관람해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011181.1.4",
          "form": "콘서트 가봤어요",
          "original_form": "콘서트 가봤어요",
          "speaker_id": "2",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011181.1.5",
          "form": "누구요?",
          "original_form": "누구요?",
          "speaker_id": "1",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011181.1.6",
          "form": "아이유 단독 콘서트요",
          "original_form": "아이유 단독 콘서트요",
          "speaker_id": "2",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011181.1.7",
          "form": "와 부럽다",
          "original_form": "와 부럽다",
          "speaker_id": "1",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011181.1.8",
          "form": "티켓이 비싸지만 그만큼 재밌어요",
          "original_form": "티켓이 비싸지만 그만큼 재밌어요",
          "speaker_id": "2",
          "time": "20210824 02:34"
        },
        {
          "id": "MDRW2100011181.1.9",
          "form": "저도 꼭 가볼래요",
          "original_form": "저도 꼭 가볼래요",
          "speaker_id": "1",
          "time": "20210824 02:34"
        },
        {
          "id": "MDRW2100011181.1.10",
          "form": "전시회 보는거 좋아하세요",
          "original_form": "전시회 보는거 좋아하세요",
          "speaker_id": "2",
          "time": "20210824 02:35"
        },
        {
          "id": "MDRW2100011181.1.11",
          "form": "네 코로나 전까지는 자주다녔어요",
          "original_form": "네 코로나 전까지는 자주다녔어요",
          "speaker_id": "1",
          "time": "20210824 02:35"
        },
        {
          "id": "MDRW2100011181.1.12",
          "form": "로뎅 전시회 갔는데 좋았어요",
          "original_form": "로뎅 전시회 갔는데 좋았어요",
          "speaker_id": "2",
          "time": "20210824 02:35"
        },
