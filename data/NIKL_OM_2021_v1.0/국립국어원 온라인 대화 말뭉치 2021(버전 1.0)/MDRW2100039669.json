      "utterance": [
        {
          "id": "MDRW2100039669.1.1",
          "form": "고양이 좋아하실거같아요!",
          "original_form": "고양이 좋아하실거같아요!",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039669.1.2",
          "form": "맞아요 고양이 좋아해요:)",
          "original_form": "맞아요 고양이 좋아해요:)",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039669.1.3",
          "form": "좋아하는 동물 있으신가요?",
          "original_form": "좋아하는 동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039669.1.4",
          "form": "저도 고양이 좋아해요!",
          "original_form": "저도 고양이 좋아해요!",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039669.1.5",
          "form": "고양이는 최강귀여움이죠",
          "original_form": "고양이는 최강귀여움이죠",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039669.1.6",
          "form": "랜선집사 하고있습니다ㅎㅎ",
          "original_form": "랜선집사 하고있습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039669.1.7",
          "form": "저도저도 랜선집사하고 있어요!",
          "original_form": "저도저도 랜선집사하고 있어요!",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039669.1.8",
          "form": "어떤 아이 보시나요?",
          "original_form": "어떤 아이 보시나요?",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039669.1.9",
          "form": "꺅 저 많이 보는건 막무가내랑 쫀덕이ㅠㅠㅠ",
          "original_form": "꺅 저 많이 보는건 막무가내랑 쫀덕이ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039669.1.10",
          "form": "쫀떡이!!!!!!!",
          "original_form": "쫀떡이!!!!!!!",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.11",
          "form": "hahaha님꺼도 한번씩 보고 크림히어로즈도 한때 많이 봤어요..,!!!",
          "original_form": "hahaha님꺼도 한번씩 보고 크림히어로즈도 한때 많이 봤어요..,!!!",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.12",
          "form": "너무귀엽죠!!!",
          "original_form": "너무귀엽죠!!!",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.13",
          "form": "하 김쫀떡",
          "original_form": "하 김쫀떡",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.14",
          "form": "역시 아시군요??",
          "original_form": "역시 아시군요??",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.15",
          "form": "ㅠㅠ진짜 귀여워요",
          "original_form": "ㅠㅠ진짜 귀여워요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.16",
          "form": "요즘 저는 하하하님",
          "original_form": "요즘 저는 하하하님",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.17",
          "form": "열심히 봅니다",
          "original_form": "열심히 봅니다",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.18",
          "form": "최애 연님이 가서 슬펐어요",
          "original_form": "최애 연님이 가서 슬펐어요",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039669.1.19",
          "form": "작년에 하하하님 한번씩 보고있어서",
          "original_form": "작년에 하하하님 한번씩 보고있어서",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039669.1.20",
          "form": "연님이가 혹시 맨첨에 고양이식빵자세",
          "original_form": "연님이가 혹시 맨첨에 고양이식빵자세",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039669.1.21",
          "form": "그 온도 쟀을때 가만히 있던 냥이가",
          "original_form": "그 온도 쟀을때 가만히 있던 냥이가",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039669.1.22",
          "form": "맞나유??",
          "original_form": "맞나유??",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039669.1.23",
          "form": "음...? 아닌거같은데요",
          "original_form": "음...? 아닌거같은데요",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039669.1.24",
          "form": "ㅋㅋㅋㅋ무래기엄마요",
          "original_form": "ㅋㅋㅋㅋ무래기엄마요",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039669.1.25",
          "form": "헉 딴 친구인가봐요",
          "original_form": "헉 딴 친구인가봐요",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039669.1.26",
          "form": "아직 제대로 못외웠습니다(머쓱",
          "original_form": "아직 제대로 못외웠습니다(머쓱",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100039669.1.27",
          "form": "아 무래기 귀엽구ㅠㅠㅠ 야통이도 넘 귀여워요",
          "original_form": "아 무래기 귀엽구ㅠㅠㅠ 야통이도 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100039669.1.28",
          "form": "마자요 ㅠㅠ 야통이도 장난아니져",
          "original_form": "마자요 ㅠㅠ 야통이도 장난아니져",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039669.1.29",
          "form": "이얏호응 중독성..장난아니에요",
          "original_form": "이얏호응 중독성..장난아니에요",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100039669.1.30",
          "form": "마자요 그래서 결국 알람음 했어요 ㅋㅋㅋㅋ",
          "original_form": "마자요 그래서 결국 알람음 했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100039669.1.31",
          "form": "헉 부럽당",
          "original_form": "헉 부럽당",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100039669.1.32",
          "form": "알람 울릴때마다 이얏호응...",
          "original_form": "알람 울릴때마다 이얏호응...",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100039669.1.33",
          "form": "오랜만에 야통이 봐야겠어요ㅋㅋ",
          "original_form": "오랜만에 야통이 봐야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100039669.1.34",
          "form": "그쵸오오 근데 고양이소린지는 다들 모르더라구요",
          "original_form": "그쵸오오 근데 고양이소린지는 다들 모르더라구요",
          "speaker_id": "1",
          "time": "20210904 01:52"
        }
      ]
