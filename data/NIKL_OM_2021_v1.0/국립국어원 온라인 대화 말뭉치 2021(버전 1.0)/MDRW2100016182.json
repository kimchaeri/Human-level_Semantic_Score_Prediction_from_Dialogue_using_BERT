        },
        {
          "id": "MDRW2100016182.1.6",
          "form": "와 축하드립니다~",
          "original_form": "와 축하드립니다~",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016182.1.7",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016182.1.8",
          "form": "넵 연애중입니다",
          "original_form": "넵 연애중입니다",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016182.1.9",
          "form": "연애도 결혼도 좋은데",
          "original_form": "연애도 결혼도 좋은데",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016182.1.10",
          "form": "육아는 무서워요",
          "original_form": "육아는 무서워요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016182.1.11",
          "form": "마음의준비를 한참 더해야합니다.. ㅎㅎ",
          "original_form": "마음의준비를 한참 더해야합니다.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016182.1.12",
          "form": "결혼하실분 이상형이신가요",
          "original_form": "결혼하실분 이상형이신가요",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016182.1.13",
          "form": "비혼주의자를 결혼으로 바꿔준사람이에요",
          "original_form": "비혼주의자를 결혼으로 바꿔준사람이에요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016182.1.14",
          "form": "와 감동이에요",
          "original_form": "와 감동이에요",
          "speaker_id": "1",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016182.1.15",
          "form": "결혼계획잇으신가요?",
          "original_form": "결혼계획잇으신가요?",
