      "utterance": [
        {
          "id": "MDRW2100012901.1.1",
          "form": "아이는 있으신가요 ㅎㅎ",
          "original_form": "아이는 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:44"
        },
        {
          "id": "MDRW2100012901.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 07:44"
        },
        {
          "id": "MDRW2100012901.1.3",
          "form": "둘이요",
          "original_form": "둘이요",
          "speaker_id": "2",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100012901.1.4",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100012901.1.5",
          "form": "아뇨 아직 ㅎㅎ",
          "original_form": "아뇨 아직 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100012901.1.6",
          "form": "아 네",
          "original_form": "아 네",
          "speaker_id": "2",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100012901.1.7",
          "form": "이제 가져볼까 준비중이에요",
          "original_form": "이제 가져볼까 준비중이에요",
          "speaker_id": "1",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100012901.1.8",
          "form": "준비중이시군요",
          "original_form": "준비중이시군요",
          "speaker_id": "2",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100012901.1.9",
          "form": "다들 그렇더라구요",
          "original_form": "다들 그렇더라구요",
          "speaker_id": "2",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100012901.1.10",
          "form": "저도 좀있다 나았어요 ㅎㅎ",
          "original_form": "저도 좀있다 나았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:46"
        },
        {
          "id": "MDRW2100012901.1.11",
          "form": "둘째는 나이 터울이 좀 있구요",
          "original_form": "둘째는 나이 터울이 좀 있구요",
          "speaker_id": "2",
          "time": "20210825 07:46"
        },
        {
          "id": "MDRW2100012901.1.12",
          "form": "애기 귀여운데",
          "original_form": "애기 귀여운데",
          "speaker_id": "1",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.13",
          "form": "예쁘죠 ㅎㅎ",
          "original_form": "예쁘죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.14",
          "form": "가끔 보는거랑",
          "original_form": "가끔 보는거랑",
          "speaker_id": "1",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.15",
          "form": "직접 키우는거랑은",
          "original_form": "직접 키우는거랑은",
          "speaker_id": "1",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.16",
          "form": "차이가 있죠?ㅋㅋ",
          "original_form": "차이가 있죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.17",
          "form": "그리고 자기랑 닮은게 신기해서 더이뿌죠",
          "original_form": "그리고 자기랑 닮은게 신기해서 더이뿌죠",
          "speaker_id": "2",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.18",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.19",
          "form": "완전다르죠",
          "original_form": "완전다르죠",
          "speaker_id": "2",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.20",
          "form": "네 아이니까 밤에 잠도 못자고 키우죠",
          "original_form": "네 아이니까 밤에 잠도 못자고 키우죠",
          "speaker_id": "2",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.21",
          "form": "정말 힘들어요",
          "original_form": "정말 힘들어요",
          "speaker_id": "2",
          "time": "20210825 07:47"
        },
        {
          "id": "MDRW2100012901.1.22",
          "form": "그전에 두분이서 맘껏 놀고 즐기고 하세요",
          "original_form": "그전에 두분이서 맘껏 놀고 즐기고 하세요",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.23",
          "form": "애생기면 밤낮으로 바빠요",
          "original_form": "애생기면 밤낮으로 바빠요",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.24",
          "form": "교대시면 밤에 잠잘때 힘드실수도 ㅜㅜ",
          "original_form": "교대시면 밤에 잠잘때 힘드실수도 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.25",
          "form": "ㅋㅋ둘다 교대근무라서",
          "original_form": "ㅋㅋ둘다 교대근무라서",
          "speaker_id": "1",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.26",
          "form": "그래도 아이들 금방크니 괜찮아요",
          "original_form": "그래도 아이들 금방크니 괜찮아요",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.27",
          "form": "어 그럼",
          "original_form": "어 그럼",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.28",
          "form": "돌아가면서 키워야죠 ㅎㅎ",
          "original_form": "돌아가면서 키워야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.29",
          "form": "육아가 힘드실텐데",
          "original_form": "육아가 힘드실텐데",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 07:48"
        },
        {
          "id": "MDRW2100012901.1.31",
          "form": "서로 교대해야지 잠을 못자요",
          "original_form": "서로 교대해야지 잠을 못자요",
          "speaker_id": "2",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012901.1.32",
          "form": "네 서로 조 엇갈리게 해서 ㅎㅎ",
          "original_form": "네 서로 조 엇갈리게 해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012901.1.33",
          "form": "그래도 좀크면 잘자요",
          "original_form": "그래도 좀크면 잘자요",
          "speaker_id": "2",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012901.1.34",
          "form": "그래야죠",
          "original_form": "그래야죠",
          "speaker_id": "2",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012901.1.35",
          "form": "그리고 공기업이라서 휴직은 좀 눈치 안보고",
          "original_form": "그리고 공기업이라서 휴직은 좀 눈치 안보고",
          "speaker_id": "1",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012901.1.36",
          "form": "오잘 도와주신다니 다행이네요",
          "original_form": "오잘 도와주신다니 다행이네요",
          "speaker_id": "2",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012901.1.37",
          "form": "쓸 수 있어서 ㅎㅎ",
          "original_form": "쓸 수 있어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012901.1.38",
          "form": "정말좋은데요",
          "original_form": "정말좋은데요",
          "speaker_id": "2",
          "time": "20210825 07:50"
        },
        {
          "id": "MDRW2100012901.1.39",
          "form": "진급 좀 느리게 가면 되죠 뭐ㅋㅋ",
          "original_form": "진급 좀 느리게 가면 되죠 뭐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 07:50"
        },
        {
          "id": "MDRW2100012901.1.40",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "2",
          "time": "20210825 07:50"
        },
        {
          "id": "MDRW2100012901.1.41",
          "form": "아이 후딱 키우고 진급하면되죠",
          "original_form": "아이 후딱 키우고 진급하면되죠",
          "speaker_id": "2",
          "time": "20210825 07:50"
        },
        {
          "id": "MDRW2100012901.1.42",
          "form": "딱 1년만 고생하면 괜찮아요",
          "original_form": "딱 1년만 고생하면 괜찮아요",
          "speaker_id": "2",
          "time": "20210825 07:50"
        },
        {
          "id": "MDRW2100012901.1.43",
          "form": "아 예쁜아기 낳으셔요",
          "original_form": "아 예쁜아기 낳으셔요",
          "speaker_id": "2",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100012901.1.44",
          "form": "전 언제적인지 ㅋㅋ",
          "original_form": "전 언제적인지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100012901.1.45",
          "form": "ㅋㅋ 네 일단 노력부터",
          "original_form": "ㅋㅋ 네 일단 노력부터",
          "speaker_id": "1",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100012901.1.46",
          "form": "그래야죠 ㅎㅎ",
          "original_form": "그래야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100012901.1.47",
          "form": "같은회사 사내 커플인가봐요",
          "original_form": "같은회사 사내 커플인가봐요",
          "speaker_id": "2",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100012901.1.48",
          "form": "네 술먹다가",
          "original_form": "네 술먹다가",
          "speaker_id": "1",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012901.1.49",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012901.1.50",
          "form": "ㅋㅋㅋ친해져서요",
          "original_form": "ㅋㅋㅋ친해져서요",
          "speaker_id": "1",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012901.1.51",
          "form": "서로 좋아하나봐요 술",
          "original_form": "서로 좋아하나봐요 술",
          "speaker_id": "2",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012901.1.52",
          "form": "술이 사람을 부드럽고 들뜨게하죠",
          "original_form": "술이 사람을 부드럽고 들뜨게하죠",
          "speaker_id": "2",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012901.1.53",
          "form": "가까워지고",
          "original_form": "가까워지고",
          "speaker_id": "2",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012901.1.54",
          "form": "저희도 술 엄청마시고 친해졌어요 호",
          "original_form": "저희도 술 엄청마시고 친해졌어요 호",
          "speaker_id": "2",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012901.1.55",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012901.1.56",
          "form": "ㅋㅋ어떻게",
          "original_form": "ㅋㅋ어떻게",
          "speaker_id": "1",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012901.1.57",
          "form": "만나셨나요?",
          "original_form": "만나셨나요?",
          "speaker_id": "1",
          "time": "20210825 07:53"
        },
        {
          "id": "MDRW2100012901.1.58",
          "form": "회사에서요",
          "original_form": "회사에서요",
          "speaker_id": "2",
          "time": "20210825 07:55"
        },
        {
          "id": "MDRW2100012901.1.59",
          "form": "달리 만나기보다 가까이서 가까워지기 편하죠",
          "original_form": "달리 만나기보다 가까이서 가까워지기 편하죠",
          "speaker_id": "2",
          "time": "20210825 07:56"
        },
        {
          "id": "MDRW2100012901.1.60",
          "form": "저희도 사내커플요 ㅎㅎ",
          "original_form": "저희도 사내커플요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 07:56"
        },
        {
          "id": "MDRW2100012901.1.61",
          "form": "제가 이직했어요 ㅋㅋ부담되서",
          "original_form": "제가 이직했어요 ㅋㅋ부담되서",
          "speaker_id": "2",
          "time": "20210825 07:56"
        },
        {
          "id": "MDRW2100012901.1.62",
          "form": "근데 결혼은 성격이 반대로 하나봐요",
          "original_form": "근데 결혼은 성격이 반대로 하나봐요",
          "speaker_id": "2",
          "time": "20210825 07:57"
        },
        {
          "id": "MDRW2100012901.1.63",
          "form": "저랑 성격반대예요",
          "original_form": "저랑 성격반대예요",
          "speaker_id": "2",
          "time": "20210825 07:57"
        },
        {
          "id": "MDRW2100012901.1.64",
          "form": "성격이 서로 같은가요?",
          "original_form": "성격이 서로 같은가요?",
          "speaker_id": "2",
          "time": "20210825 07:57"
        },
        {
          "id": "MDRW2100012901.1.65",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 08:01"
        },
        {
          "id": "MDRW2100012901.1.66",
          "form": "반대에요",
          "original_form": "반대에요",
          "speaker_id": "1",
          "time": "20210825 08:01"
        },
        {
          "id": "MDRW2100012901.1.67",
          "form": "저는 좀 외향적이고",
          "original_form": "저는 좀 외향적이고",
          "speaker_id": "1",
          "time": "20210825 08:01"
        },
        {
          "id": "MDRW2100012901.1.68",
          "form": "와이프는 집순이에요",
          "original_form": "와이프는 집순이에요",
          "speaker_id": "1",
          "time": "20210825 08:01"
        },
        {
          "id": "MDRW2100012901.1.69",
          "form": "오저랑 같네요",
          "original_form": "오저랑 같네요",
          "speaker_id": "2",
          "time": "20210825 08:03"
        },
        {
