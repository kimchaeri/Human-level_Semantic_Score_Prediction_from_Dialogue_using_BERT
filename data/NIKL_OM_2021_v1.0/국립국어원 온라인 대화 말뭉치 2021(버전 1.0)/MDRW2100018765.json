        },
        {
          "id": "MDRW2100018765.1.4",
          "form": "게임에 미쳐사는군",
          "original_form": "게임에 미쳐사는군",
          "speaker_id": "1",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018765.1.5",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018765.1.6",
          "form": "도전을 받아주죠",
          "original_form": "도전을 받아주죠",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018765.1.7",
          "form": "저 어제도 지옥의.밸런스 3시까지 하자 잤어요",
          "original_form": "저 어제도 지옥의.밸런스 3시까지 하자 잤어요",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018765.1.8",
          "form": "테런 vs메이플",
          "original_form": "테런 vs메이플",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018765.1.9",
          "form": "조아요 님 그러면",
          "original_form": "조아요 님 그러면",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
