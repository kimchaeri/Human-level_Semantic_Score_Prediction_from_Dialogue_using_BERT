          "speaker_id": "1",
          "time": "20210831 23:41"
        },
        {
          "id": "MDRW2100037429.1.4",
          "form": "내일 머먹으새요?",
          "original_form": "내일 머먹으새요?",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100037429.1.5",
          "form": "저는 동생이랑 명란파스타 묵으려고용",
          "original_form": "저는 동생이랑 명란파스타 묵으려고용",
          "speaker_id": "1",
          "time": "20210831 23:49"
        },
        {
          "id": "MDRW2100037429.1.6",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210831 23:52"
        },
        {
          "id": "MDRW2100037429.1.7",
          "form": "네.. 우리 동생이가\r\n너무 고생하셔서",
          "original_form": "네.. 우리 동생이가\r\n너무 고생하셔서",
          "speaker_id": "1",
          "time": "20210831 23:53"
        },
        {
          "id": "MDRW2100037429.1.8",
          "form": "과연 하실까요?",
          "original_form": "과연 하실까요?",
          "speaker_id": "2",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100037429.1.9",
          "form": "아..지치니까 안해야즤",
          "original_form": "아..지치니까 안해야즤",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100037429.1.10",
          "form": "네.. 그럴거같았어요ㅠ",
          "original_form": "네.. 그럴거같았어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100037429.1.11",
          "form": "하면 어쩔건가요",
          "original_form": "하면 어쩔건가요",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100037429.1.12",
          "form": "저는 기대를 안하는 인간이 되었어요",
          "original_form": "저는 기대를 안하는 인간이 되었어요",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100037429.1.13",
          "form": "아핫 그러시구낫",
          "original_form": "아핫 그러시구낫",
          "speaker_id": "1",
          "time": "20210901 00:24"
