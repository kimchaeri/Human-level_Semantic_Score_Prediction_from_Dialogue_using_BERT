      "utterance": [
        {
          "id": "MDRW2100044845.1.1",
          "form": "바르다김선생이여",
          "original_form": "바르다김선생이여",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044845.1.2",
          "form": "대박 얼마야 한줄에",
          "original_form": "대박 얼마야 한줄에",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.3",
          "form": "나중네 먹어봐 중독됨",
          "original_form": "나중네 먹어봐 중독됨",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.4",
          "form": "4500?...",
          "original_form": "4500?...",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.5",
          "form": "대박 바르다가먄 무조간먹어여지",
          "original_form": "대박 바르다가먄 무조간먹어여지",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.6",
          "form": "확실하지 않으넼ㅋㅋㅋ",
          "original_form": "확실하지 않으넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.7",
          "form": "거기서 또 머랑먹어",
          "original_form": "거기서 또 머랑먹어",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.8",
          "form": "가락떡볶이인가",
          "original_form": "가락떡볶이인가",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.9",
          "form": "누들떡볶쓰",
          "original_form": "누들떡볶쓰",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.10",
          "form": "와 그건또뭐여",
          "original_form": "와 그건또뭐여",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.11",
          "form": "주변애 바르다 없어",
          "original_form": "주변애 바르다 없어",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044845.1.12",
          "form": "이분안에 하나만빨리하자",
          "original_form": "이분안에 하나만빨리하자",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.13",
          "form": "무슨김치조아해",
          "original_form": "무슨김치조아해",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.14",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.15",
          "form": "배추김치 짜ㅏㅇ",
          "original_form": "배추김치 짜ㅏㅇ",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.16",
          "form": "물김치는?",
          "original_form": "물김치는?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.17",
          "form": "맛잇지",
          "original_form": "맛잇지",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.18",
          "form": "근데 찾기 어려워",
          "original_form": "근데 찾기 어려워",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.19",
          "form": "시워워원한게 맛잇자너",
          "original_form": "시워워원한게 맛잇자너",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.20",
          "form": "마자 은근 없어",
          "original_form": "마자 은근 없어",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044845.1.21",
          "form": "ㅇㅇ 요즘 잘 못봐",
          "original_form": "ㅇㅇ 요즘 잘 못봐",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.22",
          "form": "갓김치인가 그거 먹어봄?",
          "original_form": "갓김치인가 그거 먹어봄?",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.23",
          "form": "ㅇㅇ맛있엉",
          "original_form": "ㅇㅇ맛있엉",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.24",
          "form": "안먹어봄?",
          "original_form": "안먹어봄?",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.25",
          "form": "진짜맛잇다던데",
          "original_form": "진짜맛잇다던데",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.26",
          "form": "헐 궁금하다",
          "original_form": "헐 궁금하다",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.27",
          "form": "깍두기 조아해?",
          "original_form": "깍두기 조아해?",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.28",
          "form": "ㅋㅋㅋ츄라이할게 많네",
          "original_form": "ㅋㅋㅋ츄라이할게 많네",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.29",
          "form": "깍두기보다 총각김치",
          "original_form": "깍두기보다 총각김치",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044845.1.30",
          "form": "나는 총각김치조아해",
          "original_form": "나는 총각김치조아해",
          "speaker_id": "1",
          "time": "20210907 23:59"
        }
      ]
