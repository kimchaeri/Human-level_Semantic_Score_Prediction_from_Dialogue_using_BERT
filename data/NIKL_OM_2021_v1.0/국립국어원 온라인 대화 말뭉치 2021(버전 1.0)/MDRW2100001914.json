      "utterance": [
        {
          "id": "MDRW2100001914.1.1",
          "form": "올림픽 경기 보셨어요?",
          "original_form": "올림픽 경기 보셨어요?",
          "speaker_id": "1",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001914.1.2",
          "form": "올림픽 경기보셨어요?",
          "original_form": "올림픽 경기보셨어요?",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001914.1.3",
          "form": "전 보통 재방송으로 봤어요",
          "original_form": "전 보통 재방송으로 봤어요",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001914.1.4",
          "form": "제가 보면 질까봐서요",
          "original_form": "제가 보면 질까봐서요",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001914.1.5",
          "form": "그래서 항상 뉴스나 재방송으로 결과 알아요",
          "original_form": "그래서 항상 뉴스나 재방송으로 결과 알아요",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001914.1.6",
          "form": "그렇셨군요 전 양궁이 오랜만에 봐서 재밌더라고요",
          "original_form": "그렇셨군요 전 양궁이 오랜만에 봐서 재밌더라고요",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001914.1.7",
          "form": "다 보셨어요?",
          "original_form": "다 보셨어요?",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001914.1.8",
          "form": "양궁 진짜 대단하더라구요",
          "original_form": "양궁 진짜 대단하더라구요",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001914.1.9",
          "form": "저도 양궁 계속할걸 ㅋㅋㅋㅋ",
          "original_form": "저도 양궁 계속할걸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001914.1.10",
          "form": "후회해요",
          "original_form": "후회해요",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001914.1.11",
          "form": "양궁하셨어요? 대박",
          "original_form": "양궁하셨어요? 대박",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001914.1.12",
          "form": "네 어릴때 했어요",
          "original_form": "네 어릴때 했어요",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001914.1.13",
          "form": "초등학생때랑 중학생때 초반에요",
          "original_form": "초등학생때랑 중학생때 초반에요",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001914.1.14",
          "form": "양궁팀 있는 학교였거든요",
          "original_form": "양궁팀 있는 학교였거든요",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001914.1.15",
          "form": "오 전 몸치라서 운동선수 부럽던데",
          "original_form": "오 전 몸치라서 운동선수 부럽던데",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001914.1.16",
          "form": "전 뛰고 노는걸 좋아해서 했는데 집에서 반대했어요",
          "original_form": "전 뛰고 노는걸 좋아해서 했는데 집에서 반대했어요",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001914.1.17",
          "form": "공부나 하라고 ㅋㅋㅋㅋ",
          "original_form": "공부나 하라고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001914.1.18",
          "form": "노란색 텐 맞출때 멋있지 않아요?",
          "original_form": "노란색 텐 맞출때 멋있지 않아요?",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001914.1.19",
          "form": "완전 통쾌!",
          "original_form": "완전 통쾌!",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001914.1.20",
          "form": "맞아요 막내들이 잘하더라고요",
          "original_form": "맞아요 막내들이 잘하더라고요",
          "speaker_id": "1",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001914.1.21",
          "form": "귀여워요 둘이 성격이 다르지만",
          "original_form": "귀여워요 둘이 성격이 다르지만",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001914.1.22",
          "form": "이번에 어린 선수들이 다 멋있었죠",
          "original_form": "이번에 어린 선수들이 다 멋있었죠",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001914.1.23",
          "form": "네 완전 귀엽더라고요",
          "original_form": "네 완전 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001914.1.24",
          "form": "다음 올림픽에 프랑스 유적지에서 한다던데 완전 기대되네요",
          "original_form": "다음 올림픽에 프랑스 유적지에서 한다던데 완전 기대되네요",
          "speaker_id": "2",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001914.1.25",
          "form": "그때는 무관중이 없어야할텐데요",
          "original_form": "그때는 무관중이 없어야할텐데요",
          "speaker_id": "1",
          "time": "20210819 22:22"
        }
      ]
