      "utterance": [
        {
          "id": "MDRW2100016783.1.1",
          "form": "오늘은 날씨가 화창하네요~",
          "original_form": "오늘은 날씨가 화창하네요~",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016783.1.2",
          "form": "네 모처럼 비가 안오네요",
          "original_form": "네 모처럼 비가 안오네요",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016783.1.3",
          "form": "비가 오면셔 흐리니 기분도 다운되더라구요ㅠ",
          "original_form": "비가 오면셔 흐리니 기분도 다운되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016783.1.4",
          "form": "맞아요 날씨의 영향이 크죠",
          "original_form": "맞아요 날씨의 영향이 크죠",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016783.1.5",
          "form": "그치만 또 비오는날 좋아하는분들도 계시더라구요~",
          "original_form": "그치만 또 비오는날 좋아하는분들도 계시더라구요~",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016783.1.6",
          "form": "낭만 ㅎ",
          "original_form": "낭만 ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016783.1.7",
          "form": "네 저도 여름에 비오고나면 시원해져서 좋았는데",
          "original_form": "네 저도 여름에 비오고나면 시원해져서 좋았는데",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016783.1.8",
          "form": "요즘 너무 자주오니 컨디션도 쳐지고",
          "original_form": "요즘 너무 자주오니 컨디션도 쳐지고",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016783.1.9",
          "form": "맑은 날이 그리웠어요",
          "original_form": "맑은 날이 그리웠어요",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016783.1.10",
          "form": "너무 꿉꿉해서 힘들더라구요ㅠㅠ!!",
          "original_form": "너무 꿉꿉해서 힘들더라구요ㅠㅠ!!",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016783.1.11",
          "form": "네 비 오니까 축축하고",
          "original_form": "네 비 오니까 축축하고",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016783.1.12",
          "form": "강하게 오니 우산써도 다젖고ㅠㅠ 밖에다니기힘들어요ㅠ",
          "original_form": "강하게 오니 우산써도 다젖고ㅠㅠ 밖에다니기힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016783.1.13",
          "form": "가을이 짧아지니 아쉬워요ㅠ",
          "original_form": "가을이 짧아지니 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016783.1.14",
          "form": "네 봄가을이 짧아져서",
          "original_form": "네 봄가을이 짧아져서",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016783.1.15",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016783.1.16",
          "form": "그쳐ㅠㅠ 그래도 전 여름보다 겨울이 좋아서 ㅠ 다행이네요ㅠ",
          "original_form": "그쳐ㅠㅠ 그래도 전 여름보다 겨울이 좋아서 ㅠ 다행이네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016783.1.17",
          "form": "네 더위보단 추위가",
          "original_form": "네 더위보단 추위가",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016783.1.18",
          "form": "더운건 견디기가힘들더라구요ㅠㅠ 실내야 에어콘틀면 좋긴해도 이동하고할때ㅠ",
          "original_form": "더운건 견디기가힘들더라구요ㅠㅠ 실내야 에어콘틀면 좋긴해도 이동하고할때ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016783.1.19",
          "form": "네네 더위 너무 지치고ㅠ",
          "original_form": "네네 더위 너무 지치고ㅠ",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016783.1.20",
          "form": "여름은 ㅠㅠ옷도 신경쓰여요 ㅠㅠㅋㅋㅋ 뚱",
          "original_form": "여름은 ㅠㅠ옷도 신경쓰여요 ㅠㅠㅋㅋㅋ 뚱",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016783.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016783.1.22",
          "form": "땀도 많이 나고",
          "original_form": "땀도 많이 나고",
          "speaker_id": "1",
          "time": "20210826 16:01"
        }
      ]
    }
