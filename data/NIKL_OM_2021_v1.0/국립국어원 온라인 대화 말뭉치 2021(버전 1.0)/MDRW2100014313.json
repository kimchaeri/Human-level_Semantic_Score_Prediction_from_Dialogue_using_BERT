      "utterance": [
        {
          "id": "MDRW2100014313.1.1",
          "form": "온동도 안하면서",
          "original_form": "온동도 안하면서",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014313.1.2",
          "form": "사면 운동을 할수도 있지 사고 싶다",
          "original_form": "사면 운동을 할수도 있지 사고 싶다",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014313.1.3",
          "form": "저번에 산 츄리닝세트 있잖아",
          "original_form": "저번에 산 츄리닝세트 있잖아",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014313.1.4",
          "form": "그건 이제 입기 싫고",
          "original_form": "그건 이제 입기 싫고",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014313.1.5",
          "form": "봐라 그러면서 또 산다고  하는데",
          "original_form": "봐라 그러면서 또 산다고  하는데",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014313.1.6",
          "form": "이쁘니 사고 싶은거지",
          "original_form": "이쁘니 사고 싶은거지",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014313.1.7",
          "form": "사고 싶다고 다 사면 망한다",
          "original_form": "사고 싶다고 다 사면 망한다",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014313.1.8",
          "form": "ㅜ.ㅜ 슬프네",
          "original_form": "ㅜ.ㅜ 슬프네",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014313.1.9",
          "form": "난중에 사라 점정 이쁜게 많이 나올꺼야",
          "original_form": "난중에 사라 점정 이쁜게 많이 나올꺼야",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014313.1.10",
          "form": "홈쇼핑 보면 진짜 혹한다",
          "original_form": "홈쇼핑 보면 진짜 혹한다",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014313.1.11",
          "form": "보지마라",
          "original_form": "보지마라",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014313.1.12",
          "form": "츄리닝도 사고 싶고 캘럭시 기어도 사고 싶다",
          "original_form": "츄리닝도 사고 싶고 캘럭시 기어도 사고 싶다",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014313.1.13",
          "form": "필요없는거다",
          "original_form": "필요없는거다",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014313.1.14",
          "form": "나 오늘 드디어 침대테이블 샀음",
          "original_form": "나 오늘 드디어 침대테이블 샀음",
