      "utterance": [
        {
          "id": "MDRW2100012873.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 05:46"
        },
        {
          "id": "MDRW2100012873.1.2",
          "form": "좋아하는 책 있으신가요?",
          "original_form": "좋아하는 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 05:46"
        },
        {
          "id": "MDRW2100012873.1.3",
          "form": "저는 이북 로판만 읽네요 ㅎㅎㅎ",
          "original_form": "저는 이북 로판만 읽네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 05:46"
        },
        {
          "id": "MDRW2100012873.1.4",
          "form": "어.. 저는 시간 날 때 읽으려고 노력하는 편이에요",
          "original_form": "어.. 저는 시간 날 때 읽으려고 노력하는 편이에요",
          "speaker_id": "1",
          "time": "20210825 05:46"
        },
        {
          "id": "MDRW2100012873.1.5",
          "form": "아 로맨스 판타지 좋아하시는구나",
          "original_form": "아 로맨스 판타지 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210825 05:46"
        },
        {
          "id": "MDRW2100012873.1.6",
          "form": "웹툰 같은 것도 보시나요?",
          "original_form": "웹툰 같은 것도 보시나요?",
          "speaker_id": "1",
          "time": "20210825 05:46"
        },
        {
          "id": "MDRW2100012873.1.7",
          "form": "오 무슨종류 좋아하세요?",
          "original_form": "오 무슨종류 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 05:46"
        },
        {
          "id": "MDRW2100012873.1.8",
          "form": "아 저는 그냥 소설은 가리지 않고 있는 것 같아요",
          "original_form": "아 저는 그냥 소설은 가리지 않고 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 05:47"
        },
        {
          "id": "MDRW2100012873.1.9",
          "form": "읽는 것 같아요",
          "original_form": "읽는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 05:47"
        },
        {
          "id": "MDRW2100012873.1.10",
          "form": "웹툰은 잘 안보고 카카페 시리즈 리디 웹소설 봐요",
          "original_form": "웹툰은 잘 안보고 카카페 시리즈 리디 웹소설 봐요",
          "speaker_id": "2",
          "time": "20210825 05:47"
        },
        {
          "id": "MDRW2100012873.1.11",
          "form": "우와 멋지다",
          "original_form": "우와 멋지다",
          "speaker_id": "2",
          "time": "20210825 05:47"
        },
        {
          "id": "MDRW2100012873.1.12",
          "form": "아하 그러시군요",
          "original_form": "아하 그러시군요",
          "speaker_id": "1",
          "time": "20210825 05:47"
        },
        {
          "id": "MDRW2100012873.1.13",
          "form": "제일 좋아하시는 책은 뭐세요?",
          "original_form": "제일 좋아하시는 책은 뭐세요?",
          "speaker_id": "2",
          "time": "20210825 05:47"
        },
        {
          "id": "MDRW2100012873.1.14",
          "form": "저는 소설은 앵무새죽이기 좋아해요.",
          "original_form": "저는 소설은 앵무새죽이기 좋아해요.",
          "speaker_id": "2",
          "time": "20210825 05:48"
        },
        {
          "id": "MDRW2100012873.1.15",
          "form": "저는 모모 좋아해요",
          "original_form": "저는 모모 좋아해요",
          "speaker_id": "1",
          "time": "20210825 05:48"
        },
        {
          "id": "MDRW2100012873.1.16",
          "form": "모모 ㅎㅎㅎ저도 좋아해요",
          "original_form": "모모 ㅎㅎㅎ저도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 05:48"
        },
        {
          "id": "MDRW2100012873.1.17",
          "form": "약간 읽을 때마다 좀 새로운 것 같아요",
          "original_form": "약간 읽을 때마다 좀 새로운 것 같아요",
          "speaker_id": "1",
          "time": "20210825 05:49"
        },
        {
          "id": "MDRW2100012873.1.18",
          "form": "앵무새 죽이기는 어떤 내용인가요",
          "original_form": "앵무새 죽이기는 어떤 내용인가요",
          "speaker_id": "1",
          "time": "20210825 05:49"
        },
        {
          "id": "MDRW2100012873.1.19",
          "form": "진짜 좋은 책은 그런거같아요",
          "original_form": "진짜 좋은 책은 그런거같아요",
          "speaker_id": "2",
          "time": "20210825 05:49"
        },
        {
          "id": "MDRW2100012873.1.20",
          "form": "아 차별 관련 사건을 아이시선으로 풀어낸책이예요",
          "original_form": "아 차별 관련 사건을 아이시선으로 풀어낸책이예요",
          "speaker_id": "2",
          "time": "20210825 05:50"
        },
        {
          "id": "MDRW2100012873.1.21",
          "form": "오래된책이라 ㅎㅎ",
          "original_form": "오래된책이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 05:50"
        },
        {
          "id": "MDRW2100012873.1.22",
