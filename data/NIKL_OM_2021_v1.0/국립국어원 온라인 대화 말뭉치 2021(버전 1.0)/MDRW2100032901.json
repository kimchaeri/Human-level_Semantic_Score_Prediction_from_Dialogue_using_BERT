      "utterance": [
        {
          "id": "MDRW2100032901.1.1",
          "form": "어제 내카드로 지마켓에서 뭐산거야??",
          "original_form": "어제 내카드로 지마켓에서 뭐산거야??",
          "speaker_id": "2",
          "time": "20210906 04:36"
        },
        {
          "id": "MDRW2100032901.1.2",
          "form": "아 불닭.ㅋㅋㅋㅋㅋ",
          "original_form": "아 불닭.ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:36"
        },
        {
          "id": "MDRW2100032901.1.3",
          "form": "요즘 붉닭 자주 먹네??",
          "original_form": "요즘 붉닭 자주 먹네??",
          "speaker_id": "2",
          "time": "20210906 04:36"
        },
        {
          "id": "MDRW2100032901.1.4",
          "form": "어 아침에 하나씩 먹게 되는듯??",
          "original_form": "어 아침에 하나씩 먹게 되는듯??",
          "speaker_id": "1",
          "time": "20210906 04:36"
        },
        {
          "id": "MDRW2100032901.1.5",
          "form": "왜 스트레스 받아서??",
          "original_form": "왜 스트레스 받아서??",
          "speaker_id": "2",
          "time": "20210906 04:37"
        },
        {
          "id": "MDRW2100032901.1.6",
          "form": "뭐 그럴지도 모르지만.. 매운게 땡기더라고",
          "original_form": "뭐 그럴지도 모르지만.. 매운게 땡기더라고",
          "speaker_id": "1",
          "time": "20210906 04:37"
        },
        {
          "id": "MDRW2100032901.1.7",
          "form": "휴 그래 많이 먹어라.",
          "original_form": "휴 그래 많이 먹어라.",
          "speaker_id": "2",
          "time": "20210906 04:37"
        },
        {
          "id": "MDRW2100032901.1.8",
          "form": "몇개 샀어??",
          "original_form": "몇개 샀어??",
          "speaker_id": "2",
          "time": "20210906 04:37"
        },
        {
          "id": "MDRW2100032901.1.9",
          "form": "8개.ㅋㅋㅋㅋ",
          "original_form": "8개.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:37"
        },
        {
          "id": "MDRW2100032901.1.10",
          "form": "핫딜이더라고.. 그래서.. 여보아이디랑 내아이디랑 둘다 써서... 총.. 16개..",
          "original_form": "핫딜이더라고.. 그래서.. 여보아이디랑 내아이디랑 둘다 써서... 총.. 16개..",
          "speaker_id": "1",
          "time": "20210906 04:37"
        },
        {
          "id": "MDRW2100032901.1.11",
          "form": "그거 냉장고에 들어 갈 곳은 있나??",
          "original_form": "그거 냉장고에 들어 갈 곳은 있나??",
          "speaker_id": "2",
          "time": "20210906 04:38"
        },
        {
          "id": "MDRW2100032901.1.12",
          "form": "그럼...",
          "original_form": "그럼...",
          "speaker_id": "1",
          "time": "20210906 04:38"
        },
        {
          "id": "MDRW2100032901.1.13",
          "form": "냉동실 차근차근 다 비워놨습죠.ㅋㅋㅋ",
          "original_form": "냉동실 차근차근 다 비워놨습죠.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:38"
        },
        {
          "id": "MDRW2100032901.1.14",
          "form": "잘했다!",
          "original_form": "잘했다!",
          "speaker_id": "2",
          "time": "20210906 04:38"
        },
        {
          "id": "MDRW2100032901.1.15",
          "form": "말에 뼈가 있는데??",
          "original_form": "말에 뼈가 있는데??",
          "speaker_id": "1",
          "time": "20210906 04:38"
        },
        {
          "id": "MDRW2100032901.1.16",
          "form": "아니야... 들어갈 곳 없을까봐..16개...",
          "original_form": "아니야... 들어갈 곳 없을까봐..16개...",
          "speaker_id": "2",
          "time": "20210906 04:38"
        },
        {
          "id": "MDRW2100032901.1.17",
          "form": "좀 많이 사긴 했다.ㅋㅋㅋ",
          "original_form": "좀 많이 사긴 했다.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:39"
        },
        {
          "id": "MDRW2100032901.1.18",
          "form": "좀???",
          "original_form": "좀???",
          "speaker_id": "2",
          "time": "20210906 04:39"
        },
        {
          "id": "MDRW2100032901.1.19",
