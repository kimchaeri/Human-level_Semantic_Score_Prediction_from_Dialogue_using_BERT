      "utterance": [
        {
          "id": "MDRW2100039829.1.1",
          "form": "음료 뭐 좋아해?",
          "original_form": "음료 뭐 좋아해?",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039829.1.2",
          "form": "커피",
          "original_form": "커피",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039829.1.3",
          "form": "당신 커피 중독이야",
          "original_form": "당신 커피 중독이야",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039829.1.4",
          "form": "나는 물 마셧어",
          "original_form": "나는 물 마셧어",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039829.1.5",
          "form": "저녁은 먹엇니",
          "original_form": "저녁은 먹엇니",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039829.1.6",
          "form": "ㅋㅋㅋㅋ먹었지",
          "original_form": "ㅋㅋㅋㅋ먹었지",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039829.1.7",
          "form": "뭐 먹엇어?",
          "original_form": "뭐 먹엇어?",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039829.1.8",
          "form": "나는 불고기 먹었어",
          "original_form": "나는 불고기 먹었어",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039829.1.9",
          "form": "미역국에 밥말아먹엇어",
          "original_form": "미역국에 밥말아먹엇어",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039829.1.10",
          "form": "미역국 맛있겟다ㅜㅜㅜ",
          "original_form": "미역국 맛있겟다ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039829.1.11",
          "form": "는 당신 생ㅇ일이엇잖아",
          "original_form": "는 당신 생ㅇ일이엇잖아",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039829.1.12",
          "form": "케이크 먹었어?",
          "original_form": "케이크 먹었어?",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039829.1.13",
          "form": "케이크는 아부지 생신파티하고 남은거 있어서 안샀구",
          "original_form": "케이크는 아부지 생신파티하고 남은거 있어서 안샀구",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039829.1.14",
          "form": "타르트 사먹었다",
          "original_form": "타르트 사먹었다",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039829.1.15",
          "form": "손바닥보다 조금 작은거",
          "original_form": "손바닥보다 조금 작은거",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039829.1.16",
          "form": "헉 무슨 맛이야?",
          "original_form": "헉 무슨 맛이야?",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039829.1.17",
          "form": "크랜베리랑 블루베리가 올라가있었는데",
          "original_form": "크랜베리랑 블루베리가 올라가있었는데",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039829.1.18",
          "form": "너무 달지 않고 좋았어",
          "original_form": "너무 달지 않고 좋았어",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039829.1.19",
          "form": "완전 맛있겟다...나도 타르트 좋아해",
          "original_form": "완전 맛있겟다...나도 타르트 좋아해",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039829.1.20",
          "form": "삐에스몽테였나 그 빵집이었는데 좀 비쌌지만 맛있어",
          "original_form": "삐에스몽테였나 그 빵집이었는데 좀 비쌌지만 맛있어",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039829.1.21",
          "form": "헐 어딘지 모르겟는데 이름부터 비싸고 맛잇어보임...",
          "original_form": "헐 어딘지 모르겟는데 이름부터 비싸고 맛잇어보임...",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039829.1.22",
          "form": "파리바게트만 가본 사람으로서...부러워ㅓ",
          "original_form": "파리바게트만 가본 사람으로서...부러워ㅓ",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039829.1.23",
          "form": "일본에서 제빵을 배워오셨대",
          "original_form": "일본에서 제빵을 배워오셨대",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039829.1.24",
          "form": "맛있을 수 밖에",
          "original_form": "맛있을 수 밖에",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039829.1.25",
          "form": "그집 푸딩도 진짜 맛있어",
          "original_form": "그집 푸딩도 진짜 맛있어",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039829.1.26",
          "form": "헐미친 나 푸딩 완전 좋아하는데ㅠㅠㅠ",
          "original_form": "헐미친 나 푸딩 완전 좋아하는데ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039829.1.27",
          "form": "가깝니...",
          "original_form": "가깝니...",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039829.1.28",
          "form": "마트킹 알아? 그 근처인뎅",
          "original_form": "마트킹 알아? 그 근처인뎅",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039829.1.29",
          "form": "안 멀어",
          "original_form": "안 멀어",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039829.1.30",
          "form": "!! 어딘지 알어",
          "original_form": "!! 어딘지 알어",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039829.1.31",
          "form": "헐 가까웟군...",
          "original_form": "헐 가까웟군...",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039829.1.32",
          "form": "가봐야 겟다",
          "original_form": "가봐야 겟다",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039829.1.33",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
          "time": "20210904 00:21"
        }
      ]
    }
