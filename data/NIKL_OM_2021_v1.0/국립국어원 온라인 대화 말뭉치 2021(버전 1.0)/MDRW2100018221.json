      "utterance": [
        {
          "id": "MDRW2100018221.1.1",
          "form": "네 좋아하죠!",
          "original_form": "네 좋아하죠!",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018221.1.2",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018221.1.3",
          "form": "주변 여행하는거 참 재밌어요!",
          "original_form": "주변 여행하는거 참 재밌어요!",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018221.1.4",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210827 06:55"
        },
        {
          "id": "MDRW2100018221.1.5",
          "form": "개인적으로 트래킹을 선호하기도 해요!",
          "original_form": "개인적으로 트래킹을 선호하기도 해요!",
          "speaker_id": "2",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018221.1.6",
          "form": "공원이나 연못 등 걸어서 구경하는거 좋아해용",
          "original_form": "공원이나 연못 등 걸어서 구경하는거 좋아해용",
          "speaker_id": "2",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018221.1.7",
          "form": "경치도 힐링이죠",
          "original_form": "경치도 힐링이죠",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100018221.1.8",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100018221.1.9",
          "form": "강원도가 딱 경치보기 좋은곳이죠",
          "original_form": "강원도가 딱 경치보기 좋은곳이죠",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100018221.1.10",
          "form": "너무 멀아요",
          "original_form": "너무 멀아요",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100018221.1.11",
          "form": "멀긴해도 가볼만하더라고요!",
          "original_form": "멀긴해도 가볼만하더라고요!",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100018221.1.12",
          "form": "도전",
          "original_form": "도전",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018221.1.13",
          "form": "도전해보시는것도 좋아요!",
          "original_form": "도전해보시는것도 좋아요!",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100018221.1.14",
          "form": "네 주말에",
          "original_form": "네 주말에",
          "speaker_id": "1",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100018221.1.15",
          "form": "오 주말에 한번 가보시는건가요",
          "original_form": "오 주말에 한번 가보시는건가요",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100018221.1.16",
          "form": "부럽네요!",
          "original_form": "부럽네요!",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100018221.1.17",
          "form": "비온다거 하던데",
          "original_form": "비온다거 하던데",
          "speaker_id": "1",
          "time": "20210827 23:20"
        }
