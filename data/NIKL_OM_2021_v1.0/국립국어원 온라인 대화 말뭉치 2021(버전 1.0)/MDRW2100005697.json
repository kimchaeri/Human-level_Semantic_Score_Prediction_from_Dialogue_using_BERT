      "utterance": [
        {
          "id": "MDRW2100005697.1.1",
          "form": "저 방금전에",
          "original_form": "저 방금전에",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005697.1.2",
          "form": "오징어젓갈로",
          "original_form": "오징어젓갈로",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005697.1.3",
          "form": "밥먹었읍니다",
          "original_form": "밥먹었읍니다",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005697.1.4",
          "form": "나는 오늘 고등어조리 먹었는데",
          "original_form": "나는 오늘 고등어조리 먹었는데",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005697.1.5",
          "form": "오늘은 해산물의 날이네",
          "original_form": "오늘은 해산물의 날이네",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005697.1.6",
          "form": "앗 조림",
          "original_form": "앗 조림",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005697.1.7",
          "form": "오징어젓갈 맛있었겠다",
          "original_form": "오징어젓갈 맛있었겠다",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005697.1.8",
          "form": "오징어젓갈 근데",
          "original_form": "오징어젓갈 근데",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005697.1.9",
          "form": "엄마가",
          "original_form": "엄마가",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005697.1.10",
          "form": "매워야 맛있다고",
          "original_form": "매워야 맛있다고",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005697.1.11",
          "form": "너무 매워요",
          "original_form": "너무 매워요",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005697.1.12",
          "form": "직접 하신거야?",
          "original_form": "직접 하신거야?",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005697.1.13",
          "form": "오징어젓갈은 보통",
          "original_form": "오징어젓갈은 보통",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005697.1.14",
          "form": "사서 먹지 않나?",
          "original_form": "사서 먹지 않나?",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005697.1.15",
          "form": "젓갈은 샀죠!",
          "original_form": "젓갈은 샀죠!",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005697.1.16",
          "form": "엄청 썰어넣은거예요",
          "original_form": "엄청 썰어넣은거예요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005697.1.17",
          "form": "지금 오징어젓갈로만",
          "original_form": "지금 오징어젓갈로만",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005697.1.18",
          "form": "며칠째 먹고있는데",
          "original_form": "며칠째 먹고있는데",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005697.1.19",
          "form": "맛은 있지만 고통",
          "original_form": "맛은 있지만 고통",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005697.1.20",
          "form": "올해는 청양고추도 맵던데",
          "original_form": "올해는 청양고추도 맵던데",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005697.1.21",
          "form": "작년보다 덜 썰어야",
          "original_form": "작년보다 덜 썰어야",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005697.1.22",
          "form": "비슷할거야",
          "original_form": "비슷할거야",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005697.1.23",
          "form": "하....",
          "original_form": "하....",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005697.1.24",
          "form": "오늘 아점도",
          "original_form": "오늘 아점도",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005697.1.25",
          "form": "오징어젓갈이었는데",
          "original_form": "오징어젓갈이었는데",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.26",
          "form": "동생이랑 같이 먹었거든요",
          "original_form": "동생이랑 같이 먹었거든요",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.27",
          "form": "근데 동생이",
          "original_form": "근데 동생이",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.28",
          "form": "죽는줄알았어요",
          "original_form": "죽는줄알았어요",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.29",
          "form": "아니 맞네",
          "original_form": "아니 맞네",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.30",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.31",
          "form": "고오...추ㅜ...유ㅠ...가",
          "original_form": "고오...추ㅜ...유ㅠ...가",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.32",
          "form": "들어간 문장이 다 안갔내",
          "original_form": "들어간 문장이 다 안갔내",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005697.1.33",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.34",
          "form": "언니 제말어케알아들으심",
          "original_form": "언니 제말어케알아들으심",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.36",
          "form": "제가",
          "original_form": "제가",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.37",
          "form": "동생이...오징어만 골라먹고",
          "original_form": "동생이...오징어만 골라먹고",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.38",
          "form": "청양...그것을 남겨서",
          "original_form": "청양...그것을 남겨서",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.39",
          "form": "제가 그걸 다 먹어야했다는",
          "original_form": "제가 그걸 다 먹어야했다는",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.40",
          "form": "오징어젓갈에 한때 진심이었기에 다 알아들었습니다",
          "original_form": "오징어젓갈에 한때 진심이었기에 다 알아들었습니다",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.41",
          "form": "이야기였답니다",
          "original_form": "이야기였답니다",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.42",
          "form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.43",
          "form": "낙지젓갈도맛있다던데",
          "original_form": "낙지젓갈도맛있다던데",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.44",
          "form": "비싸서",
          "original_form": "비싸서",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.45",
          "form": "못먹어봤어요",
          "original_form": "못먹어봤어요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005697.1.46",
          "form": "어 응응 맛있지",
          "original_form": "어 응응 맛있지",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005697.1.47",
          "form": "오징어젓갈 두배가격이야 ㅠ",
          "original_form": "오징어젓갈 두배가격이야 ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005697.1.48",
          "form": "먹는걸로 가자",
          "original_form": "먹는걸로 가자",
          "speaker_id": "1",
          "time": "20210822 00:00"
        }
      ]
    }
