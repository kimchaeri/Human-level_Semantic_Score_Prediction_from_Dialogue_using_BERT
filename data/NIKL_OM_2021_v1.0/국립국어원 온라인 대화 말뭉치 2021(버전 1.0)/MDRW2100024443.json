      "utterance": [
        {
          "id": "MDRW2100024443.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100024443.1.2",
          "form": "하는 운동 있으신가요?",
          "original_form": "하는 운동 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100024443.1.3",
          "form": "요즘 암벽등반 시작했어요!",
          "original_form": "요즘 암벽등반 시작했어요!",
          "speaker_id": "2",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100024443.1.4",
          "form": "우와! 진짜 멋있어요",
          "original_form": "우와! 진짜 멋있어요",
          "speaker_id": "1",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100024443.1.5",
          "form": "제가 해보고 싶은 운동 중 하나인데ㅜㅜ",
          "original_form": "제가 해보고 싶은 운동 중 하나인데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100024443.1.6",
          "form": "다행히 집근처에 할만한 곳이 있더라구요",
          "original_form": "다행히 집근처에 할만한 곳이 있더라구요",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100024443.1.7",
          "form": "원래 운동 잘 하셨어요?",
          "original_form": "원래 운동 잘 하셨어요?",
          "speaker_id": "1",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100024443.1.8",
          "form": "알아봤는데 손힘이 세야 한다고 해서요ㅠ",
          "original_form": "알아봤는데 손힘이 세야 한다고 해서요ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100024443.1.9",
          "form": "손힘도 중요하지만 다리힘이 더 중요하대요",
          "original_form": "손힘도 중요하지만 다리힘이 더 중요하대요",
          "speaker_id": "2",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100024443.1.10",
          "form": "둘다 사는데 중요한거니 잘됐죠 ㅋㅋ",
          "original_form": "둘다 사는데 중요한거니 잘됐죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100024443.1.11",
          "form": "그냥 운동하시는거예요?",
          "original_form": "그냥 운동하시는거예요?",
          "speaker_id": "1",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100024443.1.12",
          "form": "취미?!",
          "original_form": "취미?!",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100024443.1.13",
          "form": "취미 겸 생존?ㅋㅋㅋㅋ",
          "original_form": "취미 겸 생존?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100024443.1.14",
          "form": "이젠 정말 운동안하면 안되겠다 싶은데",
          "original_form": "이젠 정말 운동안하면 안되겠다 싶은데",
          "speaker_id": "2",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100024443.1.15",
          "form": "와.... 저는 마음만 그렇고 의지가 없어서ㅜㅜ",
          "original_form": "와.... 저는 마음만 그렇고 의지가 없어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 09:56"
        },
        {
          "id": "MDRW2100024443.1.16",
          "form": "건강 때문에 다이어트 해야하는데 운동이 너무 싫은거잇죠..",
          "original_form": "건강 때문에 다이어트 해야하는데 운동이 너무 싫은거잇죠..",
          "speaker_id": "1",
          "time": "20210831 09:56"
        },
        {
          "id": "MDRW2100024443.1.17",
          "form": "저도 싫어해요....",
          "original_form": "저도 싫어해요....",
          "speaker_id": "2",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100024443.1.18",
          "form": "헬스나 필라테스같은건 영 맘이 안가고",
          "original_form": "헬스나 필라테스같은건 영 맘이 안가고",
          "speaker_id": "2",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100024443.1.19",
          "form": "춤같은건 잘 못따라가겠고",
          "original_form": "춤같은건 잘 못따라가겠고",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100024443.1.20",
          "form": "혼자 할 수 있는 것 중에서 그나마 재밌는거 찾은게 암벽등반인거죠",
          "original_form": "혼자 할 수 있는 것 중에서 그나마 재밌는거 찾은게 암벽등반인거죠",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100024443.1.21",
          "form": "멋있어요..",
          "original_form": "멋있어요..",
          "speaker_id": "1",
          "time": "20210831 09:59"
        },
        {
          "id": "MDRW2100024443.1.22",
          "form": "배운지 얼마나 된거예요?",
          "original_form": "배운지 얼마나 된거예요?",
          "speaker_id": "1",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100024443.1.23",
          "form": "얼마 안됐어요",
          "original_form": "얼마 안됐어요",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100024443.1.24",
          "form": "한달정도",
          "original_form": "한달정도",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100024443.1.25",
          "form": "그 이전에 한강공원에 있는 암벽장에서 좀 깔짝여보긴 했지만요",
          "original_form": "그 이전에 한강공원에 있는 암벽장에서 좀 깔짝여보긴 했지만요",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100024443.1.26",
          "form": "멋있다 정말....",
          "original_form": "멋있다 정말....",
          "speaker_id": "1",
          "time": "20210831 10:03"
        }
      ]
    }
  ]
