      "utterance": [
        {
          "id": "MDRW2100023532.1.1",
          "form": "거기 날씨는 어떤가요",
          "original_form": "거기 날씨는 어떤가요",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100023532.1.2",
          "form": "맑아요",
          "original_form": "맑아요",
          "speaker_id": "2",
          "time": "20210830 11:14"
        },
        {
          "id": "MDRW2100023532.1.3",
          "form": "그래요? 여기 흐려요",
          "original_form": "그래요? 여기 흐려요",
          "speaker_id": "1",
          "time": "20210830 11:27"
        },
        {
          "id": "MDRW2100023532.1.4",
          "form": "날씨 좋다니 부럽네요",
          "original_form": "날씨 좋다니 부럽네요",
          "speaker_id": "1",
          "time": "20210830 11:27"
        },
        {
          "id": "MDRW2100023532.1.5",
          "form": "가을장마인가바요",
          "original_form": "가을장마인가바요",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023532.1.6",
          "form": "그니까요 가을장마가 원래 있던 거죠?",
          "original_form": "그니까요 가을장마가 원래 있던 거죠?",
          "speaker_id": "1",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023532.1.7",
          "form": "잘모르겟어요ㅜㅜ 올해 유독 장마가 긴 느낌이에요",
          "original_form": "잘모르겟어요ㅜㅜ 올해 유독 장마가 긴 느낌이에요",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023532.1.8",
          "form": "추석쯤 진짜 가을일듯싶어요 ㅎㅎ",
          "original_form": "추석쯤 진짜 가을일듯싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023532.1.9",
          "form": "그쵸... 차 사자마자 장마왔어요ㅋㅋ",
          "original_form": "그쵸... 차 사자마자 장마왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023532.1.10",
          "form": "추석되면 확 추워지는거 아닌지 모르겠어요",
          "original_form": "추석되면 확 추워지는거 아닌지 모르겠어요",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023532.1.11",
          "form": "앗 ㅋ큐ㅠㅠ",
          "original_form": "앗 ㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023532.1.12",
          "form": "진짜 그럴꺼같아요",
          "original_form": "진짜 그럴꺼같아요",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023532.1.13",
          "form": "가을 더 짧아질듯싶어요 흑흑",
          "original_form": "가을 더 짧아질듯싶어요 흑흑",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023532.1.14",
          "form": "그니까요 가을 별로 안 좋아했는데",
          "original_form": "그니까요 가을 별로 안 좋아했는데",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023532.1.15",
          "form": "그리워지는거같아요",
          "original_form": "그리워지는거같아요",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023532.1.16",
          "form": "어떤계절 좋아하세욤",
          "original_form": "어떤계절 좋아하세욤",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100023532.1.17",
          "form": "원래는 여름이었는데 이번여름 겪으면서 가을로 바뀌었어요ㅋㅋ",
          "original_form": "원래는 여름이었는데 이번여름 겪으면서 가을로 바뀌었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100023532.1.18",
          "form": "여름이 좋으시다니ㅜㅜ\n저는 땀쟁이에 더위쥐약이라서 여름빼고 다좋아여 흑",
          "original_form": "여름이 좋으시다니ㅜㅜ\n저는 땀쟁이에 더위쥐약이라서 여름빼고 다좋아여 흑",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023532.1.19",
          "form": "아ㅋㅋ 전 더위를 안타서 여름좋아했는데\n이번 여름을 겪으면서 여름 싫어졌어요ㅋㅋ",
          "original_form": "아ㅋㅋ 전 더위를 안타서 여름좋아했는데\n이번 여름을 겪으면서 여름 싫어졌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:02"
        }
      ]
