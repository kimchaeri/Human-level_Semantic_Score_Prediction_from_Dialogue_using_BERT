        },
        {
          "id": "MDRW2100038891.1.2",
          "form": "잘 갠찮게하는 회사만나서",
          "original_form": "잘 갠찮게하는 회사만나서",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100038891.1.3",
          "form": "잘하려고하는거같더라",
          "original_form": "잘하려고하는거같더라",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100038891.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ마마무 친구는 솔로앨범 기대하고 있겠네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ마마무 친구는 솔로앨범 기대하고 있겠네",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038891.1.5",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038891.1.6",
          "form": "지난번",
          "original_form": "지난번",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038891.1.7",
          "form": "솔로앨범 완전 맘에들었는데",
          "original_form": "솔로앨범 완전 맘에들었는데",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038891.1.8",
          "form": "안그래돜ㅋㅋㅋㅋ 타멤이랑퀄리티가 달라서 말나오더라고",
          "original_form": "안그래돜ㅋㅋㅋㅋ 타멤이랑퀄리티가 달라서 말나오더라고",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038891.1.9",
          "form": "거기도 슬슬 갠팬화되서",
          "original_form": "거기도 슬슬 갠팬화되서",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038891.1.10",
          "form": "거기도 쌉고인물밖에없어서",
          "original_form": "거기도 쌉고인물밖에없어서",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038891.1.11",
          "form": "그 약간 제왑과 나이브같이",
          "original_form": "그 약간 제왑과 나이브같이",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038891.1.12",
          "form": "철밥통하는 업체만쓰는데",
          "original_form": "철밥통하는 업체만쓰는데",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038891.1.13",
          "form": "댄서도 실력 심각하고...",
          "original_form": "댄서도 실력 심각하고...",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038891.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제왑은 그래도 최근 트와 탈 나이브했잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제왑은 그래도 최근 트와 탈 나이브했잖아",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038891.1.15",
          "form": "뮤비도 맨날 같은업체라",
          "original_form": "뮤비도 맨날 같은업체라",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038891.1.16",
          "form": "별로였다가",
          "original_form": "별로였다가",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038891.1.17",
          "form": "휘인이 솔로앨범때 저스트절크랑",
          "original_form": "휘인이 솔로앨범때 저스트절크랑",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038891.1.18",
          "form": "타뮤비감독 데려와서",
          "original_form": "타뮤비감독 데려와서",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038891.1.19",
          "form": "타멤팬이 열폭했다고",
          "original_form": "타멤팬이 열폭했다고",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038891.1.20",
          "form": "하더라고여",
          "original_form": "하더라고여",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038891.1.21",
          "form": "근데 마마무는 되게 갠활 다 잘 지원해주는 편 같던데ㅜㅜ",
          "original_form": "근데 마마무는 되게 갠활 다 잘 지원해주는 편 같던데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038891.1.22",
          "form": "솔앨도 잘 내주고",
          "original_form": "솔앨도 잘 내주고",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038891.1.23",
          "form": "그런편인데",
          "original_form": "그런편인데",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038891.1.24",
          "form": "멤버들 건강 케어안하고",
          "original_form": "멤버들 건강 케어안하고",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038891.1.25",
          "form": "너무심하게 행사돌리고",
          "original_form": "너무심하게 행사돌리고",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038891.1.26",
          "form": "진짜 학대하듯이 굴려머어서",
          "original_form": "진짜 학대하듯이 굴려머어서",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038891.1.27",
          "form": "소속사에 불만도 많았고...",
          "original_form": "소속사에 불만도 많았고...",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038891.1.28",
          "form": "멤버들끼리 비교도 되고...성적같은거로",
          "original_form": "멤버들끼리 비교도 되고...성적같은거로",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038891.1.29",
          "form": "아...솔로 성적 비교는 진짜",
          "original_form": "아...솔로 성적 비교는 진짜",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038891.1.30",
          "form": "정병의 지름길^_ㅜ",
          "original_form": "정병의 지름길^_ㅜ",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038891.1.31",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100038891.1.32",
          "form": "이게왜안갔담",
          "original_form": "이게왜안갔담",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100038891.1.33",
          "form": "전멤버가 다 솔로하니까 그건 그거나름대로 힘들다고",
          "original_form": "전멤버가 다 솔로하니까 그건 그거나름대로 힘들다고",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038891.1.34",
          "form": "게다가 자기 가수 아니니깐 신나서 비교질하는 애들 꼭 있으니깐ㅜㅜㅜ",
          "original_form": "게다가 자기 가수 아니니깐 신나서 비교질하는 애들 꼭 있으니깐ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100038891.1.35",
          "form": "그리고 화사 솔로가 너무 잘되서 이런 비교 정병들 붙기 딱 좋을듯",
          "original_form": "그리고 화사 솔로가 너무 잘되서 이런 비교 정병들 붙기 딱 좋을듯",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038891.1.36",
          "form": "ㅋㅋㄱㄱㅋㄱ ㄹㅇ",
          "original_form": "ㅋㅋㄱㄱㅋㄱ ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038891.1.37",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038891.1.38",
          "form": "그리고 마마무가 그 소수자팬덤이세거든여",
          "original_form": "그리고 마마무가 그 소수자팬덤이세거든여",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038891.1.39",
          "form": "그 급진페미니스트들하고",
          "original_form": "그 급진페미니스트들하고",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038891.1.40",
          "form": "팬덤ㅇ꽤형성되어있는것 같은데",
          "original_form": "팬덤ㅇ꽤형성되어있는것 같은데",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100038891.1.41",
          "form": "그 뭐라그러지 탈코??",
          "original_form": "그 뭐라그러지 탈코??",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100038891.1.42",
          "form": "남자들이세워놓는 여성상",
          "original_form": "남자들이세워놓는 여성상",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100038891.1.43",
          "form": "그런거탈피가 걔네목표잖아",
          "original_form": "그런거탈피가 걔네목표잖아",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100038891.1.44",
          "form": "그래서 솔라랑 화사가 입고싶은대로 입ㄴㅇㄴ걸",
          "original_form": "그래서 솔라랑 화사가 입고싶은대로 입ㄴㅇㄴ걸",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038891.1.45",
          "form": "엄청지지하는데",
          "original_form": "엄청지지하는데",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038891.1.46",
          "form": "노출이좀많거든여 그둘이",
          "original_form": "노출이좀많거든여 그둘이",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038891.1.47",
          "form": "근데 화사스타일이 걔들노선이고",
          "original_form": "근데 화사스타일이 걔들노선이고",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038891.1.48",
          "form": "솔라가 약간...",
          "original_form": "솔라가 약간...",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038891.1.49",
          "form": "",
          "original_form": "{censored}",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100038891.1.50",
          "form": "무대할때도 그런 컨셉으로잘하고",
          "original_form": "무대할때도 그런 컨셉으로잘하고",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100038891.1.51",
          "form": "ㅇㅇ무슨 연말무대 때 핀업걸틱한 의상 입은거랑",
