      "utterance": [
        {
          "id": "MDRW2100044455.1.1",
          "form": "안녕하게요",
          "original_form": "안녕하게요",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044455.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044455.1.3",
          "form": "기다리고 있는 상품이 있으세요?",
          "original_form": "기다리고 있는 상품이 있으세요?",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044455.1.4",
          "form": "저는 우삼겹살 기다리고있어요.",
          "original_form": "저는 우삼겹살 기다리고있어요.",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044455.1.5",
          "form": "오ㅋㅋㅋ 좋은거 드시네요",
          "original_form": "오ㅋㅋㅋ 좋은거 드시네요",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044455.1.6",
          "form": "수입산이지만 이것저것 요리하려고요",
          "original_form": "수입산이지만 이것저것 요리하려고요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044455.1.7",
          "form": "요리 하시는거 좋아하세요?",
          "original_form": "요리 하시는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044455.1.8",
          "form": "네 ㅎㅎ\r\n샤부샤부 요리하려고요.",
          "original_form": "네 ㅎㅎ\r\n샤부샤부 요리하려고요.",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044455.1.9",
          "form": "너무 맛있겠어요.  ㅜ 초대 부탁드려요",
          "original_form": "너무 맛있겠어요.  ㅜ 초대 부탁드려요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044455.1.10",
          "form": "정말 근처에있으면 불러드리고싶네요",
          "original_form": "정말 근처에있으면 불러드리고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044455.1.11",
          "form": "마음만 받을게요. 감사합니다.",
          "original_form": "마음만 받을게요. 감사합니다.",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044455.1.12",
          "form": "뭘 만들어야할지 설레네요",
          "original_form": "뭘 만들어야할지 설레네요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044455.1.13",
          "form": "배송이 곧 오시나봅니다",
          "original_form": "배송이 곧 오시나봅니다",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044455.1.14",
          "form": "네.ㅎㅎ 이번주 내로 올것으로 예상중이에요",
          "original_form": "네.ㅎㅎ 이번주 내로 올것으로 예상중이에요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044455.1.15",
          "form": "꼭 자랑해주세요",
          "original_form": "꼭 자랑해주세요",
