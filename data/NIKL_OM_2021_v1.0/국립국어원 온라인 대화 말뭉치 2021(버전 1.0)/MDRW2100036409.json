      "utterance": [
        {
          "id": "MDRW2100036409.1.1",
          "form": "학교 앞에 있어서 가끔 가요",
          "original_form": "학교 앞에 있어서 가끔 가요",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100036409.1.2",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100036409.1.3",
          "form": "무 무슨얘기중이었죠",
          "original_form": "무 무슨얘기중이었죠",
          "speaker_id": "1",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100036409.1.4",
          "form": "서브웨이 좋아하는지 물어보셨어요!",
          "original_form": "서브웨이 좋아하는지 물어보셨어요!",
          "speaker_id": "2",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100036409.1.5",
          "form": "앗 맞아요 기억났내요",
          "original_form": "앗 맞아요 기억났내요",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100036409.1.6",
          "form": "저는 최근 푹 빠졌어요",
          "original_form": "저는 최근 푹 빠졌어요",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100036409.1.7",
          "form": "먹어도 되는 얼마안되는 외식중 하나 ㅎㅎ",
          "original_form": "먹어도 되는 얼마안되는 외식중 하나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100036409.1.8",
          "form": "어떤 메뉴 많이 드시나요?",
          "original_form": "어떤 메뉴 많이 드시나요?",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100036409.1.9",
          "form": "베지나 로티세리요!",
          "original_form": "베지나 로티세리요!",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036409.1.10",
          "form": "로티세리애 머스타드 + 핫칠리하면 넘 맛나요 ㅠㅠ",
          "original_form": "로티세리애 머스타드 + 핫칠리하면 넘 맛나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036409.1.11",
          "form": "저도 서브웨이 칠리 소스 진짜 좋아해요",
          "original_form": "저도 서브웨이 칠리 소스 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036409.1.12",
          "form": "너무 잘어울려요",
          "original_form": "너무 잘어울려요",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036409.1.13",
          "form": "진짜 어떻게 그렇개 맛있는지",
          "original_form": "진짜 어떻게 그렇개 맛있는지",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036409.1.14",
          "form": "좋아하시는 조합있어요?",
          "original_form": "좋아하시는 조합있어요?",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036409.1.15",
          "form": "저는 로스트 치킨에 올리브오일이랑 후추도 많이 먹어요!",
          "original_form": "저는 로스트 치킨에 올리브오일이랑 후추도 많이 먹어요!",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036409.1.16",
          "form": "다이어트 조합인데 너무 맛있어요",
          "original_form": "다이어트 조합인데 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036409.1.17",
          "form": "아아 그렇게 먹어도 맛나죠!!",
          "original_form": "아아 그렇게 먹어도 맛나죠!!",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100036409.1.18",
          "form": "근데 제가 매운거에 넘 환장해서 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 제가 매운거에 넘 환장해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100036409.1.19",
          "form": "학식은 드셔보샸어요?",
          "original_form": "학식은 드셔보샸어요?",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100036409.1.20",
          "form": "코로나 때문에 운영을 얼마 못 해서 못 먹어 봤어요ㅠㅠ",
          "original_form": "코로나 때문에 운영을 얼마 못 해서 못 먹어 봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100036409.1.21",
          "form": "아이고 ㅠㅠ",
          "original_form": "아이고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100036409.1.22",
          "form": "이번학기도 비대면이에요?",
          "original_form": "이번학기도 비대면이에요?",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100036409.1.23",
          "form": "네 아직은 비대면이에요",
          "original_form": "네 아직은 비대면이에요",
