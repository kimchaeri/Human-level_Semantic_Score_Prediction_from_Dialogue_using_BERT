        {
          "id": "MDRW2100002622.1.1",
          "form": "여행은 좋아하시나요?",
          "original_form": "여행은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002622.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002622.1.3",
          "form": "어디가보셨나요?",
          "original_form": "어디가보셨나요?",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002622.1.4",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002622.1.5",
          "form": "좋네요 저도남해가고싶어요",
          "original_form": "좋네요 저도남해가고싶어요",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002622.1.6",
          "form": "운전을 할줄을몰라서아쉬워요ㅋㅋㅋ",
          "original_form": "운전을 할줄을몰라서아쉬워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002622.1.7",
          "form": "금방 배워요",
          "original_form": "금방 배워요",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002622.1.8",
          "form": "ㅋㅋㅋ아직면허도 없네요",
          "original_form": "ㅋㅋㅋ아직면허도 없네요",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002622.1.9",
          "form": "따야하는데ㅠㅠ",
          "original_form": "따야하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002622.1.10",
          "form": "무서워하지말거 도전이요",
          "original_form": "무서워하지말거 도전이요",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002622.1.11",
          "form": "무섭다기보단",
          "original_form": "무섭다기보단",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002622.1.12",
          "form": "아직 차가없어서요ㅋㅋㅋ",
          "original_form": "아직 차가없어서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002622.1.13",
          "form": "면허따고 사야죠",
          "original_form": "면허따고 사야죠",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002622.1.14",
          "form": "ㅋㅋㅋ이번에따려고요!",
          "original_form": "ㅋㅋㅋ이번에따려고요!",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002622.1.15",
          "form": "올해안에",
          "original_form": "올해안에",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002622.1.16",
          "form": "황팅",
          "original_form": "황팅",
          "speaker_id": "2",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002622.1.17",
          "form": "강원도 가고싶더라고요ㅋㅋㅋ",
          "original_form": "강원도 가고싶더라고요ㅋㅋㅋ",
