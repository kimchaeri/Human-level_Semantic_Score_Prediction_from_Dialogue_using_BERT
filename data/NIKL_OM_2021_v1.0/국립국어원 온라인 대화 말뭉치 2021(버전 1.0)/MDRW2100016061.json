      "utterance": [
        {
          "id": "MDRW2100016061.1.1",
          "form": "ㅋㅋㅋㅋ 메일 놀고 먹어라",
          "original_form": "ㅋㅋㅋㅋ 메일 놀고 먹어라",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016061.1.2",
          "form": "언니 일하니까 지금 손으로 치고 있죠",
          "original_form": "언니 일하니까 지금 손으로 치고 있죠",
          "speaker_id": "1",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100016061.1.3",
          "form": "난 너무 편하네",
          "original_form": "난 너무 편하네",
          "speaker_id": "1",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100016061.1.4",
          "form": "신랑이 옆에서 등*같대요",
          "original_form": "신랑이 옆에서 등*같대요",
          "speaker_id": "1",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100016061.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ 어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ 어",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016061.1.6",
          "form": "우리신랑도 뭐하냐고 내보고 ㅋㅋㅋㅋㅋ",
          "original_form": "우리신랑도 뭐하냐고 내보고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016061.1.7",
          "form": "룸메 오늘 집에 있네",
          "original_form": "룸메 오늘 집에 있네",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016061.1.8",
          "form": "야간이가",
          "original_form": "야간이가",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016061.1.9",
          "form": "룸메 일주일간 쉽니다",
          "original_form": "룸메 일주일간 쉽니다",
          "speaker_id": "1",
          "time": "20210826 09:35"
        },
        {
          "id": "MDRW2100016061.1.10",
          "form": "어 부럽네",
          "original_form": "어 부럽네",
          "speaker_id": "2",
          "time": "20210826 09:35"
        },
        {
          "id": "MDRW2100016061.1.11",
          "form": "휴가야??",
          "original_form": "휴가야??",
          "speaker_id": "2",
          "time": "20210826 09:35"
        },
        {
          "id": "MDRW2100016061.1.12",
          "form": "어깨수술했잖아요 언니",
          "original_form": "어깨수술했잖아요 언니",
          "speaker_id": "1",
          "time": "20210826 09:35"
        },
        {
          "id": "MDRW2100016061.1.13",
          "form": "룸메 쉬어서 오늘 할 일이 많구만",
          "original_form": "룸메 쉬어서 오늘 할 일이 많구만",
          "speaker_id": "2",
          "time": "20210826 09:35"
        },
        {
          "id": "MDRW2100016061.1.14",
          "form": "난 내일 이리 가요",
          "original_form": "난 내일 이리 가요",
          "speaker_id": "1",
          "time": "20210826 09:35"
        },
        {
          "id": "MDRW2100016061.1.15",
          "form": "신랑 있을 때 볼 일 다 몰아 봐야 됨",
          "original_form": "신랑 있을 때 볼 일 다 몰아 봐야 됨",
          "speaker_id": "1",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.16",
          "form": "아 맞다 내 일 아니라고 맨날 까먹음",
          "original_form": "아 맞다 내 일 아니라고 맨날 까먹음",
          "speaker_id": "2",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.17",
          "form": "쏴리",
          "original_form": "쏴리",
          "speaker_id": "2",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.18",
          "form": "혼자 가면 너무 더워요",
          "original_form": "혼자 가면 너무 더워요",
          "speaker_id": "1",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.19",
          "form": "그래덥다",
          "original_form": "그래덥다",
          "speaker_id": "2",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.20",
          "form": "수술한 데는 괜찮나??",
          "original_form": "수술한 데는 괜찮나??",
          "speaker_id": "2",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.21",
          "form": "운동해야 된대요",
          "original_form": "운동해야 된대요",
          "speaker_id": "1",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.22",
          "form": "아 어깨는 무슨 운동을 하노??",
          "original_form": "아 어깨는 무슨 운동을 하노??",
          "speaker_id": "2",
          "time": "20210826 09:36"
        },
        {
          "id": "MDRW2100016061.1.23",
          "form": "언니 팔 돌리기",
          "original_form": "언니 팔 돌리기",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.24",
          "form": "나 지금 소근거리면서 마이크로 말한다",
          "original_form": "나 지금 소근거리면서 마이크로 말한다",
          "speaker_id": "2",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.25",
          "form": "팔이 위로 안 올라가서 수술했어요",
          "original_form": "팔이 위로 안 올라가서 수술했어요",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.26",
          "form": "언니 조금 부끄럽네요",
          "original_form": "언니 조금 부끄럽네요",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.27",
          "form": "아하.....",
          "original_form": "아하.....",
          "speaker_id": "2",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.28",
          "form": "칠번들 안 웃어요",
          "original_form": "칠번들 안 웃어요",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.29",
          "form": "사람올때만 치고 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "사람올때만 치고 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.30",
          "form": "직원들",
          "original_form": "직원들",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.31",
          "form": "신랑인 이는 안 부끄럽네요",
          "original_form": "신랑인 이는 안 부끄럽네요",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.32",
          "form": "ㅋㅋㅋㅋㄱ 칠번들 너무 한거아니가",
          "original_form": "ㅋㅋㅋㅋㄱ 칠번들 너무 한거아니가",
          "speaker_id": "2",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.33",
          "form": "난 부끄럽지 않아요",
          "original_form": "난 부끄럽지 않아요",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016061.1.34",
          "form": "도저히 해석 불가네",
          "original_form": "도저히 해석 불가네",
          "speaker_id": "2",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100016061.1.35",
          "form": "럭키 세븐",
          "original_form": "럭키 세븐",
          "speaker_id": "1",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100016061.1.36",
          "form": "확인좀 해라",
          "original_form": "확인좀 해라",
          "speaker_id": "2",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100016061.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100016061.1.38",
          "form": "이제 미용실 가서 뿌 염하고",
          "original_form": "이제 미용실 가서 뿌 염하고",
          "speaker_id": "1",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100016061.1.39",
          "form": "애들 통장 만들러 가요",
          "original_form": "애들 통장 만들러 가요",
          "speaker_id": "1",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100016061.1.40",
          "form": "동사무소에서 서류된다고 시간 다 보냈네요",
          "original_form": "동사무소에서 서류된다고 시간 다 보냈네요",
          "speaker_id": "1",
          "time": "20210826 09:38"
        }
      ]
    }
