      "utterance": [
        {
          "id": "MDRW2100037848.1.1",
          "form": "방굼요 name1님은요?",
          "original_form": "방굼요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100037848.1.2",
          "form": "아직 퇴근을 못했어요ㅜㅜ",
          "original_form": "아직 퇴근을 못했어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037848.1.3",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037848.1.4",
          "form": "볶음밥이요ㅜ",
          "original_form": "볶음밥이요ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037848.1.5",
          "form": "그래두 또 먹고싶어요ㅜ",
          "original_form": "그래두 또 먹고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037848.1.6",
          "form": "맛있었겠다ㅜㅜ",
          "original_form": "맛있었겠다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037848.1.7",
          "form": "뭘 볶았나요?",
          "original_form": "뭘 볶았나요?",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037848.1.8",
          "form": "김치와 밥이요",
          "original_form": "김치와 밥이요",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100037848.1.9",
          "form": "최상의 커플이네요",
          "original_form": "최상의 커플이네요",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037848.1.10",
          "form": "한국인의 쏘울!",
          "original_form": "한국인의 쏘울!",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037848.1.11",
          "form": "쏘울푸드",
          "original_form": "쏘울푸드",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100037848.1.12",
          "form": "융님이제는 저녁드셨나요",
          "original_form": "융님이제는 저녁드셨나요",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100037848.1.13",
          "form": "네 배가 부르네요ㅋㅋ",
          "original_form": "네 배가 부르네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037848.1.14",
          "form": "머드셨어요",
          "original_form": "머드셨어요",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037848.1.15",
          "form": "호박잎에 쌈싸먹었어요ㅋ",
          "original_form": "호박잎에 쌈싸먹었어요ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100037848.1.16",
          "form": "마싯겠다",
          "original_form": "마싯겠다",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037848.1.17",
          "form": "저도 멸치 먹을래요",
          "original_form": "저도 멸치 먹을래요",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037848.1.18",
          "form": "아 뼈는 튼튼해지겠네요",
          "original_form": "아 뼈는 튼튼해지겠네요",
          "speaker_id": "1",
          "time": "20210901 22:58"
        }
