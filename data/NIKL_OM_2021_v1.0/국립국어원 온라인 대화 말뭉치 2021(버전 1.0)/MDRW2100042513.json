      "utterance": [
        {
          "id": "MDRW2100042513.1.1",
          "form": "디저트류 좋아하세요?",
          "original_form": "디저트류 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042513.1.2",
          "form": "저 티라미수좋아하다 마카롱으로 넘어와 이젠 레몬과자랑 버터바",
          "original_form": "저 티라미수좋아하다 마카롱으로 넘어와 이젠 레몬과자랑 버터바",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042513.1.3",
          "form": "오 저랑 비슷한 맛흐름이시네요",
          "original_form": "오 저랑 비슷한 맛흐름이시네요",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042513.1.4",
          "form": "저도 티라미수 좋아하다 마카롱/마들렌 지나서",
          "original_form": "저도 티라미수 좋아하다 마카롱/마들렌 지나서",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042513.1.5",
          "form": "지금은?",
          "original_form": "지금은?",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042513.1.6",
          "form": "지금은 약간 휴식기요ㅎㅎ",
          "original_form": "지금은 약간 휴식기요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042513.1.7",
          "form": "아 초콜릿도 한동안 즐겨 먹었어요",
          "original_form": "아 초콜릿도 한동안 즐겨 먹었어요",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042513.1.8",
          "form": "아... 사실 저도 건강때문에 끊었다가..\n다시...ㅜㅠ",
          "original_form": "아... 사실 저도 건강때문에 끊었다가..\n다시...ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042513.1.9",
          "form": "버터바는 어떤 건가요?",
          "original_form": "버터바는 어떤 건가요?",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042513.1.10",
          "form": "버터바 카라멜처럼 단 쫀득한 토핑이 스콘식감의 파이지 위에 있다고 해야할까요",
          "original_form": "버터바 카라멜처럼 단 쫀득한 토핑이 스콘식감의 파이지 위에 있다고 해야할까요",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042513.1.11",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042513.1.12",
          "form": "잇다제과가 유명하다는데",
          "original_form": "잇다제과가 유명하다는데",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042513.1.13",
          "form": "넘 맛있겠네요!!",
          "original_form": "넘 맛있겠네요!!",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042513.1.14",
          "form": "잇다제과...",
          "original_form": "잇다제과...",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042513.1.15",
          "form": "회사근처 마카롱집에서 팔아요",
          "original_form": "회사근처 마카롱집에서 팔아요",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042513.1.16",
          "form": "적어둬야겠어요",
          "original_form": "적어둬야겠어요",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042513.1.17",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042513.1.18",
          "form": "가산동 가까우시면",
          "original_form": "가산동 가까우시면",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042513.1.19",
          "form": "가산 멀지 않아요~",
          "original_form": "가산 멀지 않아요~",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042513.1.20",
          "form": "플레잉디오븐\n네이버페이\n카페,디저트",
          "original_form": "플레잉디오븐\n네이버페이\n카페,디저트",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042513.1.21",
          "form": "여기예요",
          "original_form": "여기예요",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042513.1.22",
          "form": "플레잉디오븐",
          "original_form": "플레잉디오븐",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042513.1.23",
          "form": "멀리서도 오더라고요",
          "original_form": "멀리서도 오더라고요",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042513.1.24",
          "form": "적어둘게요!!",
          "original_form": "적어둘게요!!",
          "speaker_id": "2",
          "time": "20210906 22:39"
        }
      ]
