      "utterance": [
        {
          "id": "MDRW2100017316.1.1",
          "form": "저녁 뭐드실건가요",
          "original_form": "저녁 뭐드실건가요",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017316.1.2",
          "form": "오늘은 초밥입니다",
          "original_form": "오늘은 초밥입니다",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017316.1.3",
          "form": "저녁 뭐드세요?",
          "original_form": "저녁 뭐드세요?",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017316.1.4",
          "form": "저는 닭갈비볶음밥이요",
          "original_form": "저는 닭갈비볶음밥이요",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017316.1.5",
          "form": "맛있는 거 드시네요ㅋㅋㅋㅋ",
          "original_form": "맛있는 거 드시네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017316.1.6",
          "form": "자본의 맛이죠 ㅋㅋㅋㅋㅋㅋ 유가네닭갈비거든요",
          "original_form": "자본의 맛이죠 ㅋㅋㅋㅋㅋㅋ 유가네닭갈비거든요",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017316.1.7",
          "form": "아 역시 닭갈비는 유가네죠",
          "original_form": "아 역시 닭갈비는 유가네죠",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017316.1.8",
          "form": "유가네는 볶음밥이 찐입니다",
          "original_form": "유가네는 볶음밥이 찐입니다",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017316.1.9",
          "form": "초밥 자주드시나요?",
          "original_form": "초밥 자주드시나요?",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017316.1.10",
          "form": "아뇨 오랜만에 먹어서 그런가 더 맛있네요 ㅋㅋㅋㅋ",
          "original_form": "아뇨 오랜만에 먹어서 그런가 더 맛있네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017316.1.11",
          "form": "저는 회를 못먹어요ㅠ",
          "original_form": "저는 회를 못먹어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017316.1.12",
          "form": "무슨음식 좋아하시나요",
          "original_form": "무슨음식 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017316.1.13",
          "form": "전 가리는 거 없어요 ㅋㅋㅋ",
          "original_form": "전 가리는 거 없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017316.1.14",
          "form": "날 거를 못드시나봐요",
          "original_form": "날 거를 못드시나봐요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017316.1.15",
          "form": "육회는 드시는지...?",
          "original_form": "육회는 드시는지...?",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017316.1.16",
          "form": "아뇨 ㅠ 날거를 못먹어요 ㅠ",
          "original_form": "아뇨 ㅠ 날거를 못먹어요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017316.1.17",
          "form": "요리는 하시나요?",
          "original_form": "요리는 하시나요?",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017316.1.18",
          "form": "요리요..?",
          "original_form": "요리요..?",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017316.1.19",
          "form": "그냥 간단한 거는 할 수는 있긴 한데... 잘은 못해요ㅋㅋㅋ",
          "original_form": "그냥 간단한 거는 할 수는 있긴 한데... 잘은 못해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017316.1.20",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017316.1.21",
          "form": "저도 간단한거만요 ㅋㅋㅋ",
          "original_form": "저도 간단한거만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017316.1.22",
          "form": "간편식러버입니다 ㅋㅋㅋ",
          "original_form": "간편식러버입니다 ㅋㅋㅋ",
