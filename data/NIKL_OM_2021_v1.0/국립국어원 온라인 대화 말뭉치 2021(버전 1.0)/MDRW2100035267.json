      "utterance": [
        {
          "id": "MDRW2100035267.1.1",
          "form": "일 하시나요?",
          "original_form": "일 하시나요?",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100035267.1.2",
          "form": "네네 ㅎ",
          "original_form": "네네 ㅎ",
          "speaker_id": "1",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100035267.1.3",
          "form": "제일 힘든점이 뭔가요?",
          "original_form": "제일 힘든점이 뭔가요?",
          "speaker_id": "2",
          "time": "20210830 07:57"
        },
        {
          "id": "MDRW2100035267.1.4",
          "form": "그닥 없어여 ㅎㅎ",
          "original_form": "그닥 없어여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035267.1.5",
          "form": "만족하시나봐요?",
          "original_form": "만족하시나봐요?",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035267.1.6",
          "form": "만족보단 ㅎㅎ 그냥 오래됐으니  익숙해졌으니 이런맘이요 ㅎㅎ",
          "original_form": "만족보단 ㅎㅎ 그냥 오래됐으니  익숙해졌으니 이런맘이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:01"
        },
        {
          "id": "MDRW2100035267.1.7",
          "form": "스트레스도 별로 없으시겠어요",
          "original_form": "스트레스도 별로 없으시겠어요",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100035267.1.8",
          "form": "있을수도 있는데 ㅎㅎ 강철멘탈이라\r\n신경을 안쓰기도 해요",
          "original_form": "있을수도 있는데 ㅎㅎ 강철멘탈이라\r\n신경을 안쓰기도 해요",
          "speaker_id": "1",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100035267.1.9",
          "form": "그래도 비교적 괜찮은 직장인거봐요. 전 계속해도 적응이 안되네요",
          "original_form": "그래도 비교적 괜찮은 직장인거봐요. 전 계속해도 적응이 안되네요",
          "speaker_id": "2",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100035267.1.10",
          "form": "적응하는데 오래 걸렷어여 저도 ㅎㅎ",
          "original_form": "적응하는데 오래 걸렷어여 저도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:30"
        },
        {
          "id": "MDRW2100035267.1.11",
          "form": "전 지금 십여년째 적응이  안되네요   얼마나 다니셨나요?",
          "original_form": "전 지금 십여년째 적응이  안되네요   얼마나 다니셨나요?",
          "speaker_id": "2",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100035267.1.12",
          "form": "저도 그정도 됐어여 ㅎㅎ",
          "original_form": "저도 그정도 됐어여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100035267.1.13",
          "form": "자도 전 부서보다는 여기가 훨 낫네요 ㅎㅎ",
          "original_form": "자도 전 부서보다는 여기가 훨 낫네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100035267.1.14",
          "form": "다행이네요",
          "original_form": "다행이네요",
