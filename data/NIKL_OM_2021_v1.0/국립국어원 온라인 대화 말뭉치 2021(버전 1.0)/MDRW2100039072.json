      "utterance": [
        {
          "id": "MDRW2100039072.1.1",
          "form": "이디야 알바는 피해야겠네요",
          "original_form": "이디야 알바는 피해야겠네요",
          "speaker_id": "2",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100039072.1.2",
          "form": "맞아요 되게 메뉴도 많고 해서 헷갈릴 것 같아요",
          "original_form": "맞아요 되게 메뉴도 많고 해서 헷갈릴 것 같아요",
          "speaker_id": "1",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100039072.1.3",
          "form": "수박주스는 저 가는 매장엔 안팔아서 못먹어봣는데 드셔보셧나요",
          "original_form": "수박주스는 저 가는 매장엔 안팔아서 못먹어봣는데 드셔보셧나요",
          "speaker_id": "1",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100039072.1.4",
          "form": "마셔보긴 했는데",
          "original_form": "마셔보긴 했는데",
          "speaker_id": "2",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100039072.1.5",
          "form": "생과일 주스보다는 그냥 과일이 좋은거 같아요",
          "original_form": "생과일 주스보다는 그냥 과일이 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100039072.1.6",
          "form": "오 어떤가유",
          "original_form": "오 어떤가유",
          "speaker_id": "1",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100039072.1.7",
          "form": "저는 수박이 좋습니다 주스 말고",
          "original_form": "저는 수박이 좋습니다 주스 말고",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100039072.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그렇게 좋은 후기는 아니네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그렇게 좋은 후기는 아니네요",
          "speaker_id": "1",
          "time": "20210903 09:10"
        },
        {
          "id": "MDRW2100039072.1.9",
          "form": "웬만한 생과일 주스가 다 그런거 같아요",
          "original_form": "웬만한 생과일 주스가 다 그런거 같아요",
          "speaker_id": "2",
          "time": "20210903 09:11"
        },
        {
          "id": "MDRW2100039072.1.10",
          "form": "하긴 찐 과일 못따라가는거같아요",
          "original_form": "하긴 찐 과일 못따라가는거같아요",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100039072.1.11",
          "form": "홍시주스는 그래도 정말 찐 홍시맛입니다..흐흐",
          "original_form": "홍시주스는 그래도 정말 찐 홍시맛입니다..흐흐",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100039072.1.12",
          "form": "안그래도 지금 나왔어요",
          "original_form": "안그래도 지금 나왔어요",
          "speaker_id": "2",
          "time": "20210903 09:17"
        },
        {
          "id": "MDRW2100039072.1.13",
          "form": "헉ㅋㅋㅋ진짜 드셨나요",
          "original_form": "헉ㅋㅋㅋ진짜 드셨나요",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100039072.1.14",
          "form": "마셔봤어요",
          "original_form": "마셔봤어요",
          "speaker_id": "2",
          "time": "20210903 12:04"
        },
        {
          "id": "MDRW2100039072.1.15",
          "form": "덕분에 오전에 행복했습니다",
          "original_form": "덕분에 오전에 행복했습니다",
          "speaker_id": "2",
          "time": "20210903 12:04"
        },
        {
          "id": "MDRW2100039072.1.16",
          "form": "잘 드셔주셔서 기쁘네요",
          "original_form": "잘 드셔주셔서 기쁘네요",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039072.1.17",
          "form": "냉동 홍시 간거같긴한데 그래도 찐 홍시맛",
