        },
        {
          "id": "MDRW2100011009.1.34",
          "form": "간혹 실수로 빼놓고 주문해서.",
          "original_form": "간혹 실수로 빼놓고 주문해서.",
          "speaker_id": "1",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011009.1.35",
          "form": "그러니까요 ㅠㅋㅋㅋ",
          "original_form": "그러니까요 ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
