      "utterance": [
        {
          "id": "MDRW2100011009.1.1",
          "form": "저는 자허블.. 자몽허니블랙티 좋아해요",
          "original_form": "저는 자허블.. 자몽허니블랙티 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011009.1.2",
          "form": "자허블은 스테디지요ㅎ",
          "original_form": "자허블은 스테디지요ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011009.1.3",
          "form": "겨울엔 자허블 여름엔 자몽피지오",
          "original_form": "겨울엔 자허블 여름엔 자몽피지오",
          "speaker_id": "1",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011009.1.4",
          "form": "스벅 푸드류는 드시는 거 있나요?",
          "original_form": "스벅 푸드류는 드시는 거 있나요?",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011009.1.5",
          "form": "음 간단하게 무난하게 먹는건",
          "original_form": "음 간단하게 무난하게 먹는건",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011009.1.6",
          "form": "그..이름이뭐지..카스테라..크림빵..그런건데",
          "original_form": "그..이름이뭐지..카스테라..크림빵..그런건데",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011009.1.7",
          "form": "그거 좋아해요 ㅋㅋㅋ",
          "original_form": "그거 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011009.1.8",
          "form": "아 가운데 크림이 백미지요 그거ㅎ",
          "original_form": "아 가운데 크림이 백미지요 그거ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100011009.1.9",
          "form": "전 수프류 좋아해요",
          "original_form": "전 수프류 좋아해요",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100011009.1.10",
          "form": "겨울이면 생각나는 트러플 머쉬룸.",
          "original_form": "겨울이면 생각나는 트러플 머쉬룸.",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100011009.1.11",
          "form": "스프 맛있죠,, 스벅은 다 좋은데",
          "original_form": "스프 맛있죠,, 스벅은 다 좋은데",
          "speaker_id": "2",
          "time": "20210824 09:16"
        },
        {
          "id": "MDRW2100011009.1.12",
          "form": "베이커리류를 더 잘 뽑았으면.. 하는 아쉬움",
          "original_form": "베이커리류를 더 잘 뽑았으면.. 하는 아쉬움",
          "speaker_id": "2",
          "time": "20210824 09:16"
        },
        {
          "id": "MDRW2100011009.1.13",
          "form": "그러게요. 굿즈 뽑는 것의 반만이라도 베이커리류를 잘 뽑았으면.",
          "original_form": "그러게요. 굿즈 뽑는 것의 반만이라도 베이커리류를 잘 뽑았으면.",
          "speaker_id": "1",
          "time": "20210824 09:19"
        },
        {
          "id": "MDRW2100011009.1.14",
          "form": "베이커리는 어느 카페가 괜찮나요?",
          "original_form": "베이커리는 어느 카페가 괜찮나요?",
          "speaker_id": "1",
          "time": "20210824 09:20"
        },
        {
          "id": "MDRW2100011009.1.15",
          "form": "그나마... 투썸..?",
          "original_form": "그나마... 투썸..?",
          "speaker_id": "2",
          "time": "20210824 09:24"
        },
        {
          "id": "MDRW2100011009.1.16",
          "form": "사실 잘 모르겠어요",
          "original_form": "사실 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210824 09:24"
        },
        {
          "id": "MDRW2100011009.1.17",
          "form": "전 빵순이라서 아예 파바나 뚜쥬는 많이가는데..ㅋㅋㅌ",
          "original_form": "전 빵순이라서 아예 파바나 뚜쥬는 많이가는데..ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210824 09:24"
        },
        {
          "id": "MDRW2100011009.1.18",
          "form": "투썸이 케이크 맛집이긴 하죠",
          "original_form": "투썸이 케이크 맛집이긴 하죠",
          "speaker_id": "1",
          "time": "20210824 09:25"
        },
        {
          "id": "MDRW2100011009.1.19",
          "form": "전 아예 끼니 떼울만한 키트 위주로 먹어서 베이커리 맛집을 잘 모르겠네요ㅎ",
          "original_form": "전 아예 끼니 떼울만한 키트 위주로 먹어서 베이커리 맛집을 잘 모르겠네요ㅎ",
          "speaker_id": "1",
          "time": "20210824 09:26"
        },
        {
          "id": "MDRW2100011009.1.20",
          "form": "파바나 뚜쥬서 추천할 메뉴가 있나요?",
          "original_form": "파바나 뚜쥬서 추천할 메뉴가 있나요?",
          "speaker_id": "1",
          "time": "20210824 09:26"
        },
        {
          "id": "MDRW2100011009.1.21",
          "form": "저는 파바 뽀드득그릴소시지,, 뚜쥬는 치즈방앗간 좋아해요",
          "original_form": "저는 파바 뽀드득그릴소시지,, 뚜쥬는 치즈방앗간 좋아해요",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011009.1.22",
          "form": "둘다 유명한거라 ㅋㅋㅋ 아 연유바게트..",
          "original_form": "둘다 유명한거라 ㅋㅋㅋ 아 연유바게트..",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011009.1.23",
          "form": "파바에 베이글 맛있어요",
          "original_form": "파바에 베이글 맛있어요",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011009.1.24",
          "form": "아침마다 아아+베이글했더니 살이..금방 찌더라구요^^..",
          "original_form": "아침마다 아아+베이글했더니 살이..금방 찌더라구요^^..",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011009.1.25",
          "form": "뽀드드그릴소시지 저도 좋아해요!",
          "original_form": "뽀드드그릴소시지 저도 좋아해요!",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011009.1.26",
          "form": "파바는 베이글이 맛있었군요",
          "original_form": "파바는 베이글이 맛있었군요",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011009.1.27",
          "form": "파바선 한번도 안먹어봤는데 베이글 도전해봐야겠어요",
          "original_form": "파바선 한번도 안먹어봤는데 베이글 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100011009.1.28",
          "form": "맛있게 먹으면 0칼로리죠ㅎ",
          "original_form": "맛있게 먹으면 0칼로리죠ㅎ",
          "speaker_id": "1",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100011009.1.29",
          "form": "베이글에는 크림치즈 곁들여 드시나요?",
          "original_form": "베이글에는 크림치즈 곁들여 드시나요?",
          "speaker_id": "1",
          "time": "20210824 09:57"
