        },
        {
          "id": "MDRW2100042695.1.2",
          "form": "있으면읽어존다주의라...",
          "original_form": "있으면읽어존다주의라...",
          "speaker_id": "2",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.3",
          "form": "기억에남을지안난을디는랜던이지만ㅋㅋㅋㅋ",
          "original_form": "기억에남을지안난을디는랜던이지만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.4",
          "form": "ㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ원래그런거신경안쓰고 해서",
          "original_form": "ㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ원래그런거신경안쓰고 해서",
          "speaker_id": "1",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.5",
          "form": "그러고보니....가되게많이나와",
          "original_form": "그러고보니....가되게많이나와",
          "speaker_id": "1",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.6",
          "form": "그러고보니왜 이거안하지...?",
          "original_form": "그러고보니왜 이거안하지...?",
          "speaker_id": "1",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.7",
          "form": "그러고보니 걔는어디갔지...?",
          "original_form": "그러고보니 걔는어디갔지...?",
          "speaker_id": "1",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.8",
          "form": "그냥....텍스트가있으이까읽는다의느낌",
          "original_form": "그냥....텍스트가있으이까읽는다의느낌",
          "speaker_id": "2",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.9",
          "form": "그러고보니 왜 이거 안되지....?",
          "original_form": "그러고보니 왜 이거 안되지....?",
          "speaker_id": "1",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042695.1.11",
          "form": "나중에 알고는",
          "original_form": "나중에 알고는",
          "speaker_id": "1",
          "time": "20210907 01:44"
        },
        {
          "id": "MDRW2100042695.1.12",
          "form": "아하 그래서....!",
          "original_form": "아하 그래서....!",
          "speaker_id": "1",
          "time": "20210907 01:44"
        },
        {
          "id": "MDRW2100042695.1.13",
          "form": "라고 뒷북치는서타일",
          "original_form": "라고 뒷북치는서타일",
          "speaker_id": "1",
          "time": "20210907 01:44"
        },
        {
          "id": "MDRW2100042695.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:45"
        },
        {
          "id": "MDRW2100042695.1.15",
          "form": "저는딸크키워서토벌에도쓰려구요...",
          "original_form": "저는딸크키워서토벌에도쓰려구요...",
          "speaker_id": "2",
          "time": "20210907 01:45"
        },
        {
          "id": "MDRW2100042695.1.16",
          "form": "닼초랑뱀파가살긴사는데",
          "original_form": "닼초랑뱀파가살긴사는데",
          "speaker_id": "2",
          "time": "20210907 01:46"
        },
        {
          "id": "MDRW2100042695.1.17",
          "form": "용쫄때문에계속뒤로밀려가지고",
          "original_form": "용쫄때문에계속뒤로밀려가지고",
          "speaker_id": "2",
          "time": "20210907 01:46"
        },
        {
          "id": "MDRW2100042695.1.18",
          "form": "뱀파가용을못물엌ㅋㅋㅋㅋㅋ",
          "original_form": "뱀파가용을못물엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:46"
        },
        {
          "id": "MDRW2100042695.1.19",
          "form": "딸크있으명몇번더물지않을까....?",
          "original_form": "딸크있으명몇번더물지않을까....?",
          "speaker_id": "2",
          "time": "20210907 01:46"
        },
        {
          "id": "MDRW2100042695.1.20",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210907 01:48"
        },
        {
          "id": "MDRW2100042695.1.21",
          "form": "딸크",
          "original_form": "딸크",
          "speaker_id": "1",
          "time": "20210907 01:48"
        },
        {
          "id": "MDRW2100042695.1.22",
          "form": "?_?",
          "original_form": "?_?",
          "speaker_id": "2",
          "time": "20210907 01:53"
        },
        {
          "id": "MDRW2100042695.1.23",
          "form": "다크초코 방어력 낮추는거라서 빼면안돼여",
          "original_form": "다크초코 방어력 낮추는거라서 빼면안돼여",
          "speaker_id": "1",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042695.1.24",
          "form": "라고 하고싶었어",
          "original_form": "라고 하고싶었어",
          "speaker_id": "1",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042695.1.25",
          "form": "딸기크레페를 같이배치하면",
          "original_form": "딸기크레페를 같이배치하면",
          "speaker_id": "1",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042695.1.26",
          "form": "몰라도",
          "original_form": "몰라도",
          "speaker_id": "1",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042695.1.27",
          "form": "둘중 하나만 쓰는건",
          "original_form": "둘중 하나만 쓰는건",
          "speaker_id": "1",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042695.1.28",
          "form": "추천하지않는다는",
          "original_form": "추천하지않는다는",
          "speaker_id": "1",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042695.1.29",
          "form": "말을하고싶었어",
          "original_form": "말을하고싶었어",
          "speaker_id": "1",
          "time": "20210907 01:56"
        },
        {
          "id": "MDRW2100042695.1.30",
          "form": "어차피에소운좋으면2타치고재수없음첫타치고날아가서....",
          "original_form": "어차피에소운좋으면2타치고재수없음첫타치고날아가서....",
          "speaker_id": "2",
          "time": "20210907 01:57"
        },
        {
          "id": "MDRW2100042695.1.31",
          "form": "추천드린다구",
          "original_form": "추천드린다구",
          "speaker_id": "1",
          "time": "20210907 01:57"
        },
        {
          "id": "MDRW2100042695.1.32",
          "form": "그거보단뱀무는횟수를늘리는게낫지않을까싶어서ㅋㅋㅋㅋ",
          "original_form": "그거보단뱀무는횟수를늘리는게낫지않을까싶어서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042695.1.33",
          "form": "블레가 딜량 많이나와",
          "original_form": "블레가 딜량 많이나와",
          "speaker_id": "1",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042695.1.34",
          "form": "탱커둘넣으면뒤로좀덜밀리겠지....",
          "original_form": "탱커둘넣으면뒤로좀덜밀리겠지....",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042695.1.35",
          "form": "ㄹㅋㄱㅋㅋㅋㅋㅋㄱ 만렙만성달면 뱀으4 쿨타임찰때마다",
          "original_form": "ㄹㅋㄱㅋㅋㅋㅋㅋㄱ 만렙만성달면 뱀으4 쿨타임찰때마다",
          "speaker_id": "1",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042695.1.36",
          "form": "다물수있어",
          "original_form": "다물수있어",
          "speaker_id": "1",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042695.1.37",
          "form": "너무만얘기에여....",
          "original_form": "너무만얘기에여....",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042695.1.38",
          "form": "지금있는쿠키로최대한해보는게빠를거같음ㅋㅋㅋㅋ",
          "original_form": "지금있는쿠키로최대한해보는게빠를거같음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042695.1.39",
          "form": "팁들도다봤는데토핑작필수라",
          "original_form": "팁들도다봤는데토핑작필수라",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042695.1.40",
          "form": "어쨌든먼얘기야다...ㅡ",
          "original_form": "어쨌든먼얘기야다...ㅡ",
          "speaker_id": "2",
          "time": "20210907 02:00"
        },
        {
          "id": "MDRW2100042695.1.41",
          "form": "그냥있는거데리고놀면서방법찾지뭐....",
          "original_form": "그냥있는거데리고놀면서방법찾지뭐....",
