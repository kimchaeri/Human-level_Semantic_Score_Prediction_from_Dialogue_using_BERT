      "utterance": [
        {
          "id": "MDRW2100000415.1.1",
          "form": "안녕! 혹시 명품 좋아해?",
          "original_form": "안녕! 혹시 명품 좋아해?",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000415.1.2",
          "form": "안녕요 명품을 전혀 몰라;",
          "original_form": "안녕요 명품을 전혀 몰라;",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000415.1.3",
          "form": "명품에 대해 알려주세요 ㅋㅋ",
          "original_form": "명품에 대해 알려주세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000415.1.4",
          "form": "최근에 쇼핑한거 알려주세요",
          "original_form": "최근에 쇼핑한거 알려주세요",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000415.1.5",
          "form": "사실 저도 잘 몰라서 물어보려구 그랬거든요 ㅋㅋㅋ",
          "original_form": "사실 저도 잘 몰라서 물어보려구 그랬거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000415.1.6",
          "form": "이번에 구찌에서 가방을 샀는데 선물로",
          "original_form": "이번에 구찌에서 가방을 샀는데 선물로",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000415.1.7",
          "form": "직구 대행으로 구매를 하다보니 진품인지 아닌지를 모르겠어요 ㅠㅠ",
          "original_form": "직구 대행으로 구매를 하다보니 진품인지 아닌지를 모르겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000415.1.8",
          "form": "와아 말로만 듣던 구찌",
          "original_form": "와아 말로만 듣던 구찌",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000415.1.9",
          "form": "아아 직구.. 진품이겠지요뭐",
          "original_form": "아아 직구.. 진품이겠지요뭐",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000415.1.10",
          "form": "직구나 대행을 이용하면 가품이 많다고 그러네요",
          "original_form": "직구나 대행을 이용하면 가품이 많다고 그러네요",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000415.1.11",
          "form": "저는 봐도 진짜 구분을 못하겠어요 ㅠㅠ",
          "original_form": "저는 봐도 진짜 구분을 못하겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000415.1.12",
          "form": "직구도 가품이 많나요? 몰랐어요 ㅠ \r\n저는 명품을 몰라서 ㅠ",
          "original_form": "직구도 가품이 많나요? 몰랐어요 ㅠ \r\n저는 명품을 몰라서 ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000415.1.13",
          "form": "저도 명품이나 그런거에 관심이 없어서 몰랐는데 엄청 많다고 그러더라구요",
          "original_form": "저도 명품이나 그런거에 관심이 없어서 몰랐는데 엄청 많다고 그러더라구요",
          "speaker_id": "1",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000415.1.14",
          "form": "명품 종류도 그렇게 많다는거 첨 알았어요",
          "original_form": "명품 종류도 그렇게 많다는거 첨 알았어요",
          "speaker_id": "1",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000415.1.15",
          "form": "와아.. 그렇군요 들어본게 구찌 프라다 루이비통 ..",
          "original_form": "와아.. 그렇군요 들어본게 구찌 프라다 루이비통 ..",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000415.1.16",
          "form": "저는 거기에 샤넬까지 ㅋㅋ",
          "original_form": "저는 거기에 샤넬까지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000415.1.17",
          "form": "아 ㅎ 맞다 샤넬^^",
          "original_form": "아 ㅎ 맞다 샤넬^^",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000415.1.18",
          "form": "저는 직구는 티비만 해봤어요",
          "original_form": "저는 직구는 티비만 해봤어요",
          "speaker_id": "2",
