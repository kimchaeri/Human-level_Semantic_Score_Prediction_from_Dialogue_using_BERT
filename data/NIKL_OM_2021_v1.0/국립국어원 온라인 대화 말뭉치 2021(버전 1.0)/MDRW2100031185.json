      "utterance": [
        {
          "id": "MDRW2100031185.1.1",
          "form": "아마도 칼국수를 먹기 싶어요",
          "original_form": "아마도 칼국수를 먹기 싶어요",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031185.1.2",
          "form": "밀키트인가요",
          "original_form": "밀키트인가요",
          "speaker_id": "1",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031185.1.3",
          "form": "육수를 우려내 놓은 것이 있어요",
          "original_form": "육수를 우려내 놓은 것이 있어요",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.4",
          "form": "육수는 무엇을 넣었어요",
          "original_form": "육수는 무엇을 넣었어요",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.5",
          "form": "멸치 다시마 새우를 넣었어요",
          "original_form": "멸치 다시마 새우를 넣었어요",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.6",
          "form": "구수 하겠네요",
          "original_form": "구수 하겠네요",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.7",
          "form": "한 번 우려내면 여러 번 먹을 수 있어요",
          "original_form": "한 번 우려내면 여러 번 먹을 수 있어요",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.8",
          "form": "저는 다시다를 써요 간편하게",
          "original_form": "저는 다시다를 써요 간편하게",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.9",
          "form": "칼국수 좋아하세요",
          "original_form": "칼국수 좋아하세요",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.10",
          "form": "칼국수 보다는 수제비를 좋아해요",
          "original_form": "칼국수 보다는 수제비를 좋아해요",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031185.1.11",
          "form": "해물넣으면 더 맛있어요",
          "original_form": "해물넣으면 더 맛있어요",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100031185.1.12",
          "form": "저는 김치 칼국수가 맛있어요",
          "original_form": "저는 김치 칼국수가 맛있어요",
