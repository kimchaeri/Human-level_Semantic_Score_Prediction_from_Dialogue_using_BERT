      "utterance": [
        {
          "id": "MDRW2100047469.1.1",
          "form": "저는 술 조금 좋아해요ㅎㅎ 술좋아하세요?",
          "original_form": "저는 술 조금 좋아해요ㅎㅎ 술좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100047469.1.2",
          "form": "네네 저도 좋아해요 !",
          "original_form": "네네 저도 좋아해요 !",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100047469.1.3",
          "form": "예전보다는 많이 안마시지만 ㅎㅎ 지금도 종종 마셔요",
          "original_form": "예전보다는 많이 안마시지만 ㅎㅎ 지금도 종종 마셔요",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100047469.1.4",
          "form": "술자리도 좋구 혼술도 좋아해요ㅋㅋ",
          "original_form": "술자리도 좋구 혼술도 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100047469.1.5",
          "form": "네 취하게말고 기분좋게 마시는게 좋아요ㅋㅋ",
          "original_form": "네 취하게말고 기분좋게 마시는게 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100047469.1.6",
          "form": "맞아요 ㅋㅋ 저는 그 적당히 취했을때 기분이 좋더라구요",
          "original_form": "맞아요 ㅋㅋ 저는 그 적당히 취했을때 기분이 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100047469.1.7",
          "form": "특히 여름에 샤워하고 맥주마실때ㅠㅠ천국이에요ㅋㅋ",
          "original_form": "특히 여름에 샤워하고 맥주마실때ㅠㅠ천국이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100047469.1.8",
          "form": "아 맞아요 ㅠㅠ 꼭 냉장고에서 꺼내야돼요 ㅋㅋㄱ",
          "original_form": "아 맞아요 ㅠㅠ 꼭 냉장고에서 꺼내야돼요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100047469.1.9",
          "form": "소주 맥주 중에 어떤거 더 자주드세요?",
          "original_form": "소주 맥주 중에 어떤거 더 자주드세요?",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047469.1.10",
          "form": "저는 둘다 좋아해요ㅋㅋ근데 더울땐 맥주 추울땐 소주같아요ㅋㅋ",
          "original_form": "저는 둘다 좋아해요ㅋㅋ근데 더울땐 맥주 추울땐 소주같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047469.1.11",
          "form": "아 맞아요 ㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100047469.1.12",
          "form": "저는 맥주는 꼭 시원하게 먹어서",
          "original_form": "저는 맥주는 꼭 시원하게 먹어서",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100047469.1.13",
          "form": "겨울에는 너무 추우니까 ㅋㅋ 소주가 좋아요",
          "original_form": "겨울에는 너무 추우니까 ㅋㅋ 소주가 좋아요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100047469.1.14",
          "form": "소주는 따뜻한 안주가 어울려서 ㅎㅎ",
          "original_form": "소주는 따뜻한 안주가 어울려서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100047469.1.15",
          "form": "국물류가 짱이죠 소주 안주는 ㅎㅎ",
          "original_form": "국물류가 짱이죠 소주 안주는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100047469.1.16",
          "form": "우동하나만 있어도 술술 들어가죠 ㅠㅠ",
          "original_form": "우동하나만 있어도 술술 들어가죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100047469.1.17",
          "form": "맞아요 ㅠㅠ 소주 한잔에 우동 한입하면 끝나죠 ㅎㅎ",
          "original_form": "맞아요 ㅠㅠ 소주 한잔에 우동 한입하면 끝나죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100047469.1.18",
          "form": "소주 생각하면 겨울이 얼른 왔으면ㅠㅜ",
          "original_form": "소주 생각하면 겨울이 얼른 왔으면ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 17:22"
        }
      ]
