      "utterance": [
        {
          "id": "MDRW2100037477.1.1",
          "form": "아니우리가왜대화를쉬고있었져....",
          "original_form": "아니우리가왜대화를쉬고있었져....",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100037477.1.2",
          "form": "오늘은야식안드셨나요!",
          "original_form": "오늘은야식안드셨나요!",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100037477.1.3",
          "form": "지금 부대찌개 먹는 중이요!",
          "original_form": "지금 부대찌개 먹는 중이요!",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100037477.1.4",
          "form": "근데 이건 야식 아니구 저녁ㅎ",
          "original_form": "근데 이건 야식 아니구 저녁ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100037477.1.5",
          "form": "야식용 쏜살치킨 사 놨어용ㅎ",
          "original_form": "야식용 쏜살치킨 사 놨어용ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100037477.1.6",
          "form": "님은 뭐 드셨나요!",
          "original_form": "님은 뭐 드셨나요!",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100037477.1.7",
          "form": "오잉ㅋㄱㄲ지금 부대찌개를 드시나요! ㅋㅋㄲ",
          "original_form": "오잉ㅋㄱㄲ지금 부대찌개를 드시나요! ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100037477.1.8",
          "form": "저닭강정이요ㅋㄱㄲ\r\n늦게주무시나용?",
          "original_form": "저닭강정이요ㅋㄱㄲ\r\n늦게주무시나용?",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100037477.1.9",
          "form": "일이 늦게 끝나서 쫌 늦게 먹기 시작했어요ㅎ",
          "original_form": "일이 늦게 끝나서 쫌 늦게 먹기 시작했어요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100037477.1.10",
          "form": "닭강정이라니",
          "original_form": "닭강정이라니",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100037477.1.11",
          "form": "오늘은 예의를 차리셨군용!",
          "original_form": "오늘은 예의를 차리셨군용!",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100037477.1.12",
          "form": "네 오늘 좀 예의를차리고 때려넣었습니다\r\nㅋㅋㅋㅋㅋ\r\n쏜살치킨도 대기중이시라니 대단합니다ㅋㄲ",
          "original_form": "네 오늘 좀 예의를차리고 때려넣었습니다\r\nㅋㅋㅋㅋㅋ\r\n쏜살치킨도 대기중이시라니 대단합니다ㅋㄲ",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100037477.1.13",
          "form": "오늘깊콘보상좀받으셨나요?",
          "original_form": "오늘깊콘보상좀받으셨나요?",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100037477.1.14",
          "form": "ㅎㅎ 근데 부대찌개가 은근 배부른데요ㅎ",
          "original_form": "ㅎㅎ 근데 부대찌개가 은근 배부른데요ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100037477.1.15",
          "form": "넹! 간당 고바삭을 받았쥬 님은요?",
          "original_form": "넹! 간당 고바삭을 받았쥬 님은요?",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100037477.1.16",
          "form": "와 고바삭까지받으셨습니깍!!!! 우와",
          "original_form": "와 고바삭까지받으셨습니깍!!!! 우와",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100037477.1.17",
          "form": "간당했어요ㅎㅎ",
          "original_form": "간당했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100037477.1.18",
          "form": "저는 아직멀었습니다;-; ..  칰을향해달리는중입니다..",
          "original_form": "저는 아직멀었습니다;-; ..  칰을향해달리는중입니다..",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100037477.1.19",
          "form": "고바삭 먹어야 하는데",
          "original_form": "고바삭 먹어야 하는데",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100037477.1.20",
          "form": "부대찌개도 맛있겠어요 ㅜㅡㅜ흐엉",
          "original_form": "부대찌개도 맛있겠어요 ㅜㅡㅜ흐엉",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100037477.1.21",
          "form": "안먹은지백만년..(?!)",
          "original_form": "안먹은지백만년..(?!)",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100037477.1.22",
          "form": "닭강정도 맛있잖아요!!",
          "original_form": "닭강정도 맛있잖아요!!",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100037477.1.23",
          "form": "그건그렇죠!!!!!",
          "original_form": "그건그렇죠!!!!!",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100037477.1.24",
          "form": "ㅋㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100037477.1.25",
          "form": "배가 불러서 쏜살치킨은 많이 못먹겠어요ㅜ",
          "original_form": "배가 불러서 쏜살치킨은 많이 못먹겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100037477.1.26",
