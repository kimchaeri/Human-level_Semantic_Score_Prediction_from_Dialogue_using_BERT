      "utterance": [
        {
          "id": "MDRW2100012042.1.1",
          "form": "싸이월드 세대이신가요?",
          "original_form": "싸이월드 세대이신가요?",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012042.1.2",
          "form": "네 싸이월드세대예요",
          "original_form": "네 싸이월드세대예요",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012042.1.3",
          "form": "열심히 한 기억은 없지만요",
          "original_form": "열심히 한 기억은 없지만요",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012042.1.4",
          "form": "싸이월드세대신가요?",
          "original_form": "싸이월드세대신가요?",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012042.1.5",
          "form": "네 초~중학교다닐때",
          "original_form": "네 초~중학교다닐때",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012042.1.6",
          "form": "열심히했어요ㅋㅋ",
          "original_form": "열심히했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012042.1.7",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012042.1.8",
          "form": "그럼 기대되시겠네요?",
          "original_form": "그럼 기대되시겠네요?",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012042.1.9",
          "form": "별로 애정이 없어서",
          "original_form": "별로 애정이 없어서",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012042.1.10",
          "form": "관심밖이에요",
          "original_form": "관심밖이에요",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012042.1.11",
          "form": "행복나라님은요?",
          "original_form": "행복나라님은요?",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012042.1.12",
          "form": "ㅎㅎ 도토리 남아있으신거아니예요?",
          "original_form": "ㅎㅎ 도토리 남아있으신거아니예요?",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012042.1.13",
          "form": "환급해준다나?",
          "original_form": "환급해준다나?",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012042.1.14",
          "form": "그러는거같던데요",
          "original_form": "그러는거같던데요",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012042.1.15",
          "form": "도토리 다쓰고 남은게 없네요ㅋ기",
          "original_form": "도토리 다쓰고 남은게 없네요ㅋ기",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012042.1.16",
          "form": "ㅎㅎ 기억나세요?",
          "original_form": "ㅎㅎ 기억나세요?",
          "speaker_id": "2",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012042.1.17",
          "form": "친구들한테 생일선물로 도토리 주고받고 했어요~",
          "original_form": "친구들한테 생일선물로 도토리 주고받고 했어요~",
          "speaker_id": "1",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012042.1.18",
          "form": "저는 그런게 있었나? 그랬거든요",
          "original_form": "저는 그런게 있었나? 그랬거든요",
          "speaker_id": "2",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012042.1.19",
          "form": "그래서 환급받을게 없을듯요",
          "original_form": "그래서 환급받을게 없을듯요",
          "speaker_id": "2",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012042.1.20",
          "form": "싸이월드 부활 기대되시나요?",
          "original_form": "싸이월드 부활 기대되시나요?",
          "speaker_id": "1",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100012042.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100012042.1.22",
          "form": "실은 별관심 없어요",
          "original_form": "실은 별관심 없어요",
          "speaker_id": "2",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100012042.1.23",
          "form": "싸이월드 다죽어갈때",
          "original_form": "싸이월드 다죽어갈때",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012042.1.24",
          "form": "부활되면 과거사 폭로된다고 걱정하는사람들도 있다는데요 ㅎㅎ",
          "original_form": "부활되면 과거사 폭로된다고 걱정하는사람들도 있다는데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012042.1.25",
          "form": "제 사진이 떠돌아다니는게실어서",
          "original_form": "제 사진이 떠돌아다니는게실어서",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012042.1.26",
          "form": "싫어서",
          "original_form": "싫어서",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012042.1.27",
          "form": "다지웠네요",
          "original_form": "다지웠네요",
          "speaker_id": "1",
          "time": "20210824 18:46"
        }
      ]
    }
