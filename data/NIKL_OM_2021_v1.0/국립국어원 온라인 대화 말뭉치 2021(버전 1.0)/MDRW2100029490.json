      "utterance": [
        {
          "id": "MDRW2100029490.1.1",
          "form": "무슨 유튜브 보시나용",
          "original_form": "무슨 유튜브 보시나용",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029490.1.2",
          "form": "크림히어로즈라고 고양이채널 봐요",
          "original_form": "크림히어로즈라고 고양이채널 봐요",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029490.1.3",
          "form": "헉!!!!! 너무 좋아요ㅠㅠ",
          "original_form": "헉!!!!! 너무 좋아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100029490.1.4",
          "form": "고양이들 너무 귀엽더라구요",
          "original_form": "고양이들 너무 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029490.1.5",
          "form": "집사님 요리하는데 방해하는것도 귀엽고요ㅋㅋ",
          "original_form": "집사님 요리하는데 방해하는것도 귀엽고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029490.1.6",
          "form": "진짜요ㅠㅠㅋㅋㅋㅋ 하나같이 다 귀여워서 액정에 뽀뽀 갈기고 있습니다...",
          "original_form": "진짜요ㅠㅠㅋㅋㅋㅋ 하나같이 다 귀여워서 액정에 뽀뽀 갈기고 있습니다...",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029490.1.7",
          "form": "보시나봐여!",
          "original_form": "보시나봐여!",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029490.1.8",
          "form": "집사님이 고양이 다루는게 프로같아서 신기하더라구요",
          "original_form": "집사님이 고양이 다루는게 프로같아서 신기하더라구요",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029490.1.9",
          "form": "진짜요ㅜㅜ 애기들 많아서 프로가 될 수밖에 없는 거 같아요",
          "original_form": "진짜요ㅜㅜ 애기들 많아서 프로가 될 수밖에 없는 거 같아요",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029490.1.10",
          "form": "예전에 달력 만들었을때 한 번 사봤는데",
          "original_form": "예전에 달력 만들었을때 한 번 사봤는데",
          "speaker_id": "1",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100029490.1.11",
          "form": "달 넘어갈때마다 사진보는게 넘 행복하더라구요",
          "original_form": "달 넘어갈때마다 사진보는게 넘 행복하더라구요",
          "speaker_id": "1",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100029490.1.12",
          "form": "헉 진짜 너무 귀여울 거 같아요... 어디로 봐도 귀엽던데ㅜㅜ",
          "original_form": "헉 진짜 너무 귀여울 거 같아요... 어디로 봐도 귀엽던데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029490.1.13",
          "form": "진짜 너무 귀여워요... 사진 아까워서 달력 다쓰고도 보관해놨어요ㅋㅋㅋ",
          "original_form": "진짜 너무 귀여워요... 사진 아까워서 달력 다쓰고도 보관해놨어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029490.1.14",
          "form": "절대 못 버리죠... 너무 귀여워요 진짜... 크림히어로즈에서 달력 판 지도 몰랐네요...",
          "original_form": "절대 못 버리죠... 너무 귀여워요 진짜... 크림히어로즈에서 달력 판 지도 몰랐네요...",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029490.1.15",
          "form": "저도 우연히 알게되가지고ㅋㅋ",
          "original_form": "저도 우연히 알게되가지고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029490.1.16",
          "form": "쪼그만 달력이었어요",
          "original_form": "쪼그만 달력이었어요",
          "speaker_id": "1",
          "time": "20210903 17:12"
        }
      ]
