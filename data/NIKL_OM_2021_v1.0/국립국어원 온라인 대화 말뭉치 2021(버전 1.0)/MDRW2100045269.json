      "utterance": [
        {
          "id": "MDRW2100045269.1.1",
          "form": "좋아하는 음식이 어떤거에용",
          "original_form": "좋아하는 음식이 어떤거에용",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045269.1.2",
          "form": "찜 종류요",
          "original_form": "찜 종류요",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045269.1.3",
          "form": "오오 어떤 찜이요",
          "original_form": "오오 어떤 찜이요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045269.1.4",
          "form": "미더덕찜 제일 좋아해요",
          "original_form": "미더덕찜 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045269.1.5",
          "form": "해물찜도 좋구요",
          "original_form": "해물찜도 좋구요",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045269.1.6",
          "form": "오!!!!! 낙지도 좋아하새요",
          "original_form": "오!!!!! 낙지도 좋아하새요",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045269.1.7",
          "form": "낙지는 부드러운 다리 끝부분만 먹어요",
          "original_form": "낙지는 부드러운 다리 끝부분만 먹어요",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045269.1.8",
          "form": "이가 부실해서.ㅋㅋ",
          "original_form": "이가 부실해서.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045269.1.9",
          "form": "오오 저도 이가 안좋아서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오오 저도 이가 안좋아서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045269.1.10",
          "form": "고기도 좋아하세요?",
          "original_form": "고기도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045269.1.11",
          "form": "고기는 삼겹살 목살 정도요",
          "original_form": "고기는 삼겹살 목살 정도요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045269.1.12",
          "form": "크게 선호 하는 편은 아니에요.ㅋㅋㅋ",
          "original_form": "크게 선호 하는 편은 아니에요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045269.1.13",
          "form": "오 진짜용??,",
          "original_form": "오 진짜용??,",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045269.1.14",
          "form": "해산물쪽을 좋아하시는구나",
          "original_form": "해산물쪽을 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045269.1.15",
          "form": "네. 굳이 따지자면 해산물 쪽이네요.^^",
          "original_form": "네. 굳이 따지자면 해산물 쪽이네요.^^",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045269.1.16",
          "form": "우아 회도 좋으시죠??",
          "original_form": "우아 회도 좋으시죠??",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045269.1.17",
          "form": "회는 별로요.ㅋㅋㅋ",
          "original_form": "회는 별로요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045269.1.18",
          "form": "해산물보다 고기 좋아하시나요?",
          "original_form": "해산물보다 고기 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045269.1.19",
          "form": "저는 둘다 좋아요 ㅋㅋ",
          "original_form": "저는 둘다 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045269.1.20",
          "form": "근데 회 연어빼면 저도 별로드라구요 ㅠ",
          "original_form": "근데 회 연어빼면 저도 별로드라구요 ㅠ",
