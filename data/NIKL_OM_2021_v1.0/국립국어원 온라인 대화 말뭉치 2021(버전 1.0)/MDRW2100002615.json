      "utterance": [
        {
          "id": "MDRW2100002615.1.1",
          "form": "최근 여행지 어디예영?",
          "original_form": "최근 여행지 어디예영?",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002615.1.2",
          "form": "최근에 여행을 많이 못갔어요ㅠ\r\n가장 마지막으로 갔던곳이 에버랜드.. 이것도 여행일까요",
          "original_form": "최근에 여행을 많이 못갔어요ㅠ\r\n가장 마지막으로 갔던곳이 에버랜드.. 이것도 여행일까요",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002615.1.3",
          "form": "여행이죠 !",
          "original_form": "여행이죠 !",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.4",
          "form": "타지역가서 놀고먹고 하면",
          "original_form": "타지역가서 놀고먹고 하면",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.5",
          "form": "여행이죵 ㅎㅎ",
          "original_form": "여행이죵 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.6",
          "form": "전 담주에 강원도고성가요!",
          "original_form": "전 담주에 강원도고성가요!",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.7",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.8",
          "form": "고성가서 뭐 할 예정이에요?",
          "original_form": "고성가서 뭐 할 예정이에요?",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.9",
          "form": "구냥 바다보러요 ㅎㅎ 한적하기도하고",
          "original_form": "구냥 바다보러요 ㅎㅎ 한적하기도하고",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.10",
          "form": "맛난것도 먹구요 ~",
          "original_form": "맛난것도 먹구요 ~",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002615.1.11",
          "form": "뭐 먹을지 정했어요?",
          "original_form": "뭐 먹을지 정했어요?",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002615.1.12",
          "form": "저는 강원도는 가본적이 없어서 뭐가 맛있는지는 잘 모르겠어요",
          "original_form": "저는 강원도는 가본적이 없어서 뭐가 맛있는지는 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002615.1.13",
          "form": "강원도도 어느지역이냐에 따라서",
          "original_form": "강원도도 어느지역이냐에 따라서",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002615.1.14",
          "form": "음식이 다 개성있어요 ㅎㅎ",
          "original_form": "음식이 다 개성있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002615.1.15",
          "form": "저도 고성은 처음이라",
          "original_form": "저도 고성은 처음이라",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002615.1.16",
          "form": "회먹으려구요 ㅎㅎ",
          "original_form": "회먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002615.1.17",
          "form": "회 너무 좋죠",
          "original_form": "회 너무 좋죠",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002615.1.18",
          "form": "바다보면서 회먹고",
          "original_form": "바다보면서 회먹고",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002615.1.19",
          "form": "흐.. 완전좋다",
          "original_form": "흐.. 완전좋다",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002615.1.20",
          "form": "네네 ^^ 회떠서 바베큐도 해먹고",
          "original_form": "네네 ^^ 회떠서 바베큐도 해먹고",
          "speaker_id": "1",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002615.1.21",
          "form": "바닷물에 발도담구게요",
          "original_form": "바닷물에 발도담구게요",
          "speaker_id": "1",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002615.1.22",
          "form": "와 진짜 너무 즐겁겠다 부러워요",
          "original_form": "와 진짜 너무 즐겁겠다 부러워요",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002615.1.23",
          "form": "여행은 진짜 언제가도 좋은 거 같아요",
          "original_form": "여행은 진짜 언제가도 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002615.1.24",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002615.1.25",
          "form": "넹 ㅎㅎ 저도 오랜만에 가는거예요",
          "original_form": "넹 ㅎㅎ 저도 오랜만에 가는거예요",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002615.1.26",
          "form": "기념품도 구경하고 해야겠어요",
          "original_form": "기념품도 구경하고 해야겠어요",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002615.1.27",
          "form": "진짜 재밌게 놀다오세요 후회없이",
          "original_form": "진짜 재밌게 놀다오세요 후회없이",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002615.1.28",
          "form": "여행계획 없으세요?",
          "original_form": "여행계획 없으세요?",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002615.1.29",
          "form": "음 아쉽게도 올해는 없어요",
          "original_form": "음 아쉽게도 올해는 없어요",
          "speaker_id": "2",
          "time": "20210820 14:14"
        }
      ]
    }
