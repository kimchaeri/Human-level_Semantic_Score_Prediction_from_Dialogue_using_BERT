      "utterance": [
        {
          "id": "MDRW2100038435.1.1",
          "form": "어떤 일 하시나용",
          "original_form": "어떤 일 하시나용",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100038435.1.2",
          "form": "저는 병원에서 일해요",
          "original_form": "저는 병원에서 일해요",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100038435.1.3",
          "form": "병원 일 힘드실것같아요 요즘은 코로나로 더욱 ㅠㅠ",
          "original_form": "병원 일 힘드실것같아요 요즘은 코로나로 더욱 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100038435.1.4",
          "form": "여기는 근무환경이 좋아서",
          "original_form": "여기는 근무환경이 좋아서",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.5",
          "form": "오래잘다니고잇어요",
          "original_form": "오래잘다니고잇어요",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.6",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.7",
          "form": "복지랑 다 넘넘 좋아요 ㅎㅎ",
          "original_form": "복지랑 다 넘넘 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.8",
          "form": "저도 지금 그만둔다는게 4년차네요 ㅋㅋ",
          "original_form": "저도 지금 그만둔다는게 4년차네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.9",
          "form": "저도 복지는 좋은데 일과 사람이 힘들어서ㅠㅠ",
          "original_form": "저도 복지는 좋은데 일과 사람이 힘들어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.10",
          "form": "ㅋㅋㅋ저는 고인물이라서 걍 영혼없이 기계처럼 함다",
          "original_form": "ㅋㅋㅋ저는 고인물이라서 걍 영혼없이 기계처럼 함다",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.11",
          "form": "아ㅜㅜ글쿤요",
          "original_form": "아ㅜㅜ글쿤요",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.12",
          "form": "잘 맞는 곳에 다니셔서 정말 부럽습니다",
          "original_form": "잘 맞는 곳에 다니셔서 정말 부럽습니다",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.13",
          "form": "저도 영혼을 조금 덜어낼까봐요 ㅋㅋ",
          "original_form": "저도 영혼을 조금 덜어낼까봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038435.1.14",
          "form": "요즘은 그냥 제멋대로에요 ㅋㅋ",
          "original_form": "요즘은 그냥 제멋대로에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100038435.1.15",
          "form": "일도편하고 사람도 머 그냥저냥ㅋㅋ",
          "original_form": "일도편하고 사람도 머 그냥저냥ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100038435.1.16",
          "form": "동기들이 더해요",
          "original_form": "동기들이 더해요",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100038435.1.17",
          "form": "네 마이웨이최고에요",
          "original_form": "네 마이웨이최고에요",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100038435.1.18",
          "form": "같은팀에 동기있으면 더 골치아파여",
          "original_form": "같은팀에 동기있으면 더 골치아파여",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100038435.1.19",
          "form": "하ㅜㅜㅜㅜ",
          "original_form": "하ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100038435.1.20",
          "form": "듣기만해도 지끈지끈하져 ㅋㅋ",
