      "utterance": [
        {
          "id": "MDRW2100009275.1.1",
          "form": "저는 아직 대학생이애요 ㅋㅋ ㅠㅠㅠ",
          "original_form": "저는 아직 대학생이애요 ㅋㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009275.1.2",
          "form": "그런데 이번이 막학기라 슬슬..",
          "original_form": "그런데 이번이 막학기라 슬슬..",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009275.1.3",
          "form": "취업을 해야갰죠 ㅠㅠ",
          "original_form": "취업을 해야갰죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009275.1.4",
          "form": "앗 글쿠나 전 작년에 졸업했어요!",
          "original_form": "앗 글쿠나 전 작년에 졸업했어요!",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009275.1.5",
          "form": "취준하시는군요ㅜㅜ....",
          "original_form": "취준하시는군요ㅜㅜ....",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009275.1.6",
          "form": "취준.. 어떤가요 ...",
          "original_form": "취준.. 어떤가요 ...",
          "speaker_id": "2",
          "time": "20210823 07:02"
        },
        {
          "id": "MDRW2100009275.1.7",
          "form": "저는 벌써 마음이....흑흑",
          "original_form": "저는 벌써 마음이....흑흑",
          "speaker_id": "2",
          "time": "20210823 07:03"
        },
        {
          "id": "MDRW2100009275.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 전...사실 취업하다 막혀서 세무사시험 준비로 돌렸답니다....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 전...사실 취업하다 막혀서 세무사시험 준비로 돌렸답니다....",
          "speaker_id": "1",
          "time": "20210823 07:18"
        },
        {
          "id": "MDRW2100009275.1.9",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "2",
          "time": "20210823 07:25"
        },
        {
          "id": "MDRW2100009275.1.10",
          "form": "요즘 취업 너무 어렵죠 ㅠㅠ",
          "original_form": "요즘 취업 너무 어렵죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 07:25"
        },
        {
          "id": "MDRW2100009275.1.11",
          "form": "경력직 신입이 어딨단 말입니가 ㅠ",
          "original_form": "경력직 신입이 어딨단 말입니가 ㅠ",
          "speaker_id": "2",
          "time": "20210823 07:25"
        },
        {
          "id": "MDRW2100009275.1.12",
          "form": "흑흑 그니까요ㅜㅜ",
          "original_form": "흑흑 그니까요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100009275.1.13",
          "form": "그럴거면 돈을 경력직만큼 주근지",
          "original_form": "그럴거면 돈을 경력직만큼 주근지",
          "speaker_id": "1",
          "time": "20210823 09:20"
        },
        {
          "id": "MDRW2100009275.1.14",
          "form": "맞아여 맞아요 ㅠㅠ",
          "original_form": "맞아여 맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100009275.1.15",
          "form": "저는 눈치보는게 제일 힘들더라고요 ..",
          "original_form": "저는 눈치보는게 제일 힘들더라고요 ..",
          "speaker_id": "2",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100009275.1.16",
          "form": "인간..",
          "original_form": "인간..",
          "speaker_id": "2",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100009275.1.17",
          "form": "ㅇㅈ.... 전 진짜 눈치없는데 소심한편이라....",
          "original_form": "ㅇㅈ.... 전 진짜 눈치없는데 소심한편이라....",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100009275.1.18",
          "form": "진짜 하루하루 피말려요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 하루하루 피말려요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100009275.1.19",
          "form": "아아 느낌알아여 ㅠㅠㅠㅠ",
          "original_form": "아아 느낌알아여 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 18:40"
        },
        {
          "id": "MDRW2100009275.1.20",
          "form": "저는 진짜 맨날 지적당했어요 ㅠㅠ",
          "original_form": "저는 진짜 맨날 지적당했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 18:40"
        },
        {
          "id": "MDRW2100009275.1.21",
          "form": "상사한테 뭐하는거 아니다 ..",
          "original_form": "상사한테 뭐하는거 아니다 ..",
          "speaker_id": "2",
          "time": "20210823 18:40"
        },
        {
          "id": "MDRW2100009275.1.22",
