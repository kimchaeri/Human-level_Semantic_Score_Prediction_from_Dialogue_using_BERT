      "utterance": [
        {
          "id": "MDRW2100014256.1.1",
          "form": "요즘 비가 많이 오죠?",
          "original_form": "요즘 비가 많이 오죠?",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014256.1.2",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014256.1.3",
          "form": "내일도 비오나요?",
          "original_form": "내일도 비오나요?",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014256.1.4",
          "form": "내일은안옵니다",
          "original_form": "내일은안옵니다",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100014256.1.5",
          "form": "오늘은요",
          "original_form": "오늘은요",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100014256.1.6",
          "form": "내일 온다는데요??",
          "original_form": "내일 온다는데요??",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100014256.1.7",
          "form": "내일은 오죠",
          "original_form": "내일은 오죠",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100014256.1.8",
          "form": "위의 내일은 오늘을 말한겁니다",
          "original_form": "위의 내일은 오늘을 말한겁니다",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100014256.1.9",
          "form": "아 빨리 답하셧어ㅛ야죠",
          "original_form": "아 빨리 답하셧어ㅛ야죠",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100014256.1.10",
          "form": "왜여",
          "original_form": "왜여",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100014256.1.11",
          "form": "대답은 내 자유임",
          "original_form": "대답은 내 자유임",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100014256.1.12",
          "form": "오늘 선선하죠?",
          "original_form": "오늘 선선하죠?",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100014256.1.13",
          "form": "넹 긴바지입깋잘함",
          "original_form": "넹 긴바지입깋잘함",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.14",
          "form": "근데 퇴근하고 집갈땐 후회함",
          "original_form": "근데 퇴근하고 집갈땐 후회함",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.15",
          "form": "너무더움",
          "original_form": "너무더움",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.17",
          "form": "전 바이크타고왔는데",
          "original_form": "전 바이크타고왔는데",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.18",
          "form": "추웠음",
          "original_form": "추웠음",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.20",
          "form": "이럴까봐 겉옷도 가져왔다고요",
          "original_form": "이럴까봐 겉옷도 가져왔다고요",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.21",
          "form": "긴팔 긴바지",
          "original_form": "긴팔 긴바지",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.22",
          "form": "입고다니세요",
          "original_form": "입고다니세요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.23",
          "form": "봐뷰ㅏ요",
          "original_form": "봐뷰ㅏ요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.24",
          "form": "번쩍들어주세요",
          "original_form": "번쩍들어주세요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.25",
          "form": "탈때만",
          "original_form": "탈때만",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.26",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100014256.1.27",
          "form": "겉옷이요?",
          "original_form": "겉옷이요?",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100014256.1.28",
