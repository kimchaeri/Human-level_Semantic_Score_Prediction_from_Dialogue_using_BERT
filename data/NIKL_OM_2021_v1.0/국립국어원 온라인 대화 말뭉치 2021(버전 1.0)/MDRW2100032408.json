      "utterance": [
        {
          "id": "MDRW2100032408.1.1",
          "form": "아직 안하신거잖아요ㅋㅋ",
          "original_form": "아직 안하신거잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032408.1.2",
          "form": "전 수없이 실수ㅋㅋ",
          "original_form": "전 수없이 실수ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032408.1.3",
          "form": "취소하고 재결제해서 완료했죠ㅎㅎ",
          "original_form": "취소하고 재결제해서 완료했죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032408.1.4",
          "form": "아직 배울게많네요ㅎㅎ",
          "original_form": "아직 배울게많네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100032408.1.5",
          "form": "쇼핑은 주로 뭐뭐하세요?",
          "original_form": "쇼핑은 주로 뭐뭐하세요?",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100032408.1.6",
          "form": "ㅋㅋ 전 주로 떠리몰이랑 지마켓 구경하거나 필요한 물건 있을때 네이버검색이요",
          "original_form": "ㅋㅋ 전 주로 떠리몰이랑 지마켓 구경하거나 필요한 물건 있을때 네이버검색이요",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032408.1.7",
          "form": "ㅎㅎ 떠리몰도 있군요ㅎ 거기는 싸나요?",
          "original_form": "ㅎㅎ 떠리몰도 있군요ㅎ 거기는 싸나요?",
          "speaker_id": "1",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100032408.1.8",
          "form": "지마켓은 배송비붙으면 비싼것 같더라구요ㅎㅎ",
          "original_form": "지마켓은 배송비붙으면 비싼것 같더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100032408.1.9",
          "form": "ㅋㅋ 가끔 득템할때가 있어요",
          "original_form": "ㅋㅋ 가끔 득템할때가 있어요",
          "speaker_id": "2",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100032408.1.10",
          "form": "버터나 치즈",
          "original_form": "버터나 치즈",
          "speaker_id": "2",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100032408.1.11",
          "form": "ㅎㅎ 그렇군요ㅎㅎ 전 특가 이런거 잘모르겟어요ㅎ",
          "original_form": "ㅎㅎ 그렇군요ㅎㅎ 전 특가 이런거 잘모르겟어요ㅎ",
          "speaker_id": "1",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032408.1.12",
          "form": "정보는있는데 제가 원하는 정보는 안나오더라구요ㅎ",
          "original_form": "정보는있는데 제가 원하는 정보는 안나오더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032408.1.13",
          "form": "그럼 뭐구매하셨어요?ㅎ",
          "original_form": "그럼 뭐구매하셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032408.1.14",
          "form": "맞아요ㅜ 근데 보이는만큼 보여요",
          "original_form": "맞아요ㅜ 근데 보이는만큼 보여요",
          "speaker_id": "2",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032408.1.15",
          "form": "저 이번엔 다크초코렛",
          "original_form": "저 이번엔 다크초코렛",
          "speaker_id": "2",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032408.1.16",
          "form": "아아ㅎㅎ 다크초콜렛먹고싶네요ㅍ",
          "original_form": "아아ㅎㅎ 다크초콜렛먹고싶네요ㅍ",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100032408.1.17",
          "form": "살빼는데 초콜릿이 좋다고하던데 사실일까요?ㅎㅎ",
          "original_form": "살빼는데 초콜릿이 좋다고하던데 사실일까요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100032408.1.18",
          "form": "ㅋㅋ그짓말같은데 확실히 식욕은 좀 없어지는거같아요",
          "original_form": "ㅋㅋ그짓말같은데 확실히 식욕은 좀 없어지는거같아요",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032408.1.19",
          "form": "맞아요ㅎ 근데 많이먹게되지않아요?ㅎㅎ",
          "original_form": "맞아요ㅎ 근데 많이먹게되지않아요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100032408.1.20",
          "form": "전초콜릿을 좋아해서ㅎ 한번먹으면 엄청많이먹어요ㅎㅎ",
          "original_form": "전초콜릿을 좋아해서ㅎ 한번먹으면 엄청많이먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100032408.1.21",
          "form": "칼로리도 높잖아요ㅎ",
          "original_form": "칼로리도 높잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100032408.1.22",
          "form": "다크는 많이 안먹혀요",
          "original_form": "다크는 많이 안먹혀요",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100032408.1.23",
          "form": "ㅎㅎ 암튼 구매할때는 합리적인가격보다 더싸게ㅎㅎ",
          "original_form": "ㅎㅎ 암튼 구매할때는 합리적인가격보다 더싸게ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100032408.1.24",
          "form": "그게 제 원칙인데ㅎ 오늘 깨버렸네요ㅎ 아이꺼 장난감산다고ㅎ 그원칙을 다무시하고 그냥 했더니ㅎ 실수할뻔했어요ㅎㅎ",
          "original_form": "그게 제 원칙인데ㅎ 오늘 깨버렸네요ㅎ 아이꺼 장난감산다고ㅎ 그원칙을 다무시하고 그냥 했더니ㅎ 실수할뻔했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100032408.1.25",
          "form": "무배도 적용안하고ㅎ 출첵했던 적립금도 안쓰곻ㅎ",
          "original_form": "무배도 적용안하고ㅎ 출첵했던 적립금도 안쓰곻ㅎ",
          "speaker_id": "1",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100032408.1.26",
          "form": "ㅋㅋ 애기꺼니까 괜찮습니다!!!!",
          "original_form": "ㅋㅋ 애기꺼니까 괜찮습니다!!!!",
          "speaker_id": "2",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032408.1.27",
          "form": "소듕한 애기꺼니까요",
          "original_form": "소듕한 애기꺼니까요",
          "speaker_id": "2",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032408.1.28",
          "form": "맞아요 ㅎ 애기꺼는 아끼기가 좀 그래요ㅎㅎ",
