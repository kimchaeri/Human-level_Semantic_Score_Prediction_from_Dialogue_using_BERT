      "utterance": [
        {
          "id": "MDRW2100012849.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012849.1.2",
          "form": "예전엔 잘읽었는데",
          "original_form": "예전엔 잘읽었는데",
          "speaker_id": "2",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012849.1.3",
          "form": "요즘엔 별로요ㅋㅋㅋ",
          "original_form": "요즘엔 별로요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012849.1.4",
          "form": "저는 부자되고 싶어서 재테크 관련 책읽어요",
          "original_form": "저는 부자되고 싶어서 재테크 관련 책읽어요",
          "speaker_id": "1",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100012849.1.5",
          "form": "ㅋㅋㅋ 오",
          "original_form": "ㅋㅋㅋ 오",
          "speaker_id": "2",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100012849.1.6",
          "form": "도움이되나요?",
          "original_form": "도움이되나요?",
          "speaker_id": "2",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100012849.1.7",
          "form": "네 안읽는 것보다는요~",
          "original_form": "네 안읽는 것보다는요~",
          "speaker_id": "1",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012849.1.8",
          "form": "저도 주식책",
          "original_form": "저도 주식책",
          "speaker_id": "2",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012849.1.9",
          "form": "사보려했어요ㅋㅋㅋ",
          "original_form": "사보려했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012849.1.10",
          "form": "이참에 사보셔요ㅋㅋ",
          "original_form": "이참에 사보셔요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:48"
        },
        {
          "id": "MDRW2100012849.1.11",
          "form": "인상깊었던 책 있으신가요?",
          "original_form": "인상깊었던 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 02:49"
        },
        {
          "id": "MDRW2100012849.1.12",
          "form": "사려던건",
          "original_form": "사려던건",
          "speaker_id": "2",
          "time": "20210825 02:53"
        },
        {
          "id": "MDRW2100012849.1.13",
          "form": "차트의기술이요ㅋㅋㅋ",
          "original_form": "차트의기술이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:53"
        },
        {
          "id": "MDRW2100012849.1.14",
          "form": "저그거 읽어봤어요ㅋ기",
          "original_form": "저그거 읽어봤어요ㅋ기",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012849.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012849.1.16",
          "form": "어떤가요???",
          "original_form": "어떤가요???",
          "speaker_id": "2",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012849.1.17",
          "form": "엘리어트 파동이론이랑 같이 봤어요",
          "original_form": "엘리어트 파동이론이랑 같이 봤어요",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012849.1.18",
          "form": "이해 안되는 부분이 있어서",
          "original_form": "이해 안되는 부분이 있어서",
          "speaker_id": "1",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012849.1.19",
          "form": "여러번 보고",
          "original_form": "여러번 보고",
          "speaker_id": "1",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012849.1.20",
          "form": "필사했어요",
          "original_form": "필사했어요",
          "speaker_id": "1",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012849.1.21",
          "form": "와우... 열심히하시네요",
          "original_form": "와우... 열심히하시네요",
          "speaker_id": "2",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100012849.1.22",
          "form": "엘리엇 진짜",
          "original_form": "엘리엇 진짜",
          "speaker_id": "2",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100012849.1.23",
          "form": "어렵더라고요ㅠㅠ",
          "original_form": "어렵더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100012849.1.24",
          "form": "맞아요 어려워요",
          "original_form": "맞아요 어려워요",
          "speaker_id": "1",
          "time": "20210825 03:00"
        },
        {
          "id": "MDRW2100012849.1.25",
          "form": "완벽하게 이해는 못했어요",
          "original_form": "완벽하게 이해는 못했어요",
          "speaker_id": "1",
          "time": "20210825 03:00"
        }
      ]
