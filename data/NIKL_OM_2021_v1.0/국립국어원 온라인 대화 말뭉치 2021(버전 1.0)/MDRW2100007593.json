      "utterance": [
        {
          "id": "MDRW2100007593.1.1",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "1",
          "time": "20210822 03:44"
        },
        {
          "id": "MDRW2100007593.1.2",
          "form": "간간히 하는편이에요",
          "original_form": "간간히 하는편이에요",
          "speaker_id": "2",
          "time": "20210822 03:44"
        },
        {
          "id": "MDRW2100007593.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 03:44"
        },
        {
          "id": "MDRW2100007593.1.4",
          "form": "저도 가끔 해요",
          "original_form": "저도 가끔 해요",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007593.1.5",
          "form": "아무래도 사기가 많다보니 자주는 안하게 되더라구요",
          "original_form": "아무래도 사기가 많다보니 자주는 안하게 되더라구요",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007593.1.6",
          "form": "최근에 구매한건 있으세요?",
          "original_form": "최근에 구매한건 있으세요?",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007593.1.7",
          "form": "최근에 고래별 아크릴을 구매했어요",
          "original_form": "최근에 고래별 아크릴을 구매했어요",
          "speaker_id": "1",
          "time": "20210822 03:47"
        },
        {
          "id": "MDRW2100007593.1.8",
          "form": "고래별이면 웹툰인가요?",
          "original_form": "고래별이면 웹툰인가요?",
          "speaker_id": "2",
          "time": "20210822 03:47"
        },
        {
          "id": "MDRW2100007593.1.9",
          "form": "들어본거같은데",
          "original_form": "들어본거같은데",
          "speaker_id": "2",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007593.1.10",
          "form": "네네! 네이버 웹툰이에요",
          "original_form": "네네! 네이버 웹툰이에요",
          "speaker_id": "1",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007593.1.11",
          "form": "name2님은 최근에 구매하신 거 있으신가요?",
          "original_form": "&name2&님은 최근에 구매하신 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007593.1.12",
          "form": "저는 최근에 신발 구매했어요 가벼운 운동화요",
          "original_form": "저는 최근에 신발 구매했어요 가벼운 운동화요",
          "speaker_id": "2",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007593.1.13",
          "form": "우왕 축하드려요",
          "original_form": "우왕 축하드려요",
          "speaker_id": "1",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007593.1.14",
          "form": "아앗.. 고맙습니다",
          "original_form": "아앗.. 고맙습니다",
          "speaker_id": "2",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007593.1.15",
          "form": "고래별 아크릴은 찾아보니 가격대가 좀 있던데",
          "original_form": "고래별 아크릴은 찾아보니 가격대가 좀 있던데",
          "speaker_id": "2",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007593.1.16",
          "form": "중고로 사신건가요?",
          "original_form": "중고로 사신건가요?",
          "speaker_id": "2",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007593.1.17",
          "form": "네네...텀블벅 펀딩 상품이라 구하기가 힘들어서 플미가 좀 많이 붙었어요ㅠ",
          "original_form": "네네...텀블벅 펀딩 상품이라 구하기가 힘들어서 플미가 좀 많이 붙었어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 03:52"
        },
        {
          "id": "MDRW2100007593.1.18",
          "form": "헉 맞다 주로 어느 사이트에서 거래하세요",
          "original_form": "헉 맞다 주로 어느 사이트에서 거래하세요",
          "speaker_id": "1",
          "time": "20210822 03:52"
        },
        {
          "id": "MDRW2100007593.1.19",
          "form": "전 중고나라 당근 둘 다 이용해요",
          "original_form": "전 중고나라 당근 둘 다 이용해요",
          "speaker_id": "2",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007593.1.20",
