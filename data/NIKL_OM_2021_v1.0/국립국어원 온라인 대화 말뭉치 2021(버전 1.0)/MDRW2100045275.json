      "utterance": [
        {
          "id": "MDRW2100045275.1.1",
          "form": "주로 어떤 쇼핑몰 쓰세요?",
          "original_form": "주로 어떤 쇼핑몰 쓰세요?",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045275.1.2",
          "form": "11번가와 네이버 스토어요 님은요?",
          "original_form": "11번가와 네이버 스토어요 님은요?",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045275.1.3",
          "form": "전 지마켓이랑 네이버요",
          "original_form": "전 지마켓이랑 네이버요",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045275.1.4",
          "form": "무엇을 사나요?",
          "original_form": "무엇을 사나요?",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045275.1.5",
          "form": "보통 생필품이랑 냉동이요ㅋㅋ",
          "original_form": "보통 생필품이랑 냉동이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045275.1.6",
          "form": "저도 생필품 할인 딜 뜰때 잘 사요",
          "original_form": "저도 생필품 할인 딜 뜰때 잘 사요",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045275.1.7",
          "form": "맞아요 그 가격사고나면 다른가격때 못상：",
          "original_form": "맞아요 그 가격사고나면 다른가격때 못상：",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045275.1.8",
          "form": "쿠폰이랑 포인트 다 써서 할인 받아야 속 시원해요",
          "original_form": "쿠폰이랑 포인트 다 써서 할인 받아야 속 시원해요",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045275.1.9",
          "form": "맞아요!!ㅈ",
          "original_form": "맞아요!!ㅈ",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045275.1.10",
          "form": "정보는 어디서 얻으세요?",
          "original_form": "정보는 어디서 얻으세요?",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045275.1.11",
          "form": "인터넷에서 공유 받으면 사거나 직접 사이트에서 찾아봐요",
          "original_form": "인터넷에서 공유 받으면 사거나 직접 사이트에서 찾아봐요",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045275.1.12",
          "form": "전 뽐뿌 가끔",
          "original_form": "전 뽐뿌 가끔",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045275.1.13",
          "form": "카드 실적채워야할때 질러요",
          "original_form": "카드 실적채워야할때 질러요",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045275.1.14",
          "form": "할인 정보가 많나봐요",
          "original_form": "할인 정보가 많나봐요",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045275.1.15",
