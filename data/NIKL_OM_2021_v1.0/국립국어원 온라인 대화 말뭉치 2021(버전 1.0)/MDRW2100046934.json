      "utterance": [
        {
          "id": "MDRW2100046934.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211106 13:50"
        },
        {
          "id": "MDRW2100046934.1.2",
          "form": "안녕하세요! 뭐하고 계세요~?",
          "original_form": "안녕하세요! 뭐하고 계세요~?",
          "speaker_id": "2",
          "time": "20211106 14:07"
        },
        {
          "id": "MDRW2100046934.1.3",
          "form": "점심먹고 뒹굴뒹굴 중입니다",
          "original_form": "점심먹고 뒹굴뒹굴 중입니다",
          "speaker_id": "1",
          "time": "20211106 14:16"
        },
        {
          "id": "MDRW2100046934.1.4",
          "form": "점심 드셨어요~?",
          "original_form": "점심 드셨어요~?",
          "speaker_id": "1",
          "time": "20211106 14:16"
        },
        {
          "id": "MDRW2100046934.1.5",
          "form": "네 저는 이삭토스트 먹었어요!",
          "original_form": "네 저는 이삭토스트 먹었어요!",
          "speaker_id": "2",
          "time": "20211106 14:16"
        },
        {
          "id": "MDRW2100046934.1.6",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20211106 14:17"
        },
        {
          "id": "MDRW2100046934.1.7",
          "form": "우아 이삭토스트 맛있는데",
          "original_form": "우아 이삭토스트 맛있는데",
          "speaker_id": "1",
          "time": "20211106 14:17"
        },
        {
          "id": "MDRW2100046934.1.8",
          "form": "동네에 없어요 ㅜㅜ",
          "original_form": "동네에 없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20211106 14:17"
        },
        {
          "id": "MDRW2100046934.1.9",
          "form": "전 라면 ㅎㅎㅎㅎㅎ",
          "original_form": "전 라면 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211106 14:17"
        },
        {
          "id": "MDRW2100046934.1.10",
          "form": "틈새오모리라면 아세요",
          "original_form": "틈새오모리라면 아세요",
          "speaker_id": "1",
          "time": "20211106 14:17"
        },
        {
          "id": "MDRW2100046934.1.11",
          "form": "매운 김치찌개라면",
          "original_form": "매운 김치찌개라면",
          "speaker_id": "1",
          "time": "20211106 14:17"
        },
        {
          "id": "MDRW2100046934.1.12",
          "form": "그거 먹었습니다",
          "original_form": "그거 먹었습니다",
          "speaker_id": "1",
          "time": "20211106 14:18"
        },
        {
          "id": "MDRW2100046934.1.13",
          "form": "오 유명하잖아요!",
          "original_form": "오 유명하잖아요!",
          "speaker_id": "2",
          "time": "20211106 14:21"
        },
        {
          "id": "MDRW2100046934.1.14",
          "form": "그거 하나만 드셨어요?!",
          "original_form": "그거 하나만 드셨어요?!",
          "speaker_id": "2",
          "time": "20211106 14:21"
        },
        {
          "id": "MDRW2100046934.1.15",
          "form": "네 !! 햇반 하나 말아서",
          "original_form": "네 !! 햇반 하나 말아서",
          "speaker_id": "1",
          "time": "20211106 14:21"
        },
        {
          "id": "MDRW2100046934.1.16",
          "form": "맵긴 매워요",
          "original_form": "맵긴 매워요",
          "speaker_id": "1",
          "time": "20211106 14:22"
        },
        {
          "id": "MDRW2100046934.1.17",
          "form": "그래도 그냥 오모리보다 맛있는거같아요",
          "original_form": "그래도 그냥 오모리보다 맛있는거같아요",
          "speaker_id": "1",
          "time": "20211106 14:22"
        },
        {
          "id": "MDRW2100046934.1.18",
          "form": "아 밥도 같이 드셨구낭 ㅋㅋㅋ",
          "original_form": "아 밥도 같이 드셨구낭 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211106 14:23"
        },
        {
          "id": "MDRW2100046934.1.19",
          "form": "밥 먹으니까 졸리네요 ㅠ",
          "original_form": "밥 먹으니까 졸리네요 ㅠ",
          "speaker_id": "2",
          "time": "20211106 14:23"
        },
        {
          "id": "MDRW2100046934.1.20",
          "form": "하핫 커피 한잔 하세요",
          "original_form": "하핫 커피 한잔 하세요",
          "speaker_id": "1",
          "time": "20211106 14:23"
        },
        {
          "id": "MDRW2100046934.1.21",
          "form": "그래야 겠어요!!",
          "original_form": "그래야 겠어요!!",
          "speaker_id": "2",
          "time": "20211106 14:23"
        },
        {
          "id": "MDRW2100046934.1.22",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20211106 14:23"
        },
        {
          "id": "MDRW2100046934.1.23",
          "form": "네 제게는 커피혈이 흐릅니다",
          "original_form": "네 제게는 커피혈이 흐릅니다",
          "speaker_id": "1",
          "time": "20211106 14:24"
        },
        {
          "id": "MDRW2100046934.1.24",
          "form": "물대신 커피",
          "original_form": "물대신 커피",
          "speaker_id": "1",
          "time": "20211106 14:24"
        },
        {
          "id": "MDRW2100046934.1.25",
          "form": "ㅋㅋㅋㅋ어떤커피 좋아하세요,",
          "original_form": "ㅋㅋㅋㅋ어떤커피 좋아하세요,",
          "speaker_id": "2",
          "time": "20211106 14:24"
        },
        {
          "id": "MDRW2100046934.1.26",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20211106 14:24"
        },
        {
          "id": "MDRW2100046934.1.27",
          "form": "아메파지만",
          "original_form": "아메파지만",
          "speaker_id": "1",
          "time": "20211106 14:24"
        },
        {
          "id": "MDRW2100046934.1.28",
          "form": "달달한 커피도 좋고",
          "original_form": "달달한 커피도 좋고",
          "speaker_id": "1",
          "time": "20211106 14:25"
        },
        {
          "id": "MDRW2100046934.1.29",
          "form": "커피 중독인거같아요 카페인 과다",
          "original_form": "커피 중독인거같아요 카페인 과다",
          "speaker_id": "1",
          "time": "20211106 14:25"
        },
        {
          "id": "MDRW2100046934.1.30",
          "form": "밤에 쓰리샷 포샷 먹어도 잠이 잘 오는",
          "original_form": "밤에 쓰리샷 포샷 먹어도 잠이 잘 오는",
          "speaker_id": "1",
          "time": "20211106 14:25"
        },
        {
          "id": "MDRW2100046934.1.31",
          "form": "맞아요 카페인은 뭔들 최고!!!",
          "original_form": "맞아요 카페인은 뭔들 최고!!!",
          "speaker_id": "2",
          "time": "20211106 14:25"
        },
        {
          "id": "MDRW2100046934.1.32",
          "form": "저도 커피마시고 정신차려야겠어요 ㅎㅎ",
          "original_form": "저도 커피마시고 정신차려야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211106 14:26"
        },
        {
          "id": "MDRW2100046934.1.33",
          "form": "즐거운 토요일 보내세요!",
          "original_form": "즐거운 토요일 보내세요!",
          "speaker_id": "2",
          "time": "20211106 14:26"
        },
