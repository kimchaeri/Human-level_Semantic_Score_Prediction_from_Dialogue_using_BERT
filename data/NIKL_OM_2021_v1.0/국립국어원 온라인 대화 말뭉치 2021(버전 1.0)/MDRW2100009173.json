      "utterance": [
        {
          "id": "MDRW2100009173.1.1",
          "form": "음 저는 한때 야구 좋아했어요 ㅋㅋ",
          "original_form": "음 저는 한때 야구 좋아했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100009173.1.2",
          "form": "아 저는 챙겨보는 스포츠는 없구ㅎㅎ이번에 올림픽 재밌게 봤어요ㅎㅎ",
          "original_form": "아 저는 챙겨보는 스포츠는 없구ㅎㅎ이번에 올림픽 재밌게 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100009173.1.3",
          "form": "배구가 너무 아까웠죠ㅜㅜ",
          "original_form": "배구가 너무 아까웠죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009173.1.4",
          "form": "맞아요 그래도 감동적이었어요ㅎㅎ",
          "original_form": "맞아요 그래도 감동적이었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009173.1.5",
          "form": "그 말 기억나네요",
          "original_form": "그 말 기억나네요",
          "speaker_id": "2",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009173.1.6",
          "form": "불교엔 불경",
          "original_form": "불교엔 불경",
          "speaker_id": "2",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009173.1.7",
          "form": "배구엔 김연경 ㅋ",
          "original_form": "배구엔 김연경 ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009173.1.8",
          "form": "완전 맞는말같아요ㅋㅋㅋ감동적이고 멋있어서",
          "original_form": "완전 맞는말같아요ㅋㅋㅋ감동적이고 멋있어서",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009173.1.9",
          "form": "은퇴 좀 아쉽기도해요ㅜ",
          "original_form": "은퇴 좀 아쉽기도해요ㅜ",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009173.1.10",
          "form": "저도 그런데 후련해보여서 국대가 큰 부담이구나 느껴졌어요",
          "original_form": "저도 그런데 후련해보여서 국대가 큰 부담이구나 느껴졌어요",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009173.1.11",
          "form": "오 뭔가 알것같아요",
          "original_form": "오 뭔가 알것같아요",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009173.1.12",
          "form": "유투브도 재밌다는데",
          "original_form": "유투브도 재밌다는데",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009173.1.13",
          "form": "근데 이제 김연아도없고 김연경도없고ㅜ",
          "original_form": "근데 이제 김연아도없고 김연경도없고ㅜ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009173.1.14",
          "form": "네 그런 배우는 진짜 자주나오는게 아니라서...아쉬워요",
          "original_form": "네 그런 배우는 진짜 자주나오는게 아니라서...아쉬워요",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009173.1.15",
          "form": "저는 좀 그거 무서워요",
          "original_form": "저는 좀 그거 무서워요",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009173.1.16",
          "form": "내가 응원하던 선수의 인성에 실망하는거?",
          "original_form": "내가 응원하던 선수의 인성에 실망하는거?",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009173.1.17",
          "form": "저도 그래서 깊게 알고 싶지가 않은거같아요",
          "original_form": "저도 그래서 깊게 알고 싶지가 않은거같아요",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009173.1.18",
          "form": "그런면에서 김연아랑 김연경 선수가 진짜 챔피언 인것같아요",
          "original_form": "그런면에서 김연아랑 김연경 선수가 진짜 챔피언 인것같아요",
          "speaker_id": "2",
          "time": "20210822 22:19"
        }
      ]
