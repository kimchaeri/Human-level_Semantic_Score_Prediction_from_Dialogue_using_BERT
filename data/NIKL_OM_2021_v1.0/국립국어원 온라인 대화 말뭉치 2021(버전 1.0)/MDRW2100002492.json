      "utterance": [
        {
          "id": "MDRW2100002492.1.1",
          "form": "너네 동생 군대 언제 간데?",
          "original_form": "너네 동생 군대 언제 간데?",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002492.1.2",
          "form": "내 동생 신검받고오더니",
          "original_form": "내 동생 신검받고오더니",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002492.1.3",
          "form": "몸무게 미달로 공익 결정됐어",
          "original_form": "몸무게 미달로 공익 결정됐어",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002492.1.4",
          "form": "대체 몇이길래 미달이 나왔데",
          "original_form": "대체 몇이길래 미달이 나왔데",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002492.1.5",
          "form": "BMI 16 미만이면 미달이야",
          "original_form": "BMI 16 미만이면 미달이야",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002492.1.6",
          "form": "와 미달인 사람 처음 봤어",
          "original_form": "와 미달인 사람 처음 봤어",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002492.1.7",
          "form": "초과는 봤는데",
          "original_form": "초과는 봤는데",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002492.1.8",
          "form": "내 동생이 좀 마르긴 했지",
          "original_form": "내 동생이 좀 마르긴 했지",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002492.1.9",
          "form": "너 동생말고 주변에 군대 간 사람은 없어??",
          "original_form": "너 동생말고 주변에 군대 간 사람은 없어??",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002492.1.10",
          "form": "내가 아는 오빠도",
          "original_form": "내가 아는 오빠도",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002492.1.11",
          "form": "몸무게 미달로 공익이야",
          "original_form": "몸무게 미달로 공익이야",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002492.1.12",
          "form": "몸무게 초과로 공익한 사람도 한명 안다",
          "original_form": "몸무게 초과로 공익한 사람도 한명 안다",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002492.1.13",
          "form": "공익 은근 많네",
          "original_form": "공익 은근 많네",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002492.1.14",
          "form": "응 그러게",
          "original_form": "응 그러게",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002492.1.15",
          "form": "어디쪽에서 일하신데?",
          "original_form": "어디쪽에서 일하신데?",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002492.1.16",
          "form": "그분은 해운사? 무슨 절에서 일한다고 했어",
          "original_form": "그분은 해운사? 무슨 절에서 일한다고 했어",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002492.1.17",
          "form": "신기하지",
          "original_form": "신기하지",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002492.1.18",
          "form": "절에서 하는건 처음 듣는다",
          "original_form": "절에서 하는건 처음 듣는다",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002492.1.19",
          "form": "어디 기관에서 일하는 사람들은 많던데",
          "original_form": "어디 기관에서 일하는 사람들은 많던데",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002492.1.20",
          "form": "나도 처음 들었어",
          "original_form": "나도 처음 들었어",
          "speaker_id": "2",
          "time": "20210820 15:20"
        }
      ]
