        },
        {
          "id": "MDRW2100024773.1.2",
          "form": "겨울여행",
          "original_form": "겨울여행",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.3",
          "form": "겨울 보라카이는 진짜 좋았어요ㅋ",
          "original_form": "겨울 보라카이는 진짜 좋았어요ㅋ",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.4",
          "form": "한국이 추우니깐ㅋㅋ더 기억에 남더라구요",
          "original_form": "한국이 추우니깐ㅋㅋ더 기억에 남더라구요",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.5",
          "form": "보라카이ㅠ 가보고싶었는데",
          "original_form": "보라카이ㅠ 가보고싶었는데",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.6",
          "form": "직항이 있었나요?",
          "original_form": "직항이 있었나요?",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.7",
          "form": "그렇겠네요 따뜻한겨울이라",
          "original_form": "그렇겠네요 따뜻한겨울이라",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.8",
          "form": "네~직항이 있어요",
          "original_form": "네~직항이 있어요",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.9",
          "form": "칼리보공항이예요",
          "original_form": "칼리보공항이예요",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024773.1.10",
          "form": "오호 거기서 또 뭐 타야하나요?",
          "original_form": "오호 거기서 또 뭐 타야하나요?",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024773.1.11",
          "form": "직항이긴한데, 내려서 버스2시간?정도타고ㅋ",
          "original_form": "직항이긴한데, 내려서 버스2시간?정도타고ㅋ",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024773.1.12",
          "form": "또 배타고 들어가야해요 ㅎㅎㅎ",
          "original_form": "또 배타고 들어가야해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024773.1.13",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024773.1.14",
          "form": "그렇죠?",
          "original_form": "그렇죠?",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.15",
          "form": "제가 그래서 포기했던거같아요 ㅎ",
          "original_form": "제가 그래서 포기했던거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.16",
          "form": "배멀미가",
          "original_form": "배멀미가",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.17",
          "form": "약간 의미 없는 직항ㅎㅎㅎ",
          "original_form": "약간 의미 없는 직항ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.18",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.19",
          "form": "공항도 무슨 시골버스터미널 수준이예요ㅋㅋ",
          "original_form": "공항도 무슨 시골버스터미널 수준이예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.20",
          "form": "ㅋㅋㅋ그래도 다녀온 사람들은 엄청 좋았다고",
          "original_form": "ㅋㅋㅋ그래도 다녀온 사람들은 엄청 좋았다고",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.21",
          "form": "아 역시 인천공항이",
          "original_form": "아 역시 인천공항이",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.22",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.23",
          "form": "맞아요ㅋㅋ화이트해변이 좋았어요",
          "original_form": "맞아요ㅋㅋ화이트해변이 좋았어요",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024773.1.24",
          "form": "인천공항이 최고죠ㅋㅋㅋ",
          "original_form": "인천공항이 최고죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024773.1.25",
          "form": "화이트해변 캬",
          "original_form": "화이트해변 캬",
          "speaker_id": "2",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024773.1.26",
          "form": "바다가고싶네요",
          "original_form": "바다가고싶네요",
          "speaker_id": "2",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024773.1.27",
          "form": "코로나땜에 못가니깐 답답하네요",
          "original_form": "코로나땜에 못가니깐 답답하네요",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024773.1.28",
          "form": "코로나 끝나고 여행 가야해요ㅋㅋㅋ",
          "original_form": "코로나 끝나고 여행 가야해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:57"
        }
      ]
    }
