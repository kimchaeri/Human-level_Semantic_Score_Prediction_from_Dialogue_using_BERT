      "utterance": [
        {
          "id": "MDRW2100046153.1.1",
          "form": "점심은",
          "original_form": "점심은",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046153.1.2",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046153.1.3",
          "form": "제육볶음 먹을까말까 고민중이에요",
          "original_form": "제육볶음 먹을까말까 고민중이에요",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046153.1.4",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046153.1.5",
          "form": "고기 먹고",
          "original_form": "고기 먹고",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046153.1.6",
          "form": "싶어요",
          "original_form": "싶어요",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046153.1.7",
          "form": "ㅋㅋㅋㅋㅋ어제 반찬가게에서",
          "original_form": "ㅋㅋㅋㅋㅋ어제 반찬가게에서",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046153.1.8",
          "form": "시켰어요",
          "original_form": "시켰어요",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046153.1.9",
          "form": "다른 건 뭐 시켰어요?",
          "original_form": "다른 건 뭐 시켰어요?",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046153.1.10",
          "form": "돼지갈비찜이욬ㅋㅋ",
          "original_form": "돼지갈비찜이욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046153.1.11",
          "form": "ㅋㅋㅋㅋ제육도 돼지고기고",
          "original_form": "ㅋㅋㅋㅋ제육도 돼지고기고",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046153.1.12",
          "form": "갈비찜도 돼지고기네요",
          "original_form": "갈비찜도 돼지고기네요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046153.1.13",
          "form": "원래 고기는",
          "original_form": "원래 고기는",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046153.1.14",
          "form": "많이 먹을수록 좋아요",
          "original_form": "많이 먹을수록 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046153.1.15",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046153.1.16",
          "form": "점심에 고기드세여!!!!",
          "original_form": "점심에 고기드세여!!!!",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046153.1.17",
          "form": "어떤 고기 좋아하세요?!",
          "original_form": "어떤 고기 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046153.1.18",
          "form": "삼겹살!!!",
          "original_form": "삼겹살!!!",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046153.1.19",
          "form": "아.. 삼겹살은 진리죠",
          "original_form": "아.. 삼겹살은 진리죠",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046153.1.20",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046153.1.21",
          "form": "ㅋㅋㅋ고기 드실거잖아요..",
          "original_form": "ㅋㅋㅋ고기 드실거잖아요..",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046153.1.22",
          "form": "양념된거랑 안된거랑 천지차이죠",
          "original_form": "양념된거랑 안된거랑 천지차이죠",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046153.1.23",
          "form": "삼겹살은 그 기름맛으로 먹고",
          "original_form": "삼겹살은 그 기름맛으로 먹고",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046153.1.24",
          "form": "제육은 양념맛",
          "original_form": "제육은 양념맛",
