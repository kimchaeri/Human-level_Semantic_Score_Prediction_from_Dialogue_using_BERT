      "utterance": [
        {
          "id": "MDRW2100013062.1.1",
          "form": "니는 어디고?",
          "original_form": "니는 어디고?",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013062.1.2",
          "form": "일하고있나?",
          "original_form": "일하고있나?",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013062.1.3",
          "form": "일하고있쥬~낼 쉽니더~ㅋㅋㅋ",
          "original_form": "일하고있쥬~낼 쉽니더~ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013062.1.4",
          "form": "낼 애들 일찍온다는 소뮨이 있던데??",
          "original_form": "낼 애들 일찍온다는 소뮨이 있던데??",
          "speaker_id": "1",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013062.1.5",
          "form": "낼 뿌염 좀 할라했디..",
          "original_form": "낼 뿌염 좀 할라했디..",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100013062.1.6",
          "form": "내일 일찍오나?",
          "original_form": "내일 일찍오나?",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100013062.1.7",
          "form": "난 정상하는줄알았는데",
          "original_form": "난 정상하는줄알았는데",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100013062.1.8",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100013062.1.9",
          "form": "난 잘 몰라서 물어보니 일찍오는거 아니냐 하길래요 ㅋㅋㅋㅋㅋ",
          "original_form": "난 잘 몰라서 물어보니 일찍오는거 아니냐 하길래요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100013062.1.10",
          "form": "난도 잘 몰러요 ㅋㅋㅋㅋ",
          "original_form": "난도 잘 몰러요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100013062.1.11",
          "form": "집에오니 또 정리할거한가득",
          "original_form": "집에오니 또 정리할거한가득",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013062.1.12",
          "form": "후텁지근하니 더하기싫네",
          "original_form": "후텁지근하니 더하기싫네",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013062.1.13",
          "form": "집에왔어요??",
          "original_form": "집에왔어요??",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.14",
          "form": "집에옴ㅋㅇㅋ",
          "original_form": "집에옴ㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.15",
          "form": "오늘 한바탕해가 더 하기 싫쳐 ㅋㅋ",
          "original_form": "오늘 한바탕해가 더 하기 싫쳐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.16",
          "form": "비이제안오는갑네ㅋ",
          "original_form": "비이제안오는갑네ㅋ",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.17",
          "form": "머리가 지끈거린다ㅡㅡ",
          "original_form": "머리가 지끈거린다ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.18",
          "form": "사람들 우산쓰고가던데요.?",
          "original_form": "사람들 우산쓰고가던데요.?",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.19",
          "form": "긍께요~훌훌 털고 마주치지.않는걸로~",
          "original_form": "긍께요~훌훌 털고 마주치지.않는걸로~",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.20",
          "form": "그래?ㅋㅇㅋ",
          "original_form": "그래?ㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013062.1.21",
          "form": "진짜 사람 스트레스가 ㅋㅋㅋㅋ",
          "original_form": "진짜 사람 스트레스가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013062.1.22",
          "form": "특히 대화안되는사람 ㅋㅋㅋ젱 무섭다 ㅋㅋ",
          "original_form": "특히 대화안되는사람 ㅋㅋㅋ젱 무섭다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013062.1.23",
          "form": "맞재?ㅋㅋ",
          "original_form": "맞재?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013062.1.24",
          "form": "본인도 힘들꺼예요~그런사람은.",
          "original_form": "본인도 힘들꺼예요~그런사람은.",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013062.1.25",
          "form": "근데..그 가족 다그럴듯 ㅋㅋㅋㅋ",
          "original_form": "근데..그 가족 다그럴듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013062.1.26",
          "form": "가족이 다그런거맞는듯ㅋ",
          "original_form": "가족이 다그런거맞는듯ㅋ",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013062.1.27",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013062.1.28",
          "form": "그래서 같이 사는거임 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 같이 사는거임 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:13"
        }
      ]
    }
