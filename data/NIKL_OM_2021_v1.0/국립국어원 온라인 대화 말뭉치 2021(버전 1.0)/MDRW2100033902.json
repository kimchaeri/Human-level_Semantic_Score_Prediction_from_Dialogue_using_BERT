      "utterance": [
        {
          "id": "MDRW2100033902.1.1",
          "form": "집안일 잘하시나요?",
          "original_form": "집안일 잘하시나요?",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033902.1.2",
          "form": "아뇨ㅠㅠ그냥 돕는정도ㅠㅠㅎㅎ",
          "original_form": "아뇨ㅠㅠ그냥 돕는정도ㅠㅠㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033902.1.3",
          "form": "그럼 그중 이건 내가 좀 잘한다 싶은건요?",
          "original_form": "그럼 그중 이건 내가 좀 잘한다 싶은건요?",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033902.1.4",
          "form": "저는 그냥 재활용?ㅋㅋ부피가 커서 그렇지 금방 끝나더라구요",
          "original_form": "저는 그냥 재활용?ㅋㅋ부피가 커서 그렇지 금방 끝나더라구요",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100033902.1.5",
          "form": "재활용도 엄청 귀찮던데요. 전 자꾸 들고 나가는거 깜박하거든요 ㅋㅋ",
          "original_form": "재활용도 엄청 귀찮던데요. 전 자꾸 들고 나가는거 깜박하거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100033902.1.6",
          "form": "저는 문앞에 두고 있어서ㅋㅋ까먹을수가 없어요ㅋㅋ",
          "original_form": "저는 문앞에 두고 있어서ㅋㅋ까먹을수가 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100033902.1.7",
          "form": "저도 문앞에 있는데요 ㅠ",
          "original_form": "저도 문앞에 있는데요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033902.1.8",
          "form": "아ㅋㅋㅋ재활용할땐 언제그렇게  뭘샀는지 신기해요ㅋㅋ",
          "original_form": "아ㅋㅋㅋ재활용할땐 언제그렇게  뭘샀는지 신기해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033902.1.9",
          "form": "집에 비닐이 엄청 쌓이지 않아요??",
          "original_form": "집에 비닐이 엄청 쌓이지 않아요??",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033902.1.10",
          "form": "맞아요 뭐만 사도 포장지가ㅠㅠ넘쳐요",
          "original_form": "맞아요 뭐만 사도 포장지가ㅠㅠ넘쳐요",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033902.1.11",
          "form": "별로 산게 없는데 비닐류가 항상 한가득이예요 ㅋㅋ",
          "original_form": "별로 산게 없는데 비닐류가 항상 한가득이예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033902.1.12",
          "form": "그러니까요ㅎㅎ 비닐보면 깜짝놀라요ㅎㅎ",
          "original_form": "그러니까요ㅎㅎ 비닐보면 깜짝놀라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033902.1.13",
          "form": "비닐이.제일 많이 나오지 않아요??",
          "original_form": "비닐이.제일 많이 나오지 않아요??",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100033902.1.14",
          "form": "네 엄청쌓여요ㅋㅋ",
          "original_form": "네 엄청쌓여요ㅋㅋ",
