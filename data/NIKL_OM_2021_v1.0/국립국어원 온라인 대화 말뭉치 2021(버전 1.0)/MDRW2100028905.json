        {
          "id": "MDRW2100028905.1.1",
          "form": "얼굴",
          "original_form": "얼굴",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028905.1.2",
          "form": "미용실 가야할 때가 오네요",
          "original_form": "미용실 가야할 때가 오네요",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028905.1.3",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028905.1.4",
          "form": "저는 날 때부터 패션이 완성되어있었어요",
          "original_form": "저는 날 때부터 패션이 완성되어있었어요",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028905.1.5",
          "form": "얼굴이 잘생기셨나봐오",
          "original_form": "얼굴이 잘생기셨나봐오",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028905.1.6",
          "form": "아무래도좀 우월하죠",
          "original_form": "아무래도좀 우월하죠",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028905.1.7",
          "form": "건강하면 최고죠",
          "original_form": "건강하면 최고죠",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028905.1.8",
          "form": "유전자의 잘못",
          "original_form": "유전자의 잘못",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028905.1.9",
          "form": "운동 하면 체질 개선되겠죠",
          "original_form": "운동 하면 체질 개선되겠죠",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028905.1.10",
          "form": "건강하신가봐요 축하드려요",
          "original_form": "건강하신가봐요 축하드려요",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028905.1.11",
          "form": "네 이참에 성형이나 할까봐요",
          "original_form": "네 이참에 성형이나 할까봐요",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028905.1.12",
          "form": "네 제발요",
          "original_form": "네 제발요",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028905.1.13",
          "form": "성형은 아니더라도 시술 정도",
          "original_form": "성형은 아니더라도 시술 정도",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028905.1.14",
          "form": "이 세계의 평화를 위해",
          "original_form": "이 세계의 평화를 위해",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028905.1.15",
          "form": "요즘은 티가 안 나더라구오",
          "original_form": "요즘은 티가 안 나더라구오",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028905.1.16",
          "form": "성형도 능력입니다",
          "original_form": "성형도 능력입니다",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028905.1.17",
          "form": "따끔하겠죠",
          "original_form": "따끔하겠죠",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028905.1.18",
          "form": "티는 나던대요",
          "original_form": "티는 나던대요",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028905.1.19",
          "form": "티나돛예쁘면 되죠",
          "original_form": "티나돛예쁘면 되죠",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028905.1.20",
          "form": "먹기좋은 떡이 보기도 좋죠",
          "original_form": "먹기좋은 떡이 보기도 좋죠",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028905.1.21",
          "form": "다 그런거아니겠어요",
          "original_form": "다 그런거아니겠어요",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028905.1.22",
          "form": "그러게요",
          "original_form": "그러게요",
