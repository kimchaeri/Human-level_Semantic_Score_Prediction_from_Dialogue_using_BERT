      "utterance": [
        {
          "id": "MDRW2100032888.1.1",
          "form": "아 좋아",
          "original_form": "아 좋아",
          "speaker_id": "2",
          "time": "20210906 04:00"
        },
        {
          "id": "MDRW2100032888.1.2",
          "form": "잘못눌러서 식음료로 왔넼ㅋㅋ",
          "original_form": "잘못눌러서 식음료로 왔넼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:01"
        },
        {
          "id": "MDRW2100032888.1.3",
          "form": "음료 좋아하는 것들 맣해죠",
          "original_form": "음료 좋아하는 것들 맣해죠",
          "speaker_id": "2",
          "time": "20210906 04:01"
        },
        {
          "id": "MDRW2100032888.1.4",
          "form": "그래도 먹을껀 할 얘기 많짘ㅋㅋ",
          "original_form": "그래도 먹을껀 할 얘기 많짘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:01"
        },
        {
          "id": "MDRW2100032888.1.5",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210906 04:01"
        },
        {
          "id": "MDRW2100032888.1.6",
          "form": "말해도말해도 끝이없지",
          "original_form": "말해도말해도 끝이없지",
          "speaker_id": "2",
          "time": "20210906 04:01"
        },
        {
          "id": "MDRW2100032888.1.7",
          "form": "나 디저트류랑 한식되게 좋아해 너는??",
          "original_form": "나 디저트류랑 한식되게 좋아해 너는??",
          "speaker_id": "1",
          "time": "20210906 04:01"
        },
        {
          "id": "MDRW2100032888.1.8",
          "form": "맞아맞아 먹는건 사는거라 할말이 많짘ㅋㅋ",
          "original_form": "맞아맞아 먹는건 사는거라 할말이 많짘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:01"
        },
        {
          "id": "MDRW2100032888.1.9",
          "form": "난 달달한 디저트에 차!",
          "original_form": "난 달달한 디저트에 차!",
          "speaker_id": "2",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032888.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ당연^^ 취향도 각각 바껴서 20시간 얘기 가능",
          "original_form": "ㅋㅋㅋㅋㅋㅋ당연^^ 취향도 각각 바껴서 20시간 얘기 가능",
          "speaker_id": "2",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032888.1.11",
          "form": "다저트에는 어떤차 마시는거 좋아해??",
          "original_form": "다저트에는 어떤차 마시는거 좋아해??",
          "speaker_id": "1",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032888.1.12",
          "form": "음 안 단 차",
          "original_form": "음 안 단 차",
          "speaker_id": "2",
          "time": "20210906 04:15"
        },
        {
          "id": "MDRW2100032888.1.13",
          "form": "달달하면 부담스러웟",
          "original_form": "달달하면 부담스러웟",
          "speaker_id": "2",
          "time": "20210906 04:15"
        },
        {
          "id": "MDRW2100032888.1.14",
          "form": "그리고 물리지.",
          "original_form": "그리고 물리지.",
          "speaker_id": "1",
          "time": "20210906 04:16"
        },
        {
          "id": "MDRW2100032888.1.15",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 04:16"
        },
        {
          "id": "MDRW2100032888.1.16",
          "form": "차로 디저트를 싹 내려줘야",
          "original_form": "차로 디저트를 싹 내려줘야",
          "speaker_id": "2",
          "time": "20210906 04:17"
        },
        {
          "id": "MDRW2100032888.1.17",
          "form": "나 먹을 수 있어",
          "original_form": "나 먹을 수 있어",
          "speaker_id": "2",
          "time": "20210906 04:17"
        },
        {
          "id": "MDRW2100032888.1.18",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210906 04:17"
        },
        {
          "id": "MDRW2100032888.1.19",
          "form": "맞아 괜히 사람들이 디저트카페가서 아메리카노를 마시는게 아니야",
          "original_form": "맞아 괜히 사람들이 디저트카페가서 아메리카노를 마시는게 아니야",
          "speaker_id": "1",
          "time": "20210906 04:17"
        },
        {
          "id": "MDRW2100032888.1.20",
          "form": "ㅋㅋㅋㅋㅋ그니까!! 달달 달달은 아우 당뇨병걸려",
