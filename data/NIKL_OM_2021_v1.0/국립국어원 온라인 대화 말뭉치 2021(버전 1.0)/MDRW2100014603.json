      "utterance": [
        {
          "id": "MDRW2100014603.1.1",
          "form": "건강 챙기기 위해서 하시는 거 있으신가요?",
          "original_form": "건강 챙기기 위해서 하시는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014603.1.2",
          "form": "저는  영양제 먹고있어요",
          "original_form": "저는  영양제 먹고있어요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014603.1.3",
          "form": "특히 밀크씨슬 이요",
          "original_form": "특히 밀크씨슬 이요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014603.1.4",
          "form": "간이건강해야해요",
          "original_form": "간이건강해야해요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014603.1.5",
          "form": "오 영양제 보면 밀크씨슬 드시고 계신분들 많더라고요",
          "original_form": "오 영양제 보면 밀크씨슬 드시고 계신분들 많더라고요",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014603.1.6",
          "form": "저는 종합영양제랑 실리마린 먹다가 다 먹고 재구매는 보류중이에요",
          "original_form": "저는 종합영양제랑 실리마린 먹다가 다 먹고 재구매는 보류중이에요",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014603.1.7",
          "form": "맞아요 간이 몸에서 중요한 장기더라고요",
          "original_form": "맞아요 간이 몸에서 중요한 장기더라고요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014603.1.8",
          "form": "확실히 드시고나면 건강해지는게 느껴지시나요?",
          "original_form": "확실히 드시고나면 건강해지는게 느껴지시나요?",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014603.1.9",
          "form": "제가 술을 즐겨서  선물로 받앗어요",
          "original_form": "제가 술을 즐겨서  선물로 받앗어요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014603.1.10",
          "form": "카카오톡 선물로 받았네요",
          "original_form": "카카오톡 선물로 받았네요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014603.1.11",
          "form": "아하 술을 즐기시는군요 name2님!",
          "original_form": "아하 술을 즐기시는군요 &name2&님!",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014603.1.12",
          "form": "그래도 밀크씨슬 드시고나면 간걱정이 조금 줄어들겠어요!",
          "original_form": "그래도 밀크씨슬 드시고나면 간걱정이 조금 줄어들겠어요!",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014603.1.13",
          "form": "자기만족아니겠습니까 ㅋㅋ",
          "original_form": "자기만족아니겠습니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014603.1.14",
          "form": "그쵸그쵸ㅎㅎ 자기만족이 있어야 효능도 올라간다구요~~!",
          "original_form": "그쵸그쵸ㅎㅎ 자기만족이 있어야 효능도 올라간다구요~~!",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014603.1.15",
          "form": "이제 여름이지나가니",
          "original_form": "이제 여름이지나가니",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014603.1.16",
          "form": "옷을 사야하자않을까요",
          "original_form": "옷을 사야하자않을까요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014603.1.17",
          "form": "맞아요 가을옷을 구매해야하는데 어떤걸 사야하나 생각중이에요",
          "original_form": "맞아요 가을옷을 구매해야하는데 어떤걸 사야하나 생각중이에요",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014603.1.18",
          "form": "요샌 또 가을이 짧아져서 말이죱",
          "original_form": "요샌 또 가을이 짧아져서 말이죱",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014603.1.19",
          "form": "전 바막을  살까해요 .",
          "original_form": "전 바막을  살까해요 .",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014603.1.20",
          "form": "아 걸칠옷 하나 있으면 좋죠!",
          "original_form": "아 걸칠옷 하나 있으면 좋죠!",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014603.1.21",
          "form": "감기 걸리기 쉬우니까요",
          "original_form": "감기 걸리기 쉬우니까요",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014603.1.22",
          "form": "저는 아마 셔츠?종류 살 것 같아요",
          "original_form": "저는 아마 셔츠?종류 살 것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014603.1.23",
          "form": "긴팔은  필요하죠",
          "original_form": "긴팔은  필요하죠",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014603.1.24",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014603.1.25",
          "form": "바람막이는 운동용이세요?",
          "original_form": "바람막이는 운동용이세요?",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014603.1.26",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 20:54"
        }
      ]
