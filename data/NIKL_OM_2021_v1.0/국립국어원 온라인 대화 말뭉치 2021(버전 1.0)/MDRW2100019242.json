      "utterance": [
        {
          "id": "MDRW2100019242.1.1",
          "form": "눈썹문신 하러가야되는데",
          "original_form": "눈썹문신 하러가야되는데",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019242.1.2",
          "form": "눈썹문신은 얼마나 감?",
          "original_form": "눈썹문신은 얼마나 감?",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019242.1.3",
          "form": "1년 좀 못가는듯",
          "original_form": "1년 좀 못가는듯",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019242.1.4",
          "form": "반영구는",
          "original_form": "반영구는",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019242.1.5",
          "form": "아 짧구나",
          "original_form": "아 짧구나",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019242.1.6",
          "form": "영구문신은 오래갈텐데",
          "original_form": "영구문신은 오래갈텐데",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019242.1.7",
          "form": "트렌드바뀌면...그렇게되지않나",
          "original_form": "트렌드바뀌면...그렇게되지않나",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019242.1.8",
          "form": "아눈썹모양?",
          "original_form": "아눈썹모양?",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019242.1.9",
          "form": "엉ㅋㅋㅋ",
          "original_form": "엉ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019242.1.10",
          "form": "나는 그냥 잇는대로 살아서ㅋㅋ",
          "original_form": "나는 그냥 잇는대로 살아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019242.1.11",
          "form": "글킨하군",
          "original_form": "글킨하군",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019242.1.12",
          "form": "색도 좀 바래지고??",
          "original_form": "색도 좀 바래지고??",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019242.1.13",
          "form": "속눈썹 펌도 하고싶은데 더짧은가바",
          "original_form": "속눈썹 펌도 하고싶은데 더짧은가바",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019242.1.14",
          "form": "그건 잘못하면 속눈썹 안빠지나",
          "original_form": "그건 잘못하면 속눈썹 안빠지나",
          "speaker_id": "1",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019242.1.15",
          "form": "모르겠네..",
          "original_form": "모르겠네..",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019242.1.16",
          "form": "근데 어디갈때 하라더라..얼마안간다고",
          "original_form": "근데 어디갈때 하라더라..얼마안간다고",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019242.1.17",
          "form": "아 놀러갈때ㅋㅋ",
          "original_form": "아 놀러갈때ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:40"
