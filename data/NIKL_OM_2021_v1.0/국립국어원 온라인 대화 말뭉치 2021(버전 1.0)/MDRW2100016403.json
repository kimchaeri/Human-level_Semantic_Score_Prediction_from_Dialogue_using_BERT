      "utterance": [
        {
          "id": "MDRW2100016403.1.1",
          "form": "요새 하는 게임 있어?",
          "original_form": "요새 하는 게임 있어?",
          "speaker_id": "2",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016403.1.2",
          "form": "나는 모바일게임",
          "original_form": "나는 모바일게임",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016403.1.3",
          "form": "아이러브커피N",
          "original_form": "아이러브커피N",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016403.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016403.1.5",
          "form": "난 머지맨션이랑 프로젝트메이크오버ㅋㅋ",
          "original_form": "난 머지맨션이랑 프로젝트메이크오버ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016403.1.6",
          "form": "둘다 걍 그 퍼즐게임류",
          "original_form": "둘다 걍 그 퍼즐게임류",
          "speaker_id": "2",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016403.1.7",
          "form": "와..진짜 완전 처음들어봐",
          "original_form": "와..진짜 완전 처음들어봐",
          "speaker_id": "1",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016403.1.8",
          "form": "아하 나 디즈니팝도 하는데 이건 캔디크러쉬같은거",
          "original_form": "아하 나 디즈니팝도 하는데 이건 캔디크러쉬같은거",
          "speaker_id": "1",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016403.1.9",
          "form": "프로젝트메이크오버가 그런 스탈이야",
          "original_form": "프로젝트메이크오버가 그런 스탈이야",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016403.1.10",
          "form": "우리 취향이 비슷하구나",
          "original_form": "우리 취향이 비슷하구나",
          "speaker_id": "1",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016403.1.11",
          "form": "캐릭터 꾸미기같은거도 있어서 심심풀이 하먄서 하기 좋더라구",
          "original_form": "캐릭터 꾸미기같은거도 있어서 심심풀이 하먄서 하기 좋더라구",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016403.1.12",
          "form": "아이러브커피는 안하는데 난?ㅎ",
          "original_form": "아이러브커피는 안하는데 난?ㅎ",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016403.1.13",
          "form": "디즈니팝도 아기자기해",
          "original_form": "디즈니팝도 아기자기해",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016403.1.14",
          "form": "별 모아서 마을 꾸며주는거",
          "original_form": "별 모아서 마을 꾸며주는거",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016403.1.15",
          "form": "너 아이러브커피 했었잖아..^^..",
          "original_form": "너 아이러브커피 했었잖아..^^..",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016403.1.16",
          "form": "ㅋㅋㅋㅋ아이러브커피 한거 거의 10년전인데요..?",
          "original_form": "ㅋㅋㅋㅋ아이러브커피 한거 거의 10년전인데요..?",
          "speaker_id": "2",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016403.1.17",
          "form": "헐 10년전이라니..너 대학생때도 했자나",
          "original_form": "헐 10년전이라니..너 대학생때도 했자나",
          "speaker_id": "1",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016403.1.18",
          "form": "부정하지마",
          "original_form": "부정하지마",
          "speaker_id": "1",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016403.1.19",
          "form": "ㅎㅎ요샌 다른 다양한 게임도 많지",
          "original_form": "ㅎㅎ요샌 다른 다양한 게임도 많지",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016403.1.20",
          "form": "심즈였나? 유튜브에서 보니까 그거도 재밌너보이더라",
          "original_form": "심즈였나? 유튜브에서 보니까 그거도 재밌너보이더라",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016403.1.21",
          "form": "오 핸드폰 심즈?",
          "original_form": "오 핸드폰 심즈?",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016403.1.22",
