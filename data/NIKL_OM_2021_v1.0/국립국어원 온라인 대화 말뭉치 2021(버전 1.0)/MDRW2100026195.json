      "utterance": [
        {
          "id": "MDRW2100026195.1.1",
          "form": "name1 학교 비대면이야?",
          "original_form": "&name1& 학교 비대면이야?",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026195.1.2",
          "form": "엉 name1 아마 다 비대면일걸?",
          "original_form": "엉 &name1& 아마 다 비대면일걸?",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026195.1.3",
          "form": "중간고사 끝나도???",
          "original_form": "중간고사 끝나도???",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026195.1.4",
          "form": "시험때는 아마 대면일 거 같고",
          "original_form": "시험때는 아마 대면일 거 같고",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026195.1.5",
          "form": "중간 끝나고 나서는 아마 시험 치고 그 때 상황봐서 정해질 거 같어",
          "original_form": "중간 끝나고 나서는 아마 시험 치고 그 때 상황봐서 정해질 거 같어",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026195.1.6",
          "form": "하긴 그때 쯤이면... 다들 백신 맞았겠지..",
          "original_form": "하긴 그때 쯤이면... 다들 백신 맞았겠지..",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026195.1.7",
          "form": "아 맞네...",
          "original_form": "아 맞네...",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026195.1.8",
          "form": "백신 맞았으니까 대면 가겠다",
          "original_form": "백신 맞았으니까 대면 가겠다",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026195.1.9",
          "form": "좀 비대면의 좋은 점을 더 누리고 싶어",
          "original_form": "좀 비대면의 좋은 점을 더 누리고 싶어",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026195.1.10",
          "form": "그래도 이번 학기까지는 비대면이었으면 좋겠어",
          "original_form": "그래도 이번 학기까지는 비대면이었으면 좋겠어",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026195.1.11",
          "form": "비대면 좋았지...",
          "original_form": "비대면 좋았지...",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026195.1.12",
          "form": "맞아 이번 학기까지는 비대면 했음 좋겠어",
          "original_form": "맞아 이번 학기까지는 비대면 했음 좋겠어",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026195.1.13",
          "form": "그거 하고 깔끔하게 휴학할테야",
          "original_form": "그거 하고 깔끔하게 휴학할테야",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026195.1.14",
          "form": "멋져",
          "original_form": "멋져",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026195.1.15",
          "form": "휴학은 대학생의 로망이지",
          "original_form": "휴학은 대학생의 로망이지",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026195.1.16",
          "form": "암 그렇지",
          "original_form": "암 그렇지",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026195.1.17",
          "form": "동기 모두 졸업하고 혼자 4학년을 한대도 휴학해버릴 거야",
          "original_form": "동기 모두 졸업하고 혼자 4학년을 한대도 휴학해버릴 거야",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026195.1.18",
          "form": "ㅋㅋㅋㅋㅋ 휴학하면 뭐할려고",
          "original_form": "ㅋㅋㅋㅋㅋ 휴학하면 뭐할려고",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026195.1.19",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026195.1.20",
          "form": "일단 운전면허 따고",
          "original_form": "일단 운전면허 따고",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026195.1.21",
          "form": "알바 구해보고",
          "original_form": "알바 구해보고",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026195.1.22",
          "form": "토익이랑 컴활... 해야겠지",
          "original_form": "토익이랑 컴활... 해야겠지",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026195.1.23",
          "form": "바쁘구나...학기 중에는",
          "original_form": "바쁘구나...학기 중에는",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026195.1.24",
          "form": "뭐할거야?",
          "original_form": "뭐할거야?",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026195.1.25",
          "form": "일단 목표는 완전 성실히 살아서 학점 잘 받기인데",
          "original_form": "일단 목표는 완전 성실히 살아서 학점 잘 받기인데",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026195.1.26",
          "form": "아마 나중가면 국장 받을 정도로만 학점 따기로 바뀔 거란 걸 수우는 알아",
          "original_form": "아마 나중가면 국장 받을 정도로만 학점 따기로 바뀔 거란 걸 수우는 알아",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026195.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ국장 받을정도로 공부하는 거면 충분하지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ국장 받을정도로 공부하는 거면 충분하지",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026195.1.28",
          "form": "name1는 대학원 안 가?",
          "original_form": "&name1&는 대학원 안 가?",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026195.1.29",
          "form": "대학원 가지 않아...",
          "original_form": "대학원 가지 않아...",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026195.1.30",
          "form": "가기 싫어",
          "original_form": "가기 싫어",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026195.1.31",
          "form": "알겠어 너의 의지 단호하네.",
          "original_form": "알겠어 너의 의지 단호하네.",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026195.1.32",
          "form": "대학원도 좀 하고자하는게 뚜렷하면 생각해보겠는데",
          "original_form": "대학원도 좀 하고자하는게 뚜렷하면 생각해보겠는데",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026195.1.33",
          "form": "그런 게 아니니까 걍 가서 돈이랑 시간만 버릴 거 같어",
          "original_form": "그런 게 아니니까 걍 가서 돈이랑 시간만 버릴 거 같어",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026195.1.34",
          "form": "아... 그럴수있지",
          "original_form": "아... 그럴수있지",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026195.1.35",
          "form": "나도 대학원 그냥",
          "original_form": "나도 대학원 그냥",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026195.1.36",
          "form": "만약 내가.  교수가 도ㅐ.",
          "original_form": "만약 내가.  교수가 도ㅐ.",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026195.1.37",
          "form": "이딴 상상할 때나 생각하지",
          "original_form": "이딴 상상할 때나 생각하지",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026195.1.38",
          "form": "진로가 확실하다고 여겨지진 않아서 가진 않을 듯",
          "original_form": "진로가 확실하다고 여겨지진 않아서 가진 않을 듯",
