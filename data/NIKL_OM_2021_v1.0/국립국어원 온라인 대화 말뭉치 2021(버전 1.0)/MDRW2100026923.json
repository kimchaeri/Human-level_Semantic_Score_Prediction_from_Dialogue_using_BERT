      "utterance": [
        {
          "id": "MDRW2100026923.1.1",
          "form": "찾아볼게",
          "original_form": "찾아볼게",
          "speaker_id": "2",
          "time": "20210902 01:24"
        },
        {
          "id": "MDRW2100026923.1.2",
          "form": "요즘 세일을 많이해",
          "original_form": "요즘 세일을 많이해",
          "speaker_id": "2",
          "time": "20210902 01:24"
        },
        {
          "id": "MDRW2100026923.1.3",
          "form": "지금 네이버에서 꿔바로우 3900원이래 사??",
          "original_form": "지금 네이버에서 꿔바로우 3900원이래 사??",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100026923.1.4",
          "form": "몇개 살 수있는디",
          "original_form": "몇개 살 수있는디",
          "speaker_id": "1",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100026923.1.5",
          "form": "여러개 살 수 있고 만원할인쿠폰이야",
          "original_form": "여러개 살 수 있고 만원할인쿠폰이야",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100026923.1.6",
          "form": "그럼 여러개 사",
          "original_form": "그럼 여러개 사",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026923.1.7",
          "form": "알겠어 잠깐만",
          "original_form": "알겠어 잠깐만",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026923.1.8",
          "form": "꿔바로우 말고는 없어??",
          "original_form": "꿔바로우 말고는 없어??",
          "speaker_id": "1",
          "time": "20210902 01:34"
        },
        {
          "id": "MDRW2100026923.1.9",
          "form": "있는데 거의 다 품절됐어",
          "original_form": "있는데 거의 다 품절됐어",
          "speaker_id": "2",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026923.1.10",
          "form": "그럼 어쩔수없지",
          "original_form": "그럼 어쩔수없지",
          "speaker_id": "1",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026923.1.11",
          "form": "맞다 오늘까지 알볼로피자에서 페이코인으로 결제하면 80%할인해준대",
          "original_form": "맞다 오늘까지 알볼로피자에서 페이코인으로 결제하면 80%할인해준대",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026923.1.12",
          "form": "내가 피자사줄게 무슨 맛으로 먹을래",
          "original_form": "내가 피자사줄게 무슨 맛으로 먹을래",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026923.1.13",
          "form": "그냥 아무거나",
          "original_form": "그냥 아무거나",
          "speaker_id": "1",
          "time": "20210902 01:39"
        },
        {
          "id": "MDRW2100026923.1.14",
          "form": "그럼 옥수수피자 먹자",
          "original_form": "그럼 옥수수피자 먹자",
          "speaker_id": "2",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026923.1.15",
          "form": "너 무슨 카드로 긁었어",
          "original_form": "너 무슨 카드로 긁었어",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026923.1.16",
          "form": "내 신용카드 중에 1.5%결제할인되는거",
          "original_form": "내 신용카드 중에 1.5%결제할인되는거",
          "speaker_id": "2",
