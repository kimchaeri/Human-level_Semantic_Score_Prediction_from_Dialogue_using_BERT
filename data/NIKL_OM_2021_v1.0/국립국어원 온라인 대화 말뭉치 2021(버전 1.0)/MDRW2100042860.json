      "utterance": [
        {
          "id": "MDRW2100042860.1.1",
          "form": "ㅋㅋㅋㅋㅋ너도 안가봄??",
          "original_form": "ㅋㅋㅋㅋㅋ너도 안가봄??",
          "speaker_id": "2",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042860.1.2",
          "form": "쫄면그거 삼쫄...먹고시퍼",
          "original_form": "쫄면그거 삼쫄...먹고시퍼",
          "speaker_id": "2",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042860.1.3",
          "form": "ㅇㅇ 나도안가봄",
          "original_form": "ㅇㅇ 나도안가봄",
          "speaker_id": "1",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042860.1.4",
          "form": "아니면 날 추우니까",
          "original_form": "아니면 날 추우니까",
          "speaker_id": "1",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042860.1.5",
          "form": "이자카야",
          "original_form": "이자카야",
          "speaker_id": "1",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042860.1.6",
          "form": "갠찮다",
          "original_form": "갠찮다",
          "speaker_id": "2",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042860.1.7",
          "form": "그때 갔던청담",
          "original_form": "그때 갔던청담",
          "speaker_id": "2",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042860.1.8",
          "form": "빗소리들으면서",
          "original_form": "빗소리들으면서",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100042860.1.9",
          "form": "사케나 소주에",
          "original_form": "사케나 소주에",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100042860.1.10",
          "form": "뜨끈한 탕",
          "original_form": "뜨끈한 탕",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100042860.1.11",
          "form": "너네 백신 언제 다맞아? 가고싶다리..",
          "original_form": "너네 백신 언제 다맞아? 가고싶다리..",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100042860.1.12",
          "form": "좋다 꼬치랑 시샤모 시켜줘",
          "original_form": "좋다 꼬치랑 시샤모 시켜줘",
          "speaker_id": "2",
          "time": "20210907 10:09"
        },
        {
          "id": "MDRW2100042860.1.13",
          "form": "꼬치진짜맛있겠다",
          "original_form": "꼬치진짜맛있겠다",
          "speaker_id": "1",
          "time": "20210907 10:09"
        },
        {
          "id": "MDRW2100042860.1.14",
          "form": "내가 젤 늡믄데",
          "original_form": "내가 젤 늡믄데",
          "speaker_id": "1",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042860.1.15",
          "form": "10월 7일",
          "original_form": "10월 7일",
          "speaker_id": "1",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042860.1.16",
          "form": "앗..그렇군... ㅠ 그래도 니생일땐 갈수있겠다",
          "original_form": "앗..그렇군... ㅠ 그래도 니생일땐 갈수있겠다",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042860.1.17",
          "form": "꼬치집 그 성대 거기 괜찮은데...비싼거빼곤..",
          "original_form": "꼬치집 그 성대 거기 괜찮은데...비싼거빼곤..",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042860.1.18",
          "form": "맞아 꼬치 일본보다맛있었어",
          "original_form": "맞아 꼬치 일본보다맛있었어",
          "speaker_id": "1",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100042860.1.19",
          "form": "인정...먹고싶다",
          "original_form": "인정...먹고싶다",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100042860.1.20",
          "form": "따뜻한거땽기네..오늘추워서",
          "original_form": "따뜻한거땽기네..오늘추워서",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100042860.1.21",
          "form": "점심 샌드위치인뎅..",
          "original_form": "점심 샌드위치인뎅..",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100042860.1.22",
          "form": "라면국물",
          "original_form": "라면국물",
          "speaker_id": "1",
          "time": "20210907 10:36"
        },
        {
          "id": "MDRW2100042860.1.23",
          "form": "우동",
          "original_form": "우동",
          "speaker_id": "1",
          "time": "20210907 10:37"
        },
        {
          "id": "MDRW2100042860.1.24",
