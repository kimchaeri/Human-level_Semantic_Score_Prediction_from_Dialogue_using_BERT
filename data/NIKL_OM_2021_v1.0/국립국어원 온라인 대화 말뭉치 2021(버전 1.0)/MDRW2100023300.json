      "utterance": [
        {
          "id": "MDRW2100023300.1.1",
          "form": "ㅋㅋㅋㅋㅋ이제 자러가야겟어요",
          "original_form": "ㅋㅋㅋㅋㅋ이제 자러가야겟어요",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023300.1.2",
          "form": "오늘도 잘 떠들었네여 ㅋㅋㅋㅋ",
          "original_form": "오늘도 잘 떠들었네여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023300.1.3",
          "form": "ㅋㅋㅋㅋㅋ저도 금토일 너무 눈빠지고 손아파욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저도 금토일 너무 눈빠지고 손아파욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023300.1.4",
          "form": "ㅋㅋㅋ컴퓨터로 연결해서 하세요",
          "original_form": "ㅋㅋㅋ컴퓨터로 연결해서 하세요",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023300.1.5",
          "form": "저 무선키보드로 치고있어요",
          "original_form": "저 무선키보드로 치고있어요",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023300.1.6",
          "form": "연결했었는데 키보드가 한글이 안쳐지는거에요",
          "original_form": "연결했었는데 키보드가 한글이 안쳐지는거에요",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023300.1.7",
          "form": "ㅋㅋㅋㅋ아주 가끔 저도 화면 멈추긴 하더라구요",
          "original_form": "ㅋㅋㅋㅋ아주 가끔 저도 화면 멈추긴 하더라구요",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023300.1.8",
          "form": "암튼 주말동안 즐거웠습니다 ㅋㅋㅋㅋ",
          "original_form": "암튼 주말동안 즐거웠습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023300.1.9",
          "form": "ㅋㅋㅋㅋ학원비만 날리고 왔네욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ학원비만 날리고 왔네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100023300.1.10",
          "form": "혹시 그 연예인 알아요?",
          "original_form": "혹시 그 연예인 알아요?",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023300.1.11",
          "form": "무브투헤븐에 나온 사람?",
          "original_form": "무브투헤븐에 나온 사람?",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023300.1.12",
          "form": "이제훈이 나왔나?",
          "original_form": "이제훈이 나왔나?",
          "speaker_id": "2",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023300.1.13",
          "form": "어 맞아요 ㅎㅎ 이제훈",
          "original_form": "어 맞아요 ㅎㅎ 이제훈",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023300.1.14",
          "form": "근데 이제훈 말고 또 다른 주연 있어요",
          "original_form": "근데 이제훈 말고 또 다른 주연 있어요",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023300.1.15",
          "form": "아스퍼거증후군 연기하시는 분",
          "original_form": "아스퍼거증후군 연기하시는 분",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023300.1.16",
          "form": "영화를 못봐서 ㅋㅋㅋㅋ 모르겟네요",
          "original_form": "영화를 못봐서 ㅋㅋㅋㅋ 모르겟네요",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100023300.1.17",
          "form": "아 ㅎㅎ 드라마인데 ㅠㅠ힝",
          "original_form": "아 ㅎㅎ 드라마인데 ㅠㅠ힝",
          "speaker_id": "1",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100023300.1.18",
          "form": "사랑의불시착이랑 라켓소년단에도 출연했어요!",
          "original_form": "사랑의불시착이랑 라켓소년단에도 출연했어요!",
          "speaker_id": "1",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100023300.1.19",
          "form": "아 드라마에요??",
          "original_form": "아 드라마에요??",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100023300.1.20",
          "form": "ㅋㅋㅋㅋㅋ세상에 그것도 몰랐네",
          "original_form": "ㅋㅋㅋㅋㅋ세상에 그것도 몰랐네",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100023300.1.21",
          "form": "ㅋㅋㅋㅋㅋ성이 되게 특이했어요",
          "original_form": "ㅋㅋㅋㅋㅋ성이 되게 특이했어요",
          "speaker_id": "1",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100023300.1.22",
          "form": "근데 저 그 세개다 못봐서 공통점을 못찾아욬ㅋㅋㅋ",
          "original_form": "근데 저 그 세개다 못봐서 공통점을 못찾아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.23",
          "form": "앗 제가 너무 어려운 퀴즈를 냈군요",
          "original_form": "앗 제가 너무 어려운 퀴즈를 냈군요",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.24",
          "form": "탕준상 배우님이라고 있거든요",
          "original_form": "탕준상 배우님이라고 있거든요",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.25",
          "form": "이름이 진짜 특이하네요",
          "original_form": "이름이 진짜 특이하네요",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.26",
          "form": "그쵸? 탕씨인게 너무 신기했어요",
          "original_form": "그쵸? 탕씨인게 너무 신기했어요",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.27",
          "form": "첨들어봐요 진짜",
          "original_form": "첨들어봐요 진짜",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.28",
          "form": "어렸을 적에 별명이 아마.. 탕....",
          "original_form": "어렸을 적에 별명이 아마.. 탕....",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.29",
          "form": "읍읍",
          "original_form": "읍읍",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.30",
          "form": "우리나라 성씨맞나..",
          "original_form": "우리나라 성씨맞나..",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.31",
          "form": "앜ㅋㅋ....",
          "original_form": "앜ㅋㅋ....",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.32",
          "form": "예명인가?ㅠ",
          "original_form": "예명인가?ㅠ",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100023300.1.33",
          "form": "03년생인 것도 놀라웠어요",
          "original_form": "03년생인 것도 놀라웠어요",
          "speaker_id": "1",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100023300.1.34",
          "form": "탕수육 이런 별명이지 않았을까요",
          "original_form": "탕수육 이런 별명이지 않았을까요",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100023300.1.35",
          "form": "아 03이에요? 왕 어리다",
          "original_form": "아 03이에요? 왕 어리다",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100023300.1.36",
          "form": "아니 요즘 02년생 03년생 이런 글자 자주 보이는데 너무 어색합니다",
          "original_form": "아니 요즘 02년생 03년생 이런 글자 자주 보이는데 너무 어색합니다",
          "speaker_id": "1",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100023300.1.37",
          "form": "뭐 어떤 연예인 좋아해요?",
          "original_form": "뭐 어떤 연예인 좋아해요?",
          "speaker_id": "1",
