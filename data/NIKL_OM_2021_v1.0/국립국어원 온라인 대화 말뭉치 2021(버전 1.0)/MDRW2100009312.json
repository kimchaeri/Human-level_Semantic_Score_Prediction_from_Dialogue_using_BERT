      "utterance": [
        {
          "id": "MDRW2100009312.1.1",
          "form": "이욜~",
          "original_form": "이욜~",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.2",
          "form": "훗 내일 비온다",
          "original_form": "훗 내일 비온다",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.3",
          "form": "태풍 온다던데",
          "original_form": "태풍 온다던데",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.4",
          "form": "오늘 여긴 비왔어여??",
          "original_form": "오늘 여긴 비왔어여??",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.5",
          "form": "헙...",
          "original_form": "헙...",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.6",
          "form": "애들 학원 우얄라나 몰라",
          "original_form": "애들 학원 우얄라나 몰라",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.7",
          "form": "아니 안왔음",
          "original_form": "아니 안왔음",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.8",
          "form": "내일 겁나 온다던데",
          "original_form": "내일 겁나 온다던데",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009312.1.9",
          "form": "낼 name3이 델꼬 가야는데 ㅋㅋㅋㅋㅋ카카오택시타야긋네여~",
          "original_form": "낼 &name3&이 델꼬 가야는데 ㅋㅋㅋㅋㅋ카카오택시타야긋네여~",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009312.1.10",
          "form": "저번처럼",
          "original_form": "저번처럼",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009312.1.11",
          "form": "우산도 필요없는거 아닌가 몰라 ㅋㅋㅋㅋ",
          "original_form": "우산도 필요없는거 아닌가 몰라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009312.1.12",
          "form": "하늘 뚫린날 ㅋㅋㅋ",
          "original_form": "하늘 뚫린날 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009312.1.13",
          "form": "ㅋㅋㅋㅋ 니 일하는것보다",
          "original_form": "ㅋㅋㅋㅋ 니 일하는것보다",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009312.1.14",
          "form": "택시비가 더 나오는거 아니가",
          "original_form": "택시비가 더 나오는거 아니가",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009312.1.15",
          "form": "언니 지금은 진짜..",
          "original_form": "언니 지금은 진짜..",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009312.1.16",
          "form": "첫찌가면 손수건접고 박스접고 3천원 정도 줘야되고",
          "original_form": "첫찌가면 손수건접고 박스접고 3천원 정도 줘야되고",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009312.1.17",
          "form": "햄버거 사줘야되고",
          "original_form": "햄버거 사줘야되고",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009312.1.18",
          "form": "택시타고 오고가고",
          "original_form": "택시타고 오고가고",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009312.1.19",
          "form": "본전임 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "본전임 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009312.1.20",
          "form": "날씨 좋음 걸어가면",
          "original_form": "날씨 좋음 걸어가면",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009312.1.21",
          "form": "운동도 되고 좋은디~",
          "original_form": "운동도 되고 좋은디~",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009312.1.22",
          "form": "ㅋㅋㅋㄱㄱㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㄱㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009312.1.23",
          "form": "햄버거까지 사주나",
          "original_form": "햄버거까지 사주나",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009312.1.24",
          "form": "그래 내일은 비 많이 온단다",
          "original_form": "그래 내일은 비 많이 온단다",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009312.1.25",
          "form": "그라니 계속 가고싶다고..",
          "original_form": "그라니 계속 가고싶다고..",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009312.1.26",
          "form": "그래도 계속 비 되있더니 3일만 내리네",
          "original_form": "그래도 계속 비 되있더니 3일만 내리네",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009312.1.27",
          "form": "남는시간은 유튜브에 ㅋㅋㅋ",
          "original_form": "남는시간은 유튜브에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009312.1.28",
          "form": "ㅋㅋㅋㅋㅋㄱㅋ너무 좋은데",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋ너무 좋은데",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.29",
          "form": "지는 더운거 보단 비가 좋아여~",
          "original_form": "지는 더운거 보단 비가 좋아여~",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.30",
          "form": "우리집 원도 델꼬가라",
          "original_form": "우리집 원도 델꼬가라",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.31",
          "form": "해 쨍쨍은 넘나 뜨거움 ㅋㅋㅋ",
          "original_form": "해 쨍쨍은 넘나 뜨거움 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.32",
          "form": "ㅋㅋㅋ 난 놀이터 못가서 비오는거 싫다",
          "original_form": "ㅋㅋㅋ 난 놀이터 못가서 비오는거 싫다",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.33",
          "form": "비오면 친구데려오는날이라고",
          "original_form": "비오면 친구데려오는날이라고",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.34",
          "form": "우리집 투가 ㅠ",
          "original_form": "우리집 투가 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ일을 잠시 쉬도록 하께여 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ일을 잠시 쉬도록 하께여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ아 귀여워 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 귀여워 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009312.1.37",
          "form": "행복한 날이네여~",
          "original_form": "행복한 날이네여~",
          "speaker_id": "2",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래 적자다 니는",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래 적자다 니는",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.39",
          "form": "어 계속 친구데려오고 싶다해서",
          "original_form": "어 계속 친구데려오고 싶다해서",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.40",
          "form": "비오는날은 놀이터 못가니",
          "original_form": "비오는날은 놀이터 못가니",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.41",
          "form": "집에서 놀자했드만",
          "original_form": "집에서 놀자했드만",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.42",
          "form": "비만 오면 이친구저친구 다 우리집가자고....",
          "original_form": "비만 오면 이친구저친구 다 우리집가자고....",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.43",
          "form": "근데 그쯤에 계속 친구 집에 놀러오라규 했던듯요~",
          "original_form": "근데 그쯤에 계속 친구 집에 놀러오라규 했던듯요~",
          "speaker_id": "2",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.44",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "2",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.45",
          "form": "내일 비온다했드만 \r\n바로 친구 데려오잖다",
          "original_form": "내일 비온다했드만 \r\n바로 친구 데려오잖다",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009312.1.46",
          "form": "그래 ㅋㅋㅋㅋ 우리 그때 이집저집 마이 댕겼지",
          "original_form": "그래 ㅋㅋㅋㅋ 우리 그때 이집저집 마이 댕겼지",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009312.1.47",
          "form": "날씨 개안네",
          "original_form": "날씨 개안네",
          "speaker_id": "1",
