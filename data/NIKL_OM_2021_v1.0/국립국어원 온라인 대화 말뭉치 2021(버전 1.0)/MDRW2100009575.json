      "utterance": [
        {
          "id": "MDRW2100009575.1.1",
          "form": "연애관..",
          "original_form": "연애관..",
          "speaker_id": "1",
          "time": "20210823 00:53"
        },
        {
          "id": "MDRW2100009575.1.2",
          "form": "저는 비혼주의자에요",
          "original_form": "저는 비혼주의자에요",
          "speaker_id": "1",
          "time": "20210823 00:53"
        },
        {
          "id": "MDRW2100009575.1.3",
          "form": "저도 비혼지향합니다",
          "original_form": "저도 비혼지향합니다",
          "speaker_id": "2",
          "time": "20210823 00:54"
        },
        {
          "id": "MDRW2100009575.1.4",
          "form": "음 비혼인 이유가 있나요?",
          "original_form": "음 비혼인 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210823 00:54"
        },
        {
          "id": "MDRW2100009575.1.5",
          "form": "저같은 경우는 결혼을 해야 할 이유를 못느껴요",
          "original_form": "저같은 경우는 결혼을 해야 할 이유를 못느껴요",
          "speaker_id": "2",
          "time": "20210823 00:54"
        },
        {
          "id": "MDRW2100009575.1.6",
          "form": "저도요 그리고 저 하나 챙기기에도 벅차거든요",
          "original_form": "저도요 그리고 저 하나 챙기기에도 벅차거든요",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009575.1.7",
          "form": "그리고 굳이 남자한테 시간을 쓰고싶지 않아요",
          "original_form": "그리고 굳이 남자한테 시간을 쓰고싶지 않아요",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009575.1.8",
          "form": "으음~ 공감합니다",
          "original_form": "으음~ 공감합니다",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009575.1.9",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009575.1.10",
          "form": "인생의 주체는 제가 되어야 하는 것 같아요",
          "original_form": "인생의 주체는 제가 되어야 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009575.1.11",
          "form": "저도 그렇게 생각해요 저는 저를 위해서 온전히 시간과 돈을 쓰고싶어요",
          "original_form": "저도 그렇게 생각해요 저는 저를 위해서 온전히 시간과 돈을 쓰고싶어요",
          "speaker_id": "1",
          "time": "20210823 00:56"
        },
        {
          "id": "MDRW2100009575.1.12",
          "form": "아주 좋은 마인드에요",
          "original_form": "아주 좋은 마인드에요",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009575.1.13",
          "form": "약간 결혼을 하면 저를 잃어버린다고 하더라구요",
          "original_form": "약간 결혼을 하면 저를 잃어버린다고 하더라구요",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009575.1.14",
          "form": "내가 아닌 남편 아이 어른들",
          "original_form": "내가 아닌 남편 아이 어른들",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009575.1.15",
          "form": "이게 좀 많이 슬픈거같아요",
          "original_form": "이게 좀 많이 슬픈거같아요",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009575.1.16",
          "form": "거기에 경력단절까지 더해지면..",
          "original_form": "거기에 경력단절까지 더해지면..",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009575.1.17",
          "form": "그니까요..",
          "original_form": "그니까요..",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009575.1.18",
          "form": "좀 더 저에게 집중하고 싶어요",
          "original_form": "좀 더 저에게 집중하고 싶어요",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009575.1.19",
          "form": "일도 오래 하고싶도요",
          "original_form": "일도 오래 하고싶도요",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009575.1.20",
          "form": "음 주위에 결혼한 사람들이 있나요?",
          "original_form": "음 주위에 결혼한 사람들이 있나요?",
          "speaker_id": "2",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009575.1.21",
          "form": "저는 있는데 그 사람들 보면 기분이 묘해져요. 약간 그런거 아세요?",
          "original_form": "저는 있는데 그 사람들 보면 기분이 묘해져요. 약간 그런거 아세요?",
          "speaker_id": "2",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009575.1.22",
          "form": "난 아직도 애같은디 애같은 내친구가 결혼을 한다고?",
          "original_form": "난 아직도 애같은디 애같은 내친구가 결혼을 한다고?",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009575.1.23",
          "form": "저는 20대 초반이라 아직은 없어요!",
          "original_form": "저는 20대 초반이라 아직은 없어요!",
          "speaker_id": "1",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009575.1.24",
          "form": "근데 친구가 결혼하면 진짜 묘할거같긴해요",
          "original_form": "근데 친구가 결혼하면 진짜 묘할거같긴해요",
          "speaker_id": "1",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100009575.1.25",
          "form": "부럽다거나 그런건 아닌데",
          "original_form": "부럽다거나 그런건 아닌데",
          "speaker_id": "2",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100009575.1.26",
          "form": "아.. 내 친구가 이렇게 떠나는구나",
          "original_form": "아.. 내 친구가 이렇게 떠나는구나",
          "speaker_id": "2",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100009575.1.27",
          "form": "결혼식 가서도 되게 싱숭생숭해요",
          "original_form": "결혼식 가서도 되게 싱숭생숭해요",
          "speaker_id": "2",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100009575.1.28",
          "form": "그럴거같아요.ㅡ",
          "original_form": "그럴거같아요.ㅡ",
          "speaker_id": "1",
          "time": "20210823 01:03"
        },
        {
          "id": "MDRW2100009575.1.29",
          "form": "아무래도 예전보다는 만날 일이 줄어들 테니까요",
          "original_form": "아무래도 예전보다는 만날 일이 줄어들 테니까요",
          "speaker_id": "1",
          "time": "20210823 01:03"
        },
        {
          "id": "MDRW2100009575.1.30",
          "form": "그것도 그렇고",
          "original_form": "그것도 그렇고",
          "speaker_id": "2",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009575.1.31",
          "form": "이제는 대화의 주제도 바뀌면서 공통사가 점점 줄어들더라구요",
          "original_form": "이제는 대화의 주제도 바뀌면서 공통사가 점점 줄어들더라구요",
          "speaker_id": "2",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009575.1.32",
          "form": "아아ㅜ..그렇겠네요.ㅡ",
          "original_form": "아아ㅜ..그렇겠네요.ㅡ",
          "speaker_id": "1",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009575.1.33",
          "form": "결혼한 친구들은 주로 남편이나 아이 이야기를 하시나요?",
          "original_form": "결혼한 친구들은 주로 남편이나 아이 이야기를 하시나요?",
          "speaker_id": "1",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009575.1.34",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "2",
          "time": "20210823 01:06"
        }
      ]
    }
