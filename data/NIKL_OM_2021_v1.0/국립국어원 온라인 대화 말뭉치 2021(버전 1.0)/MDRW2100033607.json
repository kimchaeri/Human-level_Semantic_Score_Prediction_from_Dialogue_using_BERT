      "utterance": [
        {
          "id": "MDRW2100033607.1.1",
          "form": "영화 보는 거 좋아하세요?",
          "original_form": "영화 보는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033607.1.2",
          "form": "시간 날 때 종종 보는 편이에요",
          "original_form": "시간 날 때 종종 보는 편이에요",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033607.1.3",
          "form": "최근에 본 영화 있으신가요 쓰신 가요 아 혹시 모가디슈 보신다고 하셨나요 저한테",
          "original_form": "최근에 본 영화 있으신가요 쓰신 가요 아 혹시 모가디슈 보신다고 하셨나요 저한테",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033607.1.4",
          "form": "주로 어떤 장르 좋아하세요?",
          "original_form": "주로 어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033607.1.5",
          "form": "저 공포 영화 빼고는 다 잘 보는 편이에요",
          "original_form": "저 공포 영화 빼고는 다 잘 보는 편이에요",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033607.1.6",
          "form": "오 저도요 ~_~",
          "original_form": "오 저도요 ~_~",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033607.1.7",
          "form": "전 좀비물도 좋아해요",
          "original_form": "전 좀비물도 좋아해요",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033607.1.8",
          "form": "좀비 영화 어떤 거 좋아하세요",
          "original_form": "좀비 영화 어떤 거 좋아하세요",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033607.1.9",
          "form": "저 그냥 다 좋아해요",
          "original_form": "저 그냥 다 좋아해요",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033607.1.10",
          "form": "재난영화도 좋아해요",
          "original_form": "재난영화도 좋아해요",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033607.1.11",
          "form": "최근에 본 영화있나요",
          "original_form": "최근에 본 영화있나요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033607.1.12",
          "form": "최근에는 없어요 ㅠㅠㅠㅠㅠ",
          "original_form": "최근에는 없어요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033607.1.13",
          "form": "보신 영화 있으세요?",
          "original_form": "보신 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033607.1.14",
          "form": "최근에 정직한 후보 그거 봤어요",
          "original_form": "최근에 정직한 후보 그거 봤어요",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033607.1.15",
          "form": "오 누구나오나요?",
          "original_form": "오 누구나오나요?",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033607.1.16",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033607.1.17",
          "form": "라미란씨 나와요 꽤 재밌었어요",
          "original_form": "라미란씨 나와요 꽤 재밌었어요",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033607.1.18",
          "form": "라미란씨 나오면 재밌죠오",
          "original_form": "라미란씨 나오면 재밌죠오",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100033607.1.19",
          "form": "귱금해요",
          "original_form": "귱금해요",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100033607.1.20",
          "form": "맞아요 연기 너무 잘하세요",
          "original_form": "맞아요 연기 너무 잘하세요",
