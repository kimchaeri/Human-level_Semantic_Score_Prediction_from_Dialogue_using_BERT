      "utterance": [
        {
          "id": "MDRW2100042721.1.1",
          "form": "그래도 6시간은 잔 것 같아요.",
          "original_form": "그래도 6시간은 잔 것 같아요.",
          "speaker_id": "2",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100042721.1.2",
          "form": "하긴 백신맞으면 돼지고기 먹으라던데",
          "original_form": "하긴 백신맞으면 돼지고기 먹으라던데",
          "speaker_id": "1",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100042721.1.3",
          "form": "잘하셧네요",
          "original_form": "잘하셧네요",
          "speaker_id": "1",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100042721.1.4",
          "form": "아프진 않으세요?",
          "original_form": "아프진 않으세요?",
          "speaker_id": "1",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100042721.1.5",
          "form": "오오 ?!",
          "original_form": "오오 ?!",
          "speaker_id": "2",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100042721.1.6",
          "form": "몸이 알아서 찾는 매직 ㅋㅋ",
          "original_form": "몸이 알아서 찾는 매직 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100042721.1.7",
          "form": "저도 이유는 모르겟는데 아는 변호사가 그러더라구요",
          "original_form": "저도 이유는 모르겟는데 아는 변호사가 그러더라구요",
          "speaker_id": "1",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100042721.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 매직 그자체",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 매직 그자체",
          "speaker_id": "1",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100042721.1.9",
          "form": "맞고 바로는 아무렇지 않았는데",
          "original_form": "맞고 바로는 아무렇지 않았는데",
          "speaker_id": "2",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100042721.1.10",
          "form": "한 숨 자고 나니까 부어있더라고요",
          "original_form": "한 숨 자고 나니까 부어있더라고요",
          "speaker_id": "2",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100042721.1.11",
          "form": "그래서 냉찜질 하고 타이레놀 하나 먹었더니",
          "original_form": "그래서 냉찜질 하고 타이레놀 하나 먹었더니",
          "speaker_id": "2",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100042721.1.12",
          "form": "헐 요리하실 수 잇으시겟어요? ㄷㄷ",
          "original_form": "헐 요리하실 수 잇으시겟어요? ㄷㄷ",
          "speaker_id": "1",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100042721.1.13",
          "form": "지금은 맞은 자리 근육통 말고는 괜찮은 것 같아요.",
          "original_form": "지금은 맞은 자리 근육통 말고는 괜찮은 것 같아요.",
          "speaker_id": "2",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100042721.1.14",
          "form": "으음 잔치국수도 샘표 인스턴트라 괜찮아요 ㅋㅋ",
          "original_form": "으음 잔치국수도 샘표 인스턴트라 괜찮아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100042721.1.15",
          "form": "타이레놀 필수템이군요,,",
          "original_form": "타이레놀 필수템이군요,,",
          "speaker_id": "1",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100042721.1.16",
          "form": "아 인스턴트 ㅋㅋㅋㅋㅋ",
          "original_form": "아 인스턴트 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100042721.1.17",
          "form": "이럴때 먹으라고 인스턴트가 잇는거같아요",
          "original_form": "이럴때 먹으라고 인스턴트가 잇는거같아요",
          "speaker_id": "1",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100042721.1.18",
          "form": "인스턴트 잔치국수 몹시 거부감이 있었는데",
          "original_form": "인스턴트 잔치국수 몹시 거부감이 있었는데",
          "speaker_id": "2",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042721.1.19",
          "form": "엄마가 갑자기 맛있다셔서 먹어보니 정말 괜찮더라고요",
          "original_form": "엄마가 갑자기 맛있다셔서 먹어보니 정말 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042721.1.20",
          "form": "오 그르게요 뭔가 공장맛 날거같고",
          "original_form": "오 그르게요 뭔가 공장맛 날거같고",
          "speaker_id": "1",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042721.1.21",
          "form": "오 샘표꺼 괜찮군요..",
          "original_form": "오 샘표꺼 괜찮군요..",
          "speaker_id": "1",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042721.1.22",
          "form": "기술이 많이 좋아진 느낌이예요.",
          "original_form": "기술이 많이 좋아진 느낌이예요.",
          "speaker_id": "2",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042721.1.23",
          "form": "요새 간편식 잘나온다하더라구요",
          "original_form": "요새 간편식 잘나온다하더라구요",
          "speaker_id": "1",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042721.1.24",
          "form": "아침에 먹기 딱이네요 잔치국수 호로록",
          "original_form": "아침에 먹기 딱이네요 잔치국수 호로록",
          "speaker_id": "1",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100042721.1.25",
          "form": "라면처럼 끓이라고 써 있는데 저는 글루텐 없는 맑은 국물이 좋아서 면을 따로 삶아 헹구고",
          "original_form": "라면처럼 끓이라고 써 있는데 저는 글루텐 없는 맑은 국물이 좋아서 면을 따로 삶아 헹구고",
          "speaker_id": "2",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100042721.1.26",
          "form": "스프는 그릇에 넣고 포트로 끓인 물을 부어요",
          "original_form": "스프는 그릇에 넣고 포트로 끓인 물을 부어요",
          "speaker_id": "2",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100042721.1.27",
          "form": "온천달걀만 만들면 딱입니다 ㅋㅋ",
          "original_form": "온천달걀만 만들면 딱입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042721.1.28",
          "form": "아 그렇게 면 한번 삶으면",
          "original_form": "아 그렇게 면 한번 삶으면",
          "speaker_id": "1",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042721.1.29",
          "form": "더 건강하게 먹을수잇다하던데 전 귀찮아서 안해봣어요",
          "original_form": "더 건강하게 먹을수잇다하던데 전 귀찮아서 안해봣어요",
          "speaker_id": "1",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042721.1.30",
          "form": "그러면 더 맑게 먹겟네요",
          "original_form": "그러면 더 맑게 먹겟네요",
          "speaker_id": "1",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042721.1.31",
          "form": "라면처럼 원터치(?)로 끓여 먹게 나온 게 기술이라는데",
          "original_form": "라면처럼 원터치(?)로 끓여 먹게 나온 게 기술이라는데",
          "speaker_id": "2",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042721.1.32",
          "form": "저의 극성으로 ㅋㅋ 맑은면 완성 ㅋㅋ",
          "original_form": "저의 극성으로 ㅋㅋ 맑은면 완성 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042721.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 인스턴트도 요리를",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 인스턴트도 요리를",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100042721.1.34",
          "form": "간편식에 정성을 더하다..",
          "original_form": "간편식에 정성을 더하다..",
          "speaker_id": "1",
          "time": "20210907 07:57"
        }
      ]
