      "utterance": [
        {
          "id": "MDRW2100017056.1.1",
          "form": "이번 올림픽 아주 좋았어요",
          "original_form": "이번 올림픽 아주 좋았어요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017056.1.2",
          "form": "맞아요ㅋㅋ 제일 좋았던 경기가 뭐에요",
          "original_form": "맞아요ㅋㅋ 제일 좋았던 경기가 뭐에요",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017056.1.3",
          "form": "전 한일전하고 양궁이요 !!!",
          "original_form": "전 한일전하고 양궁이요 !!!",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017056.1.4",
          "form": "양궁.. 진짜 짜릿 흑",
          "original_form": "양궁.. 진짜 짜릿 흑",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017056.1.5",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017056.1.6",
          "form": "안산선수 개인전 진짜 못보겠더라구요",
          "original_form": "안산선수 개인전 진짜 못보겠더라구요",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017056.1.7",
          "form": "넘 떨려서ㅋㅋㅋㅋ",
          "original_form": "넘 떨려서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017056.1.8",
          "form": "ㅋㅋㅋㅋㅋ 진짜.. 엎치락뒤치락이여서...",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜.. 엎치락뒤치락이여서...",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017056.1.9",
          "form": "전 바로 못챙겨봤어요",
          "original_form": "전 바로 못챙겨봤어요",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017056.1.10",
          "form": "결과 나온 뒤 봤죠ㅋㅋㅋㅋㅋ",
          "original_form": "결과 나온 뒤 봤죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017056.1.11",
          "form": "ㅋㅋㅋㅋ손 모으면서 보고 막...",
          "original_form": "ㅋㅋㅋㅋ손 모으면서 보고 막...",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017056.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 햄버거 먹으면서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 햄버거 먹으면서",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017056.1.13",
          "form": "미소가 지어졌어요ㅎ",
          "original_form": "미소가 지어졌어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017056.1.14",
          "form": "제덕 선수 응원하는거 왠지 따라하게 되요ㅋㅋ",
          "original_form": "제덕 선수 응원하는거 왠지 따라하게 되요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017056.1.15",
          "form": "ㅋㅋㅋㅋ 퐈이팅... 이거요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 퐈이팅... 이거요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017056.1.16",
          "form": "네ㅋㅋㅋ 외국선수들도 신기하게 보던데요",
