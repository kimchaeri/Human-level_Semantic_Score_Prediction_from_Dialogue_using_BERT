      "utterance": [
        {
          "id": "MDRW2100041804.1.1",
          "form": "샤브샤브에 먹기좋죠",
          "original_form": "샤브샤브에 먹기좋죠",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100041804.1.2",
          "form": "아 먹고싶네요",
          "original_form": "아 먹고싶네요",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100041804.1.3",
          "form": "맞아여!!",
          "original_form": "맞아여!!",
          "speaker_id": "1",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041804.1.4",
          "form": "아 샤브샤브 먹은지 오래됐어요",
          "original_form": "아 샤브샤브 먹은지 오래됐어요",
          "speaker_id": "1",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041804.1.5",
          "form": "저도요 이런날에 먹으면 좋은데",
          "original_form": "저도요 이런날에 먹으면 좋은데",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041804.1.6",
          "form": "먹고 싶네요 저 버섯도 사놓은거 있는데 그거 먹어봐야되거든요",
          "original_form": "먹고 싶네요 저 버섯도 사놓은거 있는데 그거 먹어봐야되거든요",
          "speaker_id": "1",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041804.1.7",
          "form": "어떤버섯이요",
          "original_form": "어떤버섯이요",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041804.1.8",
          "form": "표고랑 송이랑 합친 종이래요",
          "original_form": "표고랑 송이랑 합친 종이래요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041804.1.9",
          "form": "이름은 까먹었어요",
          "original_form": "이름은 까먹었어요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041804.1.10",
          "form": "버섯도 진짜다양해요",
          "original_form": "버섯도 진짜다양해요",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041804.1.11",
          "form": "저 새로 보는 버섯이라 사봤어요",
          "original_form": "저 새로 보는 버섯이라 사봤어요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041804.1.12",
          "form": "그러니까여",
          "original_form": "그러니까여",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041804.1.13",
          "form": "버섯은 계란에  부쳐도 맛나져",
          "original_form": "버섯은 계란에  부쳐도 맛나져",
          "speaker_id": "2",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041804.1.14",
          "form": "네 진짜 맛있겠다",
          "original_form": "네 진짜 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041804.1.15",
          "form": "저희 동네는 지금 비가 조금씩 내리거든요",
          "original_form": "저희 동네는 지금 비가 조금씩 내리거든요",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041804.1.16",
          "form": "딱 오늘 전먹으면 좋을거 같네요!!!",
          "original_form": "딱 오늘 전먹으면 좋을거 같네요!!!",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100041804.1.17",
          "form": "그렇네요 칼국수도 좋아요",
          "original_form": "그렇네요 칼국수도 좋아요",
          "speaker_id": "2",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100041804.1.18",
          "form": "저 오늘 간식 정해졌어요 달다님 덕분에 ㅋㅋㅋㅋ",
          "original_form": "저 오늘 간식 정해졌어요 달다님 덕분에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100041804.1.19",
          "form": "햄버거 먹을까 했는데 집에 있는 버섯을 부쳐먹겠어요",
          "original_form": "햄버거 먹을까 했는데 집에 있는 버섯을 부쳐먹겠어요",
          "speaker_id": "1",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100041804.1.20",
          "form": "ㅋㅋㅋㅋㅋ 감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋ 감사합니다",
          "speaker_id": "1",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100041804.1.21",
          "form": "ㅋㅋㅋ전 김치부침개라도 해야하나요",
          "original_form": "ㅋㅋㅋ전 김치부침개라도 해야하나요",
          "speaker_id": "2",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100041804.1.22",
          "form": "네 김치부침개 추천해드립니닼ㅋㅋㅋ",
