      "utterance": [
        {
          "id": "MDRW2100014076.1.1",
          "form": "제일 재밌게 봤던 영화 있으신가요?",
          "original_form": "제일 재밌게 봤던 영화 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014076.1.2",
          "form": "마이너리티리포트요",
          "original_form": "마이너리티리포트요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014076.1.3",
          "form": "그리고 또... 뭐있을까 일단 공포영화를 못봐서 그거는 없고요",
          "original_form": "그리고 또... 뭐있을까 일단 공포영화를 못봐서 그거는 없고요",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014076.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014076.1.5",
          "form": "저는 미드나잇인 파리를 제일 좋아하긴했는데",
          "original_form": "저는 미드나잇인 파리를 제일 좋아하긴했는데",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014076.1.6",
          "form": "내용은 그냥 그런데  노래랑 배경이 좋아요",
          "original_form": "내용은 그냥 그런데  노래랑 배경이 좋아요",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014076.1.7",
          "form": "저도 공포영화를 잘 못봐서",
          "original_form": "저도 공포영화를 잘 못봐서",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014076.1.8",
          "form": "미드나잇인파리 한번봐바야겠네요 볼 영화중에 있었어요",
          "original_form": "미드나잇인파리 한번봐바야겠네요 볼 영화중에 있었어요",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014076.1.9",
          "form": "공포영화는 찍는사람도 대단한거같아요",
          "original_form": "공포영화는 찍는사람도 대단한거같아요",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014076.1.10",
          "form": "어우 너무 무서워요 진짜로",
          "original_form": "어우 너무 무서워요 진짜로",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014076.1.11",
          "form": "심장이 약한거 같아요 저는ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 도저히 버틸수가 없어요 영화관에서 소리질러서 창피한적도 있고",
          "original_form": "심장이 약한거 같아요 저는ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 도저히 버틸수가 없어요 영화관에서 소리질러서 창피한적도 있고",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014076.1.12",
          "form": "마지막으로 본 공포영화는 컨저링...",
          "original_form": "마지막으로 본 공포영화는 컨저링...",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014076.1.13",
          "form": "앗 컨저링을 봤을정도면 심장이 강철로되신거아닌가요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 컨저링을 봤을정도면 심장이 강철로되신거아닌가요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014076.1.14",
          "form": "그거는 애인이 보자고 해서... 끌려간거였어요",
          "original_form": "그거는 애인이 보자고 해서... 끌려간거였어요",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014076.1.15",
          "form": "정말로 보고싶지 않았는데!!",
          "original_form": "정말로 보고싶지 않았는데!!",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014076.1.16",
          "form": "근데 웃긴게 뭔지 아세요?",
          "original_form": "근데 웃긴게 뭔지 아세요?",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014076.1.17",
          "form": "애인이 저보다 더 크게 놀라서 외마디 비명 질렀어요",
          "original_form": "애인이 저보다 더 크게 놀라서 외마디 비명 질렀어요",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014076.1.18",
          "form": "두고두고 놀림감...",
          "original_form": "두고두고 놀림감...",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014076.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014076.1.20",
          "form": "그냥 진짜 엌!",
          "original_form": "그냥 진짜 엌!",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014076.1.21",
          "form": "해가지고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "해가지고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014076.1.22",
          "form": "아무튼 그래서 그 이후로는 일절 안봤답니다 공포영화는",
          "original_form": "아무튼 그래서 그 이후로는 일절 안봤답니다 공포영화는",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014076.1.23",
          "form": "흑심으로데려갔는데  더 놀라셨네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "흑심으로데려갔는데  더 놀라셨네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014076.1.24",
          "form": "아 ㅋㅋㅋㅋ 아녜요 흑심같은건 아니었을거예요 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 아녜요 흑심같은건 아니었을거예요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014076.1.25",
          "form": "그냥 가족같은 친구였어서요",
          "original_form": "그냥 가족같은 친구였어서요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014076.1.26",
          "form": "그리고 또 좋아하는 영화는",
          "original_form": "그리고 또 좋아하는 영화는",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014076.1.27",
          "form": "이터널선샤인 트루먼쇼 라라랜드",
          "original_form": "이터널선샤인 트루먼쇼 라라랜드",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014076.1.28",
          "form": "이렁거 좋아해요 재밌었던건 마이너리티리포트고",
          "original_form": "이렁거 좋아해요 재밌었던건 마이너리티리포트고",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014076.1.29",
          "form": "라라랜드 저도 재밌게봤어요!",
          "original_form": "라라랜드 저도 재밌게봤어요!",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014076.1.30",
          "form": "마이너리티리포트도 꼭봐야겠네요",
          "original_form": "마이너리티리포트도 꼭봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.31",
          "form": "라라랜드~",
          "original_form": "라라랜드~",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.32",
          "form": "저 라라랜드 좋아하는 사람 만나서 라라랜드 얘기하고 싶었어요....",
          "original_form": "저 라라랜드 좋아하는 사람 만나서 라라랜드 얘기하고 싶었어요....",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.33",
          "form": "라라랜드 마지막장면이 너무 좋아서 영화관에서 두번봤어요ㅋㅋㅋㅋㅋ",
          "original_form": "라라랜드 마지막장면이 너무 좋아서 영화관에서 두번봤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.34",
          "form": "제 주변엔 이상하게 없어서",
          "original_form": "제 주변엔 이상하게 없어서",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.35",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.36",
          "form": "맞아요 좋았죠",
          "original_form": "맞아요 좋았죠",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.37",
          "form": "저 아는분은 그거 중간에 보다가 주무신분도 있어요",
          "original_form": "저 아는분은 그거 중간에 보다가 주무신분도 있어요",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014076.1.38",
          "form": "음악도 좋았고...",
          "original_form": "음악도 좋았고...",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014076.1.39",
          "form": "생각보다 취향타더라구요",
          "original_form": "생각보다 취향타더라구요",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014076.1.40",
          "form": "전 너무 좋았어요",
          "original_form": "전 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014076.1.41",
          "form": "네 진짜요 ㅋㅋㅋ",
          "original_form": "네 진짜요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014076.1.42",
          "form": "음악 너무좋죠",
          "original_form": "음악 너무좋죠",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014076.1.43",
          "form": "처음에 도로에서 춤추는 장면봤을때부터",
          "original_form": "처음에 도로에서 춤추는 장면봤을때부터",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014076.1.44",
          "form": "처음하고 끝이 진짜 좋아요",
          "original_form": "처음하고 끝이 진짜 좋아요",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014076.1.45",
          "form": "아 맞아요 ㅠㅠ 처음부터 그냥 매료돼서",
          "original_form": "아 맞아요 ㅠㅠ 처음부터 그냥 매료돼서",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014076.1.46",
          "form": "끝까지 재밌게 봤어요 벅차고 뭉클하고 마음아프고 후련하고",
          "original_form": "끝까지 재밌게 봤어요 벅차고 뭉클하고 마음아프고 후련하고",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014076.1.47",
          "form": "그런 복합적인 감정을 느낄 수 있는 영화였어요",
          "original_form": "그런 복합적인 감정을 느낄 수 있는 영화였어요",
