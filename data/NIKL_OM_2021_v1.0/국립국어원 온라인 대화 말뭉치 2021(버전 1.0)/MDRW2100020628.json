      "utterance": [
        {
          "id": "MDRW2100020628.1.1",
          "form": "백신맞고 어떠셨어오ㅡ?",
          "original_form": "백신맞고 어떠셨어오ㅡ?",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020628.1.2",
          "form": "저는 한 이틀의 근육통 정도?",
          "original_form": "저는 한 이틀의 근육통 정도?",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020628.1.3",
          "form": "2차가 아프다해서 걱정이에요",
          "original_form": "2차가 아프다해서 걱정이에요",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020628.1.4",
          "form": "언제 2차 맞으세요?",
          "original_form": "언제 2차 맞으세요?",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020628.1.5",
          "form": "다음주 토요일에 맞으러 가요 ㅎㅎ",
          "original_form": "다음주 토요일에 맞으러 가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020628.1.6",
          "form": "타이레놀 딱 먹고 주말 내내 자려구요",
          "original_form": "타이레놀 딱 먹고 주말 내내 자려구요",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020628.1.7",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020628.1.8",
          "form": "얼마 안남았네요?",
          "original_form": "얼마 안남았네요?",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020628.1.9",
          "form": "그때까지 계속 대화하면서 후기 들려주세요",
          "original_form": "그때까지 계속 대화하면서 후기 들려주세요",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020628.1.10",
          "form": "넼ㅋㅋㅋ 원하시면 후기 들려드릴게욬ㅋㅋㅋㅌ",
          "original_form": "넼ㅋㅋㅋ 원하시면 후기 들려드릴게욬ㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020628.1.11",
          "form": "원합니다",
          "original_form": "원합니다",
          "speaker_id": "1",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020628.1.12",
          "form": "저도 맞아야하니",
          "original_form": "저도 맞아야하니",
          "speaker_id": "1",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020628.1.13",
          "form": "가족들이 az 얀센 화이자 모더나 다양하게 맞을뻔 했는데",
          "original_form": "가족들이 az 얀센 화이자 모더나 다양하게 맞을뻔 했는데",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020628.1.14",
          "form": "모더나가 모자라서 화이자로 맞으셨더라구요",
          "original_form": "모더나가 모자라서 화이자로 맞으셨더라구요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020628.1.15",
          "form": "회사 상무님이 얀센 맞았는데",
          "original_form": "회사 상무님이 얀센 맞았는데",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020628.1.16",
          "form": "아프다고 저한테 출근길 카풀신청했다는....",
          "original_form": "아프다고 저한테 출근길 카풀신청했다는....",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020628.1.17",
          "form": "오잉ㅋㅋㅋㅋㅋ",
          "original_form": "오잉ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020628.1.18",
          "form": "얀센 제동생은 그냥 무탈하게 지나갔긴 했는데 아픈 사람도 있었나보네요",
          "original_form": "얀센 제동생은 그냥 무탈하게 지나갔긴 했는데 아픈 사람도 있었나보네요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020628.1.19",
          "form": "새벽6시에 전화해서",
          "original_form": "새벽6시에 전화해서",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020628.1.20",
          "form": "저한테 자기 데릴러 오라는거에요",
          "original_form": "저한테 자기 데릴러 오라는거에요",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020628.1.21",
          "form": "얀센이 그렇게 아픈가 했는데",
          "original_form": "얀센이 그렇게 아픈가 했는데",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020628.1.22",
          "form": "다르군요 ㅋㅋㅋ",
          "original_form": "다르군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020628.1.23",
          "form": "헐ㅋㅋㅋ 썸이세요??",
          "original_form": "헐ㅋㅋㅋ 썸이세요??",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020628.1.24",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020628.1.25",
          "form": "너무짜증",
          "original_form": "너무짜증",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020628.1.26",
          "form": "ㅋㅋㅋㅋ태워드렸어요?",
          "original_form": "ㅋㅋㅋㅋ태워드렸어요?",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020628.1.27",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020628.1.28",
          "form": "어떡해요 그럼 ㅜㅜ",
          "original_form": "어떡해요 그럼 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020628.1.29",
          "form": "시른데? 할수도 없고",
          "original_form": "시른데? 할수도 없고",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020628.1.30",
          "form": "아 ㅠㅠ 때아닌 갑질이네요ㅋㅋㅋ",
          "original_form": "아 ㅠㅠ 때아닌 갑질이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020628.1.31",
          "form": "그만둔대요 ㅋㅋㅋㅋㅋ",
          "original_form": "그만둔대요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020628.1.32",
          "form": "그게 6월초인데",
          "original_form": "그게 6월초인데",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020628.1.33",
          "form": "아 그런분들은 맨날 말로만 그만둔다 하고 사라지질 않더라구요ㅋㅋㅋㅋ",
          "original_form": "아 그런분들은 맨날 말로만 그만둔다 하고 사라지질 않더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020628.1.34",
          "form": "입맛살아가지고",
          "original_form": "입맛살아가지고",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020628.1.35",
          "form": "얀센맞은 동생은 그냥 멀쩡했나요?",
          "original_form": "얀센맞은 동생은 그냥 멀쩡했나요?",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020628.1.36",
          "form": "열같은거 없이?",
          "original_form": "열같은거 없이?",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020628.1.37",
          "form": "네 뭐 아프다는 말을 안하던데요",
          "original_form": "네 뭐 아프다는 말을 안하던데요",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020628.1.38",
          "form": "원래 아프다는 말 잘 안하긴 하는 사람이라 아팠을수도 있어요 ㅋㅋㅋ",
          "original_form": "원래 아프다는 말 잘 안하긴 하는 사람이라 아팠을수도 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020628.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 동생이랑 친해요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 동생이랑 친해요?",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020628.1.40",
          "form": "쎈척하는 동생?ㅋ",
          "original_form": "쎈척하는 동생?ㅋ",
