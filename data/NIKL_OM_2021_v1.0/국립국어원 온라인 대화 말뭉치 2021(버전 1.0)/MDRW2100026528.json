      "utterance": [
        {
          "id": "MDRW2100026528.1.1",
          "form": "다이어트 해?",
          "original_form": "다이어트 해?",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026528.1.2",
          "form": "영상이라 음 요즘 보는거 있어?",
          "original_form": "영상이라 음 요즘 보는거 있어?",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026528.1.3",
          "form": "음! 나는 요즘 슬의생 보고있어!",
          "original_form": "음! 나는 요즘 슬의생 보고있어!",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026528.1.4",
          "form": "오 어때? 재밌어? 그거 2인가 나온건가? 1은 끝났지?",
          "original_form": "오 어때? 재밌어? 그거 2인가 나온건가? 1은 끝났지?",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026528.1.5",
          "form": "응 2야!",
          "original_form": "응 2야!",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026528.1.6",
          "form": "1보다는 재미가 없어...",
          "original_form": "1보다는 재미가 없어...",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026528.1.7",
          "form": "1은 재밌게 봤는데 2는 재미 없드라...",
          "original_form": "1은 재밌게 봤는데 2는 재미 없드라...",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026528.1.8",
          "form": "넷플이나 유튜브는 자주봐??",
          "original_form": "넷플이나 유튜브는 자주봐??",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026528.1.9",
          "form": "나 유튜브는 자주 봐! 넷플말고 나 라프텔 구독해",
          "original_form": "나 유튜브는 자주 봐! 넷플말고 나 라프텔 구독해",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026528.1.10",
          "form": "오! 애니 좋아하는구나~",
          "original_form": "오! 애니 좋아하는구나~",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026528.1.11",
          "form": "어떤거 좋아해??",
          "original_form": "어떤거 좋아해??",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026528.1.12",
          "form": "나 애니는 스포츠 애니도 좋아하고 힐링물도 좋아해",
          "original_form": "나 애니는 스포츠 애니도 좋아하고 힐링물도 좋아해",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026528.1.13",
          "form": "너는 애니 좋아해?",
          "original_form": "너는 애니 좋아해?",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026528.1.14",
          "form": "나 이누야샤 되게 좋아해!",
          "original_form": "나 이누야샤 되게 좋아해!",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026528.1.15",
          "form": "유튜브로 심심할때 가끔 챙겨봐!",
          "original_form": "유튜브로 심심할때 가끔 챙겨봐!",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026528.1.16",
          "form": "이누야샤 너무 좋지ㅠㅠ 나 이누야샤에서 셋쇼마루 팬이야",
          "original_form": "이누야샤 너무 좋지ㅠㅠ 나 이누야샤에서 셋쇼마루 팬이야",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026528.1.17",
          "form": "나도! 이누야샤보다 셋쇼마루에 더 몰입해서 본거같아",
          "original_form": "나도! 이누야샤보다 셋쇼마루에 더 몰입해서 본거같아",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026528.1.18",
          "form": "이누야샤는 노래도 너무 좋은거같아ㅎㅎ",
          "original_form": "이누야샤는 노래도 너무 좋은거같아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026528.1.19",
          "form": "맞아ㅠㅠ 나 이누야샤 노래 노래방에서 다 부른당ㅋㅋ",
          "original_form": "맞아ㅠㅠ 나 이누야샤 노래 노래방에서 다 부른당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026528.1.20",
          "form": "아 노래방도 급 가고 싶네 ..",
          "original_form": "아 노래방도 급 가고 싶네 ..",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026528.1.21",
          "form": "ㅋㅋㅋㅋㅋ 나도 그립 진짜 자주불러!",
          "original_form": "ㅋㅋㅋㅋㅋ 나도 그립 진짜 자주불러!",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026528.1.22",
          "form": "코로나때문에 노래방 안간지 엄청 오래됐다",
          "original_form": "코로나때문에 노래방 안간지 엄청 오래됐다",
          "speaker_id": "1",
          "time": "20210901 23:09"
        }
      ]
    }
