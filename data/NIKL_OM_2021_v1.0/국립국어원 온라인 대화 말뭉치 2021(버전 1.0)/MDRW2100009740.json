      "utterance": [
        {
          "id": "MDRW2100009740.1.1",
          "form": "가훈이 있나요",
          "original_form": "가훈이 있나요",
          "speaker_id": "2",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009740.1.2",
          "form": "저희집은 가훈 없어서 가훈삼을만한 말을 찾고 있어요",
          "original_form": "저희집은 가훈 없어서 가훈삼을만한 말을 찾고 있어요",
          "speaker_id": "1",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100009740.1.3",
          "form": "혹시 가훈 있으세요?",
          "original_form": "혹시 가훈 있으세요?",
          "speaker_id": "1",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100009740.1.4",
          "form": "어 정직하게 살자 이거에요",
          "original_form": "어 정직하게 살자 이거에요",
          "speaker_id": "2",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100009740.1.5",
          "form": "약간 정직을 추구하셔서",
          "original_form": "약간 정직을 추구하셔서",
          "speaker_id": "2",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100009740.1.6",
          "form": "오 가풍이 정직이신가요",
          "original_form": "오 가풍이 정직이신가요",
          "speaker_id": "1",
          "time": "20210823 09:22"
        },
        {
          "id": "MDRW2100009740.1.7",
          "form": "그럼 어릴 때 거짓말하면 많이 혼내시고 그러셨나요",
          "original_form": "그럼 어릴 때 거짓말하면 많이 혼내시고 그러셨나요",
          "speaker_id": "1",
          "time": "20210823 09:22"
        },
        {
          "id": "MDRW2100009740.1.8",
          "form": "네 속이고 거짓말 하는걸 싫어하셨어요",
          "original_form": "네 속이고 거짓말 하는걸 싫어하셨어요",
          "speaker_id": "2",
          "time": "20210823 09:22"
        },
        {
          "id": "MDRW2100009740.1.9",
          "form": "근데 저는 굴하지않고 거짓말을 했죠",
          "original_form": "근데 저는 굴하지않고 거짓말을 했죠",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009740.1.10",
          "form": "그래서 더 혼났어요ㅎ",
          "original_form": "그래서 더 혼났어요ㅎ",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009740.1.11",
          "form": "ㅋㅋㅋㅋㅋ 교육이랑 반대로 자라는 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 교육이랑 반대로 자라는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009740.1.12",
          "form": "그런거 같아요ㅋㅋㅋ",
          "original_form": "그런거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009740.1.13",
          "form": "저 친구네 집도 어릴때 라면 못먹게 했는데 얘 어른되고 나서 라면 엄청먹더라구요",
          "original_form": "저 친구네 집도 어릴때 라면 못먹게 했는데 얘 어른되고 나서 라면 엄청먹더라구요",
          "speaker_id": "1",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009740.1.14",
          "form": "약간 하지말라면 더 하고 싶고",
          "original_form": "약간 하지말라면 더 하고 싶고",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009740.1.15",
          "form": "청개구리처럼ㅋㅋㅋ",
          "original_form": "청개구리처럼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:24"
        },
        {
          "id": "MDRW2100009740.1.16",
          "form": "그게 당장은 아니더라도 나중에 팍 터지더라구요",
          "original_form": "그게 당장은 아니더라도 나중에 팍 터지더라구요",
          "speaker_id": "2",
          "time": "20210823 09:24"
        },
        {
          "id": "MDRW2100009740.1.17",
          "form": "결국 가훈의 반대로..",
          "original_form": "결국 가훈의 반대로..",
          "speaker_id": "1",
          "time": "20210823 09:24"
        },
        {
          "id": "MDRW2100009740.1.18",
          "form": "ㅋㅋㅋㅋ 그냥 뭐든 적당한게 좋은 거 같아요",
          "original_form": "ㅋㅋㅋㅋ 그냥 뭐든 적당한게 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210823 09:24"
        },
        {
          "id": "MDRW2100009740.1.19",
          "form": "맞아요 어른되어서 팍 터지면 이제 막을 사람도 없구",
          "original_form": "맞아요 어른되어서 팍 터지면 이제 막을 사람도 없구",
          "speaker_id": "1",
          "time": "20210823 09:24"
        }
