      "utterance": [
        {
          "id": "MDRW2100026589.1.1",
          "form": "혹시 1인가구세요?",
          "original_form": "혹시 1인가구세요?",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026589.1.2",
          "form": "네,, 그래서 가사얘긴 잘 ㅋㅋ",
          "original_form": "네,, 그래서 가사얘긴 잘 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026589.1.3",
          "form": "왜요?? 1인 가구면",
          "original_form": "왜요?? 1인 가구면",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026589.1.4",
          "form": "가사할 사람이 본인 밖에 없어서",
          "original_form": "가사할 사람이 본인 밖에 없어서",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026589.1.5",
          "form": "할 얘기가 좀 있지 않나요?",
          "original_form": "할 얘기가 좀 있지 않나요?",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026589.1.6",
          "form": "아 ㅠ 이거 하다가 만난 분들은 가족 밥차려주는 얘기 이런거 하셔가지구",
          "original_form": "아 ㅠ 이거 하다가 만난 분들은 가족 밥차려주는 얘기 이런거 하셔가지구",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026589.1.7",
          "form": "좀 난감했어요 ㅋㅋ",
          "original_form": "좀 난감했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026589.1.8",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026589.1.9",
          "form": "재밌네요",
          "original_form": "재밌네요",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026589.1.10",
          "form": "빨래 자주 하는 편인가요",
          "original_form": "빨래 자주 하는 편인가요",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026589.1.11",
          "form": "3일에 한번 하는데 그냥 해서 건조대에 걸구",
          "original_form": "3일에 한번 하는데 그냥 해서 건조대에 걸구",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026589.1.12",
          "form": "안개고 거기서 대충 다시 스팀다리미 쬐서 입고나가요 ㅋㅋ",
          "original_form": "안개고 거기서 대충 다시 스팀다리미 쬐서 입고나가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026589.1.13",
          "form": "개꿀",
          "original_form": "개꿀",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026589.1.14",
          "form": "와 꿀이네요.",
          "original_form": "와 꿀이네요.",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026589.1.15",
          "form": "스팀다리미 가격대",
          "original_form": "스팀다리미 가격대",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026589.1.16",
          "form": "얼마정도 쓰세요",
          "original_form": "얼마정도 쓰세요",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026589.1.17",
          "form": "살까 했는데 ㅋㅋㅋ",
          "original_form": "살까 했는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026589.1.18",
          "form": "집이 좁으면 큰거 추천안해요",
          "original_form": "집이 좁으면 큰거 추천안해요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026589.1.19",
          "form": "큰거 쓰심 집이 사우나돼요",
          "original_form": "큰거 쓰심 집이 사우나돼요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026589.1.20",
          "form": "그렇다고 핸디사면 좀 스팀이 약해요",
          "original_form": "그렇다고 핸디사면 좀 스팀이 약해요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026589.1.21",
          "form": "헉 그렇군요.",
          "original_form": "헉 그렇군요.",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026589.1.22",
          "form": "밥은 사드세요 아님 요리하나요",
          "original_form": "밥은 사드세요 아님 요리하나요",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026589.1.23",
          "form": "전 요리하면 최대한 공정을 줄여요",
          "original_form": "전 요리하면 최대한 공정을 줄여요",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026589.1.24",
          "form": "밥에 냉동야채볶음에 에프에 구운 닭다리살 같이",
          "original_form": "밥에 냉동야채볶음에 에프에 구운 닭다리살 같이",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026589.1.25",
          "form": "오... 1인가구 프로 살림꾼 같네요",
          "original_form": "오... 1인가구 프로 살림꾼 같네요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026589.1.26",
          "form": "냉장고나 선반에 항시 쟁여두는 아이템이 잇나요?",
          "original_form": "냉장고나 선반에 항시 쟁여두는 아이템이 잇나요?",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026589.1.27",
          "form": "햇반이요 ?",
          "original_form": "햇반이요 ?",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026589.1.28",
          "form": "햇반은 쟁여둘만해요 맛있구,,",
          "original_form": "햇반은 쟁여둘만해요 맛있구,,",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026589.1.29",
          "form": "하지만 밥솥없애고 햇반만 두진 못하겠어요",
          "original_form": "하지만 밥솥없애고 햇반만 두진 못하겠어요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026589.1.30",
          "form": "3분카레나 컵라면은 많이 안드세요?",
          "original_form": "3분카레나 컵라면은 많이 안드세요?",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026589.1.31",
          "form": "전 3분 카레보다는 그냥 간단하게",
          "original_form": "전 3분 카레보다는 그냥 간단하게",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026589.1.32",
          "form": "참치캔에 소금 마요뿌려서 참치마요해서 먹어요",
          "original_form": "참치캔에 소금 마요뿌려서 참치마요해서 먹어요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026589.1.33",
          "form": "소금이여?? 그냥 참치도 짭짤하지 않아요?",
          "original_form": "소금이여?? 그냥 참치도 짭짤하지 않아요?",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026589.1.34",
          "form": "마요네즈를 넣으면 섞여서 소금 후추 좀 부리고 양파넣구 해야 맛있어요",
          "original_form": "마요네즈를 넣으면 섞여서 소금 후추 좀 부리고 양파넣구 해야 맛있어요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026589.1.35",
          "form": "마요만 걍 넣음 밥반찬하긴 좀 모자란 느낌? 제기준이에요",
          "original_form": "마요만 걍 넣음 밥반찬하긴 좀 모자란 느낌? 제기준이에요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        }
      ]
