      "utterance": [
        {
          "id": "MDRW2100015998.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210826 06:57"
        },
        {
          "id": "MDRW2100015998.1.2",
          "form": "네 좋아해요!ㅎㅎ",
          "original_form": "네 좋아해요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 06:58"
        },
        {
          "id": "MDRW2100015998.1.3",
          "form": "어디 가보셨나요?",
          "original_form": "어디 가보셨나요?",
          "speaker_id": "1",
          "time": "20210826 06:58"
        },
        {
          "id": "MDRW2100015998.1.4",
          "form": "저는 주로 국내여행 위주로 다녔고",
          "original_form": "저는 주로 국내여행 위주로 다녔고",
          "speaker_id": "2",
          "time": "20210826 06:59"
        },
        {
          "id": "MDRW2100015998.1.5",
          "form": "해외는 일본 오사카 가봤어요!",
          "original_form": "해외는 일본 오사카 가봤어요!",
          "speaker_id": "2",
          "time": "20210826 06:59"
        },
        {
          "id": "MDRW2100015998.1.6",
          "form": "방사능 걱정이에요",
          "original_form": "방사능 걱정이에요",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100015998.1.7",
          "form": "그렇긴 한데 그래도 난생 처음으로 한번 다녀와 봤네요ㅎㅎ",
          "original_form": "그렇긴 한데 그래도 난생 처음으로 한번 다녀와 봤네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100015998.1.8",
          "form": "가장 마지막 여행이 어디세요?",
          "original_form": "가장 마지막 여행이 어디세요?",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100015998.1.9",
          "form": "남해여",
          "original_form": "남해여",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015998.1.10",
          "form": "남해 좋죠 저도 바다 가고 싶네요",
          "original_form": "남해 좋죠 저도 바다 가고 싶네요",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015998.1.11",
          "form": "편안함은 물론내가 좋아하는것",
          "original_form": "편안함은 물론내가 좋아하는것",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015998.1.12",
          "form": "또 어디 가고 싶으세요?",
          "original_form": "또 어디 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015998.1.13",
          "form": "파리요",
          "original_form": "파리요",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015998.1.14",
          "form": "저도 파리 에펠탑 가보고 싶어요 맨날 사진으로만 봤는데 실제로는 어떨지 궁금해요",
          "original_form": "저도 파리 에펠탑 가보고 싶어요 맨날 사진으로만 봤는데 실제로는 어떨지 궁금해요",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015998.1.15",
          "form": "어마어마 할기듯",
          "original_form": "어마어마 할기듯",
          "speaker_id": "1",
          "time": "20210826 12:52"
        }
      ]
