      "utterance": [
        {
          "id": "MDRW2100026454.1.1",
          "form": "간식 좋아하시나요",
          "original_form": "간식 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026454.1.2",
          "form": "네  디저트류 너무 좋아해요",
          "original_form": "네  디저트류 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026454.1.3",
          "form": "마카롱 좋아하세요?",
          "original_form": "마카롱 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026454.1.4",
          "form": "넹넹 ..마카롱..하나당 칼로리 어마어마해서",
          "original_form": "넹넹 ..마카롱..하나당 칼로리 어마어마해서",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026454.1.5",
          "form": "한때는 한자리서 6개도 먹었는데..",
          "original_form": "한때는 한자리서 6개도 먹었는데..",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026454.1.6",
          "form": "이제..자제합ㄴㅣ다",
          "original_form": "이제..자제합ㄴㅣ다",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026454.1.7",
          "form": "맛있으면 0칼로리지만",
          "original_form": "맛있으면 0칼로리지만",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.9",
          "form": "저도 찌더라구여ㅋㅋ",
          "original_form": "저도 찌더라구여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.10",
          "form": "0칼로린데 살은 찌는",
          "original_form": "0칼로린데 살은 찌는",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.12",
          "form": "한개씩 가끔 먹으면 너무 맛나요",
          "original_form": "한개씩 가끔 먹으면 너무 맛나요",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.13",
          "form": "마자요 ㅠㅠㅠ",
          "original_form": "마자요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.14",
          "form": "황치즈 땡기네요",
          "original_form": "황치즈 땡기네요",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.15",
          "form": "크으크으크으ㅜㅜㅜ",
          "original_form": "크으크으크으ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.16",
          "form": "먹고싶다..",
          "original_form": "먹고싶다..",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026454.1.17",
          "form": "아메리랑 먹으면...",
          "original_form": "아메리랑 먹으면...",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026454.1.18",
          "form": "ㅋㅋㅋㅋㅋ ㅜㅜ 아메 궁합 넘좋죠",
          "original_form": "ㅋㅋㅋㅋㅋ ㅜㅜ 아메 궁합 넘좋죠",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026454.1.19",
