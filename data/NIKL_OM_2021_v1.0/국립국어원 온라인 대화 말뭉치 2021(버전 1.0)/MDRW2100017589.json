        {
          "id": "MDRW2100017589.1.1",
          "form": "있자나",
          "original_form": "있자나",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.2",
          "form": "너는 갈아먹는 배 맛 어때?",
          "original_form": "너는 갈아먹는 배 맛 어때?",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.3",
          "form": "좀 별로야",
          "original_form": "좀 별로야",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.4",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.5",
          "form": "뭔가 안에 그 알갱이가",
          "original_form": "뭔가 안에 그 알갱이가",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.6",
          "form": "그 알갱이 응응",
          "original_form": "그 알갱이 응응",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.7",
          "form": "코코팜 알갱이처럼",
          "original_form": "코코팜 알갱이처럼",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.8",
          "form": "옹 그거",
          "original_form": "옹 그거",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017589.1.9",
          "form": "맛있는게아니라",
          "original_form": "맛있는게아니라",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017589.1.10",
          "form": "오히려?",
          "original_form": "오히려?",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017589.1.11",
          "form": "뭔가 들어가면 안되는거같은게",
          "original_form": "뭔가 들어가면 안되는거같은게",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017589.1.12",
          "form": "들어가있는느낌이야",
          "original_form": "들어가있는느낌이야",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017589.1.13",
          "form": "ㅋㅌㅋㅌㅌ그게 뭐냐거",
          "original_form": "ㅋㅌㅋㅌㅌ그게 뭐냐거",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017589.1.14",
          "form": "배주스에 부유물이 떠다니는걸 마시는느낌",
          "original_form": "배주스에 부유물이 떠다니는걸 마시는느낌",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017589.1.15",
          "form": "목이 칼칼해져서??",
          "original_form": "목이 칼칼해져서??",
