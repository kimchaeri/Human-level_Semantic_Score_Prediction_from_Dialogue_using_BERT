      "utterance": [
        {
          "id": "MDRW2100009764.1.1",
          "form": "안녕하세요 요즘 운동하는거 있으세요?",
          "original_form": "안녕하세요 요즘 운동하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009764.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 09:39"
        },
        {
          "id": "MDRW2100009764.1.3",
          "form": "저는 집에서 홈트해요",
          "original_form": "저는 집에서 홈트해요",
          "speaker_id": "2",
          "time": "20210823 09:39"
        },
        {
          "id": "MDRW2100009764.1.4",
          "form": "지금 출근길 달리기도 하고있네요ㅋㅋ",
          "original_form": "지금 출근길 달리기도 하고있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:39"
        },
        {
          "id": "MDRW2100009764.1.5",
          "form": "저는 홈트하다가 잘안하게되서 헬스다녀요ㅎㅎ",
          "original_form": "저는 홈트하다가 잘안하게되서 헬스다녀요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:39"
        },
        {
          "id": "MDRW2100009764.1.6",
          "form": "아 헬스장 열리나요? 제친구가 다니다 말다 하더라구요",
          "original_form": "아 헬스장 열리나요? 제친구가 다니다 말다 하더라구요",
          "speaker_id": "2",
          "time": "20210823 09:40"
        },
        {
          "id": "MDRW2100009764.1.7",
          "form": "네 그래서 강제로 운동쉬는날이 많아요ㅠ",
          "original_form": "네 그래서 강제로 운동쉬는날이 많아요ㅠ",
          "speaker_id": "1",
          "time": "20210823 09:40"
        },
        {
          "id": "MDRW2100009764.1.8",
          "form": "홈트 주로 뭐하세요?",
          "original_form": "홈트 주로 뭐하세요?",
          "speaker_id": "1",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009764.1.9",
          "form": "ㅋㅋㅋ 그렇군요",
          "original_form": "ㅋㅋㅋ 그렇군요",
          "speaker_id": "2",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009764.1.10",
          "form": "보면서 따라해요",
          "original_form": "보면서 따라해요",
          "speaker_id": "2",
          "time": "20210823 09:42"
        },
        {
          "id": "MDRW2100009764.1.11",
          "form": "아 유투브채널 보시는구나ㅎㅎ",
          "original_form": "아 유투브채널 보시는구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:42"
        },
        {
          "id": "MDRW2100009764.1.12",
          "form": "저는 스쿼트하다가ㅋㅋ안하게 되서ㅠㅠ",
          "original_form": "저는 스쿼트하다가ㅋㅋ안하게 되서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 09:43"
        },
        {
          "id": "MDRW2100009764.1.13",
          "form": "ㅋㅋ저도ㅜ홈트는 의지가 있어야 할수있는거같아요",
          "original_form": "ㅋㅋ저도ㅜ홈트는 의지가 있어야 할수있는거같아요",
          "speaker_id": "2",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009764.1.14",
          "form": "제가 꾸준히 한다는건아니고요ㅋㅋㅋ",
          "original_form": "제가 꾸준히 한다는건아니고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009764.1.15",
          "form": "헬스장은 러닝머신속도 조절한다면서요?",
          "original_form": "헬스장은 러닝머신속도 조절한다면서요?",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009764.1.16",
          "form": "호흡때문에",
          "original_form": "호흡때문에",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009764.1.17",
          "form": "아 그런곳도 있구나 여긴 그렇지는 않은데 한자리씩 띄우고 해야되서 기다려야돼요ㅠㅠㅋㅋ",
          "original_form": "아 그런곳도 있구나 여긴 그렇지는 않은데 한자리씩 띄우고 해야되서 기다려야돼요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009764.1.18",
          "form": "아하 헬스장도 또 장소마다 방식이 다르군요",
          "original_form": "아하 헬스장도 또 장소마다 방식이 다르군요",
          "speaker_id": "2",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009764.1.19",
          "form": "지역마다도 다를거같아요ㅎㅎ 운동하러가서 기다리기ㅠㅠ",
          "original_form": "지역마다도 다를거같아요ㅎㅎ 운동하러가서 기다리기ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009764.1.20",
          "form": "족욕실이나 좁은실내는 또 폐쇄시켰어요",
          "original_form": "족욕실이나 좁은실내는 또 폐쇄시켰어요",
          "speaker_id": "2",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009764.1.21",
          "form": "샤워장도 폐쇄하고",
          "original_form": "샤워장도 폐쇄하고",
          "speaker_id": "2",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009764.1.22",
          "form": "네 맞아요..샤워도 집가서 하라고 하더라구요ㅠ",
          "original_form": "네 맞아요..샤워도 집가서 하라고 하더라구요ㅠ",
