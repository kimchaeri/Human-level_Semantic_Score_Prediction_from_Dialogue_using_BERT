        {
          "id": "MDRW2100040745.1.1",
          "form": "오늘 머드셧어요?",
          "original_form": "오늘 머드셧어요?",
          "speaker_id": "2",
          "time": "20210905 01:43"
        },
        {
          "id": "MDRW2100040745.1.2",
          "form": "불고기 먹었어요",
          "original_form": "불고기 먹었어요",
          "speaker_id": "1",
          "time": "20210905 01:45"
        },
        {
          "id": "MDRW2100040745.1.3",
          "form": "오 불고기 좋아하세요?",
          "original_form": "오 불고기 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100040745.1.4",
          "form": "아니요 집에 불고기가 있었어요",
          "original_form": "아니요 집에 불고기가 있었어요",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100040745.1.5",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210905 01:53"
        },
        {
          "id": "MDRW2100040745.1.6",
          "form": "저는 초밥 먹엇어요",
          "original_form": "저는 초밥 먹엇어요",
          "speaker_id": "2",
          "time": "20210905 01:53"
        },
        {
          "id": "MDRW2100040745.1.7",
          "form": "내가 아까 초밥 먹고 왔다고 말하셨어요",
          "original_form": "내가 아까 초밥 먹고 왔다고 말하셨어요",
          "speaker_id": "1",
          "time": "20210905 01:53"
        },
        {
          "id": "MDRW2100040745.1.8",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210905 01:55"
        },
        {
          "id": "MDRW2100040745.1.9",
          "form": "아까 연어초밥이야기",
          "original_form": "아까 연어초밥이야기",
          "speaker_id": "1",
          "time": "20210905 01:55"
        },
        {
          "id": "MDRW2100040745.1.10",
          "form": "하지않으셨나요",
          "original_form": "하지않으셨나요",
          "speaker_id": "1",
          "time": "20210905 01:55"
        },
        {
          "id": "MDRW2100040745.1.11",
          "form": "부대찌개 좋아하세요?",
          "original_form": "부대찌개 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 01:57"
        },
        {
          "id": "MDRW2100040745.1.12",
          "form": "좋아하진 않고 잇음 그냥 먹어여",
          "original_form": "좋아하진 않고 잇음 그냥 먹어여",
