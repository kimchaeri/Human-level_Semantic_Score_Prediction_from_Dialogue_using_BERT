      "utterance": [
        {
          "id": "MDRW2100040826.1.1",
          "form": "아침식사 하셨나요?ㅎㅎ",
          "original_form": "아침식사 하셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:08"
        },
        {
          "id": "MDRW2100040826.1.2",
          "form": "좋은 아침이에요오~",
          "original_form": "좋은 아침이에요오~",
          "speaker_id": "2",
          "time": "20210905 09:14"
        },
        {
          "id": "MDRW2100040826.1.3",
          "form": "아직 먹지 않았어요 ㅋㅋ name1님은 드셨나요?",
          "original_form": "아직 먹지 않았어요 ㅋㅋ &name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040826.1.4",
          "form": "네네^^ 안녕히 주무셨어요~~",
          "original_form": "네네^^ 안녕히 주무셨어요~~",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040826.1.5",
          "form": "저는 방금 아침식사 했어요",
          "original_form": "저는 방금 아침식사 했어요",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040826.1.6",
          "form": "우왕 어떤거 드셨나요?",
          "original_form": "우왕 어떤거 드셨나요?",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040826.1.7",
          "form": "늙은호박전이요",
          "original_form": "늙은호박전이요",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100040826.1.8",
          "form": "어제 호박 생겼다고 얘기했었나요?",
          "original_form": "어제 호박 생겼다고 얘기했었나요?",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100040826.1.9",
          "form": "그걸로 전 잔뜩 부쳐놔서",
          "original_form": "그걸로 전 잔뜩 부쳐놔서",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100040826.1.10",
          "form": "오늘 아침에도 먹었어요",
          "original_form": "오늘 아침에도 먹었어요",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100040826.1.11",
          "form": "호박이 어쩌다가 생겼어요?",
          "original_form": "호박이 어쩌다가 생겼어요?",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040826.1.12",
          "form": "호박전! 굉장히 건강한 아침 식사를 하셨네요 ㅎㅎ",
          "original_form": "호박전! 굉장히 건강한 아침 식사를 하셨네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040826.1.13",
          "form": "어머니 지인분이 농사지셨다고",
          "original_form": "어머니 지인분이 농사지셨다고",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040826.1.14",
          "form": "호박을 4개나 갖다주셔서",
          "original_form": "호박을 4개나 갖다주셔서",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040826.1.15",
          "form": "우와... 그거.. 다 먹으려면 한참 먹겠어요!",
          "original_form": "우와... 그거.. 다 먹으려면 한참 먹겠어요!",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100040826.1.16",
          "form": "호박으로 할 수 있는 음식은 다 시도 해봐도 좋겠는데요?",
          "original_form": "호박으로 할 수 있는 음식은 다 시도 해봐도 좋겠는데요?",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100040826.1.17",
          "form": "네 안그래도 호박 요리 검색해서",
          "original_form": "네 안그래도 호박 요리 검색해서",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040826.1.18",
          "form": "애호박은 찌개 끓이고",
          "original_form": "애호박은 찌개 끓이고",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040826.1.19",
          "form": "채썰어서 볶고",
          "original_form": "채썰어서 볶고",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040826.1.20",
          "form": "늙은 호박은 전부치고",
          "original_form": "늙은 호박은 전부치고",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040826.1.21",
          "form": "요리를 잘 하시나요??",
          "original_form": "요리를 잘 하시나요??",
          "speaker_id": "2",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040826.1.22",
          "form": "아뇨ㅎㅎ 옆에서 거드는 정도",
          "original_form": "아뇨ㅎㅎ 옆에서 거드는 정도",
          "speaker_id": "1",
          "time": "20210905 09:25"
        },
        {
          "id": "MDRW2100040826.1.23",
          "form": "훌륭한 서포트가 있으면 음식이 빨리 만들어지죠 ㅎㅎ",
          "original_form": "훌륭한 서포트가 있으면 음식이 빨리 만들어지죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:25"
        },
        {
          "id": "MDRW2100040826.1.24",
          "form": "서포트 하려면 어느정도 잘 알아야 서포트를 하니까 아마 name1님께서도 은근한 실력자 일 것 같아요",
          "original_form": "서포트 하려면 어느정도 잘 알아야 서포트를 하니까 아마 &name1&님께서도 은근한 실력자 일 것 같아요",
          "speaker_id": "2",
          "time": "20210905 09:26"
        },
        {
          "id": "MDRW2100040826.1.25",
          "form": "ㅎㅎ요리하는 것 좋아하긴 해요",
          "original_form": "ㅎㅎ요리하는 것 좋아하긴 해요",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040826.1.26",
          "form": "먹는 걸 더 좋아하지만",
          "original_form": "먹는 걸 더 좋아하지만",
