      "utterance": [
        {
          "id": "MDRW2100004673.1.1",
          "form": "안녕하세요! 요즘 유튜브중에 먹방 재미있지 않나요?",
          "original_form": "안녕하세요! 요즘 유튜브중에 먹방 재미있지 않나요?",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004673.1.2",
          "form": "사실 자주 보는 편은 아니예요",
          "original_form": "사실 자주 보는 편은 아니예요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004673.1.3",
          "form": "그 놀토에 나오는 햇님?",
          "original_form": "그 놀토에 나오는 햇님?",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004673.1.4",
          "form": "그 분꺼 본 적 있어요",
          "original_form": "그 분꺼 본 적 있어요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004673.1.5",
          "form": "말을 재밌게 하시더라구요ㅎㅎ",
          "original_form": "말을 재밌게 하시더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004673.1.6",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004673.1.7",
          "form": "전 히밥님꺼 좋아하는데",
          "original_form": "전 히밥님꺼 좋아하는데",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004673.1.8",
          "form": "그럼 유튜브는 보통 어떤 장르를 많이 보세요?",
          "original_form": "그럼 유튜브는 보통 어떤 장르를 많이 보세요?",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004673.1.9",
          "form": "저는 주로 브이로그? 패션하울이나 올리브영템 터는거 그런거봐요ㅋㅋㅋㅋ",
          "original_form": "저는 주로 브이로그? 패션하울이나 올리브영템 터는거 그런거봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004673.1.10",
          "form": "뭔가 틀어놓고 편하게 딴짓하기도 좋고 잠도 잘오고?ㅋㅋㅋㅋㅋ",
          "original_form": "뭔가 틀어놓고 편하게 딴짓하기도 좋고 잠도 잘오고?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004673.1.11",
          "form": "아 브이로그 저도 자주봐요",
          "original_form": "아 브이로그 저도 자주봐요",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004673.1.12",
          "form": "옛날엔 한별님 하울도 보고 그랬는데",
          "original_form": "옛날엔 한별님 하울도 보고 그랬는데",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004673.1.13",
          "form": "요즘은 안 보긴 해요",
          "original_form": "요즘은 안 보긴 해요",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004673.1.14",
          "form": "넷플이나 드라마는 자주 보시나요?",
          "original_form": "넷플이나 드라마는 자주 보시나요?",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004673.1.15",
          "form": "넷플에서 어제 마션 봤어요",
          "original_form": "넷플에서 어제 마션 봤어요",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004673.1.16",
          "form": "마션 괜찮죠",
          "original_form": "마션 괜찮죠",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004673.1.17",
          "form": "예전에 영화관에서 봤는데 화면 큰데서 보니까 좋았어요",
          "original_form": "예전에 영화관에서 봤는데 화면 큰데서 보니까 좋았어요",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004673.1.18",
          "form": "저는 나의 아저씨를 다시 정주행했습니다",
          "original_form": "저는 나의 아저씨를 다시 정주행했습니다",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004673.1.19",
          "form": "아이유 나오는 영화 말하는 거죠?",
          "original_form": "아이유 나오는 영화 말하는 거죠?",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004673.1.20",
          "form": "넹 드라마요!",
          "original_form": "넹 드라마요!",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004673.1.21",
          "form": "그거 재밌어요! 좀 우울하긴 한데..",
          "original_form": "그거 재밌어요! 좀 우울하긴 한데..",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004673.1.22",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004673.1.23",
          "form": "나이차이 많이 나는 이야기 인줄 알았어요.",
          "original_form": "나이차이 많이 나는 이야기 인줄 알았어요.",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004673.1.24",
          "form": "연애물이 아닌거에요?",
          "original_form": "연애물이 아닌거에요?",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004673.1.25",
          "form": "네 연애물 아니예요 걍 주변 사람들 사는 이야기 이런건데",
          "original_form": "네 연애물 아니예요 걍 주변 사람들 사는 이야기 이런건데",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004673.1.26",
          "form": "주인공이 아픔이 많은 그래서 그걸 치유해나가는?",
          "original_form": "주인공이 아픔이 많은 그래서 그걸 치유해나가는?",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004673.1.27",
          "form": "그런 스토리예요ㅎㅎㅎ",
          "original_form": "그런 스토리예요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004673.1.28",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004673.1.29",
          "form": "예전에 페미니즘이랑 역였던거 같아서",
          "original_form": "예전에 페미니즘이랑 역였던거 같아서",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004673.1.30",
          "form": "괜히 안 봤나봐요",
          "original_form": "괜히 안 봤나봐요",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004673.1.31",
          "form": "아 그런 말이 있긴 했는데 다 보고 나면 그게 아니었구나 생각드실거예요",
          "original_form": "아 그런 말이 있긴 했는데 다 보고 나면 그게 아니었구나 생각드실거예요",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004673.1.32",
          "form": "요즘 슬의생도 잘 보고 있어요ㅎㅎ",
          "original_form": "요즘 슬의생도 잘 보고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004673.1.33",
          "form": "슬의생이 뭐에요?",
          "original_form": "슬의생이 뭐에요?",
          "speaker_id": "1",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100004673.1.34",
          "form": "슬기로운 의사생활이요!",
          "original_form": "슬기로운 의사생활이요!",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004673.1.35",
          "form": "오 그것도 유명하더라고요",
          "original_form": "오 그것도 유명하더라고요",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004673.1.36",
          "form": "누구누구 나와요?",
          "original_form": "누구누구 나와요?",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004673.1.37",
          "form": "조정석 전미도 유연석 정경호 또 있는데",
          "original_form": "조정석 전미도 유연석 정경호 또 있는데",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004673.1.38",
          "form": "편하게 보기 좋아요 주1이라 아쉽지만ㅎㅎ",
          "original_form": "편하게 보기 좋아요 주1이라 아쉽지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004673.1.39",
          "form": "응답시리즈 제작진이 만든거래요",
