      "utterance": [
        {
          "id": "MDRW2100029969.1.1",
          "form": "김연경",
          "original_form": "김연경",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029969.1.2",
          "form": "멋있다",
          "original_form": "멋있다",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029969.1.3",
          "form": "갓연경",
          "original_form": "갓연경",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029969.1.4",
          "form": "코보컵 열렸다는데 안봄",
          "original_form": "코보컵 열렸다는데 안봄",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029969.1.5",
          "form": "안산은 봤나요",
          "original_form": "안산은 봤나요",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029969.1.6",
          "form": "할수있다면 직접 가서 보고 싶은데 여자배구 왜인지 서쪽지방에 몰려있는 듯한?",
          "original_form": "할수있다면 직접 가서 보고 싶은데 여자배구 왜인지 서쪽지방에 몰려있는 듯한?",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029969.1.7",
          "form": "그래요? 몰랐네.. 제 지인은 김연경 팬이라 여배직관 가더라",
          "original_form": "그래요? 몰랐네.. 제 지인은 김연경 팬이라 여배직관 가더라",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029969.1.8",
          "form": "안산을 어디서 봐요",
          "original_form": "안산을 어디서 봐요",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029969.1.9",
          "form": "연고지가 대구 경북에 하나도 안 보이던걸",
          "original_form": "연고지가 대구 경북에 하나도 안 보이던걸",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029969.1.10",
          "form": "올림픽 경기를 봤냐구...",
          "original_form": "올림픽 경기를 봤냐구...",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029969.1.11",
          "form": "오... 아뇨",
          "original_form": "오... 아뇨",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029969.1.12",
          "form": "저 한창 학원다닐 때라",
          "original_form": "저 한창 학원다닐 때라",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029969.1.13",
          "form": "여배 직관해보고 싶어요",
          "original_form": "여배 직관해보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029969.1.14",
          "form": "기왕이면 홍삼준다는 자리에서요",
          "original_form": "기왕이면 홍삼준다는 자리에서요",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029969.1.15",
          "form": "바쁜사람들이 슬펏다. 남들 다 보는 올림픽 흘러가버림",
          "original_form": "바쁜사람들이 슬펏다. 남들 다 보는 올림픽 흘러가버림",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029969.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아니 직관 홍삼도  준대요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아니 직관 홍삼도  준대요?",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029969.1.17",
          "form": "네! 자리에 따라 첨 온 사람들 많은 거 같은 자리엔 홍삼도 주고~ 뭐 그런댓어요",
          "original_form": "네! 자리에 따라 첨 온 사람들 많은 거 같은 자리엔 홍삼도 주고~ 뭐 그런댓어요",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029969.1.18",
          "form": "오.... 신기하다 처음들어요 배구 직관 인심좋네",
          "original_form": "오.... 신기하다 처음들어요 배구 직관 인심좋네",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029969.1.19",
          "form": "돈내고 여배도 보고 냠냠이도 먹으면 최고아닌가",
          "original_form": "돈내고 여배도 보고 냠냠이도 먹으면 최고아닌가",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029969.1.20",
          "form": "구웃. 전 직관해본거 야구뿐이라...",
          "original_form": "구웃. 전 직관해본거 야구뿐이라...",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029969.1.21",
          "form": "그마저도 야구는 그냥 그랬고 응원가랑 간식이 더 좋더라",
          "original_form": "그마저도 야구는 그냥 그랬고 응원가랑 간식이 더 좋더라",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029969.1.22",
          "form": "전 축구뿐이에요",
          "original_form": "전 축구뿐이에요",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029969.1.23",
          "form": "포항 스틸러스",
          "original_form": "포항 스틸러스",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029969.1.24",
          "form": "근데 간식 안주더라",
          "original_form": "근데 간식 안주더라",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029969.1.25",
          "form": "야구볼때 간식은 사먹었어요",
          "original_form": "야구볼때 간식은 사먹었어요",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029969.1.26",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210903 17:30"
        }
      ]
    }
  ]
