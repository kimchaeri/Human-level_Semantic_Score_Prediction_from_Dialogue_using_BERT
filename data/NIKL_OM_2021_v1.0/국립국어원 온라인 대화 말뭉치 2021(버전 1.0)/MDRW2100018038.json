        },
        {
          "id": "MDRW2100018038.1.5",
          "form": "우와 좋은데 다니시네요",
          "original_form": "우와 좋은데 다니시네요",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018038.1.6",
          "form": "어디다니세요?",
          "original_form": "어디다니세요?",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018038.1.7",
          "form": "전 서비스직이에요",
          "original_form": "전 서비스직이에요",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018038.1.8",
          "form": "진상 많나요",
          "original_form": "진상 많나요",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018038.1.9",
          "form": "더러 있죠ㅜㅜ",
          "original_form": "더러 있죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018038.1.10",
          "form": "고생하시네요ㅠㅜ",
          "original_form": "고생하시네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018038.1.11",
          "form": "좋은 사람이 더 많아요",
          "original_form": "좋은 사람이 더 많아요",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018038.1.12",
          "form": "긍정적이셔서 더 직장이랑 맞겠어요",
          "original_form": "긍정적이셔서 더 직장이랑 맞겠어요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018038.1.13",
          "form": "네ㅎㅎ 사람좋아하기도하고 좋은점이 더 잘보여서요",
          "original_form": "네ㅎㅎ 사람좋아하기도하고 좋은점이 더 잘보여서요",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018038.1.14",
          "form": "그거 진짜 축복받은재능인 거 아시죠",
          "original_form": "그거 진짜 축복받은재능인 거 아시죠",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018038.1.15",
          "form": "아우 과찬이십니다",
          "original_form": "아우 과찬이십니다",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018038.1.16",
          "form": "손님들이 엄청 좋아하시겠어요",
          "original_form": "손님들이 엄청 좋아하시겠어요",
