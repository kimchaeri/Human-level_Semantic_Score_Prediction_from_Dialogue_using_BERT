      "utterance": [
        {
          "id": "MDRW2100025578.1.1",
          "form": "name2은 집안일 중에 어느 것이 제일 하기 싫은가요?",
          "original_form": "&name2&은 집안일 중에 어느 것이 제일 하기 싫은가요?",
          "speaker_id": "1",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100025578.1.2",
          "form": "저 빨래너는거요ㅋㅋㅋㅋㅋ",
          "original_form": "저 빨래너는거요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025578.1.3",
          "form": "저랑 같아요~",
          "original_form": "저랑 같아요~",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025578.1.4",
          "form": "저도 세탁이 젤 시러요",
          "original_form": "저도 세탁이 젤 시러요",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025578.1.5",
          "form": "세탁하고 널고 개고",
          "original_form": "세탁하고 널고 개고",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025578.1.6",
          "form": "다른건 뭐 더러우니 하는거고 하니까",
          "original_form": "다른건 뭐 더러우니 하는거고 하니까",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025578.1.7",
          "form": "이상하게 빨래는 귀찮더라구요 짜증남ㅋㅋㅋ",
          "original_form": "이상하게 빨래는 귀찮더라구요 짜증남ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025578.1.8",
          "form": "하긴해야하는데",
          "original_form": "하긴해야하는데",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025578.1.9",
          "form": "저는 세탁시 분리하는거요",
          "original_form": "저는 세탁시 분리하는거요",
          "speaker_id": "1",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025578.1.10",
          "form": "근데 이 얘기 한 것 같은데요ㅋㅋㅋ",
          "original_form": "근데 이 얘기 한 것 같은데요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025578.1.11",
          "form": "저흰 통 세개 놓고 분리합니다",
          "original_form": "저흰 통 세개 놓고 분리합니다",
          "speaker_id": "2",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025578.1.12",
          "form": "애초에 넣을 때",
          "original_form": "애초에 넣을 때",
          "speaker_id": "2",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025578.1.13",
          "form": "그랬나요?",
          "original_form": "그랬나요?",
          "speaker_id": "1",
          "time": "20210901 14:24"
        },
        {
          "id": "MDRW2100025578.1.14",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025578.1.15",
          "form": "저는 청소 좋아한다는 말도 했엇나요?",
          "original_form": "저는 청소 좋아한다는 말도 했엇나요?",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025578.1.16",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025578.1.17",
          "form": "저도 발에 밟히는거 싫어한다고ㅋㅋㅋ",
          "original_form": "저도 발에 밟히는거 싫어한다고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025578.1.18",
          "form": "ㅋㅋㅋㅋㅋ 이미 했던 대화군요....ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 이미 했던 대화군요....ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025578.1.19",
          "form": "얼른 가족으로 넘어갑시다",
          "original_form": "얼른 가족으로 넘어갑시다",
          "speaker_id": "1",
          "time": "20210901 14:29"
        },
        {
          "id": "MDRW2100025578.1.20",
          "form": "가족분들은 집안일 많이 도와주나요?",
          "original_form": "가족분들은 집안일 많이 도와주나요?",
          "speaker_id": "1",
          "time": "20210901 14:29"
        },
        {
          "id": "MDRW2100025578.1.21",
          "form": "ㅋㅋㅋ적당히 할거하는데",
          "original_form": "ㅋㅋㅋ적당히 할거하는데",
          "speaker_id": "2",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025578.1.22",
          "form": "부모님이 주로..",
          "original_form": "부모님이 주로..",
          "speaker_id": "2",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025578.1.23",
          "form": "그렇군요.. 좋네요~ 돕는 입장이시군요",
          "original_form": "그렇군요.. 좋네요~ 돕는 입장이시군요",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025578.1.24",
          "form": "돕는다기보단 내가 쓴거 내가 치우자?ㅋㅋㅋ",
          "original_form": "돕는다기보단 내가 쓴거 내가 치우자?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025578.1.25",
          "form": "청소기는 열심히 돌립니다",
          "original_form": "청소기는 열심히 돌립니다",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025578.1.26",
          "form": "밟히는거 싫어서ㅋㅋㅋ",
          "original_form": "밟히는거 싫어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025578.1.27",
          "form": "네.. 자기 방이라도 하면 부모님이 편하죠..ㅎㅎ",
          "original_form": "네.. 자기 방이라도 하면 부모님이 편하죠..ㅎㅎ",
          "speaker_id": "1",
