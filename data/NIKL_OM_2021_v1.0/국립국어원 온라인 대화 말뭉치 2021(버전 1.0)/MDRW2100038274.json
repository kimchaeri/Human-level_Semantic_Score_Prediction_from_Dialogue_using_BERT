        {
          "id": "MDRW2100038274.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 07:39"
        },
        {
          "id": "MDRW2100038274.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100038274.1.3",
          "form": "즐겨듣는 가수 있으세요?",
          "original_form": "즐겨듣는 가수 있으세요?",
          "speaker_id": "1",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100038274.1.4",
          "form": "아니요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100038274.1.5",
          "form": "편식하지 않고\r\n고루고루 듣습니다ㅋㅋㅋ",
          "original_form": "편식하지 않고\r\n고루고루 듣습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100038274.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100038274.1.7",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "1",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100038274.1.8",
          "form": "저는 한 아이돌의 팬이라",
          "original_form": "저는 한 아이돌의 팬이라",
          "speaker_id": "1",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100038274.1.9",
          "form": "그 아이돌 위주로 들어요",
          "original_form": "그 아이돌 위주로 들어요",
          "speaker_id": "1",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100038274.1.10",
          "form": "bts?",
          "original_form": "bts?",
          "speaker_id": "2",
          "time": "20210902 07:43"
        },
        {
          "id": "MDRW2100038274.1.11",
          "form": "ㅋㅋㅋㅋ 어떻게 아셨어요",
          "original_form": "ㅋㅋㅋㅋ 어떻게 아셨어요",
          "speaker_id": "1",
          "time": "20210902 07:43"
        },
        {
          "id": "MDRW2100038274.1.12",
          "form": "아이돌을 bts바께 모릅니닼ㅋㅋㅋ",
          "original_form": "아이돌을 bts바께 모릅니닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 07:44"
        },
        {
          "id": "MDRW2100038274.1.13",
          "form": "최애멤버는 누군가요?",
          "original_form": "최애멤버는 누군가요?",
          "speaker_id": "2",
          "time": "20210902 07:44"
        },
        {
          "id": "MDRW2100038274.1.14",
          "form": "진 좋아해요",
          "original_form": "진 좋아해요",
          "speaker_id": "1",
          "time": "20210902 07:44"
        },
        {
          "id": "MDRW2100038274.1.15",
          "form": "얼굴보시는군요.ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "얼굴보시는군요.ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 07:45"
        },
        {
          "id": "MDRW2100038274.1.16",
          "form": "얼굴도 보고 성격도 봤습니다ㅋㅋ",
          "original_form": "얼굴도 보고 성격도 봤습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 07:46"
        },
        {
          "id": "MDRW2100038274.1.17",
          "form": "저 근데 쓰고나니\r\n다들 얼굴도 성격도 굿굿이라",
          "original_form": "저 근데 쓰고나니\r\n다들 얼굴도 성격도 굿굿이라",
          "speaker_id": "2",
          "time": "20210902 07:46"
        },
        {
          "id": "MDRW2100038274.1.18",
          "form": "너무 매력있는 친구들이더라구요",
          "original_form": "너무 매력있는 친구들이더라구요",
          "speaker_id": "2",
          "time": "20210902 07:46"
        },
        {
          "id": "MDRW2100038274.1.19",
          "form": "ㅋㅋㅋ 너무 잘 아시는데요",
          "original_form": "ㅋㅋㅋ 너무 잘 아시는데요",
          "speaker_id": "1",
          "time": "20210902 07:47"
        },
        {
          "id": "MDRW2100038274.1.20",
          "form": "유툽에서 아무 생각없이 본\r\n방탄영상 하나에 밤이 다 지나갔었죠ㅋㅋㅋ",
          "original_form": "유툽에서 아무 생각없이 본\r\n방탄영상 하나에 밤이 다 지나갔었죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100038274.1.21",
          "form": "저도 그랬다가 아미 됐어요",
          "original_form": "저도 그랬다가 아미 됐어요",
          "speaker_id": "1",
          "time": "20210902 07:49"
        }
