      "utterance": [
        {
          "id": "MDRW2100047264.1.1",
          "form": "요기요는 하셨어요?",
          "original_form": "요기요는 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100047264.1.2",
          "form": "15000원으로 바껴서 어제 패스ㅠ",
          "original_form": "15000원으로 바껴서 어제 패스ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100047264.1.3",
          "form": "나이트쿠폰",
          "original_form": "나이트쿠폰",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100047264.1.4",
          "form": "안그래도 봤어요ㅠ",
          "original_form": "안그래도 봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 01:53"
        },
        {
          "id": "MDRW2100047264.1.5",
          "form": "그래서 걍 낮에 2천원주고 사려구요",
          "original_form": "그래서 걍 낮에 2천원주고 사려구요",
          "speaker_id": "2",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100047264.1.6",
          "form": "너무 0원이어서 바꿨나보네요 그것도생각안하고 기획하는건지ㅜ",
          "original_form": "너무 0원이어서 바꿨나보네요 그것도생각안하고 기획하는건지ㅜ",
          "speaker_id": "1",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100047264.1.7",
          "form": "슈클쓰고 샐러드나",
          "original_form": "슈클쓰고 샐러드나",
          "speaker_id": "2",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100047264.1.8",
          "form": "런치도15고ㅠㅜ",
          "original_form": "런치도15고ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100047264.1.9",
          "form": "아 글고보니 윙잇도 써야되는데 까먹고 있었네요",
          "original_form": "아 글고보니 윙잇도 써야되는데 까먹고 있었네요",
          "speaker_id": "2",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100047264.1.10",
          "form": "걍 쿠폰버리고 슈클만 쓰려구요",
          "original_form": "걍 쿠폰버리고 슈클만 쓰려구요",
          "speaker_id": "2",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100047264.1.11",
          "form": "2만원어치고르고 4천원결제가 나은듯요",
          "original_form": "2만원어치고르고 4천원결제가 나은듯요",
          "speaker_id": "2",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100047264.1.12",
          "form": "3천원 나와도 15000원어치삼 4천결제라",
          "original_form": "3천원 나와도 15000원어치삼 4천결제라",
          "speaker_id": "2",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100047264.1.13",
          "form": "쿠폰 잘 쓰셨나요?ㅎㅎ",
          "original_form": "쿠폰 잘 쓰셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100047264.1.14",
          "form": "윙잇",
          "original_form": "윙잇",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100047264.1.15",
          "form": "이따써야돼요ㅎ h패션몰도ㅎ",
          "original_form": "이따써야돼요ㅎ h패션몰도ㅎ",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100047264.1.16",
          "form": "쇼핑할게많아서 정신없네요",
          "original_form": "쇼핑할게많아서 정신없네요",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100047264.1.17",
          "form": "저 이제500끝이예요ㅎ",
          "original_form": "저 이제500끝이예요ㅎ",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100047264.1.18",
          "form": "세상에나~  이제볼수없으시겠네여ㅜ",
          "original_form": "세상에나~  이제볼수없으시겠네여ㅜ",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047264.1.19",
          "form": "ㅋㅋ 톡은 열어놓을게요",
          "original_form": "ㅋㅋ 톡은 열어놓을게요",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100047264.1.20",
          "form": "심심하실때 톡주세요",
          "original_form": "심심하실때 톡주세요",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.21",
          "form": "저 h패션몰 타미 긴팔티 470원주고샀어요",
          "original_form": "저 h패션몰 타미 긴팔티 470원주고샀어요",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.22",
          "form": "와~~~  대박",
          "original_form": "와~~~  대박",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.23",
          "form": "뽑기?인가요?",
          "original_form": "뽑기?인가요?",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.24",
          "form": "룰렛이 쿠폰만나와서 아쉽",
          "original_form": "룰렛이 쿠폰만나와서 아쉽",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.25",
          "form": "전암것도없는데",
          "original_form": "전암것도없는데",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.26",
          "form": "룰렛 잘뽑으면 되나요?",
          "original_form": "룰렛 잘뽑으면 되나요?",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.27",
          "form": "신규아니어도",
          "original_form": "신규아니어도",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100047264.1.28",
          "form": "출첵만근 2만포요",
          "original_form": "출첵만근 2만포요",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047264.1.29",
          "form": "룰렛은 랜덤",
          "original_form": "룰렛은 랜덤",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047264.1.30",
          "form": "오늘 끝나서 담에 이벤트뜨면 하셔요",
          "original_form": "오늘 끝나서 담에 이벤트뜨면 하셔요",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047264.1.31",
          "form": "아 출첵을 안했어어ㅠㅜ",
          "original_form": "아 출첵을 안했어어ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100047264.1.32",
          "form": "안타깝네요ㅠ",
          "original_form": "안타깝네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100047264.1.33",
          "form": "그렇게 챙겨하는건 힘들더라구요ㅠ",
          "original_form": "그렇게 챙겨하는건 힘들더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:47"
        }
      ]
