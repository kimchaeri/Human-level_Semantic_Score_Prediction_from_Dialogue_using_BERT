      "utterance": [
        {
          "id": "MDRW2100001775.1.1",
          "form": "어떤 커피를 좋아하시나려",
          "original_form": "어떤 커피를 좋아하시나려",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100001775.1.2",
          "form": "좋아하시나요",
          "original_form": "좋아하시나요",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100001775.1.3",
          "form": "저는 커피는 카페모카가 제일 좋아요",
          "original_form": "저는 커피는 카페모카가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100001775.1.4",
          "form": "믹스커피나",
          "original_form": "믹스커피나",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100001775.1.5",
          "form": "저도 카페모카좋아해요 특히 잠깰때 많이먹어요",
          "original_form": "저도 카페모카좋아해요 특히 잠깰때 많이먹어요",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100001775.1.6",
          "form": "저는 커피먹어도 잠이 잘와서 ㅋㅋ",
          "original_form": "저는 커피먹어도 잠이 잘와서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100001775.1.7",
          "form": "부러워요 전 커피마시면 상태변화가 확 와요",
          "original_form": "부러워요 전 커피마시면 상태변화가 확 와요",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001775.1.8",
          "form": "저는 하도 멀쩡해서 ㅋㅋㅋ",
          "original_form": "저는 하도 멀쩡해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001775.1.9",
          "form": "부러워요 저는 아이스티 먹어도 잠깨요",
          "original_form": "부러워요 저는 아이스티 먹어도 잠깨요",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001775.1.10",
          "form": "오... 카페인 그런거에 되게 예민하신가봐요",
          "original_form": "오... 카페인 그런거에 되게 예민하신가봐요",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001775.1.11",
          "form": "네 힘이 없을땐 과부하돼서 체력이 깎이고 그래요",
          "original_form": "네 힘이 없을땐 과부하돼서 체력이 깎이고 그래요",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001775.1.12",
          "form": "아니다 반대로 힘이 날때",
          "original_form": "아니다 반대로 힘이 날때",
          "speaker_id": "2",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001775.1.13",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "2",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001775.1.14",
          "form": "아아 저는 잠깨야할때 치약을.. 음료는 안받아서 ㅋㅋ",
          "original_form": "아아 저는 잠깨야할때 치약을.. 음료는 안받아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001775.1.15",
          "form": "민트초코에 약하시군요",
          "original_form": "민트초코에 약하시군요",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
