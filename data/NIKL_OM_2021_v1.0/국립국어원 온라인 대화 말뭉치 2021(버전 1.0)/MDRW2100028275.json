      "utterance": [
        {
          "id": "MDRW2100028275.1.1",
          "form": "여긴 계속 비가 오네요ㅜㅜ",
          "original_form": "여긴 계속 비가 오네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028275.1.2",
          "form": "여긴 오늘 맑았어요!",
          "original_form": "여긴 오늘 맑았어요!",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028275.1.3",
          "form": "흐려져서 비오나 했는데 다행히 안 오더라구요",
          "original_form": "흐려져서 비오나 했는데 다행히 안 오더라구요",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028275.1.4",
          "form": "우와! 부러워요ㅜㅜ",
          "original_form": "우와! 부러워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028275.1.5",
          "form": "주룩주룩 많이 오고 있어요ㅜㅜ",
          "original_form": "주룩주룩 많이 오고 있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028275.1.6",
          "form": "간만에 비타민D 많이 쬐시지ㅎㅎㅎ",
          "original_form": "간만에 비타민D 많이 쬐시지ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028275.1.7",
          "form": "ㅠ. 일이 바빠서 나갈 틈도 없었네용 ...ㅠㅠ",
          "original_form": "ㅠ. 일이 바빠서 나갈 틈도 없었네용 ...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028275.1.8",
          "form": "ㅜㅠ 슬프다...",
          "original_form": "ㅜㅠ 슬프다...",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028275.1.9",
          "form": "대신 약으로 챙겨드세요...ㅜㅜ",
          "original_form": "대신 약으로 챙겨드세요...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028275.1.10",
          "form": "비타민은 중요하니까요",
          "original_form": "비타민은 중요하니까요",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028275.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 안그래도 조만간 백신 맞을 예정이라",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 안그래도 조만간 백신 맞을 예정이라",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028275.1.12",
          "form": "급 비타민 샀어요 챙겨먹으려고요",
          "original_form": "급 비타민 샀어요 챙겨먹으려고요",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028275.1.13",
          "form": "ㅎㅎㅎ잘하셨어요~~!!",
          "original_form": "ㅎㅎㅎ잘하셨어요~~!!",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028275.1.14",
          "form": "백신이 워낙 세서ㅜㅜ",
          "original_form": "백신이 워낙 세서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028275.1.15",
          "form": "귀신같이 환절기 되니까 구내염도 나더라구요ㅠㅠ",
          "original_form": "귀신같이 환절기 되니까 구내염도 나더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028275.1.16",
          "form": "미지근한 물 많이 드세요ㅠㅠ",
          "original_form": "미지근한 물 많이 드세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028275.1.17",
