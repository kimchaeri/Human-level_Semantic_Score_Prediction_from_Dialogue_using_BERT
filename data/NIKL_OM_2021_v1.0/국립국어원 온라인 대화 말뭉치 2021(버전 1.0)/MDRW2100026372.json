      "utterance": [
        {
          "id": "MDRW2100026372.1.1",
          "form": "동물 좋아해여?",
          "original_form": "동물 좋아해여?",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026372.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026372.1.3",
          "form": "ㅋㅋㅋㅋㅋ 근데 보는 거만 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋ 근데 보는 거만 좋아해요",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026372.1.4",
          "form": "대리님두 안 키우시죠",
          "original_form": "대리님두 안 키우시죠",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026372.1.5",
          "form": "ㅋㅋㅋㅋㅋ네 안키움",
          "original_form": "ㅋㅋㅋㅋㅋ네 안키움",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026372.1.6",
          "form": "저는 동물에 정을 준적이 없는 비싼사람?ㅋㅋㅋㅋㅋ",
          "original_form": "저는 동물에 정을 준적이 없는 비싼사람?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026372.1.7",
          "form": "ㅋㅋ 전 자신이 없어요",
          "original_form": "ㅋㅋ 전 자신이 없어요",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026372.1.8",
          "form": "걔 죽을 때까지 책임질 자신이ㅜㅜ",
          "original_form": "걔 죽을 때까지 책임질 자신이ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026372.1.9",
          "form": "그리구 저 아침 7시부터 오후 7시까지 하루에 12시간을 밖에 있는데",
          "original_form": "그리구 저 아침 7시부터 오후 7시까지 하루에 12시간을 밖에 있는데",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026372.1.10",
          "form": "걔 혼자 집에 있기 하기두 불쌍하구ㅠ",
          "original_form": "걔 혼자 집에 있기 하기두 불쌍하구ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026372.1.11",
          "form": "그런부분도 있긴해요",
          "original_form": "그런부분도 있긴해요",
          "speaker_id": "1",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100026372.1.12",
          "form": "내몸하나 건사하기도 힘든데ㅋㅋ",
          "original_form": "내몸하나 건사하기도 힘든데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100026372.1.13",
          "form": "엄마 일 그만두시면 강아지 사드릴까고민중",
          "original_form": "엄마 일 그만두시면 강아지 사드릴까고민중",
          "speaker_id": "1",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100026372.1.14",
          "form": "어머님이 키우고 싶대여?",
          "original_form": "어머님이 키우고 싶대여?",
          "speaker_id": "2",
          "time": "20210902 07:44"
        },
        {
          "id": "MDRW2100026372.1.15",
          "form": "ㅋㅋㅋ 잘 물어보고 하세요",
          "original_form": "ㅋㅋㅋ 잘 물어보고 하세요",
          "speaker_id": "2",
          "time": "20210902 07:44"
        },
        {
          "id": "MDRW2100026372.1.16",
          "form": "어무니가 집에 혼자 있으면 외로울것같다고",
          "original_form": "어무니가 집에 혼자 있으면 외로울것같다고",
          "speaker_id": "1",
          "time": "20210902 07:45"
        },
        {
          "id": "MDRW2100026372.1.17",
          "form": "여기가 시골급 위치라서",
          "original_form": "여기가 시골급 위치라서",
          "speaker_id": "1",
          "time": "20210902 07:45"
        },
        {
          "id": "MDRW2100026372.1.18",
          "form": "엄마 차도 없공",
          "original_form": "엄마 차도 없공",
          "speaker_id": "1",
          "time": "20210902 07:45"
        },
        {
          "id": "MDRW2100026372.1.19",
          "form": "집이 진짜 조용해서 적적할것같아여",
          "original_form": "집이 진짜 조용해서 적적할것같아여",
          "speaker_id": "1",
          "time": "20210902 07:45"
        },
        {
          "id": "MDRW2100026372.1.20",
          "form": "그러면 괜찮을 수도 있겠네요 ㅋㅋㅋㅋ",
          "original_form": "그러면 괜찮을 수도 있겠네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:40"
        },
        {
          "id": "MDRW2100026372.1.21",
          "form": "그쵸 근데 또 무지개다리 건널때가 진짜 힘들데요",
          "original_form": "그쵸 근데 또 무지개다리 건널때가 진짜 힘들데요",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100026372.1.22",
          "form": "저 아는사람은 그게 힘들어서 못키우겠다고 함",
          "original_form": "저 아는사람은 그게 힘들어서 못키우겠다고 함",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026372.1.23",
          "form": "맞아요 저도 그 걱정도 있어요ㅠ",
          "original_form": "맞아요 저도 그 걱정도 있어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100026372.1.24",
          "form": "너무.. 상실감 클 거 같늠",
          "original_form": "너무.. 상실감 클 거 같늠",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100026372.1.25",
          "form": "어떤감정일지 가늠이 안가여",
          "original_form": "어떤감정일지 가늠이 안가여",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100026372.1.26",
          "form": "그쳐 저도요",
          "original_form": "그쳐 저도요",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100026372.1.27",
          "form": "근데 막 식물한테도 정들지 않아요??",
          "original_form": "근데 막 식물한테도 정들지 않아요??",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100026372.1.28",
          "form": "대리님 그 쪼끄만 뭉고기 키우셨었자나요",
          "original_form": "대리님 그 쪼끄만 뭉고기 키우셨었자나요",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100026372.1.29",
          "form": "죽었지만 ㅜ",
          "original_form": "죽었지만 ㅜ",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100026372.1.30",
          "form": "아 맞다ㅋㅋㅋㅋ구피....추억돋네요",
          "original_form": "아 맞다ㅋㅋㅋㅋ구피....추억돋네요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100026372.1.31",
          "form": "그르넹...생각해보니깐요",
          "original_form": "그르넹...생각해보니깐요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100026372.1.32",
          "form": "정이 무서운거였네요",
          "original_form": "정이 무서운거였네요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        }
      ]
