        },
        {
          "id": "MDRW2100010228.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010228.1.3",
          "form": "알바해보신적 있으신가요?",
          "original_form": "알바해보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010228.1.4",
          "form": "네 알바 해봤죠ㅎㅎ 카페랑 시청알바같은거 했었어요",
          "original_form": "네 알바 해봤죠ㅎㅎ 카페랑 시청알바같은거 했었어요",
          "speaker_id": "2",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010228.1.5",
          "form": "오 카페알바 할만한가요?",
          "original_form": "오 카페알바 할만한가요?",
          "speaker_id": "1",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010228.1.6",
          "form": "일은 익숙해지는데 사람대하는게 어려웠어요",
          "original_form": "일은 익숙해지는데 사람대하는게 어려웠어요",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010228.1.7",
          "form": "맞아요 어디서나사람이",
          "original_form": "맞아요 어디서나사람이",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010228.1.8",
          "form": "문제죠ㅠㅠ",
          "original_form": "문제죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010228.1.9",
          "form": "네 프랜차이즈라 다 죄송하다고 해야하는 입장이라 더 힘들었네요 알바하세요?",
          "original_form": "네 프랜차이즈라 다 죄송하다고 해야하는 입장이라 더 힘들었네요 알바하세요?",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010228.1.10",
          "form": "저는 안한지 꽤됐고",
          "original_form": "저는 안한지 꽤됐고",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010228.1.11",
          "form": "빵집알바 해봤답니다ㅋㅋㅋ",
          "original_form": "빵집알바 해봤답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010228.1.12",
          "form": "오 빵집알바ㅎㅎ 재밌을거같아요",
          "original_form": "오 빵집알바ㅎㅎ 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010228.1.13",
          "form": "ㅋㅋㅋ추천은",
          "original_form": "ㅋㅋㅋ추천은",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010228.1.14",
          "form": "안한답니다ㅠ",
          "original_form": "안한답니다ㅠ",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010228.1.15",
          "form": "알바는 그냥 안할수있으면 안하는게 맞는ㅎㅎ",
          "original_form": "알바는 그냥 안할수있으면 안하는게 맞는ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010228.1.16",
          "form": "빵집은 진짜",
          "original_form": "빵집은 진짜",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010228.1.17",
          "form": "가성비 떨어지는 알바같아요ㅋㅋㅋ",
          "original_form": "가성비 떨어지는 알바같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:11"
        }
      ]
