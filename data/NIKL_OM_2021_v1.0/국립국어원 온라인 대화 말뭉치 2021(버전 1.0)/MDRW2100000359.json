        },
        {
          "id": "MDRW2100000359.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000359.1.3",
          "form": "여름휴가 다녀오셨어요?",
          "original_form": "여름휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000359.1.4",
          "form": "아니요~ 아직이요ㅎㅎ",
          "original_form": "아니요~ 아직이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000359.1.5",
          "form": "다녀오셨나요?",
          "original_form": "다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000359.1.6",
          "form": "저는 호캉스요",
          "original_form": "저는 호캉스요",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000359.1.7",
          "form": "와 좋으셨겄어요~ 요즘엔 코로나라ㅠㅠ 어디 맘대로 못가겠더라구요",
          "original_form": "와 좋으셨겄어요~ 요즘엔 코로나라ㅠㅠ 어디 맘대로 못가겠더라구요",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000359.1.8",
          "form": "네~사람 많은곳 가기 좀 그래서 호캉스로 갔다왔어요",
          "original_form": "네~사람 많은곳 가기 좀 그래서 호캉스로 갔다왔어요",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000359.1.9",
          "form": "맞아요. 안전한게 우선이죠 ㅎ",
          "original_form": "맞아요. 안전한게 우선이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000359.1.10",
          "form": "네~ 휴가 어디 안가시나요?",
          "original_form": "네~ 휴가 어디 안가시나요?",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000359.1.11",
          "form": "저희는 9월쯤 갈수있을것같아요",
          "original_form": "저희는 9월쯤 갈수있을것같아요",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000359.1.12",
          "form": "아~날씨가 선선해서 좋겠네요",
          "original_form": "아~날씨가 선선해서 좋겠네요",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000359.1.13",
          "form": "네~ 그때쯤이면코로나도 잠잠해지길ㅠ",
          "original_form": "네~ 그때쯤이면코로나도 잠잠해지길ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000359.1.14",
          "form": "그래야 여행도 좀 마음놓고 다닐수있을것 같아요ㅠㅠ",
          "original_form": "그래야 여행도 좀 마음놓고 다닐수있을것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000359.1.15",
          "form": "네 제발요ㅠ",
          "original_form": "네 제발요ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000359.1.16",
          "form": "여행지는 정하셨어요?",
          "original_form": "여행지는 정하셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000359.1.17",
          "form": "아직이요ㅜㅠ슬슬정해봐야죵",
          "original_form": "아직이요ㅜㅠ슬슬정해봐야죵",
          "speaker_id": "2",
