      "utterance": [
        {
          "id": "MDRW2100011911.1.1",
          "form": "오버워치 안한지 오래됐는데 요즘도 재밌을까유",
          "original_form": "오버워치 안한지 오래됐는데 요즘도 재밌을까유",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011911.1.2",
          "form": "전 오버워치 안해봐서요.. 요즘은 어떤 게임이 재밌죠?",
          "original_form": "전 오버워치 안해봐서요.. 요즘은 어떤 게임이 재밌죠?",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011911.1.3",
          "form": "요즘 저는 심즈하고있어요",
          "original_form": "요즘 저는 심즈하고있어요",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011911.1.4",
          "form": "역시 질리지않는 심즈",
          "original_form": "역시 질리지않는 심즈",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011911.1.5",
          "form": "아 저도 해보고싶었는데",
          "original_form": "아 저도 해보고싶었는데",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011911.1.6",
          "form": "심즈 몇기가 젤 재밌나요?",
          "original_form": "심즈 몇기가 젤 재밌나요?",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011911.1.7",
          "form": "이번에 세일하던데",
          "original_form": "이번에 세일하던데",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011911.1.8",
          "form": "한번 사보세여",
          "original_form": "한번 사보세여",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011911.1.9",
          "form": "저는 4하구있어요",
          "original_form": "저는 4하구있어요",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011911.1.10",
          "form": "그렇구나 캐릭터도 직접 디자인 할수있나요?",
          "original_form": "그렇구나 캐릭터도 직접 디자인 할수있나요?",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011911.1.11",
          "form": "네!! 근데 그게제일어려워욬ㅋㅋ",
          "original_form": "네!! 근데 그게제일어려워욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011911.1.12",
          "form": "볓시간씩걸려서ㅠㅠ",
          "original_form": "볓시간씩걸려서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011911.1.13",
          "form": "그래도 애착가고 더 재밌을거같아요",
          "original_form": "그래도 애착가고 더 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011911.1.14",
          "form": "마자여 그래서 가족만들때",
          "original_form": "마자여 그래서 가족만들때",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011911.1.15",
          "form": "3-4시간걸리는데",
          "original_form": "3-4시간걸리는데",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011911.1.16",
          "form": "플레이하기재밌어요",
          "original_form": "플레이하기재밌어요",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011911.1.17",
          "form": "한번 해봐야겠네요\r\n유튜브에서 보면 연예인이랑 똑같이 만들던데 그건 진짜 금손이어야 가능한거죠?",
          "original_form": "한번 해봐야겠네요\r\n유튜브에서 보면 연예인이랑 똑같이 만들던데 그건 진짜 금손이어야 가능한거죠?",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011911.1.18",
          "form": "아녀!! 막손도 합니닼ㅋㅋ",
          "original_form": "아녀!! 막손도 합니닼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100011911.1.19",
          "form": "세일할때구매하셔애해요",
          "original_form": "세일할때구매하셔애해요",
          "speaker_id": "1",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100011911.1.20",
          "form": "오 좋네요! 세일할때랑 안할때랑 차이가 큰가봐요?",
          "original_form": "오 좋네요! 세일할때랑 안할때랑 차이가 큰가봐요?",
          "speaker_id": "2",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100011911.1.21",
          "form": "네네 엄청 무조건 세일할때",
          "original_form": "네네 엄청 무조건 세일할때",
          "speaker_id": "1",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011911.1.22",
          "form": "사셔야해요...",
          "original_form": "사셔야해요...",
          "speaker_id": "1",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011911.1.23",
          "form": "팩이많아서",
          "original_form": "팩이많아서",
          "speaker_id": "1",
          "time": "20210824 17:36"
        }
      ]
    }
