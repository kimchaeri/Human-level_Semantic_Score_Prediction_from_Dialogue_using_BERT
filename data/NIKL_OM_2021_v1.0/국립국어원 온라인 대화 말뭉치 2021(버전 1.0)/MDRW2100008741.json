      "utterance": [
        {
          "id": "MDRW2100008741.1.1",
          "form": "오늘 점심 뭐드세요?",
          "original_form": "오늘 점심 뭐드세요?",
          "speaker_id": "2",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100008741.1.2",
          "form": "저 쫄면에",
          "original_form": "저 쫄면에",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100008741.1.3",
          "form": "돈까스먹었답니다ㅋㅋㅋ",
          "original_form": "돈까스먹었답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100008741.1.4",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100008741.1.5",
          "form": "김밥헤븐 쫄정 가끔 먹는데 맛났겠네요.\r\n전 뼈해장국 먹었어요.",
          "original_form": "김밥헤븐 쫄정 가끔 먹는데 맛났겠네요.\r\n전 뼈해장국 먹었어요.",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100008741.1.6",
          "form": "아 김밥천국좋죠ㅋㅋㅋ 뼈해장국도 좋네요",
          "original_form": "아 김밥천국좋죠ㅋㅋㅋ 뼈해장국도 좋네요",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100008741.1.7",
          "form": "비가오는게 저는 차돌짬뽕 먹고싶어요ㅠㅠ",
          "original_form": "비가오는게 저는 차돌짬뽕 먹고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100008741.1.8",
          "form": "일요일날 소고기 짬뽕 먹었는데 불향입은 소고기랑 얼큰 국물이 맛나더라구요ㅡ",
          "original_form": "일요일날 소고기 짬뽕 먹었는데 불향입은 소고기랑 얼큰 국물이 맛나더라구요ㅡ",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100008741.1.9",
          "form": "와우 짬뽕너무좋죠",
          "original_form": "와우 짬뽕너무좋죠",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100008741.1.10",
          "form": "중국집은 신속배달이라",
          "original_form": "중국집은 신속배달이라",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100008741.1.11",
          "form": "좋은것같아요",
          "original_form": "좋은것같아요",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100008741.1.12",
          "form": "오늘은 또 뭘 먹어야 할까요.\r\n전 평일엔 맨날 혼밥하는뎅",
          "original_form": "오늘은 또 뭘 먹어야 할까요.\r\n전 평일엔 맨날 혼밥하는뎅",
          "speaker_id": "2",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100008741.1.13",
          "form": "순댓국어때요??",
          "original_form": "순댓국어때요??",
          "speaker_id": "1",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100008741.1.14",
          "form": "혼밥의 성지!",
          "original_form": "혼밥의 성지!",
          "speaker_id": "1",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100008741.1.15",
          "form": "순대국이 혼밥의 성지에요??\r\n뜨거운걸 빨리 못먹어서.. ㅋ",
          "original_form": "순대국이 혼밥의 성지에요??\r\n뜨거운걸 빨리 못먹어서.. ㅋ",
          "speaker_id": "2",
          "time": "20210824 11:13"
        },
        {
          "id": "MDRW2100008741.1.16",
          "form": "ㅋㅋㅋ네 순댓국집은",
          "original_form": "ㅋㅋㅋ네 순댓국집은",
          "speaker_id": "1",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100008741.1.17",
          "form": "혼밥이 진짜많더라고요",
          "original_form": "혼밥이 진짜많더라고요",
          "speaker_id": "1",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100008741.1.18",
          "form": "더 추워지면 순대국 도전해보려구요 ㅎ",
          "original_form": "더 추워지면 순대국 도전해보려구요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100008741.1.19",
          "form": "갑자기 배고프네요",
          "original_form": "갑자기 배고프네요",
          "speaker_id": "1",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100008741.1.20",
          "form": "김밥천국 돈까스먹고싶어요ㅠ",
          "original_form": "김밥천국 돈까스먹고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100008741.1.21",
          "form": "돈까스 맛있죠! 카레돈까스 철판치즈돈까스 그냥 돈까스",
          "original_form": "돈까스 맛있죠! 카레돈까스 철판치즈돈까스 그냥 돈까스",
          "speaker_id": "2",
          "time": "20210824 11:40"
        }
      ]
    }
