      "utterance": [
        {
          "id": "MDRW2100004004.1.1",
          "form": "요즘 보시는거 있으시나요",
          "original_form": "요즘 보시는거 있으시나요",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100004004.1.2",
          "form": "저 한창 슬의생보다가 이제 안봐요..",
          "original_form": "저 한창 슬의생보다가 이제 안봐요..",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004004.1.3",
          "form": "슬의생 1이 더 재밌더라고요",
          "original_form": "슬의생 1이 더 재밌더라고요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004004.1.4",
          "form": "슬의 보셨어요?",
          "original_form": "슬의 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004004.1.5",
          "form": "아뇨 안봤는데",
          "original_form": "아뇨 안봤는데",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004004.1.6",
          "form": "저빼고다보더라고요",
          "original_form": "저빼고다보더라고요",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004004.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004004.1.8",
          "form": "그렇게재밌나요?",
          "original_form": "그렇게재밌나요?",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004004.1.9",
          "form": "시즌1 완전 재밌어요ㅠㅠ",
          "original_form": "시즌1 완전 재밌어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004004.1.10",
          "form": "근데 시즌2는... 2화까지 보고 하차했어요 ㅋㅋㅋㅋ",
          "original_form": "근데 시즌2는... 2화까지 보고 하차했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004004.1.11",
          "form": "요즘 보는 거 있으세요?",
          "original_form": "요즘 보는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004004.1.12",
          "form": "시즌2가 좀 별론가보네요ㅠ",
          "original_form": "시즌2가 좀 별론가보네요ㅠ",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004004.1.13",
          "form": "아뇨 저는 요새 보는게 .. 딱이없구",
          "original_form": "아뇨 저는 요새 보는게 .. 딱이없구",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004004.1.14",
          "form": "걍 유튜브에서 이것저것 보네요",
          "original_form": "걍 유튜브에서 이것저것 보네요",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004004.1.15",
          "form": "아 어떤 유튜버 영상 보세요??",
          "original_form": "아 어떤 유튜버 영상 보세요??",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004004.1.16",
          "form": "저는 예랑가랑 해쭈 이런 유튜버들이 웃기더라구여..",
          "original_form": "저는 예랑가랑 해쭈 이런 유튜버들이 웃기더라구여..",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004004.1.17",
          "form": "헐 저도 해쭈 너무웃겨서 자주봐요",
          "original_form": "헐 저도 해쭈 너무웃겨서 자주봐요",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004004.1.18",
          "form": "요새 해쭈동생분이랑",
          "original_form": "요새 해쭈동생분이랑",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004004.1.19",
          "form": "조카분도 유튜브를 시작해서",
          "original_form": "조카분도 유튜브를 시작해서",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004004.1.20",
          "form": "챙겨보느라 즐거워요..",
          "original_form": "챙겨보느라 즐거워요..",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004004.1.21",
          "form": "헐 해쭈 조카 카야 너무 귀여워요",
          "original_form": "헐 해쭈 조카 카야 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004004.1.22",
          "form": "해쭈 유튜브 보면 저도 기분 좋아지는 거 같아요 가족 분위기가 넘 좋아서요",
          "original_form": "해쭈 유튜브 보면 저도 기분 좋아지는 거 같아요 가족 분위기가 넘 좋아서요",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004004.1.23",
          "form": "맞아요 어떻게이렇게 다같이 사이좋을수있지 부러워요",
          "original_form": "맞아요 어떻게이렇게 다같이 사이좋을수있지 부러워요",
          "speaker_id": "1",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004004.1.24",
          "form": "저도 카야같은조카있으면 엄청 귀여워할거같아요",
          "original_form": "저도 카야같은조카있으면 엄청 귀여워할거같아요",
          "speaker_id": "1",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004004.1.25",
          "form": "헐 저도요ㅠㅠㅠ 카야 완전 귀염둥이..",
          "original_form": "헐 저도요ㅠㅠㅠ 카야 완전 귀염둥이..",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004004.1.26",
          "form": "혿시 유튜브 햄튜브도 보시나요...?",
          "original_form": "혿시 유튜브 햄튜브도 보시나요...?",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004004.1.27",
          "form": "햄튜브는 가끔씩 봐요ㅋㅋ챙겨보진않지만",
          "original_form": "햄튜브는 가끔씩 봐요ㅋㅋ챙겨보진않지만",
          "speaker_id": "1",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004004.1.28",
          "form": "오 저도요 가끔 웃겨요",
          "original_form": "오 저도요 가끔 웃겨요",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004004.1.29",
          "form": "요즘에는 티비보다는 유튜브가 더 재밌는 거 같아요",
          "original_form": "요즘에는 티비보다는 유튜브가 더 재밌는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004004.1.30",
          "form": "맞아요 방송사에서도 유튭 채널 개설하고",
          "original_form": "맞아요 방송사에서도 유튭 채널 개설하고",
          "speaker_id": "1",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004004.1.31",
          "form": "네고왕같은건 지상파보다도 화제성있고",
          "original_form": "네고왕같은건 지상파보다도 화제성있고",
          "speaker_id": "1",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004004.1.32",
          "form": "저부터도 유튜브 더많이보네요",
          "original_form": "저부터도 유튜브 더많이보네요",
          "speaker_id": "1",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004004.1.33",
          "form": "맞아요 저도요. 네고왕은 그리고 실제로 할인혜택같은걸 체감할 수 있어서 더 자주 봤던 거 같아요.",
          "original_form": "맞아요 저도요. 네고왕은 그리고 실제로 할인혜택같은걸 체감할 수 있어서 더 자주 봤던 거 같아요.",
          "speaker_id": "2",
          "time": "20210821 14:50"
        }
      ]
    }
