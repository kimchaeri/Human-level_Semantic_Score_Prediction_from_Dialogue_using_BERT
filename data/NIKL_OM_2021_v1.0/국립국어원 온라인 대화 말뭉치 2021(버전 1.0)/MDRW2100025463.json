      "utterance": [
        {
          "id": "MDRW2100025463.1.1",
          "form": "음식은 머 좋아하시나요?",
          "original_form": "음식은 머 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025463.1.2",
          "form": "치즈 들어간건 다요..ㅎㅎ",
          "original_form": "치즈 들어간건 다요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025463.1.3",
          "form": "치즈 좋아라시는군요...",
          "original_form": "치즈 좋아라시는군요...",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025463.1.4",
          "form": "전떡볶이 좋아해요!",
          "original_form": "전떡볶이 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025463.1.5",
          "form": "떡볶이에 치즈 참 잘어울리죠..ㅎㅎㅎ",
          "original_form": "떡볶이에 치즈 참 잘어울리죠..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025463.1.6",
          "form": "떡복이 브랜드도 많던데 어디꺼 젤 좋아해요?",
          "original_form": "떡복이 브랜드도 많던데 어디꺼 젤 좋아해요?",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025463.1.7",
          "form": "저는 시장께 젤좋긴하더라규요",
          "original_form": "저는 시장께 젤좋긴하더라규요",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025463.1.8",
          "form": "구지찾는다면 치킨을 사랑한 떡볶이 먹어요",
          "original_form": "구지찾는다면 치킨을 사랑한 떡볶이 먹어요",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025463.1.9",
          "form": "어.. 그런 것도 있군요.. 저희 동네는 엽떡 신전 죠스 이런거 있어요",
          "original_form": "어.. 그런 것도 있군요.. 저희 동네는 엽떡 신전 죠스 이런거 있어요",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025463.1.10",
          "form": "다 매운 애들이네요",
          "original_form": "다 매운 애들이네요",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025463.1.11",
          "form": "저가또 맵찔이라",
          "original_form": "저가또 맵찔이라",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025463.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025463.1.13",
          "form": "아~ 매운게 치즈랑 잘 어울리는데요..ㅎㅎㅎㅎ",
          "original_form": "아~ 매운게 치즈랑 잘 어울리는데요..ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025463.1.14",
          "form": "그럼 마라탕도 못먹겠어요? 그거 매운데",
          "original_form": "그럼 마라탕도 못먹겠어요? 그거 매운데",
          "speaker_id": "2",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025463.1.15",
          "form": "근데 또 그건 먹어요!!ㅋㅋ",
          "original_form": "근데 또 그건 먹어요!!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025463.1.16",
          "form": "좋아해서",
          "original_form": "좋아해서",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025463.1.17",
          "form": "신라면맵기정도인게있어서",
          "original_form": "신라면맵기정도인게있어서",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025463.1.18",
          "form": "그런거찾아서 먹어요",
          "original_form": "그런거찾아서 먹어요",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025463.1.19",
          "form": "아.. 그럼 마라탕 순한맛 이렇게 먹나보다",
          "original_form": "아.. 그럼 마라탕 순한맛 이렇게 먹나보다",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025463.1.20",
          "form": "전 최고 매운맛..ㅋㅋㅋㅋ",
          "original_form": "전 최고 매운맛..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025463.1.21",
          "form": "헐 진짜 매운거 잘드시나바요",
          "original_form": "헐 진짜 매운거 잘드시나바요",
          "speaker_id": "1",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025463.1.22",
          "form": "매운건 통각이라 배웠어요!ㅋㅋ",
          "original_form": "매운건 통각이라 배웠어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025463.1.23",
          "form": "그래서 몸에 좋을리는 없는데 왜 맛있는걸까요...ㅠ.ㅠ",
          "original_form": "그래서 몸에 좋을리는 없는데 왜 맛있는걸까요...ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025463.1.24",
          "form": "자극적인건 항상 맛있죠!!ㅋㅋㅋ",
          "original_form": "자극적인건 항상 맛있죠!!ㅋㅋㅋ",
