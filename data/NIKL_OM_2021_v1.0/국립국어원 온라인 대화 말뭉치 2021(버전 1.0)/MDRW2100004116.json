      "utterance": [
        {
          "id": "MDRW2100004116.1.1",
          "form": "영화 최근에 뭐봣어?",
          "original_form": "영화 최근에 뭐봣어?",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004116.1.2",
          "form": "언브로큰봤어요",
          "original_form": "언브로큰봤어요",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004116.1.3",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004116.1.4",
          "form": "못봤어요 재밌나요??",
          "original_form": "못봤어요 재밌나요??",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004116.1.5",
          "form": "안젤리나 졸리감독에 올림픽선수가 일본 포로수용소에서 생활하는 내용인데",
          "original_form": "안젤리나 졸리감독에 올림픽선수가 일본 포로수용소에서 생활하는 내용인데",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004116.1.6",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004116.1.7",
          "form": "와 안젤리나졸리가 감독을 했군요",
          "original_form": "와 안젤리나졸리가 감독을 했군요",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004116.1.8",
          "form": "영화 자주 보러다니세요?",
          "original_form": "영화 자주 보러다니세요?",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004116.1.9",
          "form": "영화관은 정말 보고싶은것만 보러가고",
          "original_form": "영화관은 정말 보고싶은것만 보러가고",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004116.1.10",
          "form": "평소에는 왓챠로 보는거같아요",
          "original_form": "평소에는 왓챠로 보는거같아요",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004116.1.11",
          "form": "집에서 보는것도 좋죠!!",
          "original_form": "집에서 보는것도 좋죠!!",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004116.1.12",
          "form": "저는 영화관 사운드 빵빵한게 좋더라구요",
          "original_form": "저는 영화관 사운드 빵빵한게 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004116.1.13",
          "form": "전시회같은것도 가셔요?",
          "original_form": "전시회같은것도 가셔요?",
          "speaker_id": "2",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004116.1.14",
          "form": "자주 가진않은데  좋아해요!!!",
          "original_form": "자주 가진않은데  좋아해요!!!",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004116.1.15",
          "form": "어떤 주제 전시회 가는거 좋아해요?",
          "original_form": "어떤 주제 전시회 가는거 좋아해요?",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004116.1.16",
          "form": "현대미술이요!! 친구가 좋아해서 따라 다녀요",
          "original_form": "현대미술이요!! 친구가 좋아해서 따라 다녀요",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004116.1.17",
          "form": "한번 가시면 오래보세요?",
          "original_form": "한번 가시면 오래보세요?",
