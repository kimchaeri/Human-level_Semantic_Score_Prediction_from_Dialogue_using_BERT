      "utterance": [
        {
          "id": "MDRW2100038428.1.1",
          "form": "맞아요 다프네!",
          "original_form": "맞아요 다프네!",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100038428.1.2",
          "form": "캐스팅을 정말 잘했어요 ㅎㅎㅎ",
          "original_form": "캐스팅을 정말 잘했어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100038428.1.3",
          "form": "넷플릭스 오리지널 자주 보세요!?",
          "original_form": "넷플릭스 오리지널 자주 보세요!?",
          "speaker_id": "2",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100038428.1.4",
          "form": "꽤 그런 편 같기도 해요",
          "original_form": "꽤 그런 편 같기도 해요",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100038428.1.5",
          "form": "아맞다 저 인간수업도 봤었어요",
          "original_form": "아맞다 저 인간수업도 봤었어요",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100038428.1.6",
          "form": "소재가 참 자극적이었죠...",
          "original_form": "소재가 참 자극적이었죠...",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100038428.1.7",
          "form": "헐 저도요!!!!",
          "original_form": "헐 저도요!!!!",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038428.1.8",
          "form": "배우들이 연기를 잘하더라고요!!!",
          "original_form": "배우들이 연기를 잘하더라고요!!!",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038428.1.9",
          "form": "저 그것도 봤어요! 그녀의 이름은 난노?",
          "original_form": "저 그것도 봤어요! 그녀의 이름은 난노?",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038428.1.10",
          "form": "아시나요?!",
          "original_form": "아시나요?!",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038428.1.11",
          "form": "오 제목 많이 들어봤어요!!!!",
          "original_form": "오 제목 많이 들어봤어요!!!!",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038428.1.12",
          "form": "일본 드라마잖아요?!",
          "original_form": "일본 드라마잖아요?!",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100038428.1.13",
          "form": "제 지인들도 좋아하더라구요!",
          "original_form": "제 지인들도 좋아하더라구요!",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100038428.1.14",
          "form": "앗 태국 드라마요!!",
          "original_form": "앗 태국 드라마요!!",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100038428.1.15",
          "form": "네네 요새 인기가 엄청 많더라고요 ㅎ.ㅎ !!",
          "original_form": "네네 요새 인기가 엄청 많더라고요 ㅎ.ㅎ !!",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100038428.1.16",
          "form": "아 태국이었군요?!?!?!",
          "original_form": "아 태국이었군요?!?!?!",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038428.1.17",
          "form": "아니 저 가끔 태국인들을",
          "original_form": "아니 저 가끔 태국인들을",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038428.1.18",
          "form": "구별을 잘 못하겠어욬ㅋㅋㅋㅋㅋ",
          "original_form": "구별을 잘 못하겠어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038428.1.19",
          "form": "헐 저도요 전 사실 일본인 중국인도 가끔",
          "original_form": "헐 저도요 전 사실 일본인 중국인도 가끔",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100038428.1.20",
          "form": "구별이 안 돼요",
          "original_form": "구별이 안 돼요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100038428.1.21",
          "form": "맞아요 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038428.1.22",
          "form": "제가 케이팝 아이돌 중에서",
          "original_form": "제가 케이팝 아이돌 중에서",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038428.1.23",
          "form": "유난히 시선이 가는 멤버들이 태국 출신이던데",
          "original_form": "유난히 시선이 가는 멤버들이 태국 출신이던데",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038428.1.24",
          "form": "예를 들면 블랙핑크에서는 리사,",
          "original_form": "예를 들면 블랙핑크에서는 리사,",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038428.1.25",
          "form": "엔시티에서는 텐이 눈길이 가더라구요",
          "original_form": "엔시티에서는 텐이 눈길이 가더라구요",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038428.1.26",
          "form": "헐헐 리사 진짜 멋지잖아요 포스가 대박인 것 같아요",
          "original_form": "헐헐 리사 진짜 멋지잖아요 포스가 대박인 것 같아요",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100038428.1.27",
          "form": "그 아이들의 민니도!",
          "original_form": "그 아이들의 민니도!",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100038428.1.28",
          "form": "오 맞아요 맞아요!!!",
          "original_form": "오 맞아요 맞아요!!!",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100038428.1.29",
          "form": "제가 요즘 아이돌은 잘 모르는데",
          "original_form": "제가 요즘 아이돌은 잘 모르는데",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100038428.1.30",
          "form": "눈길 가는 아이돌만 파요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "눈길 가는 아이돌만 파요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038428.1.31",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038428.1.32",
          "form": "어떤 아이돌 좋아하세요?!!",
          "original_form": "어떤 아이돌 좋아하세요?!!",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038428.1.33",
          "form": "저는 뭔가 예전부터",
          "original_form": "저는 뭔가 예전부터",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038428.1.34",
          "form": "스엠을 좋아해왔어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "스엠을 좋아해왔어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038428.1.35",
          "form": "제.... 정서에 맞나봐요!!!",
          "original_form": "제.... 정서에 맞나봐요!!!",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038428.1.36",
          "form": "저 학창시절에는 소녀시대 vs 원더걸스였는데",
          "original_form": "저 학창시절에는 소녀시대 vs 원더걸스였는데",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100038428.1.37",
          "form": "저는 소녀시대 팬이었어요 ㅋㅋㅋㅋ",
          "original_form": "저는 소녀시대 팬이었어요 ㅋㅋㅋㅋ",
