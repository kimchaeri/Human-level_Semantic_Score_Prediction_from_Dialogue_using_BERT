      "utterance": [
        {
          "id": "MDRW2100036907.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100036907.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036907.1.3",
          "form": "오늘 점심 드셨나요?",
          "original_form": "오늘 점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036907.1.4",
          "form": "아니요 아직이요",
          "original_form": "아니요 아직이요",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100036907.1.5",
          "form": "배 안고프세요?ㅠ",
          "original_form": "배 안고프세요?ㅠ",
          "speaker_id": "1",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100036907.1.6",
          "form": "네 크게 고프지 않네요",
          "original_form": "네 크게 고프지 않네요",
          "speaker_id": "2",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036907.1.7",
          "form": "그러고보니 제가 어제 좀 독특한 다이어트방법을 봤어요 식사 규칙적으로 챙겨먹지 않고 배고플때만 끼니를 챙기는 방법이 있다더라고요",
          "original_form": "그러고보니 제가 어제 좀 독특한 다이어트방법을 봤어요 식사 규칙적으로 챙겨먹지 않고 배고플때만 끼니를 챙기는 방법이 있다더라고요",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100036907.1.8",
          "form": "음 그런방법도 있군요",
          "original_form": "음 그런방법도 있군요",
          "speaker_id": "2",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100036907.1.9",
          "form": "다이어트는 될꺼 같은데,",
          "original_form": "다이어트는 될꺼 같은데,",
          "speaker_id": "2",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100036907.1.10",
          "form": "몸에는 안 좋겠죠?ㅎㅎ",
          "original_form": "몸에는 안 좋겠죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036907.1.11",
          "form": "저도 그 생각했는데 꼭 그렇진 않다는 의사 의견을 몇개 봤어요ㅋㅋ 뭐 어차피 할생각은 없지만ㅎㅎ",
          "original_form": "저도 그 생각했는데 꼭 그렇진 않다는 의사 의견을 몇개 봤어요ㅋㅋ 뭐 어차피 할생각은 없지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036907.1.12",
          "form": "그렇군요ㅎㅎ",
          "original_form": "그렇군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100036907.1.13",
          "form": "다이어트 시도해 보신적 있으신가요?",
          "original_form": "다이어트 시도해 보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100036907.1.14",
          "form": "다이어트 시도는 많이 해봣죠ㅎ 하지만 의지가 부족했네요ㅋㅋ",
          "original_form": "다이어트 시도는 많이 해봣죠ㅎ 하지만 의지가 부족했네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100036907.1.15",
          "form": "안타깝네요ㅠㅠ\r\n조금씩 달성하기 쉬운 목표부터 세워서 하는게 좋은 것 같아요",
          "original_form": "안타깝네요ㅠㅠ\r\n조금씩 달성하기 쉬운 목표부터 세워서 하는게 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100036907.1.16",
          "form": "전 운동하려해도 항상 작심삼일로 끝나네요",
          "original_form": "전 운동하려해도 항상 작심삼일로 끝나네요",
          "speaker_id": "1",
          "time": "20210831 17:57"
        }
      ]
