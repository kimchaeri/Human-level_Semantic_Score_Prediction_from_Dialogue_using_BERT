      "utterance": [
        {
          "id": "MDRW2100006788.1.1",
          "form": "저그때 그거에 꽂혀서 자주먹엇어욬ㅋㅋ",
          "original_form": "저그때 그거에 꽂혀서 자주먹엇어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006788.1.2",
          "form": "그러니까요 ㅋㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100006788.1.3",
          "form": "좀 굵은면 라면이 맛있는거 같아요",
          "original_form": "좀 굵은면 라면이 맛있는거 같아요",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100006788.1.4",
          "form": "네네 ㅠㅠ 너구리가 굵죠??",
          "original_form": "네네 ㅠㅠ 너구리가 굵죠??",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100006788.1.5",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100006788.1.6",
          "form": "저 그래서 너구리도 좋아해요",
          "original_form": "저 그래서 너구리도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100006788.1.7",
          "form": "ㅜㅜ 라면 저는 무조건 매운맛으로 먹어욬ㅋ",
          "original_form": "ㅜㅜ 라면 저는 무조건 매운맛으로 먹어욬ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100006788.1.8",
          "form": "저는 두루두루 좋아하는 편인데",
          "original_form": "저는 두루두루 좋아하는 편인데",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100006788.1.9",
          "form": "네내",
          "original_form": "네내",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100006788.1.10",
          "form": "너구리는 순한맛 너무 순한 거 같더라고요 ㅋㅋㅋ",
          "original_form": "너구리는 순한맛 너무 순한 거 같더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100006788.1.11",
          "form": "그쵸그쵸 ㅠㅠ 진라면도 순한맛은 별로에요",
          "original_form": "그쵸그쵸 ㅠㅠ 진라면도 순한맛은 별로에요",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006788.1.12",
          "form": "전 진라면은 둘다 잘먹어요ㅋㅋㅋ",
          "original_form": "전 진라면은 둘다 잘먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100006788.1.13",
          "form": "라면에 김치 먹고싶어요 ㅋㅋㅋ",
          "original_form": "라면에 김치 먹고싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100006788.1.14",
          "form": "으악 저도요 ㅠㅠ",
          "original_form": "으악 저도요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100006788.1.15",
          "form": "열무에다 먹어도 맛잇드라구용",
          "original_form": "열무에다 먹어도 맛잇드라구용",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100006788.1.16",
          "form": "라면 끓일때 뭐 많이 넣어 드시나요??",
          "original_form": "라면 끓일때 뭐 많이 넣어 드시나요??",
          "speaker_id": "2",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100006788.1.17",
          "form": "저는 라면 계란이요!!",
