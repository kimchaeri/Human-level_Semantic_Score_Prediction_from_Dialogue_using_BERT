      "utterance": [
        {
          "id": "MDRW2100030680.1.1",
          "form": "그냥 꽃이 좋대요",
          "original_form": "그냥 꽃이 좋대요",
          "speaker_id": "2",
          "time": "20210904 14:45"
        },
        {
          "id": "MDRW2100030680.1.2",
          "form": "4인가족의 주말은 어떤가여?",
          "original_form": "4인가족의 주말은 어떤가여?",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100030680.1.3",
          "form": "북악스카이 가려고 나왔는ㄷ니ㅣ",
          "original_form": "북악스카이 가려고 나왔는ㄷ니ㅣ",
          "speaker_id": "1",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100030680.1.4",
          "form": "주차장 들어가려면\r\n1시간 걸릴거같아서",
          "original_form": "주차장 들어가려면\r\n1시간 걸릴거같아서",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100030680.1.5",
          "form": "집으로 갑니다 ㅋㅋㅋ",
          "original_form": "집으로 갑니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100030680.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100030680.1.7",
          "form": "1시간요?",
          "original_form": "1시간요?",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100030680.1.8",
          "form": "차들이 끝이 없더라구요 ㅠㅠ",
          "original_form": "차들이 끝이 없더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100030680.1.9",
          "form": "데이트는 잘하셨어요?",
          "original_form": "데이트는 잘하셨어요?",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100030680.1.10",
          "form": "네 방금왔어요  ㅋㅋ",
          "original_form": "네 방금왔어요  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100030680.1.11",
          "form": "일찍일찍 다니셔야죵",
          "original_form": "일찍일찍 다니셔야죵",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100030680.1.12",
          "form": "엄마한테 안 혼나요? ㅋㅋㅋ",
          "original_form": "엄마한테 안 혼나요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100030680.1.13",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100030680.1.14",
          "form": "나이가 몇살인데",
          "original_form": "나이가 몇살인데",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100030680.1.15",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100030680.1.16",
          "form": "나이 많아서....",
          "original_form": "나이 많아서....",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100030680.1.17",
          "form": "아놔 진짜",
          "original_form": "아놔 진짜",
          "speaker_id": "2",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100030680.1.18",
          "form": "그런식으로 하시겠다 이거죠?",
          "original_form": "그런식으로 하시겠다 이거죠?",
          "speaker_id": "2",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100030680.1.19",
          "form": "오늘은 데이트 안하세여?",
          "original_form": "오늘은 데이트 안하세여?",
          "speaker_id": "1",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100030680.1.20",
          "form": "어제 했으니까 오늘은 쉬는날",
          "original_form": "어제 했으니까 오늘은 쉬는날",
          "speaker_id": "2",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100030680.1.21",
          "form": "여자친구 사는 곳이랑은\r\n거리가 멀어요?",
          "original_form": "여자친구 사는 곳이랑은\r\n거리가 멀어요?",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100030680.1.22",
          "form": "차로 40분",
          "original_form": "차로 40분",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100030680.1.23",
          "form": "직선거리 42킬로정도",
          "original_form": "직선거리 42킬로정도",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100030680.1.24",
          "form": "멀죠",
          "original_form": "멀죠",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100030680.1.25",
          "form": "와... 멀군요!",
          "original_form": "와... 멀군요!",
          "speaker_id": "1",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100030680.1.26",
          "form": "뭐 마음의 거리만 가까움 되져ㅋㅋ",
          "original_form": "뭐 마음의 거리만 가까움 되져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100030680.1.27",
          "form": "ㅋㅋㅋㅋ 가까운게 좋은거 같아요",
          "original_form": "ㅋㅋㅋㅋ 가까운게 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210905 11:23"
        }
      ]
    }
