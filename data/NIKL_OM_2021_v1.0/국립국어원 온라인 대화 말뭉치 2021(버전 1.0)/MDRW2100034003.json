      "utterance": [
        {
          "id": "MDRW2100034003.1.1",
          "form": "건강은 잘 챙기고 계신가요 어떠세요",
          "original_form": "건강은 잘 챙기고 계신가요 어떠세요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100034003.1.2",
          "form": "치아 건강 챙겨요",
          "original_form": "치아 건강 챙겨요",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100034003.1.3",
          "form": "오 치과 잘 다니시나요",
          "original_form": "오 치과 잘 다니시나요",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100034003.1.4",
          "form": "네 6개월에서 3개월에 한번요",
          "original_form": "네 6개월에서 3개월에 한번요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034003.1.5",
          "form": "더 저주 가게됐어요",
          "original_form": "더 저주 가게됐어요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034003.1.6",
          "form": "와 엄청 잘 챙기시네요 근데 치과가는거 안 무서우신가요? 전 무서워요 아직 ㅋㅋㅋ",
          "original_form": "와 엄청 잘 챙기시네요 근데 치과가는거 안 무서우신가요? 전 무서워요 아직 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034003.1.7",
          "form": "잇몸이 안좋아서요",
          "original_form": "잇몸이 안좋아서요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034003.1.8",
          "form": "엄청 무서워요",
          "original_form": "엄청 무서워요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034003.1.9",
          "form": "아 잇몸때문에..",
          "original_form": "아 잇몸때문에..",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034003.1.10",
          "form": "눈물찔끔",
          "original_form": "눈물찔끔",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034003.1.11",
          "form": "ㅠㅠㅠ 그 소리가 너무 공포예요 ㅋㅋㅋ",
          "original_form": "ㅠㅠㅠ 그 소리가 너무 공포예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100034003.1.12",
          "form": "맞아요 발이 오므러지고",
          "original_form": "맞아요 발이 오므러지고",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100034003.1.13",
          "form": "ㅜㅜ손꼭 잡고",
          "original_form": "ㅜㅜ손꼭 잡고",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100034003.1.14",
          "form": "아 진짜.. 늦게갈수록 돈깨지는걸 아는데도 ㅋㅋㅋ",
          "original_form": "아 진짜.. 늦게갈수록 돈깨지는걸 아는데도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100034003.1.15",
          "form": "네 그러다 수백 들어요",
          "original_form": "네 그러다 수백 들어요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100034003.1.16",
          "form": "저 임플란트 했어요",
          "original_form": "저 임플란트 했어요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100034003.1.17",
          "form": "임플란트가 신경치료보다 더 아프죠? ㅠ",
          "original_form": "임플란트가 신경치료보다 더 아프죠? ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100034003.1.18",
          "form": "무서웠어요",
          "original_form": "무서웠어요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100034003.1.19",
          "form": "아프기도하고",
          "original_form": "아프기도하고",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100034003.1.20",
          "form": "마취를 왕창하고 ㅜㅜ",
          "original_form": "마취를 왕창하고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100034003.1.21",
          "form": "쇠가 들어간다는 공포가 ㅜㅜ",
          "original_form": "쇠가 들어간다는 공포가 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100034003.1.22",
          "form": "아 ㅠㅠㅠ 그 뼈처럼 심는거요.. 느껴지나요? ㅠㅠ",
          "original_form": "아 ㅠㅠㅠ 그 뼈처럼 심는거요.. 느껴지나요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100034003.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100034003.1.24",
          "form": "잇몸도 소뼈 이식했어요 ㅜㅜ",
          "original_form": "잇몸도 소뼈 이식했어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100034003.1.25",
          "form": "3개월 ㅜㅜ",
          "original_form": "3개월 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100034003.1.26",
          "form": "걸렸어요",
          "original_form": "걸렸어요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100034003.1.27",
          "form": "헐 소뼈 이식도 있군요 몰랐어요 그게 더 좋은가요?",
          "original_form": "헐 소뼈 이식도 있군요 몰랐어요 그게 더 좋은가요?",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034003.1.28",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034003.1.29",
          "form": "제 뼈가 없어서 한거라 ㅜㅜ",
          "original_form": "제 뼈가 없어서 한거라 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034003.1.30",
          "form": "아 저는 인공으로만 이식하는줄 알았어요!! 소뼈는 몰랐네요",
          "original_form": "아 저는 인공으로만 이식하는줄 알았어요!! 소뼈는 몰랐네요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        }
      ]
    }
