      "utterance": [
        {
          "id": "MDRW2100037738.1.1",
          "form": "실내에서 말릴 수 있는거 너무 부러워요",
          "original_form": "실내에서 말릴 수 있는거 너무 부러워요",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037738.1.2",
          "form": "롱패딩 겨울에 안입을 순 없는데",
          "original_form": "롱패딩 겨울에 안입을 순 없는데",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037738.1.3",
          "form": "솔직히 거추장스럽긴 하죠",
          "original_form": "솔직히 거추장스럽긴 하죠",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037738.1.4",
          "form": "사실 부모님이랑 같이 큰 집 사니까 가능한거죠",
          "original_form": "사실 부모님이랑 같이 큰 집 사니까 가능한거죠",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100037738.1.5",
          "form": "저도 뭐 원룸 자취를 한다던가 하면 세탁 맡기지 않을까요",
          "original_form": "저도 뭐 원룸 자취를 한다던가 하면 세탁 맡기지 않을까요",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100037738.1.6",
          "form": "저는 자취 안해도 맡기고 있어요",
          "original_form": "저는 자취 안해도 맡기고 있어요",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100037738.1.7",
          "form": "부모님이 전부 빨래할 수는 없다고 하셔서ㅋㅋ",
          "original_form": "부모님이 전부 빨래할 수는 없다고 하셔서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037738.1.8",
          "form": "그러신가요...?",
          "original_form": "그러신가요...?",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037738.1.9",
          "form": "저랑 반대네요",
          "original_form": "저랑 반대네요",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037738.1.10",
          "form": "전 한번 맡기자 그랬는데",
          "original_form": "전 한번 맡기자 그랬는데",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037738.1.11",
          "form": "패딩은 물빨래 하는거라고 반대하셔서 ㅋㅋ",
          "original_form": "패딩은 물빨래 하는거라고 반대하셔서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037738.1.12",
          "form": "부모님의 성향 때문에 달랐군요",
          "original_form": "부모님의 성향 때문에 달랐군요",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037738.1.13",
          "form": "그러네요.",
          "original_form": "그러네요.",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037738.1.14",
          "form": "이제 제 빨래는 제가 해야하니까 니트도 잘 안입게 되고 그래요",
          "original_form": "이제 제 빨래는 제가 해야하니까 니트도 잘 안입게 되고 그래요",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037738.1.15",
          "form": "겨울엔 니튼데",
          "original_form": "겨울엔 니튼데",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037738.1.16",
          "form": "맞아요 니트가 최고인데 관리하기 귀찮죠",
          "original_form": "맞아요 니트가 최고인데 관리하기 귀찮죠",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037738.1.17",
          "form": "손빨래해야하고",
          "original_form": "손빨래해야하고",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037738.1.18",
          "form": "뉘여서 말려야하고",
          "original_form": "뉘여서 말려야하고",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037738.1.19",
          "form": "가끔 보풀제거해야하고",
          "original_form": "가끔 보풀제거해야하고",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100037738.1.20",
          "form": "말만 들어도 벌써 피곤해요",
          "original_form": "말만 들어도 벌써 피곤해요",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100037738.1.21",
          "form": "그래도 적당히 차려입어야 할거 같은데 정장은 좀 에바일땐 니트만한게 없죠",
          "original_form": "그래도 적당히 차려입어야 할거 같은데 정장은 좀 에바일땐 니트만한게 없죠",
          "speaker_id": "2",
          "time": "20210901 17:58"
        },
        {
          "id": "MDRW2100037738.1.22",
          "form": "맞아요 단정한 느낌으로는 정장 다음인거 같아요",
          "original_form": "맞아요 단정한 느낌으로는 정장 다음인거 같아요",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037738.1.23",
          "form": "겨울 옷이 스타일이 좀 살기는 한데",
          "original_form": "겨울 옷이 스타일이 좀 살기는 한데",
          "speaker_id": "2",
          "time": "20210901 18:11"
        },
        {
          "id": "MDRW2100037738.1.24",
          "form": "관리가 거추장스러워서..  휴",
          "original_form": "관리가 거추장스러워서..  휴",
          "speaker_id": "2",
          "time": "20210901 18:12"
        },
        {
          "id": "MDRW2100037738.1.25",
          "form": "이쁜만큼 힘들죠...",
          "original_form": "이쁜만큼 힘들죠...",
          "speaker_id": "1",
          "time": "20210901 18:15"
        }
      ]
    }
