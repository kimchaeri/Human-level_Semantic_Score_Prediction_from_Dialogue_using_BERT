      "utterance": [
        {
          "id": "MDRW2100020619.1.1",
          "form": "어디가 맛있던가요",
          "original_form": "어디가 맛있던가요",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020619.1.2",
          "form": "쥬씨가 항상 기본은 하는 거 같아요",
          "original_form": "쥬씨가 항상 기본은 하는 거 같아요",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020619.1.3",
          "form": "음 쥬씨 좋지",
          "original_form": "음 쥬씨 좋지",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020619.1.4",
          "form": "개인카페들은 편차가 크더라구요",
          "original_form": "개인카페들은 편차가 크더라구요",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.5",
          "form": "당도는 어떻게 해성드세요",
          "original_form": "당도는 어떻게 해성드세요",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.6",
          "form": "잘하는 집가면 굿굿",
          "original_form": "잘하는 집가면 굿굿",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.7",
          "form": "저 당도 50?",
          "original_form": "저 당도 50?",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.8",
          "form": "약간 달달하게 드시는구나",
          "original_form": "약간 달달하게 드시는구나",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.9",
          "form": "근데 당도를... 말해본 기억이 없어요!",
          "original_form": "근데 당도를... 말해본 기억이 없어요!",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.10",
          "form": "아..!",
          "original_form": "아..!",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.11",
          "form": "주는대로 드시는타입",
          "original_form": "주는대로 드시는타입",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.12",
          "form": "네 그렇습니다",
          "original_form": "네 그렇습니다",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.13",
          "form": "좋습니다 사실저도",
          "original_form": "좋습니다 사실저도",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020619.1.14",
          "form": "주는대로먹어요",
          "original_form": "주는대로먹어요",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020619.1.15",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020619.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ어련히 잘 만들어주시더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ어련히 잘 만들어주시더라구요",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020619.1.17",
          "form": "참외주스 드셔보셨나요",
          "original_form": "참외주스 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020619.1.18",
          "form": "아 안먹어봤는데 맛있을거같네요",
          "original_form": "아 안먹어봤는데 맛있을거같네요",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020619.1.19",
          "form": "참외는 항상 옳좨",
          "original_form": "참외는 항상 옳좨",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020619.1.20",
          "form": "같은 박 종류라 그런가 메로나 맛이 나더라구요",
          "original_form": "같은 박 종류라 그런가 메로나 맛이 나더라구요",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020619.1.21",
          "form": "아주 어릴 땐 메로나만 먹었답니다",
          "original_form": "아주 어릴 땐 메로나만 먹었답니다",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020619.1.22",
          "form": "메로나랑 뽕따..",
          "original_form": "메로나랑 뽕따..",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020619.1.23",
          "form": "예전에 메로나 만들 때 참외넣어서 만들었대요",
          "original_form": "예전에 메로나 만들 때 참외넣어서 만들었대요",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020619.1.24",
          "form": "아.. 그럼이거 해줘야지",
          "original_form": "아.. 그럼이거 해줘야지",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020619.1.25",
          "form": "헉 지금도 그렇지 않나요?",
          "original_form": "헉 지금도 그렇지 않나요?",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020619.1.26",
          "form": "올 때 메로나",
          "original_form": "올 때 메로나",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020619.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ/",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ/",
