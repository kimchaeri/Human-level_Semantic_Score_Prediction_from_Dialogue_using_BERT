      "utterance": [
        {
          "id": "MDRW2100039183.1.1",
          "form": "저는 그마저도 맛나게 먹어요",
          "original_form": "저는 그마저도 맛나게 먹어요",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039183.1.2",
          "form": "두부로 단백질 채우면되지 캄스 ㅋ",
          "original_form": "두부로 단백질 채우면되지 캄스 ㅋ",
          "speaker_id": "2",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100039183.1.3",
          "form": "맛은 있지만 아쉽긴하져ㅋㅋ",
          "original_form": "맛은 있지만 아쉽긴하져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100039183.1.4",
          "form": "저는 배부른소리같이 들려서 ㅋㅋ\r\n올드보이처럼 주5일 짜장면 군만두 먹어보면",
          "original_form": "저는 배부른소리같이 들려서 ㅋㅋ\r\n올드보이처럼 주5일 짜장면 군만두 먹어보면",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039183.1.5",
          "form": "저런소리 안할텐데 이런생각해요 ㅎㅎ",
          "original_form": "저런소리 안할텐데 이런생각해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039183.1.6",
          "form": "ㅋㅋㅋ격어봐야 알아요",
          "original_form": "ㅋㅋㅋ격어봐야 알아요",
          "speaker_id": "1",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039183.1.7",
          "form": "글쵸 ㅎㅎ",
          "original_form": "글쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100039183.1.8",
          "form": "어떤음식 좋아하세요?",
          "original_form": "어떤음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100039183.1.9",
          "form": "저는 매운거 회 이런거",
          "original_form": "저는 매운거 회 이런거",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100039183.1.10",
          "form": "빼고 다 잘먹어요 ㅋ",
          "original_form": "빼고 다 잘먹어요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100039183.1.11",
          "form": "아아ㅋ 전 고기랑 면류 맛나요",
          "original_form": "아아ㅋ 전 고기랑 면류 맛나요",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039183.1.12",
          "form": "둘다 좋죠 ㅎㅎ",
          "original_form": "둘다 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039183.1.13",
          "form": "막창 대창 드세요?",
          "original_form": "막창 대창 드세요?",
          "speaker_id": "2",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039183.1.14",
          "form": "와 너무맛있죠",
          "original_form": "와 너무맛있죠",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039183.1.15",
          "form": "오~~ 맛을 좀아시네요 ㅎ",
          "original_form": "오~~ 맛을 좀아시네요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100039183.1.16",
          "form": "막창 먹어보고 반했는데",
          "original_form": "막창 먹어보고 반했는데",
          "speaker_id": "2",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100039183.1.17",
          "form": "지금 쓰신거만보고 침이 고엿네요ㅋ",
          "original_form": "지금 쓰신거만보고 침이 고엿네요ㅋ",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100039183.1.18",
          "form": "대창먹어보고 우와 이런맛이",
          "original_form": "대창먹어보고 우와 이런맛이",
          "speaker_id": "2",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100039183.1.19",
          "form": "두눈이 번쩍하더라구요 ㅎ",
          "original_form": "두눈이 번쩍하더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100039183.1.20",
          "form": "맞아요\r\n전 곱창전골도 좋아해요",
          "original_form": "맞아요\r\n전 곱창전골도 좋아해요",
