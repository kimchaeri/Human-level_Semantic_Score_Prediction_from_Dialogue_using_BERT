      "utterance": [
        {
          "id": "MDRW2100007251.1.1",
          "form": "요즘 유튜브 뭐봐?",
          "original_form": "요즘 유튜브 뭐봐?",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007251.1.2",
          "form": "축구",
          "original_form": "축구",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007251.1.3",
          "form": "축구하이라이트?",
          "original_form": "축구하이라이트?",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007251.1.4",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007251.1.5",
          "form": "해외축구만 봐?",
          "original_form": "해외축구만 봐?",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007251.1.6",
          "form": "축구는 해외축구만 보고 야구는 국내만 봐",
          "original_form": "축구는 해외축구만 보고 야구는 국내만 봐",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007251.1.7",
          "form": "야구 유튜브는 안봐?",
          "original_form": "야구 유튜브는 안봐?",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007251.1.8",
          "form": "응 알람 설정 안했어 누난 머봐?",
          "original_form": "응 알람 설정 안했어 누난 머봐?",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007251.1.9",
          "form": "난 피식대학도 보고 펭수도 보고",
          "original_form": "난 피식대학도 보고 펭수도 보고",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007251.1.10",
          "form": "음악은 요즘 뭐 많이 들어?",
          "original_form": "음악은 요즘 뭐 많이 들어?",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007251.1.11",
          "form": "나는 류이치사카모토 음악 많이 듣고있어",
          "original_form": "나는 류이치사카모토 음악 많이 듣고있어",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007251.1.12",
          "form": "유명한 사람이야? 뭐하는 사람인데?",
          "original_form": "유명한 사람이야? 뭐하는 사람인데?",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007251.1.13",
          "form": "뉴에이지로 유명한 사람이야",
          "original_form": "뉴에이지로 유명한 사람이야",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007251.1.14",
          "form": "유키구라모토 알아?",
          "original_form": "유키구라모토 알아?",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007251.1.15",
          "form": "아니 잘 몰라",
          "original_form": "아니 잘 몰라",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007251.1.16",
          "form": "히사이시조!",
          "original_form": "히사이시조!",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007251.1.17",
          "form": "히사이시조는 알아",
          "original_form": "히사이시조는 알아",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007251.1.18",
          "form": "그런 류의 음악 작곡하는 사람이야",
          "original_form": "그런 류의 음악 작곡하는 사람이야",
          "speaker_id": "1",
          "time": "20210822 01:09"
        }
      ]
