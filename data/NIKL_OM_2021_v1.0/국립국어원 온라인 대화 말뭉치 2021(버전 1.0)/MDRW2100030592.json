      "utterance": [
        {
          "id": "MDRW2100030592.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 09:39"
        },
        {
          "id": "MDRW2100030592.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030592.1.3",
          "form": "주제가 좀그렇긴하네요",
          "original_form": "주제가 좀그렇긴하네요",
          "speaker_id": "1",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030592.1.4",
          "form": "군대이야기라",
          "original_form": "군대이야기라",
          "speaker_id": "1",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030592.1.5",
          "form": "ㅋㅋ 요즘 넷플에 디피가 인기더라고요",
          "original_form": "ㅋㅋ 요즘 넷플에 디피가 인기더라고요",
          "speaker_id": "2",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030592.1.6",
          "form": "정해인 주연에 군대이야기에요",
          "original_form": "정해인 주연에 군대이야기에요",
          "speaker_id": "2",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030592.1.7",
          "form": "여성이시죠? 저도여자라 군대얘기는",
          "original_form": "여성이시죠? 저도여자라 군대얘기는",
          "speaker_id": "1",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030592.1.8",
          "form": "아..드라마가있조",
          "original_form": "아..드라마가있조",
          "speaker_id": "1",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030592.1.9",
          "form": "네저도 ㅋㅋ",
          "original_form": "네저도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030592.1.10",
          "form": "디피가 저는 재미없던데",
          "original_form": "디피가 저는 재미없던데",
          "speaker_id": "2",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030592.1.11",
          "form": "남자분들은 좋아하시더라고요",
          "original_form": "남자분들은 좋아하시더라고요",
          "speaker_id": "2",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030592.1.12",
          "form": "저도 관심이없어서",
          "original_form": "저도 관심이없어서",
          "speaker_id": "1",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030592.1.13",
          "form": "재미도없고..",
          "original_form": "재미도없고..",
          "speaker_id": "1",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030592.1.14",
          "form": "군대는 갔다온사람들만 신난듯해요..",
          "original_form": "군대는 갔다온사람들만 신난듯해요..",
          "speaker_id": "1",
          "time": "20210904 09:42"
        },
        {
          "id": "MDRW2100030592.1.15",
          "form": "얘기하면 끝도없더라구요",
          "original_form": "얘기하면 끝도없더라구요",
          "speaker_id": "1",
          "time": "20210904 09:42"
        },
        {
          "id": "MDRW2100030592.1.16",
          "form": "전 너무재미없어요..",
          "original_form": "전 너무재미없어요..",
          "speaker_id": "1",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030592.1.17",
          "form": "여자들은 모두 그런것같아요",
          "original_form": "여자들은 모두 그런것같아요",
          "speaker_id": "1",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030592.1.18",
          "form": "아닌가? 저만그런가?",
          "original_form": "아닌가? 저만그런가?",
          "speaker_id": "1",
          "time": "20210904 09:50"
        },
        {
          "id": "MDRW2100030592.1.19",
          "form": "군대는 다녀온사람들의 이야기거리~~ㅎㅎ",
          "original_form": "군대는 다녀온사람들의 이야기거리~~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 09:50"
        },
        {
          "id": "MDRW2100030592.1.20",
          "form": "맞아요 저도 잘 모르겠어요",
          "original_form": "맞아요 저도 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030592.1.21",
          "form": "좀 안타까운 일이 많잖아요ㅠㅠ",
          "original_form": "좀 안타까운 일이 많잖아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030592.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030592.1.23",
          "form": "죽는사람도많고",
          "original_form": "죽는사람도많고",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030592.1.24",
          "form": "그러게요 여러모로 위험한 집단",
          "original_form": "그러게요 여러모로 위험한 집단",
          "speaker_id": "2",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030592.1.25",
          "form": "고생도 많고요ㅠㅠ",
          "original_form": "고생도 많고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030592.1.26",
          "form": "실험대상이기도하고",
          "original_form": "실험대상이기도하고",
          "speaker_id": "1",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030592.1.27",
          "form": "에휴ㅠㅠ 밥도 부실하고 안타까워요",
          "original_form": "에휴ㅠㅠ 밥도 부실하고 안타까워요",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030592.1.28",
          "form": "젤 잘해줘야하는 집단인데",
          "original_form": "젤 잘해줘야하는 집단인데",
          "speaker_id": "2",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030592.1.29",
          "form": "아들있는데 군대보내기무서워요",
          "original_form": "아들있는데 군대보내기무서워요",
          "speaker_id": "1",
          "time": "20210904 10:06"
        }
      ]
    }
