      "utterance": [
        {
          "id": "MDRW2100020530.1.1",
          "form": "아비꼬인가 야비꼬카레",
          "original_form": "아비꼬인가 야비꼬카레",
          "speaker_id": "2",
          "time": "20210828 09:09"
        },
        {
          "id": "MDRW2100020530.1.2",
          "form": "그 카레 너무 맛있었어요",
          "original_form": "그 카레 너무 맛있었어요",
          "speaker_id": "2",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100020530.1.3",
          "form": "저 혹시 그만 하고 싶으시면 말하주세요.\n억지로 하시는 걸 까봐..\n쿨하게 보내드릴테니 꼭 말해주세요",
          "original_form": "저 혹시 그만 하고 싶으시면 말하주세요.\n억지로 하시는 걸 까봐..\n쿨하게 보내드릴테니 꼭 말해주세요",
          "speaker_id": "1",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100020530.1.4",
          "form": "아비꼬 너무 맛있어요",
          "original_form": "아비꼬 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100020530.1.5",
          "form": "거기약간 카레전문점이라 더 맛있는거 같아요",
          "original_form": "거기약간 카레전문점이라 더 맛있는거 같아요",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100020530.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ아니 그걸 걱정하고 계셨나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니 그걸 걱정하고 계셨나요",
          "speaker_id": "2",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100020530.1.7",
          "form": "저는... 아마 어느 순간 사라졌다가",
          "original_form": "저는... 아마 어느 순간 사라졌다가",
          "speaker_id": "2",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.8",
          "form": "어느 순간 다시 나타날 거예요...",
          "original_form": "어느 순간 다시 나타날 거예요...",
          "speaker_id": "2",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.9",
          "form": "가끔ㅋㅋㅋ너무 몰아붙혀서 질려하시는 분 계셔서",
          "original_form": "가끔ㅋㅋㅋ너무 몰아붙혀서 질려하시는 분 계셔서",
          "speaker_id": "1",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.10",
          "form": "저도 그런 타입입니다",
          "original_form": "저도 그런 타입입니다",
          "speaker_id": "1",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.11",
          "form": "후후 걱정안하셔도 됩니다",
          "original_form": "후후 걱정안하셔도 됩니다",
          "speaker_id": "2",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.12",
          "form": "제가 사라지면 아 어디 일있어서 잡혀갔구나 생각하셔요",
          "original_form": "제가 사라지면 아 어디 일있어서 잡혀갔구나 생각하셔요",
          "speaker_id": "2",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.13",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.14",
          "form": "전 다 이해합니다",
          "original_form": "전 다 이해합니다",
          "speaker_id": "1",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020530.1.15",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020530.1.16",
          "form": "그 돈까스 좋아하세요",
          "original_form": "그 돈까스 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020530.1.17",
          "form": "네 돈까스 엄청 좋아해요",
          "original_form": "네 돈까스 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020530.1.18",
          "form": "종류불문 좋아합니다",
          "original_form": "종류불문 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020530.1.19",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020530.1.20",
          "form": "근데 신포우리꺼는 못먹게써요 어제 첨 먹었는데",
          "original_form": "근데 신포우리꺼는 못먹게써요 어제 첨 먹었는데",
          "speaker_id": "1",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020530.1.21",
          "form": "돈까스라고.. 할수있나 싶던 맛..",
          "original_form": "돈까스라고.. 할수있나 싶던 맛..",
          "speaker_id": "1",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020530.1.22",
          "form": "엇 거기 돈까스는 뭐가 달랐나요",
          "original_form": "엇 거기 돈까스는 뭐가 달랐나요",
          "speaker_id": "2",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020530.1.23",
          "form": "전 엄청 얇은데는 싫더라구요",
          "original_form": "전 엄청 얇은데는 싫더라구요",
          "speaker_id": "2",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020530.1.24",
          "form": "아니요 거긴 빵튀김을 가장한 ..",
          "original_form": "아니요 거긴 빵튀김을 가장한 ..",
          "speaker_id": "1",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020530.1.25",
          "form": "사진 한번 보고 오겠어요",
          "original_form": "사진 한번 보고 오겠어요",
          "speaker_id": "2",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020530.1.26",
          "form": "돈까스..라고 말하기도.. 아까운",
          "original_form": "돈까스..라고 말하기도.. 아까운",
          "speaker_id": "1",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020530.1.27",
          "form": "사진은 엄청 바삭하고 잘되어있는데 이건 먹어봐야 아.. 이걸 돈까스라고... 생각하게됩니다",
          "original_form": "사진은 엄청 바삭하고 잘되어있는데 이건 먹어봐야 아.. 이걸 돈까스라고... 생각하게됩니다",
          "speaker_id": "1",
          "time": "20210828 09:14"
        }
      ]
