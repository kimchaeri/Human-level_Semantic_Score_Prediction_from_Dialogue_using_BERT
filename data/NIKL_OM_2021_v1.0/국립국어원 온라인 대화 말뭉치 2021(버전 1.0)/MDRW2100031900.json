      "utterance": [
        {
          "id": "MDRW2100031900.1.1",
          "form": "운동같은거 하세용?",
          "original_form": "운동같은거 하세용?",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100031900.1.2",
          "form": "아니요 지금은 안 해요 예전에 수영 좀 했었어요",
          "original_form": "아니요 지금은 안 해요 예전에 수영 좀 했었어요",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100031900.1.3",
          "form": "오 좋은 운동입니다",
          "original_form": "오 좋은 운동입니다",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100031900.1.4",
          "form": "저도 수영을 했었죠",
          "original_form": "저도 수영을 했었죠",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100031900.1.5",
          "form": "오래 벌만 있으면 바다를 떠다니고 도 했죠",
          "original_form": "오래 벌만 있으면 바다를 떠다니고 도 했죠",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.6",
          "form": "오리발",
          "original_form": "오리발",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.7",
          "form": "오 바다 수영도 하셨나요",
          "original_form": "오 바다 수영도 하셨나요",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.8",
          "form": "작살 들고 물고기도 잡고 했어요",
          "original_form": "작살 들고 물고기도 잡고 했어요",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.9",
          "form": "내 바다 수영 좋아했습니다 크크",
          "original_form": "내 바다 수영 좋아했습니다 크크",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.10",
          "form": "바다는 무서워 안가 봤어요",
          "original_form": "바다는 무서워 안가 봤어요",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.11",
          "form": "운동을 좋아해서",
          "original_form": "운동을 좋아해서",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.12",
          "form": "바다 너무 예뻐요",
          "original_form": "바다 너무 예뻐요",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.13",
          "form": "저는 바다 말고 실내 수영장 2미터 풀 같은 거 하면 재밌더라고요",
          "original_form": "저는 바다 말고 실내 수영장 2미터 풀 같은 거 하면 재밌더라고요",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.14",
          "form": "오래 하셨으면",
          "original_form": "오래 하셨으면",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.15",
          "form": "수심 깊은 데서 수영하면 수영이 잘되는 거 같아요",
          "original_form": "수심 깊은 데서 수영하면 수영이 잘되는 거 같아요",
          "speaker_id": "2",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031900.1.16",
          "form": "수심이 깊으면 더 잘 뜨는 게 정상입니다",
          "original_form": "수심이 깊으면 더 잘 뜨는 게 정상입니다",
          "speaker_id": "1",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031900.1.17",
          "form": "확실히 그랬어요 그게 느껴져요",
          "original_form": "확실히 그랬어요 그게 느껴져요",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031900.1.18",
          "form": "수영 말고 딴 거는 안하시나요",
          "original_form": "수영 말고 딴 거는 안하시나요",
          "speaker_id": "1",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031900.1.19",
          "form": "아 그리고",
          "original_form": "아 그리고",
          "speaker_id": "1",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031900.1.20",
          "form": "수영 오래 하셨으면",
          "original_form": "수영 오래 하셨으면",
          "speaker_id": "1",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031900.1.21",
          "form": "라이프가드 한번 해보시죠",
          "original_form": "라이프가드 한번 해보시죠",
