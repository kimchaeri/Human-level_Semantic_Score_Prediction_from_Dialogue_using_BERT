      "utterance": [
        {
          "id": "MDRW2100025559.1.1",
          "form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "original_form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100025559.1.2",
          "form": "예전에 요크셔 한녀석 있었지요",
          "original_form": "예전에 요크셔 한녀석 있었지요",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100025559.1.3",
          "form": "반려동물 키우시나봐요?",
          "original_form": "반려동물 키우시나봐요?",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100025559.1.4",
          "form": "아니요 키우는 아이는 없는데",
          "original_form": "아니요 키우는 아이는 없는데",
          "speaker_id": "1",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025559.1.5",
          "form": "있으신분들 이야기 듣는것도 좋아하고 ㅋㅋ",
          "original_form": "있으신분들 이야기 듣는것도 좋아하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025559.1.6",
          "form": "동물 다 이쁘죠!",
          "original_form": "동물 다 이쁘죠!",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025559.1.7",
          "form": "밥주는 길냥이는 있어요",
          "original_form": "밥주는 길냥이는 있어요",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025559.1.8",
          "form": "하얀색에 갈색 무늬가 드문드문ㅎ",
          "original_form": "하얀색에 갈색 무늬가 드문드문ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025559.1.9",
          "form": "어머 너무 예쁘겠다",
          "original_form": "어머 너무 예쁘겠다",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100025559.1.10",
          "form": "한참 거리두기 중이신가요",
          "original_form": "한참 거리두기 중이신가요",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100025559.1.11",
          "form": "아님 가끔 와서 애교도부려주나요?",
          "original_form": "아님 가끔 와서 애교도부려주나요?",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100025559.1.12",
          "form": "애교만점이에요",
          "original_form": "애교만점이에요",
          "speaker_id": "2",
          "time": "20210901 13:25"
        },
        {
          "id": "MDRW2100025559.1.13",
          "form": "동네 냥이들이 친화적이에요ㅎ",
          "original_form": "동네 냥이들이 친화적이에요ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:25"
        },
        {
          "id": "MDRW2100025559.1.14",
          "form": "언젠가 입양할거에요",
          "original_form": "언젠가 입양할거에요",
          "speaker_id": "2",
          "time": "20210901 13:25"
        },
        {
          "id": "MDRW2100025559.1.15",
          "form": "오 강아지라 아니라 고양이를 키우고 싶으신거군요!",
          "original_form": "오 강아지라 아니라 고양이를 키우고 싶으신거군요!",
          "speaker_id": "1",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100025559.1.16",
          "form": "유기견아니면 길냥이요",
          "original_form": "유기견아니면 길냥이요",
          "speaker_id": "2",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100025559.1.17",
          "form": "저는 고양이였다가 최근엔 또 강아지들이 예쁘더라고요",
          "original_form": "저는 고양이였다가 최근엔 또 강아지들이 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100025559.1.18",
          "form": "네 다이뻐요 동물들은^^",
          "original_form": "네 다이뻐요 동물들은^^",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025559.1.19",
          "form": "그러고보니 며칠전에 김희철인가 기사뜬거보셨나요",
          "original_form": "그러고보니 며칠전에 김희철인가 기사뜬거보셨나요",
          "speaker_id": "1",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025559.1.20",
          "form": "전문가들은 절대 유기견 추천안한다 라고했던 ㄷㄷ",
          "original_form": "전문가들은 절대 유기견 추천안한다 라고했던 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025559.1.21",
          "form": "혹시 반려동물 자주 바뀐다는?",
          "original_form": "혹시 반려동물 자주 바뀐다는?",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025559.1.22",
          "form": "자세히는 못봤는데",
          "original_form": "자세히는 못봤는데",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025559.1.23",
          "form": "저한텐 그런분들 비호감이에요",
          "original_form": "저한텐 그런분들 비호감이에요",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025559.1.24",
          "form": "최애 바뀌듯이 ㅋㅋ고양이사진보면 나만 고양이없어ㅜㅜㅜ",
          "original_form": "최애 바뀌듯이 ㅋㅋ고양이사진보면 나만 고양이없어ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025559.1.25",
          "form": "요새는 또 노곤하개보면서 하 산책가고싶다ㅜㅜ",
          "original_form": "요새는 또 노곤하개보면서 하 산책가고싶다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025559.1.26",
          "form": "책임감때문에 망설여져요",
          "original_form": "책임감때문에 망설여져요",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025559.1.27",
          "form": "동물은 힐링이에요",
          "original_form": "동물은 힐링이에요",
          "speaker_id": "2",
          "time": "20210901 13:30"
        }
      ]
    }
