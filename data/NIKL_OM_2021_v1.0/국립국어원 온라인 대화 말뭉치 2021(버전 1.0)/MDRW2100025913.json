      "utterance": [
        {
          "id": "MDRW2100025913.1.1",
          "form": "불닭게티 맛있죠ㅜㅠ!!!",
          "original_form": "불닭게티 맛있죠ㅜㅠ!!!",
          "speaker_id": "1",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025913.1.2",
          "form": "맛아요 너무맛있어요\r\n전주로 라면은 진라면먹어요",
          "original_form": "맛아요 너무맛있어요\r\n전주로 라면은 진라면먹어요",
          "speaker_id": "2",
          "time": "20210901 17:58"
        },
        {
          "id": "MDRW2100025913.1.3",
          "form": "순한맛드세요 매운맛드세요?",
          "original_form": "순한맛드세요 매운맛드세요?",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100025913.1.4",
          "form": "매운맛이요ㅋ",
          "original_form": "매운맛이요ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100025913.1.5",
          "form": "저도 진라면은 매운맛이요",
          "original_form": "저도 진라면은 매운맛이요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100025913.1.6",
          "form": "매운맛이 짱이죠!",
          "original_form": "매운맛이 짱이죠!",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100025913.1.7",
          "form": "맞아여 신라면 먹다 맛변해서 갈아탓어여",
          "original_form": "맞아여 신라면 먹다 맛변해서 갈아탓어여",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025913.1.8",
          "form": "신라면 맛이 많이 변했나요?",
          "original_form": "신라면 맛이 많이 변했나요?",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025913.1.9",
          "form": "신라면 자주안먹어서 몰라서요",
          "original_form": "신라면 자주안먹어서 몰라서요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025913.1.10",
          "form": "예전엔 그거만 먹엇는데 어느순간 다르더라고요",
          "original_form": "예전엔 그거만 먹엇는데 어느순간 다르더라고요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025913.1.11",
          "form": "와 신기하네요",
          "original_form": "와 신기하네요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100025913.1.12",
          "form": "전에 블랙먹었는데 별로여서",
          "original_form": "전에 블랙먹었는데 별로여서",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100025913.1.13",
          "form": "그뒤로 안먹었던기억이나요",
          "original_form": "그뒤로 안먹었던기억이나요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100025913.1.14",
          "form": "이번에 신라면볶음면 나와서 주문해봤어요 맛있길",
          "original_form": "이번에 신라면볶음면 나와서 주문해봤어요 맛있길",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100025913.1.15",
          "form": "오오오 맞아요 유튜브에 자주 나오더라고요",
          "original_form": "오오오 맞아요 유튜브에 자주 나오더라고요",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100025913.1.16",
          "form": "네 요즘 광고 많이 나오더라고요",
          "original_form": "네 요즘 광고 많이 나오더라고요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100025913.1.17",
          "form": "아 광고구나ㅋㅋㅋ",
          "original_form": "아 광고구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100025913.1.18",
          "form": "신라면볶음면이 다른 라면들이랑 조합이 좋대요",
          "original_form": "신라면볶음면이 다른 라면들이랑 조합이 좋대요",
          "speaker_id": "1",
          "time": "20210901 23:18"
        }
