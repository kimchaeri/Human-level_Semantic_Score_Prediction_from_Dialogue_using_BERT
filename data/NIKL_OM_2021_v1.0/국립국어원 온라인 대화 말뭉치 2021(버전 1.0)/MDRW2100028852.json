      "utterance": [
        {
          "id": "MDRW2100028852.1.1",
          "form": "그럴 때일수록 운동을 해야 하는 거 같아요 ㅋㅋㅋ",
          "original_form": "그럴 때일수록 운동을 해야 하는 거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028852.1.2",
          "form": "겨울은 운동하기 좋은 날씨죠",
          "original_form": "겨울은 운동하기 좋은 날씨죠",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028852.1.3",
          "form": "name2님 운동 하시는 거 있으신가요?",
          "original_form": "&name2&님 운동 하시는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100028852.1.4",
          "form": "따로 운동하는 건 없고 가끔 걷기 하고 있어요",
          "original_form": "따로 운동하는 건 없고 가끔 걷기 하고 있어요",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100028852.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100028852.1.6",
          "form": "저는 헬스피티 받고 있어요!",
          "original_form": "저는 헬스피티 받고 있어요!",
          "speaker_id": "1",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100028852.1.7",
          "form": "시작한 지는 얼마 안 됐네여",
          "original_form": "시작한 지는 얼마 안 됐네여",
          "speaker_id": "1",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100028852.1.8",
          "form": "호옥 재미있나요?",
          "original_form": "호옥 재미있나요?",
          "speaker_id": "2",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100028852.1.9",
          "form": "재미보단 유용한 거 같아요!",
          "original_form": "재미보단 유용한 거 같아요!",
          "speaker_id": "1",
          "time": "20210903 14:07"
        },
        {
          "id": "MDRW2100028852.1.10",
          "form": "주기적으로 운동 나갈 수도 있고요...",
          "original_form": "주기적으로 운동 나갈 수도 있고요...",
          "speaker_id": "1",
          "time": "20210903 14:08"
        },
        {
          "id": "MDRW2100028852.1.11",
          "form": "아 그렇겠네요! 규칙적으로 꾸준히 운동 하는게 중요하니까...",
          "original_form": "아 그렇겠네요! 규칙적으로 꾸준히 운동 하는게 중요하니까...",
          "speaker_id": "2",
          "time": "20210903 14:12"
        },
        {
          "id": "MDRW2100028852.1.12",
          "form": "그쵸 복싱도 했었어요!",
          "original_form": "그쵸 복싱도 했었어요!",
          "speaker_id": "1",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100028852.1.13",
          "form": "헉 복싱이요? 주변 지인분들이 종종 하긴 하던데 복싱은 좀 재밌나요?",
          "original_form": "헉 복싱이요? 주변 지인분들이 종종 하긴 하던데 복싱은 좀 재밌나요?",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100028852.1.14",
          "form": "복싱 정말 재밌어요!",
          "original_form": "복싱 정말 재밌어요!",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100028852.1.15",
          "form": "코로나 때문에 그만두긴 했지만요...",
          "original_form": "코로나 때문에 그만두긴 했지만요...",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100028852.1.16",
          "form": "아이고 정말 코로나가 문제네요 ㅠ.ㅠ",
          "original_form": "아이고 정말 코로나가 문제네요 ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100028852.1.17",
          "form": "복싱 배우면 주로 어떤 걸 하나요?",
          "original_form": "복싱 배우면 주로 어떤 걸 하나요?",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100028852.1.18",
          "form": "스텝도 뛰는데 주로 잽을 배워요!",
          "original_form": "스텝도 뛰는데 주로 잽을 배워요!",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100028852.1.19",
          "form": "팔은 어떻게 뻗는지...등등",
          "original_form": "팔은 어떻게 뻗는지...등등",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100028852.1.20",
          "form": "샌드백 치는 게 넘 재밌어요",
          "original_form": "샌드백 치는 게 넘 재밌어요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100028852.1.21",
          "form": "호올 샌드백 치는게 재밌군요! 맨날 어디 매달려있는것만 봐서 잘 몰랐는데",
          "original_form": "호올 샌드백 치는게 재밌군요! 맨날 어디 매달려있는것만 봐서 잘 몰랐는데",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100028852.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ네 그게 치는 거에도 리듬이 있어서 게임하는 기분이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네 그게 치는 거에도 리듬이 있어서 게임하는 기분이에요",
          "speaker_id": "1",
          "time": "20210904 22:23"
        }
      ]
