        },
        {
          "id": "MDRW2100030507.1.2",
          "form": "게임방송은 주로 누구 보시나요?!",
          "original_form": "게임방송은 주로 누구 보시나요?!",
          "speaker_id": "2",
          "time": "20210904 02:23"
        },
        {
          "id": "MDRW2100030507.1.3",
          "form": "저는 선바만 알았는데",
          "original_form": "저는 선바만 알았는데",
          "speaker_id": "2",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.4",
          "form": "최근에 일 터져서;;;",
          "original_form": "최근에 일 터져서;;;",
          "speaker_id": "2",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.5",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.6",
          "form": "저 선바말하려다가 망설였는데",
          "original_form": "저 선바말하려다가 망설였는데",
          "speaker_id": "1",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.8",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.9",
          "form": "원래 엄청 봤었는데 종종이라고말한것도",
          "original_form": "원래 엄청 봤었는데 종종이라고말한것도",
          "speaker_id": "1",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.10",
          "form": "최근에는 안봐서그랬거든요",
          "original_form": "최근에는 안봐서그랬거든요",
          "speaker_id": "1",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.11",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.12",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.13",
          "form": "아니근데 너무 빨리",
          "original_form": "아니근데 너무 빨리",
          "speaker_id": "2",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.14",
          "form": "원래는 달고살았어요",
          "original_form": "원래는 달고살았어요",
          "speaker_id": "1",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.16",
          "form": "복귀했던데....",
          "original_form": "복귀했던데....",
          "speaker_id": "2",
          "time": "20210904 02:24"
        },
        {
          "id": "MDRW2100030507.1.17",
          "form": "네네 제 지인들도...",
          "original_form": "네네 제 지인들도...",
          "speaker_id": "2",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030507.1.18",
          "form": "그렇다 하더라구요",
          "original_form": "그렇다 하더라구요",
          "speaker_id": "2",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030507.1.19",
          "form": "근데 너무 인생 쉽게 사는거같아서 현타와요ㅜㅜㅜㅋㅋㅋㅋㅋ하",
          "original_form": "근데 너무 인생 쉽게 사는거같아서 현타와요ㅜㅜㅜㅋㅋㅋㅋㅋ하",
          "speaker_id": "2",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030507.1.20",
          "form": "ㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋ그러게요",
          "speaker_id": "1",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030507.1.21",
          "form": "그래도 매일같이방송하던 사람이",
          "original_form": "그래도 매일같이방송하던 사람이",
          "speaker_id": "1",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030507.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030507.1.23",
          "form": "방송은 계속 쉬고있으니까요",
          "original_form": "방송은 계속 쉬고있으니까요",
          "speaker_id": "1",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030507.1.24",
          "form": "아 그럼 영상만 올라오나요?!",
          "original_form": "아 그럼 영상만 올라오나요?!",
          "speaker_id": "2",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.25",
          "form": "방송수입은 끊겼을것같더라구요",
          "original_form": "방송수입은 끊겼을것같더라구요",
          "speaker_id": "1",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.26",
          "form": "네네 옛날에 방송했던것들 편집본 올라오더라구요",
          "original_form": "네네 옛날에 방송했던것들 편집본 올라오더라구요",
          "speaker_id": "1",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.27",
          "form": "옛날이라해봤자 8월이지만",
          "original_form": "옛날이라해봤자 8월이지만",
          "speaker_id": "1",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.28",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.29",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.30",
          "form": "아혹시 좋아하는 아이돌있으세요?",
          "original_form": "아혹시 좋아하는 아이돌있으세요?",
          "speaker_id": "1",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.31",
          "form": "네네!! 저는",
          "original_form": "네네!! 저는",
          "speaker_id": "2",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030507.1.32",
          "form": "스엠 애들 좋아해요!!!",
          "original_form": "스엠 애들 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100030507.1.33",
          "form": "근데 에스파는 아직 낯가려요,,,",
          "original_form": "근데 에스파는 아직 낯가려요,,,",
