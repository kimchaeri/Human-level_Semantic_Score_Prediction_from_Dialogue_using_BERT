      "utterance": [
        {
          "id": "MDRW2100040717.1.1",
          "form": "쌀로새발견하면말해주기",
          "original_form": "쌀로새발견하면말해주기",
          "speaker_id": "1",
          "time": "20210905 00:36"
        },
        {
          "id": "MDRW2100040717.1.2",
          "form": "오케ㅋㅋ 난지금에프기로 탕슉돌림",
          "original_form": "오케ㅋㅋ 난지금에프기로 탕슉돌림",
          "speaker_id": "2",
          "time": "20210905 00:36"
        },
        {
          "id": "MDRW2100040717.1.3",
          "form": "야 너무 부럽다",
          "original_form": "야 너무 부럽다",
          "speaker_id": "1",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100040717.1.4",
          "form": "탕수육뭐삼?",
          "original_form": "탕수육뭐삼?",
          "speaker_id": "1",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100040717.1.5",
          "form": "아니배달ㅋㅋ 애들이 어제 먹고싶다그래서 시켰는데 남아서 ㅋㅋ",
          "original_form": "아니배달ㅋㅋ 애들이 어제 먹고싶다그래서 시켰는데 남아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:38"
        },
        {
          "id": "MDRW2100040717.1.6",
          "form": "난또 냉동 괜찮은거있는줄",
          "original_form": "난또 냉동 괜찮은거있는줄",
          "speaker_id": "1",
          "time": "20210905 00:39"
        },
        {
          "id": "MDRW2100040717.1.7",
          "form": "나 생선까스먹고싶다",
          "original_form": "나 생선까스먹고싶다",
          "speaker_id": "1",
          "time": "20210905 00:39"
        },
        {
          "id": "MDRW2100040717.1.8",
          "form": "옛날에 배터지는 생돈까스에서 생선까스 먹어보고 안먹어봄-ㅋㅋ",
          "original_form": "옛날에 배터지는 생돈까스에서 생선까스 먹어보고 안먹어봄-ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:40"
        },
        {
          "id": "MDRW2100040717.1.9",
          "form": "이야 추억의 이름",
          "original_form": "이야 추억의 이름",
          "speaker_id": "1",
          "time": "20210905 00:41"
        },
        {
          "id": "MDRW2100040717.1.10",
          "form": "그거이제없지?",
          "original_form": "그거이제없지?",
          "speaker_id": "1",
          "time": "20210905 00:41"
        },
        {
          "id": "MDRW2100040717.1.11",
          "form": "아직있을껄?ㅋㅋㄱ",
          "original_form": "아직있을껄?ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:44"
        },
        {
          "id": "MDRW2100040717.1.12",
          "form": "에이진짜.??? 거기 쫄면맛있는데",
          "original_form": "에이진짜.??? 거기 쫄면맛있는데",
          "speaker_id": "1",
          "time": "20210905 00:45"
        },
        {
          "id": "MDRW2100040717.1.13",
          "form": "우리딩네는 없어지고 전히동인가는 있을껄?ㅋㄱ",
          "original_form": "우리딩네는 없어지고 전히동인가는 있을껄?ㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:45"
        },
        {
          "id": "MDRW2100040717.1.14",
          "form": "아 배달을 안시키면 이런게 불편하다 ㅋㅋ",
          "original_form": "아 배달을 안시키면 이런게 불편하다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:46"
        },
        {
          "id": "MDRW2100040717.1.15",
          "form": "ㅋㅋㅋ우리동네있을때 자주시켜먹었는데",
          "original_form": "ㅋㅋㅋ우리동네있을때 자주시켜먹었는데",
          "speaker_id": "2",
          "time": "20210905 00:46"
        },
        {
          "id": "MDRW2100040717.1.16",
          "form": "나도나도 ㅋㅋㅋ 딱 국물까지 주잖아 ㅋㅋㅋ아직도기억남ㅋㅋㅋ",
          "original_form": "나도나도 ㅋㅋㅋ 딱 국물까지 주잖아 ㅋㅋㅋ아직도기억남ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:48"
        },
        {
          "id": "MDRW2100040717.1.17",
          "form": "ㅋㅋㅋㄱ우리동네는 배달인해주던데ㅜㅜ지금은 모르겠다 먹고싶다",
          "original_form": "ㅋㅋㅋㄱ우리동네는 배달인해주던데ㅜㅜ지금은 모르겠다 먹고싶다",
          "speaker_id": "2",
          "time": "20210905 00:49"
        },
        {
          "id": "MDRW2100040717.1.18",
          "form": "돈까스맛집 진짜없지않냐",
          "original_form": "돈까스맛집 진짜없지않냐",
          "speaker_id": "1",
