      "utterance": [
        {
          "id": "MDRW2100040580.1.1",
          "form": "전에웹툰에서 상사가 이거고쳐! 해서 고치니 내가 하라는 대로만 고치면 너는 여기 왜있니! 더 창의적으로 고쳐야지! 하면서",
          "original_form": "전에웹툰에서 상사가 이거고쳐! 해서 고치니 내가 하라는 대로만 고치면 너는 여기 왜있니! 더 창의적으로 고쳐야지! 하면서",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040580.1.2",
          "form": "엄청 괴롭히던 걸 보며 ㄷㄷㄷ했었던 기억이...",
          "original_form": "엄청 괴롭히던 걸 보며 ㄷㄷㄷ했었던 기억이...",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040580.1.3",
          "form": "ㅋㅋㅋㅋ 충분히 있을법 해요",
          "original_form": "ㅋㅋㅋㅋ 충분히 있을법 해요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040580.1.4",
          "form": "일하는게 되게 시시해보이거든요",
          "original_form": "일하는게 되게 시시해보이거든요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040580.1.5",
          "form": "사실 시비걸기 좋아요",
          "original_form": "사실 시비걸기 좋아요",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.6",
          "form": "진짜 저런 사람이 있다니",
          "original_form": "진짜 저런 사람이 있다니",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.7",
          "form": "근데 게임 콘텐츠가 선택이 됐네욬ㅋㅋㅋㅋ 어쩌다",
          "original_form": "근데 게임 콘텐츠가 선택이 됐네욬ㅋㅋㅋㅋ 어쩌다",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.8",
          "form": "기가 쪽쪽 빨릴것 같아요",
          "original_form": "기가 쪽쪽 빨릴것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.9",
          "form": "게임세상이 나을지도 몰라요",
          "original_form": "게임세상이 나을지도 몰라요",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.10",
          "form": "차라리 솔직하게 욕이라도 하지",
          "original_form": "차라리 솔직하게 욕이라도 하지",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.11",
          "form": "악 ㅋㅋㅋ 그러네요",
          "original_form": "악 ㅋㅋㅋ 그러네요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.12",
          "form": "ㅋㅋㅋㅋ 그쵸?",
          "original_form": "ㅋㅋㅋㅋ 그쵸?",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040580.1.13",
          "form": "돌려말하는거에 짜증이 나서",
          "original_form": "돌려말하는거에 짜증이 나서",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040580.1.14",
          "form": "저는 게임은 잘 못하지만집에 플스4가있거든요",
          "original_form": "저는 게임은 잘 못하지만집에 플스4가있거든요",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040580.1.15",
          "form": "게임 좋아하시나보네요??",
          "original_form": "게임 좋아하시나보네요??",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040580.1.16",
          "form": "좀비 게임을 해야하는데 인트로가 너무 무서워서다가가질 못하다가",
          "original_form": "좀비 게임을 해야하는데 인트로가 너무 무서워서다가가질 못하다가",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040580.1.17",
          "form": "전 게임은 잘 몰라서",
          "original_form": "전 게임은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040580.1.18",
          "form": "결국 못했어요",
          "original_form": "결국 못했어요",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040580.1.19",
          "form": "ㅋㅋㅋㅋㅋ 무서운거 안좋아하시나봐요",
          "original_form": "ㅋㅋㅋㅋㅋ 무서운거 안좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100040580.1.20",
          "form": "저도 ...ㅋㅋㅋㅋㅋ",
          "original_form": "저도 ...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100040580.1.21",
          "form": "네 깜짝 튀어나오는거완전 싫어하는데",
          "original_form": "네 깜짝 튀어나오는거완전 싫어하는데",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100040580.1.22",
          "form": "하필 플스4사면서 함께 사온 추천게임이 좀비였던거죠",
          "original_form": "하필 플스4사면서 함께 사온 추천게임이 좀비였던거죠",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.23",
          "form": "커텐쪽으로 가야게임이 진행되는데",
          "original_form": "커텐쪽으로 가야게임이 진행되는데",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.24",
          "form": "분명 뭐가 나올것 같아서 결국 포기했어요",
          "original_form": "분명 뭐가 나올것 같아서 결국 포기했어요",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.25",
          "form": "ㅋㅋㅋㅋ 전 평화로운 게임이 좋더라고요",
          "original_form": "ㅋㅋㅋㅋ 전 평화로운 게임이 좋더라고요",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.26",
          "form": "맞아요 전 타이쿤같은겈ㅋㅋ",
          "original_form": "맞아요 전 타이쿤같은겈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.27",
          "form": "저 드라마나 영화도 무서운거 잘 못봐욬ㅋㅋㅋ",
          "original_form": "저 드라마나 영화도 무서운거 잘 못봐욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.28",
          "form": "맞아요 타이쿤 재미있어요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 타이쿤 재미있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.29",
          "form": "저도 무서운건 다 싫어요",
          "original_form": "저도 무서운건 다 싫어요",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100040580.1.30",
          "form": "롤러코스터게임같은거",
          "original_form": "롤러코스터게임같은거",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040580.1.31",
          "form": "그런게 좋더라고요",
          "original_form": "그런게 좋더라고요",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040580.1.32",
          "form": "ㅋㅋㅋㅋ 저 그거 좋아하는데",
          "original_form": "ㅋㅋㅋㅋ 저 그거 좋아하는데",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040580.1.33",
          "form": "투가 제일 재미있어요",
          "original_form": "투가 제일 재미있어요",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040580.1.34",
          "form": "ㅋㅋㅋㅋ 그래픽 좋지 않아서 귀엽고",
          "original_form": "ㅋㅋㅋㅋ 그래픽 좋지 않아서 귀엽고",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040580.1.35",
          "form": "하다보면 시간 순삭",
          "original_form": "하다보면 시간 순삭",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040580.1.36",
          "form": "지나가다 우웩하는 토치우는 재미가 있더라고요",
          "original_form": "지나가다 우웩하는 토치우는 재미가 있더라고요",
          "speaker_id": "2",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100040580.1.37",
          "form": "ㅋㅋㅋ 맞아요 은근 재미있는데 그거 좋아한다고 하면 살인 게임 좋아한다고 하더라고요",
          "original_form": "ㅋㅋㅋ 맞아요 은근 재미있는데 그거 좋아한다고 하면 살인 게임 좋아한다고 하더라고요",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100040580.1.38",
          "form": "아닌뎅 그런거 안해도 재미있는데",
          "original_form": "아닌뎅 그런거 안해도 재미있는데",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100040580.1.39",
          "form": "살인게임이요??",
          "original_form": "살인게임이요??",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100040580.1.40",
          "form": "그 입장객 눌러서 물에 빠트리고",
          "original_form": "그 입장객 눌러서 물에 빠트리고",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100040580.1.41",
          "form": "헉 저는 아닙니다 ㅎㅎ",
          "original_form": "헉 저는 아닙니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100040580.1.42",
          "form": "투에서는 그게 되잖아요 ㅋㅋㅋ",
          "original_form": "투에서는 그게 되잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100040580.1.43",
          "form": "평화로운 게임 유저",
          "original_form": "평화로운 게임 유저",
          "speaker_id": "2",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100040580.1.44",
          "form": "그러니까욬ㅋㅋ",
          "original_form": "그러니까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100040580.1.45",
          "form": "평화로움 그 쾌감이 있는데",
          "original_form": "평화로움 그 쾌감이 있는데",
          "speaker_id": "1",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100040580.1.46",
          "form": "그걸 모르는 사람들이 그러더라고요",
          "original_form": "그걸 모르는 사람들이 그러더라고요",
