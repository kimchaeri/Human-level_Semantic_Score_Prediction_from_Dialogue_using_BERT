      "utterance": [
        {
          "id": "MDRW2100014817.1.1",
          "form": "구독하는 ott있으세요?",
          "original_form": "구독하는 ott있으세요?",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014817.1.2",
          "form": "전 왓챠 넷플 티빙 쿠플",
          "original_form": "전 왓챠 넷플 티빙 쿠플",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014817.1.3",
          "form": "다해요?",
          "original_form": "다해요?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014817.1.4",
          "form": "전 넷플해요",
          "original_form": "전 넷플해요",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014817.1.5",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014817.1.6",
          "form": "쿠플은 머에요?",
          "original_form": "쿠플은 머에요?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014817.1.7",
          "form": "모르는사람들끼리 공유해요",
          "original_form": "모르는사람들끼리 공유해요",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014817.1.8",
          "form": "넷플하고 왓챠는",
          "original_form": "넷플하고 왓챠는",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014817.1.9",
          "form": "쿠팡플레이요 잘 안쓰지만",
          "original_form": "쿠팡플레이요 잘 안쓰지만",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014817.1.10",
          "form": "모른사람이랑 우에 공유해요?",
          "original_form": "모른사람이랑 우에 공유해요?",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014817.1.11",
          "form": "공유자 모으는 카페가 있어서 네명 모집해서 한달비용으로 네달",
          "original_form": "공유자 모으는 카페가 있어서 네명 모집해서 한달비용으로 네달",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014817.1.12",
          "form": "봐요",
          "original_form": "봐요",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014817.1.13",
          "form": "아 그런것도있군요",
          "original_form": "아 그런것도있군요",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014817.1.14",
          "form": "좋은정보네요",
          "original_form": "좋은정보네요",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014817.1.15",
          "form": "넹 그래서 부담이 없어요",
          "original_form": "넹 그래서 부담이 없어요",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014817.1.16",
          "form": "재밋는 거 있나요?",
          "original_form": "재밋는 거 있나요?",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014817.1.17",
          "form": "요즘 보는건 왓챠에서 도라에몽 봐요 ㅋㅋ",
          "original_form": "요즘 보는건 왓챠에서 도라에몽 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014817.1.18",
          "form": "도라에몽",
          "original_form": "도라에몽",
