      "utterance": [
        {
          "id": "MDRW2100036179.1.1",
          "form": "여행가시면 기념품 사오세요??",
          "original_form": "여행가시면 기념품 사오세요??",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036179.1.2",
          "form": "아뇨 거의 안 사요 여행지는 가격이...",
          "original_form": "아뇨 거의 안 사요 여행지는 가격이...",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036179.1.3",
          "form": "너무 비싸요",
          "original_form": "너무 비싸요",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036179.1.4",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100036179.1.5",
          "form": "전에 그래도 간 기념으로 살까했는데 바로 접었어요",
          "original_form": "전에 그래도 간 기념으로 살까했는데 바로 접었어요",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100036179.1.6",
          "form": "그래서 사진만 남네요 ㅎㅎ",
          "original_form": "그래서 사진만 남네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:55"
        },
        {
          "id": "MDRW2100036179.1.7",
          "form": "사진 많이 찍으세용?",
          "original_form": "사진 많이 찍으세용?",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100036179.1.8",
          "form": "풍경 사진 주로 찍어요",
          "original_form": "풍경 사진 주로 찍어요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036179.1.9",
          "form": "오! 사진이 남는거긴 하더라구요",
          "original_form": "오! 사진이 남는거긴 하더라구요",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036179.1.10",
          "form": "전엔 잘 안찍었는데 최근엔 찍게 되더라구요",
          "original_form": "전엔 잘 안찍었는데 최근엔 찍게 되더라구요",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036179.1.11",
          "form": "사진이 막 나중에 보면 여행지 생각나고 그래요",
          "original_form": "사진이 막 나중에 보면 여행지 생각나고 그래요",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036179.1.12",
          "form": "요즘은 엽서 사고 싶더라구요 여행지에서",
          "original_form": "요즘은 엽서 사고 싶더라구요 여행지에서",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036179.1.13",
          "form": "그것도 좋네요",
          "original_form": "그것도 좋네요",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036179.1.14",
          "form": "기억도 살 것 같고!",
          "original_form": "기억도 살 것 같고!",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036179.1.15",
          "form": "뭔가 여행 갔다가 기념되는건 하나 사고싶더라구요",
          "original_form": "뭔가 여행 갔다가 기념되는건 하나 사고싶더라구요",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036179.1.16",
          "form": "맞아요 참 연예인 누군가는 향수 산데요 향기로 기억한다고",
          "original_form": "맞아요 참 연예인 누군가는 향수 산데요 향기로 기억한다고",
          "speaker_id": "2",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100036179.1.17",
          "form": "아! 맞아요 정유미!",
          "original_form": "아! 맞아요 정유미!",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100036179.1.18",
          "form": "그거보고 담에 따라해봐야겠다 했는데 여행을 못가고 있네요ㅋㅋㅠ",
          "original_form": "그거보고 담에 따라해봐야겠다 했는데 여행을 못가고 있네요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100036179.1.19",
          "form": "아 정유미ㅋㅋ 그쵸 코롸... 코시국 끝나면 여행지에 사람이 바글바글 하겠어요",
          "original_form": "아 정유미ㅋㅋ 그쵸 코롸... 코시국 끝나면 여행지에 사람이 바글바글 하겠어요",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036179.1.20",
          "form": "맞아용ㅋㅋㅋ 사람 구경 가게 생겼어요ㅋㅋㅋ",
          "original_form": "맞아용ㅋㅋㅋ 사람 구경 가게 생겼어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:44"
        }
      ]
