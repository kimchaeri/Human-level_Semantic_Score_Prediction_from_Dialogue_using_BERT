      "utterance": [
        {
          "id": "MDRW2100040842.1.1",
          "form": "건강을 위해 먹는 영양제 있나요?",
          "original_form": "건강을 위해 먹는 영양제 있나요?",
          "speaker_id": "1",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040842.1.2",
          "form": "멀티비타민. 칼마. 비타민디. 오메가3등등이요",
          "original_form": "멀티비타민. 칼마. 비타민디. 오메가3등등이요",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040842.1.3",
          "form": "칼마는 뭔가요?",
          "original_form": "칼마는 뭔가요?",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040842.1.4",
          "form": "아 칼슘 마그네슘?",
          "original_form": "아 칼슘 마그네슘?",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040842.1.5",
          "form": "저도 그거 먹어요~",
          "original_form": "저도 그거 먹어요~",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040842.1.6",
          "form": "ㅎㅎ 나이드니 이것저것 챙겨야죠 ㅎ",
          "original_form": "ㅎㅎ 나이드니 이것저것 챙겨야죠 ㅎ",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100040842.1.7",
          "form": "어쩔수 없네요 세월은 무시못해요",
          "original_form": "어쩔수 없네요 세월은 무시못해요",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040842.1.8",
          "form": "저도 영양제 안먹던 사람이었는데",
          "original_form": "저도 영양제 안먹던 사람이었는데",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040842.1.9",
          "form": "작년 코로나 터지고 부터 면역력 키우려고",
          "original_form": "작년 코로나 터지고 부터 면역력 키우려고",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040842.1.10",
          "form": "영양제 막 먹기 시작했어요",
          "original_form": "영양제 막 먹기 시작했어요",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040842.1.11",
          "form": "비타민씨도 그래서 먹는거구요",
          "original_form": "비타민씨도 그래서 먹는거구요",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040842.1.12",
          "form": "가끔은 이래 먹어도 되나 싶게 먹기도 ㅋㅋㅋ",
          "original_form": "가끔은 이래 먹어도 되나 싶게 먹기도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100040842.1.13",
          "form": "영양제로 배부르겠다는 말도 있잖아요",
          "original_form": "영양제로 배부르겠다는 말도 있잖아요",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100040842.1.14",
          "form": "진짜 한움쿰 먹게 되네요",
          "original_form": "진짜 한움쿰 먹게 되네요",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100040842.1.15",
          "form": "흑흑 ㅋㅋ 나이드니 챙기거보자 ㅋ",
          "original_form": "흑흑 ㅋㅋ 나이드니 챙기거보자 ㅋ",
          "speaker_id": "2",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100040842.1.16",
          "form": "요새 땅도 많이 약해져서 예전만큼 식품에 영양소가 들어있지 않다고 하더라구요",
          "original_form": "요새 땅도 많이 약해져서 예전만큼 식품에 영양소가 들어있지 않다고 하더라구요",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100040842.1.17",
          "form": "그래서 영양제 필수로 먹어야 한다고",
          "original_form": "그래서 영양제 필수로 먹어야 한다고",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100040842.1.18",
          "form": "뭐 상술일 수도 있지만 일단 사먹게 되더라구요",
          "original_form": "뭐 상술일 수도 있지만 일단 사먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100040842.1.19",
          "form": "ㅎㅎ 어릴땐 관심도 없었는데",
          "original_form": "ㅎㅎ 어릴땐 관심도 없었는데",
          "speaker_id": "2",
          "time": "20210905 18:18"
        },
        {
          "id": "MDRW2100040842.1.20",
          "form": "나이들면 의존하게 되더라구요",
          "original_form": "나이들면 의존하게 되더라구요",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100040842.1.21",
          "form": "ㅎㅎ 공감해여 ㅎㄹ",
          "original_form": "ㅎㅎ 공감해여 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100040842.1.22",
          "form": "뭐 운동도 해야하지만 영양제도 먹어야 하는 건 맞는거 같아요",
          "original_form": "뭐 운동도 해야하지만 영양제도 먹어야 하는 건 맞는거 같아요",
          "speaker_id": "1",
          "time": "20210905 20:32"
        }
      ]
