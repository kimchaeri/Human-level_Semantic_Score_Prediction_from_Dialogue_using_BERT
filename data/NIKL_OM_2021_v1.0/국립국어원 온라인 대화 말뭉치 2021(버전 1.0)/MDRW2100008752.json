        },
        {
          "id": "MDRW2100008752.1.3",
          "form": "브롤이여",
          "original_form": "브롤이여",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008752.1.4",
          "form": "브롤 어떤거에요?",
          "original_form": "브롤 어떤거에요?",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008752.1.5",
          "form": "초딩겜이요",
          "original_form": "초딩겜이요",
          "speaker_id": "1",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008752.1.6",
          "form": "오처음 들어봐요 초등들이 많이 하나봐요",
          "original_form": "오처음 들어봐요 초등들이 많이 하나봐요",
          "speaker_id": "2",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008752.1.7",
          "form": "네 아들이 레벨업해달라거",
          "original_form": "네 아들이 레벨업해달라거",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
