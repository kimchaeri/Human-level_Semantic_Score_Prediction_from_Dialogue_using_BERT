      "utterance": [
        {
          "id": "MDRW2100025179.1.1",
          "form": "이번 올림픽 대박이었지",
          "original_form": "이번 올림픽 대박이었지",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.2",
          "form": "올림픽 봤니?",
          "original_form": "올림픽 봤니?",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.3",
          "form": "이번 올림픽 대박이었지",
          "original_form": "이번 올림픽 대박이었지",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.4",
          "form": "양궁이 특히 멋졌어",
          "original_form": "양궁이 특히 멋졌어",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.5",
          "form": "나는 양궁이 그렇게 재밌더라",
          "original_form": "나는 양궁이 그렇게 재밌더라",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.6",
          "form": "오 취향 맞았어",
          "original_form": "오 취향 맞았어",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.7",
          "form": "역시 감명깊었지",
          "original_form": "역시 감명깊었지",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.8",
          "form": "3금!",
          "original_form": "3금!",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025179.1.9",
          "form": "진짜 대단한 거 같아",
          "original_form": "진짜 대단한 거 같아",
          "speaker_id": "1",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025179.1.10",
          "form": "안산선수",
          "original_form": "안산선수",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025179.1.11",
          "form": "실물로 한번 보고싶어",
          "original_form": "실물로 한번 보고싶어",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025179.1.12",
          "form": "넘멋져",
          "original_form": "넘멋져",
          "speaker_id": "2",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025179.1.13",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025179.1.14",
          "form": "여자배구도 멋있더라",
          "original_form": "여자배구도 멋있더라",
          "speaker_id": "2",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025179.1.15",
          "form": "맞아 나도 손에 땀을 쥐면서 봤어",
          "original_form": "맞아 나도 손에 땀을 쥐면서 봤어",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025179.1.16",
          "form": "메달 못딴 건 아쉬웠는데 우리나라 선수들 잘하더라",
          "original_form": "메달 못딴 건 아쉬웠는데 우리나라 선수들 잘하더라",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025179.1.17",
          "form": "한일전 너무재밌었어",
          "original_form": "한일전 너무재밌었어",
          "speaker_id": "2",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025179.1.18",
          "form": "이겨서 다행이야",
          "original_form": "이겨서 다행이야",
          "speaker_id": "2",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025179.1.19",
          "form": "한일전은 이겨야지",
          "original_form": "한일전은 이겨야지",
          "speaker_id": "2",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025179.1.20",
          "form": "와 일본 선수 리베로 장난 아니었잖아",
          "original_form": "와 일본 선수 리베로 장난 아니었잖아",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025179.1.21",
          "form": "이건 못봤겠지 하는데 나 받아버려서 나중에는 소리지르면서 봤어",
          "original_form": "이건 못봤겠지 하는데 나 받아버려서 나중에는 소리지르면서 봤어",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025179.1.22",
          "form": "받겠지",
          "original_form": "받겠지",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025179.1.23",
          "form": "아진짜,?,???",
          "original_form": "아진짜,?,???",
          "speaker_id": "2",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025179.1.24",
          "form": "난 연경신보느라",
          "original_form": "난 연경신보느라",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025179.1.25",
          "form": "눈치를못챘어ㅋㅋ",
          "original_form": "눈치를못챘어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025179.1.26",
          "form": "담에 배구보러가자",
          "original_form": "담에 배구보러가자",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025179.1.27",
          "form": "좋지 나 이번에 여자배구 관심생겼잖아",
          "original_form": "좋지 나 이번에 여자배구 관심생겼잖아",
          "speaker_id": "1",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025179.1.28",
          "form": "좋아 난 한국인삼공사 직관하고싶더라",
          "original_form": "좋아 난 한국인삼공사 직관하고싶더라",
