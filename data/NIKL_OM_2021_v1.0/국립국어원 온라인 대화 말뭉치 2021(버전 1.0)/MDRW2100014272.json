      "utterance": [
        {
          "id": "MDRW2100014272.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014272.1.2",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014272.1.3",
          "form": "종교가있으세요?",
          "original_form": "종교가있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014272.1.4",
          "form": "아뇨 없어요 name1은요?",
          "original_form": "아뇨 없어요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014272.1.5",
          "form": "저도 무교입니다 !",
          "original_form": "저도 무교입니다 !",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014272.1.6",
          "form": "산 같은데 가면 구경하러 절에 가기는 해요 ㅋㅋ",
          "original_form": "산 같은데 가면 구경하러 절에 가기는 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014272.1.7",
          "form": "경치구경 ㅋㅋㅋ",
          "original_form": "경치구경 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014272.1.8",
          "form": "저도요! 절은 몇번가봤어요 !",
          "original_form": "저도요! 절은 몇번가봤어요 !",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014272.1.9",
          "form": "날좋을때가면 좋더라구요!",
          "original_form": "날좋을때가면 좋더라구요!",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014272.1.10",
          "form": "그쵸 ㅋㅋ 템플스테이 같은것도 해보고 싶었는데 이시국이라",
          "original_form": "그쵸 ㅋㅋ 템플스테이 같은것도 해보고 싶었는데 이시국이라",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014272.1.11",
          "form": "전 잡식파인데 절밥도 맛있어요 ㅎ 몇번먹었던기억이나네요",
          "original_form": "전 잡식파인데 절밥도 맛있어요 ㅎ 몇번먹었던기억이나네요",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014272.1.12",
          "form": "오 ㅋㅋㅋ 지금 절밥 땡기네요 ㅋㅋㅋㅋ 맛있겠다...",
          "original_form": "오 ㅋㅋㅋ 지금 절밥 땡기네요 ㅋㅋㅋㅋ 맛있겠다...",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014272.1.13",
          "form": "하지만 고기를 너무 좋아해서 매일 먹긴 힘들거 같긴 해요",
          "original_form": "하지만 고기를 너무 좋아해서 매일 먹긴 힘들거 같긴 해요",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014272.1.14",
          "form": "저도 잡식파",
          "original_form": "저도 잡식파",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014272.1.15",
          "form": "ㅎㅎ맞아요 육류가 필요해요",
          "original_form": "ㅎㅎ맞아요 육류가 필요해요",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014272.1.16",
          "form": "요새 종교모임도 줄고 아에 행사가 축소되어서 이벤트가 적네요 ㅠ",
          "original_form": "요새 종교모임도 줄고 아에 행사가 축소되어서 이벤트가 적네요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014272.1.17",
          "form": "그렇겠어요",
          "original_form": "그렇겠어요",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014272.1.18",
          "form": "일단 모일수가 없어서",
          "original_form": "일단 모일수가 없어서",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014272.1.19",
          "form": "다들 잘지켜서 이겨내면 좋겠는데.. 이럼안되지만 ㅠㅠ 교회인들이 문제에요 모두는 아니겠지만요ㅠ",
          "original_form": "다들 잘지켜서 이겨내면 좋겠는데.. 이럼안되지만 ㅠㅠ 교회인들이 문제에요 모두는 아니겠지만요ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014272.1.20",
          "form": "교인보단 사이버종교들!!",
          "original_form": "교인보단 사이버종교들!!",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014272.1.21",
          "form": "아 맞아요 ㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014272.1.22",
          "form": "그런 종교는 참 무서운거 같아요",
          "original_form": "그런 종교는 참 무서운거 같아요",
