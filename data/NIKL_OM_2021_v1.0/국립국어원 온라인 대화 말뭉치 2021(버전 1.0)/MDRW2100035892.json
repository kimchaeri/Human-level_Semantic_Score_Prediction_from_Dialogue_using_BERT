      "utterance": [
        {
          "id": "MDRW2100035892.1.1",
          "form": "언니 화이자 맞았어요?",
          "original_form": "언니 화이자 맞았어요?",
          "speaker_id": "1",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100035892.1.2",
          "form": "평온이라......",
          "original_form": "평온이라......",
          "speaker_id": "2",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100035892.1.3",
          "form": "그냥 바쁘게 후다닥 ㅋㅋㅋㄱ",
          "original_form": "그냥 바쁘게 후다닥 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100035892.1.4",
          "form": "난 모더나",
          "original_form": "난 모더나",
          "speaker_id": "2",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100035892.1.5",
          "form": "그렇죠 아침에 병원 할 수 있는 사람이 몇 명이나 되겠어요",
          "original_form": "그렇죠 아침에 병원 할 수 있는 사람이 몇 명이나 되겠어요",
          "speaker_id": "1",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100035892.1.6",
          "form": "저도 요즘은 그냥 주말이 편하네요",
          "original_form": "저도 요즘은 그냥 주말이 편하네요",
          "speaker_id": "1",
          "time": "20210830 10:54"
        },
        {
          "id": "MDRW2100035892.1.7",
          "form": "아침에 안 서둘러도 되고",
          "original_form": "아침에 안 서둘러도 되고",
          "speaker_id": "1",
          "time": "20210830 10:54"
        },
        {
          "id": "MDRW2100035892.1.8",
          "form": "나와 이렇게 피곤하죠",
          "original_form": "나와 이렇게 피곤하죠",
          "speaker_id": "1",
          "time": "20210830 10:54"
        },
        {
          "id": "MDRW2100035892.1.9",
          "form": "병원하다니 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "병원하다니 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035892.1.10",
          "form": "아침에 병원ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아침에 병원ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035892.1.11",
          "form": "확인좀 해줄래?? 말만 하지말고",
          "original_form": "확인좀 해줄래?? 말만 하지말고",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035892.1.12",
          "form": "아 웃긴다",
          "original_form": "아 웃긴다",
          "speaker_id": "1",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035892.1.13",
          "form": "ㅋㅋㅋㅋㅋ 그래 방학이 뭔가 더 평온했다",
          "original_form": "ㅋㅋㅋㅋㅋ 그래 방학이 뭔가 더 평온했다",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035892.1.14",
          "form": "보내놓고도 아무 생각 없었네요",
          "original_form": "보내놓고도 아무 생각 없었네요",
          "speaker_id": "1",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035892.1.15",
          "form": "늦잠자도 닥달안해도 되고",
          "original_form": "늦잠자도 닥달안해도 되고",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035892.1.16",
          "form": "그러니까요 시간적 여유가 있으니",
          "original_form": "그러니까요 시간적 여유가 있으니",
          "speaker_id": "1",
          "time": "20210830 11:02"
        },
        {
          "id": "MDRW2100035892.1.17",
          "form": "맞아요 밥하는 게 조금 귀찮아서 그렇지",
          "original_form": "맞아요 밥하는 게 조금 귀찮아서 그렇지",
          "speaker_id": "1",
          "time": "20210830 11:02"
        },
        {
          "id": "MDRW2100035892.1.18",
          "form": "그러니",
          "original_form": "그러니",
          "speaker_id": "2",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100035892.1.19",
          "form": "나도 방학이 나은듯",
          "original_form": "나도 방학이 나은듯",
          "speaker_id": "2",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100035892.1.20",
          "form": "개학하니 또 공부는 저멀리...",
          "original_form": "개학하니 또 공부는 저멀리...",
          "speaker_id": "2",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100035892.1.21",
          "form": "그나마 방학때는 시간 많으니 문제집도 풀고",
          "original_form": "그나마 방학때는 시간 많으니 문제집도 풀고",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035892.1.22",
          "form": "받아쓰기도 했는데",
          "original_form": "받아쓰기도 했는데",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035892.1.23",
          "form": "맞아요 언니 뭐 할래도 시간이 없어요",
          "original_form": "맞아요 언니 뭐 할래도 시간이 없어요",
          "speaker_id": "1",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100035892.1.24",
          "form": "나도 귀찮아서 그냥 포기요",
          "original_form": "나도 귀찮아서 그냥 포기요",
          "speaker_id": "1",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100035892.1.25",
          "form": "그니께",
          "original_form": "그니께",
          "speaker_id": "2",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100035892.1.26",
          "form": "영어숙제가 매일 있으니까",
          "original_form": "영어숙제가 매일 있으니까",
          "speaker_id": "2",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100035892.1.27",
          "form": "해야될게 너무 많음",
          "original_form": "해야될게 너무 많음",
          "speaker_id": "2",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100035892.1.28",
          "form": "놀다가 6시반되서 집에오니",
          "original_form": "놀다가 6시반되서 집에오니",
          "speaker_id": "2",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100035892.1.29",
          "form": "밥먹고 씻고 촉박하다;;;",
          "original_form": "밥먹고 씻고 촉박하다;;;",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035892.1.30",
          "form": "한글 수학은 할 시간이 음네",
          "original_form": "한글 수학은 할 시간이 음네",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035892.1.31",
          "form": "수학도 언니두 자리 더하기 빼기 정말 짜증난",
          "original_form": "수학도 언니두 자리 더하기 빼기 정말 짜증난",
          "speaker_id": "1",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035892.1.32",
          "form": "우린 국어 수학 하는 대신 영어를 제낌",
          "original_form": "우린 국어 수학 하는 대신 영어를 제낌",
          "speaker_id": "1",
          "time": "20210830 11:21"
        },
        {
          "id": "MDRW2100035892.1.33",
          "form": "하나는 포기해야 함ㅋㅋㅋ",
          "original_form": "하나는 포기해야 함ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:21"
        }
      ]
