      "utterance": [
        {
          "id": "MDRW2100029732.1.1",
          "form": "안녕하세요~ ><",
          "original_form": "안녕하세요~ ><",
          "speaker_id": "1",
          "time": "20210903 12:27"
        },
        {
          "id": "MDRW2100029732.1.2",
          "form": "혹시 게임 좋아하시나요?",
          "original_form": "혹시 게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 12:27"
        },
        {
          "id": "MDRW2100029732.1.3",
          "form": "안녕하세요~~~~~~",
          "original_form": "안녕하세요~~~~~~",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029732.1.4",
          "form": "전 메이플 합니다!",
          "original_form": "전 메이플 합니다!",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029732.1.5",
          "form": "하시는 게임 있나요??",
          "original_form": "하시는 게임 있나요??",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029732.1.6",
          "form": "헐 저도 메이플해요!",
          "original_form": "헐 저도 메이플해요!",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100029732.1.7",
          "form": "저 원래 링크 이런거 몰랐다가 이제 알아버렸습니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 원래 링크 이런거 몰랐다가 이제 알아버렸습니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100029732.1.8",
          "form": "앗 저는 링크에서 어려워가지고 맨날 실패했는데ㅜㅜㅜ",
          "original_form": "앗 저는 링크에서 어려워가지고 맨날 실패했는데ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 12:32"
        },
        {
          "id": "MDRW2100029732.1.9",
          "form": "그래서 레벨 200 넘은 애들이 몇 없어요ㅋㅋㅋ",
          "original_form": "그래서 레벨 200 넘은 애들이 몇 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:32"
        },
        {
          "id": "MDRW2100029732.1.10",
          "form": "링크 배우면 더 재미있나요??",
          "original_form": "링크 배우면 더 재미있나요??",
          "speaker_id": "2",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029732.1.11",
          "form": "저도 200넘은 애들 몇 없어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 200넘은 애들 몇 없어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029732.1.12",
          "form": "이제야 60 가까이 올리는 중입니다...",
          "original_form": "이제야 60 가까이 올리는 중입니다...",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029732.1.13",
          "form": "제 친구가 링크 하는데 엄청 재밋어보이더라구요",
          "original_form": "제 친구가 링크 하는데 엄청 재밋어보이더라구요",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100029732.1.14",
          "form": "역시 어려워서 그렇지 배우면 더 재미있구나..",
          "original_form": "역시 어려워서 그렇지 배우면 더 재미있구나..",
          "speaker_id": "2",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029732.1.15",
          "form": "나중에 시간내서 링크 배워봐야겠어요!",
          "original_form": "나중에 시간내서 링크 배워봐야겠어요!",
          "speaker_id": "2",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029732.1.16",
          "form": "메이플 할때 현질은 많이 하세요???",
          "original_form": "메이플 할때 현질은 많이 하세요???",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100029732.1.17",
          "form": "그니까요... 전 맨날 노가다만 햇는뎈ㅋㅋㅋ",
          "original_form": "그니까요... 전 맨날 노가다만 햇는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100029732.1.18",
          "form": "현질은 안해욧 그지라서ㅜ",
          "original_form": "현질은 안해욧 그지라서ㅜ",
          "speaker_id": "1",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100029732.1.19",
          "form": "저도요ㅎㅎ 뭔가 게임에 돈쓰는게 아직은 돈아깝달까...",
          "original_form": "저도요ㅎㅎ 뭔가 게임에 돈쓰는게 아직은 돈아깝달까...",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029732.1.20",
          "form": "주위에 현질해서 캐릭터 예쁘게 꾸미고 다니는거 보면 예쁘긴 한데 내돈주고 하자니 아까워서ㅋㅋ",
          "original_form": "주위에 현질해서 캐릭터 예쁘게 꾸미고 다니는거 보면 예쁘긴 한데 내돈주고 하자니 아까워서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029732.1.21",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋ 저도 캐릭터 예쁘게는 하고싶은데",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋ 저도 캐릭터 예쁘게는 하고싶은데",
          "speaker_id": "1",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100029732.1.22",
          "form": "돈이 너무아까워요...",
          "original_form": "돈이 너무아까워요...",
          "speaker_id": "1",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100029732.1.23",
          "form": "그래서 막 아무거나 입고다니다가 가끔 이벤트로 캐시템 주면 입혀요...",
          "original_form": "그래서 막 아무거나 입고다니다가 가끔 이벤트로 캐시템 주면 입혀요...",
          "speaker_id": "2",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029732.1.24",
          "form": "메이플 기본 옷들은 다 너무 못생겼어요ㅋㅋㅋㅋ",
          "original_form": "메이플 기본 옷들은 다 너무 못생겼어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029732.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞아요 메이플 기본옷은 정말 어휴",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞아요 메이플 기본옷은 정말 어휴",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100029732.1.26",
          "form": "하지만 신캐들은 쫌 예쁘더라구요",
          "original_form": "하지만 신캐들은 쫌 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100029732.1.27",
          "form": "요즘 신캐 뭐 나왔나요?",
          "original_form": "요즘 신캐 뭐 나왔나요?",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100029732.1.28",
          "form": "안들어간지 좀 되가지고ㅋㅋㅋ",
          "original_form": "안들어간지 좀 되가지고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100029732.1.29",
          "form": "아 이름 까먹었는데 사기캐 하나 나왔대요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 이름 까먹었는데 사기캐 하나 나왔대요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100029732.1.30",
          "form": "그래서 밸런스 하나도 안맞는다곸ㅋㅋ",
          "original_form": "그래서 밸런스 하나도 안맞는다곸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:26"
        }
      ]
