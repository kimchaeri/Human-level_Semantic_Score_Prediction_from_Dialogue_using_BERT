      "utterance": [
        {
          "id": "MDRW2100007252.1.1",
          "form": "나 요즘 민초파우더랑 그린티 파우더 사서",
          "original_form": "나 요즘 민초파우더랑 그린티 파우더 사서",
          "speaker_id": "1",
