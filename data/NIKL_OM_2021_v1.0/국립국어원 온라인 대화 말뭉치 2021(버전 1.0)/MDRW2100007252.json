          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.4",
          "form": "민초파우더랑",
          "original_form": "민초파우더랑",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.5",
          "form": "그린티파우더 사서",
          "original_form": "그린티파우더 사서",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.7",
          "form": "프라페 해먹는즁",
          "original_form": "프라페 해먹는즁",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.8",
          "form": "어디꺼삼",
          "original_form": "어디꺼삼",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.9",
          "form": "프라페 갈아서?",
          "original_form": "프라페 갈아서?",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.10",
          "form": "타코꺼",
          "original_form": "타코꺼",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.11",
          "form": "얼음갈아서?",
          "original_form": "얼음갈아서?",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.12",
          "form": "쿠팡에서 팔길래 로켓배송으로",
          "original_form": "쿠팡에서 팔길래 로켓배송으로",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.13",
          "form": "믹서기에?",
          "original_form": "믹서기에?",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.14",
          "form": "ㅇㅇ얼음이랑 우유랑",
          "original_form": "ㅇㅇ얼음이랑 우유랑",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.15",
          "form": "ㅇㅇㅇ믹서기에!",
          "original_form": "ㅇㅇㅇ믹서기에!",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.16",
          "form": "설거지귀찬자나",
          "original_form": "설거지귀찬자나",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.17",
          "form": "민초그린티...하",
          "original_form": "민초그린티...하",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007252.1.18",
          "form": "민초가싫어요",
          "original_form": "민초가싫어요",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007252.1.19",
          "form": "설거지는 그냥 싱크대에 놔두곤 해요",
          "original_form": "설거지는 그냥 싱크대에 놔두곤 해요",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007252.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ아니 민초가 왜ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니 민초가 왜ㅜ",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007252.1.21",
          "form": "민초맛잇는데",
          "original_form": "민초맛잇는데",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007252.1.22",
          "form": "민초는너무",
          "original_form": "민초는너무",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007252.1.23",
          "form": "치약맛나요",
          "original_form": "치약맛나요",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007252.1.24",
          "form": "자취생은 역시 다르네요",
          "original_form": "자취생은 역시 다르네요",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007252.1.25",
          "form": "ㅋㅋ방학이라 대구에 와사 해먹엇서",
          "original_form": "ㅋㅋ방학이라 대구에 와사 해먹엇서",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007252.1.26",
          "form": "이제 다시 부산가야대지만ㅠ",
          "original_form": "이제 다시 부산가야대지만ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007252.1.27",
          "form": "헐... 치약이 민트맛인건데",
          "original_form": "헐... 치약이 민트맛인건데",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007252.1.28",
          "form": "선후관계모르시나요",
          "original_form": "선후관계모르시나요",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007252.1.29",
          "form": "아ㅎ언제부산에식사하러가세요?",
          "original_form": "아ㅎ언제부산에식사하러가세요?",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007252.1.30",
          "form": "민트는호불호가갈리죠",
          "original_form": "민트는호불호가갈리죠",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007252.1.31",
          "form": "호불호음식~!",
          "original_form": "호불호음식~!",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007252.1.32",
          "form": "부산에 돼지국밥 먹으러 가려규요",
          "original_form": "부산에 돼지국밥 먹으러 가려규요",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007252.1.33",
          "form": "흠.... 호불호 음식이긴한데",
          "original_form": "흠.... 호불호 음식이긴한데",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007252.1.34",
          "form": "민초는 호가 더 많다구요!!",
          "original_form": "민초는 호가 더 많다구요!!",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007252.1.35",
          "form": "민초는 불~",
          "original_form": "민초는 불~",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007252.1.36",
          "form": "부산에 돼지국밥 누구랑먹어",
          "original_form": "부산에 돼지국밥 누구랑먹어",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007252.1.37",
          "form": "돼지국밥은 혼밥가능한 메뉴임",
          "original_form": "돼지국밥은 혼밥가능한 메뉴임",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007252.1.38",
          "form": "민초 호호 ㅡㅡ",
          "original_form": "민초 호호 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007252.1.39",
