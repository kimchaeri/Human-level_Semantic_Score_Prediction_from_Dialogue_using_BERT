        },
        {
          "id": "MDRW2100020968.1.18",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100020968.1.19",
          "form": "아니면 그냥 크림",
