        {
          "id": "MDRW2100020968.1.1",
          "form": "아 ㅠㅠ 그럼 포장은 못 하시겠네요",
          "original_form": "아 ㅠㅠ 그럼 포장은 못 하시겠네요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.2",
          "form": "아 네 파스타는",
          "original_form": "아 네 파스타는",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.3",
          "form": "무조건 배달아니면",
          "original_form": "무조건 배달아니면",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.4",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.5",
          "form": "소스사다가 해먹어요",
          "original_form": "소스사다가 해먹어요",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.7",
          "form": "시판소스가 최고긴해요 ㅋㅋㅋㅋ",
          "original_form": "시판소스가 최고긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.8",
          "form": "소스가",
          "original_form": "소스가",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020968.1.9",
          "form": "너무 잘나와요",
          "original_form": "너무 잘나와요",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020968.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020968.1.11",
          "form": "특히 크림파스타는 전 무조건 사먹어요 ㅌㅋ",
          "original_form": "특히 크림파스타는 전 무조건 사먹어요 ㅌㅋ",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020968.1.12",
          "form": "아 크림소스 너무 잘나오죠",
          "original_form": "아 크림소스 너무 잘나오죠",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020968.1.13",
          "form": "로제는 아직까지",
          "original_form": "로제는 아직까지",
