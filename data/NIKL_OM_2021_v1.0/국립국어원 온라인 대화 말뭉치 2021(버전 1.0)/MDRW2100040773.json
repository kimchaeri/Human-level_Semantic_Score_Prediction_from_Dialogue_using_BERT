      "utterance": [
        {
          "id": "MDRW2100040773.1.1",
          "form": "오래앉아있는게나쁜거같아요",
          "original_form": "오래앉아있는게나쁜거같아요",
          "speaker_id": "2",
          "time": "20210905 04:24"
        },
        {
          "id": "MDRW2100040773.1.2",
          "form": "오래앉어있으면 모든곳에힘빠져서",
          "original_form": "오래앉어있으면 모든곳에힘빠져서",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.3",
          "form": "뇌에도 힘빠지고",
          "original_form": "뇌에도 힘빠지고",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.4",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.5",
          "form": "아 힘 빠진대요?!?!",
          "original_form": "아 힘 빠진대요?!?!",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.6",
          "form": "누워있는것도안좋은고같아요",
          "original_form": "누워있는것도안좋은고같아요",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.7",
          "form": "아니뮤ㅓㄴ가 무의식적으로그렇지않나요?!",
          "original_form": "아니뮤ㅓㄴ가 무의식적으로그렇지않나요?!",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.8",
          "form": "맞아요... 어느정도 사람 몸에",
          "original_form": "맞아요... 어느정도 사람 몸에",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.9",
          "form": "앗 그건 그래요 그리고",
          "original_form": "앗 그건 그래요 그리고",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.10",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.11",
          "form": "뉴워서폰하는거 제일나쁜..",
          "original_form": "뉴워서폰하는거 제일나쁜..",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.12",
          "form": "노트북을 하거나 공부를 하면",
          "original_form": "노트북을 하거나 공부를 하면",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.13",
          "form": "손목도아파요",
          "original_form": "손목도아파요",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.14",
          "form": "맞아요 저 맨날 그러는데 ㅠ",
          "original_form": "맞아요 저 맨날 그러는데 ㅠ",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.15",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.16",
          "form": "그러다가 시력도 짝짝이 되었어요",
          "original_form": "그러다가 시력도 짝짝이 되었어요",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.18",
          "form": "저 통근시간이 길어서",
          "original_form": "저 통근시간이 길어서",
          "speaker_id": "1",
          "time": "20210905 04:25"
        },
        {
          "id": "MDRW2100040773.1.19",
          "form": "옆으로누워계시는구나ㅠ",
          "original_form": "옆으로누워계시는구나ㅠ",
          "speaker_id": "2",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.20",
          "form": "지하철에서 맨날 폰 하는데",
          "original_form": "지하철에서 맨날 폰 하는데",
          "speaker_id": "1",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.21",
          "form": "이게 진짜",
          "original_form": "이게 진짜",
          "speaker_id": "1",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.22",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.23",
          "form": "안대요ㅠㅠㅠ",
          "original_form": "안대요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.24",
          "form": "지금은 노트북 중이에요! ><",
          "original_form": "지금은 노트북 중이에요! ><",
          "speaker_id": "1",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.25",
          "form": "헉헉",
          "original_form": "헉헉",
          "speaker_id": "2",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.26",
          "form": "그러니까 진짜",
          "original_form": "그러니까 진짜",
          "speaker_id": "1",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.27",
          "form": "어깨가 너무 아파서",
          "original_form": "어깨가 너무 아파서",
          "speaker_id": "1",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.28",
          "form": "노트북도 책받침같은곳에 올려놓고",
          "original_form": "노트북도 책받침같은곳에 올려놓고",
          "speaker_id": "2",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.29",
          "form": "그래서 도수치료 간거였어욬ㅋㅋㅋㅋ",
          "original_form": "그래서 도수치료 간거였어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.30",
          "form": "하는게좋다면서요",
          "original_form": "하는게좋다면서요",
          "speaker_id": "2",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.31",
          "form": "아 맞아요 그래야한다면서요ㅠ",
          "original_form": "아 맞아요 그래야한다면서요ㅠ",
          "speaker_id": "1",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.32",
          "form": "ㅠㅠ그렇다네요ㅠ",
          "original_form": "ㅠㅠ그렇다네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 04:26"
        },
        {
          "id": "MDRW2100040773.1.33",
          "form": "역시 인생은 아이템빨인가",
          "original_form": "역시 인생은 아이템빨인가",
          "speaker_id": "1",
          "time": "20210905 04:26"
        }
      ]
