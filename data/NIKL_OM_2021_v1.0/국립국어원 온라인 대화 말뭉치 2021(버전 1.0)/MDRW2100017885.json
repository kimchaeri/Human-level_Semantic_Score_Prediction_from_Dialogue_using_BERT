      "utterance": [
        {
          "id": "MDRW2100017885.1.1",
          "form": "저녁 뭐드샸어요?",
          "original_form": "저녁 뭐드샸어요?",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017885.1.2",
          "form": "저 오늘은 배달음식 먹었어요",
          "original_form": "저 오늘은 배달음식 먹었어요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100017885.1.3",
          "form": "월남쌈요",
          "original_form": "월남쌈요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100017885.1.4",
          "form": "월남쌈도 배달 되나요?",
          "original_form": "월남쌈도 배달 되나요?",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100017885.1.5",
          "form": "네네~ 베트남식 월남쌈이라고 있길래 주문했는대 맛있네요",
          "original_form": "네네~ 베트남식 월남쌈이라고 있길래 주문했는대 맛있네요",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100017885.1.6",
          "form": "저녁 뭐드셨어요?",
          "original_form": "저녁 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100017885.1.7",
          "form": "오 괜찮은 메뉴네요",
          "original_form": "오 괜찮은 메뉴네요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100017885.1.8",
          "form": "저는 양념게장에 밥먹었어요",
          "original_form": "저는 양념게장에 밥먹었어요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100017885.1.9",
          "form": "와 게장 맛있겠네요 저도 좋아하는 메뉴인데 가족이 안좋아해서ㅠ 시킬수가없네요",
          "original_form": "와 게장 맛있겠네요 저도 좋아하는 메뉴인데 가족이 안좋아해서ㅠ 시킬수가없네요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100017885.1.10",
          "form": "아하ㅠㅠ저도 사실은 알레르기있어서 게 못먹는데",
          "original_form": "아하ㅠㅠ저도 사실은 알레르기있어서 게 못먹는데",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017885.1.11",
          "form": "오기로 먹어요ㅋㅋㅋㅋ",
          "original_form": "오기로 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017885.1.12",
          "form": "아앗 게알레르기 있으시구나ㅠㅠㅠ 좋아하는데 먹기가 어려우시면 슬프겠네요",
          "original_form": "아앗 게알레르기 있으시구나ㅠㅠㅠ 좋아하는데 먹기가 어려우시면 슬프겠네요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100017885.1.13",
          "form": "맞아요ㅠ그래서 가끔씩밖에 못먹어요",
          "original_form": "맞아요ㅠ그래서 가끔씩밖에 못먹어요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100017885.1.14",
          "form": "못먹는 음식 없으세요?",
          "original_form": "못먹는 음식 없으세요?",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100017885.1.15",
          "form": "네~ 저는 음식 스펙트럼은 넓어요ㅎㅎ",
          "original_form": "네~ 저는 음식 스펙트럼은 넓어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100017885.1.16",
          "form": "대신 많이 못먹어요 소식+예민한장이라서",
          "original_form": "대신 많이 못먹어요 소식+예민한장이라서",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100017885.1.17",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100017885.1.18",
          "form": "세상에 맛있는 음식이 얼마나 많은데",
          "original_form": "세상에 맛있는 음식이 얼마나 많은데",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100017885.1.19",
          "form": "많이 못먹는건 좀 아쉽겠어요",
          "original_form": "많이 못먹는건 좀 아쉽겠어요",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100017885.1.20",
          "form": "과식이나 좀 장이 안좋은 날엔 바로 장염이라ㅠ 슬퍼요",
          "original_form": "과식이나 좀 장이 안좋은 날엔 바로 장염이라ㅠ 슬퍼요",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100017885.1.21",
          "form": "ㅠㅜ뷔페도 가면 많이 못먹겠네요",
          "original_form": "ㅠㅜ뷔페도 가면 많이 못먹겠네요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100017885.1.22",
          "form": "넵 적당히 먹어요~ 병원비가 더나와요ㅋㅋㅋ 뷔페값이 아까워서 잘안가기도 하구요",
          "original_form": "넵 적당히 먹어요~ 병원비가 더나와요ㅋㅋㅋ 뷔페값이 아까워서 잘안가기도 하구요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100017885.1.23",
          "form": "아하 체하기도 잘 체하겠어요",
          "original_form": "아하 체하기도 잘 체하겠어요",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100017885.1.24",
          "form": "네 체하는건 덜한데 장염이 더큰문제죠ㅠㅜ 그래서 그런지 부담안되고 단백한 야채나 해산물 쪽으로 더 좋아하는거같아요ㅋㅋ",
          "original_form": "네 체하는건 덜한데 장염이 더큰문제죠ㅠㅜ 그래서 그런지 부담안되고 단백한 야채나 해산물 쪽으로 더 좋아하는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100017885.1.25",
          "form": "맛있는거 드셨어요?",
          "original_form": "맛있는거 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 21:42"
        }
      ]
    }
