      "utterance": [
        {
          "id": "MDRW2100025967.1.1",
          "form": "ㅋㅋㅋㅋ오픈이벤트는 또 참여해줘여지...",
          "original_form": "ㅋㅋㅋㅋ오픈이벤트는 또 참여해줘여지...",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025967.1.2",
          "form": "ㅋㅋㅋㅋ메뉴는 세트3번?",
          "original_form": "ㅋㅋㅋㅋ메뉴는 세트3번?",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025967.1.3",
          "form": "당연",
          "original_form": "당연",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100025967.1.4",
          "form": "3번에 케찹? 떡볶이 같은 거 붙어왔어",
          "original_form": "3번에 케찹? 떡볶이 같은 거 붙어왔어",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100025967.1.5",
          "form": "오 떡볶이? 그것도 세트일부야?",
          "original_form": "오 떡볶이? 그것도 세트일부야?",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100025967.1.6",
          "form": "그런가봐 ㅋㅋㅋ",
          "original_form": "그런가봐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100025967.1.7",
          "form": "오랜만에 먹으니까 땅땅 너무 맛잇더라....",
          "original_form": "오랜만에 먹으니까 땅땅 너무 맛잇더라....",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100025967.1.8",
          "form": "하고 현타 ㅋ큨",
          "original_form": "하고 현타 ㅋ큨",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100025967.1.9",
          "form": "아 맛잇겟다 ㅋㅋㅋㅋ",
          "original_form": "아 맛잇겟다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100025967.1.10",
          "form": "나는 목살 구워먹음",
          "original_form": "나는 목살 구워먹음",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100025967.1.11",
          "form": "구운김치랑 올만에 먹엇더니 김치와래맛잇늠",
          "original_form": "구운김치랑 올만에 먹엇더니 김치와래맛잇늠",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100025967.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100025967.1.13",
          "form": "집에서?",
          "original_form": "집에서?",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100025967.1.14",
          "form": "김치보끔밥은 안먹엇니...",
          "original_form": "김치보끔밥은 안먹엇니...",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100025967.1.15",
          "form": "ㅋㅋㅋㅋㅋㅇㅇ너무맛잇드라 밥은 그냥밥 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅇㅇ너무맛잇드라 밥은 그냥밥 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100025967.1.16",
          "form": "엄마가 그 고깃집 철판삼ㅋㅋㅋㅋㅋ",
          "original_form": "엄마가 그 고깃집 철판삼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100025967.1.17",
          "form": "널찍한겈ㅋㅋㅋㅋ",
          "original_form": "널찍한겈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100025967.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100025967.1.19",
          "form": "기울어지고 그거?",
          "original_form": "기울어지고 그거?",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100025967.1.20",
          "form": "거기에 고기 굽고 밑에 콩나물이랑 김치 부추...크...",
          "original_form": "거기에 고기 굽고 밑에 콩나물이랑 김치 부추...크...",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100025967.1.21",
          "form": "크 맛잇겟다ㅋㅋㅋㅋㅋ",
          "original_form": "크 맛잇겟다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100025967.1.22",
          "form": "나는 양파장 파채좋아혀",
          "original_form": "나는 양파장 파채좋아혀",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100025967.1.23",
          "form": "고기먹고싶네...",
          "original_form": "고기먹고싶네...",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100025967.1.24",
          "form": "양파장 필수지",
          "original_form": "양파장 필수지",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100025967.1.25",
          "form": "그 뭐지 재래기라고하나? 야채도 존맛",
          "original_form": "그 뭐지 재래기라고하나? 야채도 존맛",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100025967.1.26",
          "form": "ㅇㅇ근데 집에선 그 맛이 잘 안나지 않니",
          "original_form": "ㅇㅇ근데 집에선 그 맛이 잘 안나지 않니",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100025967.1.27",
          "form": "마자 그거 어케만드는거지 ㅋㅋㅋㅋㅋ",
          "original_form": "마자 그거 어케만드는거지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:58"
        }
      ]
