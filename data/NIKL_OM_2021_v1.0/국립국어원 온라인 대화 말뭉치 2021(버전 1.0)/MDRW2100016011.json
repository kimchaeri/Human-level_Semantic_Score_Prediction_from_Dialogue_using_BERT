      "utterance": [
        {
          "id": "MDRW2100016011.1.1",
          "form": "좋아하는 스포츠가 있나요",
          "original_form": "좋아하는 스포츠가 있나요",
          "speaker_id": "1",
          "time": "20210826 07:32"
        },
        {
          "id": "MDRW2100016011.1.2",
          "form": "음..올림픽때 양궁은 꼭 봐요",
          "original_form": "음..올림픽때 양궁은 꼭 봐요",
          "speaker_id": "2",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016011.1.3",
          "form": "저도 꼭 봅니다",
          "original_form": "저도 꼭 봅니다",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016011.1.4",
          "form": "어느 선수 관심 있게 보셨어요?",
          "original_form": "어느 선수 관심 있게 보셨어요?",
          "speaker_id": "2",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016011.1.5",
          "form": "이번엔 안산 선수가 눈에 보이더라고요",
          "original_form": "이번엔 안산 선수가 눈에 보이더라고요",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100016011.1.6",
          "form": "그쵸 멘탈 대단한듯",
          "original_form": "그쵸 멘탈 대단한듯",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100016011.1.7",
          "form": "보이시한 모습이 매력이 있네요",
          "original_form": "보이시한 모습이 매력이 있네요",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100016011.1.8",
          "form": "어떤 선수에 관심이 가나요 ?",
          "original_form": "어떤 선수에 관심이 가나요 ?",
          "speaker_id": "1",
          "time": "20210829 07:31"
        },
        {
          "id": "MDRW2100016011.1.9",
          "form": "저도 김제덕 안산 양궁선수들이랑 안창림이요",
          "original_form": "저도 김제덕 안산 양궁선수들이랑 안창림이요",
          "speaker_id": "2",
          "time": "20210829 09:08"
        },
        {
          "id": "MDRW2100016011.1.10",
          "form": "양궁선수들을 잘알고 계신가요",
          "original_form": "양궁선수들을 잘알고 계신가요",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100016011.1.11",
          "form": "이번에 첨봤어요",
          "original_form": "이번에 첨봤어요",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100016011.1.12",
          "form": "저도 이번 올림픽때 자세히 보았어요",
          "original_form": "저도 이번 올림픽때 자세히 보았어요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100016011.1.13",
          "form": "그쵸 그이후로 방송나오는 것도 관심있게 보게 되더라구요",
          "original_form": "그쵸 그이후로 방송나오는 것도 관심있게 보게 되더라구요",
          "speaker_id": "2",
