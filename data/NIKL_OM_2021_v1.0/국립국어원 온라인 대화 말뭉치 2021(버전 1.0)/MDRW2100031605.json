      "utterance": [
        {
          "id": "MDRW2100031605.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031605.1.2",
          "form": "라멘 맛잇나요?",
          "original_form": "라멘 맛잇나요?",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031605.1.3",
          "form": "못 먹었어요ㅜㅜ",
          "original_form": "못 먹었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031605.1.4",
          "form": "왜영",
          "original_form": "왜영",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031605.1.5",
          "form": "그게여..",
          "original_form": "그게여..",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100031605.1.6",
          "form": "라멘 먹기로 거의 확정이었는데",
          "original_form": "라멘 먹기로 거의 확정이었는데",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100031605.1.7",
          "form": "막내가 자기는 싫대요ㅜㅜ\r\n사실 남동생이 오후에 엄마한테 라멘 어떻냐고 하면서 막내 못 먹어봤으니까 같이 가자 했던건데",
          "original_form": "막내가 자기는 싫대요ㅜㅜ\r\n사실 남동생이 오후에 엄마한테 라멘 어떻냐고 하면서 막내 못 먹어봤으니까 같이 가자 했던건데",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031605.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031605.1.9",
          "form": "중ㅈ요한건 막내뿐만이 아니라",
          "original_form": "중ㅈ요한건 막내뿐만이 아니라",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031605.1.10",
          "form": "남동생도 자고 일어나니 생각이 바뀐건지 찜닭타령해서",
          "original_form": "남동생도 자고 일어나니 생각이 바뀐건지 찜닭타령해서",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100031605.1.11",
          "form": "ㅋㅋㅋㅋㅋ 변덕이..ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 변덕이..ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100031605.1.12",
          "form": "막내도 싫다하고 겸사겸사 그래서 찜닭이 저녁이 되었습니다",
          "original_form": "막내도 싫다하고 겸사겸사 그래서 찜닭이 저녁이 되었습니다",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100031605.1.13",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031605.1.14",
          "form": "간장맛?",
          "original_form": "간장맛?",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031605.1.15",
          "form": "아니요 로제와 묵은지를 시키더라구요",
          "original_form": "아니요 로제와 묵은지를 시키더라구요",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031605.1.16",
          "form": "자기가 먹고 싶은 맛은 로제이고, 묵은지는 엄마가 지난번에 맛이 궁금하다고 하셨었나봐요",
          "original_form": "자기가 먹고 싶은 맛은 로제이고, 묵은지는 엄마가 지난번에 맛이 궁금하다고 하셨었나봐요",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031605.1.17",
          "form": "로제 유명하죠. 저도 집에서 해먹어봣어용",
          "original_form": "로제 유명하죠. 저도 집에서 해먹어봣어용",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031605.1.18",
          "form": "오! 직접 요리하셨군요",
          "original_form": "오! 직접 요리하셨군요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031605.1.19",
          "form": "나중에 레시피 알려드릴게여",
          "original_form": "나중에 레시피 알려드릴게여",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031605.1.20",
          "form": "네!  좋아요",
          "original_form": "네!  좋아요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        }
      ]
