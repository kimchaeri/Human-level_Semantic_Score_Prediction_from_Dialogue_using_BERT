      "utterance": [
        {
          "id": "MDRW2100026945.1.1",
          "form": "매점있었는데",
          "original_form": "매점있었는데",
          "speaker_id": "2",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100026945.1.2",
          "form": "코로나때문에 없어졌어요",
          "original_form": "코로나때문에 없어졌어요",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.3",
          "form": "억울함...",
          "original_form": "억울함...",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.4",
          "form": "네네네.....아무래도....",
          "original_form": "네네네.....아무래도....",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.5",
          "form": "슬프군요   ....",
          "original_form": "슬프군요   ....",
          "speaker_id": "1",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.6",
          "form": "그럼 햄버거나 뭐 사가도",
          "original_form": "그럼 햄버거나 뭐 사가도",
          "speaker_id": "1",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.7",
          "form": "못먹자나여",
          "original_form": "못먹자나여",
          "speaker_id": "1",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.8",
          "form": "네.....",
          "original_form": "네.....",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.9",
          "form": "그래서 애들보면",
          "original_form": "그래서 애들보면",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.10",
          "form": "보통 과일싸와서 먹거나",
          "original_form": "보통 과일싸와서 먹거나",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.11",
          "form": "그냥 빵같은거 먹더라구요",
          "original_form": "그냥 빵같은거 먹더라구요",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.12",
          "form": "뿌셔뿌셔 ...",
          "original_form": "뿌셔뿌셔 ...",
          "speaker_id": "1",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026945.1.13",
          "form": "어때용",
          "original_form": "어때용",
          "speaker_id": "1",
          "time": "20210902 01:34"
        },
        {
          "id": "MDRW2100026945.1.14",
          "form": "뿌셔..뿌셔",
          "original_form": "뿌셔..뿌셔",
          "speaker_id": "2",
          "time": "20210902 01:34"
        },
        {
          "id": "MDRW2100026945.1.15",
          "form": "오랜만이네요..",
          "original_form": "오랜만이네요..",
          "speaker_id": "2",
          "time": "20210902 01:34"
        },
        {
          "id": "MDRW2100026945.1.16",
          "form": "갑자기 양감먹고싶다",
          "original_form": "갑자기 양감먹고싶다",
          "speaker_id": "2",
          "time": "20210902 01:34"
        },
        {
          "id": "MDRW2100026945.1.17",
          "form": "손가락 깨물어드세요",
          "original_form": "손가락 깨물어드세요",
          "speaker_id": "1",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026945.1.18",
          "form": "어떻게 이런 발언이",
          "original_form": "어떻게 이런 발언이",
          "speaker_id": "2",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026945.1.19",
          "form": "님 내일 1교시라고했죠",
          "original_form": "님 내일 1교시라고했죠",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.20",
          "form": "저 내일 1교시부터 생물발전 어쩌구 듣는데",
          "original_form": "저 내일 1교시부터 생물발전 어쩌구 듣는데",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.21",
          "form": "생각만해도",
          "original_form": "생각만해도",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.22",
          "form": "네네....",
          "original_form": "네네....",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.23",
          "form": "졸리네요",
          "original_form": "졸리네요",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.24",
          "form": "꿀잠자겠네요",
          "original_form": "꿀잠자겠네요",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.25",
          "form": "저이것까지만하고",
          "original_form": "저이것까지만하고",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.26",
          "form": "ㄹㅇ 원어민 교수라",
          "original_form": "ㄹㅇ 원어민 교수라",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.27",
          "form": "내일을유ㅣ해서 자야겟어요..",
          "original_form": "내일을유ㅣ해서 자야겟어요..",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.28",
          "form": "잘듯",
          "original_form": "잘듯",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.29",
          "form": "헐랭",
          "original_form": "헐랭",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.30",
          "form": "네네....",
          "original_form": "네네....",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.31",
          "form": "저두요....",
          "original_form": "저두요....",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.32",
          "form": "두배로 잘잘듯",
          "original_form": "두배로 잘잘듯",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.33",
          "form": "꿀잠예약",
          "original_form": "꿀잠예약",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.34",
          "form": "저 중학교때 중국어 시간에",
          "original_form": "저 중학교때 중국어 시간에",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.35",
          "form": "맨날 잤어요",
          "original_form": "맨날 잤어요",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.36",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.37",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026945.1.38",
          "form": "저 중국어 한번도 안배워봄",
          "original_form": "저 중국어 한번도 안배워봄",
          "speaker_id": "1",
