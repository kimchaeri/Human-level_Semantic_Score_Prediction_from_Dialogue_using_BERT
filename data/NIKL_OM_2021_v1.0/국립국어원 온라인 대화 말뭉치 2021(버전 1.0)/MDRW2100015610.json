      "utterance": [
        {
          "id": "MDRW2100015610.1.1",
          "form": "영화하니깐 갑자기드는생각ㅎㅎ",
          "original_form": "영화하니깐 갑자기드는생각ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015610.1.2",
          "form": "영화가 많잖아요ㅎㅎ",
          "original_form": "영화가 많잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015610.1.3",
          "form": "거기서 관객들은 천만영화를 선택을 하는 이유가 뭘까요?",
          "original_form": "거기서 관객들은 천만영화를 선택을 하는 이유가 뭘까요?",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015610.1.4",
          "form": "네네~",
          "original_form": "네네~",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015610.1.5",
          "form": "그 많은 영화중에",
          "original_form": "그 많은 영화중에",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.6",
          "form": "배우도 있겠고~",
          "original_form": "배우도 있겠고~",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.7",
          "form": "입소문이 젤 무섭죠 ㅎㅎ",
          "original_form": "입소문이 젤 무섭죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.8",
          "form": "천만을 달성하는것도 힘든데",
          "original_form": "천만을 달성하는것도 힘든데",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.9",
          "form": "입소문이고ㅎㅎ",
          "original_form": "입소문이고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.10",
          "form": "믿고 보는 배우도 ㅎㅎ",
          "original_form": "믿고 보는 배우도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.11",
          "form": "또 마케팅도 있겟죠?ㅎ",
          "original_form": "또 마케팅도 있겟죠?ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.12",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.13",
          "form": "저는 조니뎁 좋아해서 ㅎㅎㅎ",
          "original_form": "저는 조니뎁 좋아해서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.14",
          "form": "배우들의 티켓파워도 있을꺼고ㅎㅎ",
          "original_form": "배우들의 티켓파워도 있을꺼고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.15",
          "form": "오ㅎㅎ",
          "original_form": "오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.16",
          "form": "저도 좋아해요ㅎ",
          "original_form": "저도 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.17",
          "form": "믿고 봅니다 ㅋ",
          "original_form": "믿고 봅니다 ㅋ",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015610.1.18",
          "form": "탕진남ㅎㅎㅎㅎㅎㅎㅎㅎㅎㄹㄹ",
