      "utterance": [
        {
          "id": "MDRW2100036708.1.1",
          "form": "저는 자몽맛이요 ㅋㅋ",
          "original_form": "저는 자몽맛이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036708.1.2",
          "form": "원래 자몽맛뿐이었는데 또 생겼군요ㄷㄷ",
          "original_form": "원래 자몽맛뿐이었는데 또 생겼군요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036708.1.3",
          "form": "이건 맛 괜찮은가요?",
          "original_form": "이건 맛 괜찮은가요?",
          "speaker_id": "2",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036708.1.4",
          "form": "맛은 좋네요",
          "original_form": "맛은 좋네요",
          "speaker_id": "1",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100036708.1.5",
          "form": "제가 어제 먹은 다이어트쉐이크보다 낫네요ㄷㄷ",
          "original_form": "제가 어제 먹은 다이어트쉐이크보다 낫네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100036708.1.6",
          "form": "ㅎㅎ 다행이에요 이틀간 그것만 마셔야하는데 맛이라도 좋아야죠",
          "original_form": "ㅎㅎ 다행이에요 이틀간 그것만 마셔야하는데 맛이라도 좋아야죠",
          "speaker_id": "2",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100036708.1.7",
          "form": "포도보단 자몽이 맛날것 같아요",
          "original_form": "포도보단 자몽이 맛날것 같아요",
          "speaker_id": "2",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100036708.1.8",
          "form": "맛은 진짜 괜찮은 과일주스네요 ㅋㅋ",
          "original_form": "맛은 진짜 괜찮은 과일주스네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:02"
        },
        {
          "id": "MDRW2100036708.1.9",
          "form": "근데 이거 건강기능식품도 아니네요 ㅋㅋ기",
          "original_form": "근데 이거 건강기능식품도 아니네요 ㅋㅋ기",
          "speaker_id": "1",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036708.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036708.1.11",
          "form": "? 근데 그 가격을 받는다고요?",
          "original_form": "? 근데 그 가격을 받는다고요?",
          "speaker_id": "2",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036708.1.12",
          "form": "진짜 양심 어디 ㅂㄷㅂㄷ",
          "original_form": "진짜 양심 어디 ㅂㄷㅂㄷ",
          "speaker_id": "2",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036708.1.13",
          "form": "저도 자세한 성분보니 어이없긴하네요",
          "original_form": "저도 자세한 성분보니 어이없긴하네요",
          "speaker_id": "1",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036708.1.14",
          "form": "광고만 그럴듯",
          "original_form": "광고만 그럴듯",
          "speaker_id": "1",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036708.1.15",
          "form": "어차피 이틀동안 물만 마시면 빠질거니ㅎ",
          "original_form": "어차피 이틀동안 물만 마시면 빠질거니ㅎ",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036708.1.16",
          "form": "말은 좋아요 클렌즈ㅋㅋ",
          "original_form": "말은 좋아요 클렌즈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036708.1.17",
          "form": "굶겨서 빼겠다는 의지가 느껴지네요ㅂㄷㅂㄷ",
          "original_form": "굶겨서 빼겠다는 의지가 느껴지네요ㅂㄷㅂㄷ",
          "speaker_id": "2",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100036708.1.18",
          "form": "아니 단백질도 안들어갔고",
          "original_form": "아니 단백질도 안들어갔고",
          "speaker_id": "1",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100036708.1.19",
          "form": "그흔한? 가르시니아?도 안들어갔네요 ㅋㅋ",
          "original_form": "그흔한? 가르시니아?도 안들어갔네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100036708.1.20",
          "form": "제돈 주고 안사서 진짜 냉정하게 평가 가능하네요 ㅋㅋ",
          "original_form": "제돈 주고 안사서 진짜 냉정하게 평가 가능하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100036708.1.21",
          "form": "? 아니ㅋ 걍 자몽주스인가요",
          "original_form": "? 아니ㅋ 걍 자몽주스인가요",
          "speaker_id": "2",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100036708.1.22",
          "form": "아 다행히 선물받으셨군요",
          "original_form": "아 다행히 선물받으셨군요",
          "speaker_id": "2",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100036708.1.23",
          "form": "그 연예인 마케팅이랑",
          "original_form": "그 연예인 마케팅이랑",
          "speaker_id": "1",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100036708.1.24",
          "form": "저는 아이돌 잘모르지만",
          "original_form": "저는 아이돌 잘모르지만",
          "speaker_id": "1",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100036708.1.25",
          "form": "네이버에 메인에 띄운 그거?가 성공적었던듯요",
          "original_form": "네이버에 메인에 띄운 그거?가 성공적었던듯요",
          "speaker_id": "1",
          "time": "20210831 10:09"
        }
      ]
    }
