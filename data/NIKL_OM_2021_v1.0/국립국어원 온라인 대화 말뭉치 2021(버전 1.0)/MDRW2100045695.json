      "utterance": [
        {
          "id": "MDRW2100045695.1.1",
          "form": "나 요즘 수업듣자나",
          "original_form": "나 요즘 수업듣자나",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045695.1.2",
          "form": "응 대학원 수업?",
          "original_form": "응 대학원 수업?",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045695.1.3",
          "form": "응응 근데 거기 팀플 수업도 있어",
          "original_form": "응응 근데 거기 팀플 수업도 있어",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045695.1.4",
          "form": "대학원에도 팀플이?",
          "original_form": "대학원에도 팀플이?",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045695.1.5",
          "form": "어휴  그니까  말야 ㅋㅋㅋ",
          "original_form": "어휴  그니까  말야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045695.1.6",
          "form": "드랍안해?",
          "original_form": "드랍안해?",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045695.1.7",
          "form": "ㅜㅜ 다른수업은 토론수업이였어",
          "original_form": "ㅜㅜ 다른수업은 토론수업이였어",
          "speaker_id": "1",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100045695.1.8",
          "form": "최악은 따로네",
          "original_form": "최악은 따로네",
          "speaker_id": "2",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100045695.1.9",
          "form": "그 수업은 바꿨어 ㅋㅋ",
          "original_form": "그 수업은 바꿨어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100045695.1.10",
          "form": "잘했네 토론은 좀..",
          "original_form": "잘했네 토론은 좀..",
          "speaker_id": "2",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100045695.1.11",
          "form": "그치 차라리 팀플이 낫네",
          "original_form": "그치 차라리 팀플이 낫네",
          "speaker_id": "1",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100045695.1.12",
          "form": "다른 수업은 어때",
          "original_form": "다른 수업은 어때",
          "speaker_id": "2",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045695.1.13",
          "form": "다른건 거의 세미나 수업이라",
          "original_form": "다른건 거의 세미나 수업이라",
          "speaker_id": "1",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045695.1.14",
          "form": "세미나라 괜차나",
          "original_form": "세미나라 괜차나",
          "speaker_id": "1",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045695.1.15",
          "form": "강사들 바뀌는거?",
          "original_form": "강사들 바뀌는거?",
          "speaker_id": "2",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045695.1.16",
          "form": "응 근데 이거 시험 어찌 보려나",
          "original_form": "응 근데 이거 시험 어찌 보려나",
          "speaker_id": "1",
          "time": "20210908 05:37"
        }
