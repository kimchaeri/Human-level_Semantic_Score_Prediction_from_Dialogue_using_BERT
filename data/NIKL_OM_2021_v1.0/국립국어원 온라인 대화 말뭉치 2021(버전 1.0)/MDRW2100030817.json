      "utterance": [
        {
          "id": "MDRW2100030817.1.1",
          "form": "그럼 나중에 할까요",
          "original_form": "그럼 나중에 할까요",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030817.1.2",
          "form": "인문학도 좋고 미래서도 좋아영",
          "original_form": "인문학도 좋고 미래서도 좋아영",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030817.1.3",
          "form": "좀쉴까여",
          "original_form": "좀쉴까여",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030817.1.4",
          "form": "30분ㅋ",
          "original_form": "30분ㅋ",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030817.1.5",
          "form": "그래요 나중에 말해 주세요",
          "original_form": "그래요 나중에 말해 주세요",
          "speaker_id": "2",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030817.1.6",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030817.1.7",
          "form": "그런데 점점 길에서 어두워지게 되는 경향 있어요",
          "original_form": "그런데 점점 길에서 어두워지게 되는 경향 있어요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030817.1.8",
          "form": "결론이 청은차군요",
          "original_form": "결론이 청은차군요",
          "speaker_id": "1",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100030817.1.9",
          "form": "통통 향",
          "original_form": "통통 향",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100030817.1.10",
          "form": "솔잎",
          "original_form": "솔잎",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100030817.1.11",
          "form": "솔잎 향",
          "original_form": "솔잎 향",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.12",
          "form": "쇼핑 잘하신 것 같네요 써보셨나요 평은 어떤가요",
          "original_form": "쇼핑 잘하신 것 같네요 써보셨나요 평은 어떤가요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.13",
          "form": "3.1네 개에 만 4천 원",
          "original_form": "3.1네 개에 만 4천 원",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.14",
          "form": "소주 한잔 들어가서 혀가 오이나 봅니다 발음이 부정확한에요",
          "original_form": "소주 한잔 들어가서 혀가 오이나 봅니다 발음이 부정확한에요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.15",
          "form": "저렴하지요",
          "original_form": "저렴하지요",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.16",
          "form": "오 괜찮네요",
          "original_form": "오 괜찮네요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.17",
          "form": "지금 지마켓 20% 세일 하거든요",
          "original_form": "지금 지마켓 20% 세일 하거든요",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.18",
          "form": "G 마켓에서 세일 하는 중이군요",
          "original_form": "G 마켓에서 세일 하는 중이군요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.19",
          "form": "그러게요 술 냄새가납니다",
          "original_form": "그러게요 술 냄새가납니다",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030817.1.20",
          "form": "아까 같은 제품을 홈플러스에서 샀거든요",
          "original_form": "아까 같은 제품을 홈플러스에서 샀거든요",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100030817.1.21",
          "form": "후 많이 나네요",
          "original_form": "후 많이 나네요",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100030817.1.22",
          "form": "한 개에 6,000원이었는데 글쎄 너무 싸서 취소하고 이걸로 샀어요",
          "original_form": "한 개에 6,000원이었는데 글쎄 너무 싸서 취소하고 이걸로 샀어요",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100030817.1.23",
          "form": "홈플러스에서도 세일을 했나요",
          "original_form": "홈플러스에서도 세일을 했나요",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100030817.1.24",
          "form": "지마켓에서 사는게 훨씬 이득 이네요",
          "original_form": "지마켓에서 사는게 훨씬 이득 이네요",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100030817.1.25",
          "form": "홈플러스는 신규 가입하면 30% 할인 쿠폰을 줍니다",
          "original_form": "홈플러스는 신규 가입하면 30% 할인 쿠폰을 줍니다",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100030817.1.26",
          "form": "그러게요 배송비가 붙어도 개이득",
          "original_form": "그러게요 배송비가 붙어도 개이득",
