      "utterance": [
        {
          "id": "MDRW2100039537.1.1",
          "form": "임영웅 콘서트. 한번 가보고싶네",
          "original_form": "임영웅 콘서트. 한번 가보고싶네",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039537.1.2",
          "form": "전에 했었제? 콘서트",
          "original_form": "전에 했었제? 콘서트",
          "speaker_id": "2",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039537.1.3",
          "form": "그 합동콘서튼가",
          "original_form": "그 합동콘서튼가",
          "speaker_id": "2",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039537.1.4",
          "form": "합동콘서트도 좋은데 못 갔지",
          "original_form": "합동콘서트도 좋은데 못 갔지",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039537.1.5",
          "form": "코로나 때메 언제 가능해지려나",
          "original_form": "코로나 때메 언제 가능해지려나",
          "speaker_id": "2",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039537.1.6",
          "form": "그때 갔어야했는데....",
          "original_form": "그때 갔어야했는데....",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039537.1.7",
          "form": "맞다 그때 가는게 나을뻔했다 이렇게 코로나 길어지고 더 심해질지 몰랐네",
          "original_form": "맞다 그때 가는게 나을뻔했다 이렇게 코로나 길어지고 더 심해질지 몰랐네",
          "speaker_id": "2",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039537.1.8",
          "form": "그러니까... 그때갔음 좋아슬텐데",
          "original_form": "그러니까... 그때갔음 좋아슬텐데",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039537.1.9",
          "form": "임영웅 콘서트  담에 하면 예매해볼게",
          "original_form": "임영웅 콘서트  담에 하면 예매해볼게",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039537.1.10",
          "form": "그래 ㅎㅎ",
          "original_form": "그래 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039537.1.11",
          "form": "임영웅 말고 가고싶은 콘서트 없나",
          "original_form": "임영웅 말고 가고싶은 콘서트 없나",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100039537.1.12",
          "form": "많지 이승철도 좋아하고 성시경도 좋고 다 좋지",
          "original_form": "많지 이승철도 좋아하고 성시경도 좋고 다 좋지",
          "speaker_id": "1",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039537.1.13",
          "form": "코로나 끝나면 콘서트 많이 가서 봐야겠네",
          "original_form": "코로나 끝나면 콘서트 많이 가서 봐야겠네",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039537.1.14",
          "form": "그래 끝나면 가서 많이 봐야지",
          "original_form": "그래 끝나면 가서 많이 봐야지",
          "speaker_id": "1",
          "time": "20210903 18:55"
        }
      ]
