        {
          "id": "MDRW2100022431.1.1",
          "form": "ㅇㅇ 유튜브 다 보면 나가야겠다",
          "original_form": "ㅇㅇ 유튜브 다 보면 나가야겠다",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100022431.1.2",
          "form": "특히 큰아들 요즘 이단뛰기 연습하니까 좀 봐줘",
          "original_form": "특히 큰아들 요즘 이단뛰기 연습하니까 좀 봐줘",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100022431.1.3",
          "form": "몇 개나 하는데?",
          "original_form": "몇 개나 하는데?",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022431.1.4",
          "form": "아직은 다섯개 정도 ㅎㅎ",
          "original_form": "아직은 다섯개 정도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022431.1.5",
          "form": "한참 더 연습해야겠네",
          "original_form": "한참 더 연습해야겠네",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022431.1.6",
          "form": "키가 커서 줄이 아슬아슬해",
          "original_form": "키가 커서 줄이 아슬아슬해",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022431.1.7",
          "form": "새로 샀잖아?",
          "original_form": "새로 샀잖아?",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022431.1.8",
          "form": "ㅇㅇ 그런데도 그렇게 줄이 길진 않더라고",
          "original_form": "ㅇㅇ 그런데도 그렇게 줄이 길진 않더라고",
          "speaker_id": "1",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022431.1.9",
          "form": "줄넘기 500개씩 하고 들어올께",
          "original_form": "줄넘기 500개씩 하고 들어올께",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022431.1.10",
          "form": "ㅎㅎ 그 정도면 적당할듯",
          "original_form": "ㅎㅎ 그 정도면 적당할듯",
          "speaker_id": "1",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022431.1.11",
          "form": "마스크 쓰고 많이 하기도 너무 힘들어",
          "original_form": "마스크 쓰고 많이 하기도 너무 힘들어",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022431.1.12",
          "form": "맞아 500새만 해도 땀찰듯",
          "original_form": "맞아 500새만 해도 땀찰듯",
          "speaker_id": "1",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022431.1.13",
          "form": "500새? ㅋㅋ",
          "original_form": "500새? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022431.1.14",
          "form": "아니 500개 오타야 ㅎㅎ",
          "original_form": "아니 500개 오타야 ㅎㅎ",
