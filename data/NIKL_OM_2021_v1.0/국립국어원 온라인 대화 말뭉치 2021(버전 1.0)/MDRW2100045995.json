      "utterance": [
        {
          "id": "MDRW2100045995.1.1",
          "form": "ㅗㄱ 아죠!!!",
          "original_form": "ㅗㄱ 아죠!!!",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045995.1.2",
          "form": "뭐래니",
          "original_form": "뭐래니",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045995.1.3",
          "form": "아뇨!! ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨!! ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045995.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ켘ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ켘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045995.1.5",
          "form": "오늘 점심은 뭐 드실 예정인가요?",
          "original_form": "오늘 점심은 뭐 드실 예정인가요?",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045995.1.6",
          "form": "마지막은 항상 잘려서 거의 못 읽고 있어요ㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "마지막은 항상 잘려서 거의 못 읽고 있어요ㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045995.1.7",
          "form": "오늘 점심 음~",
          "original_form": "오늘 점심 음~",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045995.1.8",
          "form": "으으으으으음 왠지 양배추가 먹고싶어요",
          "original_form": "으으으으으음 왠지 양배추가 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045995.1.9",
          "form": "맞춰보실래요?",
          "original_form": "맞춰보실래요?",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045995.1.10",
          "form": "으앗",
          "original_form": "으앗",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045995.1.11",
          "form": "서브웨이.",
          "original_form": "서브웨이.",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045995.1.12",
          "form": "앗 섭웨 아닙니다 후ㅜ",
          "original_form": "앗 섭웨 아닙니다 후ㅜ",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045995.1.13",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045995.1.14",
          "form": "앗 음",
          "original_form": "앗 음",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045995.1.15",
          "form": "샐러드?",
          "original_form": "샐러드?",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045995.1.16",
          "form": "아니요! 샐러드도 아니지롱",
          "original_form": "아니요! 샐러드도 아니지롱",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045995.1.17",
          "form": "재료를 물어보시는 건 어떠심까",
          "original_form": "재료를 물어보시는 건 어떠심까",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045995.1.18",
          "form": "으아악",
          "original_form": "으아악",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045995.1.19",
          "form": "일단 양배추가 들어가긴 하는거져?",
          "original_form": "일단 양배추가 들어가긴 하는거져?",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045995.1.20",
          "form": "네! 양배추 겁나 왕창 넣을거예요 낄낄낄ㄴ깅낄",
          "original_form": "네! 양배추 겁나 왕창 넣을거예요 낄낄낄ㄴ깅낄",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045995.1.21",
          "form": "직접 만들어 드시는 건가요?",
          "original_form": "직접 만들어 드시는 건가요?",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045995.1.22",
          "form": "네! 가정집에서도 충분히 만들어먹을 수 있어요",
          "original_form": "네! 가정집에서도 충분히 만들어먹을 수 있어요",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045995.1.23",
          "form": "기름으로 하는 요리인가요!",
          "original_form": "기름으로 하는 요리인가요!",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045995.1.24",
          "form": "핫 네! 맞아요 기름 들어가요",
          "original_form": "핫 네! 맞아요 기름 들어가요",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045995.1.25",
          "form": "부침가루도 들어가나요..!",
          "original_form": "부침가루도 들어가나요..!",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045995.1.26",
          "form": "아니요 밀가루 안 들어가요 호호",
          "original_form": "아니요 밀가루 안 들어가요 호호",
          "speaker_id": "2",
          "time": "20210908 09:33"
        }
      ]
