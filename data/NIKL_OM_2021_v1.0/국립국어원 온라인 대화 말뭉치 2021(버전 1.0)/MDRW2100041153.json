      "utterance": [
        {
          "id": "MDRW2100041153.1.1",
          "form": "오 어땠어요",
          "original_form": "오 어땠어요",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100041153.1.2",
          "form": "생각보가걍",
          "original_form": "생각보가걍",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100041153.1.3",
          "form": "마라가 안강하네요",
          "original_form": "마라가 안강하네요",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041153.1.4",
          "form": "가서먹는게 나아요",
          "original_form": "가서먹는게 나아요",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041153.1.5",
          "form": "이런이런 국물도 별로였나요?",
          "original_form": "이런이런 국물도 별로였나요?",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041153.1.6",
          "form": "실망하지 않앗다면 거짓말이겟죠",
          "original_form": "실망하지 않앗다면 거짓말이겟죠",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041153.1.7",
          "form": "ㅂ국물 깊지않다고 말할수 있겟어요",
          "original_form": "ㅂ국물 깊지않다고 말할수 있겟어요",
          "speaker_id": "1",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100041153.1.8",
          "form": "이런.. 면이랑 야채는요?",
          "original_form": "이런.. 면이랑 야채는요?",
          "speaker_id": "2",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100041153.1.9",
          "form": "면 많진 않은데 나쁘지 않앗어요",
          "original_form": "면 많진 않은데 나쁘지 않앗어요",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100041153.1.10",
          "form": "직접 마라소스 사서 만들어 먹는게 나을듯 해요",
          "original_form": "직접 마라소스 사서 만들어 먹는게 나을듯 해요",
          "speaker_id": "1",
          "time": "20210905 18:44"
        },
        {
          "id": "MDRW2100041153.1.11",
          "form": "아항 고기는 소고기들어있나요?",
          "original_form": "아항 고기는 소고기들어있나요?",
          "speaker_id": "2",
          "time": "20210905 18:45"
        },
        {
          "id": "MDRW2100041153.1.12",
          "form": "내 고기양 갠츈",
          "original_form": "내 고기양 갠츈",
          "speaker_id": "1",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100041153.1.13",
          "form": "구성은 갠춘",
          "original_form": "구성은 갠춘",
          "speaker_id": "1",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100041153.1.14",
          "form": "맛만 아래",
          "original_form": "맛만 아래",
          "speaker_id": "1",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100041153.1.15",
          "form": "아항 마라맛좀 잘하지 프레시지 안되겠네요",
          "original_form": "아항 마라맛좀 잘하지 프레시지 안되겠네요",
          "speaker_id": "2",
          "time": "20210905 18:50"
        },
        {
          "id": "MDRW2100041153.1.16",
          "form": "그죠잉",
          "original_form": "그죠잉",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100041153.1.17",
          "form": "이금기소스사묵지 ㅎㅎ",
          "original_form": "이금기소스사묵지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100041153.1.18",
          "form": "아스크림 먹고싶어요",
          "original_form": "아스크림 먹고싶어요",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100041153.1.19",
          "form": "무슨 아이스크림먹고싶어요?",
          "original_form": "무슨 아이스크림먹고싶어요?",
          "speaker_id": "2",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100041153.1.20",
          "form": "배라",
          "original_form": "배라",
          "speaker_id": "1",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100041153.1.21",
          "form": "아심 ?",
          "original_form": "아심 ?",
          "speaker_id": "1",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100041153.1.22",
          "form": "배라 무슨맛 먹고 싶어용?",
          "original_form": "배라 무슨맛 먹고 싶어용?",
          "speaker_id": "2",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100041153.1.23",
          "form": "레인보우샤베트",
          "original_form": "레인보우샤베트",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041153.1.24",
          "form": "나머지 맞춰바요",
          "original_form": "나머지 맞춰바요",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041153.1.25",
          "form": "맞쵸바!",
          "original_form": "맞쵸바!",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041153.1.26",
          "form": "(아이유노래)",
          "original_form": "(아이유노래)",
          "speaker_id": "1",
          "time": "20210905 18:55"
        }
      ]
