          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043068.1.4",
          "form": "아 맛있죠!",
          "original_form": "아 맛있죠!",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043068.1.5",
          "form": "이름이 그렇더라구요 ㅎㅎㅎ",
          "original_form": "이름이 그렇더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043068.1.6",
          "form": "약간 페스츄리 맛 나는거 맞져??",
          "original_form": "약간 페스츄리 맛 나는거 맞져??",
          "speaker_id": "2",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043068.1.7",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043068.1.8",
          "form": "맞아영 !! 바삭하니",
          "original_form": "맞아영 !! 바삭하니",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043068.1.9",
          "form": "그거 좋아했어요!!",
          "original_form": "그거 좋아했어요!!",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043068.1.10",
          "form": "저번주에 크림빵 간만에 먹으니까 너무 맛있더라고요",
          "original_form": "저번주에 크림빵 간만에 먹으니까 너무 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043068.1.11",
          "form": "그 네모난 긴걸로 샀는데 다 먹어버릴뻔 했어요 ㅋㅋㅋ",
          "original_form": "그 네모난 긴걸로 샀는데 다 먹어버릴뻔 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043068.1.12",
          "form": "이름이 뭘까요?",
          "original_form": "이름이 뭘까요?",
          "speaker_id": "1",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043068.1.13",
          "form": "ㅋㅋㅋ 잠시만요 요기요에서 시켰던거라",
          "original_form": "ㅋㅋㅋ 잠시만요 요기요에서 시켰던거라",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043068.1.14",
          "form": "후레쉬크림샌드빵이래요!!",
          "original_form": "후레쉬크림샌드빵이래요!!",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043068.1.15",
          "form": "아 그거 너무 맛있죠ㅠㅠ 안에 크림든거",
          "original_form": "아 그거 너무 맛있죠ㅠㅠ 안에 크림든거",
          "speaker_id": "1",
          "time": "20210907 14:21"
        },
        {
          "id": "MDRW2100043068.1.16",
          "form": "두겹짜리",
          "original_form": "두겹짜리",
          "speaker_id": "1",
          "time": "20210907 14:21"
        },
        {
          "id": "MDRW2100043068.1.17",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210907 14:21"
        },
        {
          "id": "MDRW2100043068.1.18",
          "form": "저 저런 땅콩크림빵도 좋아해요ㅋㅋㅋ",
