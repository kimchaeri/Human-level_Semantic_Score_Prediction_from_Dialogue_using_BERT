      "utterance": [
        {
          "id": "MDRW2100043068.1.1",
          "form": "아 이 빵 먹어본적 있어요",
          "original_form": "아 이 빵 먹어본적 있어요",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043068.1.2",
          "form": "이름이 초면이라",
