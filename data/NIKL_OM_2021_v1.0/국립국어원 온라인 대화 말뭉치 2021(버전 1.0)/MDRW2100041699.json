      "utterance": [
        {
          "id": "MDRW2100041699.1.1",
          "form": "이제 퇴근",
          "original_form": "이제 퇴근",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100041699.1.2",
          "form": "반찬사고 사과사서 가야지",
          "original_form": "반찬사고 사과사서 가야지",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100041699.1.3",
          "form": "우유도 사고",
          "original_form": "우유도 사고",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100041699.1.4",
          "form": "응응 살게 많네 ㅋㅋㅋ",
          "original_form": "응응 살게 많네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:01"
        },
        {
          "id": "MDRW2100041699.1.5",
          "form": "고생했따 ㅋㅋ",
          "original_form": "고생했따 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:01"
        },
        {
          "id": "MDRW2100041699.1.6",
          "form": "언니 ㅋㅋ",
          "original_form": "언니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041699.1.7",
          "form": "첫째 집에 왔는데 간식으로 김치볶음밥을 해달래",
          "original_form": "첫째 집에 왔는데 간식으로 김치볶음밥을 해달래",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041699.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 간식으로 김치볶음밥이라니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 간식으로 김치볶음밥이라니",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041699.1.9",
          "form": "정말 너랑 똑같구나",
          "original_form": "정말 너랑 똑같구나",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041699.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041699.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저녁에 해주기로햇다 ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저녁에 해주기로햇다 ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041699.1.12",
          "form": "밥솥에 밥이 없다",
          "original_form": "밥솥에 밥이 없다",
          "speaker_id": "1",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041699.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 엄마가 김치볶음밥 먹는다고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 엄마가 김치볶음밥 먹는다고",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041699.1.14",
          "form": "밥 다 먹어버렸네",
          "original_form": "밥 다 먹어버렸네",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041699.1.15",
          "form": "ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041699.1.16",
          "form": "응 그러니깐 ㅋㅋ",
          "original_form": "응 그러니깐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041699.1.17",
          "form": "먹을 줄 알았으면 남겼지...ㅋㅋ",
          "original_form": "먹을 줄 알았으면 남겼지...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100041699.1.18",
          "form": "푸핫 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "푸핫 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100041699.1.19",
          "form": "쌀이 20 키로 가지고는 뭐 며칠 가도 못하겠다",
          "original_form": "쌀이 20 키로 가지고는 뭐 며칠 가도 못하겠다",
          "speaker_id": "2",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041699.1.20",
          "form": "대단하다 대단해",
          "original_form": "대단하다 대단해",
          "speaker_id": "2",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041699.1.21",
          "form": "ㅋㅋㅋ그러니깐 언니 ㅋㅋ",
          "original_form": "ㅋㅋㅋ그러니깐 언니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100041699.1.22",
          "form": "그래서 매달 쌀을 사는 느낌이야",
          "original_form": "그래서 매달 쌀을 사는 느낌이야",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100041699.1.23",
          "form": "그래 그 정도 먹으면",
          "original_form": "그래 그 정도 먹으면",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041699.1.24",
          "form": "매달 사야겠는데",
          "original_form": "매달 사야겠는데",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041699.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041699.1.26",
          "form": "그러니깐 ㅋㅋ",
          "original_form": "그러니깐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041699.1.27",
          "form": "다들 탄수화물 중독이다",
          "original_form": "다들 탄수화물 중독이다",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041699.1.28",
          "form": "이러니 살이 안빠진다 ...내가...ㅋㅋ",
          "original_form": "이러니 살이 안빠진다 ...내가...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041699.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ 그래 잘먹는다 정말",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ 그래 잘먹는다 정말",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041699.1.30",
          "form": "너 집 쌀 싸는 속도를 내가 느낄 정도니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너 집 쌀 싸는 속도를 내가 느낄 정도니 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041699.1.31",
          "form": "아 웃겨 ㅋㅋ",
          "original_form": "아 웃겨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041699.1.32",
          "form": "오늘 뭐 하기 귀찮다 급 ㅋㅋㅋ 라면 생각나네 ㅋ",
          "original_form": "오늘 뭐 하기 귀찮다 급 ㅋㅋㅋ 라면 생각나네 ㅋ",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041699.1.33",
          "form": "라면 좋지",
          "original_form": "라면 좋지",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041699.1.34",
          "form": "난 오늘 다 반찬나무숟가락",
          "original_form": "난 오늘 다 반찬나무숟가락",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041699.1.35",
          "form": "쿡이랑 반찬",
          "original_form": "쿡이랑 반찬",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041699.1.36",
          "form": "해결해야겠다",
          "original_form": "해결해야겠다",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041699.1.37",
          "form": "나무숟가락이 제일이쥬",
          "original_form": "나무숟가락이 제일이쥬",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041699.1.38",
          "form": "나도 내일은 나무숟가락반찬으로 ㅋㅋ",
