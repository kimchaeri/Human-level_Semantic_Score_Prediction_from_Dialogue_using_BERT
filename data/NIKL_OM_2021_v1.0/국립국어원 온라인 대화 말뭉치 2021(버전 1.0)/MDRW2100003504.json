      "utterance": [
        {
          "id": "MDRW2100003504.1.1",
          "form": "넷플릭스로 드라마보는데 막장이야",
          "original_form": "넷플릭스로 드라마보는데 막장이야",
          "speaker_id": "1",
          "time": "20210820 23:56"
        },
        {
          "id": "MDRW2100003504.1.2",
          "form": "잼는거 보는거 뭐 이썽?",
          "original_form": "잼는거 보는거 뭐 이썽?",
          "speaker_id": "1",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003504.1.3",
          "form": "신서유기 보고있어",
          "original_form": "신서유기 보고있어",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003504.1.4",
          "form": "신서유기 본적있어?",
          "original_form": "신서유기 본적있어?",
          "speaker_id": "2",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003504.1.5",
          "form": "완전",
          "original_form": "완전",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003504.1.6",
          "form": "너무재밌지",
          "original_form": "너무재밌지",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003504.1.7",
          "form": "제일좋아해",
          "original_form": "제일좋아해",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003504.1.8",
          "form": "이번 스프링캠프 보다가 말았는데",
          "original_form": "이번 스프링캠프 보다가 말았는데",
          "speaker_id": "2",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003504.1.9",
          "form": "감질맛나서 몰아보려고",
          "original_form": "감질맛나서 몰아보려고",
          "speaker_id": "2",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003504.1.10",
          "form": "ㅋㅋㅋㅋ하나씩 보기엔 감질맛나긴 해",
          "original_form": "ㅋㅋㅋㅋ하나씩 보기엔 감질맛나긴 해",
          "speaker_id": "1",
          "time": "20210821 13:02"
        },
        {
          "id": "MDRW2100003504.1.11",
          "form": "요즘 연애의참견도 많이 봐",
          "original_form": "요즘 연애의참견도 많이 봐",
          "speaker_id": "1",
          "time": "20210821 13:02"
        },
        {
          "id": "MDRW2100003504.1.12",
          "form": "그건 첨들어보는데",
          "original_form": "그건 첨들어보는데",
          "speaker_id": "2",
          "time": "20210821 13:08"
        },
        {
          "id": "MDRW2100003504.1.13",
          "form": "나 넷플릭스 가입도 안했어",
          "original_form": "나 넷플릭스 가입도 안했어",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003504.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003504.1.15",
          "form": "아ㅋㅋㅋ그럼 유튜브는 봐?",
          "original_form": "아ㅋㅋㅋ그럼 유튜브는 봐?",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100003504.1.16",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100003504.1.17",
          "form": "현대인의 필수 덕목 아니겠냐",
          "original_form": "현대인의 필수 덕목 아니겠냐",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100003504.1.18",
          "form": "유튜브 뭐봐!!!",
          "original_form": "유튜브 뭐봐!!!",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100003504.1.19",
          "form": "이것저것 보는데 요즘은 침착맨 유튜브가 재밌더라",
          "original_form": "이것저것 보는데 요즘은 침착맨 유튜브가 재밌더라",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100003504.1.20",
          "form": "침착맨ㅋㅋㅋ재밌어보이더라",
          "original_form": "침착맨ㅋㅋㅋ재밌어보이더라",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100003504.1.21",
          "form": "너는 유튜브 뭐봐?",
          "original_form": "너는 유튜브 뭐봐?",
          "speaker_id": "2",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100003504.1.22",
          "form": "나는 한사랑산악회...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나는 한사랑산악회...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100003504.1.23",
          "form": "요즘 피식세계관에 푹 빠졌어",
          "original_form": "요즘 피식세계관에 푹 빠졌어",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100003504.1.24",
          "form": "ㅋㅋㅋㅋ그것도 재밌지",
          "original_form": "ㅋㅋㅋㅋ그것도 재밌지",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100003504.1.25",
          "form": "그거 보다가 알수없는 알고리즘으로 매드몬스터 봤는데",
          "original_form": "그거 보다가 알수없는 알고리즘으로 매드몬스터 봤는데",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100003504.1.26",
          "form": "ㅋㅋㅋㅋ매드몬스터 넘웃겨 진짜",
          "original_form": "ㅋㅋㅋㅋ매드몬스터 넘웃겨 진짜",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100003504.1.27",
          "form": "쓸데없이 너무 고퀄이라 열받아ㅋㅋㅋㅋ",
          "original_form": "쓸데없이 너무 고퀄이라 열받아ㅋㅋㅋㅋ",
