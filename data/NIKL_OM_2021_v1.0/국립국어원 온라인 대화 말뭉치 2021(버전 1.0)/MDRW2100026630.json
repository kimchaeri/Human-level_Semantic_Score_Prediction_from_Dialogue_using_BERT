      "utterance": [
        {
          "id": "MDRW2100026630.1.1",
          "form": "섬유유연제 어디꺼?",
          "original_form": "섬유유연제 어디꺼?",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026630.1.2",
          "form": "나는 피앤지꺼씀ㅋ",
          "original_form": "나는 피앤지꺼씀ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026630.1.3",
          "form": "나는다우디써ㅋㅋㅋ",
          "original_form": "나는다우디써ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026630.1.4",
          "form": "다우니실내건조추천ㅋ",
          "original_form": "다우니실내건조추천ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026630.1.5",
          "form": "나는스너글이제일좋더라",
          "original_form": "나는스너글이제일좋더라",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026630.1.6",
          "form": "곰돌이그려져있는거?",
          "original_form": "곰돌이그려져있는거?",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026630.1.7",
          "form": "그거ㅋㅋ애기",
          "original_form": "그거ㅋㅋ애기",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026630.1.8",
          "form": "냄새나",
          "original_form": "냄새나",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026630.1.9",
          "form": "오ㅋㅋㅋ좋다ㅋㅋ지속력은?",
          "original_form": "오ㅋㅋㅋ좋다ㅋㅋ지속력은?",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026630.1.10",
          "form": "오래가는편이야ㅋ",
          "original_form": "오래가는편이야ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026630.1.11",
          "form": "다우니는?",
          "original_form": "다우니는?",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026630.1.12",
          "form": "괜춘",
          "original_form": "괜춘",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026630.1.13",
          "form": "지속력오래가ㅍㅋ",
          "original_form": "지속력오래가ㅍㅋ",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026630.1.14",
          "form": "달콤한향이야?",
          "original_form": "달콤한향이야?",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026630.1.15",
          "form": "완전단향은아닌데ㅋㅋ좋아",
          "original_form": "완전단향은아닌데ㅋㅋ좋아",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026630.1.16",
          "form": "코튼향도좋은데",
          "original_form": "코튼향도좋은데",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026630.1.17",
          "form": "샴푸는 뭐써?",
          "original_form": "샴푸는 뭐써?",
