      "utterance": [
        {
          "id": "MDRW2100043374.1.1",
          "form": "최근에 뭐 사신거 있으세요?",
          "original_form": "최근에 뭐 사신거 있으세요?",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043374.1.2",
          "form": "최근에 사신것 중에 자랑하실 것 있으신가요",
          "original_form": "최근에 사신것 중에 자랑하실 것 있으신가요",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043374.1.3",
          "form": "저는 가을 브라운 로퍼요",
          "original_form": "저는 가을 브라운 로퍼요",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043374.1.4",
          "form": "와 바바리에 어울리겠네요",
          "original_form": "와 바바리에 어울리겠네요",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043374.1.5",
          "form": "신어보셨나요",
          "original_form": "신어보셨나요",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043374.1.6",
          "form": "네네 사이즈도 잘 맞고 좋아요",
          "original_form": "네네 사이즈도 잘 맞고 좋아요",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043374.1.7",
          "form": "저는 인쇼로 신발사면 거의다 실패해서",
          "original_form": "저는 인쇼로 신발사면 거의다 실패해서",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043374.1.8",
          "form": "제발이 볼은 넓은데 길이는 짧아서",
          "original_form": "제발이 볼은 넓은데 길이는 짧아서",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043374.1.9",
          "form": "인쇼로사면 안맞네요",
          "original_form": "인쇼로사면 안맞네요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043374.1.10",
          "form": "잘맞는 분들 부러워요",
          "original_form": "잘맞는 분들 부러워요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043374.1.11",
          "form": "맞아요ㅠㅠㅠ 실패가 많다가 하나씩 잘 맞아요",
          "original_form": "맞아요ㅠㅠㅠ 실패가 많다가 하나씩 잘 맞아요",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043374.1.12",
          "form": "ㅋㅋ인쇼는 또 반품하기도 아깝고 번거로워서",
          "original_form": "ㅋㅋ인쇼는 또 반품하기도 아깝고 번거로워서",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043374.1.13",
          "form": "실패하면 그냥 방치하게되네요",
          "original_form": "실패하면 그냥 방치하게되네요",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043374.1.14",
          "form": "맞아요ㅠㅠㅠ 잘사면 또 잘사서 또 사고 ㅋㅋㅋ",
          "original_form": "맞아요ㅠㅠㅠ 잘사면 또 잘사서 또 사고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043374.1.15",
          "form": "돈없어져요",
          "original_form": "돈없어져요",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043374.1.16",
          "form": "한번 성공하면 거기서 자주사게 되더라구요",
          "original_form": "한번 성공하면 거기서 자주사게 되더라구요",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043374.1.17",
          "form": "자주 구매하시는 브랜드있으세요?",
          "original_form": "자주 구매하시는 브랜드있으세요?",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043374.1.18",
          "form": "맞아요 저는 가내스라 원로그 좋아해요",
          "original_form": "맞아요 저는 가내스라 원로그 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043374.1.19",
          "form": "아 로퍼브랜인가요?",
          "original_form": "아 로퍼브랜인가요?",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043374.1.20",
          "form": "아뇨 옷가게에요 ㅋㅋㅋ",
          "original_form": "아뇨 옷가게에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043374.1.21",
          "form": "신발은 아무데거나 사요 ㅋㅋ",
          "original_form": "신발은 아무데거나 사요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043374.1.22",
          "form": "아아ㅋㅋ 뭔가 브랜드이름이 캐쥬얼 쪽인거같아요",
          "original_form": "아아ㅋㅋ 뭔가 브랜드이름이 캐쥬얼 쪽인거같아요",
          "speaker_id": "2",
          "time": "20210907 19:19"
        }
