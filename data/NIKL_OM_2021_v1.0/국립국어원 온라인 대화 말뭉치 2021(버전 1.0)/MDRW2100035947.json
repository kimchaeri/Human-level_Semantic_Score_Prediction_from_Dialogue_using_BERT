      "utterance": [
        {
          "id": "MDRW2100035947.1.1",
          "form": "말씀만으로도 감사해요ㅎㅎ 점심 드셨나요?",
          "original_form": "말씀만으로도 감사해요ㅎㅎ 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100035947.1.2",
          "form": "넵! 바베큐치킨 먹었어욬ㅋㅋ",
          "original_form": "넵! 바베큐치킨 먹었어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100035947.1.3",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100035947.1.4",
          "form": "저도 방금 돼지불고기 먹었어요ㅎㅎ",
          "original_form": "저도 방금 돼지불고기 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035947.1.5",
          "form": "오~~ 역시 점심에 고기 먹어줘야 힘이나죵",
          "original_form": "오~~ 역시 점심에 고기 먹어줘야 힘이나죵",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100035947.1.6",
          "form": "맞아요ㅋㅋㅋㅋ 점심엔 빵빵하게 먹어줘야죠ㅎㅎ",
          "original_form": "맞아요ㅋㅋㅋㅋ 점심엔 빵빵하게 먹어줘야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100035947.1.7",
          "form": "맞아용ㅋㅋㅋ",
          "original_form": "맞아용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100035947.1.8",
          "form": "후식으로 수박 먹었슴닷!",
          "original_form": "후식으로 수박 먹었슴닷!",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100035947.1.9",
          "form": "후식 드셨어요?",
          "original_form": "후식 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100035947.1.10",
          "form": "저는 사과 먹었어요!",
          "original_form": "저는 사과 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100035947.1.11",
          "form": "오오! 역시 과일%~",
          "original_form": "오오! 역시 과일%~",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100035947.1.12",
          "form": "밥먹고 나면 후식 먹고싶어요ㅋㅋ",
          "original_form": "밥먹고 나면 후식 먹고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100035947.1.13",
          "form": "입이 뭔가 심심하죠ㅋㅋㅋㅋㅋ",
          "original_form": "입이 뭔가 심심하죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100035947.1.14",
          "form": "넼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100035947.1.15",
          "form": "습관을 잘못드렸어요ㅋㅋ",
          "original_form": "습관을 잘못드렸어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100035947.1.16",
          "form": "밥 먹고 단 걸 먹으면 안 좋다는대 말이져ㅠ",
          "original_form": "밥 먹고 단 걸 먹으면 안 좋다는대 말이져ㅠ",
          "speaker_id": "2",
          "time": "20210830 16:46"
        },
        {
          "id": "MDRW2100035947.1.17",
          "form": "맞아여ㅜㅠ 근데 제 몸이 너무 좋아하네요ㅜ",
          "original_form": "맞아여ㅜㅠ 근데 제 몸이 너무 좋아하네요ㅜ",
