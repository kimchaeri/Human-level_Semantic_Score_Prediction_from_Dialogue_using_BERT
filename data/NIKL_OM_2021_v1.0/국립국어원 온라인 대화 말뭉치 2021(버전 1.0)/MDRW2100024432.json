      "utterance": [
        {
          "id": "MDRW2100024432.1.1",
          "form": "name1님께 닿을 날을 은근 바라보면서..ㅋㅋ",
          "original_form": "&name1&님께 닿을 날을 은근 바라보면서..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:18"
        },
        {
          "id": "MDRW2100024432.1.2",
          "form": "앟ㅋㅋㅋㅋ",
          "original_form": "앟ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:18"
        },
        {
          "id": "MDRW2100024432.1.3",
          "form": "제가 잘은 모르겠는데 마켓은 사업자 등록증이 필요하니까",
          "original_form": "제가 잘은 모르겠는데 마켓은 사업자 등록증이 필요하니까",
          "speaker_id": "1",
          "time": "20210831 09:18"
        },
        {
          "id": "MDRW2100024432.1.4",
          "form": "뭐였지..? 약간 플리마켓같은걸로 먼저 시작하곤 한가봐요!",
          "original_form": "뭐였지..? 약간 플리마켓같은걸로 먼저 시작하곤 한가봐요!",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100024432.1.5",
          "form": "제 친구도 먼저 그런걸로 했던 것 같아요",
          "original_form": "제 친구도 먼저 그런걸로 했던 것 같아요",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100024432.1.6",
          "form": "오옹!",
          "original_form": "오옹!",
          "speaker_id": "2",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100024432.1.7",
          "form": "플리마켓은 사업자 없이 신청 가능한건가봐용?!",
          "original_form": "플리마켓은 사업자 없이 신청 가능한건가봐용?!",
          "speaker_id": "2",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100024432.1.8",
          "form": "음...사실 저도 자세히 아는게 아니라 확실힌 말씀 못드리겠어요",
          "original_form": "음...사실 저도 자세히 아는게 아니라 확실힌 말씀 못드리겠어요",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100024432.1.9",
          "form": "근데 아마 음식이 아니면 그렇게 까다롭진 않을거에요!",
          "original_form": "근데 아마 음식이 아니면 그렇게 까다롭진 않을거에요!",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100024432.1.10",
          "form": "오오 알아봐야겠어용 감사합니당!",
          "original_form": "오오 알아봐야겠어용 감사합니당!",
          "speaker_id": "2",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100024432.1.11",
          "form": "그런 생각은 못해봤거든요!",
          "original_form": "그런 생각은 못해봤거든요!",
          "speaker_id": "2",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024432.1.12",
          "form": "ㅎㅎ name2님 뭘해도 잘되셨음 좋겠네욥!!",
          "original_form": "ㅎㅎ &name2&님 뭘해도 잘되셨음 좋겠네욥!!",
          "speaker_id": "1",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100024432.1.13",
          "form": "또 사고 싶으신거 있으세요?",
          "original_form": "또 사고 싶으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100024432.1.14",
          "form": "감사합니다ㅠㅅㅠ name1님덕분에 잘할 수 있을 것만 같아용",
          "original_form": "감사합니다ㅠㅅㅠ &name1&님덕분에 잘할 수 있을 것만 같아용",
          "speaker_id": "2",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100024432.1.15",
          "form": "가디건이요!!",
          "original_form": "가디건이요!!",
          "speaker_id": "2",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100024432.1.16",
          "form": "가디건의 계절이니 신상을 하나",
          "original_form": "가디건의 계절이니 신상을 하나",
          "speaker_id": "2",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100024432.1.17",
          "form": "사고싶네용!",
          "original_form": "사고싶네용!",
          "speaker_id": "2",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100024432.1.18",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100024432.1.19",
          "form": "저도 얇은 가디건 하나 사고 싶어서",
          "original_form": "저도 얇은 가디건 하나 사고 싶어서",
          "speaker_id": "1",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100024432.1.20",
          "form": "쇼핑몰 돌아다니는 중이에요ㅋㅋㅋ",
          "original_form": "쇼핑몰 돌아다니는 중이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100024432.1.21",
          "form": "오오!! 예쁜게 너무 많아 못고르겠어요ㅠ",
          "original_form": "오오!! 예쁜게 너무 많아 못고르겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100024432.1.22",
          "form": "찜해두신 거 있으세요?",
          "original_form": "찜해두신 거 있으세요?",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100024432.1.23",
          "form": "넵! 전 로엠에서 맘에 드는 가디건이 있어서",
          "original_form": "넵! 전 로엠에서 맘에 드는 가디건이 있어서",
          "speaker_id": "1",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100024432.1.24",
          "form": "장바구니에 담아뒀어요ㅎㅎ",
          "original_form": "장바구니에 담아뒀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:12"
        },
        {
          "id": "MDRW2100024432.1.25",
          "form": "요즘 세일 많이 하더라구요!",
          "original_form": "요즘 세일 많이 하더라구요!",
          "speaker_id": "1",
          "time": "20210831 10:12"
        },
        {
          "id": "MDRW2100024432.1.26",
          "form": "오오ㅋㅋㅋ 명절이 가까워져서 그런걸까요?",
          "original_form": "오오ㅋㅋㅋ 명절이 가까워져서 그런걸까요?",
          "speaker_id": "2",
          "time": "20210831 10:12"
        },
        {
          "id": "MDRW2100024432.1.27",
          "form": "세일하는곳이 꽤 있는거같아요",
          "original_form": "세일하는곳이 꽤 있는거같아요",
          "speaker_id": "2",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100024432.1.28",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100024432.1.29",
          "form": "작년상품들도",
          "original_form": "작년상품들도",
          "speaker_id": "1",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100024432.1.30",
          "form": "이월돼서 싸게 나오고 있고 넘 좋아요ㅋㅋㅋ",
          "original_form": "이월돼서 싸게 나오고 있고 넘 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100024432.1.31",
          "form": "이렇게 지갑이 순삭..",
          "original_form": "이렇게 지갑이 순삭..",
          "speaker_id": "1",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024432.1.32",
          "form": "맞아용! 예전엔 이월 별로인게 많았는데 요즘은 아니더라구용",
          "original_form": "맞아용! 예전엔 이월 별로인게 많았는데 요즘은 아니더라구용",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024432.1.33",
          "form": "텅텅 비는 텅장..",
          "original_form": "텅텅 비는 텅장..",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024432.1.34",
          "form": "맞아요..ㅎㅎㅎㅎ",
          "original_form": "맞아요..ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100024432.1.35",
          "form": "코로나라 다른데 돈쓰는데가 별로 없으니",
          "original_form": "코로나라 다른데 돈쓰는데가 별로 없으니",
          "speaker_id": "1",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100024432.1.36",
          "form": "옷만 더 사는 것 같아요ㅋㅋ",
          "original_form": "옷만 더 사는 것 같아요ㅋㅋ",
