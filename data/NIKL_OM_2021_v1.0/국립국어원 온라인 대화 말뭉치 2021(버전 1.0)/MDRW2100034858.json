      "utterance": [
        {
          "id": "MDRW2100034858.1.1",
          "form": "댄스곡은 잘 안들으세요?",
          "original_form": "댄스곡은 잘 안들으세요?",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100034858.1.2",
          "form": "신곡 나오면 몇 번 들어보는 정도로 들어요",
          "original_form": "신곡 나오면 몇 번 들어보는 정도로 들어요",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100034858.1.3",
          "form": "댄스곡 좋아하세요?",
          "original_form": "댄스곡 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100034858.1.4",
          "form": "네 저는 좋아해요 ㅎㅎ",
          "original_form": "네 저는 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100034858.1.5",
          "form": "노래 추천해주실 수 있나요?",
          "original_form": "노래 추천해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100034858.1.6",
          "form": "음 좀 어떤 계열 좋아하세요?!",
          "original_form": "음 좀 어떤 계열 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100034858.1.7",
          "form": "댄스곡은 신나는 게 좋죠!",
          "original_form": "댄스곡은 신나는 게 좋죠!",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100034858.1.8",
          "form": "음 그러면",
          "original_form": "음 그러면",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034858.1.9",
          "form": "세븐틴의 우리의 새벽은 낮보다 뜨겁다",
          "original_form": "세븐틴의 우리의 새벽은 낮보다 뜨겁다",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034858.1.10",
          "form": "들어보세요 !",
          "original_form": "들어보세요 !",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034858.1.11",
          "form": "네네 감사합니다ㅎㅎ",
          "original_form": "네네 감사합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034858.1.12",
          "form": "유튜브 자주 보시나요?",
          "original_form": "유튜브 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034858.1.13",
          "form": "네 자주 봐요 ㅎㅎㅎ",
          "original_form": "네 자주 봐요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034858.1.14",
          "form": "어떤 유튜버 자주 보세요?",
          "original_form": "어떤 유튜버 자주 보세요?",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034858.1.15",
          "form": "전 먹방 좋아해서 입짧은햇님이요!",
          "original_form": "전 먹방 좋아해서 입짧은햇님이요!",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034858.1.16",
          "form": "저도 햇님 님 영상 많이 봐요!",
          "original_form": "저도 햇님 님 영상 많이 봐요!",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034858.1.17",
          "form": "김치찜에 계란말이 드시는 거 제 최애 영상이에요ㅋㅋㅋ",
          "original_form": "김치찜에 계란말이 드시는 거 제 최애 영상이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034858.1.18",
          "form": "아 저두욬ㅋㅋㅋ 전 햇님님이 한식 먹는거 좋아해요 !",
          "original_form": "아 저두욬ㅋㅋㅋ 전 햇님님이 한식 먹는거 좋아해요 !",
          "speaker_id": "2",
