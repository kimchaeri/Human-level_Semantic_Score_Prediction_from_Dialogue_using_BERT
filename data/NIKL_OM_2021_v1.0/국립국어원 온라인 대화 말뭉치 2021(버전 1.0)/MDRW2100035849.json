      "utterance": [
        {
          "id": "MDRW2100035849.1.1",
          "form": "후라이드의 근본 황올이죵",
          "original_form": "후라이드의 근본 황올이죵",
          "speaker_id": "1",
          "time": "20210830 09:07"
        },
        {
          "id": "MDRW2100035849.1.2",
          "form": "진짜..",
          "original_form": "진짜..",
          "speaker_id": "2",
          "time": "20210830 09:07"
        },
        {
          "id": "MDRW2100035849.1.3",
          "form": "근본 그 자체더라고요",
          "original_form": "근본 그 자체더라고요",
          "speaker_id": "2",
          "time": "20210830 09:07"
        },
        {
          "id": "MDRW2100035849.1.4",
          "form": "바삭바삭!ㅋㅋㅋ",
          "original_form": "바삭바삭!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035849.1.5",
          "form": "동네 지점에서 시켜봤는데 ㅂㄹ길래 안먹다가",
          "original_form": "동네 지점에서 시켜봤는데 ㅂㄹ길래 안먹다가",
          "speaker_id": "2",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035849.1.6",
          "form": "아 저희두 이사오기전 지점은 뱔로였어요ㅠㅠ",
          "original_form": "아 저희두 이사오기전 지점은 뱔로였어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035849.1.7",
          "form": "포장주문 시켜놓고 좀 멀리에서 사왔거든요",
          "original_form": "포장주문 시켜놓고 좀 멀리에서 사왔거든요",
          "speaker_id": "2",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035849.1.8",
          "form": "저랑 뭔가 통하시는것같아요ㅋㅋㅋㅋ",
          "original_form": "저랑 뭔가 통하시는것같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035849.1.9",
          "form": "근데ㅠㅠ 사와서 씻고 먹었더니",
          "original_form": "근데ㅠㅠ 사와서 씻고 먹었더니",
          "speaker_id": "2",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035849.1.10",
          "form": "뜨끈함이 없어져서 아쉬웠는데도 진짜 jmt",
          "original_form": "뜨끈함이 없어져서 아쉬웠는데도 진짜 jmt",
          "speaker_id": "2",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100035849.1.11",
          "form": "아아ㅠㅠ 그럴땐 에어프라이기로 소생술해줘야해요ㅠㅠㅠ",
          "original_form": "아아ㅠㅠ 그럴땐 에어프라이기로 소생술해줘야해요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100035849.1.12",
          "form": "ㅠㅠ하 에어프라이기 없는게 한이에요",
          "original_form": "ㅠㅠ하 에어프라이기 없는게 한이에요",
          "speaker_id": "2",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100035849.1.13",
          "form": "저도 비비큐 치킨은 배달료까지하면 거즘 이만원이라",
          "original_form": "저도 비비큐 치킨은 배달료까지하면 거즘 이만원이라",
          "speaker_id": "1",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100035849.1.14",
          "form": "방문포장 자주해여",
          "original_form": "방문포장 자주해여",
          "speaker_id": "1",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100035849.1.15",
          "form": "식어서 아쉽다ㅠㅠ 하면서 얌얌..",
          "original_form": "식어서 아쉽다ㅠㅠ 하면서 얌얌..",
          "speaker_id": "2",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100035849.1.16",
          "form": "진짜 요즘 치킨값 너무 비싸죠",
          "original_form": "진짜 요즘 치킨값 너무 비싸죠",
          "speaker_id": "2",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100035849.1.17",
          "form": "ㅠㅠ 그래도 맛있는건 식어도 맛있긴해요ㅋㅋㅋ",
          "original_form": "ㅠㅠ 그래도 맛있는건 식어도 맛있긴해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:09"
        },
