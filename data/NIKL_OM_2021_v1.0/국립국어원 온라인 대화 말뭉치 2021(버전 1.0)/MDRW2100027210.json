        },
        {
          "id": "MDRW2100027210.1.2",
          "form": "여긴더운뎅",
          "original_form": "여긴더운뎅",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027210.1.3",
          "form": "해떴엉",
          "original_form": "해떴엉",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027210.1.4",
          "form": "쫌따가다시 흐려지면서 비오겟찌",
          "original_form": "쫌따가다시 흐려지면서 비오겟찌",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027210.1.5",
          "form": "날씨가 오락가락해",
          "original_form": "날씨가 오락가락해",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027210.1.6",
          "form": "난 후집입었는디",
          "original_form": "난 후집입었는디",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027210.1.7",
          "form": "에어컨도 안킴",
          "original_form": "에어컨도 안킴",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027210.1.8",
          "form": "난반팔입엇는뎅",
          "original_form": "난반팔입엇는뎅",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027210.1.9",
          "form": "선풍기켯는뎅",
          "original_form": "선풍기켯는뎅",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027210.1.10",
          "form": "제습으로 켜놓지",
          "original_form": "제습으로 켜놓지",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027210.1.11",
          "form": "선풍기를?",
          "original_form": "선풍기를?",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027210.1.12",
          "form": "에어컨",
          "original_form": "에어컨",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027210.1.13",
          "form": "에어컨안켰는뎅",
          "original_form": "에어컨안켰는뎅",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027210.1.14",
          "form": "해떳다가금방사라진당",
          "original_form": "해떳다가금방사라진당",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027210.1.15",
          "form": "비내리면더움",
          "original_form": "비내리면더움",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027210.1.16",
          "form": "제습 27도나 26도해놓으면",
          "original_form": "제습 27도나 26도해놓으면",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027210.1.17",
          "form": "뽀송뽀송하고 좋은뎅",
