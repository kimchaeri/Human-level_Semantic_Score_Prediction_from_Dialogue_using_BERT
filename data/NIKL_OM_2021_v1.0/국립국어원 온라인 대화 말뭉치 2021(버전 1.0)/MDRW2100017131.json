      "utterance": [
        {
          "id": "MDRW2100017131.1.1",
          "form": "이번에 무선 청소기 샀어",
          "original_form": "이번에 무선 청소기 샀어",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017131.1.2",
          "form": "얼마나 커?",
          "original_form": "얼마나 커?",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017131.1.3",
          "form": "엄청 크진않을거야",
          "original_form": "엄청 크진않을거야",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017131.1.4",
          "form": "세우면 배 중앙까지 오려나",
          "original_form": "세우면 배 중앙까지 오려나",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017131.1.5",
          "form": "나도 다이슨같은거 사고싶다",
          "original_form": "나도 다이슨같은거 사고싶다",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017131.1.6",
          "form": "근데 있어도 귀찮아서 청소 빈도는 똑같아질거같앜ㅋㅋ",
          "original_form": "근데 있어도 귀찮아서 청소 빈도는 똑같아질거같앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017131.1.7",
          "form": "ㅋㅋ 우리집은 다이슨 못써서 일렉트로룩스인가 거기꺼 샀어",
          "original_form": "ㅋㅋ 우리집은 다이슨 못써서 일렉트로룩스인가 거기꺼 샀어",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017131.1.8",
          "form": "할머니가 못쓰실거같아서",
          "original_form": "할머니가 못쓰실거같아서",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017131.1.9",
          "form": "아하 맞아 다이슨 좀 무겁지",
          "original_form": "아하 맞아 다이슨 좀 무겁지",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017131.1.10",
          "form": "무게중심이 아래인게 좋더라구",
          "original_form": "무게중심이 아래인게 좋더라구",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017131.1.11",
          "form": "바닥에 뭐만 많이 없었어도 로봇청소기를 살텐데",
          "original_form": "바닥에 뭐만 많이 없었어도 로봇청소기를 살텐데",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017131.1.12",
          "form": "그러게 우리집은 개털만 먹다 애가 멈출거같고 거실과 방 사이에 턱도있지",
          "original_form": "그러게 우리집은 개털만 먹다 애가 멈출거같고 거실과 방 사이에 턱도있지",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017131.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017131.1.14",
          "form": "우리집도..",
          "original_form": "우리집도..",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017131.1.15",
          "form": "무선 청소기가 답인가봐",
          "original_form": "무선 청소기가 답인가봐",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017131.1.16",
          "form": "ㅋㄱㅋ",
          "original_form": "ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017131.1.17",
          "form": "아 돈 들어왔는데",
          "original_form": "아 돈 들어왔는데",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017131.1.18",
          "form": "근로장려금",
          "original_form": "근로장려금",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017131.1.19",
          "form": "굿즈 사고싶다",
          "original_form": "굿즈 사고싶다",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017131.1.20",
          "form": "이마트에 쿠키런 굿즈를 판대서 갈까말까 고민하다가",
          "original_form": "이마트에 쿠키런 굿즈를 판대서 갈까말까 고민하다가",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017131.1.21",
          "form": "인터넷으로도 판매한대서 기다리고 있어",
          "original_form": "인터넷으로도 판매한대서 기다리고 있어",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017131.1.22",
          "form": "나도  들어왔어!",
          "original_form": "나도  들어왔어!",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017131.1.23",
          "form": "흐흐흐 모사고싶어",
          "original_form": "흐흐흐 모사고싶어",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017131.1.24",
          "form": "그걸로 워터픽샀어",
          "original_form": "그걸로 워터픽샀어",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017131.1.25",
          "form": "사만원정도",
          "original_form": "사만원정도",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017131.1.26",
          "form": "써봤어?",
          "original_form": "써봤어?",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017131.1.27",
          "form": "김컴횟집 정장 살까 고민했어",
          "original_form": "김컴횟집 정장 살까 고민했어",
