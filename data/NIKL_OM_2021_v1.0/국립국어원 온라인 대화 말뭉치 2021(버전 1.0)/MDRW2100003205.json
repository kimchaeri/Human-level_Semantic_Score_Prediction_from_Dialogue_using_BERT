      "utterance": [
        {
          "id": "MDRW2100003205.1.1",
          "form": "핸드폰게임 하고 있는거 있으세요?",
          "original_form": "핸드폰게임 하고 있는거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003205.1.2",
          "form": "전 소속사에서 만든 게임하고 있어요",
          "original_form": "전 소속사에서 만든 게임하고 있어요",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003205.1.3",
          "form": "약간 예전 리듬스타st 요",
          "original_form": "약간 예전 리듬스타st 요",
          "speaker_id": "2",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003205.1.4",
          "form": "오 저는 리듬스타류 진짜 못해요",
          "original_form": "오 저는 리듬스타류 진짜 못해요",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003205.1.5",
          "form": "슈스엠 이런거 맞죠!",
          "original_form": "슈스엠 이런거 맞죠!",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003205.1.6",
          "form": "헐 네 딱 맞추셨어요 !!!!!",
          "original_form": "헐 네 딱 맞추셨어요 !!!!!",
          "speaker_id": "2",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003205.1.7",
          "form": "저도 잘 못했는데 하다보니 늘더라구요",
          "original_form": "저도 잘 못했는데 하다보니 늘더라구요",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003205.1.8",
          "form": "저 진짜 어렸을때부터 못했어요ㅋㅋㅋ",
          "original_form": "저 진짜 어렸을때부터 못했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003205.1.9",
          "form": "잘하는 사람들 보면 진짜 신기해요",
          "original_form": "잘하는 사람들 보면 진짜 신기해요",
          "speaker_id": "1",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003205.1.10",
          "form": "ㅋ ㅋㅋㅋㅋㅋ 어렵죠 ㅎ",
          "original_form": "ㅋ ㅋㅋㅋㅋㅋ 어렵죠 ㅎ",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003205.1.11",
          "form": "폰으로 게임 뭐하세요?",
          "original_form": "폰으로 게임 뭐하세요?",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003205.1.12",
          "form": "저는 루미큐브요!",
          "original_form": "저는 루미큐브요!",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003205.1.13",
          "form": "재밌어요ㅋㅋㅋ 그거 말고는 이것저것 깔다 지우고 하는 거 같아요",
          "original_form": "재밌어요ㅋㅋㅋ 그거 말고는 이것저것 깔다 지우고 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003205.1.14",
          "form": "오 루미큐브 하는 사람이 많네요",
          "original_form": "오 루미큐브 하는 사람이 많네요",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003205.1.15",
          "form": "그거 어렵지 않나요?",
          "original_form": "그거 어렵지 않나요?",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003205.1.16",
          "form": "룰 알면 재밌어요!",
          "original_form": "룰 알면 재밌어요!",
          "speaker_id": "1",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003205.1.17",
          "form": "오프라인으로 익히고 어플로 게임하는게 좋은거 같아요",
          "original_form": "오프라인으로 익히고 어플로 게임하는게 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003205.1.18",
          "form": "나중에 한번 해보고싶네요",
          "original_form": "나중에 한번 해보고싶네요",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003205.1.19",
          "form": "혹시 스토쿠도 아시나요?",
          "original_form": "혹시 스토쿠도 아시나요?",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003205.1.20",
          "form": "네 알아요!",
          "original_form": "네 알아요!",
          "speaker_id": "1",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003205.1.21",
          "form": "잘하지는 못하지만",
          "original_form": "잘하지는 못하지만",
          "speaker_id": "1",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003205.1.22",
          "form": "전 그것도 즐겨해요",
          "original_form": "전 그것도 즐겨해요",
          "speaker_id": "2",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003205.1.23",
          "form": "그렇게라도 머리를 쓰려고 노력합니다 ㅎ",
          "original_form": "그렇게라도 머리를 쓰려고 노력합니다 ㅎ",
          "speaker_id": "2",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003205.1.24",
          "form": "종이에다 직접 하세요? 아니면 핸드폰 게임으로 하세요?",
          "original_form": "종이에다 직접 하세요? 아니면 핸드폰 게임으로 하세요?",
          "speaker_id": "1",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100003205.1.25",
          "form": "휴대폰으로요 !!!!",
          "original_form": "휴대폰으로요 !!!!",
          "speaker_id": "2",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003205.1.26",
          "form": "종이까진 힘들어요 ㅎ",
          "original_form": "종이까진 힘들어요 ㅎ",
