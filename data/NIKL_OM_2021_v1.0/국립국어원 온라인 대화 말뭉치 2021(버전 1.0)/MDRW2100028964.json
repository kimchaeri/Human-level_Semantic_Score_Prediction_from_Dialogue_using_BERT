      "utterance": [
        {
          "id": "MDRW2100028964.1.1",
          "form": "집안일말야",
          "original_form": "집안일말야",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.2",
          "form": "설거지랑 청소중에서",
          "original_form": "설거지랑 청소중에서",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.3",
          "form": "뭐가 더 할만하다구 생각해?",
          "original_form": "뭐가 더 할만하다구 생각해?",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.4",
          "form": "난 설거지",
          "original_form": "난 설거지",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.5",
          "form": "그럼 내일 설거지 해",
          "original_form": "그럼 내일 설거지 해",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.6",
          "form": "알겟징?",
          "original_form": "알겟징?",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.7",
          "form": "헐..너가 청소할거야?",
          "original_form": "헐..너가 청소할거야?",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.8",
          "form": "언니가 설거지하면",
          "original_form": "언니가 설거지하면",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.9",
          "form": "할데",
          "original_form": "할데",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028964.1.10",
          "form": "집안일은 공평하게 해야지 ㅎㅎ",
          "original_form": "집안일은 공평하게 해야지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.11",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.12",
          "form": "그럿게 하자 ㅎㅎ",
          "original_form": "그럿게 하자 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.13",
          "form": "맨날 할무니만 하잖아",
          "original_form": "맨날 할무니만 하잖아",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.14",
          "form": "난 언니니깐",
          "original_form": "난 언니니깐",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.15",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.16",
          "form": "원래",
          "original_form": "원래",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.17",
          "form": "언니가 더 집안일 많이하는거자나",
          "original_form": "언니가 더 집안일 많이하는거자나",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.18",
          "form": "왜 그럿게 생각하지?",
          "original_form": "왜 그럿게 생각하지?",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.19",
          "form": "언니는 더 나이가 많구",
          "original_form": "언니는 더 나이가 많구",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028964.1.20",
          "form": "더 튼튼하니깐",
          "original_form": "더 튼튼하니깐",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.21",
          "form": "나는 동생이 더 튼튼하다고 봐",
          "original_form": "나는 동생이 더 튼튼하다고 봐",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.22",
          "form": "더 젊잖아",
          "original_form": "더 젊잖아",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.23",
          "form": "난 아니라고 봐",
          "original_form": "난 아니라고 봐",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.24",
          "form": "집안일은",
          "original_form": "집안일은",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.25",
          "form": "언니를 존중해야단다구 봐!",
          "original_form": "언니를 존중해야단다구 봐!",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.26",
          "form": "언니>>>>>동생>할무니=엄마",
          "original_form": "언니>>>>>동생>할무니=엄마",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.27",
          "form": "가 공평하다구 봐",
          "original_form": "가 공평하다구 봐",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.28",
          "form": "아빠가 낀다면",
          "original_form": "아빠가 낀다면",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.29",
          "form": "언니>>>>아빠>>>>동생>엄마=할무니",
          "original_form": "언니>>>>아빠>>>>동생>엄마=할무니",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.30",
          "form": "체력많은순 ㅋ",
          "original_form": "체력많은순 ㅋ",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.31",
          "form": "씽 ㅋㅋ",
          "original_form": "씽 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028964.1.32",
          "form": "언니 체력을 과대평가하네..",
          "original_form": "언니 체력을 과대평가하네..",
          "speaker_id": "2",
          "time": "20210902 23:09"
        }
      ]
    }
