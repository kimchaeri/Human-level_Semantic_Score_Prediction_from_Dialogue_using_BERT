      "utterance": [
        {
          "id": "MDRW2100004933.1.1",
          "form": "일해여?",
          "original_form": "일해여?",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004933.1.2",
          "form": "학생이라서 일은 안 해요!",
          "original_form": "학생이라서 일은 안 해요!",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004933.1.3",
          "form": "알바 구하는 중이에요!",
          "original_form": "알바 구하는 중이에요!",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004933.1.4",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004933.1.5",
          "form": "일하세요?",
          "original_form": "일하세요?",
          "speaker_id": "2",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004933.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004933.1.7",
          "form": "어떤 일하세요?",
          "original_form": "어떤 일하세요?",
          "speaker_id": "2",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004933.1.8",
          "form": "사무직이요",
          "original_form": "사무직이요",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004933.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004933.1.10",
          "form": "좋은 일하시네요!",
          "original_form": "좋은 일하시네요!",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004933.1.11",
          "form": "정신 스트레스 장난 아니죠",
          "original_form": "정신 스트레스 장난 아니죠",
          "speaker_id": "1",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004933.1.12",
          "form": "그건... 그럴거 같아요..",
          "original_form": "그건... 그럴거 같아요..",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004933.1.13",
          "form": "언제 제일 스트레스 받으세요?",
          "original_form": "언제 제일 스트레스 받으세요?",
          "speaker_id": "2",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004933.1.14",
          "form": "상급자 문서 수정이오",
          "original_form": "상급자 문서 수정이오",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004933.1.15",
          "form": "힘드시겠네요ㅠㅜ",
          "original_form": "힘드시겠네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100004933.1.16",
          "form": "네 스타일이 달라서",
          "original_form": "네 스타일이 달라서",
