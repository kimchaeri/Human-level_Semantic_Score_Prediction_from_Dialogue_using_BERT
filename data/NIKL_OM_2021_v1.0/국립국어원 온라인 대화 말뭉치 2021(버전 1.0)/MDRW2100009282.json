      "utterance": [
        {
          "id": "MDRW2100009282.1.1",
          "form": "운동 하시는거 있으세요?",
          "original_form": "운동 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009282.1.2",
          "form": "네 저 헬스하고 있어요ㅎㅎ운동하고 계신거 있어요?",
          "original_form": "네 저 헬스하고 있어요ㅎㅎ운동하고 계신거 있어요?",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009282.1.3",
          "form": "아 저도 헬스해요! 근데 배우고 싶은 운동은 많아요",
          "original_form": "아 저도 헬스해요! 근데 배우고 싶은 운동은 많아요",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009282.1.4",
          "form": "테니스 승마 골프 이런 것ㄷ들 ... 근데 시도가 쉽지 않네요",
          "original_form": "테니스 승마 골프 이런 것ㄷ들 ... 근데 시도가 쉽지 않네요",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009282.1.5",
          "form": "웨이트랑 유산소중애 뭘 조ㅗ아하시나요",
          "original_form": "웨이트랑 유산소중애 뭘 조ㅗ아하시나요",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009282.1.6",
          "form": "맞아요 웨이트 해야하는데 전 런닝이 제일 편하네요ㅠㅠ",
          "original_form": "맞아요 웨이트 해야하는데 전 런닝이 제일 편하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009282.1.7",
          "form": "저는 유산소 너무 싫어해서 웨이트만 해요..",
          "original_form": "저는 유산소 너무 싫어해서 웨이트만 해요..",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009282.1.8",
          "form": "그래서 살은 안빠지더라구요",
          "original_form": "그래서 살은 안빠지더라구요",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009282.1.9",
          "form": "운동은 얼마나 자주하세요?",
          "original_form": "운동은 얼마나 자주하세요?",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009282.1.10",
          "form": "일주일에 3번은 하고 싶은데...웨이트는 너무  힘들어서 자꾸 피하게 되네요ㅠ",
          "original_form": "일주일에 3번은 하고 싶은데...웨이트는 너무  힘들어서 자꾸 피하게 되네요ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009282.1.11",
          "form": "맞죠 저는 그래서 집에서라도 하려고 아령을 샀어요",
          "original_form": "맞죠 저는 그래서 집에서라도 하려고 아령을 샀어요",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009282.1.12",
          "form": "그래도 잇으니까 열심히 하게되더라구요",
          "original_form": "그래도 잇으니까 열심히 하게되더라구요",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009282.1.13",
          "form": "배우고싶은 운동 있으세요?",
          "original_form": "배우고싶은 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009282.1.14",
          "form": "테니스 배우고 싶어요ㅎㅎ 멋있더라구요",
          "original_form": "테니스 배우고 싶어요ㅎㅎ 멋있더라구요",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009282.1.15",
          "form": "그쵸 근데 그 테니스 채가 진짜 무겁더라구요",
          "original_form": "그쵸 근데 그 테니스 채가 진짜 무겁더라구요",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009282.1.16",
          "form": "절대 얻고싶지않은 질병이 있다면 무엇이신가료?",
          "original_form": "절대 얻고싶지않은 질병이 있다면 무엇이신가료?",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009282.1.17",
          "form": "알츠하이머 인거같아요 제가 제자신을 통제할수없는ㅜㅜ",
          "original_form": "알츠하이머 인거같아요 제가 제자신을 통제할수없는ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009282.1.18",
          "form": "맞아요 ㅜㅜ 기억 없어지는 건 너무 슬픈 일인 것 같아요",
          "original_form": "맞아요 ㅜㅜ 기억 없어지는 건 너무 슬픈 일인 것 같아요",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009282.1.19",
          "form": "건강관리를 위해 꼭 하시는게 있으신가요?",
          "original_form": "건강관리를 위해 꼭 하시는게 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009282.1.20",
          "form": "저 영양제 챙겨먹어요ㅎㅎ",
          "original_form": "저 영양제 챙겨먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009282.1.21",
          "form": "오 저도요! 나이드니까 열심히 먹게되는 것 같아요",
          "original_form": "오 저도요! 나이드니까 열심히 먹게되는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 22:51"
        }
      ]
    }
