        {
          "id": "MDRW2100041460.1.1",
          "form": "공연 잘 못 보러 가사죠 ㅠ",
          "original_form": "공연 잘 못 보러 가사죠 ㅠ",
          "speaker_id": "2",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100041460.1.2",
          "form": "너무 아쉬워요ㅠ",
          "original_form": "너무 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041460.1.3",
          "form": "코로나 전 많이 보셨나요?",
          "original_form": "코로나 전 많이 보셨나요?",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100041460.1.4",
          "form": "많이는 못봐도 꾸준히 봤었는데",
          "original_form": "많이는 못봐도 꾸준히 봤었는데",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041460.1.5",
          "form": "이젠 못보네요ㅠ",
          "original_form": "이젠 못보네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041460.1.6",
          "form": "그러게요 ㅠ그나마 아이꺼도 콧구멍을 후비면 이후 정지 ㅜㅜ",
          "original_form": "그러게요 ㅠ그나마 아이꺼도 콧구멍을 후비면 이후 정지 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100041460.1.7",
          "form": "온라인으로만 보네요ㅠ",
          "original_form": "온라인으로만 보네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 01:23"
        },
        {
          "id": "MDRW2100041460.1.8",
          "form": "그러게요 근데 안가니 집중을 잘 못하네요 ㅜ",
          "original_form": "그러게요 근데 안가니 집중을 잘 못하네요 ㅜ",
          "speaker_id": "2",
          "time": "20210906 01:27"
        },
        {
          "id": "MDRW2100041460.1.9",
          "form": "주리를 트는ㅠ",
          "original_form": "주리를 트는ㅠ",
          "speaker_id": "1",
          "time": "20210906 01:30"
        },
        {
          "id": "MDRW2100041460.1.10",
          "form": "ㅋㅋㅋㅋ 공연장을 왜 가는지 알겠더라구요 ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 공연장을 왜 가는지 알겠더라구요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100041460.1.11",
          "form": "그 열기ㅎㅎ",
          "original_form": "그 열기ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 10:58"
        },
        {
          "id": "MDRW2100041460.1.12",
          "form": "그립네요",
          "original_form": "그립네요",
          "speaker_id": "1",
          "time": "20210906 10:58"
        },
        {
          "id": "MDRW2100041460.1.13",
          "form": "그러게요 다시 느낄 수 있겠죠 ㅜ",
          "original_form": "그러게요 다시 느낄 수 있겠죠 ㅜ",
