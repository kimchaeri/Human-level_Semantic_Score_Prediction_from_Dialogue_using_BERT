      "utterance": [
        {
          "id": "MDRW2100003247.1.1",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003247.1.2",
          "form": "일본만 세번 갔었어요ㅎㅎ",
          "original_form": "일본만 세번 갔었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003247.1.3",
          "form": "일본 어디가보셨어요?",
          "original_form": "일본 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003247.1.4",
          "form": "텐리시 아세요? 거기만 세번 갔었어욬ㅋㅋ",
          "original_form": "텐리시 아세요? 거기만 세번 갔었어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003247.1.5",
          "form": "텐리시 처음들어보네요...",
          "original_form": "텐리시 처음들어보네요...",
          "speaker_id": "1",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003247.1.6",
          "form": "거기 유명한건 뭐있어요?",
          "original_form": "거기 유명한건 뭐있어요?",
          "speaker_id": "1",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003247.1.7",
          "form": "텐리교라고 종교도시래요 친구가 거기 유학중이라 놀러 갔었어요",
          "original_form": "텐리교라고 종교도시래요 친구가 거기 유학중이라 놀러 갔었어요",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003247.1.8",
          "form": "종교가 텐리교에요?",
          "original_form": "종교가 텐리교에요?",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003247.1.9",
          "form": "네 그랬어요 우리나라에서는 천리교? 그렇게 부르던데요?ㅎ",
          "original_form": "네 그랬어요 우리나라에서는 천리교? 그렇게 부르던데요?ㅎ",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003247.1.10",
          "form": "거기서는 뭐가 좋았어요?",
          "original_form": "거기서는 뭐가 좋았어요?",
          "speaker_id": "1",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003247.1.11",
          "form": "저녁에 일정시간되니까 다들 교회로 예배드리러 가더라고요",
          "original_form": "저녁에 일정시간되니까 다들 교회로 예배드리러 가더라고요",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003247.1.12",
          "form": "상점도 예배시간되니 문닫는곳이 많더라구요",
          "original_form": "상점도 예배시간되니 문닫는곳이 많더라구요",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003247.1.13",
          "form": "좋았다기보단 신기했어요ㅎㅎ",
          "original_form": "좋았다기보단 신기했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003247.1.14",
          "form": "친구분도 그 종교 믿으세요?",
          "original_form": "친구분도 그 종교 믿으세요?",
          "speaker_id": "1",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003247.1.15",
          "form": "아뇨 친구는 그 지역 대학에 다녀서 거기 살았어요ㅎㅎ",
          "original_form": "아뇨 친구는 그 지역 대학에 다녀서 거기 살았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003247.1.16",
          "form": "아 어느대학교에요?",
          "original_form": "아 어느대학교에요?",
          "speaker_id": "1",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003247.1.17",
          "form": "대학교 이름도 텐리대학이에요ㅋㅋㅋ",
          "original_form": "대학교 이름도 텐리대학이에요ㅋㅋㅋ",
