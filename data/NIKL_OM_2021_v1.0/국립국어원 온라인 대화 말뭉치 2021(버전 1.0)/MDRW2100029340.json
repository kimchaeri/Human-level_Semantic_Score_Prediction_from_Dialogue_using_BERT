      "utterance": [
        {
          "id": "MDRW2100029340.1.1",
          "form": "응 나 완전 멀쩡해",
          "original_form": "응 나 완전 멀쩡해",
          "speaker_id": "2",
          "time": "20210903 01:56"
        },
        {
          "id": "MDRW2100029340.1.2",
          "form": "엄마는 열나고 팔아프고 비염 심해졌는데",
          "original_form": "엄마는 열나고 팔아프고 비염 심해졌는데",
          "speaker_id": "1",
          "time": "20210903 01:56"
        },
        {
          "id": "MDRW2100029340.1.3",
          "form": "나도 어제랑 오늘 오전까지는 팔엄청 아팠거든",
          "original_form": "나도 어제랑 오늘 오전까지는 팔엄청 아팠거든",
          "speaker_id": "2",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029340.1.4",
          "form": "근데 지금은 팔도 멀쩡해",
          "original_form": "근데 지금은 팔도 멀쩡해",
          "speaker_id": "2",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029340.1.5",
          "form": "열은 안나??",
          "original_form": "열은 안나??",
          "speaker_id": "1",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029340.1.6",
          "form": "응 열도 안나고 오히려 체력이 좋아진기분",
          "original_form": "응 열도 안나고 오히려 체력이 좋아진기분",
          "speaker_id": "2",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029340.1.7",
          "form": "영양제 맞은거같아",
          "original_form": "영양제 맞은거같아",
          "speaker_id": "2",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029340.1.8",
          "form": "다행이네 다른 사람들은 열나고 엄청 아팠다는데",
          "original_form": "다행이네 다른 사람들은 열나고 엄청 아팠다는데",
          "speaker_id": "1",
          "time": "20210903 01:58"
        },
        {
          "id": "MDRW2100029340.1.9",
          "form": "나도 그래서 걱정 많이 했는디 너무 멀쩡해서",
          "original_form": "나도 그래서 걱정 많이 했는디 너무 멀쩡해서",
          "speaker_id": "2",
          "time": "20210903 01:58"
        },
        {
          "id": "MDRW2100029340.1.10",
          "form": "오히려 당황스러워",
          "original_form": "오히려 당황스러워",
          "speaker_id": "2",
          "time": "20210903 01:58"
        },
        {
          "id": "MDRW2100029340.1.11",
          "form": "근데 1차보다 2차가 더 아프대",
          "original_form": "근데 1차보다 2차가 더 아프대",
          "speaker_id": "1",
          "time": "20210903 01:59"
        },
        {
          "id": "MDRW2100029340.1.12",
          "form": "지금 겁주는거야?!!",
          "original_form": "지금 겁주는거야?!!",
          "speaker_id": "2",
          "time": "20210903 01:59"
        },
        {
          "id": "MDRW2100029340.1.13",
          "form": "아냐 나도 무서워서그래....",
          "original_form": "아냐 나도 무서워서그래....",
          "speaker_id": "1",
          "time": "20210903 01:59"
        },
        {
          "id": "MDRW2100029340.1.14",
          "form": "아빠는 멀쩡했자나 괜찮을거야",
          "original_form": "아빠는 멀쩡했자나 괜찮을거야",
          "speaker_id": "2",
          "time": "20210903 02:00"
        },
        {
          "id": "MDRW2100029340.1.15",
          "form": "덜 아팠으면 좋겠구만",
          "original_form": "덜 아팠으면 좋겠구만",
          "speaker_id": "1",
          "time": "20210903 02:00"
        },
        {
          "id": "MDRW2100029340.1.16",
          "form": "내가 약을 미리 사다둘게",
          "original_form": "내가 약을 미리 사다둘게",
          "speaker_id": "2",
          "time": "20210903 02:00"
        },
        {
          "id": "MDRW2100029340.1.17",
