      "utterance": [
        {
          "id": "MDRW2100012309.1.1",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㄱㅋ",
          "speaker_id": "2",
