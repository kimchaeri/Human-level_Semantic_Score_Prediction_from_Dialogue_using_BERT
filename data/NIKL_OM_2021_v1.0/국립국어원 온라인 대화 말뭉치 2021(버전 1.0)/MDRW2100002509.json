      "utterance": [
        {
          "id": "MDRW2100002509.1.1",
          "form": "요즘 더위가 가셔서 좋아요",
          "original_form": "요즘 더위가 가셔서 좋아요",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002509.1.2",
          "form": "요즘 날씨 너무좋죠",
          "original_form": "요즘 날씨 너무좋죠",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002509.1.3",
          "form": "비는 좀 와도",
          "original_form": "비는 좀 와도",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.4",
          "form": "안더운게 진짜 살거같아요",
          "original_form": "안더운게 진짜 살거같아요",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.5",
          "form": "확실히 훨 시원해졌어요",
          "original_form": "확실히 훨 시원해졌어요",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.6",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.7",
          "form": "맞아여 밤에 에어컨안튼게",
          "original_form": "맞아여 밤에 에어컨안튼게",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.8",
          "form": "얼마만인지 모르겠어요",
          "original_form": "얼마만인지 모르겠어요",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.9",
          "form": "창문만열어도 시원하고",
          "original_form": "창문만열어도 시원하고",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.10",
          "form": "선풍기만돌려도 살거같고",
          "original_form": "선풍기만돌려도 살거같고",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.11",
          "form": "어제 저 사는 지역은 진짜시원해서",
          "original_form": "어제 저 사는 지역은 진짜시원해서",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.12",
          "form": "선풍기도 안틀어도 시원하더라구요",
          "original_form": "선풍기도 안틀어도 시원하더라구요",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.13",
          "form": "어디에요?",
          "original_form": "어디에요?",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002509.1.14",
          "form": "저는 여수인데 낮엔 약간더워서 틀어놔요",
          "original_form": "저는 여수인데 낮엔 약간더워서 틀어놔요",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.15",
          "form": "인천살아요ㅎㅎ",
          "original_form": "인천살아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.16",
          "form": "인천은 시원하군요!",
          "original_form": "인천은 시원하군요!",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.17",
          "form": "다음주에 일주일내내 비소식있던데",
          "original_form": "다음주에 일주일내내 비소식있던데",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.18",
          "form": "여주는 어때용??",
          "original_form": "여주는 어때용??",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.19",
          "form": "앗 여수",
          "original_form": "앗 여수",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.20",
          "form": "여기는 안올거같아요",
          "original_form": "여기는 안올거같아요",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.21",
          "form": "흐리기만하더라구요",
          "original_form": "흐리기만하더라구요",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.22",
          "form": "인천은 비왔어요?",
          "original_form": "인천은 비왔어요?",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.23",
          "form": "넹 어제도 좀 오고",
          "original_form": "넹 어제도 좀 오고",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002509.1.24",
          "form": "다음주 내내 비가 떠있네요",
          "original_form": "다음주 내내 비가 떠있네요",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.25",
          "form": "천둥번개도쳤어요?",
          "original_form": "천둥번개도쳤어요?",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.26",
          "form": "으으음 그건 잘 기억이...",
          "original_form": "으으음 그건 잘 기억이...",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.27",
          "form": "보니까 윗지방은 엄청쳤다던데",
          "original_form": "보니까 윗지방은 엄청쳤다던데",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.28",
          "form": "앗 그러시군요.;!",
          "original_form": "앗 그러시군요.;!",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.29",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.30",
          "form": "기억아난느거보면",
          "original_form": "기억아난느거보면",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.31",
          "form": "여긴 안쳤나봐요ㅋㅋㅋ",
          "original_form": "여긴 안쳤나봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002509.1.32",
          "form": "여름에 그나마 미세먼지는 없어서 좋았는데",
          "original_form": "여름에 그나마 미세먼지는 없어서 좋았는데",
          "speaker_id": "1",
          "time": "20210820 13:34"
        }
      ]
    }
  ]
