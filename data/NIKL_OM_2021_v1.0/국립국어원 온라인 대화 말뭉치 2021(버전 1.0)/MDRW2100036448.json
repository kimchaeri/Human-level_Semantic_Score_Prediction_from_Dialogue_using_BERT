      "utterance": [
        {
          "id": "MDRW2100036448.1.1",
          "form": "너 요즘 배 괜챦아?",
          "original_form": "너 요즘 배 괜챦아?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.2",
          "form": "하 여름이라 배가 자주 아파",
          "original_form": "하 여름이라 배가 자주 아파",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.3",
          "form": "너 배 자주 아프지 않아?",
          "original_form": "너 배 자주 아프지 않아?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.4",
          "form": "자니?",
          "original_form": "자니?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.5",
          "form": "음식들도 조심해야하고",
          "original_form": "음식들도 조심해야하고",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.7",
          "form": "아이스크림을 많니 먹은거야?",
          "original_form": "아이스크림을 많니 먹은거야?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.8",
          "form": "그것도 그렇고",
          "original_form": "그것도 그렇고",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.9",
          "form": "아니면 좀 상한 음식을 먹은거야?",
          "original_form": "아니면 좀 상한 음식을 먹은거야?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.10",
          "form": "나도 요즘 계속 배가 안좋아",
          "original_form": "나도 요즘 계속 배가 안좋아",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.11",
          "form": "응응 그것도 그렇고",
          "original_form": "응응 그것도 그렇고",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036448.1.12",
          "form": "소화도 잘 안돠는데",
          "original_form": "소화도 잘 안돠는데",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.13",
          "form": "사람은 너무 약한 것 같아",
          "original_form": "사람은 너무 약한 것 같아",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.14",
          "form": "건강에 이상이 생긴걸까",
          "original_form": "건강에 이상이 생긴걸까",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.15",
          "form": "맞아 운동해야해",
          "original_form": "맞아 운동해야해",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.16",
          "form": "운동하자",
          "original_form": "운동하자",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.17",
          "form": "ㅠㅠ조심하자 건강이 최고여",
          "original_form": "ㅠㅠ조심하자 건강이 최고여",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.18",
          "form": "이제 날 선선해지니까 운동 할 수 있겠어",
          "original_form": "이제 날 선선해지니까 운동 할 수 있겠어",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.19",
          "form": "나는 요즘 자꾸 밥먹고나면",
          "original_form": "나는 요즘 자꾸 밥먹고나면",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.20",
          "form": "나는 오늘 링피트했어",
          "original_form": "나는 오늘 링피트했어",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.21",
          "form": "조깅했어",
          "original_form": "조깅했어",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.22",
          "form": "너무 잠이와서",
          "original_form": "너무 잠이와서",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.23",
          "form": "그건정상이야",
          "original_form": "그건정상이야",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.24",
          "form": "역류성 식도염 걸릴 것 같아",
          "original_form": "역류성 식도염 걸릴 것 같아",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.25",
          "form": "이겨내야지.",
          "original_form": "이겨내야지.",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036448.1.26",
          "form": "링피트를 하다니 진짜 부지런하다",
          "original_form": "링피트를 하다니 진짜 부지런하다",
          "speaker_id": "1",
          "time": "20210830 23:15"
        }
      ]
    }
