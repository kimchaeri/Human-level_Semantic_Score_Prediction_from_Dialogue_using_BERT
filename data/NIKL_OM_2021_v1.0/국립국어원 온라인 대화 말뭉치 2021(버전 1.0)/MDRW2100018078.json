      "utterance": [
        {
          "id": "MDRW2100018078.1.1",
          "form": "성형에 대해 어떻게 생각하세요?",
          "original_form": "성형에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100018078.1.2",
          "form": "저도 한번 해보고싶어요",
          "original_form": "저도 한번 해보고싶어요",
          "speaker_id": "2",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100018078.1.3",
          "form": "저도 해보고 싶은데 어디에 관심 있어요?",
          "original_form": "저도 해보고 싶은데 어디에 관심 있어요?",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100018078.1.4",
          "form": "코...전신 ? ㅋㅋ",
          "original_form": "코...전신 ? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100018078.1.5",
          "form": "오~ 전신 성형은 어떻게 하는거에요?",
          "original_form": "오~ 전신 성형은 어떻게 하는거에요?",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100018078.1.6",
          "form": "지방흡입 같은건가요?",
          "original_form": "지방흡입 같은건가요?",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100018078.1.7",
          "form": "네네네 ㅋㅋㅋㅋㅋ",
          "original_form": "네네네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100018078.1.8",
          "form": "보톡스도 맞고...",
          "original_form": "보톡스도 맞고...",
          "speaker_id": "2",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100018078.1.9",
          "form": "입술필러? ㅋㅋㅋ",
          "original_form": "입술필러? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100018078.1.10",
          "form": "오 되게 성형에 관심 많으시네요!",
          "original_form": "오 되게 성형에 관심 많으시네요!",
          "speaker_id": "1",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100018078.1.11",
          "form": "저도 한때 복근성형 같은거 하고싶었어요 ㅋㅋ",
          "original_form": "저도 한때 복근성형 같은거 하고싶었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100018078.1.12",
          "form": "그런거도 가능해요? ㅋㅋㅋㅋ",
          "original_form": "그런거도 가능해요? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100018078.1.13",
          "form": "네 가능하다고 하더라구요 게다가 자국도 안남는데요..",
          "original_form": "네 가능하다고 하더라구요 게다가 자국도 안남는데요..",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100018078.1.14",
          "form": "운동 안하고 복근 만드는 시대가 왔어요 대박 ㅋㅋ",
          "original_form": "운동 안하고 복근 만드는 시대가 왔어요 대박 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100018078.1.15",
          "form": "ㄷㄷㄷㄷㄷㄷㄷ진짜신기하네요",
          "original_form": "ㄷㄷㄷㄷㄷㄷㄷ진짜신기하네요",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100018078.1.16",
          "form": "저도 받고싶어욬ㅋㅋ 일자복근 또ㅏㅎ!",
          "original_form": "저도 받고싶어욬ㅋㅋ 일자복근 또ㅏㅎ!",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100018078.1.17",
          "form": "그쵸? ㅋㅋ 근데 그렇게 수술해도 운동 안하면 결국 사라진다고 들었어요...",
          "original_form": "그쵸? ㅋㅋ 근데 그렇게 수술해도 운동 안하면 결국 사라진다고 들었어요...",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100018078.1.18",
          "form": "결국엔 기승전운동인가봐요 ㅜㅜ",
          "original_form": "결국엔 기승전운동인가봐요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100018078.1.19",
          "form": "오잉...ㅋㅋㅋㅋ 이런",
          "original_form": "오잉...ㅋㅋㅋㅋ 이런",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100018078.1.20",
          "form": "ㅜㅜ 그러게요ㅠㅠㅠ후",
          "original_form": "ㅜㅜ 그러게요ㅠㅠㅠ후",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100018078.1.21",
          "form": "평소에 운동 하시는 편이에요?",
          "original_form": "평소에 운동 하시는 편이에요?",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100018078.1.22",
          "form": "넹 운동 그냥 이거저것 가끔 해용",
          "original_form": "넹 운동 그냥 이거저것 가끔 해용",
