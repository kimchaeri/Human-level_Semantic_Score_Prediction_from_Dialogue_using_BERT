      "utterance": [
        {
          "id": "MDRW2100005566.1.1",
          "form": "안녕하세요 반갑습니다!",
          "original_form": "안녕하세요 반갑습니다!",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005566.1.2",
          "form": "반갑습니다ㅋㅋ 요즘쇼핑하셨던 물건 있나요?",
          "original_form": "반갑습니다ㅋㅋ 요즘쇼핑하셨던 물건 있나요?",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005566.1.3",
          "form": "저 usb 믹서기 샀어요!!",
          "original_form": "저 usb 믹서기 샀어요!!",
          "speaker_id": "1",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005566.1.4",
          "form": "좋드라구요",
          "original_form": "좋드라구요",
          "speaker_id": "1",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005566.1.5",
          "form": "와 유에스비꽂으면 믹서기돌아가요? 대박",
          "original_form": "와 유에스비꽂으면 믹서기돌아가요? 대박",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005566.1.6",
          "form": "좋은데요..?저도사고싶어요",
          "original_form": "좋은데요..?저도사고싶어요",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005566.1.7",
          "form": "ㅋㅋㅋusb로 충전 시켜서 돌리는 건데",
          "original_form": "ㅋㅋㅋusb로 충전 시켜서 돌리는 건데",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005566.1.8",
          "form": "텐바이텐에서 삼만원보다 적게 주고 샀어요!!",
          "original_form": "텐바이텐에서 삼만원보다 적게 주고 샀어요!!",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005566.1.9",
          "form": "짱 좋아요!!",
          "original_form": "짱 좋아요!!",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005566.1.10",
          "form": "역시 텐바이텐..온갖걸 다파네요",
          "original_form": "역시 텐바이텐..온갖걸 다파네요",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005566.1.11",
          "form": "잘 갈리나오ㅓ?",
          "original_form": "잘 갈리나오ㅓ?",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005566.1.12",
          "form": "네네 근데",
          "original_form": "네네 근데",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005566.1.13",
          "form": "원래 아몬드 우유 해먹으려 샀는데",
          "original_form": "원래 아몬드 우유 해먹으려 샀는데",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005566.1.14",
          "form": "먹어보니 맛 없어서 그냥 집에 장식해둬요....",
          "original_form": "먹어보니 맛 없어서 그냥 집에 장식해둬요....",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005566.1.15",
          "form": "예뻐서 감성 용품이라 생각하고 말라구요ㅎㅎ....",
          "original_form": "예뻐서 감성 용품이라 생각하고 말라구요ㅎㅎ....",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005566.1.16",
          "form": "아ㅋㅋㅋㅋ 저도그렇게 장식해둔거 많아요..ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ 저도그렇게 장식해둔거 많아요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005566.1.17",
          "form": "쓰려고산건데 분명..안쓰게되는..",
          "original_form": "쓰려고산건데 분명..안쓰게되는..",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005566.1.18",
          "form": "그죠ㅋㅋㄱㄱ ㅋ",
          "original_form": "그죠ㅋㅋㄱㄱ ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005566.1.19",
          "form": "name2님은 최근에 뭐 사셨나요??",
          "original_form": "&name2&님은 최근에 뭐 사셨나요??",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005566.1.20",
          "form": "저는 혈압계샀어용..",
          "original_form": "저는 혈압계샀어용..",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005566.1.21",
          "form": "부모님이랑 같이쓰려고ㅋㅋ건강을위해",
