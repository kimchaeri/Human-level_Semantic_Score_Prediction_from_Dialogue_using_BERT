      "utterance": [
        {
          "id": "MDRW2100021630.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021630.1.2",
          "form": "당근자주했었는데 요샌안하네요",
          "original_form": "당근자주했었는데 요샌안하네요",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021630.1.3",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021630.1.4",
          "form": "머 사세요?",
          "original_form": "머 사세요?",
          "speaker_id": "1",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100021630.1.5",
          "form": "사는것보단 안쓰는거 많이 팔았어여",
          "original_form": "사는것보단 안쓰는거 많이 팔았어여",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100021630.1.6",
          "form": "저도 팔기만뇨",
          "original_form": "저도 팔기만뇨",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100021630.1.7",
          "form": "집에 안쓰는거 팔고 화분도 정리하고 많이팔았네여ㅠㅠ",
          "original_form": "집에 안쓰는거 팔고 화분도 정리하고 많이팔았네여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100021630.1.8",
          "form": "화분까지요?",
          "original_form": "화분까지요?",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100021630.1.9",
          "form": "네 화분이 좀 많았는데ㅠ 제가 겨울이되면서 감당이안되서요ㅠㅠ 좋은주인분께 드렸네여 잘키워줄실",
          "original_form": "네 화분이 좀 많았는데ㅠ 제가 겨울이되면서 감당이안되서요ㅠㅠ 좋은주인분께 드렸네여 잘키워줄실",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100021630.1.10",
          "form": "화분 벌레 없어요?",
          "original_form": "화분 벌레 없어요?",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100021630.1.11",
          "form": "작은벌레들이 생긴적도잇어요ㅠㅠ",
          "original_form": "작은벌레들이 생긴적도잇어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100021630.1.12",
          "form": "우째요?",
          "original_form": "우째요?",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100021630.1.13",
          "form": "바이오킬? 인가 사서 뿌렸어요!",
          "original_form": "바이오킬? 인가 사서 뿌렸어요!",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100021630.1.14",
          "form": "아 들었어요",
          "original_form": "아 들었어요",
          "speaker_id": "1",
          "time": "20210829 18:11"
        }
      ]
