      "utterance": [
        {
          "id": "MDRW2100020582.1.1",
          "form": "스페인어 쓰는 국가로 여행 준비하시나요",
          "original_form": "스페인어 쓰는 국가로 여행 준비하시나요",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020582.1.2",
          "form": "네 ㅎㅎ 나중에 남미 가고 싶어요",
          "original_form": "네 ㅎㅎ 나중에 남미 가고 싶어요",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020582.1.3",
          "form": "코로나가 언제 끝날 지는 모르겠지만",
          "original_form": "코로나가 언제 끝날 지는 모르겠지만",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020582.1.4",
          "form": "name2님은 마지막 여행지가 어디셨어요?",
          "original_form": "&name2&님은 마지막 여행지가 어디셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020582.1.5",
          "form": "저는 마지막은 타이페이였어요",
          "original_form": "저는 마지막은 타이페이였어요",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020582.1.6",
          "form": "국내여행도 잘 못다녀가지고 ㅋㅋ",
          "original_form": "국내여행도 잘 못다녀가지고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020582.1.7",
          "form": "오 타이페이면 대만인가요?",
          "original_form": "오 타이페이면 대만인가요?",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020582.1.8",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020582.1.9",
          "form": "오 어떠셨어요?",
          "original_form": "오 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020582.1.10",
          "form": "뭔가 중국이랑 일본이 섞인 느낌이 나서 독특했어요",
          "original_form": "뭔가 중국이랑 일본이 섞인 느낌이 나서 독특했어요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020582.1.11",
          "form": "교통 편리해서 다니기에도 좋고 ㅋㅋ",
          "original_form": "교통 편리해서 다니기에도 좋고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020582.1.12",
          "form": "아아 중국보다 깔끔하고",
          "original_form": "아아 중국보다 깔끔하고",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020582.1.13",
          "form": "일본처럼 아기자기한 느낌이 있다하더라구요",
          "original_form": "일본처럼 아기자기한 느낌이 있다하더라구요",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020582.1.14",
          "form": "밀크티 드셔보셨어요?",
          "original_form": "밀크티 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020582.1.15",
          "form": "네네 부모님이랑 간거라서 막 많이 마시진 않았는데",
          "original_form": "네네 부모님이랑 간거라서 막 많이 마시진 않았는데",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020582.1.16",
          "form": "근데 밀크티는 우리나라에서 먹는거랑 똑같아서 ㅋㅋㅋㅋ크게 감흥이 있진 않았어요",
          "original_form": "근데 밀크티는 우리나라에서 먹는거랑 똑같아서 ㅋㅋㅋㅋ크게 감흥이 있진 않았어요",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020582.1.17",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020582.1.18",
          "form": "본토의 맛은 다를 줄...",
          "original_form": "본토의 맛은 다를 줄...",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020582.1.19",
          "form": "어느게 제일 좋으셨어요?",
          "original_form": "어느게 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020582.1.20",
          "form": "음 타이페이에서 송상문화공원이라는 곳이 있는데",
          "original_form": "음 타이페이에서 송상문화공원이라는 곳이 있는데",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020582.1.21",
          "form": "담배공장을 문화예술공간으로 바꾼 곳이에요",
          "original_form": "담배공장을 문화예술공간으로 바꾼 곳이에요",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020582.1.22",
          "form": "거기가 분위기가 참 좋아요",
          "original_form": "거기가 분위기가 참 좋아요",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020582.1.23",
          "form": "아아 분위기가 좋을 법 하네요",
          "original_form": "아아 분위기가 좋을 법 하네요",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020582.1.24",
          "form": "또 어디가 좋으셨어요?",
          "original_form": "또 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020582.1.25",
          "form": "지우펀? 타이페이 외곽지역에 있는데 그 센과치히로 동네처럼 생겼다고 유명한 곳이에요",
          "original_form": "지우펀? 타이페이 외곽지역에 있는데 그 센과치히로 동네처럼 생겼다고 유명한 곳이에요",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020582.1.26",
          "form": "오..센과 치히로!",
          "original_form": "오..센과 치히로!",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020582.1.27",
          "form": "센과 치히로 좋아해요",
          "original_form": "센과 치히로 좋아해요",
          "speaker_id": "1",
          "time": "20210828 10:24"
        }
      ]
    }
