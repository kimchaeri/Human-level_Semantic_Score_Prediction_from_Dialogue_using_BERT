      "utterance": [
        {
          "id": "MDRW2100002720.1.1",
          "form": "학교가니?",
          "original_form": "학교가니?",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.2",
          "form": "응 지금도 학교야",
          "original_form": "응 지금도 학교야",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.3",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.4",
          "form": "정말 유능한 학생이구나.",
          "original_form": "정말 유능한 학생이구나.",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.5",
          "form": "요즘 정말 바빠",
          "original_form": "요즘 정말 바빠",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.6",
          "form": "정말?",
          "original_form": "정말?",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.7",
          "form": "실험실에서 할일이 많거든",
          "original_form": "실험실에서 할일이 많거든",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.8",
          "form": "수업이 있니?",
          "original_form": "수업이 있니?",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002720.1.9",
          "form": "수업은 아직 없어",
          "original_form": "수업은 아직 없어",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.10",
          "form": "아 실험을 하는구나",
          "original_form": "아 실험을 하는구나",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.11",
          "form": "수강신청을 얼마전에 했는데",
          "original_form": "수강신청을 얼마전에 했는데",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.12",
          "form": "실패해서 기분이 안좋더라",
          "original_form": "실패해서 기분이 안좋더라",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.13",
          "form": "실험실에서 어떤 실험을 하는데?",
          "original_form": "실험실에서 어떤 실험을 하는데?",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.14",
          "form": "너의 전공이 어떤 것이었더라?",
          "original_form": "너의 전공이 어떤 것이었더라?",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.15",
          "form": "나는 화학실험을해",
          "original_form": "나는 화학실험을해",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.16",
          "form": "수강신청에 실패했다니 그 이야기 나중에 들려주렴.",
          "original_form": "수강신청에 실패했다니 그 이야기 나중에 들려주렴.",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.17",
          "form": "내 전공은 응용화학과야",
          "original_form": "내 전공은 응용화학과야",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.18",
          "form": "오 화학.",
          "original_form": "오 화학.",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.19",
          "form": "내 전공은 심리학과 법학이야.",
          "original_form": "내 전공은 심리학과 법학이야.",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.20",
          "form": "너는 둘중에 뭐를 더 좋아하니",
          "original_form": "너는 둘중에 뭐를 더 좋아하니",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.21",
          "form": "응용화학과는 무엇을 응용하니?",
          "original_form": "응용화학과는 무엇을 응용하니?",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.22",
          "form": "둘 다 좋아해. 편애는 안된단다.",
          "original_form": "둘 다 좋아해. 편애는 안된단다.",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002720.1.23",
          "form": "ㅋㅋㅋㅋㅋ바람직한 학생이네",
          "original_form": "ㅋㅋㅋㅋㅋ바람직한 학생이네",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002720.1.24",
          "form": "수강신청이 있었다며?",
          "original_form": "수강신청이 있었다며?",
          "speaker_id": "2",
          "time": "20210820 15:03"
        }
      ]
    }
