      "utterance": [
        {
          "id": "MDRW2100027215.1.1",
          "form": "덥죠?",
          "original_form": "덥죠?",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027215.1.2",
          "form": "맞아요 요즘 덥더라고요",
          "original_form": "맞아요 요즘 덥더라고요",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027215.1.3",
          "form": "요 며칠 계속 비오고 좀 쌀쌀해지길래 가을 오려나 했는데 아직은 아닌가봐요",
          "original_form": "요 며칠 계속 비오고 좀 쌀쌀해지길래 가을 오려나 했는데 아직은 아닌가봐요",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027215.1.4",
          "form": "습해여",
          "original_form": "습해여",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027215.1.5",
          "form": "습해서 더 더운 거도 있죠",
          "original_form": "습해서 더 더운 거도 있죠",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027215.1.6",
          "form": "빨리 가을이 왔으면 좋겠어요",
          "original_form": "빨리 가을이 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027215.1.7",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027215.1.8",
          "form": "거기는 날씨 어때요?",
          "original_form": "거기는 날씨 어때요?",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027215.1.9",
          "form": "여긴 비 조금 오다가 다시 그친 거 같아요",
          "original_form": "여긴 비 조금 오다가 다시 그친 거 같아요",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027215.1.10",
          "form": "흐려뇨",
          "original_form": "흐려뇨",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027215.1.11",
          "form": "비 올지도 모르겠네요",
          "original_form": "비 올지도 모르겠네요",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027215.1.12",
          "form": "혹시 모르니 나갈 때 우산 잘 챙기고 다니세요",
          "original_form": "혹시 모르니 나갈 때 우산 잘 챙기고 다니세요",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027215.1.13",
          "form": "네 그래야겠어요",
          "original_form": "네 그래야겠어요",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027215.1.14",
          "form": "요즘 소나기도 자주 오더라고요",
          "original_form": "요즘 소나기도 자주 오더라고요",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027215.1.15",
          "form": "비오는 날 좋아하세요?",
          "original_form": "비오는 날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027215.1.16",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027215.1.17",
          "form": "그렇구나 저도요",
          "original_form": "그렇구나 저도요",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100027215.1.18",
          "form": "밖에 나가면 비 맞을까봐 나가지도 못하겠고요",
          "original_form": "밖에 나가면 비 맞을까봐 나가지도 못하겠고요",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100027215.1.19",
          "form": "우산 챙겨여",
          "original_form": "우산 챙겨여",
          "speaker_id": "1",
