      "utterance": [
        {
          "id": "MDRW2100011284.1.1",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100011284.1.2",
          "form": "네 저는 구피 키워요.",
          "original_form": "네 저는 구피 키워요.",
          "speaker_id": "2",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100011284.1.3",
          "form": "구피라면 물고기인가요?",
          "original_form": "구피라면 물고기인가요?",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100011284.1.4",
          "form": "네 맞아요.",
          "original_form": "네 맞아요.",
          "speaker_id": "2",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100011284.1.5",
          "form": "몇마리 키우시나요",
          "original_form": "몇마리 키우시나요",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100011284.1.6",
          "form": "15마리요!그런데 최근들어 자꾸 죽네요ㅠㅠ",
          "original_form": "15마리요!그런데 최근들어 자꾸 죽네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011284.1.7",
          "form": "마음이 안좋으시겠네요..",
          "original_form": "마음이 안좋으시겠네요..",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011284.1.8",
          "form": "키우신지 오래되셨어요?",
          "original_form": "키우신지 오래되셨어요?",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011284.1.9",
          "form": "1년 조금 넘었어요ㅎㅎ",
          "original_form": "1년 조금 넘었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011284.1.10",
          "form": "구피 수명이 긴편인가요?",
          "original_form": "구피 수명이 긴편인가요?",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011284.1.11",
          "form": "아뇨ㅠ1년정도에요.",
          "original_form": "아뇨ㅠ1년정도에요.",
          "speaker_id": "2",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100011284.1.12",
          "form": "아 그러시구나 그래도 건강히 잘 키우셨나보네요ㅎㅎ",
          "original_form": "아 그러시구나 그래도 건강히 잘 키우셨나보네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100011284.1.13",
          "form": "네 뭐 열심히 돌봐주고는있는데 자기들끼리 싸워서ㅠㅠ",
          "original_form": "네 뭐 열심히 돌봐주고는있는데 자기들끼리 싸워서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100011284.1.14",
          "form": "저도 예전에 물고기 키워봤어요 구피인지는 모르겠네요.",
          "original_form": "저도 예전에 물고기 키워봤어요 구피인지는 모르겠네요.",
          "speaker_id": "1",
          "time": "20210824 10:46"
        }
