      "utterance": [
        {
          "id": "MDRW2100005507.1.1",
          "form": "무슨 게임 좋아해요?",
          "original_form": "무슨 게임 좋아해요?",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005507.1.2",
          "form": "저는 요즘 쿠키런킹덤 하고있어요!",
          "original_form": "저는 요즘 쿠키런킹덤 하고있어요!",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005507.1.3",
          "form": "어떤 게임하세요?",
          "original_form": "어떤 게임하세요?",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005507.1.4",
          "form": "저는 pc 축구만 하는거같아요 모바일 게임은 잘 안하거든요",
          "original_form": "저는 pc 축구만 하는거같아요 모바일 게임은 잘 안하거든요",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005507.1.5",
          "form": "게임 자주 하시나요?",
          "original_form": "게임 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005507.1.6",
          "form": "자주는 안하는 편이에요..",
          "original_form": "자주는 안하는 편이에요..",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005507.1.7",
          "form": "가끔씩 시간날때만 해요 게임을 잘하지 못해서 ㅠㅠ",
          "original_form": "가끔씩 시간날때만 해요 게임을 잘하지 못해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005507.1.8",
          "form": "아 그러시구나 pc게임은 해본적 없으세요?",
          "original_form": "아 그러시구나 pc게임은 해본적 없으세요?",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005507.1.9",
          "form": "어렸을때 크아 외엔 pc게임은 안해봤네요",
          "original_form": "어렸을때 크아 외엔 pc게임은 안해봤네요",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005507.1.10",
          "form": "닌텐도는 자주 했었는데.. 닌텐도 해보셨나요?",
          "original_form": "닌텐도는 자주 했었는데.. 닌텐도 해보셨나요?",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005507.1.11",
          "form": "엄청 어릴때 닌텐도로 슈퍼마리오 해본거같는데 그때 추억이 아직도 기억나네요",
          "original_form": "엄청 어릴때 닌텐도로 슈퍼마리오 해본거같는데 그때 추억이 아직도 기억나네요",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005507.1.12",
          "form": "마리오 재밌었죠.. 마리오카트도 했었는데 잘은 못했어요 ..ㅎㅎ",
          "original_form": "마리오 재밌었죠.. 마리오카트도 했었는데 잘은 못했어요 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005507.1.13",
          "form": "축구게임 보니까 실제 선수들이 캐릭터로 나오더라구요 신기해요",
          "original_form": "축구게임 보니까 실제 선수들이 캐릭터로 나오더라구요 신기해요",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005507.1.14",
          "form": "그리고 폰이 출시된 후부터  닌텐도는 저절로 안하게 된거같아요",
          "original_form": "그리고 폰이 출시된 후부터  닌텐도는 저절로 안하게 된거같아요",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005507.1.15",
          "form": "축구게임은 해본적 있나요?",
          "original_form": "축구게임은 해본적 있나요?",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005507.1.16",
          "form": "직접 해보진 않고 본적은 있어요!",
          "original_form": "직접 해보진 않고 본적은 있어요!",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005507.1.17",
          "form": "어려워보여서 뭔가 시도를 안해봤네요ㅎㅎ..",
          "original_form": "어려워보여서 뭔가 시도를 안해봤네요ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005507.1.18",
          "form": "보는것도 재미를 느끼죠 그렇게하다보면 하게되더라고요",
          "original_form": "보는것도 재미를 느끼죠 그렇게하다보면 하게되더라고요",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005507.1.19",
          "form": "그렇긴하죠ㅋㅋ 재밌어보이면 계속보다가 결국에는 하게 되더라구요",
          "original_form": "그렇긴하죠ㅋㅋ 재밌어보이면 계속보다가 결국에는 하게 되더라구요",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005507.1.20",
          "form": "그렇다면 어떻게 게임방손 보시는거 있나요?",
          "original_form": "그렇다면 어떻게 게임방손 보시는거 있나요?",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005507.1.21",
          "form": "최근에는 게임방송 어떻게 보셨어여?",
          "original_form": "최근에는 게임방송 어떻게 보셨어여?",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005507.1.22",
          "form": "딱히 정해서보진 않고 가끔 유튜브에 뜨는거봐요!",
          "original_form": "딱히 정해서보진 않고 가끔 유튜브에 뜨는거봐요!",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005507.1.23",
          "form": "하나보면은 알고리즘으로 유튜브에 계속 뜨더라구요ㅎ",
          "original_form": "하나보면은 알고리즘으로 유튜브에 계속 뜨더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:47"
        }
      ]
    }
