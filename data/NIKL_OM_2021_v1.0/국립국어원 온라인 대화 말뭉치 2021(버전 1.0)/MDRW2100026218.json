      "utterance": [
        {
          "id": "MDRW2100026218.1.1",
          "form": "당근 하세요?",
          "original_form": "당근 하세요?",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026218.1.2",
          "form": "중고거래",
          "original_form": "중고거래",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026218.1.3",
          "form": "저 오늘도 했죠",
          "original_form": "저 오늘도 했죠",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026218.1.4",
          "form": "열심히 합니다 ㅋㅋㅋ",
          "original_form": "열심히 합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026218.1.5",
          "form": "ㅋㅋ 뭐 했어요?",
          "original_form": "ㅋㅋ 뭐 했어요?",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026218.1.6",
          "form": "안경 거래했어요 제가 파는 걸로 ㅎㅎ",
          "original_form": "안경 거래했어요 제가 파는 걸로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026218.1.7",
          "form": "오~~ 안경을 당근으로",
          "original_form": "오~~ 안경을 당근으로",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026218.1.8",
          "form": "근데 써봐야 어울리는지 알수있지않아여??",
          "original_form": "근데 써봐야 어울리는지 알수있지않아여??",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026218.1.9",
          "form": "도수 없는 안경이었거든요~ 그거 테만 가져가신다고 하시더라구요",
          "original_form": "도수 없는 안경이었거든요~ 그거 테만 가져가신다고 하시더라구요",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026218.1.10",
          "form": "그럴거 같은데 쿨거래 하시더라구요 ㅋㅋㅋㅋ",
          "original_form": "그럴거 같은데 쿨거래 하시더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026218.1.11",
          "form": "ㅎㅎ 진짜쿨거래네요",
          "original_form": "ㅎㅎ 진짜쿨거래네요",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026218.1.12",
          "form": "쿨거래가 좋은 것 같아요 ㅋㅋㅋ 너무 깐깐하면 그냥 새상품 사는게 서로를 위한 길..ㅋㅋ",
          "original_form": "쿨거래가 좋은 것 같아요 ㅋㅋㅋ 너무 깐깐하면 그냥 새상품 사는게 서로를 위한 길..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026218.1.13",
          "form": "맞아요.  꼭 질문이 많은사람들이",
          "original_form": "맞아요.  꼭 질문이 많은사람들이",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026218.1.14",
          "form": "쿨거래가 안되는것같아요",
          "original_form": "쿨거래가 안되는것같아요",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026218.1.15",
          "form": "진짜 살 사람은 바로 사죠",
          "original_form": "진짜 살 사람은 바로 사죠",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026218.1.16",
          "form": "질문 많이하고 거래되면 좋은데 꼭 안사요 ㅋㅋㅋ",
          "original_form": "질문 많이하고 거래되면 좋은데 꼭 안사요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026218.1.17",
          "form": "살 사람은 약간 티가 나요",
          "original_form": "살 사람은 약간 티가 나요",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026218.1.18",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026218.1.19",
          "form": "저는당근으로 식물거래 많이해요",
          "original_form": "저는당근으로 식물거래 많이해요",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026218.1.20",
          "form": "우와 신기해요",
          "original_form": "우와 신기해요",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026218.1.21",
          "form": "식물 좋아하시나봐요",
          "original_form": "식물 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026218.1.22",
          "form": "베란다에 식물 가득 넣어놓고싶어서",
          "original_form": "베란다에 식물 가득 넣어놓고싶어서",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026218.1.23",
          "form": "하나씩 늘리고있어요",
          "original_form": "하나씩 늘리고있어요",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026218.1.24",
          "form": "식물은 진짜 저렴하게도 올라오거든요",
          "original_form": "식물은 진짜 저렴하게도 올라오거든요",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026218.1.25",
          "form": "우와~ 어떤 식물 주로 모으세요? 선인장?",
          "original_form": "우와~ 어떤 식물 주로 모으세요? 선인장?",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026218.1.26",
          "form": "골고루있어요.  선인장도있고 행운목, 홍콩야자, 여인초, 해피트리, 녹보수, 몬스테라 등등",
          "original_form": "골고루있어요.  선인장도있고 행운목, 홍콩야자, 여인초, 해피트리, 녹보수, 몬스테라 등등",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026218.1.27",
          "form": "큰화분좋아해요",
          "original_form": "큰화분좋아해요",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026218.1.28",
          "form": "카페같이 만들고싶어요",
          "original_form": "카페같이 만들고싶어요",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026218.1.29",
          "form": "우와 너무 좋네요~",
          "original_form": "우와 너무 좋네요~",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026218.1.30",
          "form": "저도 식물 좋아하긴 하는데 꽃병에다가 꽃 사서 꽂아놔요",
          "original_form": "저도 식물 좋아하긴 하는데 꽃병에다가 꽃 사서 꽂아놔요",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026218.1.31",
          "form": "저두 꽃 좋아해요!",
          "original_form": "저두 꽃 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026218.1.32",
          "form": "양재꽃시장 가는거 좋아해요!",
          "original_form": "양재꽃시장 가는거 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026218.1.33",
          "form": "우와~ 꽃시장까지!",
          "original_form": "우와~ 꽃시장까지!",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026218.1.34",
          "form": "제대로 즐기시네요!",
          "original_form": "제대로 즐기시네요!",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026218.1.35",
          "form": "꽃시장은 모든 꽃 다 파는 거죠?",
          "original_form": "꽃시장은 모든 꽃 다 파는 거죠?",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026218.1.36",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026218.1.37",
          "form": "구경만해도 행복해져요",
          "original_form": "구경만해도 행복해져요",
          "speaker_id": "2",
          "time": "20210901 21:30"
        }
      ]
