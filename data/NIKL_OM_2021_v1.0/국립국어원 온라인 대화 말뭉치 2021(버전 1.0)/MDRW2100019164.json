      "utterance": [
        {
          "id": "MDRW2100019164.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019164.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019164.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019164.1.4",
          "form": "네! 전시 보는 거 좋아해서 서울 자주 가고 그랬어요",
          "original_form": "네! 전시 보는 거 좋아해서 서울 자주 가고 그랬어요",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019164.1.5",
          "form": "어디 어디 여행 가보셨어요?",
          "original_form": "어디 어디 여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019164.1.6",
          "form": "저는 국내는 경주 전주 안동 광주 가봤어요",
          "original_form": "저는 국내는 경주 전주 안동 광주 가봤어요",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019164.1.7",
          "form": "저도 경주 가봤어요!",
          "original_form": "저도 경주 가봤어요!",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019164.1.8",
          "form": "경주 좋죠!",
          "original_form": "경주 좋죠!",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019164.1.9",
          "form": "안압지 가보셨나요?",
          "original_form": "안압지 가보셨나요?",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019164.1.10",
          "form": "매번 더울 때 가봤는데 선선할 때 가보면 좋을 것 같아요..",
          "original_form": "매번 더울 때 가봤는데 선선할 때 가보면 좋을 것 같아요..",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019164.1.11",
          "form": "네ㅋㅋㅋ밤에 예쁘더라구요",
          "original_form": "네ㅋㅋㅋ밤에 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019164.1.12",
          "form": "선생님은 언제 가보셨어요?",
          "original_form": "선생님은 언제 가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019164.1.13",
          "form": "저도 꽤되었어요 한 3년되었네요",
          "original_form": "저도 꽤되었어요 한 3년되었네요",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019164.1.14",
          "form": "이맘때쯤 가면 참좋죠...",
          "original_form": "이맘때쯤 가면 참좋죠...",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019164.1.15",
          "form": "요새 보니까 놀거리 정말 많아졌더라구요!",
          "original_form": "요새 보니까 놀거리 정말 많아졌더라구요!",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019164.1.16",
          "form": "어떤게 생겼나요!",
          "original_form": "어떤게 생겼나요!",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019164.1.17",
          "form": "알려주세요",
          "original_form": "알려주세요",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019164.1.18",
          "form": "필름카메라 자판기도 생겼고",
          "original_form": "필름카메라 자판기도 생겼고",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019164.1.19",
          "form": "도깨비 운세였나...?운세 보는 게 있더라구요",
          "original_form": "도깨비 운세였나...?운세 보는 게 있더라구요",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019164.1.20",
          "form": "제가 사는 곳엔 없어서 신기했어요",
          "original_form": "제가 사는 곳엔 없어서 신기했어요",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019164.1.21",
          "form": "아하 그거 혹시 운세 뽑기같은거 있는 그런건가요?",
          "original_form": "아하 그거 혹시 운세 뽑기같은거 있는 그런건가요?",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019164.1.22",
          "form": "네 자기 띠로 운세 보는 거였는데 신기했어요",
          "original_form": "네 자기 띠로 운세 보는 거였는데 신기했어요",
          "speaker_id": "1",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019164.1.23",
          "form": "그거 전주에도 있더라구요",
          "original_form": "그거 전주에도 있더라구요",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019164.1.24",
          "form": "관광지마다 있나봐요 신기하네요",
          "original_form": "관광지마다 있나봐요 신기하네요",
          "speaker_id": "2",
          "time": "20210827 16:51"
        }
      ]
    }
  ]
