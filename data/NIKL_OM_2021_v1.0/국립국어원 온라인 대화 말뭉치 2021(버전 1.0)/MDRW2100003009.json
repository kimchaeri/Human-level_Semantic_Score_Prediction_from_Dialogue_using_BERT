      "utterance": [
        {
          "id": "MDRW2100003009.1.1",
          "form": "넷플에서 최근에 본 것 중에",
          "original_form": "넷플에서 최근에 본 것 중에",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003009.1.2",
          "form": "우리는 영원히 어리지 않다는 다큐멘터리 있어",
          "original_form": "우리는 영원히 어리지 않다는 다큐멘터리 있어",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003009.1.3",
          "form": "그게 뭐야?",
          "original_form": "그게 뭐야?",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003009.1.4",
          "form": "미국 국가대표 체조 선수단의",
          "original_form": "미국 국가대표 체조 선수단의",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003009.1.5",
          "form": "선수단 의사의 성폭력을 고발한 다큐멘터리야",
          "original_form": "선수단 의사의 성폭력을 고발한 다큐멘터리야",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003009.1.6",
          "form": "다큐멘터리면 나 보다가 잘것 같은데",
          "original_form": "다큐멘터리면 나 보다가 잘것 같은데",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003009.1.7",
          "form": "잘 만든 다큐멘터리라",
          "original_form": "잘 만든 다큐멘터리라",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003009.1.8",
          "form": "시선을 잡아끌어서 그럴 일은 없을거야",
          "original_form": "시선을 잡아끌어서 그럴 일은 없을거야",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003009.1.9",
          "form": "그럼 다행이네",
          "original_form": "그럼 다행이네",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003009.1.10",
          "form": "나 최근에 지루한거 보면 잘자서 자는 용으로 쓸뻔",
          "original_form": "나 최근에 지루한거 보면 잘자서 자는 용으로 쓸뻔",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003009.1.11",
          "form": "ㅋㅋ 잠에 못들거야",
          "original_form": "ㅋㅋ 잠에 못들거야",
          "speaker_id": "1",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100003009.1.12",
          "form": "충격적이라",
          "original_form": "충격적이라",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100003009.1.13",
          "form": "또 본건...그거 있다",
          "original_form": "또 본건...그거 있다",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100003009.1.14",
          "form": "글로우 업이라는 프로그램 넷플에서 봤어",
          "original_form": "글로우 업이라는 프로그램 넷플에서 봤어",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100003009.1.15",
          "form": "그건 무슨 내용이야?",
          "original_form": "그건 무슨 내용이야?",
          "speaker_id": "2",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003009.1.16",
          "form": "메이크업 콘테스트야",
          "original_form": "메이크업 콘테스트야",
          "speaker_id": "1",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003009.1.17",
          "form": "일반적인 화장이 아니라",
          "original_form": "일반적인 화장이 아니라",
          "speaker_id": "1",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003009.1.18",
          "form": "매주 특수분장을 해",
          "original_form": "매주 특수분장을 해",
          "speaker_id": "1",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003009.1.19",
          "form": "그런 것도 있구나",
          "original_form": "그런 것도 있구나",
          "speaker_id": "2",
          "time": "20210820 19:42"
        },
        {
          "id": "MDRW2100003009.1.20",
          "form": "난 화장 관련은 아에 안봐서 몰랐네",
          "original_form": "난 화장 관련은 아에 안봐서 몰랐네",
          "speaker_id": "2",
          "time": "20210820 19:42"
        },
        {
          "id": "MDRW2100003009.1.21",
          "form": "나도 화장은 관심없었는데",
          "original_form": "나도 화장은 관심없었는데",
          "speaker_id": "1",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003009.1.22",
          "form": "이건 화장이 아니라 분장이더라고",
          "original_form": "이건 화장이 아니라 분장이더라고",
          "speaker_id": "1",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003009.1.23",
          "form": "넌 메이크업 영상은 관심없어?",
          "original_form": "넌 메이크업 영상은 관심없어?",
          "speaker_id": "1",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003009.1.24",
          "form": "메이크업을 잘 안하니까 안보게 되더라구",
          "original_form": "메이크업을 잘 안하니까 안보게 되더라구",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100003009.1.25",
          "form": "하긴 그럴것같아",
          "original_form": "하긴 그럴것같아",
          "speaker_id": "1",
          "time": "20210823 10:51"
        }
      ]
    }
  ]
