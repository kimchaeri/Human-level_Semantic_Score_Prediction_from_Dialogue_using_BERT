      "utterance": [
        {
          "id": "MDRW2100023335.1.1",
          "form": "오 전 식음료하면 커피",
          "original_form": "오 전 식음료하면 커피",
          "speaker_id": "2",
          "time": "20210830 00:18"
        },
        {
          "id": "MDRW2100023335.1.2",
          "form": "마라탕",
          "original_form": "마라탕",
          "speaker_id": "2",
          "time": "20210830 00:18"
        },
        {
          "id": "MDRW2100023335.1.3",
          "form": "전 마라탕 먹어본적이 없어요",
          "original_form": "전 마라탕 먹어본적이 없어요",
          "speaker_id": "1",
          "time": "20210830 00:18"
        },
        {
          "id": "MDRW2100023335.1.4",
          "form": "전 쌀국수 라떼!!",
          "original_form": "전 쌀국수 라떼!!",
          "speaker_id": "1",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023335.1.5",
          "form": "쌀국수 좋쵸",
          "original_form": "쌀국수 좋쵸",
          "speaker_id": "2",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023335.1.6",
          "form": "맛나여",
          "original_form": "맛나여",
          "speaker_id": "2",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023335.1.7",
          "form": "맞아요~ 오늘도 먹었는데 맛있어요",
          "original_form": "맞아요~ 오늘도 먹었는데 맛있어요",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023335.1.8",
          "form": "쌀국수 배달은 안시켜보셨죠",
          "original_form": "쌀국수 배달은 안시켜보셨죠",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023335.1.9",
          "form": "마라탕은 꽤 매워요",
          "original_form": "마라탕은 꽤 매워요",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023335.1.10",
          "form": "오늘 배달시켜먹었어요 ㅎㅎ",
          "original_form": "오늘 배달시켜먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023335.1.11",
          "form": "아 면이 안불었나요",
          "original_form": "아 면이 안불었나요",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023335.1.12",
          "form": "포장해서 자주 먹는편이라~~",
          "original_form": "포장해서 자주 먹는편이라~~",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023335.1.13",
          "form": "한.. 10분거리에 있는 가겐데 포장해와도 괜찮았어요",
          "original_form": "한.. 10분거리에 있는 가겐데 포장해와도 괜찮았어요",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023335.1.14",
          "form": "배달은 더 빨리와서 괜찮더라구요",
          "original_form": "배달은 더 빨리와서 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023335.1.15",
          "form": "갑자기 먹고싶네요",
          "original_form": "갑자기 먹고싶네요",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023335.1.16",
          "form": "저는 훠궈먹고 너무 이상한 맛에 놀라서 ㅎㅎ 마라탕은 시도도 안해봤어요",
          "original_form": "저는 훠궈먹고 너무 이상한 맛에 놀라서 ㅎㅎ 마라탕은 시도도 안해봤어요",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023335.1.17",
          "form": "훠궈는 저도 별루예요",
          "original_form": "훠궈는 저도 별루예요",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023335.1.18",
          "form": "어디사세여",
          "original_form": "어디사세여",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023335.1.19",
          "form": "동네 맛집 있어요?",
          "original_form": "동네 맛집 있어요?",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023335.1.20",
          "form": "쌀국수 맛집이요?",
          "original_form": "쌀국수 맛집이요?",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023335.1.21",
          "form": "제가 자주먹는곳은 체인이에욤",
          "original_form": "제가 자주먹는곳은 체인이에욤",
          "speaker_id": "1",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100023335.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100023335.1.23",
          "form": "체인점이 안전한것 같어요",
          "original_form": "체인점이 안전한것 같어요",
          "speaker_id": "2",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100023335.1.24",
          "form": "맛집은.. 그냥 장어집? 빵집?",
          "original_form": "맛집은.. 그냥 장어집? 빵집?",
          "speaker_id": "1",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100023335.1.25",
          "form": "근데 막 유명한 맛집에 비할건 아니죠ㅠㅠ",
          "original_form": "근데 막 유명한 맛집에 비할건 아니죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100023335.1.26",
          "form": "아무래도 동네보단 번화가에 맛집도 더 있을테니.",
          "original_form": "아무래도 동네보단 번화가에 맛집도 더 있을테니.",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023335.1.27",
          "form": "빵집도 요즘 작은 가게들이 맛있는데 많아요",
          "original_form": "빵집도 요즘 작은 가게들이 맛있는데 많아요",
          "speaker_id": "2",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023335.1.28",
          "form": "숨어있어요 ㅎ",
          "original_form": "숨어있어요 ㅎ",
