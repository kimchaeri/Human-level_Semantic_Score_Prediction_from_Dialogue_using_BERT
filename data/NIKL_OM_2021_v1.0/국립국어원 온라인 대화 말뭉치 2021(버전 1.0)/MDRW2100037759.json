      "utterance": [
        {
          "id": "MDRW2100037759.1.1",
          "form": "사시는 곳 오늘 날씨는 어떠신가요?",
          "original_form": "사시는 곳 오늘 날씨는 어떠신가요?",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100037759.1.2",
          "form": "쫌습하지만",
          "original_form": "쫌습하지만",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100037759.1.3",
          "form": "날이 시원하네요",
          "original_form": "날이 시원하네요",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100037759.1.4",
          "form": "좀 춥다싶죠? ㅋㅋ",
          "original_form": "좀 춥다싶죠? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100037759.1.5",
          "form": "오후부턴 전국에 비온다고ㅜㅠ",
          "original_form": "오후부턴 전국에 비온다고ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100037759.1.6",
          "form": "그니까여ㅜㅜ",
          "original_form": "그니까여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100037759.1.7",
          "form": "이번주도 비왓다갓다하나바여",
          "original_form": "이번주도 비왓다갓다하나바여",
          "speaker_id": "2",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100037759.1.8",
          "form": "지긋지긋하죠ㅜㅠ",
          "original_form": "지긋지긋하죠ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100037759.1.9",
          "form": "ㅠㅠ가을장마",
          "original_form": "ㅠㅠ가을장마",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100037759.1.10",
          "form": "주말엔 날씨가 되게 좋았어요",
          "original_form": "주말엔 날씨가 되게 좋았어요",
          "speaker_id": "1",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100037759.1.11",
          "form": "여기는 어제 비 왓엇어요 흑",
          "original_form": "여기는 어제 비 왓엇어요 흑",
          "speaker_id": "2",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100037759.1.12",
          "form": "아...ㅜㅠ 그럼 진짜 지긋지긋ㅜ 긴팔 입으셨나요??",
          "original_form": "아...ㅜㅠ 그럼 진짜 지긋지긋ㅜ 긴팔 입으셨나요??",
          "speaker_id": "1",
          "time": "20210906 12:32"
        },
        {
          "id": "MDRW2100037759.1.13",
          "form": "긴팔에 반바지 입어요ㅎㅎ",
          "original_form": "긴팔에 반바지 입어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:33"
        },
        {
          "id": "MDRW2100037759.1.14",
          "form": "근무지 에어컨 켜져있나요? ㅋㅋ",
          "original_form": "근무지 에어컨 켜져있나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:15"
        },
        {
          "id": "MDRW2100037759.1.15",
          "form": "네 넘빵빵해서 추워서",
          "original_form": "네 넘빵빵해서 추워서",
          "speaker_id": "2",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100037759.1.16",
          "form": "일할땐가디건필수에여",
          "original_form": "일할땐가디건필수에여",
          "speaker_id": "2",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100037759.1.17",
          "form": "그러니까요 일할땐 에어컨 안켤수가 없네요ㅜㅜ ㅋㅋ",
          "original_form": "그러니까요 일할땐 에어컨 안켤수가 없네요ㅜㅜ ㅋㅋ",
