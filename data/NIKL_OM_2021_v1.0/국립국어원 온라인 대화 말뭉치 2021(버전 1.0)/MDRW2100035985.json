      "utterance": [
        {
          "id": "MDRW2100035985.1.1",
          "form": "요즘 드라마나 영화 보신거있으세요?",
          "original_form": "요즘 드라마나 영화 보신거있으세요?",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035985.1.2",
          "form": "요즘 드라마 본거는 로스쿨!! 아시나요?!?!",
          "original_form": "요즘 드라마 본거는 로스쿨!! 아시나요?!?!",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035985.1.3",
          "form": "넹넹! 그거 결말까지 잘 났나요??",
          "original_form": "넹넹! 그거 결말까지 잘 났나요??",
          "speaker_id": "1",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035985.1.4",
          "form": "네네!! 시즌2 소망하시는 분들도 많더라구요 ㅋㅋㅋ",
          "original_form": "네네!! 시즌2 소망하시는 분들도 많더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100035985.1.5",
          "form": "name1님은",
          "original_form": "&name1&님은",
          "speaker_id": "2",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100035985.1.6",
          "form": "최근 재밌게 보신거 있나여??",
          "original_form": "최근 재밌게 보신거 있나여??",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.7",
          "form": "오 저는 완결난 드라마 후기 찾아보고 괜찮으면",
          "original_form": "오 저는 완결난 드라마 후기 찾아보고 괜찮으면",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.8",
          "form": "보는 타입이라",
          "original_form": "보는 타입이라",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.9",
          "form": "로스쿨도 메모해둘게요!",
          "original_form": "로스쿨도 메모해둘게요!",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.10",
          "form": "저는 올해 드라마는 아닌데 카이로스 되게 재밌게 봤어요!",
          "original_form": "저는 올해 드라마는 아닌데 카이로스 되게 재밌게 봤어요!",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.11",
          "form": "와 저도 기다리는거못해서 왠만하면 완결나고 봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 저도 기다리는거못해서 왠만하면 완결나고 봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.13",
          "form": "카이로스",
          "original_form": "카이로스",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.14",
          "form": "후반으로 갈수록 점점 더 재밌다고 들었어여 ㅋㅋㅋㅋ",
          "original_form": "후반으로 갈수록 점점 더 재밌다고 들었어여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100035985.1.15",
          "form": "아 맞아요ㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100035985.1.16",
          "form": "전개가 휘몰아쳐요",
          "original_form": "전개가 휘몰아쳐요",
          "speaker_id": "1",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100035985.1.17",
          "form": "소재도 신선하고 추천드려용",
          "original_form": "소재도 신선하고 추천드려용",
          "speaker_id": "1",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100035985.1.18",
          "form": "네네!! 안그래도 보구싶었어여!! 넷플에 올라오면 좋겠네여 결제하고 있는데 넷플뿐이라 ㅠㅠ",
          "original_form": "네네!! 안그래도 보구싶었어여!! 넷플에 올라오면 좋겠네여 결제하고 있는데 넷플뿐이라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035985.1.19",
          "form": "아아ㅠㅠ 이거 그런 얘기 많았어요",
          "original_form": "아아ㅠㅠ 이거 그런 얘기 많았어요",
          "speaker_id": "1",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035985.1.20",
          "form": "해외에서 좋아할 소재같다고",
          "original_form": "해외에서 좋아할 소재같다고",
          "speaker_id": "1",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035985.1.21",
          "form": "넷플릭스 오픈하면 인기 많을텐데ㅠㅠ",
          "original_form": "넷플릭스 오픈하면 인기 많을텐데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035985.1.22",
          "form": "넷플아...!! 일해라 !!!!!",
          "original_form": "넷플아...!! 일해라 !!!!!",
          "speaker_id": "2",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100035985.1.23",
          "form": "넷플릭스 최근에",
          "original_form": "넷플릭스 최근에",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100035985.1.24",
          "form": "디피 오픈했던데",
          "original_form": "디피 오픈했던데",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100035985.1.25",
          "form": "보셨어용??",
          "original_form": "보셨어용??",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100035985.1.26",
          "form": "아 그거 인기많더라규요 아직 보진못했어요!!",
          "original_form": "아 그거 인기많더라규요 아직 보진못했어요!!",
          "speaker_id": "2",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100035985.1.27",
          "form": "보셨나여??",
          "original_form": "보셨나여??",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035985.1.28",
          "form": "저도 후기가 넘 좋아서",
          "original_form": "저도 후기가 넘 좋아서",
          "speaker_id": "1",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035985.1.29",
          "form": "킵해두고 있고 아직 못봤어요ㅠㅠ",
          "original_form": "킵해두고 있고 아직 못봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035985.1.30",
          "form": "저도 담에 볼래여 ㅋㅋㅋㅋ",
          "original_form": "저도 담에 볼래여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035985.1.31",
          "form": "넷플에서 재밌게 본거 하나 추천해보자면",
          "original_form": "넷플에서 재밌게 본거 하나 추천해보자면",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035985.1.32",
          "form": "별나도 괜찮아",
          "original_form": "별나도 괜찮아",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100035985.1.33",
          "form": "오오 추천해주세요!",
          "original_form": "오오 추천해주세요!",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100035985.1.34",
          "form": "드라마인가용?",
          "original_form": "드라마인가용?",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100035985.1.35",
          "form": "재밌어용ㅋㅋㅋ 대사도 좋고 ㅋㅋㅋㅋ",
          "original_form": "재밌어용ㅋㅋㅋ 대사도 좋고 ㅋㅋㅋㅋ",
