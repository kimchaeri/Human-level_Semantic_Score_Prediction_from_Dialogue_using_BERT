      "utterance": [
        {
          "id": "MDRW2100014621.1.1",
          "form": "요즘 많은 콜라보 제품나오잖아요",
          "original_form": "요즘 많은 콜라보 제품나오잖아요",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014621.1.2",
          "form": "곰표 맥주 마셔보셨어요?",
          "original_form": "곰표 맥주 마셔보셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014621.1.3",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014621.1.4",
          "form": "저는 곰표는 젤리만 먹어봤어요",
          "original_form": "저는 곰표는 젤리만 먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014621.1.5",
          "form": "곰표 젤리요?",
          "original_form": "곰표 젤리요?",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014621.1.6",
          "form": "무슨맛이예요?",
          "original_form": "무슨맛이예요?",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014621.1.7",
          "form": "화한 맛이어서 잠깰 때 먹자고 그랬었네요",
          "original_form": "화한 맛이어서 잠깰 때 먹자고 그랬었네요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014621.1.8",
          "form": "비추합니다",
          "original_form": "비추합니다",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014621.1.9",
          "form": "읔",
          "original_form": "읔",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014621.1.10",
          "form": "곰표맥주는 맛있나요?",
          "original_form": "곰표맥주는 맛있나요?",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014621.1.11",
          "form": "맥주는 맛있는데",
          "original_form": "맥주는 맛있는데",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014621.1.12",
          "form": "그렇군요 역시 젤리만 실패였네요 ㅜㅜ",
          "original_form": "그렇군요 역시 젤리만 실패였네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014621.1.13",
          "form": "사보지는 않았지만 지코가 광고했던 꼬뜨게랑이 나름 재밌었던 것 같아요",
          "original_form": "사보지는 않았지만 지코가 광고했던 꼬뜨게랑이 나름 재밌었던 것 같아요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014621.1.14",
          "form": "무슨 명품같이 옷도 나오고 그랬던 것 같은데 기억에 남았네요",
          "original_form": "무슨 명품같이 옷도 나오고 그랬던 것 같은데 기억에 남았네요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014621.1.15",
          "form": "저어... 그거 가방 갖고있어요 ㅋ",
          "original_form": "저어... 그거 가방 갖고있어요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014621.1.16",
          "form": "우와~",
          "original_form": "우와~",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014621.1.17",
          "form": "그거 가지신 분 주변에는 없어서 궁금했어요",
          "original_form": "그거 가지신 분 주변에는 없어서 궁금했어요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014621.1.18",
          "form": "고급스러운 느낌이었던 것 같은데 어때요?",
          "original_form": "고급스러운 느낌이었던 것 같은데 어때요?",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014621.1.19",
          "form": "실용성 좋고 찐 가죽은 아니지만 가죽같아서 보여서 고급져요",
          "original_form": "실용성 좋고 찐 가죽은 아니지만 가죽같아서 보여서 고급져요",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014621.1.20",
          "form": "가지고 다녀도 괜찮겠네요 어떤 건 정말 기념품 같아서 모셔두기만 하기도 하는데 말이죠",
          "original_form": "가지고 다녀도 괜찮겠네요 어떤 건 정말 기념품 같아서 모셔두기만 하기도 하는데 말이죠",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014621.1.21",
          "form": "특히 로브 ㅋㅋ 지코아니면 소화못한다능",
          "original_form": "특히 로브 ㅋㅋ 지코아니면 소화못한다능",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014621.1.22",
          "form": "집에서만 착용이 가능하다는...",
          "original_form": "집에서만 착용이 가능하다는...",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014621.1.23",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014621.1.24",
          "form": "가방이랑 티셔츠가 가장실용적이었어요",
          "original_form": "가방이랑 티셔츠가 가장실용적이었어요",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014621.1.25",
          "form": "가방은 바로 매진",
          "original_form": "가방은 바로 매진",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014621.1.26",
          "form": "희귀템이었군요",
          "original_form": "희귀템이었군요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014621.1.27",
          "form": "티셔츠도 무슨 명품티셔츠랑 비슷했던 것 같았거든요",
          "original_form": "티셔츠도 무슨 명품티셔츠랑 비슷했던 것 같았거든요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014621.1.28",
          "form": "ㅋㅋ 그걸 산 나란 녀석",
          "original_form": "ㅋㅋ 그걸 산 나란 녀석",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014621.1.29",
          "form": "넹 그걸. 노린듯",
          "original_form": "넹 그걸. 노린듯",
          "speaker_id": "1",
          "time": "20210825 20:36"
        }
      ]
    }
