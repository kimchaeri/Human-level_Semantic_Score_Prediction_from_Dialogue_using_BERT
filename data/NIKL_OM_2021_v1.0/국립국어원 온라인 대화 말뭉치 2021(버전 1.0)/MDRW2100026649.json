      "utterance": [
        {
          "id": "MDRW2100026649.1.1",
          "form": "하이요",
          "original_form": "하이요",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026649.1.2",
          "form": "어떤 직업을 가지고 계십니까?",
          "original_form": "어떤 직업을 가지고 계십니까?",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026649.1.3",
          "form": "평범한 회사원입니다",
          "original_form": "평범한 회사원입니다",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026649.1.4",
          "form": "매일 아침",
          "original_form": "매일 아침",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026649.1.5",
          "form": "출근하기 너무싫어요",
          "original_form": "출근하기 너무싫어요",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026649.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026649.1.7",
          "form": "저랑 똑같으시네요",
          "original_form": "저랑 똑같으시네요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026649.1.8",
          "form": "퇴사가 꿈입니다 ㅎ",
          "original_form": "퇴사가 꿈입니다 ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026649.1.9",
          "form": "아..퇴사하고싶은데",
          "original_form": "아..퇴사하고싶은데",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026649.1.10",
          "form": "먹고살아야해서",
          "original_form": "먹고살아야해서",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026649.1.11",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026649.1.12",
          "form": "그쵸 ㅎㅎ",
          "original_form": "그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026649.1.13",
          "form": "그 이유만 아니면..",
          "original_form": "그 이유만 아니면..",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026649.1.14",
          "form": "요즘은 이직도 쉽지 않습니다",
          "original_form": "요즘은 이직도 쉽지 않습니다",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026649.1.15",
          "form": "코로나로 실직자도 많고",
          "original_form": "코로나로 실직자도 많고",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026649.1.16",
          "form": "직장이..",
          "original_form": "직장이..",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026649.1.17",
          "form": "눈뜨면 퇴사생각",
          "original_form": "눈뜨면 퇴사생각",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026649.1.18",
          "form": "마음만....가득이요..",
          "original_form": "마음만....가득이요..",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026649.1.19",
          "form": "직장이 있다는거에",
          "original_form": "직장이 있다는거에",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026649.1.20",
          "form": "요즘은 감사해야ㅛ",
          "original_form": "요즘은 감사해야ㅛ",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026649.1.21",
          "form": "야죠",
          "original_form": "야죠",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026649.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026649.1.23",
          "form": "쉬운일이 없네요",
          "original_form": "쉬운일이 없네요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026649.1.24",
          "form": "정답입니다",
          "original_form": "정답입니다",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026649.1.25",
          "form": "코로나가 제일 문제같아요",
          "original_form": "코로나가 제일 문제같아요",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026649.1.26",
          "form": "급여가 올랐으면 좋겠어요",
          "original_form": "급여가 올랐으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026649.1.27",
          "form": "취업이든 이직이든..",
          "original_form": "취업이든 이직이든..",
          "speaker_id": "1",
          "time": "20210901 23:35"
        }
      ]
    }
