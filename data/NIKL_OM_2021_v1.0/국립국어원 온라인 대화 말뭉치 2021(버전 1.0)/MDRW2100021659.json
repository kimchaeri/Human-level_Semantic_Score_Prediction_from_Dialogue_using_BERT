      "utterance": [
        {
          "id": "MDRW2100021659.1.1",
          "form": "청소요 ㅋㅋ",
          "original_form": "청소요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021659.1.2",
          "form": "아잇 청소!!",
          "original_form": "아잇 청소!!",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021659.1.3",
          "form": "청소도 먼지털기, 바닥닦기, 변기닦기, 창문닦기, 바닥쓸기 등 다양하더라구요",
          "original_form": "청소도 먼지털기, 바닥닦기, 변기닦기, 창문닦기, 바닥쓸기 등 다양하더라구요",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021659.1.4",
          "form": "어떤 게 젤 싫으세요?",
          "original_form": "어떤 게 젤 싫으세요?",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021659.1.5",
          "form": "화장실청소가 젤 싫어요",
          "original_form": "화장실청소가 젤 싫어요",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021659.1.6",
          "form": "저희집은 곰팡이도 잘생기는편이라",
          "original_form": "저희집은 곰팡이도 잘생기는편이라",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021659.1.7",
          "form": "곰팡이싹도 수시로 뿌려줘야하고",
          "original_form": "곰팡이싹도 수시로 뿌려줘야하고",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021659.1.8",
          "form": "허걱 화장실에 혹시 환기 없어요?",
          "original_form": "허걱 화장실에 혹시 환기 없어요?",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021659.1.9",
          "form": "아니 그 창문이요!",
          "original_form": "아니 그 창문이요!",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021659.1.10",
          "form": "변기닦는것도 허허",
          "original_form": "변기닦는것도 허허",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021659.1.11",
          "form": "있긴한데 현관으로 뚫린 창문이라",
          "original_form": "있긴한데 현관으로 뚫린 창문이라",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021659.1.12",
          "form": "환기가 잘되진 않아요 ㅜ",
          "original_form": "환기가 잘되진 않아요 ㅜ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021659.1.13",
          "form": "아.. 진짜 많이 힘들겠는데요?",
          "original_form": "아.. 진짜 많이 힘들겠는데요?",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021659.1.14",
          "form": "저희 집은 변기 아래 실리콘에만 곰팡이 생기네요",
          "original_form": "저희 집은 변기 아래 실리콘에만 곰팡이 생기네요",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021659.1.15",
          "form": "올 때부터 이랬는데 보기가 넘 안좋아요 ㅜㅜ",
          "original_form": "올 때부터 이랬는데 보기가 넘 안좋아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021659.1.16",
          "form": "저희집은 홈스타착붙는락스 쓰는데 이거 곰팡이 제거 잘돼요!!",
          "original_form": "저희집은 홈스타착붙는락스 쓰는데 이거 곰팡이 제거 잘돼요!!",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021659.1.17",
          "form": "오 그거 lg생활건강꺼죠?",
          "original_form": "오 그거 lg생활건강꺼죠?",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021659.1.18",
          "form": "저도 그거 썼는데 그거로도 소생불가더라구요^^....",
          "original_form": "저도 그거 썼는데 그거로도 소생불가더라구요^^....",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021659.1.19",
          "form": "아 그래요 ㅜ 저는 이거 진짜 잘 쓰고 있는데...",
          "original_form": "아 그래요 ㅜ 저는 이거 진짜 잘 쓰고 있는데...",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021659.1.20",
          "form": "다른데엔 다 효과 엄청 있던데!!",
          "original_form": "다른데엔 다 효과 엄청 있던데!!",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021659.1.21",
          "form": "딱 거기만 효과 없었어요.. 눈물..",
          "original_form": "딱 거기만 효과 없었어요.. 눈물..",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021659.1.22",
          "form": "지독한 곰팡이네요",
          "original_form": "지독한 곰팡이네요",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021659.1.23",
          "form": "실리콘을 새로 바르는 수밖에 없는 걸까요",
          "original_form": "실리콘을 새로 바르는 수밖에 없는 걸까요",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021659.1.24",
          "form": "집주인이 그걸 좀 해줬으면 좋겠는데 안해주실 것 같아요 ㅜㅜ",
          "original_form": "집주인이 그걸 좀 해줬으면 좋겠는데 안해주실 것 같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021659.1.25",
          "form": "아 ㅜ 원룸 자취하세요?",
          "original_form": "아 ㅜ 원룸 자취하세요?",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021659.1.26",
          "form": "네ㅔㄴ 저 원룸 자취인데 집주인이 건물주에요",
          "original_form": "네ㅔㄴ 저 원룸 자취인데 집주인이 건물주에요",
