      "utterance": [
        {
          "id": "MDRW2100010626.1.1",
          "form": "이번 추석때 다들 모이나???",
          "original_form": "이번 추석때 다들 모이나???",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010626.1.2",
          "form": "모르것어~\n우린 코로나 심하면 그냥 얼굴만보고 가기도해서",
          "original_form": "모르것어~\n우린 코로나 심하면 그냥 얼굴만보고 가기도해서",
          "speaker_id": "2",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010626.1.3",
          "form": "자기네 집은 무조건 모이지?",
          "original_form": "자기네 집은 무조건 모이지?",
          "speaker_id": "2",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010626.1.4",
          "form": "인천선 안올테고",
          "original_form": "인천선 안올테고",
          "speaker_id": "1",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010626.1.5",
          "form": "큰매형네도 코로나땜에 올란지 모르겄다",
          "original_form": "큰매형네도 코로나땜에 올란지 모르겄다",
          "speaker_id": "1",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010626.1.6",
          "form": "자기두 가지말궁 집에 있엉~,ㅋ",
          "original_form": "자기두 가지말궁 집에 있엉~,ㅋ",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010626.1.7",
          "form": "근데 나까지 안가는것도 좀 그래서..",
          "original_form": "근데 나까지 안가는것도 좀 그래서..",
          "speaker_id": "1",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010626.1.8",
          "form": "응응 난 코로나 함 봐서 갈까그래",
          "original_form": "응응 난 코로나 함 봐서 갈까그래",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100010626.1.9",
          "form": "엄마 건강이 그리 좋진 않으니 자주 가야하는데",
          "original_form": "엄마 건강이 그리 좋진 않으니 자주 가야하는데",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100010626.1.10",
          "form": "걱정이다 정말ㅠㅠ",
          "original_form": "걱정이다 정말ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100010626.1.11",
          "form": "자주까진 아니라도 가야할땐 좀 가~~~",
          "original_form": "자주까진 아니라도 가야할땐 좀 가~~~",
          "speaker_id": "1",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100010626.1.12",
          "form": "ㅇㅇ 안그래도 엄마가 많이 보고싶어하긴함",
          "original_form": "ㅇㅇ 안그래도 엄마가 많이 보고싶어하긴함",
          "speaker_id": "2",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100010626.1.13",
          "form": "자기는 안보고싶고??? ㅎ",
          "original_form": "자기는 안보고싶고??? ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:37"
        },
        {
          "id": "MDRW2100010626.1.14",
          "form": "나도 보고싶지",
          "original_form": "나도 보고싶지",
          "speaker_id": "2",
          "time": "20210824 11:37"
        },
        {
          "id": "MDRW2100010626.1.15",
          "form": "너무 멀리 떨어져 있으니 부모님도 난  손님대접이야ㅋ",
          "original_form": "너무 멀리 떨어져 있으니 부모님도 난  손님대접이야ㅋ",
          "speaker_id": "2",
          "time": "20210824 11:37"
        },
        {
          "id": "MDRW2100010626.1.16",
          "form": "대화할거도 많이 없엉ㅋ",
          "original_form": "대화할거도 많이 없엉ㅋ",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100010626.1.17",
          "form": "으이구~~~ 자주 안가서 그래",
          "original_form": "으이구~~~ 자주 안가서 그래",
          "speaker_id": "1",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100010626.1.18",
          "form": "이번 추석땐 둘 다 내려가자",
          "original_form": "이번 추석땐 둘 다 내려가자",
          "speaker_id": "1",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100010626.1.19",
          "form": "난 전화도 자주 안하잖아 그래서 더그런듯",
          "original_form": "난 전화도 자주 안하잖아 그래서 더그런듯",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100010626.1.20",
          "form": "아부지, 어머니한테 돌아가면서 최소 1주일에 한번씩은 전화혀~~~~",
          "original_form": "아부지, 어머니한테 돌아가면서 최소 1주일에 한번씩은 전화혀~~~~",
          "speaker_id": "1",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100010626.1.21",
          "form": "안그래도 그럴라구\n나중에 후회하기전에\n안그래도 어제 전화했는데 안받으시더니 아침에 통화했어",
          "original_form": "안그래도 그럴라구\n나중에 후회하기전에\n안그래도 어제 전화했는데 안받으시더니 아침에 통화했어",
          "speaker_id": "2",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100010626.1.22",
          "form": "부모님도 이제 많은 연세야...",
          "original_form": "부모님도 이제 많은 연세야...",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100010626.1.23",
          "form": "그냥그냥 전화 한통..  갈 수 있을때 가고..",
          "original_form": "그냥그냥 전화 한통..  갈 수 있을때 가고..",
          "speaker_id": "1",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100010626.1.24",
          "form": "그래야지.\n건강하실때 추억만들구해야지",
          "original_form": "그래야지.\n건강하실때 추억만들구해야지",
          "speaker_id": "2",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100010626.1.25",
          "form": "그럽시다!!!!",
          "original_form": "그럽시다!!!!",
          "speaker_id": "1",
