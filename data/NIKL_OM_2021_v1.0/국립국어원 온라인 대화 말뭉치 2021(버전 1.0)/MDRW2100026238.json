      "utterance": [
        {
          "id": "MDRW2100026238.1.1",
          "form": "음 결혼 하셨나요? ㅎㅎ",
          "original_form": "음 결혼 하셨나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026238.1.2",
          "form": "오 ㅎㅎ",
          "original_form": "오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026238.1.3",
          "form": "네. 옆방에 남편있어요",
          "original_form": "네. 옆방에 남편있어요",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026238.1.4",
          "form": "저는 미혼이라서 ㅋㅋ",
          "original_form": "저는 미혼이라서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026238.1.5",
          "form": "음 제코가 석자라ㅜ",
          "original_form": "음 제코가 석자라ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026238.1.6",
          "form": "저도 아이없이 친구처럼 지내요",
          "original_form": "저도 아이없이 친구처럼 지내요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026238.1.7",
          "form": "의외로 괜찮아요^^",
          "original_form": "의외로 괜찮아요^^",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026238.1.8",
          "form": "진짜 찐친이돼서",
          "original_form": "진짜 찐친이돼서",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026238.1.9",
          "form": "음식 두개 시켜 나눠먹을수 있어요!  큰장점ㅋ",
          "original_form": "음식 두개 시켜 나눠먹을수 있어요!  큰장점ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026238.1.10",
          "form": "장점이 깨알같은데 진짜 소소해요",
          "original_form": "장점이 깨알같은데 진짜 소소해요",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026238.1.11",
          "form": "진정한행복 ㄱㅋ",
          "original_form": "진정한행복 ㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026238.1.12",
          "form": "네. 짐꾼이자 베프이자 벌레잡고 청소하는 사람 ㅎㅎ",
          "original_form": "네. 짐꾼이자 베프이자 벌레잡고 청소하는 사람 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026238.1.13",
          "form": "결혼안해도돼요",
          "original_form": "결혼안해도돼요",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026238.1.14",
          "form": "한국은 두사람이 아니라 두가족의 만남이라 신경쓸게 많죠",
          "original_form": "한국은 두사람이 아니라 두가족의 만남이라 신경쓸게 많죠",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026238.1.15",
          "form": "저는 사실 애는 안놓고싶은데 결혼은하고싶다",
          "original_form": "저는 사실 애는 안놓고싶은데 결혼은하고싶다",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026238.1.16",
          "form": "이렇게말하니까",
          "original_form": "이렇게말하니까",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026238.1.17",
          "form": "누가 그럴거면 뭐하러 결혼하냐고하더라고요",
          "original_form": "누가 그럴거면 뭐하러 결혼하냐고하더라고요",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026238.1.18",
          "form": "음 아직도 그런 고리타분한",
          "original_form": "음 아직도 그런 고리타분한",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026238.1.19",
          "form": "사람들이 정해놓은 기준선과 닮아가려는 삶이 내가 원하는 삶은 아니지요",
          "original_form": "사람들이 정해놓은 기준선과 닮아가려는 삶이 내가 원하는 삶은 아니지요",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026238.1.20",
          "form": "즐ㅡ 무시해야져^^",
          "original_form": "즐ㅡ 무시해야져^^",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026238.1.21",
          "form": "결혼해ㅡ하면 결혼이란 무엇인가?  되물어봐요ㅋ",
          "original_form": "결혼해ㅡ하면 결혼이란 무엇인가?  되물어봐요ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026238.1.22",
          "form": "자식낳아ㅡ하면 자식이란 무엇인가?",
          "original_form": "자식낳아ㅡ하면 자식이란 무엇인가?",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026238.1.23",
          "form": "전 좀 그러고나서 더 소극적이된거같기도해요",
          "original_form": "전 좀 그러고나서 더 소극적이된거같기도해요",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026238.1.24",
          "form": "주변에 결혼한 친구들은",
          "original_form": "주변에 결혼한 친구들은",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026238.1.25",
          "form": "대학때부터 사귀다가 결혼한애들이많아서",
          "original_form": "대학때부터 사귀다가 결혼한애들이많아서",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026238.1.26",
          "form": "비교를 안하도록 노력해보아요",
          "original_form": "비교를 안하도록 노력해보아요",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026238.1.27",
          "form": "사람은 사람때매 힘들죠ㅜ",
          "original_form": "사람은 사람때매 힘들죠ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026238.1.28",
          "form": "키큰사람있으니 작은게 보이듯이",
          "original_form": "키큰사람있으니 작은게 보이듯이",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026238.1.29",
          "form": "아 그런의미에서 이 시국에 남친 생긴 제동생 대단해보이더라고요ㄷㄷ",
          "original_form": "아 그런의미에서 이 시국에 남친 생긴 제동생 대단해보이더라고요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026238.1.30",
          "form": "결혼은 안해도 연애는 삶의 활력을 주지않을까요",
          "original_form": "결혼은 안해도 연애는 삶의 활력을 주지않을까요",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026238.1.31",
          "form": "친구가 채워주지 못하는 다른 유대관계^^",
          "original_form": "친구가 채워주지 못하는 다른 유대관계^^",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026238.1.32",
          "form": "적극적으로 막 나서야 사람도 만나게되는거같아요 ㅋㅋ",
          "original_form": "적극적으로 막 나서야 사람도 만나게되는거같아요 ㅋㅋ",
