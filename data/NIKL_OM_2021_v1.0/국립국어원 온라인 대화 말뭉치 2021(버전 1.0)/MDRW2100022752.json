      "utterance": [
        {
          "id": "MDRW2100022752.1.1",
          "form": "일은 하세요?",
          "original_form": "일은 하세요?",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022752.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022752.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022752.1.4",
          "form": "내 일해여",
          "original_form": "내 일해여",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022752.1.5",
          "form": "일 만족도 어때요",
          "original_form": "일 만족도 어때요",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022752.1.6",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022752.1.7",
          "form": "무슨 직종인데요? 물어봐도되나",
          "original_form": "무슨 직종인데요? 물어봐도되나",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022752.1.8",
          "form": "사무직이오",
          "original_form": "사무직이오",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022752.1.9",
          "form": "오 힘들겠다",
          "original_form": "오 힘들겠다",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022752.1.10",
          "form": "무슨 일이 않그렇겠냐만은",
          "original_form": "무슨 일이 않그렇겠냐만은",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022752.1.11",
          "form": "자기자리가 가장 힘들조 ㅎ",
          "original_form": "자기자리가 가장 힘들조 ㅎ",
          "speaker_id": "1",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022752.1.12",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022752.1.13",
          "form": "저는 부모님이 ㅋㅋ 자리를 꽂아주셔서",
          "original_form": "저는 부모님이 ㅋㅋ 자리를 꽂아주셔서",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022752.1.14",
          "form": "그래도 좀 편하게 다니고있어요",
          "original_form": "그래도 좀 편하게 다니고있어요",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022752.1.15",
          "form": "저도 좀 ㅋㅋ",
          "original_form": "저도 좀 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022752.1.16",
          "form": "근데 힘들어요?",
          "original_form": "근데 힘들어요?",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022752.1.17",
          "form": "님과 함께면 ㅋ",
          "original_form": "님과 함께면 ㅋ",
          "speaker_id": "1",
          "time": "20210829 17:47"
        }
