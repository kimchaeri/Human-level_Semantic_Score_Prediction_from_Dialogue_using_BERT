      "utterance": [
        {
          "id": "MDRW2100017135.1.1",
          "form": "요즘하시는 운동 있으세요",
          "original_form": "요즘하시는 운동 있으세요",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017135.1.2",
          "form": "소소하게 홈트하는 중이에요",
          "original_form": "소소하게 홈트하는 중이에요",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017135.1.3",
          "form": "오 홈트하시는군요",
          "original_form": "오 홈트하시는군요",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017135.1.4",
          "form": "저는 산책이랑 방송댄스용 !! ㅋㅋㅋ",
          "original_form": "저는 산책이랑 방송댄스용 !! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017135.1.5",
          "form": "케이팝댠스",
          "original_form": "케이팝댠스",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017135.1.6",
          "form": "오오 방송댄스!!",
          "original_form": "오오 방송댄스!!",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017135.1.7",
          "form": "저는 몸치라 엄두도 못내는 방송댄스 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 몸치라 엄두도 못내는 방송댄스 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017135.1.8",
          "form": "재밌어요 미루고미루다 등록했어요",
          "original_form": "재밌어요 미루고미루다 등록했어요",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017135.1.9",
          "form": "최근엔 아스트로 애프터 미드나잇 배웠어요",
          "original_form": "최근엔 아스트로 애프터 미드나잇 배웠어요",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017135.1.10",
          "form": "우와 멋잇으시다",
          "original_form": "우와 멋잇으시다",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017135.1.11",
          "form": "재밌습니다",
          "original_form": "재밌습니다",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017135.1.12",
          "form": "매일하세요?",
          "original_form": "매일하세요?",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017135.1.13",
          "form": "아니용 토요일마다 레슨이에요",
          "original_form": "아니용 토요일마다 레슨이에요",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017135.1.14",
          "form": "코로나라서 쌤과 저 친구 3명이서",
          "original_form": "코로나라서 쌤과 저 친구 3명이서",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017135.1.15",
          "form": "친구랑 둘이 수업받으시는구나",
          "original_form": "친구랑 둘이 수업받으시는구나",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017135.1.16",
          "form": "더 신나겟는데요? ㅋㅋ",
          "original_form": "더 신나겟는데요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017135.1.17",
          "form": "네네 그래서 더재믺어요",
          "original_form": "네네 그래서 더재믺어요",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017135.1.18",
          "form": "그럼 토요일엔 레슨받고 주중엔 혼자 연습하세요?",
          "original_form": "그럼 토요일엔 레슨받고 주중엔 혼자 연습하세요?",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017135.1.19",
          "form": "네 그냥 유투브 틀어두고 한번씩 해봐요 ㅋㅋ",
          "original_form": "네 그냥 유투브 틀어두고 한번씩 해봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017135.1.20",
          "form": "아이돌 댄스 격하니까 제대로 운동될것같아요",
          "original_form": "아이돌 댄스 격하니까 제대로 운동될것같아요",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017135.1.21",
          "form": "네 땀 많이나요",
          "original_form": "네 땀 많이나요",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017135.1.22",
          "form": "저도 홈트하다가 질리면 혼자 아이돌댄스라도 따라해볼까봐요 ㅋㅋㅋㅋ",
          "original_form": "저도 홈트하다가 질리면 혼자 아이돌댄스라도 따라해볼까봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:35"
        }
      ]
