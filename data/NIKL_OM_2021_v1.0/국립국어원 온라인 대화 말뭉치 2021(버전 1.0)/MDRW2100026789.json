      "utterance": [
        {
          "id": "MDRW2100026789.1.1",
          "form": "안녕하세요 벌써 12시네요",
          "original_form": "안녕하세요 벌써 12시네요",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026789.1.2",
          "form": "네 반가워요",
          "original_form": "네 반가워요",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026789.1.3",
          "form": "그러게요 벌써 목요일이네요ㅠㅠ 시간 참 빨라요",
          "original_form": "그러게요 벌써 목요일이네요ㅠㅠ 시간 참 빨라요",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026789.1.4",
          "form": "네네 전 방금 교회가서 밤 10시예배 드리고 왔네요ㅎㅎ",
          "original_form": "네네 전 방금 교회가서 밤 10시예배 드리고 왔네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026789.1.5",
          "form": "아 교회 다니세요? 저도 교회 다녀요~",
          "original_form": "아 교회 다니세요? 저도 교회 다녀요~",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026789.1.6",
          "form": "이렇게 같은 교인을 만나네요~^^",
          "original_form": "이렇게 같은 교인을 만나네요~^^",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026789.1.7",
          "form": "우와 반가워요 ㅎㅎ",
          "original_form": "우와 반가워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026789.1.8",
          "form": "밤10시 예배 다녀오셨구나",
          "original_form": "밤10시 예배 다녀오셨구나",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026789.1.9",
          "form": "매주 수요일마다 가시는 거에요?",
          "original_form": "매주 수요일마다 가시는 거에요?",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026789.1.10",
          "form": "네네 매주 수요일마다 가여",
          "original_form": "네네 매주 수요일마다 가여",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026789.1.11",
          "form": "수, 토, 일 이렇게 3일은 예배 참석하는거 같아요~",
          "original_form": "수, 토, 일 이렇게 3일은 예배 참석하는거 같아요~",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026789.1.12",
          "form": "아하 저는 주일예배만 참석해요",
          "original_form": "아하 저는 주일예배만 참석해요",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026789.1.13",
          "form": "주일예배가 가장 중요하죠 너무 좋네요!",
          "original_form": "주일예배가 가장 중요하죠 너무 좋네요!",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026789.1.14",
          "form": "저는 청년부인데 아는 언니들이랑 주일예배 끝나고 같이 놀러가고 그래여",
          "original_form": "저는 청년부인데 아는 언니들이랑 주일예배 끝나고 같이 놀러가고 그래여",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026789.1.15",
          "form": "아 맞아요 전 장년부인데 저도 친해진 언니가 있어서요",
          "original_form": "아 맞아요 전 장년부인데 저도 친해진 언니가 있어서요",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026789.1.16",
          "form": "같이 교제하면서 카페도 가고 연락도 해요ㅎ",
          "original_form": "같이 교제하면서 카페도 가고 연락도 해요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026789.1.17",
          "form": "그러시구나! 같은 성도들과 모임을 통해 교제하는거 넘 좋은거 같아요",
          "original_form": "그러시구나! 같은 성도들과 모임을 통해 교제하는거 넘 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026789.1.18",
          "form": "맞아요 또 교회에서 바자회나 부흥회 같은 행사하는거도 즐겁죠",
          "original_form": "맞아요 또 교회에서 바자회나 부흥회 같은 행사하는거도 즐겁죠",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026789.1.19",
          "form": "공감합니다 그런 활동을 통해서 더욱 가까워지는거 같아용",
          "original_form": "공감합니다 그런 활동을 통해서 더욱 가까워지는거 같아용",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026789.1.20",
          "form": "또 자식들 교회 수련회 보내면 믿음도 키워오고",
          "original_form": "또 자식들 교회 수련회 보내면 믿음도 키워오고",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026789.1.21",
          "form": "부모들은 개인시간도 누리고ㅎㅎ",
          "original_form": "부모들은 개인시간도 누리고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026789.1.22",
          "form": "아하ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026789.1.23",
          "form": "일석이조네요ㅎㅎ",
          "original_form": "일석이조네요ㅎㅎ",
