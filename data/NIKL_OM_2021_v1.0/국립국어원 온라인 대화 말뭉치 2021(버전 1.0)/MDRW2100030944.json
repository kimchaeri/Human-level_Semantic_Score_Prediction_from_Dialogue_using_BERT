      "utterance": [
        {
          "id": "MDRW2100030944.1.1",
          "form": "앗 name1님이요",
          "original_form": "앗 &name1&님이요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030944.1.2",
          "form": "저는 설거지를 해요",
          "original_form": "저는 설거지를 해요",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030944.1.3",
          "form": "오 저는 요리를 해요",
          "original_form": "오 저는 요리를 해요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030944.1.4",
          "form": "요리 대단하시네요",
          "original_form": "요리 대단하시네요",
          "speaker_id": "1",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100030944.1.5",
          "form": "별ㄹ것 아니에요 ㅋㅋ",
          "original_form": "별ㄹ것 아니에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100030944.1.6",
          "form": "요리 어렵지 않나요?",
          "original_form": "요리 어렵지 않나요?",
          "speaker_id": "1",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100030944.1.7",
          "form": "할만하더라구요",
          "original_form": "할만하더라구요",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100030944.1.8",
          "form": "종교 있으신가요",
          "original_form": "종교 있으신가요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030944.1.9",
          "form": "저는 없어요",
          "original_form": "저는 없어요",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030944.1.10",
          "form": "앗 무교시군요",
          "original_form": "앗 무교시군요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030944.1.11",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030944.1.12",
          "form": "저는 교회 다녀요",
          "original_form": "저는 교회 다녀요",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030944.1.13",
          "form": "저도 어렸을 땐 잠깐 다녔어요",
          "original_form": "저도 어렸을 땐 잠깐 다녔어요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030944.1.14",
          "form": "무교인데 다니셨나요?",
          "original_form": "무교인데 다니셨나요?",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030944.1.15",
          "form": "네 부모님이 교회를 다니셔서",
          "original_form": "네 부모님이 교회를 다니셔서",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030944.1.16",
          "form": "오! 그렇군요",
          "original_form": "오! 그렇군요",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030944.1.17",
          "form": "보통 부모님 따라 많이오죠",
          "original_form": "보통 부모님 따라 많이오죠",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030944.1.18",
          "form": "네 근데 커서는 안가게 되더라구요",
          "original_form": "네 근데 커서는 안가게 되더라구요",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030944.1.19",
          "form": "그쵸 그런 분들 꽤 많으시더라고요",
          "original_form": "그쵸 그런 분들 꽤 많으시더라고요",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
