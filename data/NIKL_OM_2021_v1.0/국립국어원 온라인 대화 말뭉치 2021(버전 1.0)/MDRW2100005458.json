      "utterance": [
        {
          "id": "MDRW2100005458.1.1",
          "form": "계단 오르내리기는 어떨까?",
          "original_form": "계단 오르내리기는 어떨까?",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005458.1.2",
          "form": "좋은 운동인데 하지마",
          "original_form": "좋은 운동인데 하지마",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005458.1.3",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005458.1.4",
          "form": "name2 무릎 다친 적 있잖아 ㅠㅠ",
          "original_form": "&name2& 무릎 다친 적 있잖아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005458.1.5",
          "form": "ㅇㅇ 지금은 괜찮은데",
          "original_form": "ㅇㅇ 지금은 괜찮은데",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005458.1.6",
          "form": "무리하다 금방 도져",
          "original_form": "무리하다 금방 도져",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005458.1.7",
          "form": "ㅎㅎ 그래서 요즘은 평지로 다니는데",
          "original_form": "ㅎㅎ 그래서 요즘은 평지로 다니는데",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005458.1.8",
          "form": "그게 무릎이 한 번 아프면 잘 재발해",
          "original_form": "그게 무릎이 한 번 아프면 잘 재발해",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005458.1.9",
          "form": "하아.. 그 때로 다시 돌아가고 싶진 않다",
          "original_form": "하아.. 그 때로 다시 돌아가고 싶진 않다",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005458.1.10",
          "form": "그러니깐..",
          "original_form": "그러니깐..",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005458.1.11",
          "form": "계단 운동이 무릎 안좋은 사람한텐 별로구나",
          "original_form": "계단 운동이 무릎 안좋은 사람한텐 별로구나",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005458.1.12",
          "form": "ㅇㅇ 그냥 경사 완만한 오르막길이나 평지에서 산책해",
          "original_form": "ㅇㅇ 그냥 경사 완만한 오르막길이나 평지에서 산책해",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005458.1.13",
          "form": "ㅎㅎ 알았어",
          "original_form": "ㅎㅎ 알았어",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005458.1.14",
          "form": "아령운동은 어때?",
          "original_form": "아령운동은 어때?",
