      "utterance": [
        {
          "id": "MDRW2100039919.1.1",
          "form": "메뉴에 따라 달라요",
          "original_form": "메뉴에 따라 달라요",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039919.1.2",
          "form": "샌드위치에는 완숙이어야 하고 볶음밥 비빔밥엔 반숙 이런 식?",
          "original_form": "샌드위치에는 완숙이어야 하고 볶음밥 비빔밥엔 반숙 이런 식?",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039919.1.3",
          "form": "줏대가 없으시군요! 양손의 계란은 허용되지 않아욧!",
          "original_form": "줏대가 없으시군요! 양손의 계란은 허용되지 않아욧!",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039919.1.4",
          "form": "삶은계란은 반숙인가요?",
          "original_form": "삶은계란은 반숙인가요?",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039919.1.5",
          "form": "네 반숙란이 좋죠",
          "original_form": "네 반숙란이 좋죠",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039919.1.6",
          "form": "음. 그건 킹정이쥬ㅎ",
          "original_form": "음. 그건 킹정이쥬ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100039919.1.7",
          "form": "감동란 정말 감동이었잖아요ㅎㅎ",
          "original_form": "감동란 정말 감동이었잖아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100039919.1.8",
          "form": "맞아요 소금간도 돼있고",
          "original_form": "맞아요 소금간도 돼있고",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039919.1.9",
          "form": "흐어어 배불러서 누워야겠어여",
          "original_form": "흐어어 배불러서 누워야겠어여",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039919.1.10",
          "form": "벌써 다 드셨어요?",
          "original_form": "벌써 다 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039919.1.11",
          "form": "아니 언제 일케 소리소문없이 해드심!!",
          "original_form": "아니 언제 일케 소리소문없이 해드심!!",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039919.1.12",
          "form": "캬캬컄 이 정도야 껌이져",
          "original_form": "캬캬컄 이 정도야 껌이져",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039919.1.13",
          "form": "후루룩 마시고 왔슴다",
          "original_form": "후루룩 마시고 왔슴다",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039919.1.14",
          "form": "한입만 하려고 했는데ㅜ",
          "original_form": "한입만 하려고 했는데ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039919.1.15",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039919.1.16",
          "form": "남았어요 드쉴?ㅋ",
          "original_form": "남았어요 드쉴?ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039919.1.17",
          "form": "쿵쿵쿵!형 저 병철인데요!!!",
          "original_form": "쿵쿵쿵!형 저 병철인데요!!!",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039919.1.18",
          "form": "어이쿠 쿵철이니~",
          "original_form": "어이쿠 쿵철이니~",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039919.1.19",
          "form": "입장료가 백만원이란더~",
          "original_form": "입장료가 백만원이란더~",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039919.1.20",
          "form": "성수기 해변가인가요",
          "original_form": "성수기 해변가인가요",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039919.1.21",
          "form": "왤케 바가지쥬",
          "original_form": "왤케 바가지쥬",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039919.1.22",
          "form": "아유 해변이라녀! 그런 더운 백사장엘 왜 가나여",
          "original_form": "아유 해변이라녀! 그런 더운 백사장엘 왜 가나여",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039919.1.23",
          "form": "저흰 계곡입니다",
          "original_form": "저흰 계곡입니다",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039919.1.24",
          "form": "계곡 단속 들어갔던데 거긴 신고 안들어갔나요",
          "original_form": "계곡 단속 들어갔던데 거긴 신고 안들어갔나요",
          "speaker_id": "1",
          "time": "20210903 23:31"
        }
      ]
