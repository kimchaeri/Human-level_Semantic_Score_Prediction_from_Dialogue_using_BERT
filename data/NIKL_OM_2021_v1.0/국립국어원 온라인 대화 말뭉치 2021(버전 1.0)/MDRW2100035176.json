      "utterance": [
        {
          "id": "MDRW2100035176.1.1",
          "form": "집에오면 또 현실만족이 들긴한다",
          "original_form": "집에오면 또 현실만족이 들긴한다",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100035176.1.2",
          "form": "그래 맞다 우리끼리 돌아다니면 된다ㅋㅋㅋ",
          "original_form": "그래 맞다 우리끼리 돌아다니면 된다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100035176.1.3",
          "form": "넓은데 가지말고 ㅋㅋㅋㅋ",
          "original_form": "넓은데 가지말고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100035176.1.4",
          "form": "ㅋㅋ웃겨 ㅋㅋ",
          "original_form": "ㅋㅋ웃겨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100035176.1.5",
          "form": "넓게보이기위해ㅋㅋ",
          "original_form": "넓게보이기위해ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100035176.1.6",
          "form": "뭐 조금씩 없애고있다",
          "original_form": "뭐 조금씩 없애고있다",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100035176.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100035176.1.8",
          "form": "나도 늘 그러고 있는데 요즘에 자꾸 늘어난다",
          "original_form": "나도 늘 그러고 있는데 요즘에 자꾸 늘어난다",
          "speaker_id": "2",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100035176.1.9",
          "form": "진짜 애들 옷이 감당이 안 된다",
          "original_form": "진짜 애들 옷이 감당이 안 된다",
          "speaker_id": "2",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100035176.1.10",
          "form": "특히 장난감",
          "original_form": "특히 장난감",
          "speaker_id": "2",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100035176.1.11",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100035176.1.12",
          "form": "나두 장난감ㅡㅡ으ㅋㅋㅋ",
          "original_form": "나두 장난감ㅡㅡ으ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100035176.1.13",
          "form": "버려도 계속 사모으니..",
          "original_form": "버려도 계속 사모으니..",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100035176.1.14",
          "form": "감당이 안된다이;;",
          "original_form": "감당이 안된다이;;",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100035176.1.15",
          "form": "맞다맞다ㅋㅋ",
          "original_form": "맞다맞다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100035176.1.16",
          "form": "내친구는 결국안오기로ㅋㅋ비와서",
          "original_form": "내친구는 결국안오기로ㅋㅋ비와서",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035176.1.17",
          "form": "난 마산왔다요ㅋㅋㅋㅋㅋㅋ여기도 비왔다리 갔다리하네~",
          "original_form": "난 마산왔다요ㅋㅋㅋㅋㅋㅋ여기도 비왔다리 갔다리하네~",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035176.1.18",
          "form": "비안왔음 모래놀이라도 할껀데~",
          "original_form": "비안왔음 모래놀이라도 할껀데~",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035176.1.19",
          "form": "그러게 ㅋ모래놀이할데가있나보네ㅋ",
          "original_form": "그러게 ㅋ모래놀이할데가있나보네ㅋ",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035176.1.20",
          "form": "여기 바다 가깝고 좋더라 ㅋㅋㅋㅋ",
          "original_form": "여기 바다 가깝고 좋더라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035176.1.21",
          "form": "낚시하고 싶었눈데 비와서 패쓰ㅠ",
          "original_form": "낚시하고 싶었눈데 비와서 패쓰ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035176.1.22",
          "form": "니도 낚시좋아하나ㅋㅋ",
          "original_form": "니도 낚시좋아하나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100035176.1.23",
          "form": "울신랑이 환장함 ㅋ",
          "original_form": "울신랑이 환장함 ㅋ",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035176.1.24",
          "form": "나도 name3이 가졌을때 낚시 믾이했는데 ㅋㅋㅋㅋ",
          "original_form": "나도 &name3&이 가졌을때 낚시 믾이했는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035176.1.25",
          "form": "ㅋㅋ 아 그렇구나 취미맞으면 진짜좋긴하겠다ㅋㅋ",
          "original_form": "ㅋㅋ 아 그렇구나 취미맞으면 진짜좋긴하겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100035176.1.26",
          "form": "신랑만 좋아하는줄ㅋㅋ",
          "original_form": "신랑만 좋아하는줄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100035176.1.27",
