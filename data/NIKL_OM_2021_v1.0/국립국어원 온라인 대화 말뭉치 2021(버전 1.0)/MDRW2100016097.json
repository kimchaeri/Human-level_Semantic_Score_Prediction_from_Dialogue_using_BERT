      "utterance": [
        {
          "id": "MDRW2100016097.1.1",
          "form": "아이가 몇명있세요?",
          "original_form": "아이가 몇명있세요?",
          "speaker_id": "1",
          "time": "20210826 09:32"
        },
        {
          "id": "MDRW2100016097.1.2",
          "form": "몇명이에요?",
          "original_form": "몇명이에요?",
          "speaker_id": "1",
          "time": "20210826 09:32"
        },
        {
          "id": "MDRW2100016097.1.3",
          "form": "2명입니다 name1님은요",
          "original_form": "2명입니다 &name1&님은요",
          "speaker_id": "2",
          "time": "20210826 09:45"
        },
        {
          "id": "MDRW2100016097.1.4",
          "form": "저는 아들 하나에요",
          "original_form": "저는 아들 하나에요",
          "speaker_id": "1",
          "time": "20210826 09:45"
        },
        {
          "id": "MDRW2100016097.1.5",
          "form": "애들..몇살인가요",
          "original_form": "애들..몇살인가요",
          "speaker_id": "1",
          "time": "20210826 09:45"
        },
        {
          "id": "MDRW2100016097.1.6",
          "form": "3살 6살 아들 둘이요",
          "original_form": "3살 6살 아들 둘이요",
          "speaker_id": "2",
          "time": "20210826 09:45"
        },
        {
          "id": "MDRW2100016097.1.7",
          "form": "한참 너무이쁠때군요 ㅎ",
          "original_form": "한참 너무이쁠때군요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016097.1.8",
          "form": "아들둘이라 정신없으시죠..",
          "original_form": "아들둘이라 정신없으시죠..",
          "speaker_id": "1",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016097.1.9",
          "form": "네 예쁜데 힘드네요^^ 몇살인가요,",
          "original_form": "네 예쁜데 힘드네요^^ 몇살인가요,",
          "speaker_id": "2",
          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100016097.1.10",
          "form": "11살 남자아이요",
          "original_form": "11살 남자아이요",
          "speaker_id": "1",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100016097.1.11",
          "form": "이제 다컸죠 ㅎ",
          "original_form": "이제 다컸죠 ㅎ",
          "speaker_id": "1",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100016097.1.12",
          "form": "가장힘들때겠어요.. 3살 6살이면.. 남자아이들이 에너지도 넘쳐나니요",
          "original_form": "가장힘들때겠어요.. 3살 6살이면.. 남자아이들이 에너지도 넘쳐나니요",
          "speaker_id": "1",
          "time": "20210826 09:58"
        },
        {
          "id": "MDRW2100016097.1.13",
          "form": "오 11살이요! 선배님이시네요!!",
          "original_form": "오 11살이요! 선배님이시네요!!",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016097.1.14",
          "form": "그렇네요 ㅎ 금방자라는거 같아여",
          "original_form": "그렇네요 ㅎ 금방자라는거 같아여",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016097.1.15",
          "form": "언제이렇게 컸는지요",
          "original_form": "언제이렇게 컸는지요",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016097.1.16",
          "form": "이젠 혼자다알아서 하니...공부에 신경을 많이쓰게되는거같아요",
          "original_form": "이젠 혼자다알아서 하니...공부에 신경을 많이쓰게되는거같아요",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016097.1.17",
          "form": "언제가 가장 힘드셨나요? 지금까지중에?",
          "original_form": "언제가 가장 힘드셨나요? 지금까지중에?",
          "speaker_id": "2",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016097.1.18",
          "form": "어릴때는 몸이힘들구요",
          "original_form": "어릴때는 몸이힘들구요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016097.1.19",
          "form": "커서는 맘이 힘들어요",
          "original_form": "커서는 맘이 힘들어요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016097.1.20",
          "form": "클수록 정신적으로 더 힘이드는거같아요",
          "original_form": "클수록 정신적으로 더 힘이드는거같아요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016097.1.21",
          "form": "하.. 지금도 힘든데 ㅠ 정신적으로도 힘들것도 걱정이네요",
          "original_form": "하.. 지금도 힘든데 ㅠ 정신적으로도 힘들것도 걱정이네요",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016097.1.22",
          "form": "네 달라요 어릴때랑은요..",
          "original_form": "네 달라요 어릴때랑은요..",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016097.1.23",
          "form": "교육때문에요..고민과 선택의 연속이죠",
          "original_form": "교육때문에요..고민과 선택의 연속이죠",
          "speaker_id": "1",
          "time": "20210826 12:22"
        }
      ]
