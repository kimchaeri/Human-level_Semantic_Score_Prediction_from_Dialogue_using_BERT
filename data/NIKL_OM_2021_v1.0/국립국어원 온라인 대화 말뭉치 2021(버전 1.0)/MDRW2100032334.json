      "utterance": [
        {
          "id": "MDRW2100032334.1.1",
          "form": "과자 좋아하세요?",
          "original_form": "과자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032334.1.2",
          "form": "자주 먹진 않지만 좋아해요",
          "original_form": "자주 먹진 않지만 좋아해요",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032334.1.3",
          "form": "화이트하임이 최애신가요",
          "original_form": "화이트하임이 최애신가요",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032334.1.4",
          "form": "놀랍게도 최애는 포카칩입니다",
          "original_form": "놀랍게도 최애는 포카칩입니다",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032334.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032334.1.6",
          "form": "화이트는 한때의 엔조이",
          "original_form": "화이트는 한때의 엔조이",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032334.1.7",
          "form": "엔조잌ㅋㅋㅋ",
          "original_form": "엔조잌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032334.1.8",
          "form": "포카칩맛있져",
          "original_form": "포카칩맛있져",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032334.1.9",
          "form": "칩포테토랑 무뚝뚝도 맛있어요",
          "original_form": "칩포테토랑 무뚝뚝도 맛있어요",
          "speaker_id": "2",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032334.1.10",
          "form": "name1님 최애는요?",
          "original_form": "&name1&님 최애는요?",
          "speaker_id": "2",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032334.1.11",
          "form": "저는 초코들어간거 좋아해요",
          "original_form": "저는 초코들어간거 좋아해요",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032334.1.12",
          "form": "다른것도 다 먹지만ㅎㅎ",
          "original_form": "다른것도 다 먹지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032334.1.13",
          "form": "오, 초코하면 빈츠 팅클 초코송이 다이제",
          "original_form": "오, 초코하면 빈츠 팅클 초코송이 다이제",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032334.1.14",
          "form": "홈런볼 초코칩 등등이 있지요ㅋㅋ",
          "original_form": "홈런볼 초코칩 등등이 있지요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100032334.1.15",
          "form": "견과류 들어간 초콜렛이나 민트초코는 어때요?",
          "original_form": "견과류 들어간 초콜렛이나 민트초코는 어때요?",
          "speaker_id": "2",
          "time": "20210905 17:17"
        },
        {
          "id": "MDRW2100032334.1.16",
          "form": "과자는 아니지만",
          "original_form": "과자는 아니지만",
          "speaker_id": "2",
          "time": "20210905 17:17"
        },
        {
          "id": "MDRW2100032334.1.17",
          "form": "민초는 좋아하는데 견과류는 가끔씩만 먹어요!",
          "original_form": "민초는 좋아하는데 견과류는 가끔씩만 먹어요!",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032334.1.18",
          "form": "민초 좋아하세요?",
          "original_form": "민초 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032334.1.19",
          "form": "민초는 피스타치오 토피넛 녹차에 이어 좋아합니다",
          "original_form": "민초는 피스타치오 토피넛 녹차에 이어 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032334.1.20",
          "form": "최애는 아니지만 먹긴 하는",
          "original_form": "최애는 아니지만 먹긴 하는",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032334.1.21",
          "form": "다 맛있는것들이죠ㅎㅎ",
          "original_form": "다 맛있는것들이죠ㅎㅎ",
