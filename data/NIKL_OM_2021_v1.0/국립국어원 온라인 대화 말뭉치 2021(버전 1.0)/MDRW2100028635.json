      "utterance": [
        {
          "id": "MDRW2100028635.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028635.1.2",
          "form": "점심을 늦게먹어서 아직이요ㅠ",
          "original_form": "점심을 늦게먹어서 아직이요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028635.1.3",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028635.1.4",
          "form": "네 저는 생선구이 먹었어요!",
          "original_form": "네 저는 생선구이 먹었어요!",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028635.1.5",
          "form": "저녁 안드시는건가요?ㅠㅠㅠㅠ",
          "original_form": "저녁 안드시는건가요?ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028635.1.6",
          "form": "그럴리없죠",
          "original_form": "그럴리없죠",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028635.1.7",
          "form": "방금먹었어요",
          "original_form": "방금먹었어요",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028635.1.8",
          "form": "ㅋㄱㅋㅋㅋㄱㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028635.1.9",
          "form": "다행이네요!!!",
          "original_form": "다행이네요!!!",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028635.1.10",
          "form": "뭐드셨어요??!!",
          "original_form": "뭐드셨어요??!!",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028635.1.11",
          "form": "김치찌개 싸악-",
          "original_form": "김치찌개 싸악-",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028635.1.12",
          "form": "했습니다",
          "original_form": "했습니다",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028635.1.13",
          "form": "아 김치찌개ㅠㅠㅠ",
          "original_form": "아 김치찌개ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028635.1.14",
          "form": "지금 아이스크림 먹는데 김치찌개 땡기네요...",
          "original_form": "지금 아이스크림 먹는데 김치찌개 땡기네요...",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028635.1.15",
          "form": "고기나 스팸 들어갔나요...?",
          "original_form": "고기나 스팸 들어갔나요...?",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028635.1.16",
          "form": "당연하죠! 돼지고기 뭉텅뭉텅!",
          "original_form": "당연하죠! 돼지고기 뭉텅뭉텅!",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028635.1.17",
          "form": "아 아이스크림!!",
          "original_form": "아 아이스크림!!",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028635.1.18",
          "form": "어떤거드세요?!!",
          "original_form": "어떤거드세요?!!",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028635.1.19",
          "form": "배라 먹고있어요!!",
          "original_form": "배라 먹고있어요!!",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028635.1.20",
          "form": "배라 종종 드시나요?",
          "original_form": "배라 종종 드시나요?",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028635.1.21",
          "form": "저 뉴치케좋아해요!",
          "original_form": "저 뉴치케좋아해요!",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028635.1.22",
          "form": "사빠딸이랑",
          "original_form": "사빠딸이랑",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028635.1.23",
          "form": "오 저도 사빠딸 제일 좋아해요!!!",
          "original_form": "오 저도 사빠딸 제일 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028635.1.24",
          "form": "원래 뉴치케 좋어했는데 사빠딸 먹어본 후론 사빠딸만 먹고있어요 ㅎㅎ",
          "original_form": "원래 뉴치케 좋어했는데 사빠딸 먹어본 후론 사빠딸만 먹고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028635.1.25",
          "form": "아이스크림 먹고싶네요,,",
          "original_form": "아이스크림 먹고싶네요,,",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028635.1.26",
          "form": "민초좋아하세요?",
          "original_form": "민초좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028635.1.27",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028635.1.28",
          "form": "민초 안먹어요ㅎㅎ",
          "original_form": "민초 안먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:49"
        }
      ]
