      "utterance": [
        {
          "id": "MDRW2100014236.1.1",
          "form": "어떤 일을 하시나용",
          "original_form": "어떤 일을 하시나용",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014236.1.2",
          "form": "저는 그냥 사무직이요ㅎㅎ 어떤일하세요?",
          "original_form": "저는 그냥 사무직이요ㅎㅎ 어떤일하세요?",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014236.1.3",
          "form": "저도 사무직 하고있습니다",
          "original_form": "저도 사무직 하고있습니다",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014236.1.4",
          "form": "곧 퇴근이신가요",
          "original_form": "곧 퇴근이신가요",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014236.1.5",
          "form": "저는 휴가라 집에 있어요ㅋㅋ퇴근하세요?",
          "original_form": "저는 휴가라 집에 있어요ㅋㅋ퇴근하세요?",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014236.1.6",
          "form": "헉 부러워요",
          "original_form": "헉 부러워요",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014236.1.7",
          "form": "네 오분 뒤 퇴근입니다",
          "original_form": "네 오분 뒤 퇴근입니다",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014236.1.8",
          "form": "햅삐..",
          "original_form": "햅삐..",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014236.1.9",
          "form": "아 제일 행복한 시간ㅎㅎㅎ",
          "original_form": "아 제일 행복한 시간ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014236.1.10",
          "form": "오늘 쭉 뭐하셨나요 ㅎㅎ",
          "original_form": "오늘 쭉 뭐하셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014236.1.11",
          "form": "목욕하구 청소하구ㅜㅠ산책도 하고싶었는데 못했네요ㅎㅎ 일 바쁘셨나요?",
          "original_form": "목욕하구 청소하구ㅜㅠ산책도 하고싶었는데 못했네요ㅎㅎ 일 바쁘셨나요?",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014236.1.12",
          "form": "음 일 평소보다 바빴던 것 같아요 ㅠ",
          "original_form": "음 일 평소보다 바빴던 것 같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100014236.1.13",
          "form": "출근하셨나요?",
          "original_form": "출근하셨나요?",
          "speaker_id": "1",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100014236.1.14",
          "form": "네ㅎㅎ 너무 싫으네요",
          "original_form": "네ㅎㅎ 너무 싫으네요",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100014236.1.15",
          "form": "극공감,,,",
          "original_form": "극공감,,,",
          "speaker_id": "1",
          "time": "20210826 09:09"
        },
        {
          "id": "MDRW2100014236.1.16",
          "form": "현 회사 얼마나 다니셨나요",
          "original_form": "현 회사 얼마나 다니셨나요",
          "speaker_id": "1",
