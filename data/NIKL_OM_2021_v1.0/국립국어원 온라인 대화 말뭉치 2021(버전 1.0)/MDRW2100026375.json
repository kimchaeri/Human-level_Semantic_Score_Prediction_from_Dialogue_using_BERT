          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.3",
          "form": "꽤",
          "original_form": "꽤",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.4",
          "form": "올",
          "original_form": "올",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.5",
          "form": "좀 맛있는거같아",
          "original_form": "좀 맛있는거같아",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.6",
          "form": "식당 앞에잇는 그거",
          "original_form": "식당 앞에잇는 그거",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.7",
          "form": "더럽지만 않앗어도",
          "original_form": "더럽지만 않앗어도",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.8",
          "form": "하 ㄹㅇ",
          "original_form": "하 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.9",
          "form": "한움큼 먹었을겨",
          "original_form": "한움큼 먹었을겨",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.10",
          "form": "그거 이제",
          "original_form": "그거 이제",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.11",
          "form": "더러워서 못먹어",
          "original_form": "더러워서 못먹어",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.12",
          "form": "난 아기때 이후로 그거 안먹는다",
          "original_form": "난 아기때 이후로 그거 안먹는다",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026375.1.13",
          "form": "무조건 개별포장된 사탕만 먹어",
          "original_form": "무조건 개별포장된 사탕만 먹어",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026375.1.14",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026375.1.15",
          "form": "난아기땐몰라서",
          "original_form": "난아기땐몰라서",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026375.1.16",
          "form": "개만이먹음",
          "original_form": "개만이먹음",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026375.1.17",
          "form": "ㄴㄷ..",
          "original_form": "ㄴㄷ..",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026375.1.18",
          "form": "그때먹고 안죽어서 다행이다",
          "original_form": "그때먹고 안죽어서 다행이다",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
