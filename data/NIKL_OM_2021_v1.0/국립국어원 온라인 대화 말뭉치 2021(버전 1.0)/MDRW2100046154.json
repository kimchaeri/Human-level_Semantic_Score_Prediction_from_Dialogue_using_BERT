      "utterance": [
        {
          "id": "MDRW2100046154.1.1",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046154.1.2",
          "form": "최근에 사고 싶으신 거 있나요",
          "original_form": "최근에 사고 싶으신 거 있나요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046154.1.3",
          "form": "저 잠옷 좀 사고 싶어요",
          "original_form": "저 잠옷 좀 사고 싶어요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046154.1.4",
          "form": "오 원피스 형이요?",
          "original_form": "오 원피스 형이요?",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046154.1.5",
          "form": "그냥 바지형이요!!",
          "original_form": "그냥 바지형이요!!",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046154.1.6",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046154.1.7",
          "form": "수면바지 같은 보들한 거 보시나요?",
          "original_form": "수면바지 같은 보들한 거 보시나요?",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046154.1.8",
          "form": "넹 수면바지 보들보들하면 좋겠어요",
          "original_form": "넹 수면바지 보들보들하면 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046154.1.9",
          "form": "이제 추워지니까 그런거 장만하기 좋죠",
          "original_form": "이제 추워지니까 그런거 장만하기 좋죠",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046154.1.10",
          "form": "겨울 수면바지 좋은거 사고 싶어영",
          "original_form": "겨울 수면바지 좋은거 사고 싶어영",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046154.1.11",
          "form": "저는 선물 받은 게 좋아서 잘 쓰고 있어요",
          "original_form": "저는 선물 받은 게 좋아서 잘 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046154.1.12",
          "form": "잠옷선물 센스있네요",
          "original_form": "잠옷선물 센스있네요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046154.1.13",
          "form": "넹 미키마우스도 그려져있고 좋아요",
          "original_form": "넹 미키마우스도 그려져있고 좋아요",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046154.1.14",
          "form": "귀엽겠어여 ㅎㅎㅎㅎ",
          "original_form": "귀엽겠어여 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046154.1.15",
          "form": "캐릭터 귀여우니까",
          "original_form": "캐릭터 귀여우니까",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046154.1.16",
          "form": "미키 마우스 좋아하셍ㅅ?",
          "original_form": "미키 마우스 좋아하셍ㅅ?",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046154.1.17",
          "form": "귀여운데 다른 디즈니캐릭터가 더 좋아요 ㅎㅎ",
          "original_form": "귀여운데 다른 디즈니캐릭터가 더 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046154.1.18",
          "form": "어느 거요?",
          "original_form": "어느 거요?",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046154.1.19",
          "form": "그 다람쥐같은 애들도 좋고",
          "original_form": "그 다람쥐같은 애들도 좋고",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046154.1.20",
          "form": "캐릭터 많잖아요 ㅎㅎ",
