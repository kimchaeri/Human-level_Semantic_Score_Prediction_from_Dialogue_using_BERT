      "utterance": [
        {
          "id": "MDRW2100036691.1.1",
          "form": "배달음식은 종종 드세요?",
          "original_form": "배달음식은 종종 드세요?",
          "speaker_id": "2",
          "time": "20210831 09:04"
        },
        {
          "id": "MDRW2100036691.1.2",
          "form": "자주 시켜먹어요",
          "original_form": "자주 시켜먹어요",
          "speaker_id": "1",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036691.1.3",
          "form": "저를 확찐자 만든 일등공신ㅜㅇ",
          "original_form": "저를 확찐자 만든 일등공신ㅜㅇ",
          "speaker_id": "1",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100036691.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100036691.1.5",
          "form": "전 배달음식 잘 안먹어요",
          "original_form": "전 배달음식 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036691.1.6",
          "form": "사실 배달앱도 없어요 ㅋㅋ",
          "original_form": "사실 배달앱도 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100036691.1.7",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "1",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100036691.1.8",
          "form": "대단하시네요ㄷㄷㄷ",
          "original_form": "대단하시네요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210831 09:18"
        },
        {
          "id": "MDRW2100036691.1.9",
          "form": "아니 배달앱 깔면 너무 시켜먹을거 같아서요 ㅋㅋㅋ",
          "original_form": "아니 배달앱 깔면 너무 시켜먹을거 같아서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036691.1.10",
          "form": "현명하시네요ㅜ",
          "original_form": "현명하시네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036691.1.11",
          "form": "제가 그 꼴이 됐죠ㅜ",
          "original_form": "제가 그 꼴이 됐죠ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036691.1.12",
          "form": "근데 그게 할인행사도 많이 한다면서요",
          "original_form": "근데 그게 할인행사도 많이 한다면서요",
          "speaker_id": "2",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036691.1.13",
          "form": "나중에 배민과 요기요에 소송해야 할듯",
          "original_form": "나중에 배민과 요기요에 소송해야 할듯",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036691.1.14",
          "form": "그래서 지금 혹하고 있어요",
          "original_form": "그래서 지금 혹하고 있어요",
          "speaker_id": "2",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036691.1.15",
          "form": "맞아요. 그리고 리뷰이벤트도 짱짱해서",
          "original_form": "맞아요. 그리고 리뷰이벤트도 짱짱해서",
          "speaker_id": "1",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036691.1.16",
          "form": "배달앱 안쓰는 고객 역차별같을 때도ㅜ",
          "original_form": "배달앱 안쓰는 고객 역차별같을 때도ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036691.1.17",
          "form": "리뷰 서비스도 있다죠? 리뷰 잘쓸 자신 있는데",
          "original_form": "리뷰 서비스도 있다죠? 리뷰 잘쓸 자신 있는데",
          "speaker_id": "2",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036691.1.18",
          "form": "저 그냥 테이크아웃하다 배달앱으로  그 가게 봤더니..",
          "original_form": "저 그냥 테이크아웃하다 배달앱으로  그 가게 봤더니..",
          "speaker_id": "1",
          "time": "20210831 09:24"
        },
        {
          "id": "MDRW2100036691.1.19",
          "form": "포장할인에 리뷰이벤트에ㅜ",
          "original_form": "포장할인에 리뷰이벤트에ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:24"
        },
        {
          "id": "MDRW2100036691.1.20",
          "form": "호구된 기분이었어요",
          "original_form": "호구된 기분이었어요",
          "speaker_id": "1",
          "time": "20210831 09:24"
        },
        {
          "id": "MDRW2100036691.1.21",
          "form": "자 앱 깔고 돼지의 세계로 오세용",
          "original_form": "자 앱 깔고 돼지의 세계로 오세용",
          "speaker_id": "1",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100036691.1.22",
          "form": "ㅋㅋㅋㅋ 이미 코로나잏로 10키로 쪘는데 더요?",
          "original_form": "ㅋㅋㅋㅋ 이미 코로나잏로 10키로 쪘는데 더요?",
          "speaker_id": "2",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100036691.1.23",
          "form": "아니ㅋㅋ 배달도 잘 안 시켜드심서",
          "original_form": "아니ㅋㅋ 배달도 잘 안 시켜드심서",
          "speaker_id": "1",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100036691.1.24",
          "form": "요리 손재주가 좋아도 문제네요",
          "original_form": "요리 손재주가 좋아도 문제네요",
          "speaker_id": "1",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100036691.1.25",
          "form": "아.. 배달은 안시키는데.. 에프때문에 냉동식품을 자주 사먹었어요..ㅋㅋ",
          "original_form": "아.. 배달은 안시키는데.. 에프때문에 냉동식품을 자주 사먹었어요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:34"
        },
        {
          "id": "MDRW2100036691.1.26",
          "form": "그리고 집에서 요리 할때 아무래도 제가 좋아하는 고칼로리 음식을 하게 되더라구요",
          "original_form": "그리고 집에서 요리 할때 아무래도 제가 좋아하는 고칼로리 음식을 하게 되더라구요",
          "speaker_id": "2",
          "time": "20210831 09:34"
        }
      ]
