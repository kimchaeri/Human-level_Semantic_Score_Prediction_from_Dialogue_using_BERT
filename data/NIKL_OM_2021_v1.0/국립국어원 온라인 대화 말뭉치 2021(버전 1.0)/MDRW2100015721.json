      "utterance": [
        {
          "id": "MDRW2100015721.1.1",
          "form": "와 저희 무슨 얘기 했었죠?",
          "original_form": "와 저희 무슨 얘기 했었죠?",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015721.1.2",
          "form": "키워드 작성하라는데 기억이...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "키워드 작성하라는데 기억이...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015721.1.3",
          "form": "name1님은 게임 좋아하시나요?",
          "original_form": "&name1&님은 게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015721.1.4",
          "form": "앗ㅋㅋㅋㅋ 저희 과 얘기 하고 있었어요ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ 저희 과 얘기 하고 있었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015721.1.5",
          "form": "저 게임 좋아해요!",
          "original_form": "저 게임 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015721.1.6",
          "form": "무슨 게임 하세요?",
          "original_form": "무슨 게임 하세요?",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015721.1.7",
          "form": "롤하고 쿠키런 하고 있어요! name2 님은 게임 좋아하세요?",
          "original_form": "롤하고 쿠키런 하고 있어요! &name2& 님은 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015721.1.8",
          "form": "저는 잘 못해서 좋아하진 않아요ㅠㅠ",
          "original_form": "저는 잘 못해서 좋아하진 않아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015721.1.9",
          "form": "옛날에... 애니팡이랑 숫자 합쳐서 커지는 게임?",
          "original_form": "옛날에... 애니팡이랑 숫자 합쳐서 커지는 게임?",
          "speaker_id": "2",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015721.1.10",
          "form": "피아노 타일도 했었어요!",
          "original_form": "피아노 타일도 했었어요!",
          "speaker_id": "2",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015721.1.11",
          "form": "오 요즘은 게임 안하시나요?",
          "original_form": "오 요즘은 게임 안하시나요?",
          "speaker_id": "1",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015721.1.12",
          "form": "저도 전엔 피아노 타일 진짜 많이 했는데ㅋㅋㅋ 추억이네요...",
          "original_form": "저도 전엔 피아노 타일 진짜 많이 했는데ㅋㅋㅋ 추억이네요...",
          "speaker_id": "1",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015721.1.13",
          "form": "요즘 게임은 뭔가 하다보면 멀미가 나서",
          "original_form": "요즘 게임은 뭔가 하다보면 멀미가 나서",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100015721.1.14",
          "form": "못하겠어요 ㅋㅋ큐ㅠㅠ",
          "original_form": "못하겠어요 ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100015721.1.15",
          "form": "마지막으로 한 게임이 모두의 마블입니다 ㅋㅋㅋㅋ",
          "original_form": "마지막으로 한 게임이 모두의 마블입니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100015721.1.16",
          "form": "아ㅋㅋㅋㅋ 모마도 재밌었죠... 맨날 파산엔딩이었지만",
          "original_form": "아ㅋㅋㅋㅋ 모마도 재밌었죠... 맨날 파산엔딩이었지만",
          "speaker_id": "1",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100015721.1.17",
          "form": "저도 멀미나는 건 이제 못하겠더라구요",
          "original_form": "저도 멀미나는 건 이제 못하겠더라구요",
          "speaker_id": "1",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100015721.1.18",
          "form": "오버워치하고 앉아서 멀미했을 때 진짜 힘들었거든요ㅋㅋㅋ",
          "original_form": "오버워치하고 앉아서 멀미했을 때 진짜 힘들었거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100015721.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 오버워치 진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 오버워치 진짜",
          "speaker_id": "2",
          "time": "20210826 09:09"
        },
        {
          "id": "MDRW2100015721.1.20",
          "form": "휙휙바뀌고 못하겠어... 이러는데 친구들은 멀미난다는 걸 이해 못하더라구요 ",
          "original_form": "휙휙바뀌고 못하겠어... 이러는데 친구들은 멀미난다는 걸 이해 못하더라구요 ",
          "speaker_id": "2",
          "time": "20210826 09:09"
        },
        {
          "id": "MDRW2100015721.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ 전 하고 나서 어지러워서 핸드폰 떨어트리고 액정 깨트린 적도 있어요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 전 하고 나서 어지러워서 핸드폰 떨어트리고 액정 깨트린 적도 있어요...",
          "speaker_id": "1",
          "time": "20210826 09:11"
        },
        {
          "id": "MDRW2100015721.1.22",
          "form": "근데 롤은 멀미는 안나더라구요ㅋㅋㅋ",
          "original_form": "근데 롤은 멀미는 안나더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:11"
        },
        {
          "id": "MDRW2100015721.1.23",
          "form": "아 그렇군요! 롤은 어떤 게임이에요?",
          "original_form": "아 그렇군요! 롤은 어떤 게임이에요?",
          "speaker_id": "2",
          "time": "20210826 09:12"
        },
        {
          "id": "MDRW2100015721.1.24",
          "form": "롤은 fps 인데 3인칭 시점이어서 좋더라구요 상대 포탑을 깨서 최종 포탑을 깨면 이기는 게임이에요!",
