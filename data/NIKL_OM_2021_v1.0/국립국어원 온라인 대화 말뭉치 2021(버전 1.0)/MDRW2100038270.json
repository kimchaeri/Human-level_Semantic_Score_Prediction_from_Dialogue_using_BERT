        },
        {
          "id": "MDRW2100038270.1.12",
          "form": "전 선선한 공기랑 경치가 좋더라구요",
          "original_form": "전 선선한 공기랑 경치가 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 13:12"
