      "utterance": [
        {
          "id": "MDRW2100005198.1.1",
          "form": "혹시 싸이월드 하셨었나요?",
          "original_form": "혹시 싸이월드 하셨었나요?",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005198.1.2",
          "form": "싸이월드 하셨나요?",
          "original_form": "싸이월드 하셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005198.1.3",
          "form": "저는 그세대는 아니지만 조금은 했었어요",
          "original_form": "저는 그세대는 아니지만 조금은 했었어요",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005198.1.4",
          "form": "아하 저는 싸이하던 세대라 잠깐이지만 하긴 했어요",
          "original_form": "아하{emoji:…} 저는 싸이하던 세대라 잠깐이지만 하긴 했어요",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005198.1.5",
          "form": "그럼 부활될 흑역사는 없으시겠어요 부럽다",
          "original_form": "그럼 부활될 흑역사는 없으시겠어요 부럽다",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005198.1.6",
          "form": "싸이월드 감성 흑역사 있으세요?",
          "original_form": "싸이월드 감성 흑역사 있으세요?",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005198.1.7",
          "form": "감성은 이니고 중2 흑역사는 있네요",
          "original_form": "감성은 이니고 중2 흑역사는 있네요",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005198.1.8",
          "form": "저는 짧게했는데도 살짝 흑역사가 있습니다...",
          "original_form": "저는 짧게했는데도 살짝 흑역사가 있습니다...",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005198.1.9",
          "form": "ㅎㅎㅎ 싸이에 흑역사없는사람 드물거예요",
          "original_form": "ㅎㅎㅎ 싸이에 흑역사없는사람 드물거예요",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005198.1.10",
          "form": "울프컷하고 나팔바지 입고",
          "original_form": "울프컷하고 나팔바지 입고",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005198.1.11",
          "form": "감성글 올리고...",
          "original_form": "감성글 올리고...",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005198.1.12",
          "form": "ㅋㅋㅋ 저도 감성글 막 올리고",
          "original_form": "ㅋㅋㅋ 저도 감성글 막 올리고",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005198.1.13",
          "form": "일촌평 되도않는거 막 남기고",
          "original_form": "일촌평 되도않는거 막 남기고",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005198.1.14",
          "form": "그때 그 글들이 부활하면",
          "original_form": "그때 그 글들이 부활하면",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005198.1.15",
          "form": "반가울까요 아니면 끔찍할까요",
          "original_form": "반가울까요 아니면 끔찍할까요",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005198.1.16",
          "form": "아우 진짜 안돼요 불살라야합니다",
          "original_form": "아우 진짜 안돼요 불살라야합니다",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005198.1.17",
          "form": "기억속에서나 추억이지 실제로 대면하면 싫을거같아요",
          "original_form": "기억속에서나 추억이지 실제로 대면하면 싫을거같아요",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005198.1.18",
          "form": "근데 재밌을것같긴 해요",
          "original_form": "근데 재밌을것같긴 해요",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005198.1.19",
          "form": "틀었는데 sg 워너비 노래랑",
          "original_form": "틀었는데 sg 워너비 노래랑",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005198.1.20",
          "form": "씨야노래 나오고",
          "original_form": "씨야노래 나오고",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005198.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005198.1.22",
          "form": "씨야 이름 엄청 오랜만에들어요",
          "original_form": "씨야 이름 엄청 오랜만에들어요",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005198.1.23",
          "form": "저는 배경음악 뭐했는지 기억도안나네요",
          "original_form": "저는 배경음악 뭐했는지 기억도안나네요",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005198.1.24",
          "form": "그때 노래들도 뭔가 다 좋았는데",
          "original_form": "그때 노래들도 뭔가 다 좋았는데",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005198.1.25",
          "form": "배치기 unfade",
          "original_form": "배치기 unfade",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005198.1.26",
          "form": "엠씨스나이퍼 bklove",
          "original_form": "엠씨스나이퍼 bklove",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005198.1.27",
          "form": "지금들으면 그렇게 좋지는 않은데",
          "original_form": "지금들으면 그렇게 좋지는 않은데",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005198.1.28",
          "form": "그땐 왜그리 좋았는지",
          "original_form": "그땐 왜그리 좋았는지",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005198.1.29",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005198.1.30",
          "form": "근데 또 들으면 그때 기억같은게나서 마음으로는 좋게들려요 머리로는 썩 아닌거알지만",
          "original_form": "근데 또 들으면 그때 기억같은게나서 마음으로는 좋게들려요 머리로는 썩 아닌거알지만",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005198.1.31",
          "form": "심장이 두근두근하죠",
          "original_form": "심장이 두근두근하죠",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005198.1.32",
          "form": "부정맥인지 뭔지는 몰라도",
          "original_form": "부정맥인지 뭔지는 몰라도",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005198.1.33",
          "form": "가을바람 불어올때 들으면 진짜",
          "original_form": "가을바람 불어올때 들으면 진짜",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005198.1.34",
          "form": "ㅋㅋㅋ 맞아요 그때기분이 다시 드는데 그땐 그냥 집걱정 노후걱정 안해도됐어서 그런지",
          "original_form": "ㅋㅋㅋ 맞아요 그때기분이 다시 드는데 그땐 그냥{emoji:…} 집걱정 노후걱정 안해도됐어서 그런지",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005198.1.35",
          "form": "그때는 그냥 멋지게 차려입고 여자나 꼬실 생각만 했죠 뭐",
          "original_form": "그때는 그냥 멋지게 차려입고 여자나 꼬실 생각만 했죠 뭐",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005198.1.36",
          "form": "지금보면 완전 촌스러운데",
          "original_form": "지금보면 완전 촌스러운데",
