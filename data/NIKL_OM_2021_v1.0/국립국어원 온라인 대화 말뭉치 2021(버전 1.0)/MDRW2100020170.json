      "utterance": [
        {
          "id": "MDRW2100020170.1.1",
          "form": "name3 언제보러가시나요",
          "original_form": "&name3& 언제보러가시나요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020170.1.2",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020170.1.3",
          "form": "아까 영통했는데 보곱",
          "original_form": "아까 영통했는데 보곱",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020170.1.4",
          "form": "name4 모해요",
          "original_form": "&name4& 모해요",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020170.1.5",
          "form": "안봐도 알것같지않나요",
          "original_form": "안봐도 알것같지않나요",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020170.1.6",
          "form": "누워있을걸요?",
          "original_form": "누워있을걸요?",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020170.1.7",
          "form": "ㅋㅋㅋㄲ",
          "original_form": "ㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020170.1.8",
          "form": "같이 없어요?",
          "original_form": "같이 없어요?",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020170.1.9",
          "form": "저는 방에있고 name4 거실에있어여",
          "original_form": "저는 방에있고 &name4& 거실에있어여",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020170.1.10",
          "form": "오늘 산책했나요",
          "original_form": "오늘 산책했나요",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020170.1.11",
          "form": "비도안왔는데",
          "original_form": "비도안왔는데",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020170.1.12",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020170.1.13",
          "form": "오늘산책당번저임",
          "original_form": "오늘산책당번저임",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020170.1.14",
          "form": "밤에갓다왓는데",
          "original_form": "밤에갓다왓는데",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020170.1.15",
          "form": "습한거없이 날씨가 좋더라구요",
          "original_form": "습한거없이 날씨가 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020170.1.16",
          "form": "ㅋㅋㅋ오늘 날씨좋더라고요",
          "original_form": "ㅋㅋㅋ오늘 날씨좋더라고요",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020170.1.17",
          "form": "name4 안 귀찮아했음요?",
          "original_form": "&name4& 안 귀찮아했음요?",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020170.1.18",
          "form": "출퇴근길엔 어땠나여",
          "original_form": "출퇴근길엔 어땠나여",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020170.1.19",
          "form": "name4 나가기싫어서",
          "original_form": "&name4& 나가기싫어서",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020170.1.20",
          "form": "버티는거 보고 엄마가 엉덩이 밀어서",
          "original_form": "버티는거 보고 엄마가 엉덩이 밀어서",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020170.1.21",
          "form": "산책갔다온거임 ㅋㅋㅋㅋ",
          "original_form": "산책갔다온거임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020170.1.22",
          "form": "ㅋㅋㅋㅋㅋ역시 name4",
          "original_form": "ㅋㅋㅋㅋㅋ역시 &name4&",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020170.1.23",
          "form": "출퇴근길",
          "original_form": "출퇴근길",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020170.1.24",
          "form": "전 널널했는데",
          "original_form": "전 널널했는데",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020170.1.25",
          "form": "name5은 죽을라함",
          "original_form": "&name5&은 죽을라함",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020170.1.26",
          "form": "둘이 거의 비슷한열차 타지않나요",
          "original_form": "둘이 거의 비슷한열차 타지않나요",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020170.1.27",
          "form": "저희 name4도 회사 데려가고싶네요",
          "original_form": "저희 &name4&도 회사 데려가고싶네요",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020170.1.28",
          "form": "데려와도 되지않나요",
          "original_form": "데려와도 되지않나요",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020170.1.29",
          "form": "옆사무실도 냥이 잇는데",
          "original_form": "옆사무실도 냥이 잇는데",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020170.1.30",
          "form": "그 좁아터진 출근길에",
          "original_form": "그 좁아터진 출근길에",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020170.1.31",
          "form": "name4를 어떻게 데려가요",
          "original_form": "&name4&를 어떻게 데려가요",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020170.1.32",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020170.1.33",
          "form": "name4 가방없죠?",
          "original_form": "&name4& 가방없죠?",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020170.1.34",
          "form": "네 글고 개무거워요!",
          "original_form": "네 글고 개무거워요!",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020170.1.35",
          "form": "그냥8키로도 아니고 움직이는8키로잖아요",
          "original_form": "그냥8키로도 아니고 움직이는8키로잖아요",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020170.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020170.1.37",
          "form": "가방에 있음 얌전하겠조",
          "original_form": "가방에 있음 얌전하겠조",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020170.1.38",
          "form": "이참에 하나사욥",
          "original_form": "이참에 하나사욥",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020170.1.39",
          "form": "근데 강아지 가방도 무거운거 하시죠",
          "original_form": "근데 강아지 가방도 무거운거 하시죠",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020170.1.40",
          "form": "아마 출퇴근이 고달파질것같은데",
          "original_form": "아마 출퇴근이 고달파질것같은데",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020170.1.41",
          "form": "그럼 어쩔수없죠",
          "original_form": "그럼 어쩔수없죠",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020170.1.42",
          "form": "name4 실물 언제 봄ㅋㅋ",
          "original_form": "&name4& 실물 언제 봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020170.1.43",
          "form": "나중에 판교오세요",
          "original_form": "나중에 판교오세요",
