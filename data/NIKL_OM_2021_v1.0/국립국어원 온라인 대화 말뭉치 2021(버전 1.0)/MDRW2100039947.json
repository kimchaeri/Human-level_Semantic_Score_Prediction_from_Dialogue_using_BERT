      "utterance": [
        {
          "id": "MDRW2100039947.1.1",
          "form": "아 넷플에 요알못들이 하는 요리프로 있는거 아세요??",
          "original_form": "아 넷플에 요알못들이 하는 요리프로 있는거 아세요??",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039947.1.2",
          "form": "그런게 있나요ㅡ ? 그럼 뒤죽박죽 요리인가요?",
          "original_form": "그런게 있나요ㅡ ? 그럼 뒤죽박죽 요리인가요?",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100039947.1.3",
          "form": "미션음식 보여주고",
          "original_form": "미션음식 보여주고",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039947.1.4",
          "form": "레시피도 알려주고",
          "original_form": "레시피도 알려주고",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039947.1.5",
          "form": "재료까지 주는데 참가자들이 멋대로 만들어요",
          "original_form": "재료까지 주는데 참가자들이 멋대로 만들어요",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039947.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039947.1.7",
          "form": "앜ㅋㅋ 초짜들 모임인거죠",
          "original_form": "앜ㅋㅋ 초짜들 모임인거죠",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039947.1.8",
          "form": "개판 예상",
          "original_form": "개판 예상",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039947.1.9",
          "form": "제목이 뭘까요",
          "original_form": "제목이 뭘까요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039947.1.10",
          "form": "ㅋㅋㅋㅋnailed it  이건거 같아요",
          "original_form": "ㅋㅋㅋㅋnailed it  이건거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039947.1.11",
          "form": "일단 과정도 웃기고",
          "original_form": "일단 과정도 웃기고",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039947.1.12",
          "form": "결과물도 웃겨요",
          "original_form": "결과물도 웃겨요",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039947.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039947.1.14",
          "form": "심지어 베이킹이잖아요???",
          "original_form": "심지어 베이킹이잖아요???",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039947.1.15",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039947.1.16",
          "form": "그건 진짜 잘지켜야하던데 ㅋㅋㅋㅋ",
          "original_form": "그건 진짜 잘지켜야하던데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039947.1.17",
          "form": "심지어 디저트라는 사실ㅋㅋㅋㅋ",
          "original_form": "심지어 디저트라는 사실ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039947.1.18",
          "form": "결과물이 장관이에요ㅋㅋㅋㅋ",
          "original_form": "결과물이 장관이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039947.1.19",
          "form": "한국에서 그거하면 시청률 대박예상 ㅋㅋㅋ",
          "original_form": "한국에서 그거하면 시청률 대박예상 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039947.1.20",
          "form": "크림 올려야되는데 다 흘러내리고",
          "original_form": "크림 올려야되는데 다 흘러내리고",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039947.1.21",
          "form": "아니 똥손 파틴데요 ㅋㅋㅋㅋ",
          "original_form": "아니 똥손 파틴데요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039947.1.22",
          "form": "분명 필요한 만큼 베이킹했는데",
          "original_form": "분명 필요한 만큼 베이킹했는데",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039947.1.23",
          "form": "빵 한쪽이 증발해버렸다거나",
          "original_form": "빵 한쪽이 증발해버렸다거나",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039947.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ재미있어요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ재미있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039947.1.25",
          "form": "와 저 넷플 결제하면 저거부터 봐야겠어요",
          "original_form": "와 저 넷플 결제하면 저거부터 봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039947.1.26",
          "form": "편수가 많길바라며 ㅋㅋㅋ",
          "original_form": "편수가 많길바라며 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039947.1.27",
          "form": "캡쳐만 봐도 웃겨요",
          "original_form": "캡쳐만 봐도 웃겨요",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039947.1.28",
          "form": "종종 밥먹을때 틀어놓는데 재미있어요ㅋㅋㅋ",
          "original_form": "종종 밥먹을때 틀어놓는데 재미있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:37"
        }
      ]
