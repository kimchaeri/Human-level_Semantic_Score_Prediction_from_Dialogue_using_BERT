        {
          "id": "MDRW2100009315.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009315.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009315.1.3",
          "form": "어떤 장르요?",
          "original_form": "어떤 장르요?",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009315.1.4",
          "form": "로맨틱코미디요",
          "original_form": "로맨틱코미디요",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009315.1.5",
          "form": "오 로코..",
          "original_form": "오 로코..",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009315.1.6",
          "form": "최고 ㅠ",
          "original_form": "최고 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009315.1.7",
          "form": "어떤 작품 재밌게 보셨어요?",
          "original_form": "어떤 작품 재밌게 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009315.1.8",
          "form": "공효진 김래원나온거 이름뭐죵..",
          "original_form": "공효진 김래원나온거 이름뭐죵..",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009315.1.9",
          "form": "그거 잼나게 봤는뎅 ㅎ",
          "original_form": "그거 잼나게 봤는뎅 ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009315.1.10",
          "form": "아 그거 그냥 유튜브로 요약해준거 잠깐 봤었는데",
          "original_form": "아 그거 그냥 유튜브로 요약해준거 잠깐 봤었는데",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009315.1.11",
          "form": "봐보세용 잼나요",
          "original_form": "봐보세용 잼나요",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009315.1.12",
          "form": "네 봐볼게요",
          "original_form": "네 봐볼게요",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009315.1.13",
          "form": "연극이나 뮤지컬도 보러가세여?",
          "original_form": "연극이나 뮤지컬도 보러가세여?",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009315.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009315.1.15",
          "form": "그건 요즘 코로나 시국이라 끝나면 보러가려구요",
          "original_form": "그건 요즘 코로나 시국이라 끝나면 보러가려구요",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009315.1.16",
          "form": "아이유 콘서트 꼭 가고싶어요",
          "original_form": "아이유 콘서트 꼭 가고싶어요",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009315.1.17",
          "form": "오 좋다 저도 가고싶어용",
          "original_form": "오 좋다 저도 가고싶어용",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009315.1.18",
          "form": "박효신 콘서트 가고싶어요 전",
          "original_form": "박효신 콘서트 가고싶어요 전",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009315.1.19",
          "form": "아아 박효신 인기 진짜 많죠 \r\n팬들 사이에서 대장님이라고 불린다고 들었는데ㅋㅋ",
