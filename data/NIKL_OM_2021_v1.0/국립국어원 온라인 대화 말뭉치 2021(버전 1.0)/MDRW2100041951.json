      "utterance": [
        {
          "id": "MDRW2100041951.1.1",
          "form": "넥슨인턴 자격 구경하러갔다가 울면서 나왔잖아요",
          "original_form": "넥슨인턴 자격 구경하러갔다가 울면서 나왔잖아요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041951.1.2",
          "form": "엥 왜요??많이 빡세요??",
          "original_form": "엥 왜요??많이 빡세요??",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041951.1.3",
          "form": "메이플관련 포트폴리오를 쓰래서 대체 뭔가 싶었어요",
          "original_form": "메이플관련 포트폴리오를 쓰래서 대체 뭔가 싶었어요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041951.1.4",
          "form": "와 그 정도로 구체적이에요?",
          "original_form": "와 그 정도로 구체적이에요?",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041951.1.5",
          "form": "기획쪽 한번봤는데 영..  그쪽으로는 경험이 없기도했고ㅋㅋㅋ",
          "original_form": "기획쪽 한번봤는데 영..  그쪽으로는 경험이 없기도했고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041951.1.6",
          "form": "진짜 본격적이던데요",
          "original_form": "진짜 본격적이던데요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041951.1.7",
          "form": "채용인턴?? 이런거라서",
          "original_form": "채용인턴?? 이런거라서",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041951.1.8",
          "form": "헐.... 맨날 욕하는 기업인데",
          "original_form": "헐.... 맨날 욕하는 기업인데",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041951.1.9",
          "form": "들어가려면 그림의떡이네요...",
          "original_form": "들어가려면 그림의떡이네요...",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041951.1.10",
          "form": "자소서 하나 쓴다는 느낌으로 신청해볼랬는데 바로포기했어요ㅋㅋㅋㅋ",
          "original_form": "자소서 하나 쓴다는 느낌으로 신청해볼랬는데 바로포기했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041951.1.11",
          "form": "심지어 인턴인데 과거경력?\r\n회사적는 칸도있더라구요\r\n다들 경력자뽑으면 신입은...???",
          "original_form": "심지어 인턴인데 과거경력?\r\n회사적는 칸도있더라구요\r\n다들 경력자뽑으면 신입은...???",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041951.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 거기도 결국 경력직 신입이군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 거기도 결국 경력직 신입이군요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041951.1.13",
          "form": "에휴 그니까요",
          "original_form": "에휴 그니까요",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041951.1.14",
          "form": "유니온 8천은 됐는데",
          "original_form": "유니온 8천은 됐는데",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041951.1.15",
          "form": "250이상도 됐는데!",
          "original_form": "250이상도 됐는데!",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041951.1.16",
          "form": "그니까요 유니온팔천 250아크 이거 적으면되나 생각했는데",
          "original_form": "그니까요 유니온팔천 250아크 이거 적으면되나 생각했는데",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041951.1.17",
          "form": "이 정도면 ㅋㅋ 경력인데 아쉽네요",
          "original_form": "이 정도면 ㅋㅋ 경력인데 아쉽네요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041951.1.18",
          "form": "게임사 포폴은 뭐가 희안해서 모르겠더라구요",
          "original_form": "게임사 포폴은 뭐가 희안해서 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041951.1.19",
          "form": "오호.....글쿤요 ㅜㅜ",
          "original_form": "오호.....글쿤요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041951.1.20",
          "form": "메이플 스펙 솔직히 살수도없는거라 큰거가지고있긴 한건데ㅋㅋㅋㅋㅋ",
          "original_form": "메이플 스펙 솔직히 살수도없는거라 큰거가지고있긴 한건데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041951.1.21",
          "form": "넥슨이면 저도 좀 흥미있는데 ㅋㅋ 절 받아주질 않겠군요",
          "original_form": "넥슨이면 저도 좀 흥미있는데 ㅋㅋ 절 받아주질 않겠군요",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041951.1.22",
          "form": "포폴이... 뭐야.. 상태라 흑흑",
          "original_form": "포폴이... 뭐야.. 상태라 흑흑",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041951.1.23",
          "form": "넥슨에게 나라는 오점을 남기고싶었는데",
          "original_form": "넥슨에게 나라는 오점을 남기고싶었는데",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041951.1.24",
          "form": "ㅜㅜ..... 쌓다보면 나중엔 넥슨에서 절하면서 받아줄 때가 오겠죠!",
          "original_form": "ㅜㅜ..... 쌓다보면 나중엔 넥슨에서 절하면서 받아줄 때가 오겠죠!",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041951.1.25",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041951.1.26",
          "form": "만약 입사하게 되시면",
          "original_form": "만약 입사하게 되시면",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041951.1.27",
