      "utterance": [
        {
          "id": "MDRW2100043232.1.1",
          "form": "돼지고기랑 소고기중에 뭐가 더 좋으세요?",
          "original_form": "돼지고기랑 소고기중에 뭐가 더 좋으세요?",
          "speaker_id": "1",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043232.1.2",
          "form": "음 저는 돼지고기요!",
          "original_form": "음 저는 돼지고기요!",
          "speaker_id": "2",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043232.1.3",
          "form": "이유가 있으신가요?",
          "original_form": "이유가 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043232.1.4",
          "form": "조금더 고소한거 같아요",
          "original_form": "조금더 고소한거 같아요",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043232.1.5",
          "form": "아 확실히 소고기는 참기름 찍어먹어야 더 맛있죠!",
          "original_form": "아 확실히 소고기는 참기름 찍어먹어야 더 맛있죠!",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043232.1.6",
          "form": "그리고 조금 더 기름져서..?ㅋㅋ",
          "original_form": "그리고 조금 더 기름져서..?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043232.1.7",
          "form": "맞아요ㅠㅠ 돼지고기기름에 김치 구워먹으면 진짜 맛있는데..",
          "original_form": "맞아요ㅠㅠ 돼지고기기름에 김치 구워먹으면 진짜 맛있는데..",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043232.1.8",
          "form": "맞아요 콩나물도 마늘도..",
          "original_form": "맞아요 콩나물도 마늘도..",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043232.1.9",
          "form": "저는 둘 다 좋아하는데",
          "original_form": "저는 둘 다 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043232.1.10",
          "form": "저도 둘 다 좋아하긴 해욬ㅋㅋ",
          "original_form": "저도 둘 다 좋아하긴 해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043232.1.11",
          "form": "소고기를 더 자주 못먹어서...비싸서...소고기가 더 좋아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "소고기를 더 자주 못먹어서...비싸서...소고기가 더 좋아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043232.1.12",
          "form": "소고기가 비싸죠...ㅋㅋ 소고기는 소고기대로 또 맛이 있어요",
          "original_form": "소고기가 비싸죠...ㅋㅋ 소고기는 소고기대로 또 맛이 있어요",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043232.1.13",
          "form": "돼지는 좀 더 만만한거 같긴해요",
          "original_form": "돼지는 좀 더 만만한거 같긴해요",
          "speaker_id": "1",
          "time": "20210907 16:21"
        }
