        },
        {
          "id": "MDRW2100000335.1.2",
          "form": "난 이제",
          "original_form": "난 이제",
          "speaker_id": "1",
          "time": "20210819 07:24"
        },
        {
          "id": "MDRW2100000335.1.3",
          "form": "뭐고 애도 안낳을거면서",
          "original_form": "뭐고 애도 안낳을거면서",
          "speaker_id": "2",
          "time": "20210819 07:24"
        },
        {
          "id": "MDRW2100000335.1.4",
          "form": "난 씻으러간다",
          "original_form": "난 씻으러간다",
          "speaker_id": "2",
          "time": "20210819 07:25"
        },
        {
          "id": "MDRW2100000335.1.5",
          "form": "난 출근하고 똥쌌다",
          "original_form": "난 출근하고 똥쌌다",
          "speaker_id": "1",
          "time": "20210819 08:09"
        },
        {
          "id": "MDRW2100000335.1.6",
          "form": "나 요새 경품재테크함",
          "original_form": "나 요새 경품재테크함",
          "speaker_id": "1",
          "time": "20210819 09:08"
        },
        {
          "id": "MDRW2100000335.1.7",
          "form": "어질어질하고만",
          "original_form": "어질어질하고만",
          "speaker_id": "2",
          "time": "20210819 09:09"
        },
        {
          "id": "MDRW2100000335.1.8",
          "form": "어제 최저점 손절 타격이 너무 크다",
          "original_form": "어제 최저점 손절 타격이 너무 크다",
          "speaker_id": "2",
          "time": "20210819 09:09"
        },
        {
          "id": "MDRW2100000335.1.9",
          "form": "와이프는 일 다니나?",
          "original_form": "와이프는 일 다니나?",
          "speaker_id": "2",
          "time": "20210819 09:10"
        },
        {
          "id": "MDRW2100000335.1.10",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210819 09:18"
        },
        {
          "id": "MDRW2100000335.1.11",
          "form": "집에서 쉬고있어",
          "original_form": "집에서 쉬고있어",
          "speaker_id": "1",
          "time": "20210819 09:18"
        },
        {
          "id": "MDRW2100000335.1.12",
          "form": "으흠",
          "original_form": "으흠",
          "speaker_id": "2",
          "time": "20210819 09:18"
        },
        {
          "id": "MDRW2100000335.1.13",
          "form": "외롭겠군",
          "original_form": "외롭겠군",
          "speaker_id": "2",
          "time": "20210819 09:18"
        },
        {
          "id": "MDRW2100000335.1.14",
          "form": "돈 무한 삭제 중이네",
          "original_form": "돈 무한 삭제 중이네",
          "speaker_id": "2",
          "time": "20210819 09:19"
        },
        {
          "id": "MDRW2100000335.1.15",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210819 09:19"
        },
        {
          "id": "MDRW2100000335.1.16",
          "form": "아 주식도 폭망이고",
          "original_form": "아 주식도 폭망이고",
          "speaker_id": "1",
          "time": "20210819 09:19"
        },
        {
          "id": "MDRW2100000335.1.17",
          "form": "오늘 아침에 괜히 다시 샀네",
          "original_form": "오늘 아침에 괜히 다시 샀네",
          "speaker_id": "2",
          "time": "20210819 09:19"
        },
        {
          "id": "MDRW2100000335.1.18",
          "form": "여태 순수익이야? 순손실이야?",
          "original_form": "여태 순수익이야? 순손실이야?",
          "speaker_id": "1",
          "time": "20210819 09:20"
        },
        {
          "id": "MDRW2100000335.1.19",
          "form": "순손실이지",
          "original_form": "순손실이지",
          "speaker_id": "2",
          "time": "20210819 09:20"
        },
        {
          "id": "MDRW2100000335.1.20",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210819 09:21"
        },
        {
          "id": "MDRW2100000335.1.21",
          "form": "결정을 너무 늦게하거나 빨리하거나",
          "original_form": "결정을 너무 늦게하거나 빨리하거나",
          "speaker_id": "2",
          "time": "20210819 09:21"
        },
        {
          "id": "MDRW2100000335.1.22",
          "form": "타이밍 정말 최악",
          "original_form": "타이밍 정말 최악",
          "speaker_id": "2",
          "time": "20210819 09:22"
        },
        {
          "id": "MDRW2100000335.1.23",
          "form": "진짜 최악적이다",
          "original_form": "진짜 최악적이다",
          "speaker_id": "1",
          "time": "20210819 09:23"
        },
        {
          "id": "MDRW2100000335.1.24",
          "form": "이번에는 매도 좀만 늦게 했어도 3-400 벌었을듯",
          "original_form": "이번에는 매도 좀만 늦게 했어도 3-400 벌었을듯",
          "speaker_id": "2",
          "time": "20210819 09:23"
        },
        {
          "id": "MDRW2100000335.1.25",
          "form": "난 운행나간다",
          "original_form": "난 운행나간다",
          "speaker_id": "2",
          "time": "20210819 09:24"
        },
        {
          "id": "MDRW2100000335.1.26",
          "form": "감바레",
          "original_form": "감바레",
          "speaker_id": "1",
