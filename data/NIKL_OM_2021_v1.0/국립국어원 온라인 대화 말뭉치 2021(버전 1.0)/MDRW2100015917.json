      "utterance": [
        {
          "id": "MDRW2100015917.1.1",
          "form": "화장품 좋아하시나용?",
          "original_form": "화장품 좋아하시나용?",
          "speaker_id": "1",
          "time": "20210826 02:55"
        },
        {
          "id": "MDRW2100015917.1.2",
          "form": "아 저는 화장 별로 안하긴해요ㅠㅠ!",
          "original_form": "아 저는 화장 별로 안하긴해요ㅠㅠ!",
          "speaker_id": "2",
          "time": "20210826 02:55"
        },
        {
          "id": "MDRW2100015917.1.3",
          "form": "그나마 립 발랐는데",
          "original_form": "그나마 립 발랐는데",
          "speaker_id": "2",
          "time": "20210826 02:55"
        },
        {
          "id": "MDRW2100015917.1.4",
          "form": "코로나라 그것도 아예 안바르고 다녀요",
          "original_form": "코로나라 그것도 아예 안바르고 다녀요",
          "speaker_id": "2",
          "time": "20210826 02:55"
        },
        {
          "id": "MDRW2100015917.1.5",
          "form": "요즘 마스크써서 안하시는분들이 많긴 한것같아요ㅎㅎ",
          "original_form": "요즘 마스크써서 안하시는분들이 많긴 한것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.6",
          "form": "저는 화장품 넘 좋아해요!",
          "original_form": "저는 화장품 넘 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.7",
          "form": "모으는 취미있어요",
          "original_form": "모으는 취미있어요",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.8",
          "form": "오 그런데 화장한거랑 안한거랑",
          "original_form": "오 그런데 화장한거랑 안한거랑",
          "speaker_id": "2",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.9",
          "form": "많이 달라진다고 느끼세요??",
          "original_form": "많이 달라진다고 느끼세요??",
          "speaker_id": "2",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.10",
          "form": "요즘 퍼스널컬러다 뭐다해서",
          "original_form": "요즘 퍼스널컬러다 뭐다해서",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.11",
          "form": "그거 한번 진단받아봤는데",
          "original_form": "그거 한번 진단받아봤는데",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.12",
          "form": "아 마자요",
          "original_form": "아 마자요",
          "speaker_id": "2",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015917.1.14",
          "form": "저는 색조를 진하게하면",
          "original_form": "저는 색조를 진하게하면",
          "speaker_id": "1",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015917.1.15",
          "form": "안어울린다고 하네요...ㅠㅠㅠ",
          "original_form": "안어울린다고 하네요...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015917.1.16",
          "form": "혹시 여쿨..??",
          "original_form": "혹시 여쿨..??",
          "speaker_id": "2",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015917.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015917.1.18",
          "form": "마자용",
          "original_form": "마자용",
          "speaker_id": "1",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015917.1.19",
          "form": "여름쿨 라이트 톤이라고 하더라구요",
          "original_form": "여름쿨 라이트 톤이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015917.1.20",
          "form": "저두요!!!",
          "original_form": "저두요!!!",
          "speaker_id": "2",
          "time": "20210826 02:58"
        },
        {
          "id": "MDRW2100015917.1.21",
          "form": "헉 신기해요ㅋㅋㅋ 반가워요!!",
          "original_form": "헉 신기해요ㅋㅋㅋ 반가워요!!",
          "speaker_id": "1",
          "time": "20210826 02:58"
        },
        {
          "id": "MDRW2100015917.1.22",
          "form": "저도 퍼컬 때문인지 화장 했을 때 차이가 그렇게 큰것 같지 않아서 못하겠어요ㅋㅋㅜ",
          "original_form": "저도 퍼컬 때문인지 화장 했을 때 차이가 그렇게 큰것 같지 않아서 못하겠어요ㅋㅋㅜ",
          "speaker_id": "2",
          "time": "20210826 02:59"
        },
        {
          "id": "MDRW2100015917.1.23",
          "form": "이 톤은 쌩얼이랑 크게 차이가 없는것같아요ㅋㅋㅋ",
          "original_form": "이 톤은 쌩얼이랑 크게 차이가 없는것같아요ㅋㅋㅋ",
          "speaker_id": "1",
