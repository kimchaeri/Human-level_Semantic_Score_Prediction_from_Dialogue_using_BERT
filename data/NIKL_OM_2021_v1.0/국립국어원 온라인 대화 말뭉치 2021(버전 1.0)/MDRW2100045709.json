      "utterance": [
        {
          "id": "MDRW2100045709.1.1",
          "form": "최근에 여행 가신 곳 있으신가요?",
          "original_form": "최근에 여행 가신 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045709.1.2",
          "form": "여행 자주다니시나요?",
          "original_form": "여행 자주다니시나요?",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045709.1.3",
          "form": "아뇨 코로나 시작하고 아무데도 안가요 ㅋㅋㅋ",
          "original_form": "아뇨 코로나 시작하고 아무데도 안가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045709.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045709.1.5",
          "form": "저도 코로나 시작하곤 간곳이 없어요",
          "original_form": "저도 코로나 시작하곤 간곳이 없어요",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045709.1.6",
          "form": "이번 추석때도 어떻게 해야할지 모르겠네요",
          "original_form": "이번 추석때도 어떻게 해야할지 모르겠네요",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045709.1.7",
          "form": "계획있으신기요?",
          "original_form": "계획있으신기요?",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045709.1.8",
          "form": "아마 집에 있을것 같아요 저희가 가면 거기 주민분들도 위험할수 있으니까요",
          "original_form": "아마 집에 있을것 같아요 저희가 가면 거기 주민분들도 위험할수 있으니까요",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045709.1.9",
          "form": "특히나 조부모님 연세도 있으셔서 찾아가는게",
          "original_form": "특히나 조부모님 연세도 있으셔서 찾아가는게",
          "speaker_id": "2",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045709.1.10",
          "form": "오히려 안좋을것같아서요",
          "original_form": "오히려 안좋을것같아서요",
          "speaker_id": "2",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045709.1.11",
          "form": "맞아요. 저는 지방에 내려가야해서 그냥 이번주에 부모님만 찾아뵐까 생각중이에요.",
          "original_form": "맞아요. 저는 지방에 내려가야해서 그냥 이번주에 부모님만 찾아뵐까 생각중이에요.",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045709.1.12",
          "form": "기차에도 사람이 많아서..",
          "original_form": "기차에도 사람이 많아서..",
          "speaker_id": "1",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045709.1.13",
          "form": "그러게요 참 어떻게해야하나 어려워요",
          "original_form": "그러게요 참 어떻게해야하나 어려워요",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045709.1.14",
          "form": "저도 집에만 있고싶은데 부모님이 서운해하셔서 어쩔수가 없네요",
          "original_form": "저도 집에만 있고싶은데 부모님이 서운해하셔서 어쩔수가 없네요",
          "speaker_id": "1",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100045709.1.15",
          "form": "ㅠㅠ그럼 가야죠 방법이 있나요 ㅠ",
          "original_form": "ㅠㅠ그럼 가야죠 방법이 있나요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045709.1.16",
          "form": "그러게요. 사실 추석 당일엔 안가려고 추석예매기간도 일부러 예매를 안했네요",
          "original_form": "그러게요. 사실 추석 당일엔 안가려고 추석예매기간도 일부러 예매를 안했네요",
          "speaker_id": "1",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045709.1.17",
          "form": "name2님은 부모님과 함께 사시나요?",
          "original_form": "&name2&님은 부모님과 함께 사시나요?",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045709.1.18",
          "form": "네 저는 함께 살고 있어요",
          "original_form": "네 저는 함께 살고 있어요",
          "speaker_id": "2",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045709.1.19",
          "form": "방역 꼼꼼히 하시면 별 일 없을거에요",
          "original_form": "방역 꼼꼼히 하시면 별 일 없을거에요",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045709.1.20",
          "form": "너무 걱정말고 다녀오세요",
          "original_form": "너무 걱정말고 다녀오세요",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045709.1.21",
          "form": "네 감사합니다 ㅎㅎ",
          "original_form": "네 감사합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045709.1.22",
          "form": "추석기간 이동안하셔도 돼서 부럽습니다",
          "original_form": "추석기간 이동안하셔도 돼서 부럽습니다",
          "speaker_id": "1",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045709.1.23",
          "form": "그러네요 ㅎㅎ 님은 콧바람이라도 쐰다 생각하세요 이왕가는거 걱정보단 기분 좋게 가면 좋잖아요",
          "original_form": "그러네요 ㅎㅎ 님은 콧바람이라도 쐰다 생각하세요 이왕가는거 걱정보단 기분 좋게 가면 좋잖아요",
          "speaker_id": "2",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100045709.1.24",
          "form": "네 그렇게 생각하려구요. 좋은말씀 감사합니다",
          "original_form": "네 그렇게 생각하려구요. 좋은말씀 감사합니다",
          "speaker_id": "1",
          "time": "20210908 06:03"
        }
      ]
    }
