      "utterance": [
        {
          "id": "MDRW2100030665.1.1",
          "form": "대신동에 성당 앞에",
          "original_form": "대신동에 성당 앞에",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.2",
          "form": "카페 가면 커피 만드시나요",
          "original_form": "카페 가면 커피 만드시나요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.3",
          "form": "홍차왕자라는 홍차 전문 음료 점이 있었는데",
          "original_form": "홍차왕자라는 홍차 전문 음료 점이 있었는데",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.4",
          "form": "홍차왕자 알지요",
          "original_form": "홍차왕자 알지요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.5",
          "form": "아직도 있나요",
          "original_form": "아직도 있나요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.6",
          "form": "지금은 문을 열지 않더라고요",
          "original_form": "지금은 문을 열지 않더라고요",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.7",
          "form": "굉장히 오래 되었지요",
          "original_form": "굉장히 오래 되었지요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.8",
          "form": "거기에서 맛있는 홍차 많이 마셨는데 안타까워요",
          "original_form": "거기에서 맛있는 홍차 많이 마셨는데 안타까워요",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.9",
          "form": "20년 정도 되었겠네요",
          "original_form": "20년 정도 되었겠네요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030665.1.10",
          "form": "코로나 유로 문을 잘 열지 않는 것 같아요",
          "original_form": "코로나 유로 문을 잘 열지 않는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030665.1.11",
          "form": "거기가 맛이 좋았나요",
          "original_form": "거기가 맛이 좋았나요",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030665.1.12",
          "form": "예전에 잘 나갈 때는 2호점도 있었는데 말이죠",
          "original_form": "예전에 잘 나갈 때는 2호점도 있었는데 말이죠",
          "speaker_id": "2",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030665.1.13",
          "form": "최근에 닫았나 봅니다",
          "original_form": "최근에 닫았나 봅니다",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030665.1.14",
          "form": "2호점은 어디에 있었어요",
          "original_form": "2호점은 어디에 있었어요",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030665.1.15",
          "form": "많은 홍차의 종류가 있어서 편리하겠지요",
          "original_form": "많은 홍차의 종류가 있어서 편리하겠지요",
          "speaker_id": "2",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030665.1.16",
          "form": "홍차 전문점 이었으니까요",
          "original_form": "홍차 전문점 이었으니까요",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030665.1.17",
          "form": "동대신동 지하철역 라인으로 가다 보면 있었어요",
          "original_form": "동대신동 지하철역 라인으로 가다 보면 있었어요",
          "speaker_id": "2",
          "time": "20210904 11:09"
        }
