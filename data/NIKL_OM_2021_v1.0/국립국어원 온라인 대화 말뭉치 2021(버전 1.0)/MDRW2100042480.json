      "utterance": [
        {
          "id": "MDRW2100042480.1.1",
          "form": "오늘 근데 비가 너무 많이와서",
          "original_form": "오늘 근데 비가 너무 많이와서",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042480.1.2",
          "form": "쌀국수 생각이 솔솔나요..!",
          "original_form": "쌀국수 생각이 솔솔나요..!",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042480.1.3",
          "form": "name1님 사시는 곳도 비 많이 오세요??",
          "original_form": "&name1&님 사시는 곳도 비 많이 오세요??",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042480.1.4",
          "form": "저는 잔치국수 먹고 싶네요!!",
          "original_form": "저는 잔치국수 먹고 싶네요!!",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042480.1.5",
          "form": "전국적으로 많이 오는 비인가 싶기도 한데 이번주에 계속 온대요",
          "original_form": "전국적으로 많이 오는 비인가 싶기도 한데 이번주에 계속 온대요",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042480.1.6",
          "form": "헐 잔지국수도 맛있을거같아요!",
          "original_form": "헐 잔지국수도 맛있을거같아요!",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042480.1.7",
          "form": "완전 비 많이오고",
          "original_form": "완전 비 많이오고",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042480.1.8",
          "form": "바람도 많이 불어요 ㅠㅠ",
          "original_form": "바람도 많이 불어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042480.1.9",
          "form": "저희 동네도 비가 잘 안오는 곳인데 많이 오고 있어요",
          "original_form": "저희 동네도 비가 잘 안오는 곳인데 많이 오고 있어요",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042480.1.10",
          "form": "저는 비와서 전 부쳐먹었어요ㅋㅋㅋ",
          "original_form": "저는 비와서 전 부쳐먹었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042480.1.11",
          "form": "헐 전..",
          "original_form": "헐 전..",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042480.1.12",
          "form": "저도 내일 전해볼까 갑자기 고민도ㅑ요",
          "original_form": "저도 내일 전해볼까 갑자기 고민도ㅑ요",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042480.1.13",
          "form": "재료는 지난번에 비왔을때 다 구비해놧거든요!",
          "original_form": "재료는 지난번에 비왔을때 다 구비해놧거든요!",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042480.1.14",
          "form": "역시 비올땐..",
          "original_form": "역시 비올땐..",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042480.1.15",
          "form": "전인가요..",
          "original_form": "전인가요..",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042480.1.16",
          "form": "부추전 먹었는데 맛있어요",
          "original_form": "부추전 먹었는데 맛있어요",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042480.1.17",
          "form": "네 기름이 끝내줍니다",
          "original_form": "네 기름이 끝내줍니다",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042480.1.18",
          "form": "비가 오니까 전이 좋네여",
          "original_form": "비가 오니까 전이 좋네여",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042480.1.19",
          "form": "오 부추전 저도 마침 재료가 구비되어이ㅛ슴다..!",
          "original_form": "오 부추전 저도 마침 재료가 구비되어이ㅛ슴다..!",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042480.1.20",
          "form": "내일 긴팔 입고 나가실건가요?",
          "original_form": "내일 긴팔 입고 나가실건가요?",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042480.1.21",
          "form": "저 얇은긴팔!!",
          "original_form": "저 얇은긴팔!!",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042480.1.22",
          "form": "오늘도 긴팔이었어요!",
          "original_form": "오늘도 긴팔이었어요!",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042480.1.23",
          "form": "오 진짜요??",
          "original_form": "오 진짜요??",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042480.1.24",
          "form": "저는 반팔이엏숨다..!",
          "original_form": "저는 반팔이엏숨다..!",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042480.1.25",
          "form": "근데 오늘 비 오고 나면 쌀쌀해질거같아서",
          "original_form": "근데 오늘 비 오고 나면 쌀쌀해질거같아서",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042480.1.26",
          "form": "비오고나면 좀 추워지니까!!",
          "original_form": "비오고나면 좀 추워지니까!!",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042480.1.27",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042480.1.28",
          "form": "고민니에요!!",
          "original_form": "고민니에요!!",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042480.1.29",
          "form": "그거져!!",
          "original_form": "그거져!!",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042480.1.30",
          "form": "저는 원래 추위를 타서",
          "original_form": "저는 원래 추위를 타서",
