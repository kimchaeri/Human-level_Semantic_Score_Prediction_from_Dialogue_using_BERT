      "utterance": [
        {
          "id": "MDRW2100028306.1.1",
          "form": "결혼에이어ㅋㅋㅋㄱ",
          "original_form": "결혼에이어ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028306.1.2",
          "form": "날씨좋은가요그쪽은",
          "original_form": "날씨좋은가요그쪽은",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028306.1.3",
          "form": "네 ㅎ 아주 맑은 가을하늘요",
          "original_form": "네 ㅎ 아주 맑은 가을하늘요",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100028306.1.4",
          "form": "거긴요",
          "original_form": "거긴요",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100028306.1.5",
          "form": "흐렸다맑았다해요ㅎㅎ",
          "original_form": "흐렸다맑았다해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100028306.1.6",
          "form": "내일뭐입고나가야할지고민입니당",
          "original_form": "내일뭐입고나가야할지고민입니당",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100028306.1.7",
          "form": "요즘은 아래위둘가 반팔으누안되요",
          "original_form": "요즘은 아래위둘가 반팔으누안되요",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100028306.1.8",
          "form": "추워죽어요ㅠㅋㄴ",
          "original_form": "추워죽어요ㅠㅋㄴ",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100028306.1.9",
          "form": "그러게요ㅋㅋㅋㄱ 오늘도 쌀쌀하네요ㅜㅜ",
          "original_form": "그러게요ㅋㅋㅋㄱ 오늘도 쌀쌀하네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100028306.1.10",
          "form": "이제 아침밤으로는 꽤 쌀쌀해요 ㅎ 정말 가을인가봐요",
          "original_form": "이제 아침밤으로는 꽤 쌀쌀해요 ㅎ 정말 가을인가봐요",
          "speaker_id": "2",
          "time": "20210904 05:53"
        },
        {
          "id": "MDRW2100028306.1.11",
          "form": "네 가을이 된것같아요 가을와서 너무 좋아요ㅎㅎ",
          "original_form": "네 가을이 된것같아요 가을와서 너무 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100028306.1.12",
          "form": "맞아요ㅠ날이 너무 좋죠",
          "original_form": "맞아요ㅠ날이 너무 좋죠",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100028306.1.13",
          "form": "네네 이제좀나갈마음이듭니다ㄲㅋㅋ",
          "original_form": "네네 이제좀나갈마음이듭니다ㄲㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100028306.1.14",
          "form": "맞아요 ㅎ 이번여름 너무 더웠습니다",
          "original_form": "맞아요 ㅎ 이번여름 너무 더웠습니다",
          "speaker_id": "2",
          "time": "20210904 11:41"
        },
        {
          "id": "MDRW2100028306.1.15",
          "form": "네 징합니다 자꾸 여름이 길어지니까ㅜㅜ흑흑",
          "original_form": "네 징합니다 자꾸 여름이 길어지니까ㅜㅜ흑흑",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100028306.1.16",
          "form": "아직은반팔입으시지여",
          "original_form": "아직은반팔입으시지여",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100028306.1.17",
          "form": "전 긴팔에 반바지를 입게되더라구요 ㅜ",
          "original_form": "전 긴팔에 반바지를 입게되더라구요 ㅜ",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100028306.1.18",
          "form": "저녁에 들어오면 추워서",
          "original_form": "저녁에 들어오면 추워서",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100028306.1.19",
          "form": "아!  긴팔에 반바지조합이시네요!",
          "original_form": "아!  긴팔에 반바지조합이시네요!",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100028306.1.20",
          "form": "저는 반팔에긴바지조합인데",
          "original_form": "저는 반팔에긴바지조합인데",
          "speaker_id": "1",
          "time": "20210904 14:23"
        }
      ]
