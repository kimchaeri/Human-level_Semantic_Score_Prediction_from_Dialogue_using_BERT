      "utterance": [
        {
          "id": "MDRW2100015000.1.1",
          "form": "오 뭐먹어",
          "original_form": "오 뭐먹어",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015000.1.2",
          "form": "넘 늦게 먹는거 아니여",
          "original_form": "넘 늦게 먹는거 아니여",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015000.1.3",
          "form": "강의 듣고 먹느라 그래도 맛있음 냠냠",
          "original_form": "강의 듣고 먹느라 그래도 맛있음 냠냠",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015000.1.4",
          "form": "청년다방 떡볶이 벌써 또 아른거림",
          "original_form": "청년다방 떡볶이 벌써 또 아른거림",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015000.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015000.1.6",
          "form": "그만 좀 먹어야되는데",
          "original_form": "그만 좀 먹어야되는데",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015000.1.7",
          "form": "먹을거 최고 오늘 삼겹살 시킬지 진짜 고민 많이했다 ㅋㅋㅋ",
          "original_form": "먹을거 최고 오늘 삼겹살 시킬지 진짜 고민 많이했다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015000.1.8",
          "form": "결국 음료시키느라 걍 아침겸 점심은 대충 때움",
          "original_form": "결국 음료시키느라 걍 아침겸 점심은 대충 때움",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015000.1.9",
          "form": "다음에 벌집 삼겹살 먹으러 가기",
          "original_form": "다음에 벌집 삼겹살 먹으러 가기",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015000.1.10",
          "form": "바닐라라떼 근데 어디거 시켜먹음",
          "original_form": "바닐라라떼 근데 어디거 시켜먹음",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015000.1.11",
          "form": "그 일반 프랜차이즈는 아니었음",
          "original_form": "그 일반 프랜차이즈는 아니었음",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015000.1.12",
          "form": "평 괜찮고 최소 주문금액이랑 배송비 괜찮아서 시켰어",
          "original_form": "평 괜찮고 최소 주문금액이랑 배송비 괜찮아서 시켰어",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015000.1.13",
          "form": "그때 먹은 마크브라운 바닐라라떼도 괜찮은듯",
          "original_form": "그때 먹은 마크브라운 바닐라라떼도 괜찮은듯",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015000.1.14",
          "form": "오 마크브라운 언제 먹었지?",
          "original_form": "오 마크브라운 언제 먹었지?",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015000.1.15",
          "form": "님 졸업식때요",
          "original_form": "님 졸업식때요",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015000.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015000.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015000.1.18",
          "form": "나 시킨곳 시럽쓰던데",
          "original_form": "나 시킨곳 시럽쓰던데",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015000.1.19",
          "form": "막 엄청 시럽맛 안나고 맛있더라",
          "original_form": "막 엄청 시럽맛 안나고 맛있더라",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015000.1.20",
          "form": "거기 카페 이름 뭐야",
          "original_form": "거기 카페 이름 뭐야",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015000.1.21",
          "form": "볼트커피",
          "original_form": "볼트커피",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015000.1.22",
          "form": "오 본 듯",
          "original_form": "오 본 듯",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015000.1.23",
