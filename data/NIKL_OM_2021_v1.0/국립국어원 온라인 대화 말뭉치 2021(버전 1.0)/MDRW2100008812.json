      "utterance": [
        {
          "id": "MDRW2100008812.1.1",
          "form": "무슨 북경오리??",
          "original_form": "무슨 북경오리??",
          "speaker_id": "1",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008812.1.2",
          "form": "ㄱ것도 이상했고",
          "original_form": "ㄱ것도 이상했고",
          "speaker_id": "1",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008812.1.3",
          "form": "길거리에는 무슨 전갈 같은 걸 꼬치로 팔고",
          "original_form": "길거리에는 무슨 전갈 같은 걸 꼬치로 팔고",
          "speaker_id": "1",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008812.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 벌레꼬치들 궁금했는데 그거 현지인들도 안먹는거라면서요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 벌레꼬치들 궁금했는데 그거 현지인들도 안먹는거라면서요",
          "speaker_id": "2",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008812.1.5",
          "form": "관광객들만먹는다는..",
          "original_form": "관광객들만먹는다는..",
          "speaker_id": "2",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008812.1.6",
          "form": "헐~ ㅠ 지낮 그런 거 왜 해놓는지...",
          "original_form": "헐~ ㅠ 지낮 그런 거 왜 해놓는지...",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100008812.1.7",
          "form": "긍까요 그게 관광상품인가봐여 ㅋㅋㅋㅋㅋ",
          "original_form": "긍까요 그게 관광상품인가봐여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100008812.1.8",
          "form": "중국화장실은 어땠어요? 북경이라서 별차이가없었을려나요",
          "original_form": "중국화장실은 어땠어요? 북경이라서 별차이가없었을려나요",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100008812.1.9",
          "form": "인터넷에서 본 화장실은 충격과공포던데",
          "original_form": "인터넷에서 본 화장실은 충격과공포던데",
          "speaker_id": "2",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008812.1.10",
          "form": "와ㅜ 진짜 너무 충격적이었어요",
          "original_form": "와ㅜ 진짜 너무 충격적이었어요",
          "speaker_id": "1",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100008812.1.11",
          "form": "화장실 문 다 열려있고",
          "original_form": "화장실 문 다 열려있고",
          "speaker_id": "1",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100008812.1.12",
          "form": "그 상태로 다 새치기 하면서 들어가고",
          "original_form": "그 상태로 다 새치기 하면서 들어가고",
          "speaker_id": "1",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100008812.1.13",
          "form": "저도 들어가려고 어디 앞에 섰난데",
          "original_form": "저도 들어가려고 어디 앞에 섰난데",
          "speaker_id": "1",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100008812.1.14",
          "form": "그 칸 안에 어떤 아주머니께서",
          "original_form": "그 칸 안에 어떤 아주머니께서",
          "speaker_id": "1",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100008812.1.15",
          "form": "큰 거... 보고 계시는데 그게 다 보여서",
          "original_form": "큰 거... 보고 계시는데 그게 다 보여서",
          "speaker_id": "1",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100008812.1.16",
          "form": "너무 충격이었어요ㅋㅋㅋㅋ ㅠㅠㅠ",
          "original_form": "너무 충격이었어요ㅋㅋㅋㅋ ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100008812.1.17",
          "form": "으악 북경에도 그렇군여",
          "original_form": "으악 북경에도 그렇군여",
          "speaker_id": "2",
          "time": "20210823 13:09"
        },
        {
          "id": "MDRW2100008812.1.18",
          "form": "시골만 그런줄알았는데",
          "original_form": "시골만 그런줄알았는데",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100008812.1.19",
          "form": "네ㅠ 충격적이었어요,,,",
          "original_form": "네ㅠ 충격적이었어요,,,",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100008812.1.20",
          "form": "간다면...변비걸릴것같아여....",
          "original_form": "간다면...변비걸릴것같아여....",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100008812.1.21",
          "form": "푸세식이런데도 사용할수있지만 그래도문은 달려있는데...넘민망할것같네요ㅜㅜ",
          "original_form": "푸세식이런데도 사용할수있지만 그래도문은 달려있는데...넘민망할것같네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100008812.1.22",
          "form": "윽 그러니까요ㅠ",
          "original_form": "윽 그러니까요ㅠ",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100008812.1.23",
          "form": "호텔 아닌 이상 화장실 가기 점 그래서",
          "original_form": "호텔 아닌 이상 화장실 가기 점 그래서",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100008812.1.24",
          "form": "참느라 혼났어요",
          "original_form": "참느라 혼났어요",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100008812.1.25",
          "form": "ㅜㅜ중국가게되면 진짜 화장실생각하고 가야겠어요",
          "original_form": "ㅜㅜ중국가게되면 진짜 화장실생각하고 가야겠어요",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100008812.1.26",
          "form": "북경갔을때  어디가 제일 인상깊으셨어요? 관광지나",
          "original_form": "북경갔을때  어디가 제일 인상깊으셨어요? 관광지나",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100008812.1.27",
          "form": "아무래도 만리장성?",
          "original_form": "아무래도 만리장성?",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100008812.1.28",
          "form": "걷는 게 힘들긴 했는데",
          "original_form": "걷는 게 힘들긴 했는데",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100008812.1.29",
          "form": "멋지더라고요",
          "original_form": "멋지더라고요",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100008812.1.30",
          "form": "오 만리장성 매번교과서로만 보던거 직접보면 멋있을것같아요",
          "original_form": "오 만리장성 매번교과서로만 보던거 직접보면 멋있을것같아요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100008812.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 15:42"
        }
      ]
