      "utterance": [
        {
          "id": "MDRW2100034292.1.1",
          "form": "운동은 해도 식단은 못하겠어요 저는 ㅠ",
          "original_form": "운동은 해도 식단은 못하겠어요 저는 ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100034292.1.2",
          "form": "저도에요ㅋㅋㅋ",
          "original_form": "저도에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100034292.1.3",
          "form": "오오 ㅋㅋ같네요",
          "original_form": "오오 ㅋㅋ같네요",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.4",
          "form": "마자요ㅋㅋㅈ",
          "original_form": "마자요ㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.5",
          "form": "보통 식단은 해도 운동 못하겟단",
          "original_form": "보통 식단은 해도 운동 못하겟단",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.6",
          "form": "저는  식탐이 많아가지구",
          "original_form": "저는  식탐이 많아가지구",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.7",
          "form": "분들이 많더라구여 ..",
          "original_form": "분들이 많더라구여 ..",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.8",
          "form": "저두요 ㅠㅠㅠ",
          "original_form": "저두요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.9",
          "form": "이해가 안가는군요",
          "original_form": "이해가 안가는군요",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.10",
          "form": "먹는 낙이 얼마나큰데ㅠ",
          "original_form": "먹는 낙이 얼마나큰데ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.11",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.12",
          "form": "먹는거 빼니까 어찌나 짜증이 나던지..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹는거 빼니까 어찌나 짜증이 나던지..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034292.1.13",
          "form": "마자요 이렇게 먹으면 안행복한데",
          "original_form": "마자요 이렇게 먹으면 안행복한데",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100034292.1.14",
          "form": "이렇게까지 해야하나싶고ㅋㅋ",
          "original_form": "이렇게까지 해야하나싶고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100034292.1.15",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100034292.1.16",
          "form": "그래놓고 다음날 몸무게재고 후회해여 먹으면 ㅋㅋㅋ",
          "original_form": "그래놓고 다음날 몸무게재고 후회해여 먹으면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100034292.1.17",
          "form": "맛있는거먹고 엄청 배부르게만 안먹으면 그래도 괜찮지 않을까  싶어요",
