      "utterance": [
        {
          "id": "MDRW2100045133.1.1",
          "form": "요즘 침착맨 넘 웃기더라구요",
          "original_form": "요즘 침착맨 넘 웃기더라구요",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045133.1.2",
          "form": "저는 생방주로봐요",
          "original_form": "저는 생방주로봐요",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045133.1.3",
          "form": "저는 생방도 보고 유투브도요",
          "original_form": "저는 생방도 보고 유투브도요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045133.1.4",
          "form": "유투브 넘 웃긴 편집 ㅋㅋ",
          "original_form": "유투브 넘 웃긴 편집 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045133.1.5",
          "form": "그거는봤어요 심윤수편",
          "original_form": "그거는봤어요 심윤수편",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045133.1.6",
          "form": "그 고양이 좋아하시는분 맞나요 ㅋㅋ",
          "original_form": "그 고양이 좋아하시는분 맞나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045133.1.7",
          "form": "네 캔따게그분 ㅋㅋ",
          "original_form": "네 캔따게그분 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045133.1.8",
          "form": "저도 고양이 조아해서 이해가 가요 ㅋㅋㅋ",
          "original_form": "저도 고양이 조아해서 이해가 가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045133.1.9",
          "form": "또 나오면 잼쓸듯요",
          "original_form": "또 나오면 잼쓸듯요",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045133.1.10",
          "form": "고양이 광공체질 ㅋㅋ",
          "original_form": "고양이 광공체질 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045133.1.11",
          "form": "고양이가 세상의 전부같은 ㅎㅎ",
          "original_form": "고양이가 세상의 전부같은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045133.1.12",
          "form": "갑자기 그분 만나보고 싶네요",
          "original_form": "갑자기 그분 만나보고 싶네요",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045133.1.13",
          "form": "좋아하시니 대화 잘통하실듯 ㅋㅋ",
          "original_form": "좋아하시니 대화 잘통하실듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045133.1.14",
          "form": "통조림행은 안될듯요 ㅎ",
          "original_form": "통조림행은 안될듯요 ㅎ",
