      "utterance": [
        {
          "id": "MDRW2100016193.1.1",
          "form": "무소음이 무소음이 아닌가요",
          "original_form": "무소음이 무소음이 아닌가요",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016193.1.2",
          "form": "날개가 돌아가는데 소리가 아예 안난다는거 자체가 말이 안되는거 같달까",
          "original_form": "날개가 돌아가는데 소리가 아예 안난다는거 자체가 말이 안되는거 같달까",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016193.1.3",
          "form": "음 모순적이네요",
          "original_form": "음 모순적이네요",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016193.1.4",
          "form": "그냥 딱 사전적의미로 생각을때요",
          "original_form": "그냥 딱 사전적의미로 생각을때요",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016193.1.5",
          "form": "약간 따뜻한 아이스아메리카노같은.. 느낌인건가",
          "original_form": "약간 따뜻한 아이스아메리카노같은.. 느낌인건가",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016193.1.6",
          "form": "쨌든 15데시벨 정도라고 막막 광고를 ㅋㅋ",
          "original_form": "쨌든 15데시벨 정도라고 막막 광고를 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016193.1.7",
          "form": "기억할게요",
          "original_form": "기억할게요",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016193.1.8",
          "form": "무소음은 별 의미가 없다.",
          "original_form": "무소음은 별 의미가 없다.",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016193.1.9",
          "form": "마트가면 주로 어떤거 구매하시나요",
          "original_form": "마트가면 주로 어떤거 구매하시나요",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016193.1.10",
          "form": "별 의미가 없지는 않고 조용하긴하지만 완전무소음은 아니다 강풍으로 올리면 소리가좀난다 ㅋㅋ",
          "original_form": "별 의미가 없지는 않고 조용하긴하지만 완전무소음은 아니다 강풍으로 올리면 소리가좀난다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016193.1.11",
          "form": "정도로 정리를 ㅋㅋ",
          "original_form": "정도로 정리를 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016193.1.12",
          "form": "아~  그런거구나 몰랐어오ㅡ",
          "original_form": "아~  그런거구나 몰랐어오ㅡ",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016193.1.13",
          "form": "항상 소음이 있는 것만 써봐서",
          "original_form": "항상 소음이 있는 것만 써봐서",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016193.1.14",
          "form": "그래도 틀고 티비 볼정도는 되요",
          "original_form": "그래도 틀고 티비 볼정도는 되요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016193.1.15",
          "form": "전에껀 진짜 너무시끄러워서",
          "original_form": "전에껀 진짜 너무시끄러워서",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016193.1.16",
          "form": "대화도 잘 안들렸 ㅋㅋ",
          "original_form": "대화도 잘 안들렸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016193.1.17",
          "form": "시끄러우면 방해되더라구요",
          "original_form": "시끄러우면 방해되더라구요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016193.1.18",
          "form": "잘 바꾸신거같아요",
          "original_form": "잘 바꾸신거같아요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016193.1.19",
          "form": "바꾸고 싶은지는 좀됐는데 고장이 안나더라고요 ㅋㅋ",
          "original_form": "바꾸고 싶은지는 좀됐는데 고장이 안나더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016193.1.20",
          "form": "올여름 최고의 딜레마 ㅋㅋ",
          "original_form": "올여름 최고의 딜레마 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016193.1.21",
          "form": "와 부럽다",
          "original_form": "와 부럽다",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016193.1.22",
          "form": "저는.. 고장나버렸어쇼",
          "original_form": "저는.. 고장나버렸어쇼",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016193.1.23",
          "form": "그를 보내주었죠",
          "original_form": "그를 보내주었죠",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016193.1.24",
          "form": "근데 그 씩씩한애가",
          "original_form": "근데 그 씩씩한애가",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016193.1.25",
          "form": "너무힘들었는지 중간에 쉬다가 다시 켜지더라고요",
          "original_form": "너무힘들었는지 중간에 쉬다가 다시 켜지더라고요",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016193.1.26",
          "form": "허허허",
          "original_form": "허허허",
