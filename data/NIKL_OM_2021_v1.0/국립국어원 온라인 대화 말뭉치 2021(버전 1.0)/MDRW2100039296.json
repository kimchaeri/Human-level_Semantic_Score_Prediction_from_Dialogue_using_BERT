      },
      "utterance": [
        {
          "id": "MDRW2100039296.1.1",
          "form": "반찬 온 거 받아놨지?",
          "original_form": "반찬 온 거 받아놨지?",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039296.1.2",
          "form": "ㅇㅇ근데 불고기가 생고기더라",
          "original_form": "ㅇㅇ근데 불고기가 생고기더라",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039296.1.3",
          "form": "바로 해서 먹으라고 그렇게 준비해줬나봐",
          "original_form": "바로 해서 먹으라고 그렇게 준비해줬나봐",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039296.1.4",
          "form": "아 그니까!",
          "original_form": "아 그니까!",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039296.1.5",
          "form": "치킨 먹어야되는데 이거 생고기부터 먹어야 되잖아ㅠㅠㅠ",
          "original_form": "치킨 먹어야되는데 이거 생고기부터 먹어야 되잖아ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039296.1.6",
          "form": "아니야 치킨 먹어도 돼",
          "original_form": "아니야 치킨 먹어도 돼",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039296.1.7",
          "form": "야채랑 양념 다 되있는데 이거 얼리고 그럼?",
          "original_form": "야채랑 양념 다 되있는데 이거 얼리고 그럼?",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039296.1.8",
          "form": "아니면 해놓기만 해도 되고",
          "original_form": "아니면 해놓기만 해도 되고",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039296.1.9",
          "form": "됐어ㅠㅜ",
          "original_form": "됐어ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039296.1.10",
          "form": "그냥 오늘 먹고 치킨 낼이나 모레 먹지 머..",
          "original_form": "그냥 오늘 먹고 치킨 낼이나 모레 먹지 머..",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039296.1.11",
          "form": "치킨 먹고 싶다며 먹어도 돼",
          "original_form": "치킨 먹고 싶다며 먹어도 돼",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039296.1.12",
          "form": "아 됐다고 낼 먹어",
          "original_form": "아 됐다고 낼 먹어",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039296.1.13",
          "form": "대신 꼭 먹어",
          "original_form": "대신 꼭 먹어",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039296.1.14",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039296.1.15",
          "form": "근데 낮에 먹어야 돼",
          "original_form": "근데 낮에 먹어야 돼",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039296.1.16",
          "form": "아 왜? 치킨집 저녁에 여는데!",
          "original_form": "아 왜? 치킨집 저녁에 여는데!",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039296.1.17",
          "form": "아저씨 나이트라 밥먹여 보내야지",
          "original_form": "아저씨 나이트라 밥먹여 보내야지",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039296.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039296.1.19",
          "form": "나 치킨 언제 먹어..",
          "original_form": "나 치킨 언제 먹어..",
          "speaker_id": "2",
          "time": "20210903 22:40"
        }
      ]
