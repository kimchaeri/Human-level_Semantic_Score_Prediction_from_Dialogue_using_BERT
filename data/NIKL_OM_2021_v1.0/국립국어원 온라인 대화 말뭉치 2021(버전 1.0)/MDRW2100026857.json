      "utterance": [
        {
          "id": "MDRW2100026857.1.1",
          "form": "님은 진짜 맛있는거 많이 드셔보신것 같으세요",
          "original_form": "님은 진짜 맛있는거 많이 드셔보신것 같으세요",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026857.1.2",
          "form": "내공이 느껴집니다",
          "original_form": "내공이 느껴집니다",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026857.1.3",
          "form": "먹을걸 찾아 헤매는 하이에나 삘이 나죠?ㅎㅎㅎ",
          "original_form": "먹을걸 찾아 헤매는 하이에나 삘이 나죠?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026857.1.4",
          "form": "악마가 자라고 해서 낼 찾아뵙겠습니다",
          "original_form": "악마가 자라고 해서 낼 찾아뵙겠습니다",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026857.1.5",
          "form": "아뇨 싸이느낌?",
          "original_form": "아뇨 싸이느낌?",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026857.1.6",
          "form": "ㅋㅋ뭐라구요?",
          "original_form": "ㅋㅋ뭐라구요?",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026857.1.7",
          "form": "낼 어여 주무세용!!!",
          "original_form": "낼 어여 주무세용!!!",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026857.1.8",
          "form": "진정즐길줄 아는 당신이 챔피어언~~><",
          "original_form": "진정즐길줄 아는 당신이 챔피어언~~><",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026857.1.9",
          "form": "낼봐욧",
          "original_form": "낼봐욧",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026857.1.10",
          "form": "아닉 ㅋㅋㅋ 때리려다 참았어욧!",
          "original_form": "아닉 ㅋㅋㅋ 때리려다 참았어욧!",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026857.1.11",
          "form": "굿나잇!!",
          "original_form": "굿나잇!!",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026857.1.12",
          "form": "살려주세용 진짜 빠이",
          "original_form": "살려주세용 진짜 빠이",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026857.1.13",
          "form": "아침드셨습니까 미숫가루한잔이랑 카스테라 가지고 출근중입니다",
          "original_form": "아침드셨습니까 미숫가루한잔이랑 카스테라 가지고 출근중입니다",
          "speaker_id": "2",
          "time": "20210902 08:27"
        },
        {
          "id": "MDRW2100026857.1.14",
          "form": "아침부터 든든히 드시네요ㅎ",
          "original_form": "아침부터 든든히 드시네요ㅎ",
          "speaker_id": "1",
          "time": "20210902 08:28"
        },
        {
          "id": "MDRW2100026857.1.15",
          "form": "전 원래 아침은 걸러서ㅎ",
          "original_form": "전 원래 아침은 걸러서ㅎ",
          "speaker_id": "1",
          "time": "20210902 08:28"
        },
        {
          "id": "MDRW2100026857.1.16",
          "form": "강아지 산책하고 잠시 누워있습니다ㅎ",
          "original_form": "강아지 산책하고 잠시 누워있습니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 08:29"
        },
        {
          "id": "MDRW2100026857.1.17",
          "form": "회사 안가니 좋네요!",
          "original_form": "회사 안가니 좋네요!",
          "speaker_id": "1",
          "time": "20210902 08:29"
        },
        {
          "id": "MDRW2100026857.1.18",
          "form": "카스테라는 어디 건가요?",
          "original_form": "카스테라는 어디 건가요?",
          "speaker_id": "1",
          "time": "20210902 08:31"
        },
        {
          "id": "MDRW2100026857.1.19",
          "form": "아침을 안드시나 보네요",
          "original_form": "아침을 안드시나 보네요",
          "speaker_id": "2",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100026857.1.20",
          "form": "강아지 산책 너무 좋네요 ",
          "original_form": "강아지 산책 너무 좋네요 ",
          "speaker_id": "2",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100026857.1.21",
          "form": "카스테라는 빠바 카스테란데",
          "original_form": "카스테라는 빠바 카스테란데",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100026857.1.22",
          "form": "사실 빠바인지도 잘 모르겠습니다 ㅋㅋㅋㅋ 다 잘라놧더라구요",
          "original_form": "사실 빠바인지도 잘 모르겠습니다 ㅋㅋㅋㅋ 다 잘라놧더라구요",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100026857.1.23",
          "form": "혹시 사장님이신가요?",
          "original_form": "혹시 사장님이신가요?",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100026857.1.24",
          "form": "회..장?",
          "original_form": "회..장?",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100026857.1.25",
          "form": "아니ㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026857.1.26",
          "form": "오늘 백신 예정이라 백신휴가 썼어요!",
          "original_form": "오늘 백신 예정이라 백신휴가 썼어요!",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026857.1.27",
          "form": "맞고 와서 몸보신해야죵",
          "original_form": "맞고 와서 몸보신해야죵",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026857.1.28",
          "form": "그래서 아침을 천사분이 차려줬다는 거군요ㅎ",
          "original_form": "그래서 아침을 천사분이 차려줬다는 거군요ㅎ",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100026857.1.29",
          "form": "천사 마즘",
          "original_form": "천사 마즘",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100026857.1.30",
          "form": "네 ㅋㅋ 비닐봉다리에 넣고 미숫가루 담아줫습니다",
          "original_form": "네 ㅋㅋ 비닐봉다리에 넣고 미숫가루 담아줫습니다",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100026857.1.31",
          "form": "쟁취해쮸",
          "original_form": "쟁취해쮸",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026857.1.32",
          "form": "한 3년간 없었어요",
          "original_form": "한 3년간 없었어요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026857.1.33",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026857.1.34",
          "form": "해달라했습니다",
          "original_form": "해달라했습니다",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026857.1.35",
          "form": "카스테라 잘라주는 정성이 어딥니까ㅋㅋ",
          "original_form": "카스테라 잘라주는 정성이 어딥니까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026857.1.36",
          "form": "애기한테만 좋은거 먹이고",
          "original_form": "애기한테만 좋은거 먹이고",
