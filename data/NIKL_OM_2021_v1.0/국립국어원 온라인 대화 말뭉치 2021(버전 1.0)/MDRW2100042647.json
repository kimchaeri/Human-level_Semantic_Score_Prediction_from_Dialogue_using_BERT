      "utterance": [
        {
          "id": "MDRW2100042647.1.1",
          "form": "구매희망 물건있으세요?",
          "original_form": "구매희망 물건있으세요?",
          "speaker_id": "1",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100042647.1.2",
          "form": "저는 그냥 집? ㅎㅎ",
          "original_form": "저는 그냥 집? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100042647.1.3",
          "form": "아 집",
          "original_form": "아 집",
          "speaker_id": "1",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100042647.1.4",
          "form": "저도 집! ㅋㅋ",
          "original_form": "저도 집! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100042647.1.5",
          "form": "어느쪽이좋으세요 지역은?",
          "original_form": "어느쪽이좋으세요 지역은?",
          "speaker_id": "1",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100042647.1.6",
          "form": "서울경기 어디든 괜찮을것같아요",
          "original_form": "서울경기 어디든 괜찮을것같아요",
          "speaker_id": "2",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042647.1.7",
          "form": "저는 아무래도 서울집값은",
          "original_form": "저는 아무래도 서울집값은",
          "speaker_id": "1",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042647.1.8",
          "form": "못잡을것같아요ㅠ",
          "original_form": "못잡을것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042647.1.9",
          "form": "가격이 정말 하루아침에 쑥쑥오르죠",
          "original_form": "가격이 정말 하루아침에 쑥쑥오르죠",
          "speaker_id": "2",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042647.1.10",
          "form": "네,,,",
          "original_form": "네,,,",
          "speaker_id": "1",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042647.1.11",
          "form": "그냥 얼른빚내서라도",
          "original_form": "그냥 얼른빚내서라도",
          "speaker_id": "1",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042647.1.12",
          "form": "사야하나싶어요",
          "original_form": "사야하나싶어요",
          "speaker_id": "1",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042647.1.13",
          "form": "요즘 대출이자보다 집값이 더 빠르게 오른다는 소리도 하긴하더라고요",
          "original_form": "요즘 대출이자보다 집값이 더 빠르게 오른다는 소리도 하긴하더라고요",
          "speaker_id": "2",
          "time": "20210907 00:21"
        },
        {
          "id": "MDRW2100042647.1.14",
          "form": "아,.,맞아요",
          "original_form": "아,.,맞아요",
          "speaker_id": "1",
          "time": "20210907 00:21"
        },
        {
          "id": "MDRW2100042647.1.15",
          "form": "저도들어봤어요",
          "original_form": "저도들어봤어요",
          "speaker_id": "1",
          "time": "20210907 00:21"
        },
        {
          "id": "MDRW2100042647.1.16",
          "form": "그럼 서울 경기 ?",
          "original_form": "그럼 서울 경기 ?",
          "speaker_id": "2",
          "time": "20210907 00:21"
        },
        {
          "id": "MDRW2100042647.1.17",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042647.1.18",
          "form": "사실이제",
          "original_form": "사실이제",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042647.1.19",
          "form": "지역에대한욕심도 없어요ㅋㅋ",
          "original_form": "지역에대한욕심도 없어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042647.1.20",
          "form": "ㅎㅎ 어디든?",
          "original_form": "ㅎㅎ 어디든?",
          "speaker_id": "2",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042647.1.21",
          "form": "아무래도 이제 가릴상황이 아니더라고요ㅋㅋ",
          "original_form": "아무래도 이제 가릴상황이 아니더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042647.1.22",
          "form": "아이고 ㅠㅠ",
          "original_form": "아이고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 00:23"
        }
      ]
