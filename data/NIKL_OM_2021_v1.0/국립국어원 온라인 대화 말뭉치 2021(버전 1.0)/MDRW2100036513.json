      "utterance": [
        {
          "id": "MDRW2100036513.1.1",
          "form": "진짜 앵콜가지고 너무 심하게",
          "original_form": "진짜 앵콜가지고 너무 심하게",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036513.1.2",
          "form": "까였었죠 ㅠㅜ",
          "original_form": "까였었죠 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036513.1.3",
          "form": "그러니까요 -.- !!!!!!!!",
          "original_form": "그러니까요 -.- !!!!!!!!",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036513.1.4",
          "form": "name1님 혹시 더보이즈도 아시나요?!",
          "original_form": "&name1&님 혹시 더보이즈도 아시나요?!",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036513.1.5",
          "form": "알죠알죠 !!!!!",
          "original_form": "알죠알죠 !!!!!",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036513.1.6",
          "form": "전 현재입니다 !!!!",
          "original_form": "전 현재입니다 !!!!",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036513.1.7",
          "form": "사랑해요 !!!!",
          "original_form": "사랑해요 !!!!",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036513.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036513.1.9",
          "form": "오마이각 저 저 주연 사랑랍니다",
          "original_form": "오마이각 저 저 주연 사랑랍니다",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036513.1.10",
          "form": "주연 영훈 중에 고민하다가 주연과 결혼하기오 햇어요",
          "original_form": "주연 영훈 중에 고민하다가 주연과 결혼하기오 햇어요",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036513.1.11",
          "form": "아 주연이 하",
          "original_form": "아 주연이 하",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100036513.1.12",
          "form": "전 주연이를 .. 주문으로 알기 시작했습니다 ..",
          "original_form": "전 주연이를 .. 주문으로 알기 시작했습니다 ..",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100036513.1.13",
          "form": "전 아직 더보이즈 알게된지 얼마 안 돼서......",
          "original_form": "전 아직 더보이즈 알게된지 얼마 안 돼서......",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036513.1.14",
          "form": "저는 뜬금업ㄱ이 우연히",
          "original_form": "저는 뜬금업ㄱ이 우연히",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036513.1.15",
          "form": "킹덤 전에ㅜ했던 미니킹덤 보고",
          "original_form": "킹덤 전에ㅜ했던 미니킹덤 보고",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036513.1.16",
          "form": "반했어요",
          "original_form": "반했어요",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036513.1.17",
          "form": "리빌? 보고",
          "original_form": "리빌? 보고",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036513.1.18",
          "form": "아아 로드투!",
          "original_form": "아아 로드투!",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036513.1.19",
          "form": "하 전 진짜 리빌",
          "original_form": "하 전 진짜 리빌",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036513.1.20",
          "form": "제 최애곡입니다요",
          "original_form": "제 최애곡입니다요",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036513.1.21",
          "form": "리빌은 주연이입니다.",
          "original_form": "리빌은 주연이입니다.",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036513.1.22",
          "form": "하 그니께로 깜짝 놀래버렸지 뭐예요 처음 무대 보고",
          "original_form": "하 그니께로 깜짝 놀래버렸지 뭐예요 처음 무대 보고",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.23",
          "form": "그리고는 이번에 스릴라이드??? 머시기",
          "original_form": "그리고는 이번에 스릴라이드??? 머시기",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.24",
          "form": "아 마자요",
          "original_form": "아 마자요",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.25",
          "form": "그거 무대 보면서 폴 인 럽",
          "original_form": "그거 무대 보면서 폴 인 럽",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.26",
          "form": "이번에 완전",
          "original_form": "이번에 완전",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.27",
          "form": "1등도 하구 !!",
          "original_form": "1등도 하구 !!",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.28",
          "form": "이제 리빌로 재컴백하면",
          "original_form": "이제 리빌로 재컴백하면",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.29",
          "form": "완벽합니다",
          "original_form": "완벽합니다",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036513.1.30",
          "form": "으악!!! 그니까요 제발 킹덤 버전 노래로",
          "original_form": "으악!!! 그니까요 제발 킹덤 버전 노래로",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036513.1.31",
          "form": "해달라",
          "original_form": "해달라",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036513.1.32",
          "form": "원곡 들으니까 쬠 심심하더라고요?",
          "original_form": "원곡 들으니까 쬠 심심하더라고요?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036513.1.33",
          "form": "아 맞아요 ! 로투킹에서 레전드로",
          "original_form": "아 맞아요 ! 로투킹에서 레전드로",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036513.1.34",
          "form": "편곡했었죠ㅜㅜ",
          "original_form": "편곡했었죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 00:06"
        }
      ]
