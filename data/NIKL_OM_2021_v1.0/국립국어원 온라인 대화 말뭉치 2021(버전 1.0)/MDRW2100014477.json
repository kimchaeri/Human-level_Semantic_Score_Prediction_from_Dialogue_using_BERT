      "utterance": [
        {
          "id": "MDRW2100014477.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014477.1.2",
          "form": "요즘 어떤 TV프로그램 보세요?",
          "original_form": "요즘 어떤 TV프로그램 보세요?",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014477.1.3",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014477.1.4",
          "form": "드라마봅니다",
          "original_form": "드라마봅니다",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014477.1.5",
          "form": "펜트하우스요",
          "original_form": "펜트하우스요",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014477.1.6",
          "form": "펜트하우스 재밌죠ㅋㅋㅋ",
          "original_form": "펜트하우스 재밌죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014477.1.7",
          "form": "너무 과할 때도 있지만 그게 또 재미요소라",
          "original_form": "너무 과할 때도 있지만 그게 또 재미요소라",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014477.1.8",
          "form": "지난주에도 보셨어요?",
          "original_form": "지난주에도 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014477.1.9",
          "form": "말도 안되서 재미있어요",
          "original_form": "말도 안되서 재미있어요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014477.1.10",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014477.1.11",
          "form": "보면서 놀랄 때가 많지요ㅋㅋㅋ",
          "original_form": "보면서 놀랄 때가 많지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014477.1.12",
          "form": "곧 끝날때가 된거같아요",
          "original_form": "곧 끝날때가 된거같아요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014477.1.13",
          "form": "네 설마 시즌4가 또 나오지는 않겠지요?",
          "original_form": "네 설마 시즌4가 또 나오지는 않겠지요?",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014477.1.14",
          "form": "헐  ㅋ  구럼  정말  코로나시국  드라마 되겟네요",
          "original_form": "헐  ㅋ  구럼  정말  코로나시국  드라마 되겟네요",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014477.1.15",
          "form": "끝날듯 끝나지 않아서",
          "original_form": "끝날듯 끝나지 않아서",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014477.1.16",
          "form": "진짜 나올지도 몰라요!",
          "original_form": "진짜 나올지도 몰라요!",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014477.1.17",
          "form": "누가 더 죽어야지 끝날지 ..",
          "original_form": "누가 더 죽어야지 끝날지 ..",
          "speaker_id": "2",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014477.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ이제 그만..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이제 그만..",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014477.1.19",
          "form": "죽었다가 살아났다가",
          "original_form": "죽었다가 살아났다가",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014477.1.20",
          "form": "헷갈려요",
          "original_form": "헷갈려요",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014477.1.21",
          "form": "ㅋㅌㅌ 주단테가 죽어야끝날듯요 ㅋㅋ",
          "original_form": "ㅋㅌㅌ 주단테가 죽어야끝날듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014477.1.22",
          "form": "안 죽을 것 같아요 근데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안 죽을 것 같아요 근데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014477.1.23",
          "form": "예고편에 또 등장..후우",
          "original_form": "예고편에 또 등장..후우",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014477.1.24",
          "form": "주단테는 쌍둥이 아닐거에요 ㅋㅋㅋㅋㅌ",
          "original_form": "주단테는 쌍둥이 아닐거에요 ㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014477.1.25",
          "form": "제발요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제발요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014477.1.26",
          "form": "근데 또 모를 일이라",
          "original_form": "근데 또 모를 일이라",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014477.1.27",
          "form": "일단 끝까지 봐야겠어요",
          "original_form": "일단 끝까지 봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014477.1.28",
          "form": "천서진은  진심 죽은거 같아요",
          "original_form": "천서진은  진심 죽은거 같아요",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014477.1.29",
          "form": "흠... 그쵸?",
          "original_form": "흠... 그쵸?",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014477.1.30",
          "form": "그렇게 피를 쏟았는데?",
          "original_form": "그렇게 피를 쏟았는데?",
          "speaker_id": "1",
          "time": "20210825 19:42"
        }
      ]
    }
