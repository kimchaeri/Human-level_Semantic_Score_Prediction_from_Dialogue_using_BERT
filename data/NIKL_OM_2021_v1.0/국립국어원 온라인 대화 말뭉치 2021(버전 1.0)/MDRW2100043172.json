      "utterance": [
        {
          "id": "MDRW2100043172.1.1",
          "form": "겨울은 니트만 입어도 멋나잖아유",
          "original_form": "겨울은 니트만 입어도 멋나잖아유",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043172.1.2",
          "form": "맞아요~ 안 그래도 니트 직구할까 고민중이에요",
          "original_form": "맞아요~ 안 그래도 니트 직구할까 고민중이에요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043172.1.3",
          "form": "오 어디서요 해외직구?",
          "original_form": "오 어디서요 해외직구?",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043172.1.4",
          "form": "크로켓이라는 플랫폼 괜찮더라구요",
          "original_form": "크로켓이라는 플랫폼 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043172.1.5",
          "form": "네!! 중개사 같은곳이에요",
          "original_form": "네!! 중개사 같은곳이에요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043172.1.6",
          "form": "오오 크로켓 알아요",
          "original_form": "오오 크로켓 알아요",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043172.1.7",
          "form": "써보셨나요?? 어때요??",
          "original_form": "써보셨나요?? 어때요??",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043172.1.8",
          "form": "저 거기서 향수 구매했었는데 배송도 생각보다 빠르고 안전하게 오더라고요",
          "original_form": "저 거기서 향수 구매했었는데 배송도 생각보다 빠르고 안전하게 오더라고요",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043172.1.9",
          "form": "우와 무슨 향수 샀나요",
          "original_form": "우와 무슨 향수 샀나요",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043172.1.10",
          "form": "조말론이랑 디올이 콜라보했던 향수였던 거 같아요 괜찮더라구요",
          "original_form": "조말론이랑 디올이 콜라보했던 향수였던 거 같아요 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043172.1.11",
          "form": "타미힐피거 니트도 비싸지 않아서 이번에 직구 고민중이에요",
          "original_form": "타미힐피거 니트도 비싸지 않아서 이번에 직구 고민중이에요",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043172.1.12",
          "form": "우아우아",
          "original_form": "우아우아",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043172.1.13",
          "form": "이번에 타미힐피거 포잊트로 모자랑 티셔츠 샀어요",
          "original_form": "이번에 타미힐피거 포잊트로 모자랑 티셔츠 샀어요",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043172.1.14",
          "form": "바이레도 블ㄹ랑쉬 향수 사고판됴",
          "original_form": "바이레도 블ㄹ랑쉬 향수 사고판됴",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043172.1.15",
          "form": "오오 좋으시겠네요",
          "original_form": "오오 좋으시겠네요",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100043172.1.16",
          "form": "저는 얼마 전에 닥터마틴 세일한대서 바로 결제했어요ㅋㅋㅋㅋ",
          "original_form": "저는 얼마 전에 닥터마틴 세일한대서 바로 결제했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100043172.1.17",
          "form": "와 닥터마틴 어떤거 샀어요?",
          "original_form": "와 닥터마틴 어떤거 샀어요?",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043172.1.18",
          "form": "저도 그냥 마음에 드는 거 고른 거라 모델명은 모르겠는데 워커같은 거 샀어요",
          "original_form": "저도 그냥 마음에 드는 거 고른 거라 모델명은 모르겠는데 워커같은 거 샀어요",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043172.1.19",
          "form": "근데 좀 무겁긴 하더라구요ㅋㅋㅋ",
          "original_form": "근데 좀 무겁긴 하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043172.1.20",
          "form": "아아 구쵸ㅠㅠ 무거울것같아요",
          "original_form": "아아 구쵸ㅠㅠ 무거울것같아요",
          "speaker_id": "1",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100043172.1.21",
          "form": "그래도 짱 간지잖아요",
          "original_form": "그래도 짱 간지잖아요",
          "speaker_id": "1",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100043172.1.22",
          "form": "맞아요ㅎㅎ 꼭 하나 장만하고 싶었거든요",
          "original_form": "맞아요ㅎㅎ 꼭 하나 장만하고 싶었거든요",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043172.1.23",
          "form": "맞아요 사고픈건 사야져",
          "original_form": "맞아요 사고픈건 사야져",
          "speaker_id": "1",
