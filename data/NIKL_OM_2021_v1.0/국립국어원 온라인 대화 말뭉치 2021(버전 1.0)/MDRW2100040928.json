      "utterance": [
        {
          "id": "MDRW2100040928.1.1",
          "form": "그냥 방에 둘건데여",
          "original_form": "그냥 방에 둘건데여",
          "speaker_id": "2",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100040928.1.2",
          "form": "안살수도 있어요",
          "original_form": "안살수도 있어요",
          "speaker_id": "2",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100040928.1.3",
          "form": "아하 왜용",
          "original_form": "아하 왜용",
          "speaker_id": "1",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100040928.1.4",
          "form": "지금은 우선 암막 커튼 하나 사서 옷위에 덮어야해요",
          "original_form": "지금은 우선 암막 커튼 하나 사서 옷위에 덮어야해요",
          "speaker_id": "2",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100040928.1.5",
          "form": "왜용 해가 마니 잇어요?",
          "original_form": "왜용 해가 마니 잇어요?",
          "speaker_id": "1",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040928.1.6",
          "form": "옷이 베란다에도 있는데요 ㅎㅎ ㅋㅋㅋㅋㅋ 그위에 덮어두던 암막커튼 이번에 아빠방에 설치해버렸거든요",
          "original_form": "옷이 베란다에도 있는데요 ㅎㅎ ㅋㅋㅋㅋㅋ 그위에 덮어두던 암막커튼 이번에 아빠방에 설치해버렸거든요",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040928.1.7",
          "form": "옷들 햇빛에 바랠것 같아서요ㅠ",
          "original_form": "옷들 햇빛에 바랠것 같아서요ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040928.1.8",
          "form": "아 그죠 얼릉 사야겟네요",
          "original_form": "아 그죠 얼릉 사야겟네요",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040928.1.9",
          "form": "암막천 ~",
          "original_form": "암막천 ~",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040928.1.10",
          "form": "넹 근데 검색해보기가 귀찮네요",
          "original_form": "넹 근데 검색해보기가 귀찮네요",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040928.1.11",
          "form": "일단 덮어둘거라도 없나요",
          "original_form": "일단 덮어둘거라도 없나요",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040928.1.12",
          "form": "없어요 그래서 오늘 바로 주문하려구요",
          "original_form": "없어요 그래서 오늘 바로 주문하려구요",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040928.1.13",
          "form": "그럼 화욜에 오겠죠",
          "original_form": "그럼 화욜에 오겠죠",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040928.1.14",
          "form": "아하 그죠",
          "original_form": "아하 그죠",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040928.1.15",
          "form": "베란다에도 옷이잇다니 옷부자시네",
          "original_form": "베란다에도 옷이잇다니 옷부자시네",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040928.1.16",
          "form": "네 제가 물욕이 좀 심해서요",
          "original_form": "네 제가 물욕이 좀 심해서요",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040928.1.17",
          "form": "지난주 옷 몇개삿어요",
          "original_form": "지난주 옷 몇개삿어요",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040928.1.18",
          "form": "자난주라... 기억이 잘안나는데 2개 같아요",
          "original_form": "자난주라... 기억이 잘안나는데 2개 같아요",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040928.1.19",
          "form": "머머삿어요",
          "original_form": "머머삿어요",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
