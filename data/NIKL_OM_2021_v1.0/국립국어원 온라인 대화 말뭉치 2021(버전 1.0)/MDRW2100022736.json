      "utterance": [
        {
          "id": "MDRW2100022736.1.1",
          "form": "최근에 본 전시 있으신가요?",
          "original_form": "최근에 본 전시 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022736.1.2",
          "form": "전시 좋아하세요?",
          "original_form": "전시 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022736.1.3",
          "form": "좋아하는 편이에요",
          "original_form": "좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022736.1.4",
          "form": "최근에는 이건희 컬렉션이 궁금해서",
          "original_form": "최근에는 이건희 컬렉션이 궁금해서",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022736.1.5",
          "form": "오 기사봤어요 그거",
          "original_form": "오 기사봤어요 그거",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022736.1.6",
          "form": "예약해보려고했는데 예약이 쉽지않네요.",
          "original_form": "예약해보려고했는데 예약이 쉽지않네요.",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022736.1.7",
          "form": "코로나때문에 그런가봐요",
          "original_form": "코로나때문에 그런가봐요",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022736.1.8",
          "form": "아 그것도 박터지나요?",
          "original_form": "아 그것도 박터지나요?",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022736.1.9",
          "form": "네 예약이 금방 다 차더라고요",
          "original_form": "네 예약이 금방 다 차더라고요",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022736.1.10",
          "form": "전시비용은",
          "original_form": "전시비용은",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022736.1.11",
          "form": "얼만가요??",
          "original_form": "얼만가요??",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022736.1.12",
          "form": "무료로 알고있어요",
          "original_form": "무료로 알고있어요",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022736.1.13",
          "form": "역시 이건희",
          "original_form": "역시 이건희",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022736.1.14",
          "form": "예전에는 예술의전당 많이갔는데..",
          "original_form": "예전에는 예술의전당 많이갔는데..",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022736.1.15",
          "form": "저는 대학로 공연은 몇번봤어요",
          "original_form": "저는 대학로 공연은 몇번봤어요",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022736.1.16",
          "form": "대학로공연은 한번도 본적이없는데 재미난가요?",
          "original_form": "대학로공연은 한번도 본적이없는데 재미난가요?",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022736.1.17",
          "form": "어떤거였나요?",
          "original_form": "어떤거였나요?",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022736.1.18",
          "form": "그냥 분위기가 좋았던것같아요",
          "original_form": "그냥 분위기가 좋았던것같아요",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022736.1.19",
          "form": "기억나는게 없는",
          "original_form": "기억나는게 없는",
          "speaker_id": "1",
          "time": "20210829 17:40"
        }
      ]
