      "utterance": [
        {
          "id": "MDRW2100036322.1.1",
          "form": "티비 볼게 없다 ㅜ.ㅜ",
          "original_form": "티비 볼게 없다 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036322.1.2",
          "form": "티비조서 틀어ㅋ",
          "original_form": "티비조서 틀어ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100036322.1.3",
          "form": "전자발찌 얘기한다",
          "original_form": "전자발찌 얘기한다",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100036322.1.4",
          "form": "그렇네 가출소한사람이 살인하고 자수해서",
          "original_form": "그렇네 가출소한사람이 살인하고 자수해서",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100036322.1.5",
          "form": "아니 2명을 살인했다ㅠ",
          "original_form": "아니 2명을 살인했다ㅠ",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100036322.1.6",
          "form": "엉 2명이나 안타깝다",
          "original_form": "엉 2명이나 안타깝다",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100036322.1.7",
          "form": "그러니깐ㅠ 잘 짤린대",
          "original_form": "그러니깐ㅠ 잘 짤린대",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100036322.1.8",
          "form": "5월에 가출소했는데 3개월만에 또 범죄를 50대남성이",
          "original_form": "5월에 가출소했는데 3개월만에 또 범죄를 50대남성이",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100036322.1.9",
          "form": "미국처럼 가석방 없는 종신형이 있어야 함",
          "original_form": "미국처럼 가석방 없는 종신형이 있어야 함",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100036322.1.10",
          "form": "그렇다 전과도 어마어마하던데",
          "original_form": "그렇다 전과도 어마어마하던데",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100036322.1.11",
          "form": "아프칸 저기도 계속 저러고 있네ㅠ",
          "original_form": "아프칸 저기도 계속 저러고 있네ㅠ",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100036322.1.12",
          "form": "그러게 남의 일이 아니다",
          "original_form": "그러게 남의 일이 아니다",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100036322.1.13",
          "form": "가수도 죽 이 고 막 죽 이  네",
          "original_form": "가수도 죽 이 고 막 죽 이  네",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100036322.1.14",
          "form": "우리나라는 그냥 노래금지곡 하고 그랬는데",
          "original_form": "우리나라는 그냥 노래금지곡 하고 그랬는데",
