        {
          "id": "MDRW2100035066.1.1",
          "form": "배가 고프다",
          "original_form": "배가 고프다",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.2",
          "form": "달달한 게 끌리지",
          "original_form": "달달한 게 끌리지",
          "speaker_id": "1",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.4",
          "form": "달달한 거 하면 마카롱이야",
          "original_form": "달달한 거 하면 마카롱이야",
          "speaker_id": "1",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.5",
          "form": "너도 마카롱 좋아하는구나",
          "original_form": "너도 마카롱 좋아하는구나",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.6",
          "form": "마카롱 싫어하는 여자애 찾기 힘들걸",
          "original_form": "마카롱 싫어하는 여자애 찾기 힘들걸",
          "speaker_id": "1",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.7",
          "form": "너 말 듣고 보니 그러네",
          "original_form": "너 말 듣고 보니 그러네",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.8",
          "form": "요즘은 마카롱이 참 다양해",
          "original_form": "요즘은 마카롱이 참 다양해",
          "speaker_id": "1",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.9",
          "form": "맞아 맛이 열 가지도 넘더라",
          "original_form": "맞아 맛이 열 가지도 넘더라",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.10",
          "form": "나는 피스타치오가 좋아",
          "original_form": "나는 피스타치오가 좋아",
          "speaker_id": "1",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035066.1.11",
          "form": "나는 치즈가 좋던데",
          "original_form": "나는 치즈가 좋던데",
          "speaker_id": "2",
          "time": "20210829 01:37"
        },
        {
          "id": "MDRW2100035066.1.12",
          "form": "초코도맛있어",
          "original_form": "초코도맛있어",
          "speaker_id": "1",
          "time": "20210829 01:37"
        },
        {
          "id": "MDRW2100035066.1.13",
          "form": "요즘은 뚱 카롱도 있어",
          "original_form": "요즘은 뚱 카롱도 있어",
