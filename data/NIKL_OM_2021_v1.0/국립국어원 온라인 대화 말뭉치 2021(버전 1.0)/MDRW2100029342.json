      "utterance": [
        {
          "id": "MDRW2100029342.1.1",
          "form": "본가랑 자취방이랑 왔다갔다 해요!",
          "original_form": "본가랑 자취방이랑 왔다갔다 해요!",
          "speaker_id": "2",
          "time": "20210903 02:00"
        },
        {
          "id": "MDRW2100029342.1.2",
          "form": "네 그래서 저 일부러 본가 근처에서 맞았어요",
          "original_form": "네 그래서 저 일부러 본가 근처에서 맞았어요",
          "speaker_id": "2",
          "time": "20210903 02:00"
        },
        {
          "id": "MDRW2100029342.1.3",
          "form": "혼자 사세요?",
          "original_form": "혼자 사세요?",
          "speaker_id": "2",
          "time": "20210903 02:01"
        },
        {
          "id": "MDRW2100029342.1.4",
          "form": "아항 잘하셧네용 ! ㅋㅋ",
          "original_form": "아항 잘하셧네용 ! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:01"
        },
        {
          "id": "MDRW2100029342.1.5",
          "form": "네 그래서 저도",
          "original_form": "네 그래서 저도",
          "speaker_id": "1",
          "time": "20210903 02:01"
        },
        {
          "id": "MDRW2100029342.1.6",
          "form": "일부러 본가가는 추 석맞춰 신청..",
          "original_form": "일부러 본가가는 추 석맞춰 신청..",
          "speaker_id": "1",
          "time": "20210903 02:01"
        },
        {
          "id": "MDRW2100029342.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210903 02:01"
        },
        {
          "id": "MDRW2100029342.1.8",
          "form": "오오 계획 잘 하셨네요ㅎㅎㅎ",
          "original_form": "오오 계획 잘 하셨네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:01"
        },
        {
          "id": "MDRW2100029342.1.9",
          "form": "추석 전이면 화이자 맞으시는거죠?",
          "original_form": "추석 전이면 화이자 맞으시는거죠?",
          "speaker_id": "2",
          "time": "20210903 02:02"
        },
        {
          "id": "MDRW2100029342.1.10",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210903 02:02"
        },
        {
          "id": "MDRW2100029342.1.11",
          "form": "아마도? 모르겟당",
          "original_form": "아마도? 모르겟당",
          "speaker_id": "1",
          "time": "20210903 02:02"
        },
        {
          "id": "MDRW2100029342.1.12",
          "form": "가바야알지않나요?",
          "original_form": "가바야알지않나요?",
          "speaker_id": "1",
          "time": "20210903 02:02"
        },
        {
          "id": "MDRW2100029342.1.13",
          "form": "그거 카톡으로 미리 알려주던데 아직 안왔나요?",
          "original_form": "그거 카톡으로 미리 알려주던데 아직 안왔나요?",
          "speaker_id": "2",
          "time": "20210903 02:02"
        },
        {
          "id": "MDRW2100029342.1.14",
          "form": "앗..네 아직 안온듯하네용 ...?",
          "original_form": "앗..네 아직 안온듯하네용 ...?",
          "speaker_id": "1",
          "time": "20210903 02:03"
        },
        {
          "id": "MDRW2100029342.1.15",
          "form": "모더나또는 화이자라고만..",
          "original_form": "모더나또는 화이자라고만..",
          "speaker_id": "1",
          "time": "20210903 02:04"
        },
        {
          "id": "MDRW2100029342.1.16",
          "form": "저도 모너다 또는 화이자라고 되어있었는데... 언제 받았는지 기억은 안나는데 접종 며칠 전에 확정해서 알려주더라고요",
          "original_form": "저도 모너다 또는 화이자라고 되어있었는데... 언제 받았는지 기억은 안나는데 접종 며칠 전에 확정해서 알려주더라고요",
          "speaker_id": "2",
          "time": "20210903 02:04"
        },
        {
          "id": "MDRW2100029342.1.17",
          "form": "아글쿠낭 ㅋㅋ 뭐.. . 그게그건듯 ㅋㅋㅋ",
          "original_form": "아글쿠낭 ㅋㅋ 뭐.. . 그게그건듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029342.1.18",
          "form": "안아팟음조켓네요",
          "original_form": "안아팟음조켓네요",
          "speaker_id": "1",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029342.1.19",
          "form": "그쳐 사실 그거나 그거나죠 ㅋㅋㅋㅋ",
          "original_form": "그쳐 사실 그거나 그거나죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029342.1.20",
          "form": "많이 안 아프시길 바라요",
          "original_form": "많이 안 아프시길 바라요",
          "speaker_id": "2",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029342.1.21",
          "form": "아제만 아니면 ㅋㅋ",
          "original_form": "아제만 아니면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029342.1.22",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029342.1.23",
          "form": "앗 저는 이제 자야 할 것 같아요",
          "original_form": "앗 저는 이제 자야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029342.1.24",
          "form": "엇 넹..꿀잠주무세용 ㅋㅋㅋ",
          "original_form": "엇 넹..꿀잠주무세용 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029342.1.25",
