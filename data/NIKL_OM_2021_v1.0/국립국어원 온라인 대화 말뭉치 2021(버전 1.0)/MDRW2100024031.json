      "utterance": [
        {
          "id": "MDRW2100024031.1.1",
          "form": "집안일중 제일 힘든게 뭘까요?",
          "original_form": "집안일중 제일 힘든게 뭘까요?",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024031.1.2",
          "form": "육아요ㅠㅠ",
          "original_form": "육아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024031.1.3",
          "form": "아~ 육아 전 육아는 안해봐서",
          "original_form": "아~ 육아 전 육아는 안해봐서",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024031.1.4",
          "form": "근데 진짜 밤낮없고",
          "original_form": "근데 진짜 밤낮없고",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024031.1.5",
          "form": "밤낮은 있는데 자유가없죠ㅠ",
          "original_form": "밤낮은 있는데 자유가없죠ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024031.1.6",
          "form": "제동생 어릴땐 엄마 화장실도 쫓아가던데",
          "original_form": "제동생 어릴땐 엄마 화장실도 쫓아가던데",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024031.1.7",
          "form": "그정돈아닌데ㅡ다른거에 다ㅜㄴ치대요ㅡㅜㅠ",
          "original_form": "그정돈아닌데ㅡ다른거에 다ㅜㄴ치대요ㅡㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024031.1.8",
          "form": "얼마나붙늣지ㅠ",
          "original_form": "얼마나붙늣지ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024031.1.9",
          "form": "아이고 그래도 이쁘쟈나요",
          "original_form": "아이고 그래도 이쁘쟈나요",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024031.1.10",
          "form": "맞아요 특히잘때",
          "original_form": "맞아요 특히잘때",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024031.1.11",
          "form": "잘땐천사란 말이맞군요",
          "original_form": "잘땐천사란 말이맞군요",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100024031.1.12",
          "form": "백프로 맞는말이죠 ㅎㅎ",
          "original_form": "백프로 맞는말이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100024031.1.13",
          "form": "백프로 ㅋㅋ 전 자면 깨우고 싶던데",
          "original_form": "백프로 ㅋㅋ 전 자면 깨우고 싶던데",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024031.1.14",
          "form": "나중에 직접 겪어보시면 안그러실꺼에요 ㅎㅎㅎ 자는데 깨우는 사람 정말 미움",
          "original_form": "나중에 직접 겪어보시면 안그러실꺼에요 ㅎㅎㅎ 자는데 깨우는 사람 정말 미움",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024031.1.15",
          "form": "남의 아가는 이뿌쟈나요",
          "original_form": "남의 아가는 이뿌쟈나요",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024031.1.16",
          "form": "그니까여 ㅎㅎ 눈으로 보는게 얼마나 이뻐여 ㅎㅎㅎ",
          "original_form": "그니까여 ㅎㅎ 눈으로 보는게 얼마나 이뻐여 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024031.1.17",
          "form": "눈으로 ㅋㅋ",
          "original_form": "눈으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:43"
        }
      ]
