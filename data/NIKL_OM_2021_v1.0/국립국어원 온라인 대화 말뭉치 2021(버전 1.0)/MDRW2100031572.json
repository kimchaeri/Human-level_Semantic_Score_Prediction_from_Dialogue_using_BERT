      "utterance": [
        {
          "id": "MDRW2100031572.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100031572.1.2",
          "form": "그래. 안녕",
          "original_form": "그래. 안녕",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031572.1.3",
          "form": "결혼 생각 있어?",
          "original_form": "결혼 생각 있어?",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031572.1.4",
          "form": "딱히?",
          "original_form": "딱히?",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031572.1.5",
          "form": "인연이 있다면 할수도 있지 그정도?",
          "original_form": "인연이 있다면 할수도 있지 그정도?",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031572.1.6",
          "form": "나도ㅋㅋㅋ",
          "original_form": "나도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031572.1.7",
          "form": "애인있어?",
          "original_form": "애인있어?",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031572.1.8",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031572.1.9",
          "form": "나도없어",
          "original_form": "나도없어",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031572.1.10",
          "form": "ㅋㅋㅋㅋ 근데 사실",
          "original_form": "ㅋㅋㅋㅋ 근데 사실",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031572.1.11",
          "form": "이성에관심이 그닥 없어 나능",
          "original_form": "이성에관심이 그닥 없어 나능",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100031572.1.12",
          "form": "나도 뭐...",
          "original_form": "나도 뭐...",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031572.1.13",
          "form": "그렇다고 딱히 동성에 관심있는것도 아닌데",
          "original_form": "그렇다고 딱히 동성에 관심있는것도 아닌데",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031572.1.14",
          "form": "굳이 날 정의한다면 로맨틱에이 랄까",
          "original_form": "굳이 날 정의한다면 로맨틱에이 랄까",
          "speaker_id": "2",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100031572.1.15",
          "form": "에이가 그 연애에 관심없는건가??",
          "original_form": "에이가 그 연애에 관심없는건가??",
          "speaker_id": "1",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100031572.1.16",
          "form": "음, 난 로맨틱이니까 연애감정은 있어",
          "original_form": "음, 난 로맨틱이니까 연애감정은 있어",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031572.1.17",
          "form": "정도이상의 스킨쉽에 관심이 없는거지",
          "original_form": "정도이상의 스킨쉽에 관심이 없는거지",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031572.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031572.1.19",
          "form": "스킨쉽이 싫어?",
          "original_form": "스킨쉽이 싫어?",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031572.1.20",
          "form": "아님 그냥 관심이 없어?",
          "original_form": "아님 그냥 관심이 없어?",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031572.1.21",
          "form": "거부감도 있긴한데 관심이 없어",
          "original_form": "거부감도 있긴한데 관심이 없어",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031572.1.22",
          "form": "거부감도 위생적인 측면의 거부감이고",
          "original_form": "거부감도 위생적인 측면의 거부감이고",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031572.1.23",
          "form": "오 글쿠나",
          "original_form": "오 글쿠나",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031572.1.24",
          "form": "근데 좀 이해되네",
          "original_form": "근데 좀 이해되네",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031572.1.25",
          "form": "나도 키스같은건 좀그래",
          "original_form": "나도 키스같은건 좀그래",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031572.1.26",
          "form": "응..  뽀뽀는 그래도 할만한데",
          "original_form": "응..  뽀뽀는 그래도 할만한데",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031572.1.27",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031572.1.28",
          "form": "글고보통",
          "original_form": "글고보통",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031572.1.29",
          "form": "치아상태가",
          "original_form": "치아상태가",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031572.1.30",
          "form": "그닥ㅋㅋㅋ이니깐",
          "original_form": "그닥ㅋㅋㅋ이니깐",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031572.1.31",
          "form": "혀도그렇코",
          "original_form": "혀도그렇코",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031572.1.32",
          "form": "아... 난 침 섞이는게 걍 비위상해서",
          "original_form": "아... 난 침 섞이는게 걍 비위상해서",
          "speaker_id": "2",
          "time": "20210904 23:26"
        }
      ]
    }
