      "utterance": [
        {
          "id": "MDRW2100025386.1.1",
          "form": "오늘 거기 비오시나요",
          "original_form": "오늘 거기 비오시나요",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100025386.1.2",
          "form": "지금 날씨어떤가요",
          "original_form": "지금 날씨어떤가요",
          "speaker_id": "2",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100025386.1.3",
          "form": "지금은 안오네요",
          "original_form": "지금은 안오네요",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025386.1.4",
          "form": "아침에 출근할때 쪼금씩 내리던데 지금은 모르겠어요",
          "original_form": "아침에 출근할때 쪼금씩 내리던데 지금은 모르겠어요",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025386.1.5",
          "form": "내일도 비오는것같던데",
          "original_form": "내일도 비오는것같던데",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025386.1.6",
          "form": "그런가요 비가 내려서 그런지 습해서 덥네요",
          "original_form": "그런가요 비가 내려서 그런지 습해서 덥네요",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025386.1.7",
          "form": "습기차서 끈적한거 진짜 싫네요",
          "original_form": "습기차서 끈적한거 진짜 싫네요",
          "speaker_id": "1",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025386.1.8",
          "form": "어떤 계절 좋아하세요",
          "original_form": "어떤 계절 좋아하세요",
          "speaker_id": "1",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025386.1.9",
          "form": "맞아요 저는 봄이나 가을이 좋아요 덥지도 춥지도않은 계절이요",
          "original_form": "맞아요 저는 봄이나 가을이 좋아요 덥지도 춥지도않은 계절이요",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025386.1.10",
          "form": "당신은요?",
          "original_form": "당신은요?",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025386.1.11",
          "form": "저도 가을이 좋은거 같아요 선선하니",
          "original_form": "저도 가을이 좋은거 같아요 선선하니",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025386.1.12",
          "form": "가을 전어도 먹고",
          "original_form": "가을 전어도 먹고",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025386.1.13",
          "form": "저번주 집근처에 번개 떨어졌어요",
          "original_form": "저번주 집근처에 번개 떨어졌어요",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025386.1.14",
          "form": "저도요 소리가 바로옆에서들려서 깜짝놀랐네요",
          "original_form": "저도요 소리가 바로옆에서들려서 깜짝놀랐네요",
          "speaker_id": "2",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025386.1.15",
          "form": "괜찮으셨어요?",
          "original_form": "괜찮으셨어요?",
          "speaker_id": "2",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025386.1.16",
          "form": "119 소방차가 6대 왔더라구요",
          "original_form": "119 소방차가 6대 왔더라구요",
          "speaker_id": "1",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025386.1.17",
          "form": "기사도 났던데 다행히 인명피해는 없다라구요",
          "original_form": "기사도 났던데 다행히 인명피해는 없다라구요",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025386.1.18",
          "form": "번개치는 날씨는 무서워요",
          "original_form": "번개치는 날씨는 무서워요",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025386.1.19",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025386.1.20",
          "form": "가을에 장마라니 날씨가 이상해지네요",
          "original_form": "가을에 장마라니 날씨가 이상해지네요",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025386.1.21",
          "form": "그러게요 얼마전 내린 비로 침수된 곳도 많고 걱정이에요",
          "original_form": "그러게요 얼마전 내린 비로 침수된 곳도 많고 걱정이에요",
          "speaker_id": "2",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025386.1.22",
          "form": "지대가 낮은곳인가봐요",
          "original_form": "지대가 낮은곳인가봐요",
          "speaker_id": "1",
          "time": "20210901 10:57"
        }
      ]
