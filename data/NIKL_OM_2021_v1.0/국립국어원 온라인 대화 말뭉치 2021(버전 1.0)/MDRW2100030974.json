      "utterance": [
        {
          "id": "MDRW2100030974.1.1",
          "form": "백신 예약 하셨어요?",
          "original_form": "백신 예약 하셨어요?",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030974.1.2",
          "form": "네 이번달 27일이요",
          "original_form": "네 이번달 27일이요",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030974.1.3",
          "form": "예약하셨어요?",
          "original_form": "예약하셨어요?",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030974.1.4",
          "form": "아...27일이시군요",
          "original_form": "아...27일이시군요",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030974.1.5",
          "form": "저는 24일에 예약했어요",
          "original_form": "저는 24일에 예약했어요",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030974.1.6",
          "form": "아직 백신종류 어떤건지 소식 없죠?",
          "original_form": "아직 백신종류 어떤건지 소식 없죠?",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030974.1.7",
          "form": "어떤건지는 모르겠어요. 모더나아님 화이자같은데",
          "original_form": "어떤건지는 모르겠어요. 모더나아님 화이자같은데",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030974.1.8",
          "form": "네 둘중 하나인데 어떤거 맞을지 궁금해요",
          "original_form": "네 둘중 하나인데 어떤거 맞을지 궁금해요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030974.1.9",
          "form": "저는 신랑이랑 같이 맞으려구요",
          "original_form": "저는 신랑이랑 같이 맞으려구요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030974.1.10",
          "form": "저도 신랑이랑 같이했어요",
          "original_form": "저도 신랑이랑 같이했어요",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030974.1.11",
          "form": "아이들 유치원보내고 백신맞으러 같이 가기로 했어요 ㅎ",
          "original_form": "아이들 유치원보내고 백신맞으러 같이 가기로 했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030974.1.12",
          "form": "아파도 같이 아프고 ㅋㅋㅋ",
          "original_form": "아파도 같이 아프고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030974.1.13",
          "form": "신랑이 혹시모르니 따로해야하는거 아니냐고~",
          "original_form": "신랑이 혹시모르니 따로해야하는거 아니냐고~",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030974.1.14",
          "form": "아픈사람 서로 챙겨주기 ㅋ",
          "original_form": "아픈사람 서로 챙겨주기 ㅋ",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030974.1.15",
          "form": "저도 그런생각했는데...회사언니가 부부 따른날로 해서 맞았는데",
          "original_form": "저도 그런생각했는데...회사언니가 부부 따른날로 해서 맞았는데",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030974.1.16",
          "form": "ㅎ 만에하나를 생각하고 맞아야하니...",
          "original_form": "ㅎ 만에하나를 생각하고 맞아야하니...",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030974.1.17",
          "form": "둘다 별다른 반응이 없더라며 ㅎㅎ",
          "original_form": "둘다 별다른 반응이 없더라며 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030974.1.18",
          "form": "그냥 같이 휴가내고 놀 목적도 있궁 ㅋㅋ",
          "original_form": "그냥 같이 휴가내고 놀 목적도 있궁 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030974.1.19",
          "form": "ㅎ 형님도 그러더라구요.  별거없으니 얼렁맞으라고요",
          "original_form": "ㅎ 형님도 그러더라구요.  별거없으니 얼렁맞으라고요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030974.1.20",
          "form": "맞은사람들은 별거 없다고 ㅎㅎㅎ",
          "original_form": "맞은사람들은 별거 없다고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030974.1.21",
          "form": "맞기전에는 오만생각 다 들어요 ㅎ",
          "original_form": "맞기전에는 오만생각 다 들어요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030974.1.22",
          "form": "많은사람이 맞아서 부작용수치도 많은것처럼 보이는거라고",
          "original_form": "많은사람이 맞아서 부작용수치도 많은것처럼 보이는거라고",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030974.1.23",
          "form": "그니깐요.  얼렁 맞고 치웠어야했는데...",
          "original_form": "그니깐요.  얼렁 맞고 치웠어야했는데...",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100030974.1.24",
          "form": "맞아요 우리 모두 맞고 평범한 사람들처럼 잘 지나가길 바래용 ㅎ",
          "original_form": "맞아요 우리 모두 맞고 평범한 사람들처럼 잘 지나가길 바래용 ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:08"
        }
      ]
    }
