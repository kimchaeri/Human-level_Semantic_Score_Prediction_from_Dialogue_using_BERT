      "utterance": [
        {
          "id": "MDRW2100047584.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047584.1.2",
          "form": "네네 헉 아 물어보고 싶은거 있었는데",
          "original_form": "네네 헉 아 물어보고 싶은거 있었는데",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047584.1.3",
          "form": "서부 다녀오셔서 살 많이 타셨나요?!",
          "original_form": "서부 다녀오셔서 살 많이 타셨나요?!",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100047584.1.4",
          "form": "아 ㅋㅋㅋ 네 그때사진이랑 지금 비교해보니깐 많이탔더라구요 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ 네 그때사진이랑 지금 비교해보니깐 많이탔더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047584.1.5",
          "form": "선크림 많이발랐던거같은데도 탔어요 ㅋ ㅠㅠㅠ",
          "original_form": "선크림 많이발랐던거같은데도 탔어요 ㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047584.1.6",
          "form": "다들 끝내주게 타서 오더라고요",
          "original_form": "다들 끝내주게 타서 오더라고요",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100047584.1.7",
          "form": "굿트립~",
          "original_form": "굿트립~",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100047584.1.8",
          "form": "누가봐도",
          "original_form": "누가봐도",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100047584.1.9",
          "form": "휴가 다녀 온 사람이더라고요",
          "original_form": "휴가 다녀 온 사람이더라고요",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100047584.1.10",
          "form": "하와이는 가보고싶으시지 않나요",
          "original_form": "하와이는 가보고싶으시지 않나요",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047584.1.11",
          "form": "저의 꿈의 여행지",
          "original_form": "저의 꿈의 여행지",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047584.1.12",
          "form": "아 저도 너무가고싶어요",
          "original_form": "아 저도 너무가고싶어요",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047584.1.13",
          "form": "저의 위시!!",
          "original_form": "저의 위시!!",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047584.1.14",
          "form": "서핑하고싶어요",
          "original_form": "서핑하고싶어요",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047584.1.15",
          "form": "크으으...",
          "original_form": "크으으...",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047584.1.16",
          "form": "바다쪽을 좋아하시는군요",
          "original_form": "바다쪽을 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047584.1.17",
          "form": "선물도 바다 쪽에서 사오는 편이신가요?",
          "original_form": "선물도 바다 쪽에서 사오는 편이신가요?",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047584.1.18",
          "form": "음 딱히바다가좋다기보단 물에서노는걸좋아해요 ㅎㅎㅎ 바다쪽선물이라면 어떤걸말씀하시는건가요? 해산물 이런거말하는건가요??",
          "original_form": "음 딱히바다가좋다기보단 물에서노는걸좋아해요 ㅎㅎㅎ 바다쪽선물이라면 어떤걸말씀하시는건가요? 해산물 이런거말하는건가요??",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100047584.1.19",
          "form": "앗 막 그 조개 모양 굿즈나 이런걸로..",
          "original_form": "앗 막 그 조개 모양 굿즈나 이런걸로..",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047584.1.20",
          "form": "오 물놀이 좋아하시는구나 넘 신나죠",
          "original_form": "오 물놀이 좋아하시는구나 넘 신나죠",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047584.1.21",
          "form": "계곡도 많이 가시나요?",
          "original_form": "계곡도 많이 가시나요?",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047584.1.22",
          "form": "전 계곡 너무 좋아해요",
          "original_form": "전 계곡 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047584.1.23",
          "form": "아 ㅋㅋㅋㅋㅋ 기념품은 워낙 안산지오래되서.. 개인덕으론 차라리 그지역의 간식같은거 사오는게나은거같더라구요~",
          "original_form": "아 ㅋㅋㅋㅋㅋ 기념품은 워낙 안산지오래되서.. 개인덕으론 차라리 그지역의 간식같은거 사오는게나은거같더라구요~",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047584.1.24",
          "form": "계곡 너무좋아요! 근데 모기가많아서 언젠가부턴 안가게되더라구요ㅠ",
          "original_form": "계곡 너무좋아요! 근데 모기가많아서 언젠가부턴 안가게되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047584.1.25",
          "form": "헉 모기 많이 물리시느느타입이군요",
          "original_form": "헉 모기 많이 물리시느느타입이군요",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100047584.1.26",
          "form": "저도요^^;;;;",
          "original_form": "저도요^^;;;;",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100047584.1.27",
          "form": "거의 뷔페",
          "original_form": "거의 뷔페",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100047584.1.28",
          "form": "뷔펰ㅋㅋㅋㅋ 맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 열심히놀고 집오면 고통스러워요 너무 많이 물려있어서 ㅜ",
          "original_form": "뷔펰ㅋㅋㅋㅋ 맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 열심히놀고 집오면 고통스러워요 너무 많이 물려있어서 ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100047584.1.29",
          "form": "맞아요 심지어 산모기 독하잖아요",
          "original_form": "맞아요 심지어 산모기 독하잖아요",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100047584.1.30",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047584.1.31",
          "form": "그쵸.. 그놈들은 뭔 약초를 달여먹었나봐요 어찌그리 독한지",
