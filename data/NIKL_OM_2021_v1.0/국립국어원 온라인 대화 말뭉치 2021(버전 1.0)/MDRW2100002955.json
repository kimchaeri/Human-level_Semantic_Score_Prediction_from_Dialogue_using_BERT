      "utterance": [
        {
          "id": "MDRW2100002955.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002955.1.2",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002955.1.3",
          "form": "최근에 뭐 구매한 거 있으세요??",
          "original_form": "최근에 뭐 구매한 거 있으세요??",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002955.1.4",
          "form": "저 두유 좋아해서 두유 구매했아요~ 뤼님은요?",
          "original_form": "저 두유 좋아해서 두유 구매했아요~ 뤼님은요?",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002955.1.5",
          "form": "두유좋아하시는구나!",
          "original_form": "두유좋아하시는구나!",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002955.1.6",
          "form": "저는 북트롤리 샀어요~~",
          "original_form": "저는 북트롤리 샀어요~~",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002955.1.7",
          "form": "공부하는 게 있는데, 방을 이동하면서 공부하는 걸 좋아해서요!",
          "original_form": "공부하는 게 있는데, 방을 이동하면서 공부하는 걸 좋아해서요!",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002955.1.8",
          "form": "그게 몬가용?",
          "original_form": "그게 몬가용?",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002955.1.9",
          "form": "작은 책장인데 바퀴달려서 이동할 수 있는거예요",
          "original_form": "작은 책장인데 바퀴달려서 이동할 수 있는거예요",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002955.1.10",
          "form": "와 왠지 비쌀거같아요~",
          "original_form": "와 왠지 비쌀거같아요~",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002955.1.11",
          "form": "그래서 중국산을 샀더니 조금 불안해요 ㅋㅋㅋㅋ",
          "original_form": "그래서 중국산을 샀더니 조금 불안해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002955.1.12",
          "form": "오늘 결제해서 아직 안왔어요 ! 좋은 상품이었으면 좋겠어요",
          "original_form": "오늘 결제해서 아직 안왔어요 ! 좋은 상품이었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002955.1.13",
          "form": "두유는 어디 거 사셨어요?",
          "original_form": "두유는 어디 거 사셨어요?",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002955.1.14",
          "form": "그런 큰 가구사면 집에와서 설치해주나요?",
          "original_form": "그런 큰 가구사면 집에와서 설치해주나요?",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002955.1.15",
          "form": "두유는 ..어디꺼였더라.. 경기도의 중소업체였는데~",
          "original_form": "두유는 ..어디꺼였더라.. 경기도의 중소업체였는데~",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002955.1.16",
          "form": "이동할 수 있는 제품이라 작아요! 총 길이가 70cm도 안돼서 그냥 제가 조립해야해요 ㅋㅋㅋ",
          "original_form": "이동할 수 있는 제품이라 작아요! 총 길이가 70cm도 안돼서 그냥 제가 조립해야해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002955.1.17",
          "form": "오 저는 베지밀 거밖에 안먹어본둣해요",
          "original_form": "오 저는 베지밀 거밖에 안먹어본둣해요",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002955.1.18",
          "form": "원래 돈아끼느라 소비를 잘 안하는데",
          "original_form": "원래 돈아끼느라 소비를 잘 안하는데",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002955.1.19",
          "form": "완전히 꽂힌 두유라서 계속 사먹게되네요",
          "original_form": "완전히 꽂힌 두유라서 계속 사먹게되네요",
          "speaker_id": "1",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002955.1.20",
          "form": "우와 궁금하네요",
          "original_form": "우와 궁금하네요",
          "speaker_id": "2",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002955.1.21",
          "form": "더 고소한가요?",
          "original_form": "더 고소한가요?",
          "speaker_id": "2",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002955.1.22",
          "form": "아뇨 호불호 갈려요 작은 회사에서 만든 잔다리 두유라는건데 아주 밍밍한 콩물 맛",
          "original_form": "아뇨 호불호 갈려요 작은 회사에서 만든 잔다리 두유라는건데 아주 밍밍한 콩물 맛",
          "speaker_id": "1",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002955.1.23",
          "form": "앗 그럼 제취향은 아닐 거 같네요 ㅋㅋㅋㅋ",
          "original_form": "앗 그럼 제취향은 아닐 거 같네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002955.1.24",
          "form": "이름이 신기해요.. 잔다리",
          "original_form": "이름이 신기해요.. 잔다리",
          "speaker_id": "2",
          "time": "20210820 17:07"
        }
      ]
