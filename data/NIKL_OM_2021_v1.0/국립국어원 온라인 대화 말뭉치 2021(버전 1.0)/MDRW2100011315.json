      "utterance": [
        {
          "id": "MDRW2100011315.1.1",
          "form": "넹ㅋㅋ name1님도 추천할게임있나요~?",
          "original_form": "넹ㅋㅋ &name1&님도 추천할게임있나요~?",
          "speaker_id": "2",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100011315.1.2",
          "form": "저는 하는게 많진 않아서ㅋㅋ",
          "original_form": "저는 하는게 많진 않아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100011315.1.3",
          "form": "모바일 카트나 심심할땐 퍼즐 게임해요",
          "original_form": "모바일 카트나 심심할땐 퍼즐 게임해요",
          "speaker_id": "1",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100011315.1.4",
          "form": "퍼즐은 어떤게임있나요?!",
          "original_form": "퍼즐은 어떤게임있나요?!",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100011315.1.5",
          "form": "예전에 퍼즐앤드래곤이라는 게임도 했구",
          "original_form": "예전에 퍼즐앤드래곤이라는 게임도 했구",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011315.1.6",
          "form": "요샌 네모로직 게임을 하고있어요",
          "original_form": "요샌 네모로직 게임을 하고있어요",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011315.1.7",
          "form": "우와 네모모직은 어떻게 하는거에요~?",
          "original_form": "우와 네모모직은 어떻게 하는거에요~?",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100011315.1.8",
          "form": "숫자를 보고 네모 칸을 채우는거에요.",
          "original_form": "숫자를 보고 네모 칸을 채우는거에요.",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100011315.1.9",
          "form": "규칙이 어렵진 않아서 시간 때우고 하기 좋아요",
          "original_form": "규칙이 어렵진 않아서 시간 때우고 하기 좋아요",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100011315.1.10",
          "form": "오오~ 약간 계산도 해야되나요~?",
          "original_form": "오오~ 약간 계산도 해야되나요~?",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100011315.1.11",
          "form": "약간이요.",
          "original_form": "약간이요.",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100011315.1.12",
          "form": "퍼즐게임은 잘 안하시나요?",
          "original_form": "퍼즐게임은 잘 안하시나요?",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100011315.1.13",
          "form": "저는 게임을 자주하는편이아닌것같아서 ㅠ 사실 잘은 모르겠어요~",
          "original_form": "저는 게임을 자주하는편이아닌것같아서 ㅠ 사실 잘은 모르겠어요~",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100011315.1.14",
          "form": "옛날에 크아~? 아시나용?!",
          "original_form": "옛날에 크아~? 아시나용?!",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100011315.1.15",
          "form": "크아 재밌었죠. 요새두 하나요?",
          "original_form": "크아 재밌었죠. 요새두 하나요?",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100011315.1.16",
          "form": "ㅎㅎ아뇽 진짜 어렸을때 하고 요즘엔 못한것같네용ㅋㅋ",
          "original_form": "ㅎㅎ아뇽 진짜 어렸을때 하고 요즘엔 못한것같네용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100011315.1.17",
          "form": "크아나 카트많이했어욬ㅋㅋ",
          "original_form": "크아나 카트많이했어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100011315.1.18",
          "form": "카트는 요새도 가끔 해요ㅋㅋ",
          "original_form": "카트는 요새도 가끔 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100011315.1.19",
          "form": "모바일도 나와서.. 재밌더라구요",
          "original_form": "모바일도 나와서.. 재밌더라구요",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100011315.1.20",
          "form": "오오 모바일카트",
          "original_form": "오오 모바일카트",
          "speaker_id": "2",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100011315.1.21",
          "form": "옛날과 비슷한 느낌인가요?",
          "original_form": "옛날과 비슷한 느낌인가요?",
          "speaker_id": "2",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100011315.1.22",
          "form": "비슷한 거 같아요. 카트 속도가 더 빠르긴 하고요.",
