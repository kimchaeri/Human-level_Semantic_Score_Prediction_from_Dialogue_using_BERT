      "utterance": [
        {
          "id": "MDRW2100024893.1.1",
          "form": "김치도 좋아해?",
          "original_form": "김치도 좋아해?",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100024893.1.2",
          "form": "응ㅋㅋㅋ특이한건 잘 안먹지만...",
          "original_form": "응ㅋㅋㅋ특이한건 잘 안먹지만...",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100024893.1.3",
          "form": "특이한거?ㅋㅋ그게뭐지?",
          "original_form": "특이한거?ㅋㅋ그게뭐지?",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024893.1.4",
          "form": "파김치같은건 좀 부담스럽더라고ㅋㅋ",
          "original_form": "파김치같은건 좀 부담스럽더라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024893.1.5",
          "form": "아니야ㅋㅋ생각보다 맛있긴한데ㅋㅋㄱ",
          "original_form": "아니야ㅋㅋ생각보다 맛있긴한데ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024893.1.6",
          "form": "거부감 있는거 인정함",
          "original_form": "거부감 있는거 인정함",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024893.1.7",
          "form": "근데 부안집이라고 고깃집에서 파김치먹었는데",
          "original_form": "근데 부안집이라고 고깃집에서 파김치먹었는데",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024893.1.8",
          "form": "괜찮긴하더라고ㅋ",
          "original_form": "괜찮긴하더라고ㅋ",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024893.1.9",
          "form": "부안집은 대파김치?ㅋㅋ같은 느낌이지?ㅋㅋ",
          "original_form": "부안집은 대파김치?ㅋㅋ같은 느낌이지?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024893.1.10",
          "form": "첨엔 보고 뭐야했는데ㅋㅋ",
          "original_form": "첨엔 보고 뭐야했는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024893.1.11",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024893.1.12",
          "form": "맞아ㅋㅋ",
          "original_form": "맞아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024893.1.13",
          "form": "혹시 시중김치중 추천해줄거 있어?",
          "original_form": "혹시 시중김치중 추천해줄거 있어?",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024893.1.14",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024893.1.15",
          "form": "나는 종갓집이무난하더라",
          "original_form": "나는 종갓집이무난하더라",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024893.1.16",
          "form": "열무김치도 괜찮겠지?",
          "original_form": "열무김치도 괜찮겠지?",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024893.1.17",
          "form": "응ㅋㅋㅋ열무김치 괜춘",
          "original_form": "응ㅋㅋㅋ열무김치 괜춘",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024893.1.18",
          "form": "가끔 마트에서 싸게 팔더라고ㅋ",
          "original_form": "가끔 마트에서 싸게 팔더라고ㅋ",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024893.1.19",
          "form": "그때 한 번 먹어봐",
          "original_form": "그때 한 번 먹어봐",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024893.1.20",
          "form": "오키오키",
          "original_form": "오키오키",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
