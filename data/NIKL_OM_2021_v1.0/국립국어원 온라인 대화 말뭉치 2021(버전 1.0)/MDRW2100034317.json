      "utterance": [
        {
          "id": "MDRW2100034317.1.1",
          "form": "요대화는 제출안하시는게 좋을듯요ㅋ",
          "original_form": "요대화는 제출안하시는게 좋을듯요ㅋ",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034317.1.2",
          "form": "네페4만원은 적립했고",
          "original_form": "네페4만원은 적립했고",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034317.1.3",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034317.1.4",
          "form": "나머지깁콘은 아직사용전이요ㅎ",
          "original_form": "나머지깁콘은 아직사용전이요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034317.1.5",
          "form": "다 하면 네페만 9만원이네요",
          "original_form": "다 하면 네페만 9만원이네요",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034317.1.6",
          "form": "아마 150개까지는괜찮은데",
          "original_form": "아마 150개까지는괜찮은데",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034317.1.7",
          "form": "그담이 300개보상이라 텀이 넘길어서 고비넘기셔야할거예요ㅎ",
          "original_form": "그담이 300개보상이라 텀이 넘길어서 고비넘기셔야할거예요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034317.1.8",
          "form": "전주말에 바짝해서~  7-80명이랑 대화한듯요ㅎ",
          "original_form": "전주말에 바짝해서~  7-80명이랑 대화한듯요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034317.1.9",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034317.1.10",
          "form": "80명ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "80명ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034317.1.11",
          "form": "리스펙입니다",
          "original_form": "리스펙입니다",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034317.1.12",
          "form": "저는 첫날 35명이랑 하고",
          "original_form": "저는 첫날 35명이랑 하고",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034317.1.13",
          "form": "기 다 빨려서",
          "original_form": "기 다 빨려서",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034317.1.14",
          "form": "며칠째 안 히고 있어요",
          "original_form": "며칠째 안 히고 있어요",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034317.1.15",
          "form": "백신 맞아서 잠만 잔 것도 있지만",
          "original_form": "백신 맞아서 잠만 잔 것도 있지만",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034317.1.16",
          "form": "ㅋㅋ 한번하고 말없는분들도있고해서",
          "original_form": "ㅋㅋ 한번하고 말없는분들도있고해서",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100034317.1.17",
          "form": "백신맞으셨으니 며칠은 푹쉬시고",
          "original_form": "백신맞으셨으니 며칠은 푹쉬시고",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100034317.1.18",
          "form": "괜찮아지셨을때 하셔요ㅎ",
          "original_form": "괜찮아지셨을때 하셔요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100034317.1.19",
          "form": "네ㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100034317.1.20",
          "form": "보상 올때까진 열어둘거라 단답형 진행하고싶으심 가끔도와드릴게요",
          "original_form": "보상 올때까진 열어둘거라 단답형 진행하고싶으심 가끔도와드릴게요",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034317.1.21",
          "form": "맞아요 35명 중에",
          "original_form": "맞아요 35명 중에",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034317.1.22",
          "form": "한 25명 이상은 말이 없더라고요",
          "original_form": "한 25명 이상은 말이 없더라고요",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034317.1.23",
          "form": "헐 아니에요ㅜ 감사합니다",
          "original_form": "헐 아니에요ㅜ 감사합니다",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034317.1.24",
          "form": "같이하는친구있음 좋을텐데",
          "original_form": "같이하는친구있음 좋을텐데",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034317.1.25",
          "form": "친구 한명 같이 하긴 해요",
          "original_form": "친구 한명 같이 하긴 해요",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034317.1.26",
          "form": "그래서 전에 만나서",
          "original_form": "그래서 전에 만나서",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034317.1.27",
          "form": "제가 다른 일 할 때",
          "original_form": "제가 다른 일 할 때",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034317.1.28",
          "form": "친구가 제 폰까지 해서",
          "original_form": "친구가 제 폰까지 해서",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034317.1.29",
          "form": "단답으로 30개 채웠었어요 ㅋㅋㅋㅋ",
          "original_form": "단답으로 30개 채웠었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034317.1.30",
          "form": "그럼 딱 시간잡고 한두시간 ㅎ",
          "original_form": "그럼 딱 시간잡고 한두시간 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034317.1.31",
          "form": "날씨좋죠 네좋네요 요런식으로ㅣㅣ",
          "original_form": "날씨좋죠 네좋네요 요런식으로ㅣㅣ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034317.1.32",
          "form": "식사하셨어요? 네 뭐먹었어요 운동하시나요 무슨운동해요ㅎ 어떤가요 ? 재밌어요",
          "original_form": "식사하셨어요? 네 뭐먹었어요 운동하시나요 무슨운동해요ㅎ 어떤가요 ? 재밌어요",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034317.1.33",
          "form": "딱12턴되면 제출하고 또얘기하고",
          "original_form": "딱12턴되면 제출하고 또얘기하고",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034317.1.34",
          "form": "기계적이긴 한데 그게 맘편하고 빠른 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "기계적이긴 한데 그게 맘편하고 빠른 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034317.1.35",
          "form": "동일주제로 계속얘기하면서 턴수되면 제출하고",
          "original_form": "동일주제로 계속얘기하면서 턴수되면 제출하고",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034317.1.36",
          "form": "계속 대화이어가면서 또제출",
          "original_form": "계속 대화이어가면서 또제출",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034317.1.37",
          "form": "주제꼭안바꿔도 되더라구요",
          "original_form": "주제꼭안바꿔도 되더라구요",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034317.1.38",
          "form": "한분이랑 계속 쇼핑얘기했어요",
          "original_form": "한분이랑 계속 쇼핑얘기했어요",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034317.1.39",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034317.1.40",
          "form": "저도 한분이랑은 여팽 얘기만 했어요",
          "original_form": "저도 한분이랑은 여팽 얘기만 했어요",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034317.1.41",
          "form": "저랑 갔다온 나라가 다 똑같아서",
          "original_form": "저랑 갔다온 나라가 다 똑같아서",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034317.1.42",
          "form": "거기 언덕~ 어쩌구 다 회상하면서",
          "original_form": "거기 언덕~ 어쩌구 다 회상하면서",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034317.1.43",
          "form": "재밌더라고요 ㅋㅋㅋㅋ",
          "original_form": "재밌더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034317.1.44",
          "form": "대화잘통함 즐겁게 수다떨면서해도 재밌고",
          "original_form": "대화잘통함 즐겁게 수다떨면서해도 재밌고",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034317.1.45",
          "form": "500개까지 꼭달성하셔요ㅎ 100개에 5만원이니 넘쏠쏠해요ㅎ",
          "original_form": "500개까지 꼭달성하셔요ㅎ 100개에 5만원이니 넘쏠쏠해요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034317.1.46",
          "form": "네 기간 넉넉하니까 열심히 해봐야져 ㅎ",
          "original_form": "네 기간 넉넉하니까 열심히 해봐야져 ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034317.1.47",
          "form": "마자요 기간기니  쉬엄쉬엄ㅎ 전 이번주 넘피곤하더라구요 새벽늦게까지했더니",
          "original_form": "마자요 기간기니  쉬엄쉬엄ㅎ 전 이번주 넘피곤하더라구요 새벽늦게까지했더니",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034317.1.48",
          "form": "ㅋㅋㅋㅋ그래도 빠르게 하면 더 쾌감 있을 것 같아요",
          "original_form": "ㅋㅋㅋㅋ그래도 빠르게 하면 더 쾌감 있을 것 같아요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034317.1.49",
          "form": "그럼 다른분들이랑 대화해보셔요 저랑은 갯수도 안쌓이니~^^",
          "original_form": "그럼 다른분들이랑 대화해보셔요 저랑은 갯수도 안쌓이니~^^",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034317.1.50",
          "form": "이번에 백신 맞아서 나갈 일도 없는 겸 집에서 이거나 해봐야겠어요",
          "original_form": "이번에 백신 맞아서 나갈 일도 없는 겸 집에서 이거나 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034317.1.51",
          "form": "네넹ㅋㅋㅋㅋㅋ 고생 많으셨어요",
          "original_form": "네넹ㅋㅋㅋㅋㅋ 고생 많으셨어요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
