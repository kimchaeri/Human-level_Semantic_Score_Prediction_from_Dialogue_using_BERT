      "utterance": [
        {
          "id": "MDRW2100013503.1.1",
          "form": "name1님당근하시나요",
          "original_form": "&name1&님당근하시나요",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013503.1.2",
          "form": "아뇨 한 번도 안 해봤어요. 종종 하세요?",
          "original_form": "아뇨 한 번도 안 해봤어요. 종종 하세요?",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013503.1.3",
          "form": "필요한게 생기면 당근검색부터해요",
          "original_form": "필요한게 생기면 당근검색부터해요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013503.1.4",
          "form": "저렴하거든요",
          "original_form": "저렴하거든요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013503.1.5",
          "form": "팔기보단 주로 구매하는 편이신가봐요",
          "original_form": "팔기보단 주로 구매하는 편이신가봐요",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013503.1.6",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013503.1.7",
          "form": "이번에 버티컬마우스 싸게 사서 기뻤어요",
          "original_form": "이번에 버티컬마우스 싸게 사서 기뻤어요",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013503.1.8",
          "form": "버티컬 마우스는 어떻게 다른가요? 게임용?",
          "original_form": "버티컬 마우스는 어떻게 다른가요? 게임용?",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013503.1.9",
          "form": "마우스가 수평으로 단추가 있는데 버티컬은 세로로 세워져있어서 손목에 편해요",
          "original_form": "마우스가 수평으로 단추가 있는데 버티컬은 세로로 세워져있어서 손목에 편해요",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013503.1.10",
          "form": "신기하네요!",
          "original_form": "신기하네요!",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013503.1.11",
          "form": "저도 요즘 손목이 안 좋아서 아대나 보호대 같은 걸 살까 고민중이에요",
          "original_form": "저도 요즘 손목이 안 좋아서 아대나 보호대 같은 걸 살까 고민중이에요",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013503.1.12",
          "form": "네 그것도 좋네요컴터 많이 쓰는 사람은 마우스땜에 터널증후군이 오거든요 마우스바꿔야해요",
          "original_form": "네 그것도 좋네요컴터 많이 쓰는 사람은 마우스땜에 터널증후군이 오거든요 마우스바꿔야해요",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100013503.1.13",
          "form": "저도 좀 알아볼까봐요",
          "original_form": "저도 좀 알아볼까봐요",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100013503.1.14",
          "form": "가격이 얼마쯤 하나요?",
          "original_form": "가격이 얼마쯤 하나요?",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100013503.1.15",
          "form": "인터넷에는 삼마넌쯤인데 저는 당근에서 삼처넌에 샀어요",
          "original_form": "인터넷에는 삼마넌쯤인데 저는 당근에서 삼처넌에 샀어요",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013503.1.16",
          "form": "엄청 싸게 사셨네요!",
          "original_form": "엄청 싸게 사셨네요!",
          "speaker_id": "1",
          "time": "20210825 17:31"
        }
      ]
