      "utterance": [
        {
          "id": "MDRW2100018210.1.1",
          "form": "아 그 예전에",
          "original_form": "아 그 예전에",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.2",
          "form": "장진 감독이랑 송은이 나온 건가요?",
          "original_form": "장진 감독이랑 송은이 나온 건가요?",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.3",
          "form": "오 네네!",
          "original_form": "오 네네!",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.4",
          "form": "장진은 파일럿 때 나왔어요",
          "original_form": "장진은 파일럿 때 나왔어요",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.5",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.6",
          "form": "원래 이 시간에 꼬꼬무 하지 않았나요??",
          "original_form": "원래 이 시간에 꼬꼬무 하지 않았나요??",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.7",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.8",
          "form": "근데 정규편성 되면서",
          "original_form": "근데 정규편성 되면서",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.9",
          "form": "몇달 후에? 다음달?",
          "original_form": "몇달 후에? 다음달?",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.10",
          "form": "그때 돌아온다더라구요",
          "original_form": "그때 돌아온다더라구요",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018210.1.11",
          "form": "원래 정규편성 아니었어요?",
          "original_form": "원래 정규편성 아니었어요?",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.12",
          "form": "네 시즌제였대요??",
          "original_form": "네 시즌제였대요??",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.14",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.15",
          "form": "당혹사 보시나요",
          "original_form": "당혹사 보시나요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.16",
          "form": "아뇨 ㅋㅋ 계속 심괴 봐요",
          "original_form": "아뇨 ㅋㅋ 계속 심괴 봐요",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.17",
          "form": "ㅎㅎ 저도 일단 심야괴담회..",
          "original_form": "ㅎㅎ 저도 일단 심야괴담회..",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.18",
          "form": "네 당혹사는 다시보기로 첨부터 보려구요 ㅋㅋㅋ",
          "original_form": "네 당혹사는 다시보기로 첨부터 보려구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018210.1.19",
          "form": "뭔 사건현장까지 보여주네뇨..",
          "original_form": "뭔 사건현장까지 보여주네뇨..",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018210.1.20",
          "form": "네 왼전 별로",
          "original_form": "네 왼전 별로",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018210.1.21",
          "form": "무서운 거나 보여주지..",
          "original_form": "무서운 거나 보여주지..",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018210.1.22",
          "form": "네 다음편은 무서운 거였으면 좋겠네요..",
          "original_form": "네 다음편은 무서운 거였으면 좋겠네요..",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018210.1.23",
          "form": "저 심야괴담회 계속 보는데 ㅋㅋ",
          "original_form": "저 심야괴담회 계속 보는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018210.1.24",
          "form": "저 패널들 넘 귀엽지 않아요?",
          "original_form": "저 패널들 넘 귀엽지 않아요?",
          "speaker_id": "1",
          "time": "20210826 23:32"
        }
      ]
    }
