      "utterance": [
        {
          "id": "MDRW2100002092.1.1",
          "form": "최근에 오디 다녀왔어요?",
          "original_form": "최근에 오디 다녀왔어요?",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002092.1.2",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002092.1.3",
          "form": "아 남해 좋아요\n전 고향이 남해쪽이에요 ㅎㅎ",
          "original_form": "아 남해 좋아요\n전 고향이 남해쪽이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002092.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002092.1.5",
          "form": "너무 좋더라구요",
          "original_form": "너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002092.1.6",
          "form": "전 자주보니 와 좋다 라기보다는\n가면 마음이 편해여 ㅎㅎ",
          "original_form": "전 자주보니 와 좋다 라기보다는\n가면 마음이 편해여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002092.1.7",
          "form": "물도 깨끗하고",
          "original_form": "물도 깨끗하고",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002092.1.8",
          "form": "음식도 맛잇어여 ㅋ",
          "original_form": "음식도 맛잇어여 ㅋ",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002092.1.9",
          "form": "추천좀요",
          "original_form": "추천좀요",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002092.1.10",
          "form": "음저희쪽은 삼합 유명해요",
          "original_form": "음저희쪽은 삼합 유명해요",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002092.1.11",
          "form": "1박 2일때문에 더유명해졌는데",
          "original_form": "1박 2일때문에 더유명해졌는데",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002092.1.12",
          "form": "전라도 말고도 삼합있나보네여",
          "original_form": "전라도 말고도 삼합있나보네여",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002092.1.13",
          "form": "어 전라도여여 ㅋㅋ",
          "original_form": "어 전라도여여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002092.1.14",
          "form": "남해 음식 추천 하는 줄  ㅋㅋ",
          "original_form": "남해 음식 추천 하는 줄  ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002092.1.15",
          "form": "경남 남해군  이야기군요 ㅋ",
          "original_form": "경남 남해군  이야기군요 ㅋ",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002092.1.16",
          "form": "다른 남해도 있나요?",
          "original_form": "다른 남해도 있나요?",
          "speaker_id": "1",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002092.1.17",
          "form": "바다로 생각했어요 ㅎㅎ 서해 남해 동해 ㅋ",
          "original_form": "바다로 생각했어요 ㅎㅎ 서해 남해 동해 ㅋ",
          "speaker_id": "2",
          "time": "20210819 23:58"
        }
