      "utterance": [
        {
          "id": "MDRW2100040289.1.1",
          "form": "넵넵 다닙니다",
          "original_form": "넵넵 다닙니다",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040289.1.2",
          "form": "직장인이신가요",
          "original_form": "직장인이신가요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040289.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040289.1.4",
          "form": "어느쪽에 근무하세요?",
          "original_form": "어느쪽에 근무하세요?",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040289.1.5",
          "form": "전 교육계요ㅋㅋㅋ 어느 직종이신가요",
          "original_form": "전 교육계요ㅋㅋㅋ 어느 직종이신가요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040289.1.6",
          "form": "저는 영상쪽이요",
          "original_form": "저는 영상쪽이요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040289.1.7",
          "form": "교육계면 뭔가 엘리트 느낌이네요",
          "original_form": "교육계면 뭔가 엘리트 느낌이네요",
          "speaker_id": "1",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040289.1.8",
          "form": "엘리트...인가..ㅋㅋㅋ 영상계도 그렇지않나요",
          "original_form": "엘리트...인가..ㅋㅋㅋ 영상계도 그렇지않나요",
          "speaker_id": "2",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040289.1.9",
          "form": "영상쪽은 접근이 쉬워요 ㅋㅋ",
          "original_form": "영상쪽은 접근이 쉬워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100040289.1.10",
          "form": "진짜 큰 대기업 아니고서야 ᆢ",
          "original_form": "진짜 큰 대기업 아니고서야 ᆢ",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100040289.1.11",
          "form": "교육계통은 일할만한가요?",
          "original_form": "교육계통은 일할만한가요?",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100040289.1.12",
          "form": "넹 하고싶었던 직업이라 만족하며ㅋㅋ",
          "original_form": "넹 하고싶었던 직업이라 만족하며ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040289.1.13",
          "form": "진짜 하고픈일 하고사시는분 얼마없는데",
          "original_form": "진짜 하고픈일 하고사시는분 얼마없는데",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040289.1.14",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040289.1.15",
          "form": "꽤 오래 노력했어요 ㅋㅋ ㅠㅠ 버티니까 되긴되더라구요",
          "original_form": "꽤 오래 노력했어요 ㅋㅋ ㅠㅠ 버티니까 되긴되더라구요",
          "speaker_id": "2",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100040289.1.16",
          "form": "끈기가 대단하세요",
          "original_form": "끈기가 대단하세요",
          "speaker_id": "1",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100040289.1.17",
          "form": "저는 그냥 쉬운쪽 온건데 ㅜ",
          "original_form": "저는 그냥 쉬운쪽 온건데 ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100040289.1.18",
          "form": "하고싶었던 일은 아니세용?",
          "original_form": "하고싶었던 일은 아니세용?",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100040289.1.19",
          "form": "재밌을거 같아서 오긴한건데 힘드네요",
          "original_form": "재밌을거 같아서 오긴한건데 힘드네요",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100040289.1.20",
          "form": "영상을 찍는 쪽이에요?",
          "original_form": "영상을 찍는 쪽이에요?",
          "speaker_id": "2",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100040289.1.21",
          "form": "아뇨 디자인쪽이예요",
          "original_form": "아뇨 디자인쪽이예요",
          "speaker_id": "1",
          "time": "20210904 18:18"
        }
