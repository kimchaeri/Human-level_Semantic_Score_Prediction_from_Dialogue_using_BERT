      "utterance": [
        {
          "id": "MDRW2100026622.1.1",
          "form": "공연조아하세요?",
          "original_form": "공연조아하세요?",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026622.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026622.1.3",
          "form": "뮤지컬, 영화 보는거 좋아해요",
          "original_form": "뮤지컬, 영화 보는거 좋아해요",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026622.1.4",
          "form": "오 뮤지컬 ~ 추천해주세요",
          "original_form": "오 뮤지컬 ~ 추천해주세요",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026622.1.5",
          "form": "시카고도 재밌게 봤고",
          "original_form": "시카고도 재밌게 봤고",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026622.1.6",
          "form": "라이온킹 내한공연 진짜 멋졌어요",
          "original_form": "라이온킹 내한공연 진짜 멋졌어요",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026622.1.7",
          "form": "우왕...라이온킹 !ㅜㅜ 자도 가고싶어요 관심가져바야겟네요",
          "original_form": "우왕...라이온킹 !ㅜㅜ 자도 가고싶어요 관심가져바야겟네요",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026622.1.8",
          "form": "내한공연오면 꼭 가보세요ㅎㅎ",
          "original_form": "내한공연오면 꼭 가보세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026622.1.9",
          "form": "공연 좋아하시나요?",
          "original_form": "공연 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026622.1.10",
          "form": "넹넹 .. 문화생활 좋죵 ㅋㅋㅋㅋ",
          "original_form": "넹넹 .. 문화생활 좋죵 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026622.1.11",
          "form": "타켓오픈 날짜 계속주시해야해서",
          "original_form": "타켓오픈 날짜 계속주시해야해서",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026622.1.12",
          "form": "그게어려운거같아요",
          "original_form": "그게어려운거같아요",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026622.1.13",
          "form": "맞아요ㅜㅜ 그래서 사실 영화를 제일 많이 보는듯ㅎㅎ",
          "original_form": "맞아요ㅜㅜ 그래서 사실 영화를 제일 많이 보는듯ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026622.1.14",
          "form": "오늘 모가디슈봣어요",
          "original_form": "오늘 모가디슈봣어요",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026622.1.15",
          "form": "오 저 그거 엄청 보고싶어요 어때요?",
          "original_form": "오 저 그거 엄청 보고싶어요 어때요?",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026622.1.16",
          "form": "ㅋㅋㅋㅋ 재밌던데용 요즘 그 아프간 생각나구..",
          "original_form": "ㅋㅋㅋㅋ 재밌던데용 요즘 그 아프간 생각나구..",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026622.1.17",
          "form": "긴박...",
          "original_form": "긴박...",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026622.1.18",
          "form": "아프간ㅜㅜ 마음 아파요",
          "original_form": "아프간ㅜㅜ 마음 아파요",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026622.1.19",
          "form": "영화관에 요즘 사람 많나요?",
          "original_form": "영화관에 요즘 사람 많나요?",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026622.1.20",
          "form": "오늘은 많던데용",
          "original_form": "오늘은 많던데용",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026622.1.21",
