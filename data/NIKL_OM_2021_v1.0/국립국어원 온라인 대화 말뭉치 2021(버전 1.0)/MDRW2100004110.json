      "utterance": [
        {
          "id": "MDRW2100004110.1.1",
          "form": "혹시 어떤 음식 좋아하세요??",
          "original_form": "혹시 어떤 음식 좋아하세요??",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004110.1.2",
          "form": "콩국수좋아해요!",
          "original_form": "콩국수좋아해요!",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004110.1.3",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004110.1.4",
          "form": "오 콩국수 맛있죠. 저는 냉면이요.!!",
          "original_form": "오 콩국수 맛있죠. 저는 냉면이요.!!",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004110.1.5",
          "form": "밀면도 좋아하고..",
          "original_form": "밀면도 좋아하고..",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004110.1.6",
          "form": "여름에 면음식은 뭐든 진리인거 같아요!!",
          "original_form": "여름에 면음식은 뭐든 진리인거 같아요!!",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004110.1.7",
          "form": "저도밀가루좋아해서요!ㅋ",
          "original_form": "저도밀가루좋아해서요!ㅋ",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004110.1.8",
          "form": "저도요!!",
          "original_form": "저도요!!",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004110.1.9",
          "form": "저녁은 뭐드세요??",
          "original_form": "저녁은 뭐드세요??",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004110.1.10",
          "form": "돈까스먹으러갈까생각중이에요",
          "original_form": "돈까스먹으러갈까생각중이에요",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004110.1.11",
          "form": "외식.ㅠㅠ",
          "original_form": "외식.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004110.1.12",
          "form": "너무 부럽습니다.",
          "original_form": "너무 부럽습니다.",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004110.1.13",
          "form": "몇주만인지ㅠ.ㅠ",
          "original_form": "몇주만인지ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004110.1.14",
          "form": "근데 또 몰라영.. 남친만나면 얘기가달라질수도",
          "original_form": "근데 또 몰라영.. 남친만나면 얘기가달라질수도",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004110.1.15",
          "form": "데이트 하시면 카페 같은 곳고 자주 가시겠어요",
          "original_form": "데이트 하시면 카페 같은 곳고 자주 가시겠어요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004110.1.16",
          "form": "아뇨 테이크아웃만!해요",
          "original_form": "아뇨 테이크아웃만!해요",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004110.1.17",
          "form": "어떤 음료 드세요??",
          "original_form": "어떤 음료 드세요??",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004110.1.18",
          "form": "저는 테이크 아웃 하러 나간것도 코로나 이전이라.ㅠㅠ",
          "original_form": "저는 테이크 아웃 하러 나간것도 코로나 이전이라.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004110.1.19",
          "form": "전 커피먹죠~~ㅎㅎ 결국 낙지볶음먹으러갑니당",
          "original_form": "전 커피먹죠~~ㅎㅎ 결국 낙지볶음먹으러갑니당",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004110.1.20",
          "form": "크 너무 부럽습니다.ㅠㅠ",
          "original_form": "크 너무 부럽습니다.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004110.1.21",
