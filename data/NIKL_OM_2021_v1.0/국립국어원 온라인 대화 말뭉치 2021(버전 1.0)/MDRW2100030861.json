      "utterance": [
        {
          "id": "MDRW2100030861.1.1",
          "form": "백신을 이미 맞았지만요 ㅎㅎ",
          "original_form": "백신을 이미 맞았지만요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030861.1.2",
          "form": "예약하셨죠?",
          "original_form": "예약하셨죠?",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030861.1.3",
          "form": "네 어제 맞았어요~ 예약 하셨나요?",
          "original_form": "네 어제 맞았어요~ 예약 하셨나요?",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030861.1.4",
          "form": "넹 저는 10부제는 아니고 직업상 더 일찍 예약했어요",
          "original_form": "넹 저는 10부제는 아니고 직업상 더 일찍 예약했어요",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030861.1.5",
          "form": "아항 그럼 2차까지 다 맞으신건가요?",
          "original_form": "아항 그럼 2차까지 다 맞으신건가요?",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030861.1.6",
          "form": "오늘 오전에 2차 맞고 왔어요!",
          "original_form": "오늘 오전에 2차 맞고 왔어요!",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100030861.1.7",
          "form": "오옷",
          "original_form": "오옷",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100030861.1.8",
          "form": "불편한곳은 없으신가요?",
          "original_form": "불편한곳은 없으신가요?",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100030861.1.9",
          "form": "네 아직은 팔만 좀 아픈 거 같아요",
          "original_form": "네 아직은 팔만 좀 아픈 거 같아요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030861.1.10",
          "form": "다행이에요 2차인데 무탈하게 넘어 갔으면 좋겟네요",
          "original_form": "다행이에요 2차인데 무탈하게 넘어 갔으면 좋겟네요",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100030861.1.11",
          "form": "직업상 일찍 맞으신거면 혹시 선생님이신가요?",
          "original_form": "직업상 일찍 맞으신거면 혹시 선생님이신가요?",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100030861.1.12",
          "form": "넹넹ㅋㅋㅋ",
          "original_form": "넹넹ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100030861.1.13",
          "form": "그간 고생이 많으셨겠네요ㅠ 수업준비도 그렇구요ㅜㅜ",
          "original_form": "그간 고생이 많으셨겠네요ㅠ 수업준비도 그렇구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100030861.1.14",
          "form": "아 작년엔 진짜 좀 그랬는데 또 어느새 적응해서 올핸 좀 수월했어요",
          "original_form": "아 작년엔 진짜 좀 그랬는데 또 어느새 적응해서 올핸 좀 수월했어요",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100030861.1.15",
          "form": "애들이 고생하네요ㅋㅋ",
          "original_form": "애들이 고생하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100030861.1.16",
          "form": "선생님도 고생이죠.. 애들은 놀지도 못하고 .. 백신도 못 맞으니까요..",
          "original_form": "선생님도 고생이죠.. 애들은 놀지도 못하고 .. 백신도 못 맞으니까요..",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100030861.1.17",
          "form": "백신 맞으면 전염력이 좀 떨어진다길래 차례가 오자마자 신청했어요",
          "original_form": "백신 맞으면 전염력이 좀 떨어진다길래 차례가 오자마자 신청했어요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        }
      ]
