      "utterance": [
        {
          "id": "MDRW2100031313.1.1",
          "form": "한달에 두번은 주말에 나가야해요 ㅎ",
          "original_form": "한달에 두번은 주말에 나가야해요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031313.1.2",
          "form": "당직하고오면 기빨려요",
          "original_form": "당직하고오면 기빨려요",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031313.1.3",
          "form": "나 요즘 일하느라 너무 피곤해서  먹고자고 반복이야",
          "original_form": "나 요즘 일하느라 너무 피곤해서  먹고자고 반복이야",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031313.1.4",
          "form": "ㅋㅋㅋ아 너구나",
          "original_form": "ㅋㅋㅋ아 너구나",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031313.1.5",
          "form": "여러사람이랑 이야기하다보니까ㅋㅋ약간 기계적으로 대답함",
          "original_form": "여러사람이랑 이야기하다보니까ㅋㅋ약간 기계적으로 대답함",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031313.1.6",
          "form": "맞아 ㅎ 피곤하지",
          "original_form": "맞아 ㅎ 피곤하지",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031313.1.7",
          "form": "일은 왜 해야하는걸까",
          "original_form": "일은 왜 해야하는걸까",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031313.1.8",
          "form": "주식 메꾸려고요.... ㅋㅋㅋ",
          "original_form": "주식 메꾸려고요.... ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031313.1.9",
          "form": "아...눈물 ........",
          "original_form": "아...눈물 ........",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031313.1.10",
          "form": "급여 한 두배로 받고싶다",
          "original_form": "급여 한 두배로 받고싶다",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031313.1.11",
          "form": "ㅋㅋ  두배받아도\r\n티끌이   티끌이죠",
          "original_form": "ㅋㅋ  두배받아도\r\n티끌이   티끌이죠",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100031313.1.12",
          "form": "난 급여 한  2000  받고싶다",
          "original_form": "난 급여 한  2000  받고싶다",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100031313.1.13",
          "form": "ㅋㅋㅋㄱ그렇군요",
          "original_form": "ㅋㅋㅋㄱ그렇군요",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031313.1.14",
          "form": "그거 참좋다",
          "original_form": "그거 참좋다",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031313.1.15",
          "form": "급여 2000이라니",
          "original_form": "급여 2000이라니",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031313.1.16",
          "form": "그치ㅋㅋ  근데 요즘도 평가 많이 나와?",
          "original_form": "그치ㅋㅋ  근데 요즘도 평가 많이 나와?",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031313.1.17",
          "form": "아니 ㅎ 코로나가 좋은점은 이거다",
          "original_form": "아니 ㅎ 코로나가 좋은점은 이거다",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031313.1.18",
          "form": "그래도 늘상 오던놈은 온다",
          "original_form": "그래도 늘상 오던놈은 온다",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100031313.1.19",
          "form": "ㅋㅋㅋㄱㄱ 욜~  우리회사에 직원한명때문에",
          "original_form": "ㅋㅋㅋㄱㄱ 욜~  우리회사에 직원한명때문에",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031313.1.20",
          "form": "전직원이 스트레스받는중이야",
          "original_form": "전직원이 스트레스받는중이야",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031313.1.21",
          "form": "와이?!",
          "original_form": "와이?!",
          "speaker_id": "2",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100031313.1.22",
          "form": "어떤직원이길래",
          "original_form": "어떤직원이길래",
          "speaker_id": "2",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100031313.1.23",
          "form": "매우 느리고 본인밖에모르는 이기적인직원이야",
          "original_form": "매우 느리고 본인밖에모르는 이기적인직원이야",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100031313.1.24",
          "form": "어우... 그런사람들 특징이 본인은 편하지",
          "original_form": "어우... 그런사람들 특징이 본인은 편하지",
          "speaker_id": "2",
          "time": "20210905 12:57"
        },
        {
          "id": "MDRW2100031313.1.25",
          "form": "응  진짜  답답하고 짜증나고.. 다른직원들이 그직원한테 그만두라고할정도야",
          "original_form": "응  진짜  답답하고 짜증나고.. 다른직원들이 그직원한테 그만두라고할정도야",
          "speaker_id": "1",
          "time": "20210905 13:03"
        }
      ]
    }
