      "utterance": [
        {
          "id": "MDRW2100025071.1.1",
          "form": "그런데 경기도민에게",
          "original_form": "그런데 경기도민에게",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.2",
          "form": "홍대는",
          "original_form": "홍대는",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.3",
          "form": "가혹해여",
          "original_form": "가혹해여",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.4",
          "form": "너무 멀져",
          "original_form": "너무 멀져",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.5",
          "form": "ㅠㅠ 잘알지여",
          "original_form": "ㅠㅠ 잘알지여",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.6",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.7",
          "form": "저희동네서도 한시간넘어서",
          "original_form": "저희동네서도 한시간넘어서",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.8",
          "form": "홍대에 맛있는 거 많은데 흑흑",
          "original_form": "홍대에 맛있는 거 많은데 흑흑",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025071.1.9",
          "form": "저도 잘 못가는 홍대...",
          "original_form": "저도 잘 못가는 홍대...",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.10",
          "form": "홍대 딱 세번 가봣어요",
          "original_form": "홍대 딱 세번 가봣어요",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.11",
          "form": "한번은 클럽 한번은 프리모바치오 한번은 우쿨렐레사러",
          "original_form": "한번은 클럽 한번은 프리모바치오 한번은 우쿨렐레사러",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.12",
          "form": "홍대 개미집이",
          "original_form": "홍대 개미집이",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.13",
          "form": "그렇게 맛잇다믄사요?!",
          "original_form": "그렇게 맛잇다믄사요?!",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.14",
          "form": "아 거기도 맛있어요!!",
          "original_form": "아 거기도 맛있어요!!",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.15",
          "form": "그치만 프차화되고 다른집은 그냥그렇더라구요",
          "original_form": "그치만 프차화되고 다른집은 그냥그렇더라구요",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.16",
          "form": "본점이랑ㄷ7ㄹ라서",
          "original_form": "본점이랑ㄷ7ㄹ라서",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.17",
          "form": "스타필드에 들어와있더라구여",
          "original_form": "스타필드에 들어와있더라구여",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.18",
          "form": "그쳐 그럴거같아서",
          "original_form": "그쳐 그럴거같아서",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.19",
          "form": "안먹었어요ㅠㅠ",
          "original_form": "안먹었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025071.1.20",
          "form": "혹시 강남오실일 있는데",
          "original_form": "혹시 강남오실일 있는데",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.21",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.22",
          "form": "돈까스가먹고싶다 는 정돈",
          "original_form": "돈까스가먹고싶다 는 정돈",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.23",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.24",
          "form": "피자가먹고싶다 하면 모터시티",
          "original_form": "피자가먹고싶다 하면 모터시티",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.25",
          "form": "진지하게",
          "original_form": "진지하게",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.26",
          "form": "돈까스는 항상 먹고 싶어야 정상 아닌가요?",
          "original_form": "돈까스는 항상 먹고 싶어야 정상 아닌가요?",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.27",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025071.1.28",
          "form": "정돈 여기",
          "original_form": "정돈 여기",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025071.1.29",
          "form": "완전미쳤어요....",
          "original_form": "완전미쳤어요....",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025071.1.30",
          "form": "핵맛있음...",
          "original_form": "핵맛있음...",
          "speaker_id": "2",
          "time": "20210831 22:53"
        }
      ]
    }
