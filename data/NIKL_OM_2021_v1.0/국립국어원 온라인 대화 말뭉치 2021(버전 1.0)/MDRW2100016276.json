        {
          "id": "MDRW2100016276.1.1",
          "form": "안녕하세요, 운동 좋아하시나요?",
          "original_form": "안녕하세요, 운동 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016276.1.2",
          "form": "네 매일 gx하러 체육관 가요",
          "original_form": "네 매일 gx하러 체육관 가요",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016276.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016276.1.4",
          "form": "너는 홈트해요",
          "original_form": "너는 홈트해요",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016276.1.5",
          "form": "저는 홈트해요",
          "original_form": "저는 홈트해요",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016276.1.6",
          "form": "요가 같은거 하세요?",
          "original_form": "요가 같은거 하세요?",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016276.1.7",
          "form": "요가는 가능한 자세가 별로 없어요..ㅠㅠ",
          "original_form": "요가는 가능한 자세가 별로 없어요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016276.1.8",
          "form": "아 그래요ㅎㅎ",
          "original_form": "아 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016276.1.9",
          "form": "하다보면 우연해 지겠죠",
          "original_form": "하다보면 우연해 지겠죠",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016276.1.10",
          "form": "저는 요즘 다이어트하려고",
          "original_form": "저는 요즘 다이어트하려고",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016276.1.11",
          "form": "식단 조절도 시작했어요",
          "original_form": "식단 조절도 시작했어요",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016276.1.12",
          "form": "식이까지 하시면 힘들 않으세요?",
          "original_form": "식이까지 하시면 힘들 않으세요?",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016276.1.13",
          "form": "식이요법이 운동보다 더힘들어요",
          "original_form": "식이요법이 운동보다 더힘들어요",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016276.1.14",
          "form": "name1은 운동의 목적이 뭐에요?",
          "original_form": "&name1&은 운동의 목적이 뭐에요?",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016276.1.15",
          "form": "저는 근육와 코어 생성이요! 체력이 부족함을 느껴서요",
          "original_form": "저는 근육와 코어 생성이요! 체력이 부족함을 느껴서요",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016276.1.16",
          "form": "오 좋다 코어근육 중요하죠",
          "original_form": "오 좋다 코어근육 중요하죠",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016276.1.17",
          "form": "근데 잘 안 생기네요ㅠ",
          "original_form": "근데 잘 안 생기네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016276.1.18",
          "form": "꾸준히하면 생기겠죠 힘내세요",
          "original_form": "꾸준히하면 생기겠죠 힘내세요",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016276.1.19",
          "form": "운동하고 근력이랑 지구력 좋아졌나요?",
          "original_form": "운동하고 근력이랑 지구력 좋아졌나요?",
