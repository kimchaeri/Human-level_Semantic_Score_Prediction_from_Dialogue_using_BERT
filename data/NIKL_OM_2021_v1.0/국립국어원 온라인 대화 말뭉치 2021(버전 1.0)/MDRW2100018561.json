        },
        {
          "id": "MDRW2100018561.1.4",
          "form": "학교다시나요",
          "original_form": "학교다시나요",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018561.1.5",
          "form": "아뇨 저도 안다녀요ㅋㅋ",
          "original_form": "아뇨 저도 안다녀요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018561.1.6",
          "form": "학교 다닐 때",
          "original_form": "학교 다닐 때",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018561.1.7",
          "form": "발표 잘하셨어요?",
          "original_form": "발표 잘하셨어요?",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018561.1.8",
          "form": "아니요.. 그렇게까지 잘하지는 않았던거같은데",
          "original_form": "아니요.. 그렇게까지 잘하지는 않았던거같은데",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018561.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018561.1.10",
          "form": "아무래도 사람들 앞에 선다는게.. 좀 부담이죠",
          "original_form": "아무래도 사람들 앞에 선다는게.. 좀 부담이죠",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018561.1.11",
          "form": "전 발표를 너무 싫어했어요",
          "original_form": "전 발표를 너무 싫어했어요",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018561.1.12",
          "form": "아마 다들 그랬을거같아요",
          "original_form": "아마 다들 그랬을거같아요",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018561.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 좋아하는 사람들 보면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 좋아하는 사람들 보면",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018561.1.14",
          "form": "너무 신기해요",
          "original_form": "너무 신기해요",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018561.1.15",
          "form": "학교 다닐 때 식당은 어땠어요",
          "original_form": "학교 다닐 때 식당은 어땠어요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018561.1.16",
          "form": "저는 완전 별로..ㅎ",
          "original_form": "저는 완전 별로..ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018561.1.17",
          "form": "ㅋㅋㅋ전 그냥저냥..",
          "original_form": "ㅋㅋㅋ전 그냥저냥..",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018561.1.18",
          "form": "보통이였어요",
          "original_form": "보통이였어요",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018561.1.19",
          "form": "그러셨구나 식당밥이 보통쯤만되도 좋았을텐데ㅎ",
          "original_form": "그러셨구나 식당밥이 보통쯤만되도 좋았을텐데ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018561.1.20",
          "form": "학교 다닐 때 뭐가 가장 기억에 남으세요",
          "original_form": "학교 다닐 때 뭐가 가장 기억에 남으세요",
