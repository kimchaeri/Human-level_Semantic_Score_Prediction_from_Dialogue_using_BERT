      "utterance": [
        {
          "id": "MDRW2100014586.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014586.1.2",
          "form": "직접하는건 좋아하는데 관람은 잘 안해봤어요",
          "original_form": "직접하는건 좋아하는데 관람은 잘 안해봤어요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100014586.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100014586.1.4",
          "form": "수영 잘하실거같은데 ㅎㅎ",
          "original_form": "수영 잘하실거같은데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100014586.1.5",
          "form": "수영 좋아해요",
          "original_form": "수영 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014586.1.6",
          "form": "코로나로 못 했다니 숨이 차여ㅠ",
          "original_form": "코로나로 못 했다니 숨이 차여ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014586.1.7",
          "form": "아이고 ㅠㅠ 수영장은 지금 운영을 안하던가요?",
          "original_form": "아이고 ㅠㅠ 수영장은 지금 운영을 안하던가요?",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100014586.1.8",
          "form": "넵 코로나로 운영을 안 하더라구요",
          "original_form": "넵 코로나로 운영을 안 하더라구요",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100014586.1.9",
          "form": "에궁",
          "original_form": "에궁",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100014586.1.10",
          "form": "저도 수영배우고싶은데",
          "original_form": "저도 수영배우고싶은데",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100014586.1.11",
          "form": "마땅치가않네요 ..",
          "original_form": "마땅치가않네요 ..",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100014586.1.12",
          "form": "마자요 뭔가 땀을 안 흘려서 그런가 제일 좋아요 ㅋㅋㅋ",
          "original_form": "마자요 뭔가 땀을 안 흘려서 그런가 제일 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100014586.1.13",
          "form": "그쵸 바로 씻을수도 있고 ㅋㅋㅋㅋ",
          "original_form": "그쵸 바로 씻을수도 있고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100014586.1.14",
          "form": "보기만해도 시원한 스포츠잔ㄹ아요",
          "original_form": "보기만해도 시원한 스포츠잔ㄹ아요",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100014586.1.15",
          "form": "맞아요 바로 씻는 것도 좋아요",
          "original_form": "맞아요 바로 씻는 것도 좋아요",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100014586.1.16",
          "form": "다른 건 땀 식힌다는 핑계로 느적느적하게 되도라구요",
          "original_form": "다른 건 땀 식힌다는 핑계로 느적느적하게 되도라구요",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100014586.1.17",
          "form": "저는 핼스장 다니는데 코로나때문에 샤워 못해서 죽갰어요 ㅋㅋ",
          "original_form": "저는 핼스장 다니는데 코로나때문에 샤워 못해서 죽갰어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100014586.1.18",
          "form": "진짜 바로 샤워하고 집에와야 개운한데ㅠㅠ",
          "original_form": "진짜 바로 샤워하고 집에와야 개운한데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100014586.1.19",
          "form": "어우 이 카테고리 끝나고 건강으로 옮겨갑시다 ㅋㅋㅋㅋ",
          "original_form": "어우 이 카테고리 끝나고 건강으로 옮겨갑시다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100014586.1.20",
          "form": "경기 직관햐본거 있어요?",
          "original_form": "경기 직관햐본거 있어요?",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100014586.1.21",
          "form": "야구경기는 직관해봤아요!!!",
          "original_form": "야구경기는 직관해봤아요!!!",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100014586.1.22",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100014586.1.23",
          "form": "저는 농구요!",
          "original_form": "저는 농구요!",
          "speaker_id": "2",
          "time": "20210826 22:08"
        }
      ]
    }
