      "utterance": [
        {
          "id": "MDRW2100043125.1.1",
          "form": "여행좋아하시나요 ㅎㅎ?",
          "original_form": "여행좋아하시나요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210907 14:36"
        },
        {
          "id": "MDRW2100043125.1.2",
          "form": "네! 여행좋아하세요?",
          "original_form": "네! 여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100043125.1.3",
          "form": "저도 좋아하는데 아직 많이 안가봤어요 ㅎㅎ",
          "original_form": "저도 좋아하는데 아직 많이 안가봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100043125.1.4",
          "form": "제주도 가보신적있나요?",
          "original_form": "제주도 가보신적있나요?",
          "speaker_id": "1",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100043125.1.5",
          "form": "제주도 수학여행때 가보고 못가봤어요",
          "original_form": "제주도 수학여행때 가보고 못가봤어요",
          "speaker_id": "2",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100043125.1.6",
          "form": "제주도 가보셨어요?",
          "original_form": "제주도 가보셨어요?",
          "speaker_id": "2",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100043125.1.7",
          "form": "오..!!",
          "original_form": "오..!!",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043125.1.8",
          "form": "저는 어릴 때 가족여행으로 가본게 끝이에요 ㅎㅎㅠㅠ",
          "original_form": "저는 어릴 때 가족여행으로 가본게 끝이에요 ㅎㅎㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043125.1.9",
          "form": "다른 여행지 다녀온 곳 있으신가요?",
          "original_form": "다른 여행지 다녀온 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043125.1.10",
          "form": "그러시구나!",
          "original_form": "그러시구나!",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043125.1.11",
          "form": "저는 국내는 전주 경주 안동 하동 평창 가평",
          "original_form": "저는 국내는 전주 경주 안동 하동 평창 가평",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043125.1.12",
          "form": "지금기억나는건 이정도 예요",
          "original_form": "지금기억나는건 이정도 예요",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043125.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043125.1.14",
          "form": "친구랑 다녀오신건가요?",
          "original_form": "친구랑 다녀오신건가요?",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043125.1.15",
          "form": "친구랑도 다녀왔고 전애인들과도 다녀왔죠",
          "original_form": "친구랑도 다녀왔고 전애인들과도 다녀왔죠",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043125.1.16",
          "form": "보통 친구들과 갔어요",
          "original_form": "보통 친구들과 갔어요",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043125.1.17",
          "form": "name1님은 어디 가보셨어요?",
          "original_form": "&name1&님은 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043125.1.18",
          "form": "오오..!",
          "original_form": "오오..!",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043125.1.19",
          "form": "저는 진짜 서울시내가 끝이에요ㅠㅠ",
          "original_form": "저는 진짜 서울시내가 끝이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043125.1.20",
          "form": "전주부산",
          "original_form": "전주부산",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043125.1.21",
          "form": "같이 유명한 곳만 한두번씩 다녀왔어요 ㅎㄹ",
          "original_form": "같이 유명한 곳만 한두번씩 다녀왔어요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043125.1.22",
          "form": "애인이랑 여행지 추천해주실 수 있울까요..?",
          "original_form": "애인이랑 여행지 추천해주실 수 있울까요..?",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043125.1.23",
          "form": "애인이랑은 경주 좋을거같아요",
          "original_form": "애인이랑은 경주 좋을거같아요",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043125.1.24",
          "form": "사진찍기도 좋고 볼거리도 많고",
          "original_form": "사진찍기도 좋고 볼거리도 많고",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043125.1.25",
          "form": "밤에 안압지나 첨성대 쪽은 분위기 좋아요",
          "original_form": "밤에 안압지나 첨성대 쪽은 분위기 좋아요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043125.1.26",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043125.1.27",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043125.1.28",
          "form": "근데 둘다 서울이라",
          "original_form": "근데 둘다 서울이라",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043125.1.29",
          "form": "혹시 가보신 곳 중이",
          "original_form": "혹시 가보신 곳 중이",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043125.1.30",
          "form": "가평어떠셨는지 여쭤봐도될까요?",
          "original_form": "가평어떠셨는지 여쭤봐도될까요?",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043125.1.31",
          "form": "가평은 대학친구들하고 갔었는데요",
          "original_form": "가평은 대학친구들하고 갔었는데요",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043125.1.32",
          "form": "수상레저가 재밌고 바비큐먹기좋았어요",
          "original_form": "수상레저가 재밌고 바비큐먹기좋았어요",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043125.1.33",
          "form": "수상레저 !!",
          "original_form": "수상레저 !!",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043125.1.34",
          "form": "근데 둘보단 단체가 더 재밌을거 같아요!",
          "original_form": "근데 둘보단 단체가 더 재밌을거 같아요!",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043125.1.35",
          "form": "어떤거하셨어요?",
          "original_form": "어떤거하셨어요?",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043125.1.36",
          "form": "빠지같은거 말씀하신건가용?",
          "original_form": "빠지같은거 말씀하신건가용?",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043125.1.37",
          "form": "바나나보트도 타고",
          "original_form": "바나나보트도 타고",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043125.1.38",
          "form": "이것저것탔는데.ㅈ..이름이 기억이 안나요ㅠ",
          "original_form": "이것저것탔는데.ㅈ..이름이 기억이 안나요ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043125.1.39",
          "form": "아아아 저도 뭔지알것같아요 ㅎㅎㅎ",
          "original_form": "아아아 저도 뭔지알것같아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043125.1.40",
          "form": "한번 빠지가봤어요!!",
          "original_form": "한번 빠지가봤어요!!",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043125.1.41",
          "form": "맞아요 다같이 가는게 재밌죠 ㅎㅎ",
          "original_form": "맞아요 다같이 가는게 재밌죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043125.1.42",
          "form": "평창은 어떠셨어요?",
          "original_form": "평창은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043125.1.43",
          "form": "평창 저는 동계올림픽때 가봤어요",
          "original_form": "평창 저는 동계올림픽때 가봤어요",
          "speaker_id": "2",
