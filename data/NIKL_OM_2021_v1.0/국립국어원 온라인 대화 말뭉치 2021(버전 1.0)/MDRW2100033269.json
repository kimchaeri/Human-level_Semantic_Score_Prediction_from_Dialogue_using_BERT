      "utterance": [
        {
          "id": "MDRW2100033269.1.1",
          "form": "집콕 좋으세요?",
          "original_form": "집콕 좋으세요?",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100033269.1.2",
          "form": "좋아해요ㅜㅜ",
          "original_form": "좋아해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100033269.1.3",
          "form": "출근안하면 거의 집에만있는편이에요",
          "original_form": "출근안하면 거의 집에만있는편이에요",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100033269.1.4",
          "form": "ㅋㅋ날이 좋아도요??ㅜ",
          "original_form": "ㅋㅋ날이 좋아도요??ㅜ",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100033269.1.5",
          "form": "집에 마당이 있어서 그런지",
          "original_form": "집에 마당이 있어서 그런지",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100033269.1.6",
          "form": "그냥 날좋아도 집에있어요ㅋㅋㅋ",
          "original_form": "그냥 날좋아도 집에있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100033269.1.7",
          "form": "찐 집순이시군요!",
          "original_form": "찐 집순이시군요!",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100033269.1.8",
          "form": "저도 나갔다 금방 들어와요 광합성만하고ㅋㅋ",
          "original_form": "저도 나갔다 금방 들어와요 광합성만하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100033269.1.9",
          "form": "오오ㅋㅋㅋ",
          "original_form": "오오ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100033269.1.10",
          "form": "나가면 너무 지쳐요ㅋㅋㅋ",
          "original_form": "나가면 너무 지쳐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100033269.1.11",
          "form": "밖에서 오래있음 기빨려요",
          "original_form": "밖에서 오래있음 기빨려요",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100033269.1.12",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100033269.1.13",
          "form": "집에서 요리해먹고 아이패드로 드라마 영화 실컷보다가 게임하고 그래요ㅋㅋ",
          "original_form": "집에서 요리해먹고 아이패드로 드라마 영화 실컷보다가 게임하고 그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100033269.1.14",
          "form": "저도 올레티비로 예능돌리고 베이킹하고 청소하고",
          "original_form": "저도 올레티비로 예능돌리고 베이킹하고 청소하고",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100033269.1.15",
          "form": "그럼 하루 후딱이죠",
          "original_form": "그럼 하루 후딱이죠",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100033269.1.16",
          "form": "우와 베이킹도하세요?",
          "original_form": "우와 베이킹도하세요?",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100033269.1.17",
          "form": "대박 멋있으시다",
          "original_form": "대박 멋있으시다",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100033269.1.18",
          "form": "ㅋㅋ 많이 먹을라고ㅋㅋ사먹는건 감질나서요ㅋㅋ",
          "original_form": "ㅋㅋ 많이 먹을라고ㅋㅋ사먹는건 감질나서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:03"
        },
        {
          "id": "MDRW2100033269.1.19",
          "form": "앜ㅋㅋㅋㅋ 맞아요",
          "original_form": "앜ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100033269.1.20",
          "form": "주로 뭐만드세영?",
          "original_form": "주로 뭐만드세영?",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100033269.1.21",
          "form": "쿠키랑 파운드요!",
          "original_form": "쿠키랑 파운드요!",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100033269.1.22",
          "form": "우와 고수시네요!!!",
          "original_form": "우와 고수시네요!!!",
          "speaker_id": "1",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100033269.1.23",
          "form": "근데 제멋대로해서 맛도 매번 들쑥날쑥해요",
          "original_form": "근데 제멋대로해서 맛도 매번 들쑥날쑥해요",
          "speaker_id": "2",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100033269.1.24",
          "form": "그래도 대단하세요",
          "original_form": "그래도 대단하세요",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100033269.1.25",
          "form": "파운드는 진짜 시도도못하는ㄷㄷ",
          "original_form": "파운드는 진짜 시도도못하는ㄷㄷ",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100033269.1.26",
          "form": "근데 대충 맛이 나요ㅋㅋ",
          "original_form": "근데 대충 맛이 나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100033269.1.27",
          "form": "선물은 절대못하고요",
          "original_form": "선물은 절대못하고요",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100033269.1.28",
          "form": "쿠키도 맨날 믹스이런거해보다 폭망하는데",
          "original_form": "쿠키도 맨날 믹스이런거해보다 폭망하는데",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100033269.1.29",
          "form": "대단하시네요!!*_*",
          "original_form": "대단하시네요!!*_*",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100033269.1.30",
          "form": "ㅋㅋ저도 스콘이나 쿠키는 믹스로 가끔해요",
          "original_form": "ㅋㅋ저도 스콘이나 쿠키는 믹스로 가끔해요",
