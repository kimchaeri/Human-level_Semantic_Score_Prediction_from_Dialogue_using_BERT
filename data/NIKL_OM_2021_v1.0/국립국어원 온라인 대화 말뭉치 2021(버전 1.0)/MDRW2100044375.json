      "utterance": [
        {
          "id": "MDRW2100044375.1.1",
          "form": "네이버에서 주문하면",
          "original_form": "네이버에서 주문하면",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044375.1.2",
          "form": "얼마나 걸려요?",
          "original_form": "얼마나 걸려요?",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044375.1.3",
          "form": "한 하루만에 올때도 있어욤",
          "original_form": "한 하루만에 올때도 있어욤",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044375.1.4",
          "form": "하루만이란게",
          "original_form": "하루만이란게",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044375.1.5",
          "form": "당일요? 아님 주문 익일?",
          "original_form": "당일요? 아님 주문 익일?",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044375.1.6",
          "form": "주문 익일?",
          "original_form": "주문 익일?",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044375.1.7",
          "form": "아 담날 도착 와우",
          "original_form": "아 담날 도착 와우",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044375.1.8",
          "form": "훌륭하네영",
          "original_form": "훌륭하네영",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044375.1.9",
          "form": "거기 대창....먹고 반했엄",
          "original_form": "거기 대창....먹고 반했엄",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044375.1.10",
          "form": "안질경",
          "original_form": "안질경",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044375.1.11",
          "form": "홀 지짜?",
          "original_form": "홀 지짜?",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044375.1.12",
          "form": "대창을 시킬까여?",
          "original_form": "대창을 시킬까여?",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044375.1.13",
          "form": "언니 대창 좋아하면 함 시켜방 ㅋㅋ",
          "original_form": "언니 대창 좋아하면 함 시켜방 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044375.1.14",
          "form": "곱창도 맛있긴한뎅",
          "original_form": "곱창도 맛있긴한뎅",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044375.1.15",
          "form": "나저기...본점 친구들이랑 먹으러갔는데",
          "original_form": "나저기...본점 친구들이랑 먹으러갔는데",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.16",
          "form": "대창도 좋아하는데\r\n질긴데 많더라궁ㅋ",
          "original_form": "대창도 좋아하는데\r\n질긴데 많더라궁ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.17",
          "form": "2시간걸렸어대기시간 ㅋㅋ",
          "original_form": "2시간걸렸어대기시간 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.18",
          "form": "맞앙 ㅜㅜ",
          "original_form": "맞앙 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.19",
          "form": "근데 여긴 안질겨",
          "original_form": "근데 여긴 안질겨",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.20",
          "form": "잘하는집이군ㅋㅋㅋㅋ",
          "original_form": "잘하는집이군ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.21",
          "form": "비싼값하나방ㅋㅋㅋㅋ",
          "original_form": "비싼값하나방ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.22",
          "form": "본점은 어디에 있어?",
          "original_form": "본점은 어디에 있어?",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044375.1.23",
          "form": "아머야 여기밖에없었구낰ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아머야 여기밖에없었구낰ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044375.1.24",
          "form": "왕십리역 저기뿐이랰ㅋㅋㅋㅋ",
          "original_form": "왕십리역 저기뿐이랰ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044375.1.25",
          "form": "2층에",
          "original_form": "2층에",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044375.1.26",
          "form": "양고기집하는거였어 ㅋㅋ",
          "original_form": "양고기집하는거였어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044375.1.27",
          "form": "주문해서 먹어도",
          "original_form": "주문해서 먹어도",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044375.1.28",
          "form": "그 맛이 나던가여?",
          "original_form": "그 맛이 나던가여?",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044375.1.29",
          "form": "넹넹 여전히 맛있드라구염 ㅋㅋㅋ",
          "original_form": "넹넹 여전히 맛있드라구염 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        }
      ]
    }
