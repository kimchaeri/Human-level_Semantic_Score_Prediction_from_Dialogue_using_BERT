      "utterance": [
        {
          "id": "MDRW2100017130.1.1",
          "form": "게임 얘기 하자",
          "original_form": "게임 얘기 하자",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017130.1.2",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017130.1.3",
          "form": "나 요즘 마법사의 약속 질렸어",
          "original_form": "나 요즘 마법사의 약속 질렸어",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017130.1.4",
          "form": "아직 하긴 하고?",
          "original_form": "아직 하긴 하고?",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017130.1.5",
          "form": "응 근데 질려서 하기 싫다...",
          "original_form": "응 근데 질려서 하기 싫다...",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017130.1.6",
          "form": "지금 몇달 했지 5달?",
          "original_form": "지금 몇달 했지 5달?",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017130.1.7",
          "form": "이제 재미없음?",
          "original_form": "이제 재미없음?",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017130.1.8",
          "form": "컨텐츠도 딱히 없고...",
          "original_form": "컨텐츠도 딱히 없고...",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017130.1.9",
          "form": "이벤트 스토리도 메인 스토리 작가가 안해서",
          "original_form": "이벤트 스토리도 메인 스토리 작가가 안해서",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017130.1.10",
          "form": "지루해",
          "original_form": "지루해",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017130.1.11",
          "form": "그럼 뭐... 일본에선 인기있는거임?",
          "original_form": "그럼 뭐... 일본에선 인기있는거임?",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017130.1.12",
          "form": "나름 앙스타랑 트위스테가 1,2위 다투고",
          "original_form": "나름 앙스타랑 트위스테가 1,2위 다투고",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017130.1.13",
          "form": "얘는 항상 3등이야",
          "original_form": "얘는 항상 3등이야",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017130.1.14",
          "form": "그럼 인기있네",
          "original_form": "그럼 인기있네",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017130.1.15",
          "form": "근데 운영을 이상하게 하네...",
          "original_form": "근데 운영을 이상하게 하네...",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017130.1.16",
          "form": "근데 앞에 둘이랑 매출 차이가",
          "original_form": "근데 앞에 둘이랑 매출 차이가",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017130.1.17",
          "form": "열배 나",
          "original_form": "열배 나",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017130.1.18",
          "form": "글쿠나.. 넘사네 그럼",
          "original_form": "글쿠나.. 넘사네 그럼",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017130.1.19",
          "form": "근데 유독 한국에 좀 잘 안 먹혀서",
          "original_form": "근데 유독 한국에 좀 잘 안 먹혀서",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017130.1.20",
          "form": "저기서는 나름 메이저인데",
          "original_form": "저기서는 나름 메이저인데",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017130.1.21",
          "form": "여기서는 마이너해...",
          "original_form": "여기서는 마이너해...",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017130.1.22",
          "form": "ㅋㅋㅋㅋㅋ한국에선 잘 안먹히는 종류긴 한거 같음",
          "original_form": "ㅋㅋㅋㅋㅋ한국에선 잘 안먹히는 종류긴 한거 같음",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017130.1.23",
          "form": "살면서 이렇게까지 작은 덕질판 안 들어가봐서",
          "original_form": "살면서 이렇게까지 작은 덕질판 안 들어가봐서",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017130.1.24",
          "form": "어색해...",
          "original_form": "어색해...",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017130.1.25",
          "form": "클릭커 오토 게임 ... 좋지 못한 선택이었어",
          "original_form": "클릭커 오토 게임 ... 좋지 못한 선택이었어",
          "speaker_id": "1",
          "time": "20210826 17:54"
        }
      ]
    }
