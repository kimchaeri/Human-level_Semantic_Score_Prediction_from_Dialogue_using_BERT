      "utterance": [
        {
          "id": "MDRW2100046644.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211103 23:12"
        },
        {
          "id": "MDRW2100046644.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211103 23:13"
        },
        {
          "id": "MDRW2100046644.1.3",
          "form": "코로나 끝나면 가장 하고싶은 일은 무엇인가요?",
          "original_form": "코로나 끝나면 가장 하고싶은 일은 무엇인가요?",
          "speaker_id": "2",
          "time": "20211103 23:13"
        },
        {
          "id": "MDRW2100046644.1.4",
          "form": "코로나 끝나면 여행갈 거예요",
          "original_form": "코로나 끝나면 여행갈 거예요",
          "speaker_id": "1",
          "time": "20211103 23:14"
        },
        {
          "id": "MDRW2100046644.1.5",
          "form": "가고싶은 여행지 있으신가요?",
          "original_form": "가고싶은 여행지 있으신가요?",
          "speaker_id": "2",
          "time": "20211103 23:14"
        },
        {
          "id": "MDRW2100046644.1.6",
          "form": "영국에 가고 싶습니다",
          "original_form": "영국에 가고 싶습니다",
          "speaker_id": "1",
          "time": "20211103 23:15"
        },
        {
          "id": "MDRW2100046644.1.7",
          "form": "영국 가보신 적 있으세요~?",
          "original_form": "영국 가보신 적 있으세요~?",
          "speaker_id": "2",
          "time": "20211103 23:15"
        },
        {
          "id": "MDRW2100046644.1.8",
          "form": "전 영국은 가본 적이 없네요",
          "original_form": "전 영국은 가본 적이 없네요",
          "speaker_id": "2",
          "time": "20211103 23:15"
        },
        {
          "id": "MDRW2100046644.1.9",
          "form": "아뇨. 안 가 봐서 가보려 합니다!",
          "original_form": "아뇨. 안 가 봐서 가보려 합니다!",
          "speaker_id": "1",
          "time": "20211103 23:15"
        },
        {
          "id": "MDRW2100046644.1.10",
          "form": "갔다온 사람들이 영국만의 감성이 있다더라구요 ㅎㅎ",
          "original_form": "갔다온 사람들이 영국만의 감성이 있다더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 23:15"
        },
        {
          "id": "MDRW2100046644.1.11",
          "form": "프랑스파랑 영국파랑 나뉜다고 ㅋㅋㅋ",
          "original_form": "프랑스파랑 영국파랑 나뉜다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 23:16"
        },
        {
          "id": "MDRW2100046644.1.12",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "1",
          "time": "20211103 23:16"
        },
        {
          "id": "MDRW2100046644.1.13",
          "form": "저는 축구를 보기 위해 가려고 해요!",
          "original_form": "저는 축구를 보기 위해 가려고 해요!",
          "speaker_id": "1",
          "time": "20211103 23:16"
        },
        {
          "id": "MDRW2100046644.1.14",
          "form": "아아 축구 좋아하시는구나!",
          "original_form": "아아 축구 좋아하시는구나!",
          "speaker_id": "2",
          "time": "20211103 23:16"
        },
        {
          "id": "MDRW2100046644.1.15",
          "form": "응원하시는 팀 있으세요?",
          "original_form": "응원하시는 팀 있으세요?",
          "speaker_id": "2",
          "time": "20211103 23:16"
        },
        {
          "id": "MDRW2100046644.1.16",
          "form": "일단 손흥민이 있는 토트넘이요!",
          "original_form": "일단 손흥민이 있는 토트넘이요!",
          "speaker_id": "1",
          "time": "20211103 23:16"
        },
        {
          "id": "MDRW2100046644.1.17",
          "form": "국뽕이죠 ㅎㅎ",
          "original_form": "국뽕이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211103 23:16"
        },
        {
          "id": "MDRW2100046644.1.18",
          "form": "한국사람이면 다 토트넘 응원하져 ㅋㅋㅋ 국뽕",
          "original_form": "한국사람이면 다 토트넘 응원하져 ㅋㅋㅋ 국뽕",
          "speaker_id": "2",
          "time": "20211103 23:17"
        },
        {
          "id": "MDRW2100046644.1.19",
          "form": "그럼 영국 놀러가시면 축구 보고 오시겠네요!",
          "original_form": "그럼 영국 놀러가시면 축구 보고 오시겠네요!",
          "speaker_id": "2",
          "time": "20211103 23:19"
        }
      ]
    }
