      "utterance": [
        {
          "id": "MDRW2100039996.1.1",
          "form": "많이 먹는사람들보면 신기해요",
          "original_form": "많이 먹는사람들보면 신기해요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039996.1.2",
          "form": "맞아요 신기하죠",
          "original_form": "맞아요 신기하죠",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039996.1.3",
          "form": "진짜 타고난게 다르더라구요",
          "original_form": "진짜 타고난게 다르더라구요",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100039996.1.4",
          "form": "위가 다른 사람보다 큰걸까요",
          "original_form": "위가 다른 사람보다 큰걸까요",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100039996.1.5",
          "form": "그럴거같아요",
          "original_form": "그럴거같아요",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100039996.1.6",
          "form": "소화력도 남다르고",
          "original_form": "소화력도 남다르고",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100039996.1.7",
          "form": "저는 소식가라서",
          "original_form": "저는 소식가라서",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039996.1.8",
          "form": "그런 사람이랑 그냥 다른 종인거 같아요",
          "original_form": "그런 사람이랑 그냥 다른 종인거 같아요",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039996.1.9",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039996.1.10",
          "form": "name1님은 대식가가 되고 싶으신가요",
          "original_form": "&name1&님은 대식가가 되고 싶으신가요",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039996.1.11",
          "form": "ㅋㅋㅋ아뇨 억지로 바뀌고싶진않아요",
          "original_form": "ㅋㅋㅋ아뇨 억지로 바뀌고싶진않아요",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039996.1.12",
          "form": "건강을위해..!",
          "original_form": "건강을위해..!",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039996.1.13",
          "form": "저는 대식도 건강을 위한거라고 생각해요",
          "original_form": "저는 대식도 건강을 위한거라고 생각해요",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039996.1.14",
          "form": "영양소도 더 많이 들어오고",
          "original_form": "영양소도 더 많이 들어오고",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039996.1.15",
          "form": "에너지도 생기니 운동하게 되잖아요",
          "original_form": "에너지도 생기니 운동하게 되잖아요",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039996.1.16",
          "form": "그렇긴하죠",
          "original_form": "그렇긴하죠",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039996.1.17",
          "form": "기초대사량이 많으면 괜찮을거같아요",
          "original_form": "기초대사량이 많으면 괜찮을거같아요",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039996.1.18",
          "form": "결국은 맞는 사람이 대식가가 되겠네요",
          "original_form": "결국은 맞는 사람이 대식가가 되겠네요",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100039996.1.19",
          "form": "그렇겠어요",
