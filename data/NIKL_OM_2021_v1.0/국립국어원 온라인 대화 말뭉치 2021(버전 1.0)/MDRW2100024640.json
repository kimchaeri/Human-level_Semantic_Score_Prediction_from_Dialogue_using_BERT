      "utterance": [
        {
          "id": "MDRW2100024640.1.1",
          "form": "온천어때 우리나라랑 달ㄹ라?",
          "original_form": "온천어때 우리나라랑 달ㄹ라?",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024640.1.2",
          "form": "엄청 다를 줄 알았는데 비슷했어",
          "original_form": "엄청 다를 줄 알았는데 비슷했어",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024640.1.3",
          "form": "목욕탕인가 그냥",
          "original_form": "목욕탕인가 그냥",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024640.1.4",
          "form": "먹는건 어때",
          "original_form": "먹는건 어때",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024640.1.5",
          "form": "응 맞아ㅋㅋㅋㅋ 대신 노천탕은 작게라도 있더라 한국은 거의 없잖아",
          "original_form": "응 맞아ㅋㅋㅋㅋ 대신 노천탕은 작게라도 있더라 한국은 거의 없잖아",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024640.1.6",
          "form": "일식 맛있더라",
          "original_form": "일식 맛있더라",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024640.1.7",
          "form": "그건  신기하나",
          "original_form": "그건  신기하나",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024640.1.8",
          "form": "그치 나도 신기했어",
          "original_form": "그치 나도 신기했어",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024640.1.9",
          "form": "일식 엄청 좋아해  나도",
          "original_form": "일식 엄청 좋아해  나도",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024640.1.10",
          "form": "회먹었니",
          "original_form": "회먹었니",
          "speaker_id": "2",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024640.1.11",
          "form": "아니 아쉽게도 회는 못 먹었어",
          "original_form": "아니 아쉽게도 회는 못 먹었어",
          "speaker_id": "1",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024640.1.12",
          "form": "아 스시 먹었구나",
          "original_form": "아 스시 먹었구나",
          "speaker_id": "1",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024640.1.13",
          "form": "첫번째날은 뭐했어?",
          "original_form": "첫번째날은 뭐했어?",
          "speaker_id": "2",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024640.1.14",
          "form": "첫 번째 날에 숙소 잡고 짐 정리했더니 해가 다 져서 많이 놀진 못했어",
          "original_form": "첫 번째 날에 숙소 잡고 짐 정리했더니 해가 다 져서 많이 놀진 못했어",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024640.1.15",
          "form": "근데 마침 숙소 주변에 편의점이 있었어",
          "original_form": "근데 마침 숙소 주변에 편의점이 있었어",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024640.1.16",
          "form": "일본은  10시이후   가게문이",
          "original_form": "일본은  10시이후   가게문이",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024640.1.17",
          "form": "많이   닫니",
          "original_form": "많이   닫니",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024640.1.18",
          "form": "응 그래서 편의점이 한국보다 잘 되어있어",
          "original_form": "응 그래서 편의점이 한국보다 잘 되어있어",
          "speaker_id": "1",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100024640.1.19",
          "form": "신기한다 진짜",
          "original_form": "신기한다 진짜",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100024640.1.20",
          "form": "편의점 맛있다던데",
          "original_form": "편의점 맛있다던데",
          "speaker_id": "2",
          "time": "20210831 16:31"
        }
      ]
