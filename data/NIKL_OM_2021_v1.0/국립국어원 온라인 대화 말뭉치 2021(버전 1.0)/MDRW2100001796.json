        },
        {
          "id": "MDRW2100001796.1.2",
          "form": "여행다니는거 좋아하세요?",
          "original_form": "여행다니는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001796.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001796.1.4",
          "form": "좋아하는데 코로나로 잘 못 돌아다니고 있어요...ㅠㅠ",
          "original_form": "좋아하는데 코로나로 잘 못 돌아다니고 있어요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001796.1.5",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001796.1.6",
          "form": "저는 여행은 좋아하는데 가본거라고는 제주도만 가봤어요ㅋㅋ",
          "original_form": "저는 여행은 좋아하는데 가본거라고는 제주도만 가봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001796.1.7",
          "form": "해외여행가신 적 있으세요?",
          "original_form": "해외여행가신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001796.1.8",
          "form": "제주도 좋죠!! 저는 제주도는 못가보고 일본 한번 가봤네요",
          "original_form": "제주도 좋죠!! 저는 제주도는 못가보고 일본 한번 가봤네요",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001796.1.9",
          "form": "불매전에요 ㅋㅋ",
          "original_form": "불매전에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001796.1.10",
          "form": "만약에 코로나 끝나고나면 어디로 여행 가고싶으세요?",
          "original_form": "만약에 코로나 끝나고나면 어디로 여행 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001796.1.11",
          "form": "제주도여 ㅋㅋ",
          "original_form": "제주도여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001796.1.12",
          "form": "원래 이번 여름 제주도 가려했는데 코로나가 심해져서 못갔어요 ㅠㅠ",
          "original_form": "원래 이번 여름 제주도 가려했는데 코로나가 심해져서 못갔어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001796.1.13",
          "form": "아ㅜㅠ 저도 제주도 한 번 더 가고싶어요",
          "original_form": "아ㅜㅠ 저도 제주도 한 번 더 가고싶어요",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001796.1.14",
          "form": "특히 돈까스집!!! 가고파요ㅋㅋㅋ",
          "original_form": "특히 돈까스집!!! 가고파요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001796.1.15",
          "form": "돈까스집이요? 제주도면 연돈??!",
          "original_form": "돈까스집이요? 제주도면 연돈??!",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001796.1.16",
          "form": "연돈이요ㅋㅋㅋ",
          "original_form": "연돈이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001796.1.17",
          "form": "이제 제주도 하면 가야하는 맛집 중 하나 아닐까 싶어요",
          "original_form": "이제 제주도 하면 가야하는 맛집 중 하나 아닐까 싶어요",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001796.1.18",
          "form": "헐 가보셨어요??? 맛 어때요?",
          "original_form": "헐 가보셨어요??? 맛 어때요?",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001796.1.19",
          "form": "가보고 싶다 ㅠㅠ",
          "original_form": "가보고 싶다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001796.1.20",
          "form": "아뇨ㅠ 저도 안가봤어요ㅜ 연돈 생기기 전에 제주도 간거라서요ㅋㅋ",
          "original_form": "아뇨ㅠ 저도 안가봤어요ㅜ 연돈 생기기 전에 제주도 간거라서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001796.1.21",
          "form": "티비로 보나 리뷰로 보나 맛있다고 하니까 꼭 한 번 가보고싶어요",
          "original_form": "티비로 보나 리뷰로 보나 맛있다고 하니까 꼭 한 번 가보고싶어요",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001796.1.22",
          "form": "저두요 ㅎㅎ",
          "original_form": "저두요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001796.1.23",
          "form": "얼마나 맛있으면 줄을 밤새서서 먹는지",
          "original_form": "얼마나 맛있으면 줄을 밤새서서 먹는지",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001796.1.24",
          "form": "제주도 가면 또 어디가보고싶어요?",
          "original_form": "제주도 가면 또 어디가보고싶어요?",
          "speaker_id": "1",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001796.1.25",
          "form": "알파카 목장이요!!",
          "original_form": "알파카 목장이요!!",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001796.1.26",
          "form": "알파카 한번도 직접 못봤는데 제주도에 목장이 있도라구요",
          "original_form": "알파카 한번도 직접 못봤는데 제주도에 목장이 있도라구요",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001796.1.27",
          "form": "와?? 알파카목장이요?  저 처음들어봐요",
          "original_form": "와?? 알파카목장이요?  저 처음들어봐요",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001796.1.28",
          "form": "말은 봤어도ㅋㅋㅋ알파카 있는건 몰랐네요",
          "original_form": "말은 봤어도ㅋㅋㅋ알파카 있는건 몰랐네요",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001796.1.29",
          "form": "저도 근래에 알았어요 제주도 놀러가려규 찾아보다가 ㅋㅋㅋ",
          "original_form": "저도 근래에 알았어요 제주도 놀러가려규 찾아보다가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001796.1.30",
          "form": "올레길도 걷고싶네요",
          "original_form": "올레길도 걷고싶네요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001796.1.31",
          "form": "올레길도 가보고 싶어요ㅋㅋ",
          "original_form": "올레길도 가보고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001796.1.32",
          "form": "거기서 자전거도 타고 돌아다니고싶은 1인...ㅜ",
          "original_form": "거기서 자전거도 타고 돌아다니고싶은 1인...ㅜ",
          "speaker_id": "1",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100001796.1.33",
          "form": "허.. 올레길",
          "original_form": "허.. 올레길",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100001796.1.34",
          "form": "저두 올레길은 안가봐서 가보고 싶네요",
          "original_form": "저두 올레길은 안가봐서 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100001796.1.35",
          "form": "자전거는 누가 훔쳐가서 못타고 있어요 ㅠㅠ",
          "original_form": "자전거는 누가 훔쳐가서 못타고 있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100001796.1.36",
          "form": "헐ㅜㅠ 자전거는 왜케 다들 훔쳐가는지...ㅜ",
          "original_form": "헐ㅜㅠ 자전거는 왜케 다들 훔쳐가는지...ㅜ",
          "speaker_id": "1",
          "time": "20210819 23:04"
        },
        {
          "id": "MDRW2100001796.1.37",
          "form": "나중에 제주도 가시게 되면 만장굴도 한 번 가보세요",
