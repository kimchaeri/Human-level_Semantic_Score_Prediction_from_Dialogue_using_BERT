      "utterance": [
        {
          "id": "MDRW2100036117.1.1",
          "form": "오늘 학생분들 개강이래요",
          "original_form": "오늘 학생분들 개강이래요",
          "speaker_id": "1",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100036117.1.2",
          "form": "월요일 맞추느라 오늘 개강한 학교도 있더라고요",
          "original_form": "월요일 맞추느라 오늘 개강한 학교도 있더라고요",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036117.1.3",
          "form": "저희는 수요일에 해요",
          "original_form": "저희는 수요일에 해요",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036117.1.4",
          "form": "학교근처 사는데 비대면으로 수업을 해서 그런가 딱히 바뀐건없어요",
          "original_form": "학교근처 사는데 비대면으로 수업을 해서 그런가 딱히 바뀐건없어요",
          "speaker_id": "1",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100036117.1.5",
          "form": "수요일날하시는구나 싫으시겠다...",
          "original_form": "수요일날하시는구나 싫으시겠다...",
          "speaker_id": "1",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100036117.1.6",
          "form": "1학기도 화요일에 학기 시작했나 그래서ㅋㅋㅋ",
          "original_form": "1학기도 화요일에 학기 시작했나 그래서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100036117.1.7",
          "form": "개강 하셨나요?",
          "original_form": "개강 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100036117.1.8",
          "form": "저는 졸업했어요",
          "original_form": "저는 졸업했어요",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036117.1.9",
          "form": "졸업하셨군요!",
          "original_form": "졸업하셨군요!",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100036117.1.10",
          "form": "부럽네요...",
          "original_form": "부럽네요...",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100036117.1.11",
          "form": "저런....",
          "original_form": "저런....",
          "speaker_id": "1",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100036117.1.12",
          "form": "그래도 이번에 비대면이라던데 조금 괜찮지않을까요...",
          "original_form": "그래도 이번에 비대면이라던데 조금 괜찮지않을까요...",
          "speaker_id": "1",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100036117.1.13",
          "form": "비대면이라 공부를 더 안 하는 거 같아요...",
          "original_form": "비대면이라 공부를 더 안 하는 거 같아요...",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100036117.1.14",
          "form": "앗..... 비대면은 좀 집중이 덜되나요?",
          "original_form": "앗..... 비대면은 좀 집중이 덜되나요?",
          "speaker_id": "1",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100036117.1.15",
          "form": "시간 강의는 그나마 괜찮은데 녹화강의를 자꾸 미루게 되더라고요...ㅎㅎ",
          "original_form": "시간 강의는 그나마 괜찮은데 녹화강의를 자꾸 미루게 되더라고요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100036117.1.16",
          "form": "헉 저가 만약 비대면 수업을 했으면 녹화는 다 시험직전에 봤을수도요...",
          "original_form": "헉 저가 만약 비대면 수업을 했으면 녹화는 다 시험직전에 봤을수도요...",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036117.1.17",
          "form": "맞아요ㅎㅎ 제가 1학기 때 그랬어요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅎㅎ 제가 1학기 때 그랬어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036117.1.18",
          "form": "심지어 시험 전에 다 못 들은 강의도 있어요... 진짜 반성하는 중이에요ㅋㅋㅋㅋ",
          "original_form": "심지어 시험 전에 다 못 들은 강의도 있어요... 진짜 반성하는 중이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036117.1.19",
          "form": "이미 경험자셨군요ㅋㅋㅋㅋ",
          "original_form": "이미 경험자셨군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100036117.1.20",
          "form": "와....성적 보호 힘들겠어요",
          "original_form": "와....성적 보호 힘들겠어요",
          "speaker_id": "1",
          "time": "20210830 17:44"
        }
      ]
