        },
        {
          "id": "MDRW2100025478.1.2",
          "form": "안녕하세요 name1님~!",
          "original_form": "안녕하세요 &name1&님~!",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100025478.1.3",
          "form": "오늘 점심은 뭐 드셨어요 ?",
          "original_form": "오늘 점심은 뭐 드셨어요 ?",
          "speaker_id": "1",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025478.1.4",
          "form": "저 초밥 먹었어요 ㅎㅎ",
          "original_form": "저 초밥 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025478.1.5",
          "form": "오랜만에 먹었는데 맛있더라구요",
          "original_form": "오랜만에 먹었는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025478.1.6",
          "form": "name1님은 뭐 드셨어요~?",
          "original_form": "&name1&님은 뭐 드셨어요~?",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025478.1.7",
          "form": "저도 초밥 먹고 싶네요",
          "original_form": "저도 초밥 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025478.1.8",
          "form": "전 집에 있는 반찬들 꺼내 먹었어요",
          "original_form": "전 집에 있는 반찬들 꺼내 먹었어요",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025478.1.9",
          "form": "저도 저녁은 집에 있는 걸로 먹으려구요 ㅎㅎ",
          "original_form": "저도 저녁은 집에 있는 걸로 먹으려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025478.1.10",
          "form": "좋아하는 반찬 있으신가요",
          "original_form": "좋아하는 반찬 있으신가요",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025478.1.11",
          "form": "전 가지볶음 좋아해요",
          "original_form": "전 가지볶음 좋아해요",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025478.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025478.1.13",
          "form": "오 가지 저도 좋아해요!",
          "original_form": "오 가지 저도 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100025478.1.14",
          "form": "살짝 덜익히면 사각한 식감이 좋아요 ㅎㅎ",
          "original_form": "살짝 덜익히면 사각한 식감이 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100025478.1.15",
          "form": "가지 토마토 스파게티에 넣어먹어도 맛있구..",
          "original_form": "가지 토마토 스파게티에 넣어먹어도 맛있구..",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100025478.1.16",
          "form": "오 토마토스파게티에도 넣는군요!! 맛있겠네요",
          "original_form": "오 토마토스파게티에도 넣는군요!! 맛있겠네요",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100025478.1.17",
          "form": "토마토소스랑 가지랑 잘어울리더라구요 ㅋㅋㅋㅋ 나폴리탄 스파게티 만들때 넣어도 좋아요 ㅎㅎ",
          "original_form": "토마토소스랑 가지랑 잘어울리더라구요 ㅋㅋㅋㅋ 나폴리탄 스파게티 만들때 넣어도 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100025478.1.18",
          "form": "생각만 해도 맛있을 것 같아요 가지 그을린 자국 나게 볶아서 먹고 싶네요",
          "original_form": "생각만 해도 맛있을 것 같아요 가지 그을린 자국 나게 볶아서 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210902 07:59"
        },
        {
          "id": "MDRW2100025478.1.19",
          "form": "ㅎㅎ 다른 건 어떤 음식 좋아하세요?",
          "original_form": "ㅎㅎ 다른 건 어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100025478.1.20",
          "form": "떡볶이요 오늘도 먹고 싶은 거 참았어요",
          "original_form": "떡볶이요 오늘도 먹고 싶은 거 참았어요",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100025478.1.21",
          "form": "아 저도 요즘 로제떡볶이 먹고싶더라구요",
          "original_form": "아 저도 요즘 로제떡볶이 먹고싶더라구요",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100025478.1.22",
          "form": "중국당면 추가해서..",
          "original_form": "중국당면 추가해서..",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100025478.1.23",
          "form": "아 그쵸 로제떡볶이에 중당은 필수입니다",
          "original_form": "아 그쵸 로제떡볶이에 중당은 필수입니다",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100025478.1.24",
          "form": "로제떡볶이 어디 꺼 드셔보셨어요?",
          "original_form": "로제떡볶이 어디 꺼 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100025478.1.25",
          "form": "저 배떡이랑 태리로제랑 명랑 먹어봤어요",
          "original_form": "저 배떡이랑 태리로제랑 명랑 먹어봤어요",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100025478.1.26",
          "form": "셋중에선 태리로제가 젤 맛있는 것 같아요 ㅎㅎ",
          "original_form": "셋중에선 태리로제가 젤 맛있는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100025478.1.27",
          "form": "배떡이나 태리로제나 비슷하긴 한데",
          "original_form": "배떡이나 태리로제나 비슷하긴 한데",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100025478.1.28",
          "form": "배떡은 소세지가 너무 맛없어서.. ㅎ",
          "original_form": "배떡은 소세지가 너무 맛없어서.. ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100025478.1.29",
          "form": "둘이 양념 맛도 비슷해요?",
          "original_form": "둘이 양념 맛도 비슷해요?",
          "speaker_id": "1",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100025478.1.30",
          "form": "네네 비슷하다고 느꼈어요",
          "original_form": "네네 비슷하다고 느꼈어요",
          "speaker_id": "2",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100025478.1.31",
          "form": "다음엔 태리로제 먹어봐야겠네요!",
          "original_form": "다음엔 태리로제 먹어봐야겠네요!",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100025478.1.32",
          "form": "오늘은 떡군이네 로제떡볶이 먹었어요",
          "original_form": "오늘은 떡군이네 로제떡볶이 먹었어요",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100025478.1.33",
          "form": "아 저 거기도 먹어봤어요 ㅋㅋㅋㅋ",
          "original_form": "아 저 거기도 먹어봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100025478.1.34",
          "form": "까먹고있었네요",
          "original_form": "까먹고있었네요",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100025478.1.35",
          "form": "떡군이네도 맛 ㄱㅊ죠",
          "original_form": "떡군이네도 맛 ㄱㅊ죠",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100025478.1.36",
          "form": "진짜 맛있어요!!",
          "original_form": "진짜 맛있어요!!",
