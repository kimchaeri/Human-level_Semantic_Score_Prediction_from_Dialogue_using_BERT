        },
        {
          "id": "MDRW2100004824.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100004824.1.4",
          "form": "저도ㅜ그정도",
          "original_form": "저도ㅜ그정도",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100004824.1.5",
          "form": "주러 어떤 거 드세요?",
          "original_form": "주러 어떤 거 드세요?",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100004824.1.6",
          "form": "주로",
          "original_form": "주로",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100004824.1.7",
          "form": "치킨",
          "original_form": "치킨",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100004824.1.8",
          "form": "아 저는 마라탕 시켜먹어요",
          "original_form": "아 저는 마라탕 시켜먹어요",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100004824.1.9",
          "form": "마라탕 드셔보셨나요?",
          "original_form": "마라탕 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100004824.1.10",
          "form": "향이 너무 진해서",
          "original_form": "향이 너무 진해서",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100004824.1.11",
          "form": "호불호 많이 갈리더라고요",
          "original_form": "호불호 많이 갈리더라고요",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100004824.1.12",
          "form": "치킨은 주로 어디에서 시켜드세요?",
          "original_form": "치킨은 주로 어디에서 시켜드세요?",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100004824.1.13",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100004824.1.14",
          "form": "콜라랑 드세요 사이다랑 드세요?",
          "original_form": "콜라랑 드세요 사이다랑 드세요?",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100004824.1.15",
          "form": "사이다요",
          "original_form": "사이다요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004824.1.16",
          "form": "저는 콜라랑 먹는 편이에요",
          "original_form": "저는 콜라랑 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004824.1.17",
          "form": "치킨무는 드시나요?",
          "original_form": "치킨무는 드시나요?",
