      "utterance": [
        {
          "id": "MDRW2100011830.1.1",
          "form": "좋아하는 동물있나요?",
          "original_form": "좋아하는 동물있나요?",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011830.1.2",
          "form": "고양이 좋아해요ㅎㅎ",
          "original_form": "고양이 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011830.1.3",
          "form": "저 고양이 반려동물로 키워요",
          "original_form": "저 고양이 반려동물로 키워요",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011830.1.4",
          "form": "와 반려동물 이름이 뭐에요?",
          "original_form": "와 반려동물 이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011830.1.5",
          "form": "집에 온지 얼마 안되서 아직 이름이 없네요ㅠ",
          "original_form": "집에 온지 얼마 안되서 아직 이름이 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011830.1.6",
          "form": "보니깐 반려동물이름도 이쁘게 짓더라고요ㅎㅎ",
          "original_form": "보니깐 반려동물이름도 이쁘게 짓더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011830.1.7",
          "form": "예쁜 동물이름 어떤게 있을까요?",
          "original_form": "예쁜 동물이름 어떤게 있을까요?",
          "speaker_id": "2",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011830.1.8",
          "form": "사람 이름처럼 짓기도 하더라고요~",
          "original_form": "사람 이름처럼 짓기도 하더라고요~",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011830.1.9",
          "form": "저때는 동물이름 냥이 멍이 이런식이라ㅎ",
          "original_form": "저때는 동물이름 냥이 멍이 이런식이라ㅎ",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011830.1.10",
          "form": "뭐 부르기 편한게 최고죠",
          "original_form": "뭐 부르기 편한게 최고죠",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011830.1.11",
          "form": "흰고양이에요 흰둥이 어떤가요",
          "original_form": "흰고양이에요 흰둥이 어떤가요",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011830.1.12",
          "form": "흰둥이는 강아지이름같아서 흰둥이보단 name3는 어떤가요",
          "original_form": "흰둥이는 강아지이름같아서 흰둥이보단 &name3&는 어떤가요",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011830.1.13",
          "form": "흰색이라 name3인가요?",
          "original_form": "흰색이라 &name3&인가요?",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011830.1.14",
          "form": "네 사람이름 name3같기도 해서ㅎㅎ",
          "original_form": "네 사람이름 &name3&같기도 해서ㅎㅎ",
