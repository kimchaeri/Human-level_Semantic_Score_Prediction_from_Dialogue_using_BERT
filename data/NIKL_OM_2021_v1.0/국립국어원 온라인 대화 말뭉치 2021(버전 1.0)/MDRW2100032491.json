      "utterance": [
        {
          "id": "MDRW2100032491.1.1",
          "form": "그래도 귀에 익숙하니까",
          "original_form": "그래도 귀에 익숙하니까",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032491.1.2",
          "form": "괜찮을 줄...",
          "original_form": "괜찮을 줄...",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032491.1.3",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032491.1.4",
          "form": "ㄱ.ㅊ ㄱ.ㅊ",
          "original_form": "ㄱ.ㅊ ㄱ.ㅊ",
          "speaker_id": "1",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032491.1.5",
          "form": "그래도 두번째 듣는거랑 아예 첨듣는건",
          "original_form": "그래도 두번째 듣는거랑 아예 첨듣는건",
          "speaker_id": "1",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032491.1.6",
          "form": "다르잖아",
          "original_form": "다르잖아",
          "speaker_id": "1",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032491.1.7",
          "form": "그렇겠지 흑흑",
          "original_form": "그렇겠지 흑흑",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032491.1.8",
          "form": "너는 수강신청 한것중에",
          "original_form": "너는 수강신청 한것중에",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.9",
          "form": "뭐가 제일 어려워",
          "original_form": "뭐가 제일 어려워",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.10",
          "form": "1주차는 거의다 오티라",
          "original_form": "1주차는 거의다 오티라",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.11",
          "form": "아직 제대로 진도 나가진 않앗는데",
          "original_form": "아직 제대로 진도 나가진 않앗는데",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.12",
          "form": "역시...미적분학이랑 일반물리학2",
          "original_form": "역시...미적분학이랑 일반물리학2",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.13",
          "form": "ㅎㅎ....",
          "original_form": "ㅎㅎ....",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.14",
          "form": "아 그 갑자기 어려워진다는",
          "original_form": "아 그 갑자기 어려워진다는",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.15",
          "form": "미적분?",
          "original_form": "미적분?",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.16",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100032491.1.18",
          "form": "근데 밑에 애들",
          "original_form": "근데 밑에 애들",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.19",
          "form": "미적분 했어?",
          "original_form": "미적분 했어?",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.21",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.22",
          "form": "하지 않앗을까?",
          "original_form": "하지 않앗을까?",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.23",
          "form": "기벡이 빠졌을걸",
          "original_form": "기벡이 빠졌을걸",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.24",
          "form": "그 이과에 그거 뭐지",
          "original_form": "그 이과에 그거 뭐지",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.25",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.26",
          "form": "내가 생각한거 기벡이다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "내가 생각한거 기벡이다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.28",
          "form": "그래도 넌 기벡까지 아는거니까",
          "original_form": "그래도 넌 기벡까지 아는거니까",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.29",
          "form": "걔들보다는 잘하겠지 뭐",
          "original_form": "걔들보다는 잘하겠지 뭐",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100032491.1.30",
          "form": "ㅋㅋㅋㅋㅋ그그그렇겟지?",
          "original_form": "ㅋㅋㅋㅋㅋ그그그렇겟지?",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032491.1.31",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032491.1.32",
          "form": "그래도 강의평보니까",
          "original_form": "그래도 강의평보니까",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032491.1.33",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032491.1.34",
          "form": "교수가 거의 연습문제대로 낸다해서",
          "original_form": "교수가 거의 연습문제대로 낸다해서",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
