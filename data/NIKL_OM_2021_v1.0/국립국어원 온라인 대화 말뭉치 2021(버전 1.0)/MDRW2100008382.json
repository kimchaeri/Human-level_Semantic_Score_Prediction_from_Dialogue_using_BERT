      "utterance": [
        {
          "id": "MDRW2100008382.1.1",
          "form": "아 맞아요 아멘이라고 해요",
          "original_form": "아 맞아요 아멘이라고 해요",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008382.1.2",
          "form": "오늘 뭐드셨나요",
          "original_form": "오늘 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008382.1.3",
          "form": "아멘-",
          "original_form": "아멘-",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008382.1.4",
          "form": "저오늘 뼈해장국이요",
          "original_form": "저오늘 뼈해장국이요",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008382.1.5",
          "form": "아멘-",
          "original_form": "아멘-",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008382.1.6",
          "form": "너무 맛있어서 기도가 절로나오는 맛",
          "original_form": "너무 맛있어서 기도가 절로나오는 맛",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008382.1.7",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008382.1.8",
          "form": "아니 아침부터 너무 맛난거 드셨다",
          "original_form": "아니 아침부터 너무 맛난거 드셨다",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008382.1.9",
          "form": "어제 술드셨어요?!",
          "original_form": "어제 술드셨어요?!",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008382.1.10",
          "form": "아니에요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니에요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008382.1.11",
          "form": "운동중이라서 술 절대 금지에요 ㅠㅠ",
          "original_form": "운동중이라서 술 절대 금지에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008382.1.12",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008382.1.13",
          "form": "헉 아뇨 저는 알쓰라",
          "original_form": "헉 아뇨 저는 알쓰라",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008382.1.14",
          "form": "오오 운동하시는구나",
          "original_form": "오오 운동하시는구나",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008382.1.15",
          "form": "요즘 카페에 프로틴 라떼 이런게 많이 생긴대요",
          "original_form": "요즘 카페에 프로틴 라떼 이런게 많이 생긴대요",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008382.1.16",
          "form": "아진짜요? ㅋㅋㅋ",
          "original_form": "아진짜요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008382.1.17",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008382.1.18",
          "form": "근데 운동인들은 뭔가 아메리카노만 먹을것같아요",
          "original_form": "근데 운동인들은 뭔가 아메리카노만 먹을것같아요",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008382.1.19",
          "form": "저는 먹어보고싶네요 ㅎ",
          "original_form": "저는 먹어보고싶네요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008382.1.20",
          "form": "맞아요 운동인들 맨날",
          "original_form": "맞아요 운동인들 맨날",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008382.1.21",
          "form": "몬스터 그 음료랑 아메 마시고",
          "original_form": "몬스터 그 음료랑 아메 마시고",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008382.1.22",
          "form": "라떼정도만 마시더라고요",
          "original_form": "라떼정도만 마시더라고요",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008382.1.23",
          "form": "그 다이어터분들중에",
          "original_form": "그 다이어터분들중에",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008382.1.24",
          "form": "당질 제한 식단하시는 분들이 있더라고요",
          "original_form": "당질 제한 식단하시는 분들이 있더라고요",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008382.1.25",
          "form": "음식은 어케 드세요?",
          "original_form": "음식은 어케 드세요?",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008382.1.26",
          "form": "저도 딱 다이어트 식처럼 먹어요",
          "original_form": "저도 딱 다이어트 식처럼 먹어요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008382.1.27",
          "form": "탄단지 맞추고 설탕 안먹고 ㅋㅋㅋ",
          "original_form": "탄단지 맞추고 설탕 안먹고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008382.1.28",
          "form": "진짜 쭉쭉 빠지긴 하더라고요",
          "original_form": "진짜 쭉쭉 빠지긴 하더라고요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008382.1.29",
          "form": "하지만.. 가끔 뼈해장국을 먹는 그런사람",
          "original_form": "하지만.. 가끔 뼈해장국을 먹는 그런사람",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008382.1.30",
          "form": "하 참기 너무 힘둘죠",
          "original_form": "하 참기 너무 힘둘죠",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008382.1.31",
          "form": "근데 뼈해장국 완전식품이잖어요",
          "original_form": "근데 뼈해장국 완전식품이잖어요",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008382.1.32",
          "form": "단탄지 완전식품",
          "original_form": "단탄지 완전식품",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008382.1.33",
          "form": "하지만 나트륨 폭탄이라 ㅋㅋㅋㅋㅋ",
          "original_form": "하지만 나트륨 폭탄이라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008382.1.34",
          "form": "그리고 저는 ..안에 들어있는 수제비를 제일 좋아해요",
          "original_form": "그리고 저는 ..안에 들어있는 수제비를 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008382.1.35",
          "form": "탄수화물은 사랑이죠",
          "original_form": "탄수화물은 사랑이죠",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008382.1.36",
          "form": "탄수화물 안먹으면 눈물나긴 하더라고요",
          "original_form": "탄수화물 안먹으면 눈물나긴 하더라고요",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008382.1.37",
          "form": "내가 호르몬에 지는 사람이구나 깨닫는",
          "original_form": "내가 호르몬에 지는 사람이구나 깨닫는",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008382.1.38",
          "form": "아 맞아요 ㅠㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008382.1.39",
          "form": "스트레스도 장난 아니고 ㅠㅠ",
          "original_form": "스트레스도 장난 아니고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008382.1.40",
          "form": "생리할때 되면 미치려해요 .",
          "original_form": "생리할때 되면 미치려해요 .",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008382.1.41",
          "form": "저희 행복한 얘기해요",
          "original_form": "저희 행복한 얘기해요",
