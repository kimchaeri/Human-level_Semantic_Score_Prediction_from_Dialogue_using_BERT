      "utterance": [
        {
          "id": "MDRW2100013341.1.1",
          "form": "다녀온 여행지 중에 여긴 또 가고싶다하는 여행지 있으신가요?!",
          "original_form": "다녀온 여행지 중에 여긴 또 가고싶다하는 여행지 있으신가요?!",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013341.1.2",
          "form": "저는 스페인이요!",
          "original_form": "저는 스페인이요!",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013341.1.3",
          "form": "사그라다 파밀리아 완공된다고 해서...",
          "original_form": "사그라다 파밀리아 완공된다고 해서...",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013341.1.4",
          "form": "으와 헐... 진짜요?",
          "original_form": "으와 헐... 진짜요?",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013341.1.5",
          "form": "아 저도 갑자기 가고싶어지는데요...?",
          "original_form": "아 저도 갑자기 가고싶어지는데요...?",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013341.1.6",
          "form": "한번쯤 꼭 가보세용",
          "original_form": "한번쯤 꼭 가보세용",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013341.1.7",
          "form": "진짜 예뻐요",
          "original_form": "진짜 예뻐요",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013341.1.8",
          "form": "하 진짜 코로나 끝나면 꼭 가야겠어요...",
          "original_form": "하 진짜 코로나 끝나면 꼭 가야겠어요...",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013341.1.9",
          "form": "그럼 가고싶은 곳은 있으신가요? 그냥 가고싶은 곳!",
          "original_form": "그럼 가고싶은 곳은 있으신가요? 그냥 가고싶은 곳!",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013341.1.10",
          "form": "터키 가보고 싶어요!",
          "original_form": "터키 가보고 싶어요!",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013341.1.11",
          "form": "음식이 맛있대서요",
          "original_form": "음식이 맛있대서요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013341.1.12",
          "form": "아 헐 맞아요 터키 음식 맛있다고 하죠ㅠ",
          "original_form": "아 헐 맞아요 터키 음식 맛있다고 하죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013341.1.13",
          "form": "전 호주가 너무 가고싶어요...",
          "original_form": "전 호주가 너무 가고싶어요...",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013341.1.14",
          "form": "헉 호주",
          "original_form": "헉 호주",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013341.1.15",
          "form": "코알라 보고 싶어요",
          "original_form": "코알라 보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013341.1.16",
          "form": "아놔 저는 캥거루요",
          "original_form": "아놔 저는 캥거루요",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013341.1.17",
          "form": "ㅋㅋㅋㅋㅋ해쭈라고...유튜버 있는데 그 영상에서 캥거루 보니까 귀엽더라구요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ해쭈라고...유튜버 있는데 그 영상에서 캥거루 보니까 귀엽더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013341.1.18",
          "form": "헉 해쭈 아시는구나",
          "original_form": "헉 해쭈 아시는구나",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013341.1.19",
          "form": "캥거루 실제로 보면 엄청... 크겠죠?",
          "original_form": "캥거루 실제로 보면 엄청... 크겠죠?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013341.1.20",
          "form": "그럴 거 같아요 ㅋㅋㅋㅋ 그 영상보면 엄청 크더라고요?!?!",
          "original_form": "그럴 거 같아요 ㅋㅋㅋㅋ 그 영상보면 엄청 크더라고요?!?!",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013341.1.21",
          "form": "괜히 복싱하는 캥거루 캐릭터가 있는 게 아닌가봐요",
          "original_form": "괜히 복싱하는 캥거루 캐릭터가 있는 게 아닌가봐요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013341.1.22",
          "form": "근육 대박",
          "original_form": "근육 대박",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013341.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요!!!!",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013341.1.24",
          "form": "그 진짜 약간....제가 질거같아요",
          "original_form": "그 진짜 약간....제가 질거같아요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013341.1.25",
          "form": "코로나 끝나면 바로 갈 여행지 있으신가요?",
          "original_form": "코로나 끝나면 바로 갈 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013341.1.26",
          "form": "음... 원래 일본 가려고 했는데",
          "original_form": "음... 원래 일본 가려고 했는데",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013341.1.27",
          "form": "방사능 때문에 또 좀 무섭고 그러네요",
          "original_form": "방사능 때문에 또 좀 무섭고 그러네요",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013341.1.28",
          "form": "아 헐 저도요ㅠ",
          "original_form": "아 헐 저도요ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013341.1.29",
          "form": "저 도쿄가 너무 가고싶어요...",
          "original_form": "저 도쿄가 너무 가고싶어요...",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013341.1.30",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013341.1.31",
          "form": "그냥 간단하게 짐싸서",
          "original_form": "그냥 간단하게 짐싸서",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013341.1.32",
          "form": "돌아다니고 싶어요",
          "original_form": "돌아다니고 싶어요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        }
      ]
