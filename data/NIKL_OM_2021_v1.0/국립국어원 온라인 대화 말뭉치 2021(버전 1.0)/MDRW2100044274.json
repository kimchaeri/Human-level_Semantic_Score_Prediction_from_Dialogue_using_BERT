      "utterance": [
        {
          "id": "MDRW2100044274.1.1",
          "form": "아뇨 동방명주 예쁘다는 얘기만 들었어요",
          "original_form": "아뇨 동방명주 예쁘다는 얘기만 들었어요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044274.1.2",
          "form": "동방멍주가 홍콩에 있군요~",
          "original_form": "동방멍주가 홍콩에 있군요~",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044274.1.3",
          "form": "아 상해에 있던가요?",
          "original_form": "아 상해에 있던가요?",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044274.1.4",
          "form": "중국 헷갈려요 ㅋㅋㅋㅋㅋ ㅠ",
          "original_form": "중국 헷갈려요 ㅋㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044274.1.5",
          "form": "네~ 상해네요",
          "original_form": "네~ 상해네요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044274.1.6",
          "form": "여긴 제가 가봤고든요",
          "original_form": "여긴 제가 가봤고든요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044274.1.7",
          "form": "와 어떤가요?",
          "original_form": "와 어떤가요?",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044274.1.8",
          "form": "하지만 제가 교유명사에 약해서 상해에 있던건 동방 뭐였지? 하고 있었어요",
          "original_form": "하지만 제가 교유명사에 약해서 상해에 있던건 동방 뭐였지? 하고 있었어요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044274.1.9",
          "form": "제가 고소공포증이 있어서 후덜덩 했어요",
          "original_form": "제가 고소공포증이 있어서 후덜덩 했어요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044274.1.10",
          "form": "아 전망대까지 올라가셨군요",
          "original_form": "아 전망대까지 올라가셨군요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044274.1.11",
          "form": "네 유리바닥이 있었는데 전 도저히 못가겠던데요",
          "original_form": "네 유리바닥이 있었는데 전 도저히 못가겠던데요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044274.1.12",
          "form": "와 유리바닥은 저도 못갈거같아요",
          "original_form": "와 유리바닥은 저도 못갈거같아요",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044274.1.13",
          "form": "이게 안무너지는거 아는데도 못가겠어요",
          "original_form": "이게 안무너지는거 아는데도 못가겠어요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044274.1.14",
          "form": "그쵸 ㅋㅋㅋ 사람 심리가...",
          "original_form": "그쵸 ㅋㅋㅋ 사람 심리가...",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044274.1.15",
          "form": "그런거 아랑곳 하지 않는 사람도 많더라고요",
          "original_form": "그런거 아랑곳 하지 않는 사람도 많더라고요",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044274.1.16",
          "form": "담력이 부럽네요...",
          "original_form": "담력이 부럽네요...",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044274.1.17",
          "form": "전 놀이공원 가도 롤러코스터 잘 못타요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "전 놀이공원 가도 롤러코스터 잘 못타요 ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044274.1.18",
          "form": "저도요ㅡ. 전 바이킹도 못타요 ㅠ",
          "original_form": "저도요ㅡ. 전 바이킹도 못타요 ㅠ",
          "speaker_id": "1",
