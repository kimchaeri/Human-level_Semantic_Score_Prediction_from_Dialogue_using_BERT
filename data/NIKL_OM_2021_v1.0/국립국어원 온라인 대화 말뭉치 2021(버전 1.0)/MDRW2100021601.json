        },
        {
          "id": "MDRW2100021601.1.3",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021601.1.4",
          "form": "그 매직 음료수 봄??",
          "original_form": "그 매직 음료수 봄??",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.5",
          "form": "문제있다고 했자나",
          "original_form": "문제있다고 했자나",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.6",
          "form": "매직음료수?",
          "original_form": "매직음료수?",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.7",
          "form": "근데 나는 곰표 후드티 누가 사입어 이랫는데",
          "original_form": "근데 나는 곰표 후드티 누가 사입어 이랫는데",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.8",
          "form": "그게 우리 동생",
          "original_form": "그게 우리 동생",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.11",
          "form": "곰표 그 디자인 인기 장난 아니더라",
          "original_form": "곰표 그 디자인 인기 장난 아니더라",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.13",
          "form": "맥주는 아직도 구하기 힘들다고...?",
          "original_form": "맥주는 아직도 구하기 힘들다고...?",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021601.1.14",
          "form": "제분회사 직원같던데",
          "original_form": "제분회사 직원같던데",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021601.1.15",
          "form": "근데 맛있나?",
          "original_form": "근데 맛있나?",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021601.1.16",
          "form": "매직 음료수 뭐냐면",
          "original_form": "매직 음료수 뭐냐면",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021601.1.17",
          "form": "매직인데 펜 모양 아니구...",
          "original_form": "매직인데 펜 모양 아니구...",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021601.1.18",
          "form": "암튼 그걸 음료수로 해서 팔았어",
          "original_form": "암튼 그걸 음료수로 해서 팔았어",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021601.1.19",
          "form": "글구 구두약 초콜렛",
          "original_form": "글구 구두약 초콜렛",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021601.1.20",
          "form": "이런거 헷갈려서 먹을 수 있다고 그런 얘기가 있더아",
          "original_form": "이런거 헷갈려서 먹을 수 있다고 그런 얘기가 있더아",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021601.1.21",
          "form": "암틈 곰표가 잘 돼서 여기저기서 콜라보하는듯",
          "original_form": "암틈 곰표가 잘 돼서 여기저기서 콜라보하는듯",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021601.1.22",
          "form": "오 그게 좀 예스러워(?)서 ㅋㅋ 힙한 감성이 됐어",
          "original_form": "오 그게 좀 예스러워(?)서 ㅋㅋ 힙한 감성이 됐어",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021601.1.23",
          "form": "두꺼비는 감자칩도 있던데 ㅋㅋㅋ",
          "original_form": "두꺼비는 감자칩도 있던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100021601.1.24",
          "form": "난 보다가 뭐야 이거 술 넣은 거야? 했옼ㅋㅋㅋ",
          "original_form": "난 보다가 뭐야 이거 술 넣은 거야? 했옼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100021601.1.25",
          "form": "아 그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100021601.1.26",
          "form": "술맛날 것 같은 비주얼",
          "original_form": "술맛날 것 같은 비주얼",
          "speaker_id": "1",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100021601.1.27",
          "form": "두꺼비는 귀여워서 그랬나?",
          "original_form": "두꺼비는 귀여워서 그랬나?",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021601.1.28",
          "form": "암튼 하나가 뜨면 원소스 멀티유즈가 너무 잘돼",
          "original_form": "암튼 하나가 뜨면 원소스 멀티유즈가 너무 잘돼",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021601.1.29",
          "form": "원소스 멀티유즈 고급단어다",
          "original_form": "원소스 멀티유즈 고급단어다",
          "speaker_id": "1",
          "time": "20210828 21:02"
        },
        {
