          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021601.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
