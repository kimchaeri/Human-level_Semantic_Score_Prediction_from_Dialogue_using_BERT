        {
          "id": "MDRW2100045665.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ name2님 배안고프세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ &name2&님 배안고프세요?",
          "speaker_id": "1",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100045665.1.2",
          "form": "ㅋㅋㅋㅋ네 안고파요",
          "original_form": "ㅋㅋㅋㅋ네 안고파요",
          "speaker_id": "2",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045665.1.3",
          "form": "ㅠㅠ 저는 너무 배고파요.. 어제 저녁 안먹었고든요",
          "original_form": "ㅠㅠ 저는 너무 배고파요.. 어제 저녁 안먹었고든요",
          "speaker_id": "1",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045665.1.4",
          "form": "헉 그러시군요",
          "original_form": "헉 그러시군요",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045665.1.5",
          "form": "무슨 일 있으셨어요?",
          "original_form": "무슨 일 있으셨어요?",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045665.1.6",
          "form": "점심에 너무 맛없는걸 먹었더니 입맛 떨어져서 못먹었어요...ㅠㅠㅠ",
          "original_form": "점심에 너무 맛없는걸 먹었더니 입맛 떨어져서 못먹었어요...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045665.1.7",
          "form": "서러워요",
          "original_form": "서러워요",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045665.1.8",
          "form": "헉 뭘 드셨길래요",
          "original_form": "헉 뭘 드셨길래요",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045665.1.9",
          "form": "편의점에사 삼겹살김밥을 샀거든요?",
          "original_form": "편의점에사 삼겹살김밥을 샀거든요?",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045665.1.10",
          "form": "제가 김밥도 삼겹살도 진짜 좋아해요..",
          "original_form": "제가 김밥도 삼겹살도 진짜 좋아해요..",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045665.1.11",
          "form": "근데 삼겹살이 아니라 그냥 비계 밖에 없고",
          "original_form": "근데 삼겹살이 아니라 그냥 비계 밖에 없고",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045665.1.12",
          "form": "김밥도 맛었었어요ㅠㅠㅠㅠ",
          "original_form": "김밥도 맛었었어요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045665.1.13",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045665.1.14",
          "form": "삼겹살 김밥...",
          "original_form": "삼겹살 김밥...",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045665.1.15",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045665.1.16",
          "form": "저도 얼마전에 삼각김밥을 샀는데요",
          "original_form": "저도 얼마전에 삼각김밥을 샀는데요",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045665.1.17",
          "form": "삼각김밥 속이 거의 없다시피 하더라구요",
          "original_form": "삼각김밥 속이 거의 없다시피 하더라구요",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045665.1.18",
          "form": "ㅠㅠㅠㅠ망할 편의점들이네요",
          "original_form": "ㅠㅠㅠㅠ망할 편의점들이네요",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045665.1.19",
          "form": "배고파서 꾸역꾸역 먹긴했는데",
          "original_form": "배고파서 꾸역꾸역 먹긴했는데",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045665.1.20",
          "form": "진짜 짜증났어요",
          "original_form": "진짜 짜증났어요",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045665.1.21",
          "form": "저희 속지말기로해요ㅠ",
          "original_form": "저희 속지말기로해요ㅠ",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045665.1.22",
          "form": "저도 당황해서 그냥 먹었는데요",
          "original_form": "저도 당황해서 그냥 먹었는데요",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045665.1.23",
          "form": "검색해보니 속이 부실하면 사진찍어서 보상요구할 수 있대요",
          "original_form": "검색해보니 속이 부실하면 사진찍어서 보상요구할 수 있대요",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045665.1.24",
          "form": "그 인건비가 더 들겠지만요 ㅡㅡ",
          "original_form": "그 인건비가 더 들겠지만요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045665.1.25",
          "form": "아ㅡㅡ 몰랐어요",
          "original_form": "아ㅡㅡ 몰랐어요",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045665.1.26",
          "form": "앞으론 사먹지도 않겠지만 주위에서 당하면 알려줘야겠네요ㅠ",
          "original_form": "앞으론 사먹지도 않겠지만 주위에서 당하면 알려줘야겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045665.1.27",
          "form": "네 주변인들께",
