      "utterance": [
        {
          "id": "MDRW2100042714.1.1",
          "form": "그래서 지금 나와서 버스타러 가는 중이어요",
          "original_form": "그래서 지금 나와서 버스타러 가는 중이어요",
          "speaker_id": "2",
          "time": "20210907 07:26"
        },
        {
          "id": "MDRW2100042714.1.2",
          "form": "버스 어제만큼 막힐거 같은데",
          "original_form": "버스 어제만큼 막힐거 같은데",
          "speaker_id": "1",
          "time": "20210907 07:26"
        },
        {
          "id": "MDRW2100042714.1.3",
          "form": "그럼 중간에 지하철로 또 갈아타야지",
          "original_form": "그럼 중간에 지하철로 또 갈아타야지",
          "speaker_id": "2",
          "time": "20210907 07:27"
        },
        {
          "id": "MDRW2100042714.1.4",
          "form": "비오니까 넘 추워",
          "original_form": "비오니까 넘 추워",
          "speaker_id": "2",
          "time": "20210907 07:27"
        },
        {
          "id": "MDRW2100042714.1.5",
          "form": "겉옷을 챙겨가",
          "original_form": "겉옷을 챙겨가",
          "speaker_id": "1",
          "time": "20210907 07:27"
        },
        {
          "id": "MDRW2100042714.1.6",
          "form": "그래서 오늘 맨투맨입고 가려구",
          "original_form": "그래서 오늘 맨투맨입고 가려구",
          "speaker_id": "2",
          "time": "20210907 07:28"
        },
        {
          "id": "MDRW2100042714.1.7",
          "form": "학원도 에어컨이 너무 빵빵해서 추워",
          "original_form": "학원도 에어컨이 너무 빵빵해서 추워",
          "speaker_id": "2",
          "time": "20210907 07:28"
        },
        {
          "id": "MDRW2100042714.1.8",
          "form": "에어컨 꺼달라고하지",
          "original_form": "에어컨 꺼달라고하지",
          "speaker_id": "1",
          "time": "20210907 07:28"
        },
        {
          "id": "MDRW2100042714.1.9",
          "form": "나만 추운거일수도 있자나",
          "original_form": "나만 추운거일수도 있자나",
          "speaker_id": "2",
          "time": "20210907 07:28"
        },
        {
          "id": "MDRW2100042714.1.10",
          "form": "내가 추위를 많이 타니까?!",
          "original_form": "내가 추위를 많이 타니까?!",
          "speaker_id": "2",
          "time": "20210907 07:29"
        },
        {
          "id": "MDRW2100042714.1.11",
          "form": "그래서 말안했지",
          "original_form": "그래서 말안했지",
          "speaker_id": "2",
          "time": "20210907 07:29"
        },
        {
          "id": "MDRW2100042714.1.12",
          "form": "그러면 두꺼운 담요라도 챙겨야겠네",
          "original_form": "그러면 두꺼운 담요라도 챙겨야겠네",
          "speaker_id": "1",
          "time": "20210907 07:29"
        },
        {
          "id": "MDRW2100042714.1.13",
          "form": "오늘도 추우면 그래야겠어",
          "original_form": "오늘도 추우면 그래야겠어",
          "speaker_id": "2",
          "time": "20210907 07:29"
        },
        {
          "id": "MDRW2100042714.1.14",
          "form": "오늘은 어땠어",
          "original_form": "오늘은 어땠어",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100042714.1.15",
          "form": "오늘도 엄청 추웠어",
          "original_form": "오늘도 엄청 추웠어",
          "speaker_id": "2",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100042714.1.16",
          "form": "내일 더 두꺼운 옷입어야겠네",
          "original_form": "내일 더 두꺼운 옷입어야겠네",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100042714.1.17",
          "form": "무릎담요 챙기려구",
          "original_form": "무릎담요 챙기려구",
          "speaker_id": "2",
          "time": "20210907 15:24"
