      "utterance": [
        {
          "id": "MDRW2100026747.1.1",
          "form": "오늘 좀 쌀쌀하지 않았어요?",
          "original_form": "오늘 좀 쌀쌀하지 않았어요?",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026747.1.2",
          "form": "네네 평소보다 시원하더라고요",
          "original_form": "네네 평소보다 시원하더라고요",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026747.1.3",
          "form": "비온다했는데 안와서 좋았어요!!",
          "original_form": "비온다했는데 안와서 좋았어요!!",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026747.1.4",
          "form": "아, 맞아요. 습도는 좀 있었는데 비는 안와서 좋았어요",
          "original_form": "아, 맞아요. 습도는 좀 있었는데 비는 안와서 좋았어요",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026747.1.5",
          "form": "어제 퇴근하는데 신발에 물들어가서 ㅠㅠ",
          "original_form": "어제 퇴근하는데 신발에 물들어가서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026747.1.6",
          "form": "아ㅠㅠ 어제 근데 진짜 비가 너무많이왔어요",
          "original_form": "아ㅠㅠ 어제 근데 진짜 비가 너무많이왔어요",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026747.1.7",
          "form": "네, 진심.",
          "original_form": "네, 진심.",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026747.1.8",
          "form": "장마철에도 그렇게 안왔는데 왜 가을 다 돼서...ㅂㄷㅂㄷ",
          "original_form": "장마철에도 그렇게 안왔는데 왜 가을 다 돼서...ㅂㄷㅂㄷ",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026747.1.9",
          "form": "아진짜욬ㅋㅋㅋ",
          "original_form": "아진짜욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026747.1.10",
          "form": "뒷북장마",
          "original_form": "뒷북장마",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026747.1.11",
          "form": "눈치챙겼으면 좋겠어요ㅋㅋ",
          "original_form": "눈치챙겼으면 좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026747.1.12",
          "form": "흑, 어젠 장화를 신었어야 했는데",
          "original_form": "흑, 어젠 장화를 신었어야 했는데",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026747.1.13",
          "form": "출근길엔 또 별로 안와서... ㅠ",
          "original_form": "출근길엔 또 별로 안와서... ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026747.1.14",
          "form": "날씨가 눈치있게",
          "original_form": "날씨가 눈치있게",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026747.1.15",
          "form": "출퇴근시간에는 알아서 비 멈춰줬음 좋겠어요ㅋㅋ",
          "original_form": "출퇴근시간에는 알아서 비 멈춰줬음 좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026747.1.16",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026747.1.17",
          "form": "조금 자기중심적이긴 한데 나 나갈때는 그쳤으면",
          "original_form": "조금 자기중심적이긴 한데 나 나갈때는 그쳤으면",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026747.1.18",
          "form": "맞아요ㅋㅋㅋㅋ 그때만 딱 맑아지면",
          "original_form": "맞아요ㅋㅋㅋㅋ 그때만 딱 맑아지면",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100026747.1.19",
          "form": "기분이 넘나좋겠지만ㅋㅋ",
          "original_form": "기분이 넘나좋겠지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100026747.1.20",
          "form": "그치만 제가 세상의 주인공이 아니라 ㅠㅠ",
          "original_form": "그치만 제가 세상의 주인공이 아니라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100026747.1.21",
          "form": "그냥 올거면 나갈때도 왔으면 좋겠다",
          "original_form": "그냥 올거면 나갈때도 왔으면 좋겠다",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100026747.1.22",
          "form": "갈때만 와서 우산도 없게 만들지 말고 ㅠㅠ",
          "original_form": "갈때만 와서 우산도 없게 만들지 말고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100026747.1.23",
          "form": "아진짜요 갈때만 오는거요ㅋㅋㅋㅋ",
          "original_form": "아진짜요 갈때만 오는거요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100026747.1.24",
          "form": "집에 쌓여가는 편의점 우산ㅋㅋ",
          "original_form": "집에 쌓여가는 편의점 우산ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100026747.1.25",
          "form": "편의점 우산만 잔뜩 쌓여가요ㅋㅋ",
          "original_form": "편의점 우산만 잔뜩 쌓여가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:55"
        }
      ]
