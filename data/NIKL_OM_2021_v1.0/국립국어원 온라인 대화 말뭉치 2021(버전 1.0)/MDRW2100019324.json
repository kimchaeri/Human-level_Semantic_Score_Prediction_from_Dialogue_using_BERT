        },
        {
          "id": "MDRW2100019324.1.6",
          "form": "등등",
          "original_form": "등등",
          "speaker_id": "2",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019324.1.7",
          "form": "많이 구매햇네요 ㅋㅋㅋ",
          "original_form": "많이 구매햇네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019324.1.8",
          "form": "오오 많이 구매하셨구나 ㅋㅋㅋㅋ",
          "original_form": "오오 많이 구매하셨구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019324.1.9",
          "form": "넹 돈좀 써서 씁쓸 ㅋㅋ",
          "original_form": "넹 돈좀 써서 씁쓸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019324.1.10",
          "form": "구래도 저렴하게 구매했으니",
          "original_form": "구래도 저렴하게 구매했으니",
          "speaker_id": "1",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019324.1.11",
          "form": "넼ㅋㅋㅋㅋ 그런편이죠",
          "original_form": "넼ㅋㅋㅋㅋ 그런편이죠",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019324.1.12",
          "form": "덜 씁슬하실듯해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "덜 씁슬하실듯해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019324.1.13",
          "form": "맞는 말씀이세요 ㅋㅋㅋㅋ 덜씁쑬",
          "original_form": "맞는 말씀이세요 ㅋㅋㅋㅋ 덜씁쑬",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019324.1.14",
          "form": "또 어디 할인하는 사이트 있나요?? 궁금",
          "original_form": "또 어디 할인하는 사이트 있나요?? 궁금",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019324.1.15",
          "form": "음 ㅜㅜ 저도ㅜ 더이상은 모르겠으나ㅋㅋㅋㅋ",
          "original_form": "음 ㅜㅜ 저도ㅜ 더이상은 모르겠으나ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019324.1.16",
          "form": "아하 그렇군요 ㅋㅋㅋㅋㅋ  또 좋아하는 브랜드잇나요?",
          "original_form": "아하 그렇군요 ㅋㅋㅋㅋㅋ  또 좋아하는 브랜드잇나요?",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
