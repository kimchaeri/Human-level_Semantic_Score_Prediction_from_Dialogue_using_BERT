      "utterance": [
        {
          "id": "MDRW2100019324.1.1",
          "form": "좋은 생각 이신듯 ㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋ 꼭사요",
          "original_form": "좋은 생각 이신듯 ㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋ 꼭사요",
          "speaker_id": "2",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100019324.1.2",
          "form": "타미 옷 뭐 사셨어요?!??!",
          "original_form": "타미 옷 뭐 사셨어요?!??!",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019324.1.3",
          "form": "자켓말고",
          "original_form": "자켓말고",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019324.1.4",
          "form": "니트랑 자켓",
          "original_form": "니트랑 자켓",
          "speaker_id": "2",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019324.1.5",
          "form": "모자",
          "original_form": "모자",
          "speaker_id": "2",
