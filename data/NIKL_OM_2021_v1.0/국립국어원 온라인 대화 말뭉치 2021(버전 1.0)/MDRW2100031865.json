      "utterance": [
        {
          "id": "MDRW2100031865.1.1",
          "form": "심심이는 핸드폰이 서브폰이애오ㅎㅎ",
          "original_form": "심심이는 핸드폰이 서브폰이애오ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100031865.1.2",
          "form": "그래서 잘 확인이안됨요ㅎㅎ",
          "original_form": "그래서 잘 확인이안됨요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100031865.1.3",
          "form": "ㅎㅎㅎ  네네",
          "original_form": "ㅎㅎㅎ  네네",
          "speaker_id": "2",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100031865.1.4",
          "form": "어제 집안일도 안해ㅆ는데",
          "original_form": "어제 집안일도 안해ㅆ는데",
          "speaker_id": "1",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100031865.1.5",
          "form": "왜케 피곤하던지ㅎㅎ",
          "original_form": "왜케 피곤하던지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:18"
        },
        {
          "id": "MDRW2100031865.1.6",
          "form": "주말이자나여",
          "original_form": "주말이자나여",
          "speaker_id": "2",
          "time": "20210905 08:18"
        },
        {
          "id": "MDRW2100031865.1.7",
          "form": "오늘은 아침부터 청소하니깐 그때까지 열씸히해야죠ㅎㅎ",
          "original_form": "오늘은 아침부터 청소하니깐 그때까지 열씸히해야죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:18"
        },
        {
          "id": "MDRW2100031865.1.8",
          "form": "ㅎㅎ 주말이네요ㅎ 가계부써야하는데ㅠ",
          "original_form": "ㅎㅎ 주말이네요ㅎ 가계부써야하는데ㅠ",
          "speaker_id": "1",
          "time": "20210905 08:18"
        },
        {
          "id": "MDRW2100031865.1.9",
          "form": "밀리지 말고 써여 ㅋ",
          "original_form": "밀리지 말고 써여 ㅋ",
          "speaker_id": "2",
          "time": "20210905 08:18"
        },
        {
          "id": "MDRW2100031865.1.10",
          "form": "어제 만들었어야 했는데",
          "original_form": "어제 만들었어야 했는데",
          "speaker_id": "1",
          "time": "20210905 08:19"
        },
        {
          "id": "MDRW2100031865.1.11",
          "form": "12시에 쓰러짐요 ㅠ",
          "original_form": "12시에 쓰러짐요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 08:19"
        },
        {
          "id": "MDRW2100031865.1.12",
          "form": "아아님은 가계부만들었어요?",
          "original_form": "아아님은 가계부만들었어요?",
          "speaker_id": "1",
          "time": "20210905 08:19"
        },
        {
          "id": "MDRW2100031865.1.13",
          "form": "네 전 달력에 써요 ㅎㅎ",
          "original_form": "네 전 달력에 써요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:19"
        },
        {
          "id": "MDRW2100031865.1.14",
          "form": "아아ㅎ 좋은 방법이네요ㅎ",
          "original_form": "아아ㅎ 좋은 방법이네요ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:20"
        },
        {
          "id": "MDRW2100031865.1.15",
          "form": "저도 매일 기록해야겟어요ㅎ",
          "original_form": "저도 매일 기록해야겟어요ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:20"
        },
        {
          "id": "MDRW2100031865.1.16",
          "form": "보인데다 쓰니ㅜ",
          "original_form": "보인데다 쓰니ㅜ",
          "speaker_id": "2",
          "time": "20210905 08:20"
        },
        {
          "id": "MDRW2100031865.1.17",
          "form": "덜 쓰게됨 ㅋㅋㅋ",
          "original_form": "덜 쓰게됨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:20"
        },
        {
          "id": "MDRW2100031865.1.18",
          "form": "어제 무지출 성공했는데",
          "original_form": "어제 무지출 성공했는데",
          "speaker_id": "1",
          "time": "20210905 08:20"
        },
        {
          "id": "MDRW2100031865.1.19",
          "form": "돈돈모 이게 효과가 있긴해요ㅎ",
          "original_form": "돈돈모 이게 효과가 있긴해요ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:20"
        },
        {
          "id": "MDRW2100031865.1.20",
          "form": "신경쓰여서 그런지ㅎ 안쓰게 되더라구요ㅎ 특히 저같은 경우는 생각도 많아서 그런지ㅎ",
          "original_form": "신경쓰여서 그런지ㅎ 안쓰게 되더라구요ㅎ 특히 저같은 경우는 생각도 많아서 그런지ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:21"
        },
        {
          "id": "MDRW2100031865.1.21",
          "form": "아무래도 같이 하니 ㅎ",
          "original_form": "아무래도 같이 하니 ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:21"
        },
        {
          "id": "MDRW2100031865.1.22",
          "form": "근데 저만 안쓰면되나요뭐ㅎㅎ 아내가 안써야지ㅎㅎ",
          "original_form": "근데 저만 안쓰면되나요뭐ㅎㅎ 아내가 안써야지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:21"
        },
        {
          "id": "MDRW2100031865.1.23",
          "form": "혼자 많이 쓰니 싶엇 ㅎㅎ",
          "original_form": "혼자 많이 쓰니 싶엇 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:21"
        },
        {
          "id": "MDRW2100031865.1.24",
          "form": "어제 다행이 안써서ㅎ",
          "original_form": "어제 다행이 안써서ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:21"
        },
        {
          "id": "MDRW2100031865.1.25",
          "form": "아 어제  차 계약 하신?",
          "original_form": "아 어제  차 계약 하신?",
          "speaker_id": "2",
          "time": "20210905 08:21"
        },
        {
          "id": "MDRW2100031865.1.26",
          "form": "모르겟네요ㅎ",
          "original_form": "모르겟네요ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:22"
        },
        {
          "id": "MDRW2100031865.1.27",
          "form": "re",
          "original_form": "re",
          "speaker_id": "2",
          "time": "20210905 08:22"
        },
        {
          "id": "MDRW2100031865.1.28",
          "form": "ㄱㄱ요ㅎ",
          "original_form": "ㄱㄱ요ㅎ",
