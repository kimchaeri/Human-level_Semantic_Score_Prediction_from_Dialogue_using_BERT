      "utterance": [
        {
          "id": "MDRW2100017779.1.1",
          "form": "여행 경험 많으신가요?",
          "original_form": "여행 경험 많으신가요?",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017779.1.2",
          "form": "아뇨 많치 않습니다  name1님은요?",
          "original_form": "아뇨 많치 않습니다  &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017779.1.3",
          "form": "저도 집돌이라서... \r\n가끔씩 기차타고 싶을때 가는 정도에요..",
          "original_form": "저도 집돌이라서... \r\n가끔씩 기차타고 싶을때 가는 정도에요..",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017779.1.4",
          "form": "해외도 가보셧어여?",
          "original_form": "해외도 가보셧어여?",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017779.1.5",
          "form": "패키지로가봤는데 좋았습니다ㅎㅎ",
          "original_form": "패키지로가봤는데 좋았습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017779.1.6",
          "form": "여행은 즐거운거같아요",
          "original_form": "여행은 즐거운거같아요",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017779.1.7",
          "form": "어느나라염?",
          "original_form": "어느나라염?",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017779.1.8",
          "form": "대만이요",
          "original_form": "대만이요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017779.1.9",
          "form": "코로나 터지기전이였는데",
          "original_form": "코로나 터지기전이였는데",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017779.1.10",
          "form": "진짜즐거웠어요ㅎㅎ",
          "original_form": "진짜즐거웠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017779.1.11",
          "form": "여름에 가면 쪄죽는다던데... \r\n그 101타워인가요?",
          "original_form": "여름에 가면 쪄죽는다던데... \r\n그 101타워인가요?",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017779.1.12",
          "form": "사각형 겹겹이올라가는..",
          "original_form": "사각형 겹겹이올라가는..",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017779.1.13",
          "form": "저는 그타워말고 예스진지 투어라는거 갔는데",
          "original_form": "저는 그타워말고 예스진지 투어라는거 갔는데",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017779.1.14",
          "form": "진저스? 탄광촌마을?",
          "original_form": "진저스? 탄광촌마을?",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017779.1.15",
          "form": "거기갔는데 복잡해서 길잃어버리는줄알았어요ㅜㅜ",
          "original_form": "거기갔는데 복잡해서 길잃어버리는줄알았어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017779.1.16",
          "form": "ㅋㅋㅋㅋ 가이드 있지 않아요ㅡ",
          "original_form": "ㅋㅋㅋㅋ 가이드 있지 않아요ㅡ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017779.1.17",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017779.1.18",
          "form": "자유시간이였는데",
          "original_form": "자유시간이였는데",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017779.1.19",
          "form": "다똑같이생기니까 돌아가는길을모르겠더라구요ㅜㅜ",
          "original_form": "다똑같이생기니까 돌아가는길을모르겠더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017779.1.20",
          "form": "그래서 결국에는 가이드분이 찾으러오셨는데 어찌나 구세주같던지",
          "original_form": "그래서 결국에는 가이드분이 찾으러오셨는데 어찌나 구세주같던지",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017779.1.21",
          "form": "ㅋㅋㅋ 해외에서 길모르고 진짜 식은땀나죠..",
          "original_form": "ㅋㅋㅋ 해외에서 길모르고 진짜 식은땀나죠..",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017779.1.22",
          "form": "말도 안통하고..",
          "original_form": "말도 안통하고..",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017779.1.23",
          "form": "심지어 골목길이니까 지피에스도 잘안먹혀서",
          "original_form": "심지어 골목길이니까 지피에스도 잘안먹혀서",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017779.1.24",
          "form": "이대로 국제미아되는건가 진짜 캄캄했는데",
          "original_form": "이대로 국제미아되는건가 진짜 캄캄했는데",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017779.1.25",
          "form": "가이드분깃발이 보이니까 진짜 안심",
          "original_form": "가이드분깃발이 보이니까 진짜 안심",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017779.1.26",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017779.1.27",
          "form": "까딱했음.. 지금 어디서 새우잡고 있으실뻔..",
          "original_form": "까딱했음.. 지금 어디서 새우잡고 있으실뻔..",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017779.1.28",
          "form": "그니까요 그래서 그뒤로 가이드분 뒤만 졸졸따라니고",
          "original_form": "그니까요 그래서 그뒤로 가이드분 뒤만 졸졸따라니고",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017779.1.29",
          "form": "가이드분한테 요주인물이라고 계속 체크당했어요ㅎ",
          "original_form": "가이드분한테 요주인물이라고 계속 체크당했어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017779.1.30",
          "form": "ㅋㅋㅋ 며칠짜리 였어요?",
          "original_form": "ㅋㅋㅋ 며칠짜리 였어요?",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017779.1.31",
          "form": "그때는 무서웠는데  지나고보니까 추억이라  즐거웠어요",
          "original_form": "그때는 무서웠는데  지나고보니까 추억이라  즐거웠어요",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
