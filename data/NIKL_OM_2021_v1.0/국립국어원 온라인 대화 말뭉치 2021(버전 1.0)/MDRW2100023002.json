      "utterance": [
        {
          "id": "MDRW2100023002.1.1",
          "form": "안녕하세요 name2",
          "original_form": "안녕하세요 &name2&",
          "speaker_id": "1",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100023002.1.2",
          "form": "안녕하세요~^^",
          "original_form": "안녕하세요~^^",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100023002.1.3",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023002.1.4",
          "form": "네 결혼했어요",
          "original_form": "네 결혼했어요",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023002.1.5",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023002.1.6",
          "form": "결혼하셨을까요?!",
          "original_form": "결혼하셨을까요?!",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023002.1.7",
          "form": "네네ㅎㅎ 저두요 ㅡㅎㅎ",
          "original_form": "네네ㅎㅎ 저두요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023002.1.8",
          "form": "어때요? 결혼하니깐?",
          "original_form": "어때요? 결혼하니깐?",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023002.1.9",
          "form": "전 3년됬어요ㅎ",
          "original_form": "전 3년됬어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023002.1.10",
          "form": "전 2년요",
          "original_form": "전 2년요",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023002.1.11",
          "form": "결혼하길 잘했다 싶어요",
          "original_form": "결혼하길 잘했다 싶어요",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023002.1.12",
          "form": "오오ㅎ 어떤부분이요?",
          "original_form": "오오ㅎ 어떤부분이요?",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.13",
          "form": "저랑 안전 반대라ㄹㅎㄹ",
          "original_form": "저랑 안전 반대라ㄹㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.14",
          "form": "완전ㅎ",
          "original_form": "완전ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.15",
          "form": "안정감이요? ㅎㅎ",
          "original_form": "안정감이요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.16",
          "form": "어떠실까요? name2은?",
          "original_form": "어떠실까요? &name2&은?",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.17",
          "form": "아ㅎㅎ 안전감은 있는데ㅎㅎ",
          "original_form": "아ㅎㅎ 안전감은 있는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.18",
          "form": "저는 좀 그래서ㅠ",
          "original_form": "저는 좀 그래서ㅠ",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.19",
          "form": "곧 이혼해요ㅎ",
          "original_form": "곧 이혼해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023002.1.20",
          "form": "뭐 이래저래 많은일들이있어서ㅎ",
          "original_form": "뭐 이래저래 많은일들이있어서ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100023002.1.21",
          "form": "아이고;;;",
          "original_form": "아이고;;;",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100023002.1.22",
          "form": "위로 건네봅니다....",
          "original_form": "위로 건네봅니다....",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100023002.1.23",
          "form": "아니에요ㅎㅎ",
          "original_form": "아니에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023002.1.24",
          "form": "서로 행복하자고 떠나는거라ㅎㅎ",
          "original_form": "서로 행복하자고 떠나는거라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023002.1.25",
          "form": "같이 붙어있으면 싸우기만하더라구요",
          "original_form": "같이 붙어있으면 싸우기만하더라구요",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023002.1.26",
          "form": "앞으로 행복한일들만 가득하시기를",
          "original_form": "앞으로 행복한일들만 가득하시기를",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023002.1.27",
          "form": "그렇군요 ㅜㅜ",
          "original_form": "그렇군요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023002.1.28",
          "form": "ㅎㅎ 뭐가 가장 좋으세요?",
          "original_form": "ㅎㅎ 뭐가 가장 좋으세요?",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023002.1.29",
          "form": "결혼해서 ㅡ? 아내분이나 남편분이 잘해주신는군요?",
          "original_form": "결혼해서 ㅡ? 아내분이나 남편분이 잘해주신는군요?",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023002.1.30",
          "form": "네네 ㅎ 연애가 긴편이 아니어서 \r\n결혼하니 더 잘맞는걸 알게되었어요 ^^;",
          "original_form": "네네 ㅎ 연애가 긴편이 아니어서 \r\n결혼하니 더 잘맞는걸 알게되었어요 ^^;",
          "speaker_id": "1",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023002.1.31",
          "form": "대화가 잘 통해서 연애했고\r\n결혼하니 대화가 더 잘 통하더라고요",
          "original_form": "대화가 잘 통해서 연애했고\r\n결혼하니 대화가 더 잘 통하더라고요",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023002.1.32",
          "form": "그래서 잘했다싶었어요",
          "original_form": "그래서 잘했다싶었어요",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023002.1.33",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023002.1.34",
          "form": "아이는요?",
          "original_form": "아이는요?",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023002.1.35",
          "form": "대화가 잘통하면 좋죠ㅎ 전 대화가 안통해요...ㅠ 일방통행입니다",
          "original_form": "대화가 잘통하면 좋죠ㅎ 전 대화가 안통해요...ㅠ 일방통행입니다",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023002.1.36",
          "form": "아직 아이는 없어요",
          "original_form": "아직 아이는 없어요",
          "speaker_id": "1",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023002.1.37",
          "form": "많이 힘드셨겠어요 ㅜㅠ일방통행",
          "original_form": "많이 힘드셨겠어요 ㅜㅠ일방통행",
          "speaker_id": "1",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100023002.1.38",
          "form": "아이를 좀 늦게가지세요ㅎ",
          "original_form": "아이를 좀 늦게가지세요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100023002.1.39",
          "form": "저희 아니때문에도 많이 싸웠어요 ㅡㅎ",
          "original_form": "저희 아니때문에도 많이 싸웠어요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100023002.1.40",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023002.1.41",
          "form": "육아가 참 쉽지않죠? ㅜ",
          "original_form": "육아가 참 쉽지않죠? ㅜ",
          "speaker_id": "1",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023002.1.42",
          "form": "아무래도 그래요ㅠ",
          "original_form": "아무래도 그래요ㅠ",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023002.1.43",
          "form": "아이가 생기면 부부사이도 차츰차츰 멀어지는것같애요",
          "original_form": "아이가 생기면 부부사이도 차츰차츰 멀어지는것같애요",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023002.1.44",
          "form": "에고. 걱정은 드네요",
          "original_form": "에고. 걱정은 드네요",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023002.1.45",
          "form": "저희는 속도위반으로 결혼해서",
          "original_form": "저희는 속도위반으로 결혼해서",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023002.1.46",
          "form": "아이가 바로생기는바람에 신혼이라고없었어요",
          "original_form": "아이가 바로생기는바람에 신혼이라고없었어요",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023002.1.47",
          "form": "그래서 더더욱 싸우는 일도 잦았나봐요ㅎ",
          "original_form": "그래서 더더욱 싸우는 일도 잦았나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023002.1.48",
          "form": "아고 ㅜㅜ그런커플들이 많더라구요",
          "original_form": "아고 ㅜㅜ그런커플들이 많더라구요",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023002.1.49",
          "form": "고생많으십니다ㅜㅜ",
          "original_form": "고생많으십니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:28"
        }
      ]
