      "utterance": [
        {
          "id": "MDRW2100043577.1.1",
          "form": "name1님은 커피 자주 드세요?",
          "original_form": "&name1&님은 커피 자주 드세요?",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043577.1.2",
          "form": "출근해서 아메리카노 때려넣죠",
          "original_form": "출근해서 아메리카노 때려넣죠",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043577.1.3",
          "form": "하루두잔 필수!!",
          "original_form": "하루두잔 필수!!",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043577.1.4",
          "form": "ㅋㅋㅋㅋㅋ포션이죠",
          "original_form": "ㅋㅋㅋㅋㅋ포션이죠",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043577.1.5",
          "form": "전 라떼파입니다",
          "original_form": "전 라떼파입니다",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043577.1.6",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043577.1.7",
          "form": "저는 얼죽아에여",
          "original_form": "저는 얼죽아에여",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043577.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ존경스럽습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ존경스럽습니다",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043577.1.9",
          "form": "전 겨울에 얼죽아 절대 못해요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 겨울에 얼죽아 절대 못해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043577.1.10",
          "form": "ㅎㅎ아메리카노없음 일못해여 ㅎㅎㅎ",
          "original_form": "ㅎㅎ아메리카노없음 일못해여 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043577.1.11",
          "form": "겨울에 가끔 핫초코!!!!",
          "original_form": "겨울에 가끔 핫초코!!!!",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043577.1.12",
          "form": "전 찬거를 잘 못먹거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 찬거를 잘 못먹거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.13",
          "form": "저 식습관안좋아서 위내시경 하고 쫄앗는데",
          "original_form": "저 식습관안좋아서 위내시경 하고 쫄앗는데",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.14",
          "form": "아직 짱짱하데유",
          "original_form": "아직 짱짱하데유",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.15",
          "form": "주로 따땃한거 마시는 편이라 얼죽아이신분들을 보면 신기해요 ㅋㅋㅋㅋㅋ",
          "original_form": "주로 따땃한거 마시는 편이라 얼죽아이신분들을 보면 신기해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.16",
          "form": "이러다 나이더 먹고 훅갈듯...",
          "original_form": "이러다 나이더 먹고 훅갈듯...",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.17",
          "form": "오 다행이네요 아직 짱짱해서",
          "original_form": "오 다행이네요 아직 짱짱해서",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.18",
          "form": "그게 몸에 훨씬좋아요",
          "original_form": "그게 몸에 훨씬좋아요",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.19",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043577.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ얼죽아에 디저트도 좋아하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ얼죽아에 디저트도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043577.1.21",
          "form": "디저트는 딱히?",
          "original_form": "디저트는 딱히?",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043577.1.22",
          "form": "잇음 아무거나 먹어요 ㅎ",
          "original_form": "잇음 아무거나 먹어요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043577.1.23",
          "form": "전 디저트 엄청 좋아하거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 디저트 엄청 좋아하거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043577.1.24",
          "form": "저야말로 훅갈까봐 걱정해야 합니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저야말로 훅갈까봐 걱정해야 합니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043577.1.25",
          "form": "머조아하세요><",
          "original_form": "머조아하세요><",
