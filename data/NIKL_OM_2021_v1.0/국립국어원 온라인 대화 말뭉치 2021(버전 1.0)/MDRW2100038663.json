        },
        {
          "id": "MDRW2100038663.1.2",
          "form": "저는 주로 자요",
          "original_form": "저는 주로 자요",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100038663.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100038663.1.4",
          "form": "집가서 씻고 빨래하고 자요",
          "original_form": "집가서 씻고 빨래하고 자요",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038663.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038663.1.6",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100038663.1.7",
          "form": "빨래는 주로 며칠에 한번씩 하시나요?",
          "original_form": "빨래는 주로 며칠에 한번씩 하시나요?",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100038663.1.8",
          "form": "음 이틀에 한번정도? 하는것같아요",
          "original_form": "음 이틀에 한번정도? 하는것같아요",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038663.1.9",
          "form": "가족이 4명이라ㅎㅎ",
          "original_form": "가족이 4명이라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038663.1.10",
          "form": "비슷하네요\r\nname1님은 그럼 빨래담당이신가요?",
          "original_form": "비슷하네요\r\n&name1&님은 그럼 빨래담당이신가요?",
