      "utterance": [
        {
          "id": "MDRW2100007046.1.1",
          "form": "커뮤 하시는거 있으세요?",
          "original_form": "커뮤 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007046.1.2",
          "form": "저 더쿠해요",
          "original_form": "저 더쿠해요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007046.1.3",
          "form": "더쿠아시나요?",
          "original_form": "더쿠아시나요?",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007046.1.4",
          "form": "앗 저도 거기서 보고왔어요",
          "original_form": "앗 저도 거기서 보고왔어요",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007046.1.5",
          "form": "혹시 덕질하시나용?",
          "original_form": "혹시 덕질하시나용?",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007046.1.6",
          "form": "앗ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007046.1.7",
          "form": "지금은 휴덕중이에요",
          "original_form": "지금은 휴덕중이에요",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007046.1.8",
          "form": "덕질 진행중이세요?",
          "original_form": "덕질 진행중이세요?",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007046.1.9",
          "form": "저는 자주바껴서 간간이 심심할때만 여기저기 쑤셔보네요ㅎ",
          "original_form": "저는 자주바껴서 간간이 심심할때만 여기저기 쑤셔보네요ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007046.1.10",
          "form": "저도 주로가는 게시판 돌아가며 해요ㅋㅋ",
          "original_form": "저도 주로가는 게시판 돌아가며 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007046.1.11",
          "form": "요즘은 드라마 많이봐서 드영배카테많이가는편이에요",
          "original_form": "요즘은 드라마 많이봐서 드영배카테많이가는편이에요",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007046.1.12",
          "form": "약간 꿀팁이 많아섴ㅋ 드라마 요즘 뭐보세요?",
          "original_form": "약간 꿀팁이 많아섴ㅋ 드라마 요즘 뭐보세요?",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007046.1.13",
          "form": "넷플결제해서 이거저거 몰아봤어요. 킹덤도 보고 빈센조도 보고~",
          "original_form": "넷플결제해서 이거저거 몰아봤어요. 킹덤도 보고 빈센조도 보고~",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007046.1.14",
          "form": "아 킹덤 많이 보던데 재밌나요?",
          "original_form": "아 킹덤 많이 보던데 재밌나요?",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007046.1.15",
          "form": "쫄보라 무서워서 못보다가 봤는데 넘 재밌더라구요",
          "original_form": "쫄보라 무서워서 못보다가 봤는데 넘 재밌더라구요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007046.1.16",
          "form": "대신 환하게 하고봤어요ㅋㅋㅋ",
          "original_form": "대신 환하게 하고봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007046.1.17",
          "form": "ㅋㅋ아 무서운건가봐요 저 무서운거 좋아하는데!",
          "original_form": "ㅋㅋ아 무서운건가봐요 저 무서운거 좋아하는데!",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007046.1.18",
          "form": "그럼 꼭 보세요ㅋㅋㅋㅋㅋ",
          "original_form": "그럼 꼭 보세요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007046.1.19",
          "form": "일단 넷플을 결제해야겠네요ㅋㅋ더쿠는 어떻게 알게되셨나요?",
          "original_form": "일단 넷플을 결제해야겠네요ㅋㅋ더쿠는 어떻게 알게되셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007046.1.20",
          "form": "덕질하느라 알게됬어요",
          "original_form": "덕질하느라 알게됬어요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007046.1.21",
          "form": "원래 여초카페했었는데 올라오는 글들이 넘 마상이라...",
          "original_form": "원래 여초카페했었는데 올라오는 글들이 넘 마상이라...",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007046.1.22",
          "form": "덕질할려고 갈아탓죠ㅋㅋ",
          "original_form": "덕질할려고 갈아탓죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007046.1.23",
          "form": "더쿠하신지 오래되셨어요?",
          "original_form": "더쿠하신지 오래되셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007046.1.24",
          "form": "아하 전 작년 가입창 열렸을때 첨 가입했어요!",
          "original_form": "아하 전 작년 가입창 열렸을때 첨 가입했어요!",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007046.1.25",
          "form": "저는 더 그전에 가입했어요. 거의 7개월 기다렸나?",
          "original_form": "저는 더 그전에 가입했어요. 거의 7개월 기다렸나?",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007046.1.26",
          "form": "댓글못보는거 얼마나 답답하던지ㅠㅠ",
          "original_form": "댓글못보는거 얼마나 답답하던지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007046.1.27",
          "form": "아 그때 7개월 전에도 열렸었군요!저도 한두달쯤 그걸 겪었죠!ㅋㅋ",
          "original_form": "아 그때 7개월 전에도 열렸었군요!저도 한두달쯤 그걸 겪었죠!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007046.1.28",
          "form": "아 기다리신게 7개월ㅠㅠ",
          "original_form": "아 기다리신게 7개월ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007046.1.29",
          "form": "네 오랜기다림끝에ㅋㅋㅋㅋㅋ",
          "original_form": "네 오랜기다림끝에ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007046.1.30",
          "form": "요즘은 더쿠말고는 다른 커뮤는 안하게 되더라구요",
          "original_form": "요즘은 더쿠말고는 다른 커뮤는 안하게 되더라구요",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007046.1.31",
          "form": "저는 직장커뮤말곤 커뮤가 첨이라ㅋㅋ 익명이라 재밌더라구요",
          "original_form": "저는 직장커뮤말곤 커뮤가 첨이라ㅋㅋ 익명이라 재밌더라구요",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007046.1.32",
          "form": "맞아요 익명이라 더 재밌는것도 있고 좀 화르륵 불타오르는 것도 있는거같아요",
          "original_form": "맞아요 익명이라 더 재밌는것도 있고 좀 화르륵 불타오르는 것도 있는거같아요",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007046.1.33",
          "form": "그리고 연령대가 저랑 비슷한거 같아서 좀 생각도 비슷한게 많은거같아요",
          "original_form": "그리고 연령대가 저랑 비슷한거 같아서 좀 생각도 비슷한게 많은거같아요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007046.1.34",
          "form": "다양한 커뮤 하는것도 좋은거같아요 한가지 커뮤만 하면 그쪽 성향에 치우치더라구요",
          "original_form": "다양한 커뮤 하는것도 좋은거같아요 한가지 커뮤만 하면 그쪽 성향에 치우치더라구요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007046.1.35",
          "form": "첨에 여초하다 옮겨왔을때 양쪽 말이 너무 달라서 놀랬어요",
          "original_form": "첨에 여초하다 옮겨왔을때 양쪽 말이 너무 달라서 놀랬어요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        }
      ]
    }
  ]
