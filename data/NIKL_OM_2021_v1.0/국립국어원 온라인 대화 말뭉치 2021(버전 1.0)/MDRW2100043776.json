      "utterance": [
        {
          "id": "MDRW2100043776.1.1",
          "form": "거기도 비 많이 오나요?",
          "original_form": "거기도 비 많이 오나요?",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043776.1.2",
          "form": "아니요 많이는 오고잇지않아요",
          "original_form": "아니요 많이는 오고잇지않아요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043776.1.3",
          "form": "비가많이오고잇나요?",
          "original_form": "비가많이오고잇나요?",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043776.1.4",
          "form": "여기는 밤새 엄청 오다가 지금은 분무기처럼 오고있어요 ㅋㅋ",
          "original_form": "여기는 밤새 엄청 오다가 지금은 분무기처럼 오고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043776.1.5",
          "form": "분무기 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "분무기 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043776.1.6",
          "form": "빵터졋네요",
          "original_form": "빵터졋네요",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043776.1.7",
          "form": "ㅋㅋㅋㅋㅋ 진짜 하늘 보는데 분무기 뿌리는 것처럼 약오르네요",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜 하늘 보는데 분무기 뿌리는 것처럼 약오르네요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043776.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우산을 쓰란건가 말란건가 싶은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우산을 쓰란건가 말란건가 싶은",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043776.1.9",
          "form": "진짜요 ㅋㅋㅋ 어느 계절 젤 좋아하세요?",
          "original_form": "진짜요 ㅋㅋㅋ 어느 계절 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043776.1.10",
          "form": "저는봄을좋아합니다",
          "original_form": "저는봄을좋아합니다",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043776.1.11",
          "form": "황사빼구요 ㅋㅋㅋㅋ",
          "original_form": "황사빼구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043776.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043776.1.13",
          "form": "ㅋㅋㅋㅋ 전 여름 젤 좋아해요",
          "original_form": "ㅋㅋㅋㅋ 전 여름 젤 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043776.1.14",
          "form": "근데 올여름은 좀 심했어요.. 넘 더워서",
          "original_form": "근데 올여름은 좀 심했어요.. 넘 더워서",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043776.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ올여름 그래도 생각보다 버틸만하지않앗나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ올여름 그래도 생각보다 버틸만하지않앗나요?",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043776.1.16",
          "form": "아 근데 최근엔 엄청 살만한데 그 7월말부터 8월초가 진짜.. 너무 덥더라고요",
          "original_form": "아 근데 최근엔 엄청 살만한데 그 7월말부터 8월초가 진짜.. 너무 덥더라고요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043776.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 그땐 진짜 덥긴 햇어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 그땐 진짜 덥긴 햇어요",
