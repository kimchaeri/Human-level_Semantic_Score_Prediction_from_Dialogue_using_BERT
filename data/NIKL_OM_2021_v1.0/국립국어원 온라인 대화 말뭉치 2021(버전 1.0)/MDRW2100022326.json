      "utterance": [
        {
          "id": "MDRW2100022326.1.1",
          "form": "홈트 쉽지않아요..",
          "original_form": "홈트 쉽지않아요..",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022326.1.2",
          "form": "집에 전신 거울 있어요?",
          "original_form": "집에 전신 거울 있어요?",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022326.1.3",
          "form": "없어요ㅜㅠ",
          "original_form": "없어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022326.1.4",
          "form": "아.. 거울이 없으면 힘들겠네여..",
          "original_form": "아.. 거울이 없으면 힘들겠네여..",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022326.1.5",
          "form": "거울이 있어야.. 자세도 보고 눈바디?라는 보람도 있는뎅..",
          "original_form": "거울이 있어야.. 자세도 보고 눈바디?라는 보람도 있는뎅..",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022326.1.6",
          "form": "아앗..ㅜㅠ 왠지..",
          "original_form": "아앗..ㅜㅠ 왠지..",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022326.1.7",
          "form": "신발장에 달린거밖에 없어요ㅜㅠ",
          "original_form": "신발장에 달린거밖에 없어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022326.1.8",
          "form": "아.. 거기 좀 좁죠? ㅋㅋㅋ",
          "original_form": "아.. 거기 좀 좁죠? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022326.1.9",
          "form": "거울 1만원정도면 사지않나여?",
          "original_form": "거울 1만원정도면 사지않나여?",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022326.1.10",
          "form": "운동할려면 진짜 필수에요..",
          "original_form": "운동할려면 진짜 필수에요..",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022326.1.11",
          "form": "네ㅋㅋ 제 몸이 좁아보이는것만 좋은..ㅋㅋ",
          "original_form": "네ㅋㅋ 제 몸이 좁아보이는것만 좋은..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022326.1.12",
          "form": "제가 고양이용품을 막 사드렸더니 공간이..",
          "original_form": "제가 고양이용품을 막 사드렸더니 공간이..",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022326.1.13",
          "form": "아.. 공간이 안되면",
          "original_form": "아.. 공간이 안되면",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022326.1.14",
          "form": "밖에 런닝 같은건 안하시나요?",
          "original_form": "밖에 런닝 같은건 안하시나요?",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022326.1.15",
          "form": "아침에 걷기하고 있어요!",
          "original_form": "아침에 걷기하고 있어요!",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022326.1.16",
          "form": "한시간정도..",
          "original_form": "한시간정도..",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022326.1.17",
          "form": "걷기가 최고인듯..",
          "original_form": "걷기가 최고인듯..",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022326.1.18",
          "form": "한시간이면 5키로 정도 되죠?",
          "original_form": "한시간이면 5키로 정도 되죠?",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022326.1.19",
          "form": "그정도 되는거 같아요!",
          "original_form": "그정도 되는거 같아요!",
          "speaker_id": "1",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022326.1.20",
          "form": "어플이 잘 못재서 늘 다르더라구요..",
          "original_form": "어플이 잘 못재서 늘 다르더라구요..",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022326.1.21",
          "form": "만보기 어플 같은거로 재시는건가요?",
          "original_form": "만보기 어플 같은거로 재시는건가요?",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022326.1.22",
          "form": "삼성 헬스요!",
          "original_form": "삼성 헬스요!",
          "speaker_id": "1",
          "time": "20210829 12:54"
        }
