      "utterance": [
        {
          "id": "MDRW2100017062.1.1",
          "form": "최근에 장난감을 샀어요",
          "original_form": "최근에 장난감을 샀어요",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017062.1.2",
          "form": "짱 귀여워요",
          "original_form": "짱 귀여워요",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017062.1.3",
          "form": "어떤 장난감인가요",
          "original_form": "어떤 장난감인가요",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017062.1.4",
          "form": "인형인데 손바닥정도 크기로 작아요",
          "original_form": "인형인데 손바닥정도 크기로 작아요",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017062.1.5",
          "form": "배송중이라 내일 도착예정이래요",
          "original_form": "배송중이라 내일 도착예정이래요",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017062.1.6",
          "form": "강아지 인형인가요",
          "original_form": "강아지 인형인가요",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017062.1.7",
          "form": "아니요 새인형이요",
          "original_form": "아니요 새인형이요",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017062.1.8",
          "form": "새도 인형을 줘요?",
          "original_form": "새도 인형을 줘요?",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017062.1.9",
          "form": "새랑 크기가 비슷할 것 같은데...",
          "original_form": "새랑 크기가 비슷할 것 같은데...",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017062.1.10",
          "form": "새모양 인형이예요",
          "original_form": "새모양 인형이예요",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017062.1.11",
          "form": "작고 귀여워욤",
          "original_form": "작고 귀여워욤",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017062.1.12",
          "form": "뭐라고 할까",
          "original_form": "뭐라고 할까",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017062.1.13",
          "form": "새랑 새 인형이랑 있으면",
          "original_form": "새랑 새 인형이랑 있으면",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017062.1.14",
          "form": "새는 마네킹 보는 기분일지도",
          "original_form": "새는 마네킹 보는 기분일지도",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017062.1.15",
          "form": "별관심없지않을까",
          "original_form": "별관심없지않을까",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017062.1.16",
          "form": "귀엽네...",
          "original_form": "귀엽네...",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017062.1.17",
          "form": "그냥 또 이상한게 생겼네...하는??",
          "original_form": "그냥 또 이상한게 생겼네...하는??",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017062.1.18",
          "form": "장난감 사줘도 의미 없잖아 ㅋㅋㅋㅋㅋ",
          "original_form": "장난감 사줘도 의미 없잖아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017062.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ내 키분이  좋다구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ내 키분이  좋다구요",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017062.1.20",
          "form": "쇼핑의 의의는 다했군여",
          "original_form": "쇼핑의 의의는 다했군여",
          "speaker_id": "2",
