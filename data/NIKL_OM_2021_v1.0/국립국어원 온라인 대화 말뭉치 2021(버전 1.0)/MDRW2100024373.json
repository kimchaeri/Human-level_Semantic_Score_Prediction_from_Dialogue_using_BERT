        },
        {
          "id": "MDRW2100024373.1.2",
          "form": "유투브에서",
          "original_form": "유투브에서",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.3",
          "form": "11마리인가 렉돌키우는집잇는데",
          "original_form": "11마리인가 렉돌키우는집잇는데",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.4",
          "form": "진짜 존경스럽더라고요",
          "original_form": "진짜 존경스럽더라고요",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.6",
          "form": "렉돌은 뭐예요?",
          "original_form": "렉돌은 뭐예요?",
          "speaker_id": "1",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.7",
          "form": "ㅋㅋㅋㅋㅋ 부내.좔좔",
          "original_form": "ㅋㅋㅋㅋㅋ 부내.좔좔",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.8",
          "form": "그 귀족냥이래여",
          "original_form": "그 귀족냥이래여",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.9",
          "form": "품종묘인가여?",
          "original_form": "품종묘인가여?",
          "speaker_id": "1",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.10",
          "form": "몸값비싼ㅋㅋ 근데 대형묘(?)라",
          "original_form": "몸값비싼ㅋㅋ 근데 대형묘(?)라",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100024373.1.12",
          "form": "ㅠㅠ 그분은 돈이많아서..",
          "original_form": "ㅠㅠ 그분은 돈이많아서..",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.13",
          "form": "어이쿠 그럼",
          "original_form": "어이쿠 그럼",
          "speaker_id": "1",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.14",
          "form": "유전병이 많을 텐데..ㅠㅠ",
          "original_form": "유전병이 많을 텐데..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.15",
          "form": "네내",
          "original_form": "네내",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.16",
          "form": "고양이는",
          "original_form": "고양이는",
          "speaker_id": "1",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.17",
          "form": "그렇죠.. ㅜ",
          "original_form": "그렇죠.. ㅜ",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.18",
          "form": "뭔가잇더라고요",
          "original_form": "뭔가잇더라고요",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.19",
          "form": "품종있으면 진짜 유전병이 심하더라고요ㅠㅠ",
          "original_form": "품종있으면 진짜 유전병이 심하더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100024373.1.20",
          "form": "그쵸.. 저는 폴드 보면 마음이아프더라고요",
