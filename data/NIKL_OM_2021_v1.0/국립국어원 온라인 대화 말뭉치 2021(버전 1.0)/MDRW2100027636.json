      "utterance": [
        {
          "id": "MDRW2100027636.1.1",
          "form": "지역에 큰 백화점 들어서서 구경갈까해요",
          "original_form": "지역에 큰 백화점 들어서서 구경갈까해요",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027636.1.2",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027636.1.3",
          "form": "근데 명품천지라고...",
          "original_form": "근데 명품천지라고...",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027636.1.4",
          "form": "앗ㅜㅜ",
          "original_form": "앗ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027636.1.5",
          "form": "그럴돈도없고 그런옷도 필오없구",
          "original_form": "그럴돈도없고 그런옷도 필오없구",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027636.1.6",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027636.1.7",
          "form": "명품지갑은 좀갖고싶네요",
          "original_form": "명품지갑은 좀갖고싶네요",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027636.1.8",
          "form": "하나쯤 있으면 좋죠",
          "original_form": "하나쯤 있으면 좋죠",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027636.1.9",
          "form": "갖고있어요?",
          "original_form": "갖고있어요?",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027636.1.10",
          "form": "메트로시티?",
          "original_form": "메트로시티?",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027636.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027636.1.12",
          "form": "근데 샤넬 갖고싶어요",
          "original_form": "근데 샤넬 갖고싶어요",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027636.1.13",
          "form": "ㅇ느",
          "original_form": "ㅇ느",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027636.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027636.1.15",
          "form": "불룩불룩한게 예뻐보여요",
          "original_form": "불룩불룩한게 예뻐보여요",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027636.1.16",
          "form": "사면 오년은 거뜬하지않나요",
          "original_form": "사면 오년은 거뜬하지않나요",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027636.1.17",
          "form": "그쳐?",
          "original_form": "그쳐?",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027636.1.18",
          "form": "삽시다",
          "original_form": "삽시다",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027636.1.19",
          "form": "근데 또 쇼핑은 귀찮아요",
          "original_form": "근데 또 쇼핑은 귀찮아요",
