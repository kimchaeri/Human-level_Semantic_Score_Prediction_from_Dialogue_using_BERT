      "utterance": [
        {
          "id": "MDRW2100039161.1.1",
          "form": "좋아하는 가수있으세요?",
          "original_form": "좋아하는 가수있으세요?",
          "speaker_id": "2",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100039161.1.2",
          "form": "엠씨더맥스 노래 좋아해요",
          "original_form": "엠씨더맥스 노래 좋아해요",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100039161.1.3",
          "form": "어떤가수 좋아하세요??",
          "original_form": "어떤가수 좋아하세요??",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100039161.1.4",
          "form": "전 포레스텔라요ㅎㅎㅎ",
          "original_form": "전 포레스텔라요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039161.1.5",
          "form": "엠씨더맥스노래참 좋죠",
          "original_form": "엠씨더맥스노래참 좋죠",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100039161.1.6",
          "form": "아 노래 좋은가요?? 처음들어보는 이름이예요^^",
          "original_form": "아 노래 좋은가요?? 처음들어보는 이름이예요^^",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039161.1.7",
          "form": "제가 요즘 가수는 잘 몰라가지구",
          "original_form": "제가 요즘 가수는 잘 몰라가지구",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039161.1.8",
          "form": "그 팬텀싱어라는 프로그램에 나온 크로스오버그룹이에요ㅎㅎ",
          "original_form": "그 팬텀싱어라는 프로그램에 나온 크로스오버그룹이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039161.1.9",
          "form": "티비에 잘안나와서 슬퍼요ㅜㅜ",
          "original_form": "티비에 잘안나와서 슬퍼요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039161.1.10",
          "form": "아 그렇군요 ㅎㅎ",
          "original_form": "아 그렇군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039161.1.11",
          "form": "제가 티비를 잘 안봐서 몰라요 ㅎㅎ",
          "original_form": "제가 티비를 잘 안봐서 몰라요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039161.1.12",
          "form": "좋아하는 배우는 있으세요??",
          "original_form": "좋아하는 배우는 있으세요??",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039161.1.13",
          "form": "저도 티비를 잘안보는지라ㅋㅋㅋㅋ",
          "original_form": "저도 티비를 잘안보는지라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100039161.1.14",
          "form": "아하 ㅎㅎ",
          "original_form": "아하 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100039161.1.15",
          "form": "전 김수현 류준열 좋아해요 ㅎㅎ",
          "original_form": "전 김수현 류준열 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100039161.1.16",
          "form": "둘다 매력적이죠ㅎㅎ",
          "original_form": "둘다 매력적이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100039161.1.17",
          "form": "네 너무 좋습니다 ㅎㅎ",
          "original_form": "네 너무 좋습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100039161.1.18",
          "form": "조만간 류준열 나오는 드라마 해서 기다리고 있어요",
          "original_form": "조만간 류준열 나오는 드라마 해서 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210903 13:24"
        },
        {
          "id": "MDRW2100039161.1.19",
          "form": "드라마는 다본것 같네요",
          "original_form": "드라마는 다본것 같네요",
          "speaker_id": "2",
          "time": "20210903 13:24"
        },
        {
          "id": "MDRW2100039161.1.20",
          "form": "뭐나오나요?",
          "original_form": "뭐나오나요?",
          "speaker_id": "2",
          "time": "20210903 13:24"
        },
        {
          "id": "MDRW2100039161.1.21",
