      "utterance": [
        {
          "id": "MDRW2100020244.1.1",
          "form": "아멘...ㅋㅋ 저는천주교에요",
          "original_form": "아멘...ㅋㅋ 저는천주교에요",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020244.1.2",
          "form": "아 저는 무교에요!",
          "original_form": "아 저는 무교에요!",
          "speaker_id": "1",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020244.1.3",
          "form": "천주교에 관심은 있어요!",
          "original_form": "천주교에 관심은 있어요!",
          "speaker_id": "1",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020244.1.4",
          "form": "ㅋㅋㅋ그렇구나 저는 엄마때문에",
          "original_form": "ㅋㅋㅋ그렇구나 저는 엄마때문에",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020244.1.5",
          "form": "천주교지만 사실은 냉담자에요",
          "original_form": "천주교지만 사실은 냉담자에요",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020244.1.6",
          "form": "누가 종교 물어보면 천주교라고는하는데...",
          "original_form": "누가 종교 물어보면 천주교라고는하는데...",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020244.1.7",
          "form": "성당은안다니는 ㅠㅠㅋ",
          "original_form": "성당은안다니는 ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020244.1.8",
          "form": "저도 어릴때는 독실한 기독교인이었어요 ㅋㅋㅋ",
          "original_form": "저도 어릴때는 독실한 기독교인이었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020244.1.9",
          "form": "진짜요?ㅋㅋ 저도 독실해지고싶은데",
          "original_form": "진짜요?ㅋㅋ 저도 독실해지고싶은데",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020244.1.10",
          "form": "어떻게 해야할지 모르겠더라구요",
          "original_form": "어떻게 해야할지 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020244.1.11",
          "form": "기도의힘은 믿지만...\n필요할때만 하는것같아서",
          "original_form": "기도의힘은 믿지만...\n필요할때만 하는것같아서",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020244.1.12",
          "form": "전그냥 모태신앙이라 따라가다보니 ㅋㅋㅋㅋ",
          "original_form": "전그냥 모태신앙이라 따라가다보니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020244.1.13",
          "form": "집앞에 성당이 있는데 안에 한번도 못들어가봤어요 ㅋㅋ",
          "original_form": "집앞에 성당이 있는데 안에 한번도 못들어가봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020244.1.14",
          "form": "ㅋㅋ 저도 모태신앙...앜ㅋㅋ",
          "original_form": "ㅋㅋ 저도 모태신앙...앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020244.1.15",
          "form": "저는 동네성당안다니늗데",
          "original_form": "저는 동네성당안다니늗데",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020244.1.16",
          "form": "유럽성당은 그렇게 많이 돌아다닌거같아요",
          "original_form": "유럽성당은 그렇게 많이 돌아다닌거같아요",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020244.1.17",
          "form": "스테인드글라스가 항상 예쁘다고 생각하며 지나갑니다 ㅋㅋㅋ",
          "original_form": "스테인드글라스가 항상 예쁘다고 생각하며 지나갑니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020244.1.18",
          "form": "우리동네도 그렇게 멋있게 지었으면 맨날 갈을텐데~~ 이러면서 ㅋㅋ",
          "original_form": "우리동네도 그렇게 멋있게 지었으면 맨날 갈을텐데~~ 이러면서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020244.1.19",
          "form": "그래도 성당은 다 그 특유의 느낌이 멋진거같아요",
          "original_form": "그래도 성당은 다 그 특유의 느낌이 멋진거같아요",
          "speaker_id": "1",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020244.1.20",
          "form": "뭔가 마음이 편해지긴해요",
          "original_form": "뭔가 마음이 편해지긴해요",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020244.1.21",
          "form": "성당가면",
          "original_form": "성당가면",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020244.1.22",
          "form": "성당가면 뭐 쓰고있잖아요 그거 뭐에요?",
          "original_form": "성당가면 뭐 쓰고있잖아요 그거 뭐에요?",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020244.1.23",
          "form": "아 미사포 에요 ㅎㅎ",
          "original_form": "아 미사포 에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020244.1.24",
          "form": "첫영성체 받은 사람들이 할 수 있는거라고해서",
          "original_form": "첫영성체 받은 사람들이 할 수 있는거라고해서",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020244.1.25",
          "form": "아 아무나 하는게 아니군요!",
          "original_form": "아 아무나 하는게 아니군요!",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020244.1.26",
          "form": "네네 ㅋㅋㅋ 열심히 기도문외워서",
          "original_form": "네네 ㅋㅋㅋ 열심히 기도문외워서",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020244.1.27",
          "form": "첫영성체 받고 미사포쓰고 미사드렸었어요",
          "original_form": "첫영성체 받고 미사포쓰고 미사드렸었어요",
          "speaker_id": "2",
