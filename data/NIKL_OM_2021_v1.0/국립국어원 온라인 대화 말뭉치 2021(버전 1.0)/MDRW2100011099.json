      "utterance": [
        {
          "id": "MDRW2100011099.1.1",
          "form": "요즘은 인턴이 금턴이라는데",
          "original_form": "요즘은 인턴이 금턴이라는데",
          "speaker_id": "1",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011099.1.2",
          "form": "아니그럼 대체 어떻게 취직하란거지",
          "original_form": "아니그럼 대체 어떻게 취직하란거지",
          "speaker_id": "1",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011099.1.3",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210824 00:56"
        },
        {
          "id": "MDRW2100011099.1.4",
          "form": "요즘취직하기 너무힘들어",
          "original_form": "요즘취직하기 너무힘들어",
          "speaker_id": "2",
          "time": "20210824 00:56"
        },
        {
          "id": "MDRW2100011099.1.5",
          "form": "심지어 인턴도 스펙이 빵빵해야",
          "original_form": "심지어 인턴도 스펙이 빵빵해야",
          "speaker_id": "1",
          "time": "20210824 00:56"
        },
        {
          "id": "MDRW2100011099.1.6",
          "form": "붙는다는데ㅜㅜ",
          "original_form": "붙는다는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 00:56"
        },
        {
          "id": "MDRW2100011099.1.7",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011099.1.8",
          "form": "토익에 자격증에",
          "original_form": "토익에 자격증에",
          "speaker_id": "2",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011099.1.9",
          "form": "컴활에 대외활동까지",
          "original_form": "컴활에 대외활동까지",
          "speaker_id": "2",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011099.1.10",
          "form": "토익도 800은 돼야 한다는데",
          "original_form": "토익도 800은 돼야 한다는데",
          "speaker_id": "1",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011099.1.11",
          "form": "진짜큰일...",
          "original_form": "진짜큰일...",
          "speaker_id": "1",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011099.1.12",
          "form": "알바도스펙이라며 요즘은...",
          "original_form": "알바도스펙이라며 요즘은...",
          "speaker_id": "2",
          "time": "20210824 01:00"
        },
        {
          "id": "MDRW2100011099.1.13",
          "form": "알바도 자소서에쓰기 딱좋은 경험...",
          "original_form": "알바도 자소서에쓰기 딱좋은 경험...",
          "speaker_id": "1",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100011099.1.14",
          "form": "아니 심지어ㅋㅋ",
          "original_form": "아니 심지어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100011099.1.15",
          "form": "알바도 요즘은 코로나때메 붙기힘듦",
          "original_form": "알바도 요즘은 코로나때메 붙기힘듦",
          "speaker_id": "1",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100011099.1.16",
          "form": "ㅋㅋ큐ㅜㅜㅜ",
          "original_form": "ㅋㅋ큐ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100011099.1.17",
          "form": "너무치열....",
          "original_form": "너무치열....",
          "speaker_id": "2",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100011099.1.18",
          "form": "이래서 공무원열풍인가",
          "original_form": "이래서 공무원열풍인가",
          "speaker_id": "1",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100011099.1.19",
          "form": "맞아 왜 그런지알겠어",
          "original_form": "맞아 왜 그런지알겠어",
          "speaker_id": "2",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100011099.1.20",
          "form": "공뭔은 인턴없어도 되고",
          "original_form": "공뭔은 인턴없어도 되고",
          "speaker_id": "1",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100011099.1.21",
          "form": "공정한 느낌",
          "original_form": "공정한 느낌",
          "speaker_id": "1",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100011099.1.22",
          "form": "그래 시험성적으로 뽑으니까",
          "original_form": "그래 시험성적으로 뽑으니까",
          "speaker_id": "2",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100011099.1.23",
          "form": "공정 100프로",
          "original_form": "공정 100프로",
          "speaker_id": "2",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100011099.1.24",
          "form": "아니 게다가 취업비리?",
          "original_form": "아니 게다가 취업비리?",
          "speaker_id": "1",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100011099.1.25",
          "form": "이런것도 있는데ㅜㅜ",
          "original_form": "이런것도 있는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100011099.1.26",
          "form": "어케 진짜 의욕상실이야",
          "original_form": "어케 진짜 의욕상실이야",
          "speaker_id": "1",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100011099.1.27",
          "form": "공감....",
          "original_form": "공감....",
