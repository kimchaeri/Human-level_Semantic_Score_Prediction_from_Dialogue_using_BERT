        },
        {
          "id": "MDRW2100034205.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034205.1.3",
          "form": "최근에 쇼핑하셨나요??",
          "original_form": "최근에 쇼핑하셨나요??",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034205.1.4",
          "form": "네 저 최근에 브리타정수기랑 섬유향수 샀어요!",
          "original_form": "네 저 최근에 브리타정수기랑 섬유향수 샀어요!",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034205.1.5",
          "form": "오 정수기 저도 알아볼까하는데 좋나요?",
          "original_form": "오 정수기 저도 알아볼까하는데 좋나요?",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034205.1.6",
          "form": "요새 생수 난리더라구요 ㅠㅠ",
          "original_form": "요새 생수 난리더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034205.1.7",
          "form": "네 저는 좋더라구요",
          "original_form": "네 저는 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034205.1.8",
          "form": "저도 고려해봐야겠어요!",
          "original_form": "저도 고려해봐야겠어요!",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100034205.1.9",
          "form": "매번 생수 사먹으면 쓰레기도 많이 생기고해서",
          "original_form": "매번 생수 사먹으면 쓰레기도 많이 생기고해서",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100034205.1.10",
          "form": "맞아요 분리수거가...",
          "original_form": "맞아요 분리수거가...",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100034205.1.11",
          "form": "최근에 사신거있으세요?",
          "original_form": "최근에 사신거있으세요?",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100034205.1.12",
          "form": "저는 헤어오일 샀어요",
          "original_form": "저는 헤어오일 샀어요",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100034205.1.13",
          "form": "머릿결이 안좋아서요 ㅜ",
          "original_form": "머릿결이 안좋아서요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100034205.1.14",
          "form": "오일 쓰면 좀 좋아지나요?",
          "original_form": "오일 쓰면 좀 좋아지나요?",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100034205.1.15",
          "form": "저도 머릿결이 안좋아서",
          "original_form": "저도 머릿결이 안좋아서",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034205.1.16",
          "form": "예전에 쓰다가 오랜만에 샀는데",
          "original_form": "예전에 쓰다가 오랜만에 샀는데",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034205.1.17",
          "form": "음 머릿결이 근본적으로 좋아지는건 모르겠어요..ㅋㅋㅋㅋ",
          "original_form": "음 머릿결이 근본적으로 좋아지는건 모르겠어요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034205.1.18",
          "form": "친구는 더 잘빗긴다하더라구요 전 그냥 안쓰는거보단 낫겠지 싶어요 ㅋㅋㅋ",
          "original_form": "친구는 더 잘빗긴다하더라구요 전 그냥 안쓰는거보단 낫겠지 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034205.1.19",
          "form": "마자요 관리하는게  그래도 좀 더 낫겠죠ㅠ",
          "original_form": "마자요 관리하는게  그래도 좀 더 낫겠죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:13"
        }
