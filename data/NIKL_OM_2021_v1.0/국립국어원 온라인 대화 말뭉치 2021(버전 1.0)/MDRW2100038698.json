      "utterance": [
        {
          "id": "MDRW2100038698.1.1",
          "form": "맛있어요 !!! 추천드립니당 ㅎㅎ",
          "original_form": "맛있어요 !!! 추천드립니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038698.1.2",
          "form": "런치샌드위치 맛있어요 ㅎㅎ",
          "original_form": "런치샌드위치 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038698.1.3",
          "form": "나중에 먹어봐야겠네요!!",
          "original_form": "나중에 먹어봐야겠네요!!",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038698.1.4",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038698.1.5",
          "form": "네 ! 간단하게 샐러드 먹었습니당 ㅎㅎ",
          "original_form": "네 ! 간단하게 샐러드 먹었습니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038698.1.6",
          "form": "뭐드셨어요!",
          "original_form": "뭐드셨어요!",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038698.1.7",
          "form": "저는 생선구이 먹었습니다!",
          "original_form": "저는 생선구이 먹었습니다!",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038698.1.8",
          "form": "생선구이 좋아하세요?",
          "original_form": "생선구이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038698.1.9",
          "form": "헐 네 ! 넘 좋아해요",
          "original_form": "헐 네 ! 넘 좋아해요",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038698.1.10",
          "form": "최애 음식 중 하나가 갈치랍니다 ㅎㅎ",
          "original_form": "최애 음식 중 하나가 갈치랍니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038698.1.11",
          "form": "오 저는 고등어구이 좋아하는데",
          "original_form": "오 저는 고등어구이 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100038698.1.12",
          "form": "오늘은 박대라는 생선구이 먹었어요ㅎㅎ",
          "original_form": "오늘은 박대라는 생선구이 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100038698.1.13",
          "form": "오 ! 맛있나요???!",
          "original_form": "오 ! 맛있나요???!",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100038698.1.14",
          "form": "나쁘진않은데 고등어구이가 더 맛있어요ㅎㅎ",
          "original_form": "나쁘진않은데 고등어구이가 더 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100038698.1.15",
          "form": "생선구이 발라먹는거 너무 귀찮지 않나요...ㅎㅎ",
          "original_form": "생선구이 발라먹는거 너무 귀찮지 않나요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100038698.1.16",
          "form": "앗 마자여..",
          "original_form": "앗 마자여..",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038698.1.17",
          "form": "그나마 고등어는 뼈가 크니까 잘발라지지",
          "original_form": "그나마 고등어는 뼈가 크니까 잘발라지지",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038698.1.18",
          "form": "갈치는 어휴... 속터집니다 !",
          "original_form": "갈치는 어휴... 속터집니다 !",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038698.1.19",
          "form": "참 맛있는데...",
          "original_form": "참 맛있는데...",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038698.1.20",
          "form": "먹기 귀찮아서 갈치는 꺼리게되네요...",
          "original_form": "먹기 귀찮아서 갈치는 꺼리게되네요...",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038698.1.21",
          "form": "아이스크림 좋아하세요??!",
          "original_form": "아이스크림 좋아하세요??!",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038698.1.22",
          "form": "네 좋아해요 !!",
          "original_form": "네 좋아해요 !!",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038698.1.23",
          "form": "배라 자주 드시나요??",
          "original_form": "배라 자주 드시나요??",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038698.1.24",
