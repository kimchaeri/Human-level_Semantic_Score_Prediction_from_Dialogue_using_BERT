      "utterance": [
        {
          "id": "MDRW2100016931.1.1",
          "form": "곰표 밀맥주 존맛",
          "original_form": "곰표 밀맥주 존맛",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016931.1.2",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016931.1.3",
          "form": "맛있었어",
          "original_form": "맛있었어",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016931.1.4",
          "form": "원래 맥주랑 다른 맛인데! 맛있음",
          "original_form": "원래 맥주랑 다른 맛인데! 맛있음",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016931.1.5",
          "form": "곰표 팝콘?",
          "original_form": "곰표 팝콘?",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016931.1.6",
          "form": "그것도 맛있어",
          "original_form": "그것도 맛있어",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.7",
          "form": "그건 뭐야",
          "original_form": "그건 뭐야",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.9",
          "form": "팝콘도 있는데",
          "original_form": "팝콘도 있는데",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.10",
          "form": "존맛임",
          "original_form": "존맛임",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.11",
          "form": "양도 많아서",
          "original_form": "양도 많아서",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.12",
          "form": "친구들이랑 나눠먹음",
          "original_form": "친구들이랑 나눠먹음",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.13",
          "form": "학교에서",
          "original_form": "학교에서",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.14",
          "form": "와그작 와그작",
          "original_form": "와그작 와그작",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.15",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.16",
          "form": "그것도 편의점에서 팔아?",
          "original_form": "그것도 편의점에서 팔아?",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.17",
          "form": "근데 일반 팝콘맛이랑 비슷해",
          "original_form": "근데 일반 팝콘맛이랑 비슷해",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.19",
          "form": "아마도?",
          "original_form": "아마도?",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.20",
          "form": "너는 어디서 샀는데",
          "original_form": "너는 어디서 샀는데",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.21",
          "form": "CU에 팔껄??",
          "original_form": "CU에 팔껄??",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016931.1.22",
          "form": "나 선배가 사온거 먹은거지",
          "original_form": "나 선배가 사온거 먹은거지",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016931.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016931.1.24",
          "form": "선배가 학생회 활동 한다고 같이 먹자고 사오셨는데",
          "original_form": "선배가 학생회 활동 한다고 같이 먹자고 사오셨는데",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016931.1.25",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016931.1.26",
          "form": "남아서",
          "original_form": "남아서",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016931.1.27",
          "form": "들고 가라고 하길래...",
          "original_form": "들고 가라고 하길래...",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016931.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016931.1.29",
          "form": "네...네... 하고 챙김",
          "original_form": "네...네... 하고 챙김",
