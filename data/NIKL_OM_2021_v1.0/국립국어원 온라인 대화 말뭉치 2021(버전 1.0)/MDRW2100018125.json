        },
        {
          "id": "MDRW2100018125.1.3",
          "form": "종교네요 앗ㅋㅋ",
          "original_form": "종교네요 앗ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100018125.1.4",
          "form": "무교신가여 ㅋㅋ",
          "original_form": "무교신가여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100018125.1.5",
          "form": "저희집은 교회다녀요",
          "original_form": "저희집은 교회다녀요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.6",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.7",
          "form": "전 무교에요  ㅎㅎ",
          "original_form": "전 무교에요  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.8",
          "form": "name2 그럼 모태신앙?",
          "original_form": "&name2& 그럼 모태신앙?",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.9",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.10",
          "form": "근데 요즘 좀",
          "original_form": "근데 요즘 좀",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.11",
          "form": "코로나이후로",
          "original_form": "코로나이후로",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.12",
          "form": "안가고있어요",
          "original_form": "안가고있어요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.13",
          "form": "그쳐 코로나라ㅜㅜ",
          "original_form": "그쳐 코로나라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100018125.1.14",
          "form": "네 요즘 직장 집 직장 집이네요",
          "original_form": "네 요즘 직장 집 직장 집이네요",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100018125.1.15",
          "form": "종교행사하면 크리스마스인데",
          "original_form": "종교행사하면 크리스마스인데",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100018125.1.16",
          "form": "어딜 나갈수가없으니가여ㅜㅜ",
          "original_form": "어딜 나갈수가없으니가여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100018125.1.17",
          "form": "크리스마스 같은거 잘 챙기시나요?",
          "original_form": "크리스마스 같은거 잘 챙기시나요?",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100018125.1.18",
          "form": "맞아요 ㅋㅋㅋ저도 친구따라 교회다니던때가있었어서",
          "original_form": "맞아요 ㅋㅋㅋ저도 친구따라 교회다니던때가있었어서",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100018125.1.19",
          "form": "그때 열심히 챙겼어요 ㅎㅎ...",
          "original_form": "그때 열심히 챙겼어요 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100018125.1.20",
          "form": "아 그렇군요ㅎㅎ 친구가 뭐라고하면서 꼬시던가요?",
          "original_form": "아 그렇군요ㅎㅎ 친구가 뭐라고하면서 꼬시던가요?",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100018125.1.21",
          "form": "ㅋㅋㅋㅋㅋ과자준다고 꼬샸어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ과자준다고 꼬샸어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100018125.1.22",
          "form": "아 어릴땐가 보네요ㅋㅋ",
          "original_form": "아 어릴땐가 보네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100018125.1.23",
          "form": "꽤 열심히다녔었는데 유초등부활동다한듯여",
          "original_form": "꽤 열심히다녔었는데 유초등부활동다한듯여",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100018125.1.24",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
