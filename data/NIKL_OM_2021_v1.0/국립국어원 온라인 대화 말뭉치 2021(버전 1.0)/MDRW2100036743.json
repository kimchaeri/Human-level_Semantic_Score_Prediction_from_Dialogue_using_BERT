      "utterance": [
        {
          "id": "MDRW2100036743.1.1",
          "form": "그동안 오메가 는 꾸준히먹었고",
          "original_form": "그동안 오메가 는 꾸준히먹었고",
          "speaker_id": "2",
          "time": "20210831 11:14"
        },
        {
          "id": "MDRW2100036743.1.2",
          "form": "아 엄마가드시는건 기억력향상에 좋다고하더라고요",
          "original_form": "아 엄마가드시는건 기억력향상에 좋다고하더라고요",
          "speaker_id": "2",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100036743.1.3",
          "form": "영양제도 진짜 종류가 많아요",
          "original_form": "영양제도 진짜 종류가 많아요",
          "speaker_id": "1",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036743.1.4",
          "form": "전 감기 예방하려고 비타민 씨 먹어요",
          "original_form": "전 감기 예방하려고 비타민 씨 먹어요",
          "speaker_id": "1",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036743.1.5",
          "form": "그것도 중요하죠",
          "original_form": "그것도 중요하죠",
          "speaker_id": "2",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036743.1.6",
          "form": "요새 감기는 절대 걸리면안되죠ㅜㅜ",
          "original_form": "요새 감기는 절대 걸리면안되죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:20"
        },
        {
          "id": "MDRW2100036743.1.7",
          "form": "감기는 절대 안돼요..ㅎ",
          "original_form": "감기는 절대 안돼요..ㅎ",
          "speaker_id": "1",
          "time": "20210831 11:24"
        },
        {
          "id": "MDRW2100036743.1.8",
          "form": "기침이라도 할라치면 어찌나 눈치가 보이는지",
          "original_form": "기침이라도 할라치면 어찌나 눈치가 보이는지",
          "speaker_id": "1",
          "time": "20210831 11:24"
        },
        {
          "id": "MDRW2100036743.1.9",
          "form": "어휴 저도 엄청 눈치가ㅜㅜ",
          "original_form": "어휴 저도 엄청 눈치가ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100036743.1.10",
          "form": "그래서 에어컨이랑 선풍기 피하려고요ㅜㅜ",
          "original_form": "그래서 에어컨이랑 선풍기 피하려고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100036743.1.11",
          "form": "맞아요.. 열나면 출입금지 당하고",
          "original_form": "맞아요.. 열나면 출입금지 당하고",
          "speaker_id": "1",
          "time": "20210831 11:29"
        },
        {
          "id": "MDRW2100036743.1.12",
          "form": "진짜 일반적인 감긴데 대역죄인 된 기분 들거 같아서 엄청 조심해요",
          "original_form": "진짜 일반적인 감긴데 대역죄인 된 기분 들거 같아서 엄청 조심해요",
          "speaker_id": "1",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100036743.1.13",
          "form": "겁도 나요 어제살짝 목이 아팠달까?",
          "original_form": "겁도 나요 어제살짝 목이 아팠달까?",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100036743.1.14",
          "form": "그랬는데 제과거를 돌아봤네요ㅜㅜ",
          "original_form": "그랬는데 제과거를 돌아봤네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100036743.1.15",
          "form": "그니까요.. 저땜에 혹시 다른사람 피해줄까봐 걱정이 이만저만이 아니고..",
          "original_form": "그니까요.. 저땜에 혹시 다른사람 피해줄까봐 걱정이 이만저만이 아니고..",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100036743.1.16",
          "form": "요즘처럼 몸 관리 한적이 업네요",
          "original_form": "요즘처럼 몸 관리 한적이 업네요",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100036743.1.17",
          "form": "저도 어제 카디건 안들고갔는데",
          "original_form": "저도 어제 카디건 안들고갔는데",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036743.1.18",
          "form": "오늘부턴 꼭챙깅게요",
          "original_form": "오늘부턴 꼭챙깅게요",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036743.1.19",
          "form": "요새 습하니 버스같은곳은 에어컨을 넘세게 틀어서ㅜ",
          "original_form": "요새 습하니 버스같은곳은 에어컨을 넘세게 틀어서ㅜ",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036743.1.20",
          "form": "오 맞아요.. 오히려 여름에도 버스 같은 곳은 춥더라구요.. 에어컨 빵빵",
          "original_form": "오 맞아요.. 오히려 여름에도 버스 같은 곳은 춥더라구요.. 에어컨 빵빵",
          "speaker_id": "1",
          "time": "20210831 11:57"
        },
        {
          "id": "MDRW2100036743.1.21",
          "form": "요즘엔 코로나가 많이 약해져서 걸려도 사망자까지는 없다고 하기도하던데",
          "original_form": "요즘엔 코로나가 많이 약해져서 걸려도 사망자까지는 없다고 하기도하던데",
          "speaker_id": "2",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100036743.1.22",
          "form": "뭘믿어야할지 모르겠네요",
          "original_form": "뭘믿어야할지 모르겠네요",
          "speaker_id": "2",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100036743.1.23",
          "form": "일단 내가 안아프면 될 거 같아요.. 면역력 높이고.. 꼭 코로나 아니더라도 다른 병도 이겨낼 수 있게 튼튼한 몸을 만들어야죠",
          "original_form": "일단 내가 안아프면 될 거 같아요.. 면역력 높이고.. 꼭 코로나 아니더라도 다른 병도 이겨낼 수 있게 튼튼한 몸을 만들어야죠",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100036743.1.24",
          "form": "그리고 열 안나게 조심하고요",
          "original_form": "그리고 열 안나게 조심하고요",
          "speaker_id": "2",
          "time": "20210831 13:54"
        },
        {
          "id": "MDRW2100036743.1.25",
          "form": "2차백신 맞을때까지요 ㄷㄷ",
          "original_form": "2차백신 맞을때까지요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 13:54"
        },
        {
          "id": "MDRW2100036743.1.26",
          "form": "맞아요.. 열관리가 중요하죠..",
          "original_form": "맞아요.. 열관리가 중요하죠..",
          "speaker_id": "1",
          "time": "20210831 14:04"
        }
      ]
