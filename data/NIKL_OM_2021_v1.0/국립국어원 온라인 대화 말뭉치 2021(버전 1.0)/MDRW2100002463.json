      "utterance": [
        {
          "id": "MDRW2100002463.1.1",
          "form": "너 스타듀밸리 언제할거야?",
          "original_form": "너 스타듀밸리 언제할거야?",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002463.1.2",
          "form": "나 혼자하는거 너무 재미없음",
          "original_form": "나 혼자하는거 너무 재미없음",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002463.1.3",
          "form": "멀티로 해야재밌어",
          "original_form": "멀티로 해야재밌어",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002463.1.4",
          "form": "마즘",
          "original_form": "마즘",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002463.1.5",
          "form": "양띵도 멀티로 8명이서 했잖아",
          "original_form": "양띵도 멀티로 8명이서 했잖아",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002463.1.6",
          "form": "나도 노트북이 있어야 하지..",
          "original_form": "나도 노트북이 있어야 하지..",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002463.1.7",
          "form": "컴퓨터도 고장났는데ㅠ",
          "original_form": "컴퓨터도 고장났는데ㅠ",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.8",
          "form": "칫",
          "original_form": "칫",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.9",
          "form": "하긴 노트북이 있어야하긴해",
          "original_form": "하긴 노트북이 있어야하긴해",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.10",
          "form": "폰 스듀는 멀티도안되고",
          "original_form": "폰 스듀는 멀티도안되고",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.11",
          "form": "불편한게 많아",
          "original_form": "불편한게 많아",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.12",
          "form": "맞아 그 캐릭터 얼굴 바꾸기도 안되잖아",
          "original_form": "맞아 그 캐릭터 얼굴 바꾸기도 안되잖아",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.13",
          "form": "그건 컴퓨터로만 가능혀",
          "original_form": "그건 컴퓨터로만 가능혀",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.14",
          "form": "언니야 스듀 가을까지 했어?",
          "original_form": "언니야 스듀 가을까지 했어?",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.15",
          "form": "아직 여름이야",
          "original_form": "아직 여름이야",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.16",
          "form": "근데 이제 곧 가을 들어갈듯",
          "original_form": "근데 이제 곧 가을 들어갈듯",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002463.1.17",
          "form": "같이해야 농사도 더 빨리되고",
          "original_form": "같이해야 농사도 더 빨리되고",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.18",
          "form": "낚시도 2배로 하고 그러눈데..",
          "original_form": "낚시도 2배로 하고 그러눈데..",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.19",
          "form": "광물도 같이캐면",
          "original_form": "광물도 같이캐면",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.20",
          "form": "100층까지 더빨리 내려갈수있음",
          "original_form": "100층까지 더빨리 내려갈수있음",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.21",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.22",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.23",
          "form": "나도 아빠한테 빨리 노트북 사달래야지",
          "original_form": "나도 아빠한테 빨리 노트북 사달래야지",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.24",
          "form": "오케오케",
          "original_form": "오케오케",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.25",
          "form": "너도 곧 대학가니께",
          "original_form": "너도 곧 대학가니께",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.26",
          "form": "어차피 필요하자나!!",
          "original_form": "어차피 필요하자나!!",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.27",
          "form": "걍 빨리 사달라하자",
          "original_form": "걍 빨리 사달라하자",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.28",
          "form": "그리고 스듀 깔아",
          "original_form": "그리고 스듀 깔아",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002463.1.29",
          "form": "그러면 언니야가",
          "original_form": "그러면 언니야가",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.30",
          "form": "미리 내 집 거기다가 지어줘",
          "original_form": "미리 내 집 거기다가 지어줘",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.31",
          "form": "오두막 하나 지어주삼",
          "original_form": "오두막 하나 지어주삼",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.32",
          "form": "오케 만드러놓고 기다리고잇을게",
          "original_form": "오케 만드러놓고 기다리고잇을게",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.33",
          "form": "수능 끝나고 올수있겠지",
          "original_form": "수능 끝나고 올수있겠지",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.34",
          "form": "ㅠㅠ?",
          "original_form": "ㅠㅠ?",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.35",
          "form": "아마도....",
          "original_form": "아마도....",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.36",
          "form": "그래도 얼마안남았으니깐",
          "original_form": "그래도 얼마안남았으니깐",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.37",
          "form": "우리 멀티 스듀할생각만 하자구",
          "original_form": "우리 멀티 스듀할생각만 하자구",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.38",
          "form": "그얘긴.슬프니깐.....",
          "original_form": "그얘긴.슬프니깐.....",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002463.1.39",
          "form": "어 어....",
          "original_form": "어 어....",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.40",
          "form": "나도 리텍할까? 스듀 애들",
          "original_form": "나도 리텍할까? 스듀 애들",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.41",
          "form": "조치",
          "original_form": "조치",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.42",
          "form": "근데 어케하는지 알아",
          "original_form": "근데 어케하는지 알아",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.43",
          "form": "???좀 어렵던데",
          "original_form": "???좀 어렵던데",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.44",
          "form": "마즘",
          "original_form": "마즘",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.45",
          "form": "그래서 귀찮아서 안하고익는데",
          "original_form": "그래서 귀찮아서 안하고익는데",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.46",
          "form": "리텍한거보면 예쁘긴 하드라",
          "original_form": "리텍한거보면 예쁘긴 하드라",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002463.1.47",
          "form": "근데 사실 나 피시방 가거싶어",
          "original_form": "근데 사실 나 피시방 가거싶어",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002463.1.48",
          "form": "우리 메이플 다시해야하는데..",
          "original_form": "우리 메이플 다시해야하는데..",
          "speaker_id": "1",
          "time": "20210820 12:54"
        }
      ]
