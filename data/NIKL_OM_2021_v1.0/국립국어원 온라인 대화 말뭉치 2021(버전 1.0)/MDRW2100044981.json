      "utterance": [
        {
          "id": "MDRW2100044981.1.1",
          "form": "완전 치킨매니아시군요!!!",
          "original_form": "완전 치킨매니아시군요!!!",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044981.1.2",
          "form": "ㅋㅋㅋㅋ비비큐가 후라이드는 뭔가 가장 바삭하고 맛있는것같아요 !!",
          "original_form": "ㅋㅋㅋㅋ비비큐가 후라이드는 뭔가 가장 바삭하고 맛있는것같아요 !!",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044981.1.3",
          "form": "접수하겠습니다!!",
          "original_form": "접수하겠습니다!!",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044981.1.4",
          "form": "ㅋㅋ술잘드시나요?",
          "original_form": "ㅋㅋ술잘드시나요?",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044981.1.5",
          "form": "못먹진않아요ㅋㅋ",
          "original_form": "못먹진않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044981.1.6",
          "form": "좋아하세요??",
          "original_form": "좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044981.1.7",
          "form": "아니요 저는 예전엔 잘 먹었는데 이제 완전 잘 못마셔요ㅠㅠㅠ",
          "original_form": "아니요 저는 예전엔 잘 먹었는데 이제 완전 잘 못마셔요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044981.1.8",
          "form": "왜 바뀌셨어요?",
          "original_form": "왜 바뀌셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044981.1.9",
          "form": "저도 모르겠어요.. 나이...때문일까요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "저도 모르겠어요.. 나이...때문일까요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044981.1.10",
          "form": "ㅋㅋ 사실 저도 피부병때매 금주 석달째",
          "original_form": "ㅋㅋ 사실 저도 피부병때매 금주 석달째",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044981.1.11",
          "form": "아악 ㅠ 치킨엔 맥주인데ㅠ ㅋㅋㅋㅋㅋ금주안힘드신가요",
          "original_form": "아악 ㅠ 치킨엔 맥주인데ㅠ ㅋㅋㅋㅋㅋ금주안힘드신가요",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044981.1.12",
          "form": "약먹는중에 금주라...ㅜ",
          "original_form": "약먹는중에 금주라...ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044981.1.13",
          "form": "아아 피부과약먹는중이면 ㅠㅠㅠ 그렇죠ㅠㅠㅠ",
          "original_form": "아아 피부과약먹는중이면 ㅠㅠㅠ 그렇죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044981.1.14",
          "form": "원랜 주종이어떻게되세요?ㅋㅋㅋ",
