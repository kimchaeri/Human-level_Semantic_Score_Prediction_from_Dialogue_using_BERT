      "utterance": [
        {
          "id": "MDRW2100045020.1.1",
          "form": "오티티",
          "original_form": "오티티",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045020.1.2",
          "form": "즈그가만든드라마 여러나라 자막붙여서 드라마 방영동안 무료로 고여줌",
          "original_form": "즈그가만든드라마 여러나라 자막붙여서 드라마 방영동안 무료로 고여줌",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045020.1.3",
          "form": "보여줌",
          "original_form": "보여줌",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045020.1.4",
          "form": "시간지나면 유료화",
          "original_form": "시간지나면 유료화",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045020.1.5",
          "form": "오 좋긴하네 실시간인거자나",
          "original_form": "오 좋긴하네 실시간인거자나",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045020.1.6",
          "form": "첨 알았어",
          "original_form": "첨 알았어",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045020.1.7",
          "form": "ㅇㅇ그건좋더라 쟤네 진짜신기한거갈켜준까",
          "original_form": "ㅇㅇ그건좋더라 쟤네 진짜신기한거갈켜준까",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045020.1.8",
          "form": "name2이 없음 유행이고 요즘세상 잘모르겠너ㅋ",
          "original_form": "&name2&이 없음 유행이고 요즘세상 잘모르겠너ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045020.1.9",
          "form": "신기?",
          "original_form": "신기?",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045020.1.10",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045020.1.11",
          "form": "공중파 방영하는 드라마 몇개안되고",
          "original_form": "공중파 방영하는 드라마 몇개안되고",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045020.1.12",
          "form": "다 웹드라마더라",
          "original_form": "다 웹드라마더라",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045020.1.13",
          "form": "중드는",
          "original_form": "중드는",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045020.1.14",
          "form": "웹드는 인터넷?",
          "original_form": "웹드는 인터넷?",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045020.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045020.1.16",
          "form": "공중파는 음청 스케일 큰것만하는가?",
          "original_form": "공중파는 음청 스케일 큰것만하는가?",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045020.1.17",
          "form": "중국 드라마 스케일크게한다들은듯해서ㅎ",
          "original_form": "중국 드라마 스케일크게한다들은듯해서ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045020.1.18",
          "form": "아니 공산당입맛에맞는거",
          "original_form": "아니 공산당입맛에맞는거",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045020.1.19",
          "form": "웹드가 훨씬 규제가 덜 하더라구",
          "original_form": "웹드가 훨씬 규제가 덜 하더라구",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045020.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045020.1.21",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045020.1.22",
          "form": "요즘 그 중국 어마무시하더만",
          "original_form": "요즘 그 중국 어마무시하더만",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045020.1.23",
          "form": "중국은 참 신기한나라야만나서한번이야기해주고싶다",
          "original_form": "중국은 참 신기한나라야만나서한번이야기해주고싶다",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045020.1.24",
          "form": "황딸 조미미?",
          "original_form": "황딸 조미미?",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045020.1.25",
          "form": "진짜재밌어",
          "original_form": "진짜재밌어",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045020.1.26",
          "form": "신기한나라 함 만나서 얘기해야것너ㅋ",
          "original_form": "신기한나라 함 만나서 얘기해야것너ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045020.1.27",
          "form": "ㅇㅇ진짜 신기한 나라여",
          "original_form": "ㅇㅇ진짜 신기한 나라여",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045020.1.28",
          "form": "규제가 덜하니 웹드가 더 번창? 더 잘돼겠다",
          "original_form": "규제가 덜하니 웹드가 더 번창? 더 잘돼겠다",
          "speaker_id": "1",
          "time": "20210908 00:42"
        }
      ]
    }
