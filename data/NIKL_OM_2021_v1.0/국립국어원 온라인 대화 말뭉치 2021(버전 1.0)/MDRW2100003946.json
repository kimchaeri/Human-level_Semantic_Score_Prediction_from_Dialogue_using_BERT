      "utterance": [
        {
          "id": "MDRW2100003946.1.1",
          "form": "배달음식 너무많이사먹어요",
          "original_form": "배달음식 너무많이사먹어요",
          "speaker_id": "1",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003946.1.2",
          "form": "중독된거같아요",
          "original_form": "중독된거같아요",
          "speaker_id": "1",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003946.1.3",
          "form": "저도 배달음식 줄이고 싶어요",
          "original_form": "저도 배달음식 줄이고 싶어요",
          "speaker_id": "2",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003946.1.4",
          "form": "건강도 그렇고 환경도 그렇고",
          "original_form": "건강도 그렇고 환경도 그렇고",
          "speaker_id": "2",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003946.1.5",
          "form": "그런데 쉽지 않더라고요",
          "original_form": "그런데 쉽지 않더라고요",
          "speaker_id": "2",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003946.1.6",
          "form": "그쵸 플라스틱생각해서라도 줄이고싶은데",
          "original_form": "그쵸 플라스틱생각해서라도 줄이고싶은데",
          "speaker_id": "1",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003946.1.7",
          "form": "게다가 돈이 생각보다 많이나가요",
          "original_form": "게다가 돈이 생각보다 많이나가요",
          "speaker_id": "1",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003946.1.8",
          "form": "맞아요ㅜㅜㅜ\r\n그럼 혹시 스스로 요리해서 드시는 것도 있나요?",
          "original_form": "맞아요ㅜㅜㅜ\r\n그럼 혹시 스스로 요리해서 드시는 것도 있나요?",
          "speaker_id": "2",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003946.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003946.1.10",
          "form": "한번씩 먹고싶으면",
          "original_form": "한번씩 먹고싶으면",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003946.1.11",
          "form": "도전해보는데.. 성공률이 높진않아요",
          "original_form": "도전해보는데.. 성공률이 높진않아요",
          "speaker_id": "1",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003946.1.12",
          "form": "ㅎㅎ저도 제가 만든 음식 맛이 없어서 자꾸 요리 안 하게 되더라고요",
          "original_form": "ㅎㅎ저도 제가 만든 음식 맛이 없어서 자꾸 요리 안 하게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003946.1.13",
          "form": "요리를 배워볼까 싶은 생각이 들기도 해요",
          "original_form": "요리를 배워볼까 싶은 생각이 들기도 해요",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003946.1.14",
          "form": "name1 님은 어떠세요?",
          "original_form": "&name1& 님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003946.1.15",
          "form": "저도 배운다면 베이킹클래스 다녀보고싶어요",
          "original_form": "저도 배운다면 베이킹클래스 다녀보고싶어요",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003946.1.16",
          "form": "오븐이 있는게 제 로망이었어요",
          "original_form": "오븐이 있는게 제 로망이었어요",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003946.1.17",
          "form": "오 요즘 진짜 가정용 오븐도 많이들 쓰시더라구요",
          "original_form": "오 요즘 진짜 가정용 오븐도 많이들 쓰시더라구요",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003946.1.18",
          "form": "베이킹클래스는 한 번 들어두면 유익할것같아요",
          "original_form": "베이킹클래스는 한 번 들어두면 유익할것같아요",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003946.1.19",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003946.1.20",
          "form": "마카롱만들어보고싶어요",
          "original_form": "마카롱만들어보고싶어요",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003946.1.21",
          "form": "한번은 꼭 들어보려고요ㅋㅋ",
          "original_form": "한번은 꼭 들어보려고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003946.1.22",
          "form": "ㅎㅎ저는 기본적인 칼질이나 양식!? 배워보고싶어요ㅎㅎ",
          "original_form": "ㅎㅎ저는 기본적인 칼질이나 양식!? 배워보고싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003946.1.23",
          "form": "파스타나 스테이크!",
          "original_form": "파스타나 스테이크!",
          "speaker_id": "2",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003946.1.24",
          "form": "헉 맛있겠네요",
          "original_form": "헉 맛있겠네요",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003946.1.25",
          "form": "갑자기 먹고싶어졌어요",
          "original_form": "갑자기 먹고싶어졌어요",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003946.1.26",
          "form": "저도 칼질 배워보고싶어요",
          "original_form": "저도 칼질 배워보고싶어요",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003946.1.27",
          "form": "저는 과일도 못깎아서 감자칼로 깎아요..",
          "original_form": "저는 과일도 못깎아서 감자칼로 깎아요..",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003946.1.28",
          "form": "ㅋㅋㅋㄱㅋㅋㄱㄲㄱㅋ과일깎는 거 생각보다 고난도죠",
          "original_form": "ㅋㅋㅋㄱㅋㅋㄱㄲㄱㅋ과일깎는 거 생각보다 고난도죠",
          "speaker_id": "2",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003946.1.29",
          "form": "요즘은 온라인 클래스로도 요리 공부 많이 하시더라고요",
          "original_form": "요즘은 온라인 클래스로도 요리 공부 많이 하시더라고요",
          "speaker_id": "2",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003946.1.30",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003946.1.31",
          "form": "온라인으로 대학수업도 하니까 그럴만도 하네여ㅋㅋ",
          "original_form": "온라인으로 대학수업도 하니까 그럴만도 하네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003946.1.32",
          "form": "언젠가 친구초대해놓고 요리대접하고싶어요",
          "original_form": "언젠가 친구초대해놓고 요리대접하고싶어요",
          "speaker_id": "1",
          "time": "20210821 14:20"
        }
      ]
