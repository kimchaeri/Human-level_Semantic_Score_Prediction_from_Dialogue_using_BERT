      "utterance": [
        {
          "id": "MDRW2100042861.1.1",
          "form": "짐 스타벅스 신메뉴 시켰는데 실패했어요",
          "original_form": "짐 스타벅스 신메뉴 시켰는데 실패했어요",
          "speaker_id": "1",
          "time": "20210907 10:09"
        },
        {
          "id": "MDRW2100042861.1.2",
          "form": "아 그 글레이즈???",
          "original_form": "아 그 글레이즈???",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042861.1.3",
          "form": "오오 맞아요",
          "original_form": "오오 맞아요",
          "speaker_id": "1",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042861.1.4",
          "form": "그거 유명템!",
          "original_form": "그거 유명템!",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042861.1.5",
          "form": "그냥 라떼먹을걸",
          "original_form": "그냥 라떼먹을걸",
          "speaker_id": "1",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042861.1.6",
          "form": "단거 싫어하세요?",
          "original_form": "단거 싫어하세요?",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042861.1.7",
          "form": "아뇨 좀 어중간한 단맛이어요",
          "original_form": "아뇨 좀 어중간한 단맛이어요",
          "speaker_id": "1",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100042861.1.8",
          "form": "망템이 아니라 유명템인가여",
          "original_form": "망템이 아니라 유명템인가여",
          "speaker_id": "1",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100042861.1.9",
          "form": "네...ㅜ 다들 좋아하더라고요",
          "original_form": "네...ㅜ 다들 좋아하더라고요",
          "speaker_id": "2",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042861.1.10",
          "form": "전 아이스를 잘안먹어서ㅋㅋ",
          "original_form": "전 아이스를 잘안먹어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042861.1.11",
          "form": "아 심지어 전 핫인데 ㅋㅋ",
          "original_form": "아 심지어 전 핫인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042861.1.12",
          "form": "제주에서만 파는 음료도 많던데",
          "original_form": "제주에서만 파는 음료도 많던데",
          "speaker_id": "2",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042861.1.13",
          "form": "맛아여",
          "original_form": "맛아여",
          "speaker_id": "1",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100042861.1.14",
          "form": "아이스로 먹어야 맛나대요!",
          "original_form": "아이스로 먹어야 맛나대요!",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100042861.1.15",
          "form": "우도땅콩라떼",
          "original_form": "우도땅콩라떼",
