      "utterance": [
        {
          "id": "MDRW2100020438.1.1",
          "form": "저는 착한 사람?",
          "original_form": "저는 착한 사람?",
          "speaker_id": "2",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.2",
          "form": "그냥 착한 사람이 너무 좋아요 ㅋㅋㅋㅋ",
          "original_form": "그냥 착한 사람이 너무 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.3",
          "form": "아 착한사람 좋죠",
          "original_form": "아 착한사람 좋죠",
          "speaker_id": "1",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.4",
          "form": "착한사람이면",
          "original_form": "착한사람이면",
          "speaker_id": "1",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.5",
          "form": "안정감도 들고",
          "original_form": "안정감도 들고",
          "speaker_id": "1",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.6",
          "form": "보통 감정기복이 크게 없는것같아요",
          "original_form": "보통 감정기복이 크게 없는것같아요",
          "speaker_id": "1",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.7",
          "form": "막 세부적인건 뭐 정하고 만나기 힘들고 ㅋㅋ",
          "original_form": "막 세부적인건 뭐 정하고 만나기 힘들고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.8",
          "form": "맞아요 무던한 성격",
          "original_form": "맞아요 무던한 성격",
          "speaker_id": "2",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.9",
          "form": "다혈질이거나 욱하는 사람 싫어하는데 착한사람들은 평온하더라구요",
          "original_form": "다혈질이거나 욱하는 사람 싫어하는데 착한사람들은 평온하더라구요",
          "speaker_id": "1",
          "time": "20210828 05:42"
        },
        {
          "id": "MDRW2100020438.1.10",
          "form": "제가 좀 무던해서 예민한 사람 만났더니 너무 힘들더라구요",
          "original_form": "제가 좀 무던해서 예민한 사람 만났더니 너무 힘들더라구요",
          "speaker_id": "2",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.11",
          "form": "아이구ㅠㅠ 고생하셨겠네요",
          "original_form": "아이구ㅠㅠ 고생하셨겠네요",
          "speaker_id": "1",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.12",
          "form": "전 제가 예민한 타입이라",
          "original_form": "전 제가 예민한 타입이라",
          "speaker_id": "1",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.13",
          "form": "같이 예민하면 파국이에요ㅋㅋ",
          "original_form": "같이 예민하면 파국이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.14",
          "form": "ㅋㅋㅋㅋ그렇겠네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그렇겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.15",
          "form": "예민한 것이 섬세해서 좋을 때도 있지만",
          "original_form": "예민한 것이 섬세해서 좋을 때도 있지만",
          "speaker_id": "2",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.16",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.17",
          "form": "그 외엔 좀 눈치 보게 되어서 ㅋㅋㅋ",
          "original_form": "그 외엔 좀 눈치 보게 되어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:43"
        },
        {
          "id": "MDRW2100020438.1.18",
          "form": "그래도 둥글둥글한 사람이 좋죠",
          "original_form": "그래도 둥글둥글한 사람이 좋죠",
          "speaker_id": "1",
          "time": "20210828 05:44"
        },
        {
          "id": "MDRW2100020438.1.19",
          "form": "대학생때",
          "original_form": "대학생때",
          "speaker_id": "1",
          "time": "20210828 05:44"
        },
        {
          "id": "MDRW2100020438.1.20",
          "form": "소개팅 진짜 많이 했는데",
          "original_form": "소개팅 진짜 많이 했는데",
          "speaker_id": "1",
          "time": "20210828 05:44"
        },
        {
          "id": "MDRW2100020438.1.21",
          "form": "뭐 잘 모르니까",
          "original_form": "뭐 잘 모르니까",
          "speaker_id": "1",
          "time": "20210828 05:44"
        },
        {
          "id": "MDRW2100020438.1.22",
          "form": "이상한 애들도 만나고ㅋㅋㅋ",
          "original_form": "이상한 애들도 만나고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:44"
        },
        {
          "id": "MDRW2100020438.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:44"
        },
        {
          "id": "MDRW2100020438.1.24",
          "form": "전 코로나 전엔 그냥 자만추였는데",
          "original_form": "전 코로나 전엔 그냥 자만추였는데",
          "speaker_id": "2",
          "time": "20210828 05:44"
        },
        {
          "id": "MDRW2100020438.1.25",
          "form": "아하 코로나 시작하면 자연스러운 만남이 힘들긴하져ㅠㅠㅋㅋㅋㅋ",
          "original_form": "아하 코로나 시작하면 자연스러운 만남이 힘들긴하져ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:45"
        },
        {
          "id": "MDRW2100020438.1.26",
          "form": "자만추가 도저히 안되더라구요 ㅋㅋ",
          "original_form": "자만추가 도저히 안되더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:45"
        },
        {
          "id": "MDRW2100020438.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 05:45"
        },
        {
          "id": "MDRW2100020438.1.28",
          "form": "좋은 인연도",
          "original_form": "좋은 인연도",
          "speaker_id": "1",
          "time": "20210828 05:45"
        },
        {
          "id": "MDRW2100020438.1.29",
          "form": "여초 직장이기도 하고 해서",
          "original_form": "여초 직장이기도 하고 해서",
          "speaker_id": "2",
          "time": "20210828 05:45"
        },
        {
          "id": "MDRW2100020438.1.30",
          "form": "노력해야 생기는것같아요",
          "original_form": "노력해야 생기는것같아요",
          "speaker_id": "1",
          "time": "20210828 05:45"
        }
      ]
