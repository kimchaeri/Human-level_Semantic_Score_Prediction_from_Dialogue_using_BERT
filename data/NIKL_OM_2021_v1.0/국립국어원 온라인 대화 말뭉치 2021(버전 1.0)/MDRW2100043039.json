      "utterance": [
        {
          "id": "MDRW2100043039.1.1",
          "form": "아 맞아요ㅋㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100043039.1.2",
          "form": "예체능도 좋아했어요 ㅋㅋㅋ",
          "original_form": "예체능도 좋아했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100043039.1.3",
          "form": "그냥 해맑게 학교 다녔던것 같아요",
          "original_form": "그냥 해맑게 학교 다녔던것 같아요",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100043039.1.4",
          "form": "주변 친구들도 선생님도 좋은 사람들이었고...운이 좋았죠 ㅎㅎ",
          "original_form": "주변 친구들도 선생님도 좋은 사람들이었고...운이 좋았죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100043039.1.5",
          "form": "아주 즐겁게 학교 다니시는 모습이 그려져요",
          "original_form": "아주 즐겁게 학교 다니시는 모습이 그려져요",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100043039.1.6",
          "form": "예체능 중에선 어떤거 제일 잘 하셨어요?",
          "original_form": "예체능 중에선 어떤거 제일 잘 하셨어요?",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100043039.1.7",
          "form": "음악 빼고 다 잘했던거 같아요ㅋㅋㅋ",
          "original_form": "음악 빼고 다 잘했던거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043039.1.8",
          "form": "name1님은",
          "original_form": "&name1&님은",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043039.1.9",
          "form": "대박 체육도잘하셨어요?",
          "original_form": "대박 체육도잘하셨어요?",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043039.1.10",
          "form": "예체능 중엔 뭘 제일 좋아하셧나요",
          "original_form": "예체능 중엔 뭘 제일 좋아하셧나요",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043039.1.11",
          "form": "저는 체육빼고요 ㅎㅎㅎ",
          "original_form": "저는 체육빼고요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043039.1.12",
          "form": "진짜 몸치라서 전과목 통틀어서 체육이 제일 싫었고",
          "original_form": "진짜 몸치라서 전과목 통틀어서 체육이 제일 싫었고",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.13",
          "form": "역시 음악 좋아했어요",
          "original_form": "역시 음악 좋아했어요",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.14",
          "form": "네 잘했어요ㅋㅋ익명이니까 밝히자면 예체능은 음악 빼고 다 1등 찍어봤습니다.",
          "original_form": "네 잘했어요ㅋㅋ익명이니까 밝히자면 예체능은 음악 빼고 다 1등 찍어봤습니다.",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.15",
          "form": "특별히 스트레스받는 부분이 적었달까나",
          "original_form": "특별히 스트레스받는 부분이 적었달까나",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.16",
          "form": "세상에....",
          "original_form": "세상에....",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.17",
          "form": "오 전 노래도 잘 못하고 해서 ㅋㅋㅋㅋㅋ",
          "original_form": "오 전 노래도 잘 못하고 해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.18",
          "form": "여기 그 유명한 전교1등이 계셨군요",
          "original_form": "여기 그 유명한 전교1등이 계셨군요",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.19",
          "form": "오... 맞아요....",
          "original_form": "오... 맞아요....",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043039.1.20",
          "form": "와 오랜만에 자기자랑 하니까",
          "original_form": "와 오랜만에 자기자랑 하니까",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043039.1.21",
          "form": "감회가 새롭네요",
          "original_form": "감회가 새롭네요",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043039.1.22",
          "form": "name1님도 자랑 좀 해주세요",
          "original_form": "&name1&님도 자랑 좀 해주세요",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043039.1.23",
          "form": "저만 하기 부끄러워요 ㅋㅋㅋ",
          "original_form": "저만 하기 부끄러워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043039.1.24",
          "form": "에이 잘난 부분에 대한 사실을 말씀하신건데요",
          "original_form": "에이 잘난 부분에 대한 사실을 말씀하신건데요",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043039.1.25",
          "form": "왜 부끄러워하시나요 ㅎㅎ",
          "original_form": "왜 부끄러워하시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043039.1.26",
          "form": "저도 음악시간엔 가창시간을 별로 좋아하지 않았어요",
          "original_form": "저도 음악시간엔 가창시간을 별로 좋아하지 않았어요",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043039.1.27",
          "form": "사람들이 많이 보는 앞에서 노래 부르는게 되게 민망했거든요",
          "original_form": "사람들이 많이 보는 앞에서 노래 부르는게 되게 민망했거든요",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043039.1.28",
          "form": "이런 이야기 되게 반갑지 않나요 ㅎㅎ",
          "original_form": "이런 이야기 되게 반갑지 않나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043039.1.29",
          "form": "그니까여 ㅋㅋㅋㅋ",
          "original_form": "그니까여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043039.1.30",
          "form": "저는 사실 특별히 눈에 띄는 학생은 아니었어요",
          "original_form": "저는 사실 특별히 눈에 띄는 학생은 아니었어요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043039.1.31",
          "form": "근데 성실하셧을것 같아요",
          "original_form": "근데 성실하셧을것 같아요",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043039.1.32",
          "form": "적당히 다 평범했던? 것 같아요 ㅋㅋㅋ",
          "original_form": "적당히 다 평범했던? 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043039.1.33",
          "form": "그거 되게 어려운건데 ㅋㅋㅋㅋ",
          "original_form": "그거 되게 어려운건데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043039.1.34",
          "form": "무탈하게 지내는게 사실 어렵자나요 ㅠㅠ",
          "original_form": "무탈하게 지내는게 사실 어렵자나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043039.1.35",
          "form": "그 이 어려운걸 하느라 1등까지는 차마 욕심을 내보지 못했던 것 같아요",
          "original_form": "그 이 어려운걸 하느라 1등까지는 차마 욕심을 내보지 못했던 것 같아요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043039.1.36",
          "form": "앗 ㅋㅋㅋㅋ 근데 고등학교 가서는 망했어요",
          "original_form": "앗 ㅋㅋㅋㅋ 근데 고등학교 가서는 망했어요",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043039.1.37",
          "form": "아.. 저도요 ㅎㅎㅎㅎ",
          "original_form": "아.. 저도요 ㅎㅎㅎㅎ",
