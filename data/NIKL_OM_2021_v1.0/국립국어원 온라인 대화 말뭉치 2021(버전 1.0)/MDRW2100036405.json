      "utterance": [
        {
          "id": "MDRW2100036405.1.1",
          "form": "반려동물의 범위가 넓어진거 같아요",
          "original_form": "반려동물의 범위가 넓어진거 같아요",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036405.1.2",
          "form": "네 반려동물이라고 하면 강아지 고양이만 생각했는데",
          "original_form": "네 반려동물이라고 하면 강아지 고양이만 생각했는데",
          "speaker_id": "2",
          "time": "20210831 07:32"
        },
        {
          "id": "MDRW2100036405.1.3",
          "form": "요즘은 정말 다양하더라고요",
          "original_form": "요즘은 정말 다양하더라고요",
          "speaker_id": "2",
          "time": "20210831 07:32"
        },
        {
          "id": "MDRW2100036405.1.4",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210831 08:24"
        },
        {
          "id": "MDRW2100036405.1.5",
          "form": "외국엔 치타나 사막여우도 키우는거보고",
          "original_form": "외국엔 치타나 사막여우도 키우는거보고",
          "speaker_id": "1",
          "time": "20210831 08:24"
        },
        {
          "id": "MDRW2100036405.1.6",
          "form": "우와 달라지는구나 했어요",
          "original_form": "우와 달라지는구나 했어요",
          "speaker_id": "1",
          "time": "20210831 08:24"
        },
        {
          "id": "MDRW2100036405.1.7",
          "form": "오 치타나 사막여우는 진짜 새롭네요",
          "original_form": "오 치타나 사막여우는 진짜 새롭네요",
          "speaker_id": "2",
          "time": "20210831 08:35"
        },
        {
          "id": "MDRW2100036405.1.8",
          "form": "그죠!",
          "original_form": "그죠!",
          "speaker_id": "1",
          "time": "20210831 08:40"
        },
        {
          "id": "MDRW2100036405.1.9",
          "form": "목걸이 차고 있는게 인상적이였어요",
          "original_form": "목걸이 차고 있는게 인상적이였어요",
          "speaker_id": "1",
          "time": "20210831 08:40"
        },
        {
          "id": "MDRW2100036405.1.10",
          "form": "유튜브에 수달영상도 인기있어서 본적있어요!",
          "original_form": "유튜브에 수달영상도 인기있어서 본적있어요!",
          "speaker_id": "1",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100036405.1.11",
          "form": "수달도 귀엽겠네요",
          "original_form": "수달도 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210831 08:53"
        },
        {
          "id": "MDRW2100036405.1.12",
          "form": "그쵸~ 진짜 귀엽더라구요!",
          "original_form": "그쵸~ 진짜 귀엽더라구요!",
          "speaker_id": "1",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036405.1.13",
          "form": "키우시는 동물 있으세요?",
          "original_form": "키우시는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036405.1.14",
          "form": "아뇨~ 저는 지금 반려동물을 기르고 있진 않아요",
          "original_form": "아뇨~ 저는 지금 반려동물을 기르고 있진 않아요",
          "speaker_id": "2",
          "time": "20210831 13:30"
        },
        {
          "id": "MDRW2100036405.1.15",
          "form": "키우신적은 있으세용?",
          "original_form": "키우신적은 있으세용?",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100036405.1.16",
          "form": "전에 소라게랑 이런 것 잠깐 키운적 있는데",
          "original_form": "전에 소라게랑 이런 것 잠깐 키운적 있는데",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100036405.1.17",
          "form": "오랫동안 반려동물 키운 적은 없어요",
          "original_form": "오랫동안 반려동물 키운 적은 없어요",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100036405.1.18",
          "form": "아항 그러시구나",
          "original_form": "아항 그러시구나",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036405.1.19",
          "form": "저는 고양이 키우는데 진짜 이뻐요!",
          "original_form": "저는 고양이 키우는데 진짜 이뻐요!",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036405.1.20",
          "form": "아 고양이 키우시는구나!",
          "original_form": "아 고양이 키우시는구나!",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036405.1.21",
          "form": "무슨 종이예요?",
          "original_form": "무슨 종이예요?",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036405.1.22",
          "form": "아메숏이에요!!",
          "original_form": "아메숏이에요!!",
          "speaker_id": "1",
          "time": "20210831 15:30"
        }
      ]
