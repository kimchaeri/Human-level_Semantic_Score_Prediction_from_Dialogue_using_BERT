        {
          "id": "MDRW2100006852.1.1",
          "form": "준비하는 시간이 짧바 좋죠",
          "original_form": "준비하는 시간이 짧바 좋죠",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006852.1.2",
          "form": "네 후딱 만들어 먹을 수 있어요",
          "original_form": "네 후딱 만들어 먹을 수 있어요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006852.1.3",
          "form": "썰고 바르고 굽고",
          "original_form": "썰고 바르고 굽고",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006852.1.4",
          "form": "샌드위치 메이커도 잘 나와요",
          "original_form": "샌드위치 메이커도 잘 나와요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006852.1.5",
          "form": "재료는요?",
          "original_form": "재료는요?",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006852.1.6",
          "form": "저는 머스타드랑 칠리소스 좋아행",
          "original_form": "저는 머스타드랑 칠리소스 좋아행",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006852.1.7",
          "form": "속은 뭐넣죠",
          "original_form": "속은 뭐넣죠",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006852.1.8",
          "form": "음 속은 햄이랑 양상추?",
          "original_form": "음 속은 햄이랑 양상추?",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006852.1.9",
          "form": "피클두요",
          "original_form": "피클두요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006852.1.10",
          "form": "그리고 계란후라이도 좋고요",
          "original_form": "그리고 계란후라이도 좋고요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006852.1.11",
          "form": "당근프라페도 좋아요",
          "original_form": "당근프라페도 좋아요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006852.1.12",
          "form": "저는 땡초도 조금 넣고 싶네요",
          "original_form": "저는 땡초도 조금 넣고 싶네요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006852.1.13",
          "form": "소스는 캐첩도 굿",
          "original_form": "소스는 캐첩도 굿",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006852.1.14",
          "form": "커피 좋아하시죠",
          "original_form": "커피 좋아하시죠",
