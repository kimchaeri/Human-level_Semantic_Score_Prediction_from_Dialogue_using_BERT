      "utterance": [
        {
          "id": "MDRW2100038364.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038364.1.2",
          "form": "안녕하세요 점심 뭐드세요?",
          "original_form": "안녕하세요 점심 뭐드세요?",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100038364.1.3",
          "form": "제육덮밥이나 닭볶음탕 먹을 것 같아요!",
          "original_form": "제육덮밥이나 닭볶음탕 먹을 것 같아요!",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100038364.1.4",
          "form": "점심 뭐 드실건가요?",
          "original_form": "점심 뭐 드실건가요?",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100038364.1.5",
          "form": "우와 맛있을거같아요",
          "original_form": "우와 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100038364.1.6",
          "form": "식당가시나요?",
          "original_form": "식당가시나요?",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100038364.1.7",
          "form": "네 나가서 먹으려구요",
          "original_form": "네 나가서 먹으려구요",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038364.1.8",
          "form": "점심시간 얼마 안남아서 배고프시겠어요",
          "original_form": "점심시간 얼마 안남아서 배고프시겠어요",
          "speaker_id": "2",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038364.1.9",
          "form": "저는 아점으로 베이글을 먹을까 싶어요",
          "original_form": "저는 아점으로 베이글을 먹을까 싶어요",
          "speaker_id": "2",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038364.1.10",
          "form": "뭐 발라 드실건가요??",
          "original_form": "뭐 발라 드실건가요??",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038364.1.11",
          "form": "그냥 먹어도 맛있는데 크림치즈 발라도 맛있더라구요",
          "original_form": "그냥 먹어도 맛있는데 크림치즈 발라도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038364.1.12",
          "form": "네! 네이버에서 크림치즈 할인 하길래",
          "original_form": "네! 네이버에서 크림치즈 할인 하길래",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038364.1.13",
          "form": "갈릭으로 세통샀어요",
          "original_form": "갈릭으로 세통샀어요",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038364.1.14",
          "form": "완전이득!",
          "original_form": "완전이득!",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038364.1.15",
          "form": "심지어 갈릭! 너무 좋은데요 ㅋㅋㅋㅋㅋ",
          "original_form": "심지어 갈릭! 너무 좋은데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038364.1.16",
          "form": "앉은 자리에서 다 먹으래도 먹을 것 같아요 ㅋㅋㅋ",
          "original_form": "앉은 자리에서 다 먹으래도 먹을 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038364.1.17",
          "form": "그쵸 넘 맛있어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 넘 맛있어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100038364.1.18",
          "form": "이걸먹고 내일은 간식으로 약과를 먹으려구요",
          "original_form": "이걸먹고 내일은 간식으로 약과를 먹으려구요",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100038364.1.19",
          "form": "미리 계획해놨답니다",
          "original_form": "미리 계획해놨답니다",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100038364.1.20",
          "form": "약과 너무 맛있죠!!",
          "original_form": "약과 너무 맛있죠!!",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100038364.1.21",
          "form": "약과 냉장고에 넣어두고 한달내내 입에 달고 살았던 것 같아요 ㅋㅋㅋ",
          "original_form": "약과 냉장고에 넣어두고 한달내내 입에 달고 살았던 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100038364.1.22",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100038364.1.23",
          "form": "한번에 다는 못먹고 야금야금 먹어야돼요",
          "original_form": "한번에 다는 못먹고 야금야금 먹어야돼요",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100038364.1.24",
          "form": "엇 저는 앉은 자리에서 한 줄씩은 먹는 것 같아요 ㅋㅋㅋ 약과 엄청 좋아해서요 ㅋㅋㅋ",
          "original_form": "엇 저는 앉은 자리에서 한 줄씩은 먹는 것 같아요 ㅋㅋㅋ 약과 엄청 좋아해서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:45"
        }
      ]
    }
  ]
