        },
        {
          "id": "MDRW2100030614.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210904 10:18"
        },
        {
          "id": "MDRW2100030614.1.3",
          "form": "프린세스메이커요 ㄹ",
          "original_form": "프린세스메이커요 ㄹ",
          "speaker_id": "1",
          "time": "20210904 10:18"
        },
        {
          "id": "MDRW2100030614.1.4",
          "form": "전 브롤해요",
          "original_form": "전 브롤해요",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100030614.1.5",
          "form": "브롤이 뭔가요?",
          "original_form": "브롤이 뭔가요?",
          "speaker_id": "1",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030614.1.6",
          "form": "초딩겜이여",
          "original_form": "초딩겜이여",
          "speaker_id": "2",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030614.1.7",
          "form": "뭐하는 게임인데요?",
          "original_form": "뭐하는 게임인데요?",
