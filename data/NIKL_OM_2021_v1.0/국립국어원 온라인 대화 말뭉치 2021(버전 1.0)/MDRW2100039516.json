      "utterance": [
        {
          "id": "MDRW2100039516.1.1",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039516.1.2",
          "form": "고등어구이에 콩나물국이요",
          "original_form": "고등어구이에 콩나물국이요",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039516.1.3",
          "form": "집밥 최고!",
          "original_form": "집밥 최고!",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039516.1.4",
          "form": "님은 뭐 드셨어요?",
          "original_form": "님은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039516.1.5",
          "form": "저는 두부부침애 파김치요",
          "original_form": "저는 두부부침애 파김치요",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039516.1.6",
          "form": "막걸리랑 같이 ㅎㅎ",
          "original_form": "막걸리랑 같이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039516.1.7",
          "form": "아ㅋㅋ 환상궁합이네요!",
          "original_form": "아ㅋㅋ 환상궁합이네요!",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039516.1.8",
          "form": "무슨 막걸리였어욪?",
          "original_form": "무슨 막걸리였어욪?",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039516.1.9",
          "form": "원주에서 산 지역막걸리였어요",
          "original_form": "원주에서 산 지역막걸리였어요",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039516.1.10",
          "form": "출렁다리 막걸리였나 ㅎㅎ",
          "original_form": "출렁다리 막걸리였나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039516.1.11",
          "form": "처음 듣네요ㅎ",
          "original_form": "처음 듣네요ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039516.1.12",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039516.1.13",
          "form": "네 저는 옥수수막걸리나 밤막걸리는 안좋아해서",
          "original_form": "네 저는 옥수수막걸리나 밤막걸리는 안좋아해서",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039516.1.14",
          "form": "그냥 기본으로 맛내는것이 좋던데 딱 그랬어요",
          "original_form": "그냥 기본으로 맛내는것이 좋던데 딱 그랬어요",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039516.1.15",
          "form": "본연의 맛을 즐기시는군요!",
          "original_form": "본연의 맛을 즐기시는군요!",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039516.1.16",
          "form": "느린마을막걸리인가 그것도 유명하던데 드셔보셨나요",
          "original_form": "느린마을막걸리인가 그것도 유명하던데 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039516.1.17",
          "form": "아니요 그거말고 국순당 유산균막걸리를 마셔봤어요",
          "original_form": "아니요 그거말고 국순당 유산균막걸리를 마셔봤어요",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039516.1.18",
          "form": "국순당 주식이 있어서 ㅋㅋㅋ",
          "original_form": "국순당 주식이 있어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100039516.1.19",
          "form": "그것도 입맛에 맞았나요",
          "original_form": "그것도 입맛에 맞았나요",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100039516.1.20",
          "form": "아니ㅋ 쥬 쥬 님 ㅋㅋ",
          "original_form": "아니ㅋ 쥬 쥬 님 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100039516.1.21",
          "form": "누가 들으면 엄청 산줄 ㅋㅋㅋ",
          "original_form": "누가 들으면 엄청 산줄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100039516.1.22",
          "form": "유산균막걸리는 더 톡쏘고 덜 되직해요",
          "original_form": "유산균막걸리는 더 톡쏘고 덜 되직해요",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039516.1.23",
          "form": "젊은 입맛같달까 ㅎㅎ",
          "original_form": "젊은 입맛같달까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039516.1.24",
          "form": "아 저 요구르트 같은 막걸리 마셨었는데",
          "original_form": "아 저 요구르트 같은 막걸리 마셨었는데",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039516.1.25",
          "form": "그 스타일이려나요ㅎ",
          "original_form": "그 스타일이려나요ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039516.1.26",
          "form": "비슷할것 같아요",
          "original_form": "비슷할것 같아요",
          "speaker_id": "2",
          "time": "20210903 22:35"
        }
      ]
    }
  ]
