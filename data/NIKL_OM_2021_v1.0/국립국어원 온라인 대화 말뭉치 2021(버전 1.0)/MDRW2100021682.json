        {
          "id": "MDRW2100021682.1.1",
          "form": "도깨비 아시나요?",
          "original_form": "도깨비 아시나요?",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021682.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021682.1.3",
          "form": "? 아신다고요? 뭔데요?",
          "original_form": "? 아신다고요? 뭔데요?",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021682.1.4",
          "form": "도깨비방망이",
          "original_form": "도깨비방망이",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021682.1.5",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021682.1.6",
          "form": "네네...",
          "original_form": "네네...",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021682.1.7",
          "form": "이번에 출시예정인 메타버스게임입니다",
          "original_form": "이번에 출시예정인 메타버스게임입니다",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021682.1.8",
          "form": "언제출시되나요?",
          "original_form": "언제출시되나요?",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021682.1.9",
          "form": "몰라요. 저는 게임에는 관심없어서",
          "original_form": "몰라요. 저는 게임에는 관심없어서",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021682.1.10",
          "form": "그냥 검은사막 만든 펄어비스에서 출시하는데",
          "original_form": "그냥 검은사막 만든 펄어비스에서 출시하는데",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021682.1.11",
          "form": "사실감이 대단하데요",
          "original_form": "사실감이 대단하데요",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021682.1.12",
          "form": "잘모르겠지만 궁금하네요",
          "original_form": "잘모르겠지만 궁금하네요",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021682.1.13",
          "form": "음 그냥 가상현실인데",
          "original_form": "음 그냥 가상현실인데",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021682.1.14",
          "form": "현실이에요",
          "original_form": "현실이에요",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021682.1.15",
          "form": "나중에는 근무도 출근도 거기서해요",
          "original_form": "나중에는 근무도 출근도 거기서해요",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021682.1.16",
          "form": "모르겠어요ㅠㅠ",
          "original_form": "모르겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021682.1.17",
          "form": "음.. 네ㅋ",
          "original_form": "음.. 네ㅋ",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021682.1.18",
          "form": "미국 로블록스나 네이버 재패토? 제패토. 이런거",
          "original_form": "미국 로블록스나 네이버 재패토? 제패토. 이런거",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021682.1.19",
          "form": "아하 가상세계는 어렵네요",
          "original_form": "아하 가상세계는 어렵네요",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100021682.1.20",
          "form": "네 아직은 십대들이 많이 하죠",
          "original_form": "네 아직은 십대들이 많이 하죠",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100021682.1.21",
          "form": "10대들이 많이 하나보네용",
          "original_form": "10대들이 많이 하나보네용",
          "speaker_id": "2",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100021682.1.22",
          "form": "음 오늘 애들이랑 도깨비 영상봤는뎌",
