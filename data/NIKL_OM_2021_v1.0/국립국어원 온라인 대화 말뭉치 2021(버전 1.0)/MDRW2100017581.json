      "utterance": [
        {
          "id": "MDRW2100017581.1.1",
          "form": "안녕하세요 닉이 비슷하시네요 ㅎㅎ",
          "original_form": "안녕하세요 닉이 비슷하시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017581.1.2",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017581.1.3",
          "form": "어머 그러네요! 전 주면 다 잘먹어요 ㅋㅋ",
          "original_form": "어머 그러네요! 전 주면 다 잘먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017581.1.4",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017581.1.5",
          "form": "그럼 주로 어떤거 시켜드세요?",
          "original_form": "그럼 주로 어떤거 시켜드세요?",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017581.1.6",
          "form": "저는 요새 브리또에 빠져살아요 ㅎㅎ",
          "original_form": "저는 요새 브리또에 빠져살아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017581.1.7",
          "form": "브리또가 배달도 돼요?",
          "original_form": "브리또가 배달도 돼요?",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017581.1.8",
          "form": "저는 픽업하러 자주나가요 ㅎㅎ",
          "original_form": "저는 픽업하러 자주나가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017581.1.9",
          "form": "아 포장주문이시구나!",
          "original_form": "아 포장주문이시구나!",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017581.1.10",
          "form": "저 연어브리또 좋아하는데 님은요?",
          "original_form": "저 연어브리또 좋아하는데 님은요?",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017581.1.11",
          "form": "네~ 저는 수비드목살에 요새 환장해요",
          "original_form": "네~ 저는 수비드목살에 요새 환장해요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017581.1.12",
          "form": "헉 진짜 맛있겠다.",
          "original_form": "헉 진짜 맛있겠다.",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017581.1.13",
          "form": "그건 얼마에요?",
          "original_form": "그건 얼마에요?",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017581.1.14",
          "form": "8천원정도 했던거같아요 ㅋㅋㅋ 좀비싸죠?ㅜ",
          "original_form": "8천원정도 했던거같아요 ㅋㅋㅋ 좀비싸죠?ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017581.1.15",
          "form": "하..하나에?",
          "original_form": "하..하나에?",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017581.1.16",
          "form": "설마 반으로 뚝 잘라서 8천원은 아니죠..?",
          "original_form": "설마 반으로 뚝 잘라서 8천원은 아니죠..?",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017581.1.17",
          "form": "아 그건아니에요 ㅋㅋ 하나에 8천원이에용 ㅋㅋ 근데 크기가 막 크진않아요 ㅜ",
          "original_form": "아 그건아니에요 ㅋㅋ 하나에 8천원이에용 ㅋㅋ 근데 크기가 막 크진않아요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017581.1.18",
          "form": "그럼 다행이네요. 가격이 사악해질 뻔 ㅎㅎ",
          "original_form": "그럼 다행이네요. 가격이 사악해질 뻔 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017581.1.19",
          "form": "브리또 만들기 어렵지 않은데 집에서 도전해봤어요?",
          "original_form": "브리또 만들기 어렵지 않은데 집에서 도전해봤어요?",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017581.1.20",
          "form": "네 ㅋㅋ 근데 번거롭고 난장판이 되어서 브리또는 그냥 사먹기로했어요",
          "original_form": "네 ㅋㅋ 근데 번거롭고 난장판이 되어서 브리또는 그냥 사먹기로했어요",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017581.1.21",
          "form": "아 ㅋㅋ 저도 한때 브리또 빠져서 매일 만들어먹었거든요",
          "original_form": "아 ㅋㅋ 저도 한때 브리또 빠져서 매일 만들어먹었거든요",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017581.1.22",
          "form": "연어브리또 만들어서 점심대용으로 먹고 그랬어요!",
          "original_form": "연어브리또 만들어서 점심대용으로 먹고 그랬어요!",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017581.1.23",
          "form": "오 저도 연어브리또 담에 도전해볼게요 한번도안먹아봤어요",
          "original_form": "오 저도 연어브리또 담에 도전해볼게요 한번도안먹아봤어요",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017581.1.24",
          "form": "연어 좋아하시면 충분히 만족하실 거에요!",
          "original_form": "연어 좋아하시면 충분히 만족하실 거에요!",
          "speaker_id": "1",
          "time": "20210826 20:38"
        }
      ]
    }
  ]
