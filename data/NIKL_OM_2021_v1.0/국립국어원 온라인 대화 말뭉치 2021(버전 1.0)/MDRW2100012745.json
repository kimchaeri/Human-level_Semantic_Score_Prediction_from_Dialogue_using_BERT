      "utterance": [
        {
          "id": "MDRW2100012745.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100012745.1.2",
          "form": "좋아하지만\n하고 있지는 않습니다ㅋㅋ",
          "original_form": "좋아하지만\n하고 있지는 않습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100012745.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100012745.1.4",
          "form": "저는 가끔 모바일게임 하는 정도?",
          "original_form": "저는 가끔 모바일게임 하는 정도?",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100012745.1.5",
          "form": "안 하시는 이유는 중독성 때문인가요?",
          "original_form": "안 하시는 이유는 중독성 때문인가요?",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100012745.1.6",
          "form": "딩동댕",
          "original_form": "딩동댕",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100012745.1.7",
          "form": "저를 너무 잘 알아서\n시작을 할 수가 없네요ㅋㅋ",
          "original_form": "저를 너무 잘 알아서\n시작을 할 수가 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100012745.1.8",
          "form": "게임 방송 같은 것도 가끔 보세요?",
          "original_form": "게임 방송 같은 것도 가끔 보세요?",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100012745.1.9",
          "form": "예전에 스타는 봤었는데\n지금은 따로 보고 있는건 없어요",
          "original_form": "예전에 스타는 봤었는데\n지금은 따로 보고 있는건 없어요",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100012745.1.10",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100012745.1.11",
          "form": "저는 가끔 심즈 유튜브 볼 때 있어요",
          "original_form": "저는 가끔 심즈 유튜브 볼 때 있어요",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100012745.1.12",
          "form": "직접 하긴 귀찮고 보는 걸로 대리만족 ㅋㅋ",
          "original_form": "직접 하긴 귀찮고 보는 걸로 대리만족 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100012745.1.13",
          "form": "ㅋㅋ게임이 보는걸로\n대리만족이 가능한가요?ㅋㅋ",
          "original_form": "ㅋㅋ게임이 보는걸로\n대리만족이 가능한가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100012745.1.14",
          "form": "ㅋㅋ 심즈는 이상하게 직접 하면 자꾸 일만하게 되더라구요",
          "original_form": "ㅋㅋ 심즈는 이상하게 직접 하면 자꾸 일만하게 되더라구요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100012745.1.15",
          "form": "ㅋㅋ그런 게임도 있군영",
          "original_form": "ㅋㅋ그런 게임도 있군영",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100012745.1.16",
          "form": "인생 게임? 같은 건데 게임에서도 애 생기면 애 보느라 딴일을 못하겠어요ㅋㅋ 너무 현실적",
          "original_form": "인생 게임? 같은 건데 게임에서도 애 생기면 애 보느라 딴일을 못하겠어요ㅋㅋ 너무 현실적",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100012745.1.17",
          "form": "개발자가 현실 어머님이시려나요ㅋㅋ",
          "original_form": "개발자가 현실 어머님이시려나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100012745.1.18",
          "form": "게임 속에서도 출근 열심히 하고ㅋㅋ",
          "original_form": "게임 속에서도 출근 열심히 하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:19"
        }
      ]
