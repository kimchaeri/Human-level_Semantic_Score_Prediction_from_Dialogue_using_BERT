      "utterance": [
        {
          "id": "MDRW2100035958.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035958.1.2",
          "form": "아직이요 점심드셨어요?",
          "original_form": "아직이요 점심드셨어요?",
          "speaker_id": "2",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100035958.1.3",
          "form": "저도 아직 못 챙겼어요ㅜㅜ",
          "original_form": "저도 아직 못 챙겼어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100035958.1.4",
          "form": "뭐 드실 예정인가요?",
          "original_form": "뭐 드실 예정인가요?",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100035958.1.5",
          "form": "저는 방금 샌드위치 해먹고 왔어요",
          "original_form": "저는 방금 샌드위치 해먹고 왔어요",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100035958.1.6",
          "form": "너무 맛있었을 거 같아요! 재료는 뭐뭐 들어갔나요?",
          "original_form": "너무 맛있었을 거 같아요! 재료는 뭐뭐 들어갔나요?",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100035958.1.7",
          "form": "양상추랑 슬라이스 햄 , 치즈 , 계란 넣었어요",
          "original_form": "양상추랑 슬라이스 햄 , 치즈 , 계란 넣었어요",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100035958.1.8",
          "form": "냉장고 털이 했죠",
          "original_form": "냉장고 털이 했죠",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100035958.1.9",
          "form": "맛없을 수 없는 조합이네요ㅎㅎ",
          "original_form": "맛없을 수 없는 조합이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:36"
        },
        {
          "id": "MDRW2100035958.1.10",
          "form": "자주 만들어 드세요?",
          "original_form": "자주 만들어 드세요?",
          "speaker_id": "1",
          "time": "20210830 14:36"
        },
        {
          "id": "MDRW2100035958.1.11",
          "form": "냉장고에 야채가 애매하게 남았을때 자주해요",
          "original_form": "냉장고에 야채가 애매하게 남았을때 자주해요",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100035958.1.12",
          "form": "에그마요도 자주 해먹구요",
          "original_form": "에그마요도 자주 해먹구요",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100035958.1.13",
          "form": "에그마요도 맛있죠~ 음료도 같이 드셨나요?",
          "original_form": "에그마요도 맛있죠~ 음료도 같이 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100035958.1.14",
          "form": "우유랑 먹었어요",
          "original_form": "우유랑 먹었어요",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100035958.1.15",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100035958.1.16",
          "form": "네 좀 전에 먹었어요!",
          "original_form": "네 좀 전에 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100035958.1.17",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100035958.1.18",
          "form": "나시고랭 먹었어요!",
          "original_form": "나시고랭 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 14:49"
        }
      ]
