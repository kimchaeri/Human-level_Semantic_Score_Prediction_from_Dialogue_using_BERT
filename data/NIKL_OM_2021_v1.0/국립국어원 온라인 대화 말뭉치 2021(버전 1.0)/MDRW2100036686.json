      "utterance": [
        {
          "id": "MDRW2100036686.1.1",
          "form": "아침 드셨나요^^",
          "original_form": "아침 드셨나요^^",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036686.1.2",
          "form": "아침은 잘 안먹어서 스킵했어요",
          "original_form": "아침은 잘 안먹어서 스킵했어요",
          "speaker_id": "1",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036686.1.3",
          "form": "야식도  늦게 먹었고ㅎ",
          "original_form": "야식도  늦게 먹었고ㅎ",
          "speaker_id": "1",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036686.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036686.1.5",
          "form": "저는 간단히 먹었어요\r\n야식은 뭐드셨나요",
          "original_form": "저는 간단히 먹었어요\r\n야식은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210831 08:56"
        },
        {
          "id": "MDRW2100036686.1.6",
          "form": "닭껍질튀김이요!",
          "original_form": "닭껍질튀김이요!",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036686.1.7",
          "form": "와 저는 먹어본기억이 없어요 배달하신건가요,",
          "original_form": "와 저는 먹어본기억이 없어요 배달하신건가요,",
          "speaker_id": "2",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036686.1.8",
          "form": "kfc에서 배달해 먹었죠ㅎ",
          "original_form": "kfc에서 배달해 먹었죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 08:58"
        },
        {
          "id": "MDRW2100036686.1.9",
          "form": "원래 오돌뼈튀김 닭껍질튀김 이런거 좋아해요",
          "original_form": "원래 오돌뼈튀김 닭껍질튀김 이런거 좋아해요",
          "speaker_id": "1",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100036686.1.10",
          "form": "오 kfc메뉴일줄이야! 전혀 몰랐던 세계군요 ㅎㅎ",
          "original_form": "오 kfc메뉴일줄이야! 전혀 몰랐던 세계군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 10:35"
        },
        {
          "id": "MDRW2100036686.1.11",
          "form": "짭짤 바삭해서 안주로 딱이네요",
          "original_form": "짭짤 바삭해서 안주로 딱이네요",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100036686.1.12",
          "form": "맥주도 한잔 하셨나요^^",
          "original_form": "맥주도 한잔 하셨나요^^",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100036686.1.13",
          "form": "빠지면 섭하죠ㅎ",
          "original_form": "빠지면 섭하죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100036686.1.14",
          "form": "요새 혼술의 매력에 빠졌어요ㅎ",
          "original_form": "요새 혼술의 매력에 빠졌어요ㅎ",
          "speaker_id": "1",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100036686.1.15",
          "form": "이제 점심이네요 뭐 드실건가요!",
          "original_form": "이제 점심이네요 뭐 드실건가요!",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100036686.1.16",
          "form": "라따뚜이를 해먹으려고 아시나요?",
          "original_form": "라따뚜이를 해먹으려고 아시나요?",
