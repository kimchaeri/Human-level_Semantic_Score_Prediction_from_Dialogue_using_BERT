      "utterance": [
        {
          "id": "MDRW2100038451.1.1",
          "form": "name1님은 영아일랜드사니까 딱히 어디안가두 돼죵",
          "original_form": "&name1&님은 영아일랜드사니까 딱히 어디안가두 돼죵",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038451.1.2",
          "form": "휴양지에 사는 사람은 여행안가도 되나요?",
          "original_form": "휴양지에 사는 사람은 여행안가도 되나요?",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038451.1.3",
          "form": "전 집순이에요",
          "original_form": "전 집순이에요",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038451.1.4",
          "form": "집에서 집으로 가죠",
          "original_form": "집에서 집으로 가죠",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038451.1.5",
          "form": "여행안좋아하세요?",
          "original_form": "여행안좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100038451.1.6",
          "form": "오 가끔 가면 좋아요",
          "original_form": "오 가끔 가면 좋아요",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038451.1.7",
          "form": "행복하쥬",
          "original_form": "행복하쥬",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038451.1.8",
          "form": "제가 2주동안 혼자 집없이 밖을 다닌적이 있는데 떠돌이는 내 체질이 아니구나를 깨달았쥬",
          "original_form": "제가 2주동안 혼자 집없이 밖을 다닌적이 있는데 떠돌이는 내 체질이 아니구나를 깨달았쥬",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100038451.1.9",
          "form": "name1님은 여행조아하나유?",
          "original_form": "&name1&님은 여행조아하나유?",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100038451.1.10",
          "form": "2주나요?ㅜㅜ",
          "original_form": "2주나요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038451.1.11",
          "form": "힘들었겠네요",
          "original_form": "힘들었겠네요",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038451.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038451.1.13",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038451.1.14",
          "form": "전 여행좋아해요",
          "original_form": "전 여행좋아해요",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038451.1.15",
          "form": "힘들엇시요",
          "original_form": "힘들엇시요",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038451.1.16",
          "form": "name1님도 이주를 떠돌이하믄",
          "original_form": "&name1&님도 이주를 떠돌이하믄",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038451.1.17",
          "form": "생각이달라질꺼요",
          "original_form": "생각이달라질꺼요",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100038451.1.18",
          "form": "안 떠돌꺼에요",
          "original_form": "안 떠돌꺼에요",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100038451.1.19",
          "form": "여행도 3일이 딱좋아요ㅋ",
          "original_form": "여행도 3일이 딱좋아요ㅋ",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100038451.1.20",
          "form": "마저용ㅋㅋ",
          "original_form": "마저용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100038451.1.21",
          "form": "여행 간 곳 중에",
          "original_form": "여행 간 곳 중에",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038451.1.22",
          "form": "기억남는 곳 있나요?",
          "original_form": "기억남는 곳 있나요?",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038451.1.23",
          "form": "제주바다요",
          "original_form": "제주바다요",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038451.1.24",
          "form": "이뻐요 name1님은요",
          "original_form": "이뻐요 &name1&님은요",
