      "utterance": [
        {
          "id": "MDRW2100026479.1.1",
          "form": "명절 음식을 직접 하시나요?",
          "original_form": "명절 음식을 직접 하시나요?",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026479.1.2",
          "form": "네 제가 합니다 ㅎㅎ",
          "original_form": "네 제가 합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026479.1.3",
          "form": "지금 5년째 하고있네요",
          "original_form": "지금 5년째 하고있네요",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026479.1.4",
          "form": "튀김먼저 하고 전해요 ㅎㅎ",
          "original_form": "튀김먼저 하고 전해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026479.1.5",
          "form": "와 대단하세요 ㅋㅋ",
          "original_form": "와 대단하세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026479.1.6",
          "form": "저도 제사 가지고온지 1년차인데 ㅋㅋ",
          "original_form": "저도 제사 가지고온지 1년차인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026479.1.7",
          "form": "추석음식 해보니 여성분들이 왜 명절 증후군 오는지 알것 같더라구요 ㅋㅋ",
          "original_form": "추석음식 해보니 여성분들이 왜 명절 증후군 오는지 알것 같더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100026479.1.8",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026479.1.9",
          "form": "저도 엄마 돌아가신 첫 해 명절음식 혼자 하다가 몸살났잖아요 ㅋㅋ",
          "original_form": "저도 엄마 돌아가신 첫 해 명절음식 혼자 하다가 몸살났잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026479.1.10",
          "form": "시댁에서 음식 다하시죠?",
          "original_form": "시댁에서 음식 다하시죠?",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026479.1.11",
          "form": "아니요 시댁 제사 제가 들고왔어요 ㅋ",
          "original_form": "아니요 시댁 제사 제가 들고왔어요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026479.1.12",
          "form": "친정제사음식은 주문하구요 ㅋ",
          "original_form": "친정제사음식은 주문하구요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026479.1.13",
          "form": "아 그럼",
          "original_form": "아 그럼",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026479.1.14",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026479.1.15",
          "form": "그럼 집에서 다 하시겠네요?",
          "original_form": "그럼 집에서 다 하시겠네요?",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026479.1.16",
          "form": "네네 ㅋㅋ 집에서 다해요 ㅋㅋ",
          "original_form": "네네 ㅋㅋ 집에서 다해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026479.1.17",
          "form": "저희집 제사음식, 친정제사음식 다 주문해요 ㅋㅋㅋ",
          "original_form": "저희집 제사음식, 친정제사음식 다 주문해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026479.1.18",
          "form": "그래서 제사만 지내요 ㅋ",
          "original_form": "그래서 제사만 지내요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026479.1.19",
          "form": "아이고 대단하십니다",
          "original_form": "아이고 대단하십니다",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026479.1.20",
          "form": "아 주문해서 하시는건가요?",
          "original_form": "아 주문해서 하시는건가요?",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026479.1.21",
          "form": "그걸 도입해야됩니다 ㅋㅋ주문",
          "original_form": "그걸 도입해야됩니다 ㅋㅋ주문",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026479.1.22",
          "form": "네네 ㅋㅋ 주문이요 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋ 주문이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026479.1.23",
          "form": "밥이랑 다른거 사는것만 해요 ㅋㅋㅋ",
          "original_form": "밥이랑 다른거 사는것만 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026479.1.24",
          "form": "음식 못하겠어요 ㅋㅋㅋㅋㅋㅋ맛도 사실 없구요 ㅋㅋ",
          "original_form": "음식 못하겠어요 ㅋㅋㅋㅋㅋㅋ맛도 사실 없구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026479.1.25",
          "form": "저도 아버지 좀더 순해지시면 그럴려구요 ㅎㅎ",
          "original_form": "저도 아버지 좀더 순해지시면 그럴려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026479.1.26",
          "form": "네네 ㅋㅋ 저희 아버지도 ㅋㅋ 좀 그러셨는데 ㅋㅋ 몇년해보니 ㅋㅋㅋ 어려운걸 느끼고는 주문으로 갔어요 ㅋㅋ",
          "original_form": "네네 ㅋㅋ 저희 아버지도 ㅋㅋ 좀 그러셨는데 ㅋㅋ 몇년해보니 ㅋㅋㅋ 어려운걸 느끼고는 주문으로 갔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026479.1.27",
          "form": "좋은 아버지십니다 ㅎㅎ",
          "original_form": "좋은 아버지십니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026479.1.28",
          "form": "아들말 잘 안들으시더라구요",
          "original_form": "아들말 잘 안들으시더라구요",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026479.1.29",
          "form": "제가 아직 어려서 그런가",
          "original_form": "제가 아직 어려서 그런가",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026479.1.30",
          "form": "아 네네 ㅋㅋ",
          "original_form": "아 네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026479.1.31",
          "form": "30대중반이라 좀 더 먹고 회유해보려고요 ㅋㅋ",
          "original_form": "30대중반이라 좀 더 먹고 회유해보려고요 ㅋㅋ",
