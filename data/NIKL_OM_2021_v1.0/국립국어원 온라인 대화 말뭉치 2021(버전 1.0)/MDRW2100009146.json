      "utterance": [
        {
          "id": "MDRW2100009146.1.1",
          "form": "크 술은 못하지만 맛있을 것 같아요ㅋㅋㅋ",
          "original_form": "크 술은 못하지만 맛있을 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009146.1.2",
          "form": "그럼 튀김에 튀김간장 찍어먹는것으로..",
          "original_form": "그럼 튀김에 튀김간장 찍어먹는것으로..",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009146.1.3",
          "form": "후룹 맛있겠네요",
          "original_form": "후룹 맛있겠네요",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009146.1.4",
          "form": "아 튀김엔 떡볶이 양념이죠",
          "original_form": "아 튀김엔 떡볶이 양념이죠",
          "speaker_id": "2",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009146.1.5",
          "form": "아 인정입니다",
          "original_form": "아 인정입니다",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009146.1.6",
          "form": "떡볶이에 튀김에 쿨피스에ㅜ 배고프네요",
          "original_form": "떡볶이에 튀김에 쿨피스에ㅜ 배고프네요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009146.1.7",
          "form": "삶은달걀이랑 참치마요컵밥..",
          "original_form": "삶은달걀이랑 참치마요컵밥..",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009146.1.8",
          "form": "역시 아는 맛이 제일 무섭네요",
          "original_form": "역시 아는 맛이 제일 무섭네요",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009146.1.9",
          "form": "당장 떡을 입안에 넣고 싶어요 ....",
          "original_form": "당장 떡을 입안에 넣고 싶어요 ....",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009146.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 역시 탄수화물이 정답입니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 역시 탄수화물이 정답입니다",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009146.1.11",
          "form": "떡볶이 좋아하는 브랜드있으신가요",
          "original_form": "떡볶이 좋아하는 브랜드있으신가요",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009146.1.12",
          "form": "음 다 좋아해요",
          "original_form": "음 다 좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100009146.1.13",
          "form": "주문할 때는 그날 먹고 싶은 떡볶이 양념의 맛으로 결정하죠!!",
          "original_form": "주문할 때는 그날 먹고 싶은 떡볶이 양념의 맛으로 결정하죠!!",
          "speaker_id": "2",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009146.1.14",
          "form": "오 맛 앞에서 공정하시군요",
          "original_form": "오 맛 앞에서 공정하시군요",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100009146.1.15",
          "form": "저는 신전 좋아해요!",
          "original_form": "저는 신전 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100009146.1.16",
          "form": "신전 진짜 맛있는데 건강 아작나는 맛이라 가끔만 먹어야해요",
          "original_form": "신전 진짜 맛있는데 건강 아작나는 맛이라 가끔만 먹어야해요",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009146.1.17",
          "form": "신전치즈김밥이랑 국물 찍어먹으면 완전 맛있는 거 아시죠?",
          "original_form": "신전치즈김밥이랑 국물 찍어먹으면 완전 맛있는 거 아시죠?",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009146.1.18",
          "form": "쩝쩝박사님이시다",
          "original_form": "쩝쩝박사님이시다",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009146.1.19",
          "form": "찐입니다 신전치즈김밥 \r\n신전은 치즈가 진짜",
          "original_form": "찐입니다 신전치즈김밥 \r\n신전은 치즈가 진짜",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009146.1.20",
          "form": "캬 역시 뭘 아시네요",
          "original_form": "캬 역시 뭘 아시네요",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009146.1.21",
          "form": "신전 오뎅튀김도 맛있지않나요",
          "original_form": "신전 오뎅튀김도 맛있지않나요",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009146.1.22",
          "form": "아 전 사실 그 오뎅튀김의 인기에 동감하지 못하는 사람입니다",
          "original_form": "아 전 사실 그 오뎅튀김의 인기에 동감하지 못하는 사람입니다",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009146.1.23",
          "form": "오 소신발언",
          "original_form": "오 소신발언",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009146.1.24",
          "form": "멋집니닼ㅋㅋㅋ",
          "original_form": "멋집니닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009146.1.25",
          "form": "꾸이맨인가요? 그 어포같은 과자랑 비슷하던데 그게 그렇게 맛있는지 전 잘 모르겠습니다(안경쓱)",
          "original_form": "꾸이맨인가요? 그 어포같은 과자랑 비슷하던데 그게 그렇게 맛있는지 전 잘 모르겠습니다(안경쓱)",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009146.1.26",
          "form": "꾸이맨ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "꾸이맨ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009146.1.27",
          "form": "생각해보니 비슷한거같아요",
          "original_form": "생각해보니 비슷한거같아요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009146.1.28",
          "form": "꾸이맨을 떡볶이에 찍어먹어볼까요..",
          "original_form": "꾸이맨을 떡볶이에 찍어먹어볼까요..",
          "speaker_id": "1",
          "time": "20210822 22:12"
        }
      ]
    }
