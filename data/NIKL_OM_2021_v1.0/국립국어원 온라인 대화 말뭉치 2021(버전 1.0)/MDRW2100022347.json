      "utterance": [
        {
          "id": "MDRW2100022347.1.1",
          "form": "셉틴 올해 안에 또 컴백한대",
          "original_form": "셉틴 올해 안에 또 컴백한대",
          "speaker_id": "1",
          "time": "20210830 10:07"
        },
        {
          "id": "MDRW2100022347.1.2",
          "form": "자주 하네",
          "original_form": "자주 하네",
          "speaker_id": "2",
          "time": "20210830 10:07"
        },
        {
          "id": "MDRW2100022347.1.3",
          "form": "컴백이라고 해도됨?",
          "original_form": "컴백이라고 해도됨?",
          "speaker_id": "2",
          "time": "20210830 10:08"
        },
        {
          "id": "MDRW2100022347.1.4",
          "form": "요즘 아이돌 엄청 컴백 빨라졌더라",
          "original_form": "요즘 아이돌 엄청 컴백 빨라졌더라",
          "speaker_id": "1",
          "time": "20210830 10:11"
        },
        {
          "id": "MDRW2100022347.1.5",
          "form": "이비에스에서 하는 거 다시보기 볼거니?",
          "original_form": "이비에스에서 하는 거 다시보기 볼거니?",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100022347.1.6",
          "form": "생각나면 보겄지?",
          "original_form": "생각나면 보겄지?",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100022347.1.7",
          "form": "대학 강의 생각나는데 그래도 일반인들이 듣는 거라는 생각으로 하시는 것 같더라",
          "original_form": "대학 강의 생각나는데 그래도 일반인들이 듣는 거라는 생각으로 하시는 것 같더라",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100022347.1.8",
          "form": "그래? 솔직히 관심없어가지구...",
          "original_form": "그래? 솔직히 관심없어가지구...",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100022347.1.9",
          "form": "뭔 강의하는데?",
          "original_form": "뭔 강의하는데?",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100022347.1.10",
          "form": "이번주는 리더십 강의야",
          "original_form": "이번주는 리더십 강의야",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100022347.1.11",
          "form": "흥미없는데ㅋㅋㅋㅋ",
          "original_form": "흥미없는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100022347.1.12",
          "form": "매주 다른거임?",
          "original_form": "매주 다른거임?",
          "speaker_id": "2",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100022347.1.13",
          "form": "매주 강연자가 달라서",
          "original_form": "매주 강연자가 달라서",
          "speaker_id": "1",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100022347.1.14",
          "form": "다다음주엔 죽은 줄 알았던 리차드 도킨스야",
          "original_form": "다다음주엔 죽은 줄 알았던 리차드 도킨스야",
          "speaker_id": "1",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100022347.1.15",
          "form": "ㅋㅋㅋㅋㅋ이비에스가 다 잘 모았나보네",
          "original_form": "ㅋㅋㅋㅋㅋ이비에스가 다 잘 모았나보네",
          "speaker_id": "2",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100022347.1.16",
          "form": "강연자 리스트 뜬 거 보면 잘 모아놨더라 총균쇠 자까도 추가됐옼ㅋㅋㅋ",
          "original_form": "강연자 리스트 뜬 거 보면 잘 모아놨더라 총균쇠 자까도 추가됐옼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100022347.1.17",
          "form": "오ㅋㅋㅋ 이비에스의 섭외력 미스테리야",
          "original_form": "오ㅋㅋㅋ 이비에스의 섭외력 미스테리야",
          "speaker_id": "2",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100022347.1.18",
          "form": "펭수로 돈 많이 벌었나바",
          "original_form": "펭수로 돈 많이 벌었나바",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100022347.1.19",
          "form": "많지는 않다던데",
          "original_form": "많지는 않다던데",
          "speaker_id": "2",
