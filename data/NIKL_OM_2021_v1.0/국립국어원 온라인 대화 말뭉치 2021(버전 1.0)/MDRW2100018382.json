      "utterance": [
        {
          "id": "MDRW2100018382.1.1",
          "form": "오늘 점심은 토스트였음",
          "original_form": "오늘 점심은 토스트였음",
          "speaker_id": "2",
          "time": "20210827 00:45"
        },
        {
          "id": "MDRW2100018382.1.2",
          "form": "오 토스트 맛있지",
          "original_form": "오 토스트 맛있지",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018382.1.3",
          "form": "식빵 버터에 자글자글 익히면",
          "original_form": "식빵 버터에 자글자글 익히면",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018382.1.4",
          "form": "세상 행복해",
          "original_form": "세상 행복해",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018382.1.5",
          "form": "다진 야채 들어간 계란 부쳐서 체다치즈 조금 올려가지고 빵이랑 먹으면 완전 맛있지",
          "original_form": "다진 야채 들어간 계란 부쳐서 체다치즈 조금 올려가지고 빵이랑 먹으면 완전 맛있지",
          "speaker_id": "2",
          "time": "20210827 11:22"
        },
        {
          "id": "MDRW2100018382.1.6",
          "form": "토잘알 아냐ㅋㅋㅋ",
          "original_form": "토잘알 아냐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100018382.1.7",
          "form": "토스트에 콘치즈올려서 먹고싶다",
          "original_form": "토스트에 콘치즈올려서 먹고싶다",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018382.1.8",
          "form": "탄수 플 탄수는 온리원 사랑이라구",
          "original_form": "탄수 플 탄수는 온리원 사랑이라구",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018382.1.9",
          "form": "오늘 점심 뭐였어",
          "original_form": "오늘 점심 뭐였어",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100018382.1.10",
          "form": "토스트에 콘치즈",
          "original_form": "토스트에 콘치즈",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100018382.1.11",
          "form": "맛잘알",
          "original_form": "맛잘알",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100018382.1.12",
          "form": "오뎅탕이었는데",
          "original_form": "오뎅탕이었는데",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100018382.1.13",
          "form": "불어서 아쉬웠음",
          "original_form": "불어서 아쉬웠음",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100018382.1.14",
          "form": "고기 반찬이 없어서",
          "original_form": "고기 반찬이 없어서",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100018382.1.15",
          "form": "더 아쉬웠음",
          "original_form": "더 아쉬웠음",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100018382.1.16",
          "form": "토스트 이즈 뭔들 아니겠어",
          "original_form": "토스트 이즈 뭔들 아니겠어",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100018382.1.17",
          "form": "오뎅탕 나는 별론데",
          "original_form": "오뎅탕 나는 별론데",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100018382.1.18",
          "form": "오뎅이 별로임",
          "original_form": "오뎅이 별로임",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100018382.1.19",
          "form": "칼칼한 해물육수는 괜찮은데",
          "original_form": "칼칼한 해물육수는 괜찮은데",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100018382.1.20",
          "form": "맞아ㅋㅋ육수는 좋은데 어묵이 밀가루가 많은건 금방 퍼져버려서",
          "original_form": "맞아ㅋㅋ육수는 좋은데 어묵이 밀가루가 많은건 금방 퍼져버려서",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100018382.1.21",
          "form": "먹기가 힘들어",
          "original_form": "먹기가 힘들어",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100018382.1.22",
          "form": "먹기도 별로고 단맛이 심해서 맛이 없음",
          "original_form": "먹기도 별로고 단맛이 심해서 맛이 없음",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100018382.1.23",
          "form": "나는 저녁을 해산물 라면으로 먹음",
          "original_form": "나는 저녁을 해산물 라면으로 먹음",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100018382.1.24",
          "form": "엄청나게 커스텀해서",
          "original_form": "엄청나게 커스텀해서",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100018382.1.25",
          "form": "게 넣고 끓임",
          "original_form": "게 넣고 끓임",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100018382.1.26",
          "form": "와 게 멋져",
          "original_form": "와 게 멋져",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018382.1.27",
          "form": "클라스가 남다르네요",
          "original_form": "클라스가 남다르네요",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018382.1.28",
          "form": "냉동 게 있어서 ㅎㅅㅎ",
          "original_form": "냉동 게 있어서 ㅎㅅㅎ",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018382.1.29",
          "form": "근데 라면 맛이 워낙 세서 별 차이는 모르겠더라",
          "original_form": "근데 라면 맛이 워낙 세서 별 차이는 모르겠더라",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100018382.1.30",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018382.1.31",
          "form": "국물에 진하게 올라오는 게 맛이 최고인데",
          "original_form": "국물에 진하게 올라오는 게 맛이 최고인데",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018382.1.32",
          "form": "아쉽네",
          "original_form": "아쉽네",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018382.1.33",
          "form": "그리고 물을 작게 넣어서 국물이 거의 없었음",
          "original_form": "그리고 물을 작게 넣어서 국물이 거의 없었음",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018382.1.34",
          "form": "아까워",
          "original_form": "아까워",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018382.1.35",
          "form": "거의 비빔식이었네",
          "original_form": "거의 비빔식이었네",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100018382.1.36",
          "form": "그것도 나쁘진 않지",
          "original_form": "그것도 나쁘진 않지",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100018382.1.37",
          "form": "자작하게 국물있는것도 좋지",
          "original_form": "자작하게 국물있는것도 좋지",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100018382.1.38",
          "form": "저녁 뭐먹지",
          "original_form": "저녁 뭐먹지",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100018382.1.39",
          "form": "맛있는걸로먹어",
          "original_form": "맛있는걸로먹어",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100018382.1.40",
          "form": "나는 이제 저녁 먹는 즁",
          "original_form": "나는 이제 저녁 먹는 즁",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100018382.1.41",
          "form": "김밥 먹었음",
          "original_form": "김밥 먹었음",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100018382.1.42",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100018382.1.43",
          "form": "오 김밥 좋지",
          "original_form": "오 김밥 좋지",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100018382.1.44",
          "form": "김밥에 라면 최고시다",
          "original_form": "김밥에 라면 최고시다",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100018382.1.45",
          "form": "그니까 김밥에는 라면이어야 하는데 라면은 안먹었네",
          "original_form": "그니까 김밥에는 라면이어야 하는데 라면은 안먹었네",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100018382.1.46",
          "form": "왜죠 8ㅁ8 아쉽쟈나",
          "original_form": "왜죠 8ㅁ8 아쉽쟈나",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100018382.1.47",
          "form": "글게용",
          "original_form": "글게용",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100018382.1.48",
