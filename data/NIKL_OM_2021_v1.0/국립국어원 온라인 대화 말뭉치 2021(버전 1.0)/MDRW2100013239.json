      "utterance": [
        {
          "id": "MDRW2100013239.1.1",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013239.1.2",
          "form": "네! 근데 너무 별로였어요..ㅠㅠ",
          "original_form": "네! 근데 너무 별로였어요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013239.1.3",
          "form": "회사 사내식당에서 인도커리 나온다고 해서 기대했는데... 그냥 카레더라구요...ㅠ",
          "original_form": "회사 사내식당에서 인도커리 나온다고 해서 기대했는데... 그냥 카레더라구요...ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013239.1.4",
          "form": "앗...커리랑 카레는 다른것인데!",
          "original_form": "앗...커리랑 카레는 다른것인데!",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013239.1.5",
          "form": "실망하셨겠다ㅠㅠ",
          "original_form": "실망하셨겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013239.1.6",
          "form": "그러니까요!ㅠㅠㅠㅠ 회사식당이 너무했어요...",
          "original_form": "그러니까요!ㅠㅠㅠㅠ 회사식당이 너무했어요...",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013239.1.7",
          "form": "별님은 점심 드셨나요?",
          "original_form": "별님은 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013239.1.8",
          "form": "네 저는 짜장밥 먹었어요",
          "original_form": "네 저는 짜장밥 먹었어요",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013239.1.9",
          "form": "짜장밥 맛있죠...",
          "original_form": "짜장밥 맛있죠...",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013239.1.10",
          "form": "짜장 카레 뭘더 좋아하시나요?",
          "original_form": "짜장 카레 뭘더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013239.1.11",
          "form": "짜장!이 더 좋은데...",
          "original_form": "짜장!이 더 좋은데...",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013239.1.12",
          "form": "인도커리와 짜장이라면 인도커리요...ㅎㅎ",
          "original_form": "인도커리와 짜장이라면 인도커리요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013239.1.13",
          "form": "향신료 그득한 그맛이 너무 좋아요",
          "original_form": "향신료 그득한 그맛이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013239.1.14",
          "form": "저도 커리좋아해요! 일본카레도 좋아하구요",
          "original_form": "저도 커리좋아해요! 일본카레도 좋아하구요",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100013239.1.15",
          "form": "향신료가득한 맛도 좋고 난까지있으면 최고죠",
          "original_form": "향신료가득한 맛도 좋고 난까지있으면 최고죠",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013239.1.16",
          "form": "맞아요 일본카레도 괜찮죠",
          "original_form": "맞아요 일본카레도 괜찮죠",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013239.1.17",
          "form": "난... 진짜 너무 좋죠",
          "original_form": "난... 진짜 너무 좋죠",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013239.1.18",
          "form": "저는 그 인도식 특유 긴 쌀밥도 좋아해요",
          "original_form": "저는 그 인도식 특유 긴 쌀밥도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013239.1.19",
          "form": "커리랑 너무 잘어울려서...",
          "original_form": "커리랑 너무 잘어울려서...",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013239.1.20",
          "form": "흩날리는 그 쌀 말하시는거죠",
          "original_form": "흩날리는 그 쌀 말하시는거죠",
          "speaker_id": "1",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013239.1.21",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013239.1.22",
          "form": "저 원래도 진밥싫어해서 그쌀 너무 좋아요",
          "original_form": "저 원래도 진밥싫어해서 그쌀 너무 좋아요",
          "speaker_id": "1",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013239.1.23",
          "form": "커리랑 너무 잘어울려요...",
          "original_form": "커리랑 너무 잘어울려요...",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013239.1.24",
          "form": "그리고 전 시금치 커리랑 치킨커리가 젤 좋더라구요",
          "original_form": "그리고 전 시금치 커리랑 치킨커리가 젤 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013239.1.25",
          "form": "커리먹고싶네요...",
          "original_form": "커리먹고싶네요...",
          "speaker_id": "2",
          "time": "20210825 13:50"
        }
      ]
    }
