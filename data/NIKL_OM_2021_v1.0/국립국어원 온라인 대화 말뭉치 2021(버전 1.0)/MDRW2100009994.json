      "utterance": [
        {
          "id": "MDRW2100009994.1.1",
          "form": "혹시 픽사 애니메이션도 좋아하시나요?",
          "original_form": "혹시 픽사 애니메이션도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100009994.1.2",
          "form": "픽사면 음 up? ㅋ",
          "original_form": "픽사면 음 up? ㅋ",
          "speaker_id": "2",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100009994.1.3",
          "form": "아 그 할아버지랑 꼬마애랑 아옹다옹하는 내용이죠 ㅋㅋㅋㅋ",
          "original_form": "아 그 할아버지랑 꼬마애랑 아옹다옹하는 내용이죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100009994.1.4",
          "form": "맞아요 그거말고",
          "original_form": "맞아요 그거말고",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100009994.1.5",
          "form": "주토피아는 어디꺼였더라",
          "original_form": "주토피아는 어디꺼였더라",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100009994.1.6",
          "form": "디즈니꺼에요!",
          "original_form": "디즈니꺼에요!",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100009994.1.7",
          "form": "좋아하시나요 ㅋㅋㅋ",
          "original_form": "좋아하시나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100009994.1.8",
          "form": "한때 그 테마곡 하루종일 듣고했었는데 ㅋㅋ추억이네요 그새",
          "original_form": "한때 그 테마곡 하루종일 듣고했었는데 ㅋㅋ추억이네요 그새",
          "speaker_id": "2",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100009994.1.9",
          "form": "아 그 무슨",
          "original_form": "아 그 무슨",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100009994.1.10",
          "form": "무슨동물이죠 ㅋㅋㅋㅋ",
          "original_form": "무슨동물이죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100009994.1.11",
          "form": "달리기 잘하는 동물이 노래불렀는데",
          "original_form": "달리기 잘하는 동물이 노래불렀는데",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100009994.1.12",
          "form": "치타인가",
          "original_form": "치타인가",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100009994.1.13",
          "form": "아니다 뿔달린",
          "original_form": "아니다 뿔달린",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100009994.1.14",
          "form": "그 주제곡",
          "original_form": "그 주제곡",
          "speaker_id": "1",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100009994.1.15",
          "form": "산양인가 아 임팔라 ㅋㅋ",
          "original_form": "산양인가 아 임팔라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100009994.1.16",
          "form": "왜 다 기억이 흐릿한짘ㅋㅋ",
          "original_form": "왜 다 기억이 흐릿한짘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100009994.1.17",
          "form": "ㅇㅏ 맞아요!!!",
          "original_form": "ㅇㅏ 맞아요!!!",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100009994.1.18",
          "form": "백댄서들이 호랑이인가 치타였어요",
          "original_form": "백댄서들이 호랑이인가 치타였어요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100009994.1.19",
          "form": "임팔라라서 기억이 안났구낰ㅋㅋㅋㅋㅋ",
          "original_form": "임팔라라서 기억이 안났구낰ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100009994.1.20",
          "form": "아 백댄서가 육식동물이엇군요 그것도 재미잇네요",
          "original_form": "아 백댄서가 육식동물이엇군요 그것도 재미잇네요",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100009994.1.21",
          "form": "새록새록 생각나는 영화많아서 좋네요",
          "original_form": "새록새록 생각나는 영화많아서 좋네요",
          "speaker_id": "2",
          "time": "20210823 21:51"
        }
      ]
