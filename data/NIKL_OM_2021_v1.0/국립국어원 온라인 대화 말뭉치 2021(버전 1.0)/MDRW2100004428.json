      "utterance": [
        {
          "id": "MDRW2100004428.1.1",
          "form": "요즘 구매하고싶은거 있으세요?!",
          "original_form": "요즘 구매하고싶은거 있으세요?!",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004428.1.2",
          "form": "접이식우산이요!",
          "original_form": "접이식우산이요!",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004428.1.3",
          "form": "너무 실용적인가요",
          "original_form": "너무 실용적인가요",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004428.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004428.1.5",
          "form": "원래 있던게 부러져서",
          "original_form": "원래 있던게 부러져서",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004428.1.6",
          "form": "구매하고싶어요",
          "original_form": "구매하고싶어요",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004428.1.7",
          "form": "앜ㅋㅋㅋㅋㅋㅋ실용적인게 좋져",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ실용적인게 좋져",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004428.1.8",
          "form": "name1님은 요즘 어떤 상품 구매하고싶으신가여?",
          "original_form": "&name1&님은 요즘 어떤 상품 구매하고싶으신가여?",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004428.1.9",
          "form": "버즈2요!",
          "original_form": "버즈2요!",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004428.1.10",
          "form": "넘 귀엽더라구요 색이",
          "original_form": "넘 귀엽더라구요 색이",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004428.1.11",
          "form": "오오 한번 봐야겠어요",
          "original_form": "오오 한번 봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004428.1.12",
          "form": "아쥬 귀여워요 오늘 사려고용",
          "original_form": "아쥬 귀여워요 오늘 사려고용",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004428.1.13",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004428.1.14",
          "form": "버즈는 할인 자주 하나요?",
          "original_form": "버즈는 할인 자주 하나요?",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004428.1.15",
          "form": "언더아머는 비올때마다 30퍼센트 할인해준대요",
          "original_form": "언더아머는 비올때마다 30퍼센트 할인해준대요",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004428.1.16",
          "form": "다 할인해주는건 아니구 강남역에 있는 매장이랑 코엑스?정도 해준대요",
          "original_form": "다 할인해주는건 아니구 강남역에 있는 매장이랑 코엑스?정도 해준대요",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004428.1.17",
          "form": "근데 또 비 그치면 안해주나보더라구요ㅋㅋㅋ",
          "original_form": "근데 또 비 그치면 안해주나보더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004428.1.18",
          "form": "김연경 선수가 언더아머 모델이라서 그거 후드티 살까 정보 찾다 알게됐어요",
          "original_form": "김연경 선수가 언더아머 모델이라서 그거 후드티 살까 정보 찾다 알게됐어요",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004428.1.19",
          "form": "헉 비랑 무슨 상관관계가 있는가요?",
          "original_form": "헉 비랑 무슨 상관관계가 있는가요?",
          "speaker_id": "1",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004428.1.20",
          "form": "오 김연경 선수 좋아하세욥?",
          "original_form": "오 김연경 선수 좋아하세욥?",
          "speaker_id": "1",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004428.1.21",
          "form": "그냥 비오면 손님이 별로 안오니까 할인해주는거같아요",
          "original_form": "그냥 비오면 손님이 별로 안오니까 할인해주는거같아요",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004428.1.22",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004428.1.23",
          "form": "여자배구도 좋아해요",
          "original_form": "여자배구도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004428.1.24",
          "form": "얼마전에 국대티셔츠티 샀어요",
          "original_form": "얼마전에 국대티셔츠티 샀어요",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004428.1.25",
          "form": "오 완전 진심이시규나",
          "original_form": "오 완전 진심이시규나",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004428.1.26",
          "form": "여자배구 짱...",
          "original_form": "여자배구 짱...",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004428.1.27",
          "form": "하나에 7만원.. ㅎㄷㄷ하지만 연경선수 마지막이니까.. 이러면서 구매했던",
          "original_form": "하나에 7만원.. ㅎㄷㄷ하지만 연경선수 마지막이니까.. 이러면서 구매했던",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004428.1.28",
          "form": "덕질하면 구매하고 싶은 물건들이 생기는거같앙ㅅ",
          "original_form": "덕질하면 구매하고 싶은 물건들이 생기는거같앙ㅅ",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004428.1.29",
          "form": "와 7만원...진짜 비싸네요",
          "original_form": "와 7만원...진짜 비싸네요",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004428.1.30",
          "form": "사실 필요없는데 사고싶은",
          "original_form": "사실 필요없는데 사고싶은",
          "speaker_id": "2",
          "time": "20210821 16:35"
        }
      ]
