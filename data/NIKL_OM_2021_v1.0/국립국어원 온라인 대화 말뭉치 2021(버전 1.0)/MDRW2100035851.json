      "utterance": [
        {
          "id": "MDRW2100035851.1.1",
          "form": "요기요 포장할인할때",
          "original_form": "요기요 포장할인할때",
          "speaker_id": "1",
          "time": "20210830 09:10"
        },
        {
          "id": "MDRW2100035851.1.2",
          "form": "자주 포장해먹었어여",
          "original_form": "자주 포장해먹었어여",
          "speaker_id": "1",
          "time": "20210830 09:10"
        },
        {
          "id": "MDRW2100035851.1.3",
          "form": "앞으로는",
          "original_form": "앞으로는",
          "speaker_id": "2",
          "time": "20210830 09:10"
        },
        {
          "id": "MDRW2100035851.1.4",
          "form": "황올 포장만 먹겠습니다 충성",
          "original_form": "황올 포장만 먹겠습니다 충성",
          "speaker_id": "2",
          "time": "20210830 09:10"
        },
        {
          "id": "MDRW2100035851.1.5",
          "form": "ㅋㅋㅋㅋ저도 황올이 진리",
          "original_form": "ㅋㅋㅋㅋ저도 황올이 진리",
          "speaker_id": "1",
          "time": "20210830 09:10"
        },
        {
          "id": "MDRW2100035851.1.6",
          "form": "그거 요즘 인스타에서 핫하던데",
          "original_form": "그거 요즘 인스타에서 핫하던데",
          "speaker_id": "1",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035851.1.7",
          "form": "그릭 복숭아 드셔보셨어요?",
          "original_form": "그릭 복숭아 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035851.1.8",
          "form": "아뇨ㅠㅠ",
          "original_form": "아뇨ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035851.1.9",
          "form": "먹어보고는 싶은데",
          "original_form": "먹어보고는 싶은데",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035851.1.10",
          "form": "귀찮음이 더 커서..",
          "original_form": "귀찮음이 더 커서..",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035851.1.11",
          "form": "이제 복숭아 거의 끝물이래서",
          "original_form": "이제 복숭아 거의 끝물이래서",
          "speaker_id": "1",
          "time": "20210830 09:12"
        },
        {
          "id": "MDRW2100035851.1.12",
          "form": "꼭 드셔보세용ㅠㅠ",
          "original_form": "꼭 드셔보세용ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:12"
        },
        {
          "id": "MDRW2100035851.1.13",
          "form": "복숭아가 사라지기전에...ㅠㅠ",
          "original_form": "복숭아가 사라지기전에...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:12"
        },
        {
          "id": "MDRW2100035851.1.14",
          "form": "요즘 복숭아",
          "original_form": "요즘 복숭아",
          "speaker_id": "2",
          "time": "20210830 09:15"
        },
        {
          "id": "MDRW2100035851.1.15",
          "form": "1식1개는 하는거같아요",
          "original_form": "1식1개는 하는거같아요",
          "speaker_id": "2",
          "time": "20210830 09:15"
        },
        {
          "id": "MDRW2100035851.1.16",
          "form": "진짜 너무 달고...",
          "original_form": "진짜 너무 달고...",
          "speaker_id": "2",
          "time": "20210830 09:15"
        },
        {
          "id": "MDRW2100035851.1.17",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.18",
          "form": "너무 맛있어요",
          "original_form": "너무 맛있어요",
          "speaker_id": "2",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.19",
          "form": "물복파세요",
          "original_form": "물복파세요",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.20",
          "form": "딱복파세요?",
          "original_form": "딱복파세요?",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.21",
          "form": "물복파지만",
          "original_form": "물복파지만",
          "speaker_id": "2",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.22",
          "form": "딱복이어도 입에 넣습니다",
          "original_form": "딱복이어도 입에 넣습니다",
          "speaker_id": "2",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.23",
          "form": "안가려요",
          "original_form": "안가려요",
          "speaker_id": "2",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.24",
          "form": "앗ㅋㅋㅋㅋㅋ 저도 똑~같아요ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ 저도 똑~같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.25",
          "form": "복숭아는 그저 맛있어요",
          "original_form": "복숭아는 그저 맛있어요",
          "speaker_id": "1",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100035851.1.26",
          "form": "그릭복숭아 맛있었나요!",
          "original_form": "그릭복숭아 맛있었나요!",
          "speaker_id": "2",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100035851.1.27",
          "form": "그릭요거트랑 꿀 집어넣고 먹으니",
          "original_form": "그릭요거트랑 꿀 집어넣고 먹으니",
          "speaker_id": "1",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100035851.1.28",
          "form": "완전 맛있었어요",
          "original_form": "완전 맛있었어요",
          "speaker_id": "1",
          "time": "20210830 09:23"
        }
      ]
    }
  ]
