      "utterance": [
        {
          "id": "MDRW2100023578.1.1",
          "form": "고수랑 시소 둘다?",
          "original_form": "고수랑 시소 둘다?",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100023578.1.2",
          "form": "응ㅋㅋ",
          "original_form": "응ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100023578.1.3",
          "form": "흰살생선에 싸먹음 향긋해",
          "original_form": "흰살생선에 싸먹음 향긋해",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100023578.1.4",
          "form": "외국인들은 깻잎 못먹는다더라?",
          "original_form": "외국인들은 깻잎 못먹는다더라?",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100023578.1.5",
          "form": "향긋까지?ㅋㅋ",
          "original_form": "향긋까지?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100023578.1.6",
          "form": "그니까ㅋㅋ 외국인들은 깻잎이 고수 같이 느껴지믄건가?",
          "original_form": "그니까ㅋㅋ 외국인들은 깻잎이 고수 같이 느껴지믄건가?",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100023578.1.7",
          "form": "그런가봐ㅋㅋ",
          "original_form": "그런가봐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100023578.1.8",
          "form": "우리나라사람 유전자가 고수가 화장품맛나게 느껴지는것처럼 외국인 유전자는 깻잎이 그런건지ㅋㅋ",
          "original_form": "우리나라사람 유전자가 고수가 화장품맛나게 느껴지는것처럼 외국인 유전자는 깻잎이 그런건지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100023578.1.9",
          "form": "나도 가끔 깻잎 센거 같긴해",
          "original_form": "나도 가끔 깻잎 센거 같긴해",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100023578.1.10",
          "form": "그니까 신기하다",
          "original_form": "그니까 신기하다",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100023578.1.11",
          "form": "점심 뭐먹었어? 구내식당이랬나?",
          "original_form": "점심 뭐먹었어? 구내식당이랬나?",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100023578.1.12",
          "form": "구내식당은 아니구\n근처 식당에서 배달해서 먹는거지",
          "original_form": "구내식당은 아니구\n근처 식당에서 배달해서 먹는거지",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100023578.1.13",
          "form": "아항 뭐먹었어?",
          "original_form": "아항 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100023578.1.14",
          "form": "오늘은 제육볶음이랑 계란장조림이랑 오징어튀김, 콩나물무침, 미역국 나왔어",
          "original_form": "오늘은 제육볶음이랑 계란장조림이랑 오징어튀김, 콩나물무침, 미역국 나왔어",
          "speaker_id": "2",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023578.1.15",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023578.1.16",
          "form": "장조림 맛있었겠다\n난 마파두부밥! 화자오 들어가서 살짝 얼얼했어",
          "original_form": "장조림 맛있었겠다\n난 마파두부밥! 화자오 들어가서 살짝 얼얼했어",
          "speaker_id": "1",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023578.1.17",
          "form": "오 마파두부밥!",
          "original_form": "오 마파두부밥!",
          "speaker_id": "2",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100023578.1.18",
          "form": "화자오가 그건가?",
          "original_form": "화자오가 그건가?",
          "speaker_id": "2",
          "time": "20210830 13:27"
        }
      ]
