      "utterance": [
        {
          "id": "MDRW2100042303.1.1",
          "form": "ㅋㅋㅋ그래도 뭔가 퓨전 치킨 피자는 아무도안먹어욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그래도 뭔가 퓨전 치킨 피자는 아무도안먹어욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042303.1.2",
          "form": "그게 핵심인데!ㅋㅋㅋㅋ",
          "original_form": "그게 핵심인데!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042303.1.3",
          "form": "ㅋㅋㅋ 진짜 맛있어서 추천합니다...",
          "original_form": "ㅋㅋㅋ 진짜 맛있어서 추천합니다...",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042303.1.4",
          "form": "정말 내년에 ㅎㅎㅎㅎㅎ",
          "original_form": "정말 내년에 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042303.1.5",
          "form": "한번 먹어봅니다",
          "original_form": "한번 먹어봅니다",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042303.1.6",
          "form": "넹 기억나신다면 꼭!!",
          "original_form": "넹 기억나신다면 꼭!!",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042303.1.7",
          "form": "ㅋㅋㅋ 근데 올해 체리는 다 실패였어요",
          "original_form": "ㅋㅋㅋ 근데 올해 체리는 다 실패였어요",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042303.1.8",
          "form": "쓱 마켓에서 시켰던것도 별로더라고요",
          "original_form": "쓱 마켓에서 시켰던것도 별로더라고요",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042303.1.9",
          "form": "아앗... 체리는 파는것도 못봤네요",
          "original_form": "아앗... 체리는 파는것도 못봤네요",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042303.1.10",
          "form": "저도 체리환장하는데",
          "original_form": "저도 체리환장하는데",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042303.1.11",
          "form": "올해 별로라 안드시는게 나으셨어요",
          "original_form": "올해 별로라 안드시는게 나으셨어요",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042303.1.12",
          "form": "좀 비싸게 주고 사도 그저 그렇더라고요",
          "original_form": "좀 비싸게 주고 사도 그저 그렇더라고요",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042303.1.13",
          "form": "진짜외국나가서 밥먹듯이 체리한번 먹어보고싶네요",
          "original_form": "진짜외국나가서 밥먹듯이 체리한번 먹어보고싶네요",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042303.1.14",
          "form": "체리는 질리게 먹어보고 싶어요",
          "original_form": "체리는 질리게 먹어보고 싶어요",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042303.1.15",
          "form": "아직 그정도로는 안사먹어본거 같으니까여ㅋㅋㅋ",
          "original_form": "아직 그정도로는 안사먹어본거 같으니까여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042303.1.16",
          "form": "저도...",
          "original_form": "저도...",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.17",
          "form": "너무비싸서",
          "original_form": "너무비싸서",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.18",
          "form": "불가능한거같아요",
          "original_form": "불가능한거같아요",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.19",
          "form": "많이사도 뭐....",
          "original_form": "많이사도 뭐....",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.20",
          "form": "ㅋㅋㅋ 넹 진짜 수입해오지 않는이상",
          "original_form": "ㅋㅋㅋ 넹 진짜 수입해오지 않는이상",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.21",
          "form": "한 3마넌치사도 1.5키로?",
          "original_form": "한 3마넌치사도 1.5키로?",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.22",
          "form": "성에찰려면....5키로는 사야하는데",
          "original_form": "성에찰려면....5키로는 사야하는데",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.23",
          "form": "그러니까여",
          "original_form": "그러니까여",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042303.1.24",
          "form": "레이메어 였나 그런 체리가 있다고 해서 먹어봤는데",
          "original_form": "레이메어 였나 그런 체리가 있다고 해서 먹어봤는데",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042303.1.25",
          "form": "걔는 좀 생김새가 다르고 달달한 체리더라고요",
          "original_form": "걔는 좀 생김새가 다르고 달달한 체리더라고요",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042303.1.26",
          "form": "검색해봅니닼ㅋㅋㅋ",
          "original_form": "검색해봅니닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042303.1.27",
          "form": "이름이 아닌거 같아요",
          "original_form": "이름이 아닌거 같아요",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042303.1.28",
          "form": "ㅋㅋㅋ 저런 비슷한 이름인데",
          "original_form": "ㅋㅋㅋ 저런 비슷한 이름인데",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042303.1.29",
          "form": "넼ㅋㅋㅋㅋ 애기들 보는 애니가 나오네요 ㅎㅎ",
