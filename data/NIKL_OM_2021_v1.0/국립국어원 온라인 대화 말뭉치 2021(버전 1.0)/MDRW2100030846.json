        },
        {
          "id": "MDRW2100030846.1.2",
          "form": "name2님은 10월",
          "original_form": "&name2&님은 10월",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030846.1.3",
          "form": "정말 가을 중의 가을",
          "original_form": "정말 가을 중의 가을",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030846.1.4",
          "form": "에 태어나셨네요!!",
          "original_form": "에 태어나셨네요!!",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030846.1.5",
          "form": "저는 언제 태어났을까요 ",
          "original_form": "저는 언제 태어났을까요 ",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030846.1.6",
          "form": "ㅋㅋㅋㅋㅋ아아아",
          "original_form": "ㅋㅋㅋㅋㅋ아아아",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.7",
          "form": "6월!",
          "original_form": "6월!",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.8",
          "form": "오오 근접해요",
          "original_form": "오오 근접해요",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.9",
          "form": "7월....?",
          "original_form": "7월....?",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.10",
          "form": "5월입니다~!~@",
          "original_form": "5월입니다~!~@",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.11",
          "form": "여름여름",
          "original_form": "여름여름",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.12",
          "form": "요ㅓㄱ헉아니",
          "original_form": "요ㅓㄱ헉아니",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.13",
          "form": "근데 6월에 가까운 5월이에요!",
          "original_form": "근데 6월에 가까운 5월이에요!",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.14",
          "form": "저 원래",
          "original_form": "저 원래",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.15",
          "form": "출생예정일이 6월 4일이긴 했어요!",
          "original_form": "출생예정일이 6월 4일이긴 했어요!",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.16",
          "form": "저희가 안지 그렇게 됏나요ㅎㅋㅋㅋㅋ",
          "original_form": "저희가 안지 그렇게 됏나요ㅎㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.17",
          "form": "근데 좀 일찍 나왔대요~~~~ 맴이 급해서~~~",
          "original_form": "근데 좀 일찍 나왔대요~~~~ 맴이 급해서~~~",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.18",
          "form": "앗 왜요왜요?!",
          "original_form": "앗 왜요왜요?!",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.19",
          "form": "분명 블로그에서 생일글보고",
          "original_form": "분명 블로그에서 생일글보고",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030846.1.20",
          "form": "축하드린다고 디엠보냇는데",
          "original_form": "축하드린다고 디엠보냇는데",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.21",
          "form": "아!!!",
          "original_form": "아!!!",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.22",
          "form": "ㅋㅋㅋㅋㅋ그게벌써",
          "original_form": "ㅋㅋㅋㅋㅋ그게벌써",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.24",
          "form": "몇달전이내욬ㅋㅋㅋㅋ",
          "original_form": "몇달전이내욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.25",
          "form": "그쵸 벌써",
          "original_form": "그쵸 벌써",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.26",
          "form": "그러니까요,, 시간이 참 빨라요,,,",
          "original_form": "그러니까요,, 시간이 참 빨라요,,,",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.27",
          "form": "제 생일 즈음에는",
          "original_form": "제 생일 즈음에는",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.28",
          "form": "길가에 장미가 많이 펴서 좋답니다",
          "original_form": "길가에 장미가 많이 펴서 좋답니다",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.29",
          "form": "name2님 생일 즈음에는 단풍이 예쁘겠어요",
          "original_form": "&name2&님 생일 즈음에는 단풍이 예쁘겠어요",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030846.1.30",
          "form": "맞아요 여름장미가 참 예쁘더라구요",
          "original_form": "맞아요 여름장미가 참 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030846.1.31",
          "form": "길거리만 걸어도 장미가 탐스럽",
          "original_form": "길거리만 걸어도 장미가 탐스럽",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030846.1.32",
          "form": "제 생일은...점점 겨울이 되어가는 중임니다..",
          "original_form": "제 생일은...점점 겨울이 되어가는 중임니다..",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030846.1.33",
          "form": "가을 뭐 .. 한 열흘 되나..",
          "original_form": "가을 뭐 .. 한 열흘 되나..",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030846.1.34",
          "form": "앗,,, 기후변화로",
          "original_form": "앗,,, 기후변화로",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030846.1.35",
          "form": "인해서요?!",
          "original_form": "인해서요?!",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030846.1.36",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
