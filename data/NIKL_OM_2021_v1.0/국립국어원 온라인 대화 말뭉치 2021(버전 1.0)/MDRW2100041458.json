        },
        {
          "id": "MDRW2100041458.1.2",
          "form": "이번 주 놀라운 토요일 봤니",
          "original_form": "이번 주 놀라운 토요일 봤니",
          "speaker_id": "1",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100041458.1.3",
          "form": "최유정 나온다고 해서 봤어",
          "original_form": "최유정 나온다고 해서 봤어",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100041458.1.4",
          "form": "장영란 도 나오는데",
          "original_form": "장영란 도 나오는데",
          "speaker_id": "1",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100041458.1.5",
          "form": "아 맞어 장영란진짜 웃기더라",
          "original_form": "아 맞어 장영란진짜 웃기더라",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100041458.1.6",
          "form": "어제 간식 게임 재밌더라",
          "original_form": "어제 간식 게임 재밌더라",
          "speaker_id": "1",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100041458.1.7",
          "form": "옛날씨에프 맞추기였어 맞어",
          "original_form": "옛날씨에프 맞추기였어 맞어",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100041458.1.8",
          "form": "누네띠네 스콘 맛있겠더라",
          "original_form": "누네띠네 스콘 맛있겠더라",
          "speaker_id": "1",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100041458.1.9",
          "form": "나 그런 스콘 처음 봐",
          "original_form": "나 그런 스콘 처음 봐",
          "speaker_id": "2",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100041458.1.10",
          "form": "그니까 말이야 안 그래도 누 네 띠네도 좋아하는데",
          "original_form": "그니까 말이야 안 그래도 누 네 띠네도 좋아하는데",
          "speaker_id": "1",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100041458.1.11",
          "form": "어제 피오 가 엄청 활약하더라",
          "original_form": "어제 피오 가 엄청 활약하더라",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100041458.1.12",
          "form": "진짜 요즘 피오가 일등 할 때 많은 들",
          "original_form": "진짜 요즘 피오가 일등 할 때 많은 들",
          "speaker_id": "1",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100041458.1.13",
          "form": "대탈출에서도 요즘 피오 가 열심히 하던데",
          "original_form": "대탈출에서도 요즘 피오 가 열심히 하던데",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100041458.1.14",
          "form": "오 피오 호감이다",
          "original_form": "오 피오 호감이다",
          "speaker_id": "1",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100041458.1.15",
          "form": "다음 주는 놀토 게스트 누구래",
          "original_form": "다음 주는 놀토 게스트 누구래",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100041458.1.16",
          "form": "다음 주에 개코 나온대",
          "original_form": "다음 주에 개코 나온대",
          "speaker_id": "1",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100041458.1.17",
          "form": "놀토는 간식 게임만 재밌더라",
          "original_form": "놀토는 간식 게임만 재밌더라",
          "speaker_id": "2",
          "time": "20210906 00:32"
        },
        {
          "id": "MDRW2100041458.1.18",
          "form": "맞아 집중이 잘 안 돼",
