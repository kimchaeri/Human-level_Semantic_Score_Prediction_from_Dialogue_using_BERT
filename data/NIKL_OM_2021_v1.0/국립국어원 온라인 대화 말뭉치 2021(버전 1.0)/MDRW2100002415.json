      "utterance": [
        {
          "id": "MDRW2100002415.1.1",
          "form": "너 방정리 어떻게 하는 편이야?",
          "original_form": "너 방정리 어떻게 하는 편이야?",
          "speaker_id": "2",
          "time": "20210820 12:00"
        },
        {
          "id": "MDRW2100002415.1.2",
          "form": "ㅋㅋㅋ 방정리 진짜 안돼",
          "original_form": "ㅋㅋㅋ 방정리 진짜 안돼",
          "speaker_id": "1",
          "time": "20210820 12:00"
        },
        {
          "id": "MDRW2100002415.1.3",
          "form": "1주일에 한번 정도 쌓인 짐을 치우는 편이야",
          "original_form": "1주일에 한번 정도 쌓인 짐을 치우는 편이야",
          "speaker_id": "1",
          "time": "20210820 12:01"
        },
        {
          "id": "MDRW2100002415.1.4",
          "form": "너도 잘 안하는 편이구나? 나도 맨날 엄마가 방에 들어와서 뭐라고 하시고 가던데",
          "original_form": "너도 잘 안하는 편이구나? 나도 맨날 엄마가 방에 들어와서 뭐라고 하시고 가던데",
          "speaker_id": "2",
          "time": "20210820 12:01"
        },
        {
          "id": "MDRW2100002415.1.5",
          "form": "짐이 너무 많아서 방정리가 안돼",
          "original_form": "짐이 너무 많아서 방정리가 안돼",
          "speaker_id": "1",
          "time": "20210820 12:01"
        },
        {
          "id": "MDRW2100002415.1.6",
          "form": "대체 뭐가 있길래 정리를 못하는거야ㅋㅋ",
          "original_form": "대체 뭐가 있길래 정리를 못하는거야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100002415.1.7",
          "form": "문제집도 엄청 많고",
          "original_form": "문제집도 엄청 많고",
          "speaker_id": "1",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100002415.1.8",
          "form": "전자기기를 좋아해서 전자기기가 많아",
          "original_form": "전자기기를 좋아해서 전자기기가 많아",
          "speaker_id": "1",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100002415.1.9",
          "form": "너는 왜 정리가 안돼?",
          "original_form": "너는 왜 정리가 안돼?",
          "speaker_id": "1",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100002415.1.10",
          "form": "난 방에 컴퓨터 관련 부품이 좀 있어서 그런것 같아",
          "original_form": "난 방에 컴퓨터 관련 부품이 좀 있어서 그런것 같아",
          "speaker_id": "2",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100002415.1.11",
          "form": "하긴 부품이 많으면 좀 어지럽지",
          "original_form": "하긴 부품이 많으면 좀 어지럽지",
          "speaker_id": "1",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100002415.1.12",
          "form": "한번은 나사 밟은 적도 있다니까",
          "original_form": "한번은 나사 밟은 적도 있다니까",
          "speaker_id": "2",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100002415.1.13",
          "form": "으악 그건 너무 발 아플거 같아",
          "original_form": "으악 그건 너무 발 아플거 같아",
          "speaker_id": "1",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100002415.1.14",
          "form": "방 잘 치워야겠네 ㅜㅜ",
          "original_form": "방 잘 치워야겠네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 12:04"
        },
        {
          "id": "MDRW2100002415.1.15",
          "form": "엄마방에가서 연고 찾으면서 말하니까 정리 좀하고 살라고 하시더라",
          "original_form": "엄마방에가서 연고 찾으면서 말하니까 정리 좀하고 살라고 하시더라",
          "speaker_id": "2",
          "time": "20210820 12:04"
        },
        {
          "id": "MDRW2100002415.1.16",
          "form": "ㅋㅋ 그래 조심하자 방 잘 치우고",
          "original_form": "ㅋㅋ 그래 조심하자 방 잘 치우고",
          "speaker_id": "1",
          "time": "20210820 12:04"
