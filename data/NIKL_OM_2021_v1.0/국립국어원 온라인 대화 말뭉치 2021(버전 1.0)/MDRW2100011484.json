      "utterance": [
        {
          "id": "MDRW2100011484.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100011484.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011484.1.3",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100011484.1.4",
          "form": "예전엔 썩 좋아하지 않았는데 최근엔 좋아하게 됐어요",
          "original_form": "예전엔 썩 좋아하지 않았는데 최근엔 좋아하게 됐어요",
          "speaker_id": "2",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100011484.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011484.1.6",
          "form": "저는 엄청좋아해요ㅠ",
          "original_form": "저는 엄청좋아해요ㅠ",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011484.1.7",
          "form": "많이는못가봤지만",
          "original_form": "많이는못가봤지만",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011484.1.8",
          "form": "와, 혹시 제일 기억에 남는 여행지 있으세요?",
          "original_form": "와, 혹시 제일 기억에 남는 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011484.1.9",
          "form": "해외는 많이못가봤어요ㅠ",
          "original_form": "해외는 많이못가봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011484.1.10",
          "form": "국내는 강릉이 좋더라구요",
          "original_form": "국내는 강릉이 좋더라구요",
          "speaker_id": "1",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011484.1.11",
          "form": "강릉 좋죠~",
          "original_form": "강릉 좋죠~",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011484.1.12",
          "form": "강릉은 아주 어릴 때 가족여행으로 딱 한 번 가본 기억이 있는데, 지금은 그때랑 많이 바뀌었을라나 모르겠네요ㅎㅎ",
          "original_form": "강릉은 아주 어릴 때 가족여행으로 딱 한 번 가본 기억이 있는데, 지금은 그때랑 많이 바뀌었을라나 모르겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011484.1.13",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100011484.1.14",
          "form": "요즘은 사람진짜많대요",
          "original_form": "요즘은 사람진짜많대요",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100011484.1.15",
          "form": "ㅋㅋ이렇게 얘기하니까 강릉 꼭 다시 한 번 가보고 싶어지네요",
          "original_form": "ㅋㅋ이렇게 얘기하니까 강릉 꼭 다시 한 번 가보고 싶어지네요",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100011484.1.16",
          "form": "먹을것도",
          "original_form": "먹을것도",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100011484.1.17",
          "form": "많아요",
          "original_form": "많아요",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100011484.1.18",
          "form": "그거 좋네요, 먹는 것도 중요하죠ㅎㅎ\r\n추천할만한 메뉴 있어요?",
          "original_form": "그거 좋네요, 먹는 것도 중요하죠ㅎㅎ\r\n추천할만한 메뉴 있어요?",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100011484.1.19",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100011484.1.20",
          "form": "강릉은 사실",
          "original_form": "강릉은 사실",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100011484.1.21",
          "form": "초당순두부가 젤 떠오르긴해요ㅋㅋㅋ",
          "original_form": "초당순두부가 젤 떠오르긴해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100011484.1.22",
          "form": "꼬막볶음밥이랑요 엄지네",
          "original_form": "꼬막볶음밥이랑요 엄지네",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100011484.1.23",
          "form": "꼬막 볶음밥은 진짜 꼭 먹어보고 싶네요ㅋㅋ",
          "original_form": "꼬막 볶음밥은 진짜 꼭 먹어보고 싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100011484.1.24",
          "form": "ㅋㅋㅋ저 먹어봤어요",
          "original_form": "ㅋㅋㅋ저 먹어봤어요",
          "speaker_id": "1",
          "time": "20210825 13:49"
        }
      ]
    }
