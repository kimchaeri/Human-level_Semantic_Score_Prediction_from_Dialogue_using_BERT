      "utterance": [
        {
          "id": "MDRW2100040602.1.1",
          "form": "어 저도 걸작떡볶이 좋아해요",
          "original_form": "어 저도 걸작떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040602.1.2",
          "form": "오!! 걸작에 어떤 떡볶이 좋아하시나요?",
          "original_form": "오!! 걸작에 어떤 떡볶이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040602.1.3",
          "form": "저는 기본도 좋아하고 숯불도 좋아해요",
          "original_form": "저는 기본도 좋아하고 숯불도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100040602.1.4",
          "form": "걸작 맛있죠\n거기 로제떡볶이 드셔보셨나요?",
          "original_form": "걸작 맛있죠\n거기 로제떡볶이 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100040602.1.5",
          "form": "아니요 아직이요\n로제 맛있나요?",
          "original_form": "아니요 아직이요\n로제 맛있나요?",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040602.1.6",
          "form": "네 저 거기 로제떡볶이가 제 인생 떡볶이 중 하나예요",
          "original_form": "네 저 거기 로제떡볶이가 제 인생 떡볶이 중 하나예요",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100040602.1.7",
          "form": "걸작 숯불떡볶이 맛있나요?",
          "original_form": "걸작 숯불떡볶이 맛있나요?",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100040602.1.8",
          "form": "네 저는 맛있었어요",
          "original_form": "네 저는 맛있었어요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040602.1.9",
          "form": "정말 고기맛에서 숯불 향이 느껴지시나요?",
          "original_form": "정말 고기맛에서 숯불 향이 느껴지시나요?",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040602.1.10",
          "form": "네 느껴지더라구요",
          "original_form": "네 느껴지더라구요",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040602.1.11",
          "form": "떡볶이 국물에도",
          "original_form": "떡볶이 국물에도",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040602.1.12",
          "form": "잔잔하게 그 숯불향이 배어있는 느낌이라 좋았어요",
          "original_form": "잔잔하게 그 숯불향이 배어있는 느낌이라 좋았어요",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040602.1.13",
          "form": "우와 떡볶이의 숯불향이 들어가면 더 맛있어지나요?",
          "original_form": "우와 떡볶이의 숯불향이 들어가면 더 맛있어지나요?",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040602.1.14",
          "form": "상상이 안가요",
          "original_form": "상상이 안가요",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040602.1.15",
          "form": "숯불향이 들어간다고 더 맛있어지는건지는 잘 모르겠지만 받아서 포장을 여는 순간",
          "original_form": "숯불향이 들어간다고 더 맛있어지는건지는 잘 모르겠지만 받아서 포장을 여는 순간",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040602.1.16",
          "form": "숯불향이 느껴지고 국물 먹을때 느껴지더라구요",
          "original_form": "숯불향이 느껴지고 국물 먹을때 느껴지더라구요",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040602.1.17",
          "form": "오!!꼭 먹어봐야겠네요",
          "original_form": "오!!꼭 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040602.1.18",
          "form": "근데 떡 먹거나 할때는 별로 안 느껴졌어요",
          "original_form": "근데 떡 먹거나 할때는 별로 안 느껴졌어요",
          "speaker_id": "2",
          "time": "20210904 23:20"
        }
      ]
