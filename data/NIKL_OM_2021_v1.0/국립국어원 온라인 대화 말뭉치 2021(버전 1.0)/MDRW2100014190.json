      "utterance": [
        {
          "id": "MDRW2100014190.1.1",
          "form": "먹는거 뭐좋아하세요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹는거 뭐좋아하세요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014190.1.2",
          "form": "저는 삼겹살요ㅋㅋㅋ",
          "original_form": "저는 삼겹살요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014190.1.3",
          "form": "저는 치킨좋아해요ㅎㅎ",
          "original_form": "저는 치킨좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014190.1.4",
          "form": "오 무슨 치킨좋아하세요?ㅋㅋㅋ 전 요새 호랑이치킨맛있던데ㅋㅋㅋ",
          "original_form": "오 무슨 치킨좋아하세요?ㅋㅋㅋ 전 요새 호랑이치킨맛있던데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014190.1.5",
          "form": "저는 뿌링클조아해요ㅋㄱㅋ",
          "original_form": "저는 뿌링클조아해요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014190.1.6",
          "form": "엇 저두요",
          "original_form": "엇 저두요",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014190.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014190.1.8",
          "form": "맛있어요ㅋㅋㅋㅋ",
          "original_form": "맛있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014190.1.9",
          "form": "뿌링클이랑 호랑이치킨 번갈아가면서 시켜요ㅋㅋㅋ",
          "original_form": "뿌링클이랑 호랑이치킨 번갈아가면서 시켜요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014190.1.10",
          "form": "뿌링치즈볼도 맛있어요ㅜㅜ",
          "original_form": "뿌링치즈볼도 맛있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014190.1.11",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014190.1.12",
          "form": "먹고싶다ㅠㅠ",
          "original_form": "먹고싶다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014190.1.13",
          "form": "카페도 가고싶어요ㅠㅠ",
          "original_form": "카페도 가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
