      "utterance": [
        {
          "id": "MDRW2100009077.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009077.1.2",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009077.1.3",
          "form": "저는 아직 안먹었어요",
          "original_form": "저는 아직 안먹었어요",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009077.1.4",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009077.1.5",
          "form": "네 먹었습니다! 집에서 삼겹살 구워먹었네요",
          "original_form": "네 먹었습니다! 집에서 삼겹살 구워먹었네요",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009077.1.6",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009077.1.7",
          "form": "그 말 들으니까 배고파졌어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그 말 들으니까 배고파졌어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009077.1.8",
          "form": "저녁은 뭐드실 예정이세요?",
          "original_form": "저녁은 뭐드실 예정이세요?",
          "speaker_id": "1",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009077.1.9",
          "form": "컵라면? 먹을까 생각 중이에요",
          "original_form": "컵라면? 먹을까 생각 중이에요",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100009077.1.10",
          "form": "오 그것도 가볍고 괜찮을 것 같네요",
          "original_form": "오 그것도 가볍고 괜찮을 것 같네요",
          "speaker_id": "1",
          "time": "20210822 21:07"
        },
        {
          "id": "MDRW2100009077.1.11",
          "form": "그쵸 틈새라면 맛있어요",
          "original_form": "그쵸 틈새라면 맛있어요",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009077.1.12",
          "form": "매운거 좋아하시나봐요!! 그거 엄청 맵지 않나요?",
          "original_form": "매운거 좋아하시나봐요!! 그거 엄청 맵지 않나요?",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009077.1.13",
          "form": "맞아요! 매운 게 땡길 때가 있죠",
          "original_form": "맞아요! 매운 게 땡길 때가 있죠",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009077.1.14",
          "form": "매운거 잘 드시나보다ㅋㅋㅋ저는 신라면 작은컵이 최대라서ㅎㅎ...",
          "original_form": "매운거 잘 드시나보다ㅋㅋㅋ저는 신라면 작은컵이 최대라서ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009077.1.15",
          "form": "아 진짜요? ㅋㅋ 그럼 매운 거 안좋아하세요?",
          "original_form": "아 진짜요? ㅋㅋ 그럼 매운 거 안좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009077.1.16",
          "form": "좋아하는데 위장이 안따라주더라구요ㅠㅠ 맵찔이에요",
          "original_form": "좋아하는데 위장이 안따라주더라구요ㅠㅠ 맵찔이에요",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009077.1.17",
          "form": "아이고 진짜 큰맘 먹고 먹어야겠네요ㅠㅠ",
          "original_form": "아이고 진짜 큰맘 먹고 먹어야겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 21:24"
        },
        {
          "id": "MDRW2100009077.1.18",
          "form": "그래서 틈새라면이나 핵불닭 이런 유명한거는 쳐다보지도 않아요",
          "original_form": "그래서 틈새라면이나 핵불닭 이런 유명한거는 쳐다보지도 않아요",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009077.1.19",
          "form": "ㅋㅋㅋㅋ 저도 핵불닭은 쳐다도 안봐요",
          "original_form": "ㅋㅋㅋㅋ 저도 핵불닭은 쳐다도 안봐요",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009077.1.20",
          "form": "그냥 불닭도 먹기 힘들어서",
          "original_form": "그냥 불닭도 먹기 힘들어서",
          "speaker_id": "2",
          "time": "20210822 21:29"
        }
      ]
