      "utterance": [
        {
          "id": "MDRW2100032711.1.1",
          "form": "셜록 재미있게 보셨어요?",
          "original_form": "셜록 재미있게 보셨어요?",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032711.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032711.1.3",
          "form": "엘리멘트리도요",
          "original_form": "엘리멘트리도요",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032711.1.4",
          "form": "미국판셜록",
          "original_form": "미국판셜록",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032711.1.5",
          "form": "오 저는 그건 못봤는데  그것도 재미있나요?",
          "original_form": "오 저는 그건 못봤는데  그것도 재미있나요?",
          "speaker_id": "2",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032711.1.6",
          "form": "네네 근데 되게 길어서 한번씩 보시면 좋아요",
          "original_form": "네네 근데 되게 길어서 한번씩 보시면 좋아요",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032711.1.7",
          "form": "루시리우가 왓슨역할로 여자 왓슨이요",
          "original_form": "루시리우가 왓슨역할로 여자 왓슨이요",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032711.1.8",
          "form": "오... 한편에 영화같이 길어요?",
          "original_form": "오... 한편에 영화같이 길어요?",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032711.1.9",
          "form": "시즌별로 나옴요~",
          "original_form": "시즌별로 나옴요~",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032711.1.10",
          "form": "한 에피소드 40-45분이었어요",
          "original_form": "한 에피소드 40-45분이었어요",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032711.1.11",
          "form": "넷플릭스인지 왓차인지 좀 헷갈리네요",
          "original_form": "넷플릭스인지 왓차인지 좀 헷갈리네요",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100032711.1.12",
          "form": "룸메가 왓차봐서요",
          "original_form": "룸메가 왓차봐서요",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100032711.1.13",
          "form": "왓챠도 좋은 작품 많죠",
          "original_form": "왓챠도 좋은 작품 많죠",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032711.1.14",
          "form": "왓챠는 아예 안써보셨어요?",
          "original_form": "왓챠는 아예 안써보셨어요?",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032711.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032711.1.16",
          "form": "왓챠에선 퍼슨오브인터레스트",
          "original_form": "왓챠에선 퍼슨오브인터레스트",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032711.1.17",
          "form": "잼있게봤어요",
          "original_form": "잼있게봤어요",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032711.1.18",
          "form": "오 처음 들어봤는데",
          "original_form": "오 처음 들어봤는데",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032711.1.19",
          "form": "나중에 봐야겠네요!",
          "original_form": "나중에 봐야겠네요!",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032711.1.20",
          "form": "님은 한국드라마 좋아하시나요",
          "original_form": "님은 한국드라마 좋아하시나요",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032711.1.21",
          "form": "전 슬의생2 재미있게 보구있어요",
          "original_form": "전 슬의생2 재미있게 보구있어요",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032711.1.22",
          "form": "어 저도요!!",
          "original_form": "어 저도요!!",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032711.1.23",
          "form": "요즘 전개 어때요...? 저는 쪼금 답답하더라고요ㅠ",
          "original_form": "요즘 전개 어때요...? 저는 쪼금 답답하더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032711.1.24",
          "form": "네 슬의생도 역시 시즌1이 좋더라구요",
          "original_form": "네 슬의생도 역시 시즌1이 좋더라구요",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032711.1.25",
          "form": "맞아요 시즌1이 좋았던거 같아요",
          "original_form": "맞아요 시즌1이 좋았던거 같아요",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100032711.1.26",
          "form": "이제 한회 남겨두고 있던가요? 시즌2가",
          "original_form": "이제 한회 남겨두고 있던가요? 시즌2가",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100032711.1.27",
          "form": "네 거의 끝나는 듯 해요",
          "original_form": "네 거의 끝나는 듯 해요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032711.1.28",
          "form": "왜 아무 커플도 안나올까 이렇게 보구있게되요",
          "original_form": "왜 아무 커플도 안나올까 이렇게 보구있게되요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        }
      ]
