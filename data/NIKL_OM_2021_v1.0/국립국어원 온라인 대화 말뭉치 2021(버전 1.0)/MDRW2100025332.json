        {
          "id": "MDRW2100025332.1.2",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100025332.1.3",
          "form": "아침 먹었어요",
          "original_form": "아침 먹었어요",
          "speaker_id": "2",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025332.1.4",
          "form": "전 시리얼이오",
          "original_form": "전 시리얼이오",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025332.1.5",
          "form": "전 점심이 시리얼인데ㅋ",
          "original_form": "전 점심이 시리얼인데ㅋ",
          "speaker_id": "2",
          "time": "20210901 09:38"
        },
        {
          "id": "MDRW2100025332.1.6",
          "form": "너무 부실한건?",
          "original_form": "너무 부실한건?",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025332.1.7",
          "form": "점심을 많이먹으면 일하기 힘들더라고요",
          "original_form": "점심을 많이먹으면 일하기 힘들더라고요",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025332.1.8",
          "form": "더부룩하죠",
          "original_form": "더부룩하죠",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025332.1.9",
          "form": "워낙 앉아만 있어서요",
          "original_form": "워낙 앉아만 있어서요",
          "speaker_id": "2",
          "time": "20210901 10:02"
        },
        {
          "id": "MDRW2100025332.1.10",
          "form": "일하면 어쩔수없는",
          "original_form": "일하면 어쩔수없는",
          "speaker_id": "1",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025332.1.11",
          "form": "네 아침저녁은 많이 먹어요ㅋ",
          "original_form": "네 아침저녁은 많이 먹어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025332.1.12",
          "form": "그게 낫죠",
          "original_form": "그게 낫죠",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025332.1.13",
          "form": "네 속은 편해요",
          "original_form": "네 속은 편해요",
          "speaker_id": "2",
          "time": "20210901 10:10"
        },
        {
