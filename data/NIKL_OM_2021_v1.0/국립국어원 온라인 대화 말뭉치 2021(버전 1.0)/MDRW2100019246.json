      "utterance": [
        {
          "id": "MDRW2100019246.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019246.1.2",
          "form": "네 대학생이에요",
          "original_form": "네 대학생이에요",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019246.1.3",
          "form": "저는 올해 졸업했어요..!",
          "original_form": "저는 올해 졸업했어요..!",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019246.1.4",
          "form": "수강신청 잘 하셨나요?",
          "original_form": "수강신청 잘 하셨나요?",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019246.1.5",
          "form": "오 졸업 축하드려요",
          "original_form": "오 졸업 축하드려요",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019246.1.6",
          "form": "저는 수강신청 올클했습니다 ㅋㅋ",
          "original_form": "저는 수강신청 올클했습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019246.1.7",
          "form": "감사해요ㅎㅎ",
          "original_form": "감사해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019246.1.8",
          "form": "수강신청 올클하셨다니 다행이네요 !",
          "original_form": "수강신청 올클하셨다니 다행이네요 !",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019246.1.9",
          "form": "정말 다행인거같아요",
          "original_form": "정말 다행인거같아요",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100019246.1.10",
          "form": "이번에도 비대면 수업인가요?",
          "original_form": "이번에도 비대면 수업인가요?",
          "speaker_id": "2",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100019246.1.11",
          "form": "대면이었는데",
          "original_form": "대면이었는데",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100019246.1.12",
          "form": "비대면으로 바뀌었어요",
          "original_form": "비대면으로 바뀌었어요",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100019246.1.13",
          "form": "그쵸 .. 계속 비대면이네요 ㅜㅠ",
          "original_form": "그쵸 .. 계속 비대면이네요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100019246.1.14",
          "form": "과제는 괜찮으신가요?",
          "original_form": "과제는 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100019246.1.15",
          "form": "과제는 항상 어렵죠 특히 조별과제",
          "original_form": "과제는 항상 어렵죠 특히 조별과제",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100019246.1.16",
          "form": "맞아요 .. 비대면하면 조별과제도 참 애매하지 않나요..",
          "original_form": "맞아요 .. 비대면하면 조별과제도 참 애매하지 않나요..",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100019246.1.17",
          "form": "온라인으로 조별과제하는데",
          "original_form": "온라인으로 조별과제하는데",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100019246.1.18",
          "form": "참.. 무임승차가 많더라구여",
          "original_form": "참.. 무임승차가 많더라구여",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100019246.1.19",
          "form": "얼굴안본다고 뻔뻔하게 나가는건지",
          "original_form": "얼굴안본다고 뻔뻔하게 나가는건지",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100019246.1.20",
          "form": "대면으로해도 그러는데 비대면은 더 하겠어요...",
          "original_form": "대면으로해도 그러는데 비대면은 더 하겠어요...",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100019246.1.21",
          "form": "조별과제할때 그런 사람들 있음 너무 짜증나죠",
          "original_form": "조별과제할때 그런 사람들 있음 너무 짜증나죠",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100019246.1.22",
          "form": "이번엔 그런사람들 없었으면 좋겠어요",
          "original_form": "이번엔 그런사람들 없었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 15:04"
        },
        {
          "id": "MDRW2100019246.1.23",
          "form": "그러니까요..",
          "original_form": "그러니까요..",
          "speaker_id": "2",
          "time": "20210828 15:07"
        }
      ]
