      "utterance": [
        {
          "id": "MDRW2100034979.1.1",
          "form": "김치국물만요??!!!",
          "original_form": "김치국물만요??!!!",
          "speaker_id": "2",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034979.1.2",
          "form": "으잉",
          "original_form": "으잉",
          "speaker_id": "2",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034979.1.3",
          "form": "아 태국인가는 김치국물만도 따로 판대요",
          "original_form": "아 태국인가는 김치국물만도 따로 판대요",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034979.1.4",
          "form": "네네 저도 놀랐어요ㅎㅎ",
          "original_form": "네네 저도 놀랐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034979.1.5",
          "form": "수프 만들기 좋다나?",
          "original_form": "수프 만들기 좋다나?",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034979.1.6",
          "form": "상상이 안되네요ㅇㅁㅇ.. 오모나",
          "original_form": "상상이 안되네요ㅇㅁㅇ.. 오모나",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034979.1.7",
          "form": "ㅎㅎ저도 상상이 안가요",
          "original_form": "ㅎㅎ저도 상상이 안가요",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034979.1.8",
          "form": "설렁탕에 김치국물 넣듯이...는 아닐텐데",
          "original_form": "설렁탕에 김치국물 넣듯이...는 아닐텐데",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034979.1.9",
          "form": "하긴 태국음식이 살짝 시큼새콤한게 있어서 은근 어울릴것 같긴 해요",
          "original_form": "하긴 태국음식이 살짝 시큼새콤한게 있어서 은근 어울릴것 같긴 해요",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034979.1.10",
          "form": "똠양꿍도아직안먹어봐서 궁금해요 ㅇㅁㅇ..ㅋㅋㄱ",
          "original_form": "똠양꿍도아직안먹어봐서 궁금해요 ㅇㅁㅇ..ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034979.1.11",
          "form": "똠양꿍 맛있어요ㅎ 새콤시큼얼큰",
          "original_form": "똠양꿍 맛있어요ㅎ 새콤시큼얼큰",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034979.1.12",
          "form": "맛있어서 태국 가서 똠양꿍맛 라면도 사왔죠",
          "original_form": "맛있어서 태국 가서 똠양꿍맛 라면도 사왔죠",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034979.1.13",
          "form": "오 음식 잘안가리시나봐요!  호불호갈릴만한데!",
          "original_form": "오 음식 잘안가리시나봐요!  호불호갈릴만한데!",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034979.1.14",
          "form": "똠양꿍맛 라면 우와",
          "original_form": "똠양꿍맛 라면 우와",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034979.1.15",
          "form": "똠양꿍이 발 담근 맛입니다ㅎ 그래도 그것도 맛있더라고요",
          "original_form": "똠양꿍이 발 담근 맛입니다ㅎ 그래도 그것도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034979.1.16",
          "form": "회사에 돌렸죠",
          "original_form": "회사에 돌렸죠",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034979.1.17",
          "form": "호불호 심하든 말든 ㅎㅎㅎ",
          "original_form": "호불호 심하든 말든 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034979.1.18",
          "form": "그래도 못접하는걸 주면 고마울거같아요ㅋㅋㅋ 생각해서준거니까ㅋㅋㅋ좋아하든말든 모\r\nㅋㅋㅋㅋㄱ",
          "original_form": "그래도 못접하는걸 주면 고마울거같아요ㅋㅋㅋ 생각해서준거니까ㅋㅋㅋ좋아하든말든 모\r\nㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.19",
          "form": "마음이중요하죠! ㅋㅋㅋ",
          "original_form": "마음이중요하죠! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.20",
          "form": "똠양꿍이 발 담근 맛..",
          "original_form": "똠양꿍이 발 담근 맛..",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.21",
          "form": "상상은안가지만..",
          "original_form": "상상은안가지만..",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.23",
          "form": "ㅎㅎ 맞습니다!! 가방끈이 기시군요",
          "original_form": "ㅎㅎ 맞습니다!! 가방끈이 기시군요",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.24",
          "form": "맛있어요ㅎ",
          "original_form": "맛있어요ㅎ",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.25",
          "form": "살짝 똠양꿍 2프로 느낌?",
          "original_form": "살짝 똠양꿍 2프로 느낌?",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034979.1.26",
          "form": "전 태국 가면 캐리어 가득 사와요ㅎㅎ",
          "original_form": "전 태국 가면 캐리어 가득 사와요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034979.1.27",
          "form": "제 주변에 있으면 막 뿌릴텐데 아쉽네요",
          "original_form": "제 주변에 있으면 막 뿌릴텐데 아쉽네요",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034979.1.28",
          "form": "와 궁금해요 태국도 궁금하구 ㅋㅋㅋ똠양꿍은 담에 한국에서라도 도전한번해봐야겠네요",
          "original_form": "와 궁금해요 태국도 궁금하구 ㅋㅋㅋ똠양꿍은 담에 한국에서라도 도전한번해봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034979.1.29",
          "form": "앗 그러게요 ㅋㅋㅋㄱ아쉽습니닼ㅋㄱㅋ",
          "original_form": "앗 그러게요 ㅋㅋㅋㄱ아쉽습니닼ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034979.1.30",
          "form": "네네 요새 태국 맛집 한국에 진출 많이 했더라고요",
          "original_form": "네네 요새 태국 맛집 한국에 진출 많이 했더라고요",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034979.1.31",
          "form": "가격 뻥튀기는 좀 심하지만",
          "original_form": "가격 뻥튀기는 좀 심하지만",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034979.1.32",
          "form": "네 꽤비싸더라구요\r\n근데 막 줄도서서먹구그러더라구요ㅋㅋㅋ",
          "original_form": "네 꽤비싸더라구요\r\n근데 막 줄도서서먹구그러더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034979.1.33",
          "form": "현지는 진짜 싼데ㅜ",
          "original_form": "현지는 진짜 싼데ㅜ",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034979.1.34",
          "form": "아 그래도 똠양꿍은 현지서도 비싼편",
          "original_form": "아 그래도 똠양꿍은 현지서도 비싼편",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034979.1.35",
          "form": "팟타이랑 쌀국수는 진짜 싸요ㅎㅎ",
          "original_form": "팟타이랑 쌀국수는 진짜 싸요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:35"
        }
      ]
