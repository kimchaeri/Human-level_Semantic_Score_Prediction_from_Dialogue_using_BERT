        },
        {
          "id": "MDRW2100015542.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015542.1.28",
          "form": "라스에 권혁수나와있는데",
          "original_form": "라스에 권혁수나와있는데",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015542.1.29",
          "form": "권혁수 오랜만이네요",
          "original_form": "권혁수 오랜만이네요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015542.1.30",
          "form": "성대모사능력하나는진짜",
          "original_form": "성대모사능력하나는진짜",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015542.1.31",
          "form": "혹시 자숙기간이었나요",
          "original_form": "혹시 자숙기간이었나요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015542.1.32",
