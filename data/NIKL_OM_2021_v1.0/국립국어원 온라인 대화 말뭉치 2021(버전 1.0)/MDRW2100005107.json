      "utterance": [
        {
          "id": "MDRW2100005107.1.1",
          "form": "여행!!",
          "original_form": "여행!!",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005107.1.2",
          "form": "누나도여행 좋아하지",
          "original_form": "누나도여행 좋아하지",
          "speaker_id": "1",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005107.1.3",
          "form": "좋아하지",
          "original_form": "좋아하지",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005107.1.4",
          "form": "근데 언제 가냐..",
          "original_form": "근데 언제 가냐..",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005107.1.5",
          "form": "가장 좋았던 여행지가어디야",
          "original_form": "가장 좋았던 여행지가어디야",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005107.1.6",
          "form": "나는 엘에이쪽? 되게 여유롭고 핫함",
          "original_form": "나는 엘에이쪽? 되게 여유롭고 핫함",
          "speaker_id": "2",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005107.1.7",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "2",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005107.1.8",
          "form": "나는 네팔이지",
          "original_form": "나는 네팔이지",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005107.1.9",
          "form": "히말라야를 이길곳을 못봣어",
          "original_form": "히말라야를 이길곳을 못봣어",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005107.1.10",
          "form": "언제 히말라야 감?",
          "original_form": "언제 히말라야 감?",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005107.1.11",
          "form": "..2번 갔어",
          "original_form": "..2번 갔어",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005107.1.12",
          "form": "2012년 2105년",
          "original_form": "2012년 2105년",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005107.1.13",
          "form": "2105년 이랰ㅋㅋㅋㅋㅋ",
          "original_form": "2105년 이랰ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005107.1.14",
          "form": "그렇게 좋았냐 2번이나 가게",
          "original_form": "그렇게 좋았냐 2번이나 가게",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005107.1.15",
          "form": "봉사활동으로 갔엇는데",
          "original_form": "봉사활동으로 갔엇는데",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005107.1.16",
          "form": "좋아서 한번 더가게 되었지",
          "original_form": "좋아서 한번 더가게 되었지",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005107.1.17",
          "form": "거기 추운데야?",
          "original_form": "거기 추운데야?",
          "speaker_id": "2",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005107.1.18",
          "form": "음 아니 안추워",
          "original_form": "음 아니 안추워",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005107.1.19",
          "form": "물론 올라가면 춥긴해",
          "original_form": "물론 올라가면 춥긴해",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005107.1.20",
          "form": "끝까지는 못 올라갔을테고",
          "original_form": "끝까지는 못 올라갔을테고",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005107.1.21",
          "form": "어디까지 올라감?",
          "original_form": "어디까지 올라감?",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
