      "utterance": [
        {
          "id": "MDRW2100044557.1.1",
          "form": "새로 생긴곳이 많구...",
          "original_form": "새로 생긴곳이 많구...",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044557.1.2",
          "form": "그림그리는 카페도 있었는데 거기도 없어지진않았는지 모르겠네요ㅜㅜ",
          "original_form": "그림그리는 카페도 있었는데 거기도 없어지진않았는지 모르겠네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044557.1.3",
          "form": "거기 수플레팬케이크가 맛있었는데",
          "original_form": "거기 수플레팬케이크가 맛있었는데",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044557.1.4",
          "form": "그림그리는 카페는 가본적이 없어요",
          "original_form": "그림그리는 카페는 가본적이 없어요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044557.1.5",
          "form": "의외로 홍대 안가본곳이 많아요 ㅋㅋ",
          "original_form": "의외로 홍대 안가본곳이 많아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044557.1.6",
          "form": "전 10대 때는 홍대만 다녔었는데 20대가 되면서 신촌으로 많이 가게되더라구요.  ㅜㅜ",
          "original_form": "전 10대 때는 홍대만 다녔었는데 20대가 되면서 신촌으로 많이 가게되더라구요.  ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044557.1.7",
          "form": "신촌으로 자주 가셨군요",
          "original_form": "신촌으로 자주 가셨군요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044557.1.8",
          "form": "저는 오히려 20대가 된 이후에 홍대를 자주 갔어요",
          "original_form": "저는 오히려 20대가 된 이후에 홍대를 자주 갔어요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044557.1.9",
          "form": "북새통때문에 홍대를 처음 가보게 됐었는데",
          "original_form": "북새통때문에 홍대를 처음 가보게 됐었는데",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044557.1.10",
          "form": "없어진다는 소식에 굉장히 슬펐어요",
          "original_form": "없어진다는 소식에 굉장히 슬펐어요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044557.1.11",
          "form": "북새통 없어져요?????",
          "original_form": "북새통 없어져요?????",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044557.1.12",
          "form": "모르셨구나 ㅠ",
          "original_form": "모르셨구나 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044557.1.13",
          "form": "없어졌어요 ㅠㅠ 기존에 있던 위치에서 다른데로 옮겨갔어요",
          "original_form": "없어졌어요 ㅠㅠ 기존에 있던 위치에서 다른데로 옮겨갔어요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044557.1.14",
          "form": "그 분위기도 아니에요 ㅠ",
          "original_form": "그 분위기도 아니에요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044557.1.15",
          "form": "허류....",
          "original_form": "허류....",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044557.1.16",
          "form": "많이 바뀌었겠네요ㄷㄷ",
          "original_form": "많이 바뀌었겠네요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044557.1.17",
          "form": "바비레드 혹시 아세요?",
          "original_form": "바비레드 혹시 아세요?",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044557.1.18",
          "form": "바비레드는 남아있었으면 좋겠는데...ㅜ",
          "original_form": "바비레드는 남아있었으면 좋겠는데...ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044557.1.19",
          "form": "어... 아니요 ㅠㅠ 잘 몰라요 ㅠ",
          "original_form": "어... 아니요 ㅠㅠ 잘 몰라요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044557.1.20",
          "form": "아마 남아있는 것 같아요",
          "original_form": "아마 남아있는 것 같아요",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044557.1.21",
          "form": "파스타랑 리조또 맛있는데ㅜ",
          "original_form": "파스타랑 리조또 맛있는데ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044557.1.22",
          "form": "지도로 한 번 찾아봐야겠어요",
          "original_form": "지도로 한 번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044557.1.23",
          "form": "임시 휴업이래요 ㅠㅠ",
          "original_form": "임시 휴업이래요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044557.1.24",
          "form": "으아니...!!!!",
          "original_form": "으아니...!!!!",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044557.1.25",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044557.1.26",
          "form": "위드코로나할 때쯤 돌아오려나...",
          "original_form": "위드코로나할 때쯤 돌아오려나...",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044557.1.27",
          "form": "아마 그럴것 같아요 ㅠ",
          "original_form": "아마 그럴것 같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044557.1.28",
          "form": "홍대도 문 닫은 가게 은근 많거든요 ㅠㅠ",
          "original_form": "홍대도 문 닫은 가게 은근 많거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044557.1.29",
          "form": "ㅜㅠ 아무래도 타격이 크죠...ㅜ",
          "original_form": "ㅜㅠ 아무래도 타격이 크죠...ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044557.1.30",
          "form": "그러니까요 ㅠㅠㅠ 아는 가게들이 다 사라지고 있어서 슬ㄹ퍼요 ㅠ",
          "original_form": "그러니까요 ㅠㅠㅠ 아는 가게들이 다 사라지고 있어서 슬ㄹ퍼요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044557.1.31",
          "form": "ㅜㅜ다시 돌아오길...",
          "original_form": "ㅜㅜ다시 돌아오길...",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044557.1.32",
          "form": "신촌에도 은근 가게들이 많은데 잘 돌아다녀보질 못했어요",
          "original_form": "신촌에도 은근 가게들이 많은데 잘 돌아다녀보질 못했어요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        }
