        {
          "id": "MDRW2100019630.1.1",
          "form": "학교 졸업한지 오래됐지만 추억은 많죠",
          "original_form": "학교 졸업한지 오래됐지만 추억은 많죠",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019630.1.2",
          "form": "와 오랜만이네요",
          "original_form": "와 오랜만이네요",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019630.1.3",
          "form": "저도 학교 졸업한 지 좀 됐어요",
          "original_form": "저도 학교 졸업한 지 좀 됐어요",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019630.1.4",
          "form": "저는 대학 때보다는 교복입을 시절 기억이 도",
          "original_form": "저는 대학 때보다는 교복입을 시절 기억이 도",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019630.1.5",
          "form": "더 많은데",
          "original_form": "더 많은데",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019630.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019630.1.7",
          "form": "교복이 진짜진짜 불편했어요",
          "original_form": "교복이 진짜진짜 불편했어요",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019630.1.8",
          "form": "다시는 입고싶지 않을만큼",
          "original_form": "다시는 입고싶지 않을만큼",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019630.1.9",
          "form": "교복 ㅋㅋㅋ맞아요",
          "original_form": "교복 ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019630.1.10",
          "form": "그리고 1평도 안되는 책상도 너무 불편했어요",
          "original_form": "그리고 1평도 안되는 책상도 너무 불편했어요",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019630.1.11",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019630.1.12",
          "form": "그나마 다행인 건",
          "original_form": "그나마 다행인 건",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019630.1.13",
          "form": "저희는 일체형 책상이 아니었어요",
          "original_form": "저희는 일체형 책상이 아니었어요",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019630.1.14",
          "form": "그 악마의 물건",
          "original_form": "그 악마의 물건",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019630.1.15",
          "form": "발명한 사람은",
          "original_form": "발명한 사람은",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019630.1.16",
          "form": "으 맞아요 일체형책상은 전부 불태워야합니다",
          "original_form": "으 맞아요 일체형책상은 전부 불태워야합니다",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.17",
          "form": "써보긴 한 건지 의문이 들지만",
          "original_form": "써보긴 한 건지 의문이 들지만",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.18",
          "form": "국내 사람이던데",
          "original_form": "국내 사람이던데",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.19",
          "form": "그리고 1교시 이전의",
          "original_form": "그리고 1교시 이전의",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.20",
          "form": "0교시의 존재",
          "original_form": "0교시의 존재",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.21",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.22",
          "form": "아 맞아요 아침자습시간",
          "original_form": "아 맞아요 아침자습시간",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.23",
          "form": "일찍 일어나는 바람에",
          "original_form": "일찍 일어나는 바람에",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019630.1.24",
          "form": "전 그시간이 매번 자는 시간이였어요",
          "original_form": "전 그시간이 매번 자는 시간이였어요",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.25",
          "form": "졸려서 졸았던 기억이 있어요",
          "original_form": "졸려서 졸았던 기억이 있어요",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.26",
          "form": "ㅋㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋㅋ저도요",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.27",
          "form": "저는 그 뭐냐",
          "original_form": "저는 그 뭐냐",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.28",
          "form": "학교 앞에서 나눠주는 곽티슈에",
          "original_form": "학교 앞에서 나눠주는 곽티슈에",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.29",
          "form": "무릎 담요 씌워서 베고 잤던 기억이 나요",
          "original_form": "무릎 담요 씌워서 베고 잤던 기억이 나요",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ곽티슈 저도 많이 받았어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ곽티슈 저도 많이 받았어요",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.31",
          "form": "그거 은근 티슈가 푹신해서 좋았어요",
          "original_form": "그거 은근 티슈가 푹신해서 좋았어요",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019630.1.33",
          "form": "교복입을 시절 동아리는 그냥 있으나마나였던 것 같아요",
          "original_form": "교복입을 시절 동아리는 그냥 있으나마나였던 것 같아요",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019630.1.34",
          "form": "교복입고 만세하면 진짜 고릴라처럼",
          "original_form": "교복입고 만세하면 진짜 고릴라처럼",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019630.1.35",
          "form": "뜯어버리고싶어요",
          "original_form": "뜯어버리고싶어요",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019630.1.36",
          "form": "동아리 어떤것 하셨어요?",
          "original_form": "동아리 어떤것 하셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019630.1.37",
          "form": "ㅋㅋㅋㅋㅋ맞아요 교복 재질이 신축성이 없어서 불편했어요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 교복 재질이 신축성이 없어서 불편했어요",
          "speaker_id": "2",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019630.1.38",
          "form": "상의 엄청 타이트하게 나옴",
          "original_form": "상의 엄청 타이트하게 나옴",
          "speaker_id": "2",
          "time": "20210827 20:54"
        },
        {
