      "utterance": [
        {
          "id": "MDRW2100045389.1.1",
          "form": "나도 사실",
          "original_form": "나도 사실",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045389.1.2",
          "form": "탄산수 버리다가",
          "original_form": "탄산수 버리다가",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045389.1.3",
          "form": "청 타먹기 시작했다가",
          "original_form": "청 타먹기 시작했다가",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045389.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045389.1.5",
          "form": "청 타는 량이",
          "original_form": "청 타는 량이",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045389.1.6",
          "form": "점점 줄었나요",
          "original_form": "점점 줄었나요",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045389.1.7",
          "form": "ㅇㅇ ㅋㅋㅋ 글고",
          "original_form": "ㅇㅇ ㅋㅋㅋ 글고",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.8",
          "form": "내가 혓바닥이",
          "original_form": "내가 혓바닥이",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.9",
          "form": "단거 짠거 별로 안좋아해",
          "original_form": "단거 짠거 별로 안좋아해",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.10",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.11",
          "form": "난 근데",
          "original_form": "난 근데",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.12",
          "form": "싱거운 물은 마시겠거든?",
          "original_form": "싱거운 물은 마시겠거든?",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.13",
          "form": "싱거운 탄산수는",
          "original_form": "싱거운 탄산수는",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.14",
          "form": "도저히 못마시겠어",
          "original_form": "도저히 못마시겠어",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045389.1.15",
          "form": "대한민국 가정이라면 어디나 하나쯤 가지고있다는 매실청",
          "original_form": "대한민국 가정이라면 어디나 하나쯤 가지고있다는 매실청",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045389.1.16",
          "form": "물에다 타면 나 진짜 매실 향만ㄴㅏ는 수준으로",
          "original_form": "물에다 타면 나 진짜 매실 향만ㄴㅏ는 수준으로",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045389.1.17",
          "form": "연하게 먹는데",
          "original_form": "연하게 먹는데",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045389.1.18",
          "form": "탄산수에 타면 거의 매실음료캔수준으로 타야해....",
          "original_form": "탄산수에 타면 거의 매실음료캔수준으로 타야해....",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045389.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아닠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아닠",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045389.1.20",
          "form": "근데 굳이 그렇게까지 탄산수를 마시기보단",
          "original_form": "근데 굳이 그렇게까지 탄산수를 마시기보단",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045389.1.21",
          "form": "걍 물을 마시면 될것같아서",
          "original_form": "걍 물을 마시면 될것같아서",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045389.1.22",
          "form": "물을 마시고있어",
          "original_form": "물을 마시고있어",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045389.1.23",
          "form": "약간 탄산은",
          "original_form": "약간 탄산은",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045389.1.24",
          "form": "자체적으로 톡 쏘다보니까",
          "original_form": "자체적으로 톡 쏘다보니까",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045389.1.25",
          "form": "같이 있는 맛도 좀 진해야",
          "original_form": "같이 있는 맛도 좀 진해야",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045389.1.26",
          "form": "느껴지는것같아",
          "original_form": "느껴지는것같아",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045389.1.27",
          "form": "그렇긴 해 ㅇㅇ",
          "original_form": "그렇긴 해 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045389.1.28",
          "form": "약간 아이스크림같이",
          "original_form": "약간 아이스크림같이",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045389.1.29",
          "form": "후",
          "original_form": "후",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045389.1.30",
          "form": "방금의 식사로",
          "original_form": "방금의 식사로",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045389.1.31",
          "form": "짧은 시간에 물 약 1.5미리를",
          "original_form": "짧은 시간에 물 약 1.5미리를",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045389.1.32",
          "form": "섭취햇다",
          "original_form": "섭취햇다",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045389.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.34",
          "form": "그렇게 마시면",
          "original_form": "그렇게 마시면",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.35",
          "form": "물배 안차?",
          "original_form": "물배 안차?",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.36",
          "form": "아니 겁나 배불러",
          "original_form": "아니 겁나 배불러",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.37",
          "form": "역류성 식도염도 있어서",
          "original_form": "역류성 식도염도 있어서",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.38",
          "form": "이제 자면안돼",
          "original_form": "이제 자면안돼",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.39",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.40",
          "form": "이게 무슨",
          "original_form": "이게 무슨",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045389.1.41",
          "form": "불면의 굴레....",
          "original_form": "불면의 굴레....",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045389.1.42",
          "form": "걱정마",
          "original_form": "걱정마",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045389.1.43",
          "form": "정 졸리면",
          "original_form": "정 졸리면",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045389.1.44",
          "form": "베개 쌓아놓고",
          "original_form": "베개 쌓아놓고",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045389.1.45",
          "form": "기대서 자면 돼",
          "original_form": "기대서 자면 돼",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045389.1.46",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045389.1.47",
          "form": "어쩜 역류성 식도염 환자들은",
          "original_form": "어쩜 역류성 식도염 환자들은",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045389.1.48",
          "form": "밥먹고 다 똑같은 소리를 할까...",
          "original_form": "밥먹고 다 똑같은 소리를 할까...",
