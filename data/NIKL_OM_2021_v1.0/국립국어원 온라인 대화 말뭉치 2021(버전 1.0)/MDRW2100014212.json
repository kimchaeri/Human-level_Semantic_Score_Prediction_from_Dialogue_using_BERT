      "utterance": [
        {
          "id": "MDRW2100014212.1.1",
          "form": "최근 관람한 공연이 있으신가요?",
          "original_form": "최근 관람한 공연이 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014212.1.2",
          "form": "공연은 기회가 많이 없어서 못 봤구요 최근에 영화는 봤어요",
          "original_form": "공연은 기회가 많이 없어서 못 봤구요 최근에 영화는 봤어요",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014212.1.3",
          "form": "최근에 보신거 있으신가요?",
          "original_form": "최근에 보신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014212.1.4",
          "form": "크루엘라 재밌게 봤어요",
          "original_form": "크루엘라 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014212.1.5",
          "form": "그거 재밌어요",
          "original_form": "그거 재밌어요",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014212.1.6",
          "form": "그 배우 이름 뭐더라",
          "original_form": "그 배우 이름 뭐더라",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014212.1.7",
          "form": "너무 매력있더라구요",
          "original_form": "너무 매력있더라구요",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014212.1.8",
          "form": "엠마스톤이요!",
          "original_form": "엠마스톤이요!",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014212.1.9",
          "form": "아 맞아요! 순간 적으로 기억이 안났네요ㅎㅎ",
          "original_form": "아 맞아요! 순간 적으로 기억이 안났네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014212.1.10",
          "form": "그 배우 연기를 그렇게 잘 하는지 몰랐는데 이번에 다시보게 됐어요",
          "original_form": "그 배우 연기를 그렇게 잘 하는지 몰랐는데 이번에 다시보게 됐어요",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014212.1.11",
          "form": "이번 영화는 눈이 정말 즐거웠어요ㅎㅎ",
          "original_form": "이번 영화는 눈이 정말 즐거웠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014212.1.12",
          "form": "맞아요 소품이나 패션도 다양해서",
          "original_form": "맞아요 소품이나 패션도 다양해서",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014212.1.13",
          "form": "볼거리가 많더라구요",
          "original_form": "볼거리가 많더라구요",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014212.1.14",
          "form": "기억에 남는 장면이 있나요",
          "original_form": "기억에 남는 장면이 있나요",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014212.1.15",
          "form": "엠마스톤이 차 위에서 드레스를 펼치는 장면이 기억에 남아요",
          "original_form": "엠마스톤이 차 위에서 드레스를 펼치는 장면이 기억에 남아요",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014212.1.16",
          "form": "아~ 그장면 되게 인상깊었어요",
          "original_form": "아~ 그장면 되게 인상깊었어요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014212.1.17",
          "form": "저는  불타면서 레드 드레스로 변하는 장면이 기억에 남아요",
          "original_form": "저는  불타면서 레드 드레스로 변하는 장면이 기억에 남아요",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014212.1.18",
          "form": "그 장면도 정말 멋있었죠",
          "original_form": "그 장면도 정말 멋있었죠",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014212.1.19",
          "form": "진짜 영상미가 너무 좋은 영화였어요",
          "original_form": "진짜 영상미가 너무 좋은 영화였어요",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014212.1.20",
          "form": "나중에 넷플에 올라온다면 다시 보려구요ㅋㅋ",
          "original_form": "나중에 넷플에 올라온다면 다시 보려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:04"
        }
      ]
