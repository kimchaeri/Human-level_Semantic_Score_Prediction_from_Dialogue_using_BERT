        {
          "id": "MDRW2100018171.1.1",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018171.1.2",
          "form": "네 가끔해요",
          "original_form": "네 가끔해요",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018171.1.3",
          "form": "최근에 거래하신거 있으신가요?",
          "original_form": "최근에 거래하신거 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018171.1.4",
          "form": "아이물건들이여",
          "original_form": "아이물건들이여",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018171.1.5",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018171.1.6",
          "form": "상태좋은건 버리기아깝더라구요",
          "original_form": "상태좋은건 버리기아깝더라구요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018171.1.7",
          "form": "맞아요 그렇긴해요",
          "original_form": "맞아요 그렇긴해요",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018171.1.8",
          "form": "그래서 중고거래하는데 생각보다 안팔리기도 하네요",
          "original_form": "그래서 중고거래하는데 생각보다 안팔리기도 하네요",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018171.1.9",
          "form": "그런건 그냥 드림하기도하고요",
          "original_form": "그런건 그냥 드림하기도하고요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018171.1.10",
          "form": "그런 방법도 있네요",
          "original_form": "그런 방법도 있네요",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018171.1.11",
          "form": "고수시네요",
          "original_form": "고수시네요",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018171.1.12",
          "form": "ㅎ 고수는 아닌데",
          "original_form": "ㅎ 고수는 아닌데",
          "speaker_id": "1",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018171.1.13",
          "form": "고수느낌이신걸요",
          "original_form": "고수느낌이신걸요",
          "speaker_id": "2",
          "time": "20210827 06:29"
        },
        {
          "id": "MDRW2100018171.1.14",
          "form": "그냠 새거사기위해ㅋ",
          "original_form": "그냠 새거사기위해ㅋ",
