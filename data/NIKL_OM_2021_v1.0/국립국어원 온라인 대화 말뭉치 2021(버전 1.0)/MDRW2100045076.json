        {
          "id": "MDRW2100045076.1.1",
          "form": "도미노용",
          "original_form": "도미노용",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045076.1.2",
          "form": "도미노 어떤 피자요?!",
          "original_form": "도미노 어떤 피자요?!",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045076.1.3",
          "form": "비싼거요",
          "original_form": "비싼거요",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045076.1.4",
          "form": "새우들어간거",
          "original_form": "새우들어간거",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045076.1.5",
          "form": "ㅋㅋㅋㅋㅋ맞아요 새우가맛있죠",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 새우가맛있죠",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045076.1.6",
          "form": "치즈크러스트 조아해요?",
          "original_form": "치즈크러스트 조아해요?",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045076.1.7",
          "form": "네 전에 트리플 도우 조아해씀",
          "original_form": "네 전에 트리플 도우 조아해씀",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045076.1.8",
          "form": "이제안팔던데",
          "original_form": "이제안팔던데",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045076.1.9",
          "form": "마자요 도미노는 꼭 맛있는거 단종시키더라구요",
          "original_form": "마자요 도미노는 꼭 맛있는거 단종시키더라구요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045076.1.10",
          "form": "최애엿어요",
          "original_form": "최애엿어요",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045076.1.11",
          "form": "님 버거 최앤요?",
          "original_form": "님 버거 최앤요?",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045076.1.12",
          "form": "저 아보카도버거요 다운타우너",
          "original_form": "저 아보카도버거요 다운타우너",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045076.1.13",
          "form": "와저도",
          "original_form": "와저도",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045076.1.14",
          "form": "프차중에선요?",
          "original_form": "프차중에선요?",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045076.1.15",
          "form": "ㅋㅋㅋㅋ근데 쉑쉑도 너무맛있어요",
          "original_form": "ㅋㅋㅋㅋ근데 쉑쉑도 너무맛있어요",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045076.1.16",
          "form": "쉑스택 더블",
          "original_form": "쉑스택 더블",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045076.1.17",
          "form": "오호 버거킹음요",
          "original_form": "오호 버거킹음요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045076.1.18",
          "form": "아 아니다 쉑버거 더블이용",
          "original_form": "아 아니다 쉑버거 더블이용",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045076.1.19",
          "form": "버거킹 어느순간 점점 창렬되더라구요ㅠㅠ",
          "original_form": "버거킹 어느순간 점점 창렬되더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045076.1.20",
