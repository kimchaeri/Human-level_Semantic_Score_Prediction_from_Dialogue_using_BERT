      "utterance": [
        {
          "id": "MDRW2100008520.1.1",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008520.1.2",
          "form": "배달의 민족 이제 그만 시켜 먹으려고요ㅜㅜㅜ",
          "original_form": "배달의 민족 이제 그만 시켜 먹으려고요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008520.1.3",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008520.1.4",
          "form": "확찐자가 되서요 ㅋㅋ",
          "original_form": "확찐자가 되서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008520.1.5",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008520.1.6",
          "form": "야식이 문제죠",
          "original_form": "야식이 문제죠",
          "speaker_id": "1",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008520.1.7",
          "form": "전 배달음식과 폭식이요",
          "original_form": "전 배달음식과 폭식이요",
          "speaker_id": "2",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008520.1.8",
          "form": "양을 줄여야죠",
          "original_form": "양을 줄여야죠",
          "speaker_id": "2",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008520.1.9",
          "form": "그래더 맛있어요",
          "original_form": "그래더 맛있어요",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008520.1.10",
          "form": "전 진짜 내일부터 도시락 싸다니려고요",
          "original_form": "전 진짜 내일부터 도시락 싸다니려고요",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008520.1.11",
          "form": "건강한 집밥 메뉴로!",
          "original_form": "건강한 집밥 메뉴로!",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008520.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008520.1.13",
          "form": "사실 주말부터 할랬는데 어제 마카롱 먹었어요ㅜ",
          "original_form": "사실 주말부터 할랬는데 어제 마카롱 먹었어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008520.1.14",
          "form": "쉽지 않조",
          "original_form": "쉽지 않조",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008520.1.15",
          "form": "오늘은 아이스크림ㅜㅜ",
          "original_form": "오늘은 아이스크림ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008520.1.16",
          "form": "맛있는게 넘 많아요ㅜ",
          "original_form": "맛있는게 넘 많아요ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008520.1.17",
          "form": "저는 치킨",
          "original_form": "저는 치킨",
          "speaker_id": "1",
