      "utterance": [
        {
          "id": "MDRW2100002103.1.1",
          "form": "게임하세용?",
          "original_form": "게임하세용?",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002103.1.2",
          "form": "저 거의 안해요ㅠ",
          "original_form": "저 거의 안해요ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002103.1.3",
          "form": "게임을 못하는 편이라..",
          "original_form": "게임을 못하는 편이라..",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002103.1.4",
          "form": "뭐 하시는 거 있으신가요?",
          "original_form": "뭐 하시는 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002103.1.5",
          "form": "전 아직 옛날게임을...",
          "original_form": "전 아직 옛날게임을...",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.6",
          "form": "메이플 던파..ㅋㅋㅋㅋㅋ",
          "original_form": "메이플 던파..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.7",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.9",
          "form": "아 알죠",
          "original_form": "아 알죠",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.10",
          "form": "메이플은 친구들이 하는 거",
          "original_form": "메이플은 친구들이 하는 거",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.11",
          "form": "많이 봤어요",
          "original_form": "많이 봤어요",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.12",
          "form": "던파는 뭐더라...",
          "original_form": "던파는 뭐더라...",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002103.1.13",
          "form": "ㅋㅋㅋㅋㅋ 요즘게임은 어려워서",
          "original_form": "ㅋㅋㅋㅋㅋ 요즘게임은 어려워서",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002103.1.14",
          "form": "못하겠어요 ㅠㅋㅋ",
          "original_form": "못하겠어요 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002103.1.15",
          "form": "그쵸 배우는 게 시간이 은근 걸려서..",
          "original_form": "그쵸 배우는 게 시간이 은근 걸려서..",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002103.1.16",
          "form": "전 폰 게임 쿠키런 예전에 했었어요",
          "original_form": "전 폰 게임 쿠키런 예전에 했었어요",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002103.1.17",
          "form": "오 쿠키런 존잼이죠",
          "original_form": "오 쿠키런 존잼이죠",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002103.1.18",
          "form": "저도 엄청했어요 진짜 ㅠ",
          "original_form": "저도 엄청했어요 진짜 ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002103.1.19",
          "form": "ㅋㅋㅋㅋ근데 요즘은 워낙",
          "original_form": "ㅋㅋㅋㅋ근데 요즘은 워낙",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002103.1.20",
          "form": "업그레이드가 많이 되서 어려워지길래",
          "original_form": "업그레이드가 많이 되서 어려워지길래",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002103.1.21",
          "form": "자연스럽게 안하게 된 거 같아요",
          "original_form": "자연스럽게 안하게 된 거 같아요",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002103.1.22",
          "form": "맞아요 .. 옛날에 막 테트리스 사천성",
          "original_form": "맞아요 .. 옛날에 막 테트리스 사천성",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002103.1.23",
          "form": "이런것도 잼났믐뎅 ㅋ",
          "original_form": "이런것도 잼났믐뎅 ㅋ",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002103.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 테트리스",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 테트리스",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002103.1.25",
          "form": "오랜만이네요",
          "original_form": "오랜만이네요",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002103.1.26",
          "form": "단순한 게 최고예요",
          "original_form": "단순한 게 최고예요",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002103.1.27",
          "form": "요즘엔 배그 많이들하던데",
          "original_form": "요즘엔 배그 많이들하던데",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002103.1.28",
          "form": "배그.. 맞아요",
          "original_form": "배그.. 맞아요",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002103.1.29",
          "form": "롤도 되게 많이 하더라구요",
          "original_form": "롤도 되게 많이 하더라구요",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002103.1.30",
          "form": "제 동생이 롤을 진짜 많이 해서 정말 많이 보고만 있어요..",
          "original_form": "제 동생이 롤을 진짜 많이 해서 정말 많이 보고만 있어요..",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002103.1.31",
          "form": "롤 재밌대용 ㅋㅋ",
          "original_form": "롤 재밌대용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:47"
        }
      ]
    }
