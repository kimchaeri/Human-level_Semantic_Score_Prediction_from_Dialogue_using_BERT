      "utterance": [
        {
          "id": "MDRW2100002353.1.1",
          "form": "나도 승모 힘 안쓰려고",
          "original_form": "나도 승모 힘 안쓰려고",
          "speaker_id": "1",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002353.1.2",
          "form": "어깨 엄청내려서 운동하는데",
          "original_form": "어깨 엄청내려서 운동하는데",
          "speaker_id": "1",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002353.1.3",
          "form": "한번 긴장되면",
          "original_form": "한번 긴장되면",
          "speaker_id": "2",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002353.1.4",
          "form": "벤치 할때도",
          "original_form": "벤치 할때도",
          "speaker_id": "2",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002353.1.5",
          "form": "긴장됨",
          "original_form": "긴장됨",
          "speaker_id": "2",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002353.1.6",
          "form": "어깨 너무 내려도 나처럼 회전근 다칠지도 몰라",
          "original_form": "어깨 너무 내려도 나처럼 회전근 다칠지도 몰라",
          "speaker_id": "2",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002353.1.7",
          "form": "우리 늙었어",
          "original_form": "우리 늙었어",
          "speaker_id": "2",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002353.1.8",
          "form": "아 그래",
          "original_form": "아 그래",
          "speaker_id": "1",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.9",
          "form": "왼쪽 어까가",
          "original_form": "왼쪽 어까가",
          "speaker_id": "1",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.10",
          "form": "말 위로 들때 아프면",
          "original_form": "말 위로 들때 아프면",
          "speaker_id": "2",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.11",
          "form": "이걸 어떻게 설명하지 특정 동작에서 찌릿한데",
          "original_form": "이걸 어떻게 설명하지 특정 동작에서 찌릿한데",
          "speaker_id": "1",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.12",
          "form": "회전근 손상이야",
          "original_form": "회전근 손상이야",
          "speaker_id": "2",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.13",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.14",
          "form": "맞을듯",
          "original_form": "맞을듯",
          "speaker_id": "2",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.15",
          "form": "위로 들때는 괜찮은데",
          "original_form": "위로 들때는 괜찮은데",
          "speaker_id": "1",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002353.1.16",
          "form": "난 위로 들때 아픈데",
          "original_form": "난 위로 들때 아픈데",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002353.1.17",
          "form": "턱걸이에서 당기는건 괜찮아",
          "original_form": "턱걸이에서 당기는건 괜찮아",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002353.1.18",
          "form": "근데 그래서 생긴 두통이면",
          "original_form": "근데 그래서 생긴 두통이면",
          "speaker_id": "1",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002353.1.19",
          "form": "타이레놀로 괜찮아져?",
          "original_form": "타이레놀로 괜찮아져?",
          "speaker_id": "1",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002353.1.20",
          "form": "각자 다르지",
          "original_form": "각자 다르지",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002353.1.21",
          "form": "나는 타이레놀 먹으면 좀 낫더라",
          "original_form": "나는 타이레놀 먹으면 좀 낫더라",
          "speaker_id": "1",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002353.1.22",
          "form": "회전근은 어깨 염증이고",
          "original_form": "회전근은 어깨 염증이고",
          "speaker_id": "2",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.23",
          "form": "아니 회전근 말고",
          "original_form": "아니 회전근 말고",
          "speaker_id": "1",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.24",
          "form": "목이랑 두통은 힘 잘못 들어간거고",
          "original_form": "목이랑 두통은 힘 잘못 들어간거고",
          "speaker_id": "2",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.25",
          "form": "목이랑 승모랑",
          "original_form": "목이랑 승모랑",
          "speaker_id": "1",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.26",
          "form": "근육긴장으로",
          "original_form": "근육긴장으로",
          "speaker_id": "1",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.27",
          "form": "그건 다 같지",
          "original_form": "그건 다 같지",
          "speaker_id": "2",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.28",
          "form": "근데 우리센터 강사도",
          "original_form": "근데 우리센터 강사도",
          "speaker_id": "1",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.29",
          "form": "타이레놀은 진통제니까",
          "original_form": "타이레놀은 진통제니까",
          "speaker_id": "2",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.30",
          "form": "그래서 운동끝나면",
          "original_form": "그래서 운동끝나면",
          "speaker_id": "1",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002353.1.31",
          "form": "어깨스트레칭하고가라고해 나한테",
          "original_form": "어깨스트레칭하고가라고해 나한테",
          "speaker_id": "1",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002353.1.32",
          "form": "내가 스트레칭 귀찮아서 안하고 운동해서 몸 더 망가진듯",
          "original_form": "내가 스트레칭 귀찮아서 안하고 운동해서 몸 더 망가진듯",
          "speaker_id": "2",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002353.1.33",
          "form": "일단 자세 다시 봐달라고희",
          "original_form": "일단 자세 다시 봐달라고희",
          "speaker_id": "2",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002353.1.34",
          "form": "윗통까고 힘 제대로 들어가는지",
          "original_form": "윗통까고 힘 제대로 들어가는지",
          "speaker_id": "2",
          "time": "20210820 10:19"
        }
      ]
    }
