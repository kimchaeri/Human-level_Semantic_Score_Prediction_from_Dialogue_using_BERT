      "utterance": [
        {
          "id": "MDRW2100044928.1.1",
          "form": "ㅋㅋㄱㄱㅋ 아마 밥때되면 그때 가장 차리기 쉽거나 엄청 끌리는 걸 먹게 되겠죠",
          "original_form": "ㅋㅋㄱㄱㅋ 아마 밥때되면 그때 가장 차리기 쉽거나 엄청 끌리는 걸 먹게 되겠죠",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044928.1.2",
          "form": "name1님은 무얼 드실 예정이신가요?",
          "original_form": "&name1&님은 무얼 드실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044928.1.3",
          "form": "저는 내일 일단 콩나물 무침 해야돼요",
          "original_form": "저는 내일 일단 콩나물 무침 해야돼요",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044928.1.4",
          "form": "콩나물 사왔거든요",
          "original_form": "콩나물 사왔거든요",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044928.1.5",
          "form": "아하 콩나물무침 좋죠",
          "original_form": "아하 콩나물무침 좋죠",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044928.1.6",
          "form": "또 뭐 만드시나요?",
          "original_form": "또 뭐 만드시나요?",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044928.1.7",
          "form": "또 오묵탕 할려구요",
          "original_form": "또 오묵탕 할려구요",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044928.1.8",
          "form": "와 어묵탕 맛있겠네요",
          "original_form": "와 어묵탕 맛있겠네요",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044928.1.9",
          "form": "저희 집에서 가까운 지하철역에 오뎅집 입점해있거든요",
          "original_form": "저희 집에서 가까운 지하철역에 오뎅집 입점해있거든요",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044928.1.10",
          "form": "지나갈때마다 냄새가 너무 먹고 싶게 하던데",
          "original_form": "지나갈때마다 냄새가 너무 먹고 싶게 하던데",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044928.1.11",
          "form": "헐 지하철역 근처 오뎅집 너무한거 아닌가요",
          "original_form": "헐 지하철역 근처 오뎅집 너무한거 아닌가요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044928.1.12",
          "form": "그니까요 ㅋㅋㅋ 거의 델리만쥬급",
          "original_form": "그니까요 ㅋㅋㅋ 거의 델리만쥬급",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044928.1.13",
          "form": "델리만쥬는 진짜 나빠요 그치않나요",
          "original_form": "델리만쥬는 진짜 나빠요 그치않나요",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044928.1.14",
          "form": "냄새만큼 못따라와요",
          "original_form": "냄새만큼 못따라와요",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044928.1.15",
          "form": "맞아요 진짜 냄새는 제일 맛있는데 말이죠",
          "original_form": "맞아요 진짜 냄새는 제일 맛있는데 말이죠",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044928.1.16",
          "form": "그래서 어렸을때만 사먹고 최근엔 먹어본적이 없는 것 같아요",
          "original_form": "그래서 어렸을때만 사먹고 최근엔 먹어본적이 없는 것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044928.1.17",
          "form": "델리만쥬 어묵 버터구이 오징어 버터옥수수",
          "original_form": "델리만쥬 어묵 버터구이 오징어 버터옥수수",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044928.1.18",
          "form": "누가 제일 나쁜가요",
          "original_form": "누가 제일 나쁜가요",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044928.1.19",
          "form": "와 다 나빠 ㅋㅋㄱㅋㄱ",
          "original_form": "와 다 나빠 ㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044928.1.20",
          "form": "그래도 다른 애들은 맛은 있는데",
          "original_form": "그래도 다른 애들은 맛은 있는데",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044928.1.21",
          "form": "델리만쥬는 냄새사기꾼이니 제일 나쁜걸로요 ㅋㅋㄱ",
          "original_form": "델리만쥬는 냄새사기꾼이니 제일 나쁜걸로요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044928.1.22",
          "form": "아그리고 제일 나쁜 이유가 있어요",
          "original_form": "아그리고 제일 나쁜 이유가 있어요",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044928.1.23",
          "form": "뭔지아세요?",
          "original_form": "뭔지아세요?",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044928.1.24",
          "form": "뭔데요?",
          "original_form": "뭔데요?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044928.1.25",
          "form": "밀폐된 지하 안에서 냄새를 피우잖아요",
          "original_form": "밀폐된 지하 안에서 냄새를 피우잖아요",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044928.1.26",
          "form": "도망도 못가요",
          "original_form": "도망도 못가요",
          "speaker_id": "1",
          "time": "20210908 00:30"
        }
