        },
        {
          "id": "MDRW2100018956.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100018956.1.3",
          "form": "쇼핑 좋아하시나요?",
          "original_form": "쇼핑 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100018956.1.4",
          "form": "아니요 그냥 필요한것 위주로만 사요",
          "original_form": "아니요 그냥 필요한것 위주로만 사요",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100018956.1.5",
          "form": "name1님은 쇼핑 자주하세요?",
          "original_form": "&name1&님은 쇼핑 자주하세요?",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100018956.1.6",
          "form": "새로운 물건 나왔나 자주 살펴보는 편이에요!",
          "original_form": "새로운 물건 나왔나 자주 살펴보는 편이에요!",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100018956.1.7",
          "form": "주로 어떤 거 구매하세요?",
          "original_form": "주로 어떤 거 구매하세요?",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100018956.1.8",
          "form": "모든 카테고리에서 다 사는 듯해요",
          "original_form": "모든 카테고리에서 다 사는 듯해요",
          "speaker_id": "2",
          "time": "20210830 10:41"
        },
        {
          "id": "MDRW2100018956.1.9",
          "form": "자급자족할 수 있는게 하나도 없어서",
          "original_form": "자급자족할 수 있는게 하나도 없어서",
          "speaker_id": "2",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100018956.1.10",
          "form": "ㅋㅋㅋ 제일 최근에 구매한 건 어떤 건가요?",
          "original_form": "ㅋㅋㅋ 제일 최근에 구매한 건 어떤 건가요?",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100018956.1.11",
          "form": "저 골드키위요ㅋㅋ",
          "original_form": "저 골드키위요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100018956.1.12",
          "form": "앗 골드키위 지금 나오나요??",
          "original_form": "앗 골드키위 지금 나오나요??",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100018956.1.13",
          "form": "가는 길에 마트 들러야겠어요 ㅋㅋㅋㅋ",
          "original_form": "가는 길에 마트 들러야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100018956.1.14",
          "form": "네 아주 맛나더라구요",
          "original_form": "네 아주 맛나더라구요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100018956.1.15",
          "form": "요즘은 딱히 농산물 제철이라는게 없나봐요",
          "original_form": "요즘은 딱히 농산물 제철이라는게 없나봐요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100018956.1.16",
          "form": "신선하고 달콤했어요",
          "original_form": "신선하고 달콤했어요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100018956.1.17",
          "form": "마트를 잘 안가니까 과일철을 놓치네요 ㅎㅎ",
          "original_form": "마트를 잘 안가니까 과일철을 놓치네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100018956.1.18",
          "form": "요즘은 구독서비스라고 주마다 알아서 과일",
          "original_form": "요즘은 구독서비스라고 주마다 알아서 과일",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100018956.1.19",
          "form": "보내주는 쇼핑사이트들도 생겼더라구요 신기했어요",
          "original_form": "보내주는 쇼핑사이트들도 생겼더라구요 신기했어요",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100018956.1.20",
          "form": "오 그런 구독서비스들 알아 놓으면 정말 좋을 것 같아요!",
          "original_form": "오 그런 구독서비스들 알아 놓으면 정말 좋을 것 같아요!",
          "speaker_id": "1",
          "time": "20210830 20:12"
        }
      ]
