      "utterance": [
        {
          "id": "MDRW2100006328.1.1",
          "form": "읍소에 거의 넘어오셨을때쯤 다른 분이 갑자기 등장해서 칼 거절 하시도라구요",
          "original_form": "읍소에 거의 넘어오셨을때쯤 다른 분이 갑자기 등장해서 칼 거절 하시도라구요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006328.1.2",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006328.1.3",
          "form": "그분만 안왔더라면...!",
          "original_form": "그분만 안왔더라면...!",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006328.1.4",
          "form": "아유 저희만요",
          "original_form": "아유 저희만요",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006328.1.5",
          "form": "이게 됐을텐데",
          "original_form": "이게 됐을텐데",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006328.1.6",
          "form": "그니까요 그 분만 아니었더라면!!!",
          "original_form": "그니까요 그 분만 아니었더라면!!!",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006328.1.7",
          "form": "ㅋㅋㅋㅋ읍소하면 들여보내준다고 어디 글 안쓸게뇨!!",
          "original_form": "ㅋㅋㅋㅋ읍소하면 들여보내준다고 어디 글 안쓸게뇨!!",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
