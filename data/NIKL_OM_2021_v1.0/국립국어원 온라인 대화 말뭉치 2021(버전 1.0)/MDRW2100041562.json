      "utterance": [
        {
          "id": "MDRW2100041562.1.1",
          "form": "어디서 사는건가요?",
          "original_form": "어디서 사는건가요?",
          "speaker_id": "2",
          "time": "20210906 10:49"
        },
        {
          "id": "MDRW2100041562.1.2",
          "form": "저는 이마트에서 주로 냉동닭가슴살사요",
          "original_form": "저는 이마트에서 주로 냉동닭가슴살사요",
          "speaker_id": "1",
          "time": "20210906 11:20"
        },
        {
          "id": "MDRW2100041562.1.3",
          "form": "와 저도 그거 봤는데 좋을것같네요",
          "original_form": "와 저도 그거 봤는데 좋을것같네요",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041562.1.4",
          "form": "추르 보다 좋아요",
          "original_form": "추르 보다 좋아요",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041562.1.5",
          "form": "장난감 추천도 해주세요~!",
          "original_form": "장난감 추천도 해주세요~!",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041562.1.6",
          "form": "저는 털공 좋아하더라구요",
          "original_form": "저는 털공 좋아하더라구요",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041562.1.7",
          "form": "저희는 낚시대 ㅋㅋ",
          "original_form": "저희는 낚시대 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041562.1.8",
          "form": "근데 침대밑에 자꾸 들어간다는 ㅜㅜ",
          "original_form": "근데 침대밑에 자꾸 들어간다는 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041562.1.9",
          "form": "ㅋㅋㅋㅋㅋ 난감쓰",
          "original_form": "ㅋㅋㅋㅋㅋ 난감쓰",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041562.1.10",
          "form": "낚시대는 어디꺼 괜찮나요",
          "original_form": "낚시대는 어디꺼 괜찮나요",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041562.1.11",
          "form": "저는 지구별고양이에서 샀어요",
          "original_form": "저는 지구별고양이에서 샀어요",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041562.1.12",
          "form": "오 저두요 궁디팡팡 가서 샀네요",
          "original_form": "오 저두요 궁디팡팡 가서 샀네요",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041562.1.13",
          "form": "궁디팡팡 좋은거 많던가요?",
          "original_form": "궁디팡팡 좋은거 많던가요?",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041562.1.14",
          "form": "네 저는 좋았어요",
          "original_form": "네 저는 좋았어요",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041562.1.15",
          "form": "근데 부산이라 서울보다 규모가 작은듯요",
