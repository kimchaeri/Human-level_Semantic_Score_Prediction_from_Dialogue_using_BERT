      "utterance": [
        {
          "id": "MDRW2100010018.1.1",
          "form": "난 강아지를 키워",
          "original_form": "난 강아지를 키워",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010018.1.2",
          "form": "좋겠다. 종이 뭐야",
          "original_form": "좋겠다. 종이 뭐야",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010018.1.3",
          "form": "푸들이야",
          "original_form": "푸들이야",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010018.1.4",
          "form": "이름이 뭐야",
          "original_form": "이름이 뭐야",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010018.1.5",
          "form": "name3야 ㅋ",
          "original_form": "&name3&야 ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010018.1.6",
          "form": "name3 아님 name3야?",
          "original_form": "&name3& 아님 &name3&야?",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100010018.1.7",
          "form": "name3!",
          "original_form": "&name3&!",
          "speaker_id": "1",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100010018.1.8",
          "form": "그래서 닉네임이 name3야 구나",
          "original_form": "그래서 닉네임이 &name3&야 구나",
          "speaker_id": "2",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010018.1.9",
          "form": "응 맞아!",
          "original_form": "응 맞아!",
          "speaker_id": "1",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010018.1.10",
          "form": "name3는 몇 살이야",
          "original_form": "&name3&는 몇 살이야",
          "speaker_id": "2",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010018.1.11",
          "form": "6살이야",
          "original_form": "6살이야",
          "speaker_id": "1",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010018.1.12",
          "form": "오래 산건가..",
          "original_form": "오래 산건가..",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.13",
          "form": "반년 살았지?",
          "original_form": "반년 살았지?",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.14",
          "form": "앞으로 반년 남았어",
          "original_form": "앞으로 반년 남았어",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.15",
          "form": "아 미안해",
          "original_form": "아 미안해",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.16",
          "form": "ㅣ내가 동물키워본적이 없어서",
          "original_form": "ㅣ내가 동물키워본적이 없어서",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.17",
          "form": "실례했네",
          "original_form": "실례했네",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.18",
          "form": "아니야 괜찮아",
          "original_form": "아니야 괜찮아",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.19",
          "form": "산책 자주 나가?",
          "original_form": "산책 자주 나가?",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010018.1.20",
          "form": "못나가 코로나 때문에..",
          "original_form": "못나가 코로나 때문에..",
          "speaker_id": "1",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100010018.1.21",
          "form": "name3 똑똑할 것 같아",
          "original_form": "&name3& 똑똑할 것 같아",
          "speaker_id": "2",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010018.1.22",
          "form": "훈련 잘 시키니",
          "original_form": "훈련 잘 시키니",
          "speaker_id": "2",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010018.1.23",
          "form": "그럼! 푸들은 똑똑해",
          "original_form": "그럼! 푸들은 똑똑해",
          "speaker_id": "1",
          "time": "20210823 13:25"
        }
      ]
