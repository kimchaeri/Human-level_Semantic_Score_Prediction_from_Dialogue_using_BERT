      "utterance": [
        {
          "id": "MDRW2100014790.1.1",
          "form": "저 재밌다고 하는 영화는 다운받아서 보는편이에요!",
          "original_form": "저 재밌다고 하는 영화는 다운받아서 보는편이에요!",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014790.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014790.1.3",
          "form": "저는 코로나 전엔 한달에 한번 영화관 갔었어요",
          "original_form": "저는 코로나 전엔 한달에 한번 영화관 갔었어요",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100014790.1.4",
          "form": "와 진짜 자주보셨구나... 지금은 어떻게보세요?",
          "original_form": "와 진짜 자주보셨구나... 지금은 어떻게보세요?",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100014790.1.5",
          "form": "지금은 안 가서 뭔 영화 나오는지 잘 몰라요ㅋㅋㅋ",
          "original_form": "지금은 안 가서 뭔 영화 나오는지 잘 몰라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100014790.1.6",
          "form": "근데 모가디슈는 봤어요",
          "original_form": "근데 모가디슈는 봤어요",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100014790.1.7",
          "form": "앗 모가디슈 재밌나요? 평이 좋던데요!",
          "original_form": "앗 모가디슈 재밌나요? 평이 좋던데요!",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100014790.1.8",
          "form": "저는 사실 그저 그랬어요",
          "original_form": "저는 사실 그저 그랬어요",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100014790.1.9",
          "form": "그냥 남북 이야기? 총싸움 좋아하면 재밌게 볼 순 있는데 전 피하는 것만 나와서 그닥이었어요",
          "original_form": "그냥 남북 이야기? 총싸움 좋아하면 재밌게 볼 순 있는데 전 피하는 것만 나와서 그닥이었어요",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100014790.1.10",
          "form": "헣 그래요? 호불호가 좀 갈리는군요...!",
          "original_form": "헣 그래요? 호불호가 좀 갈리는군요...!",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100014790.1.11",
          "form": "name1님 최애영화 있나요?",
          "original_form": "&name1&님 최애영화 있나요?",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100014790.1.12",
          "form": "저 최근 영화로는",
          "original_form": "저 최근 영화로는",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100014790.1.13",
          "form": "작은아씨들 진짜 좋아했아요",
          "original_form": "작은아씨들 진짜 좋아했아요",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100014790.1.14",
          "form": "영화관에서 4번이나 봤으니까요",
          "original_form": "영화관에서 4번이나 봤으니까요",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100014790.1.15",
          "form": "하 저도요",
          "original_form": "하 저도요",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100014790.1.16",
          "form": "작은아씨들...영화관에서 본 머지막 영화인데",
          "original_form": "작은아씨들...영화관에서 본 머지막 영화인데",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100014790.1.17",
          "form": "너무좋은거예요ㅠ하",
          "original_form": "너무좋은거예요ㅠ하",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100014790.1.18",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100014790.1.19",
          "form": "저 조 진짜 좋아해요",
          "original_form": "저 조 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100014790.1.20",
          "form": "조처럼 살고 싶달까,,",
          "original_form": "조처럼 살고 싶달까,,",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100014790.1.21",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100014790.1.22",
          "form": "저도요...",
          "original_form": "저도요...",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100014790.1.23",
          "form": "저 진짜 결말을 너무 좋아했거든요",
          "original_form": "저 진짜 결말을 너무 좋아했거든요",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100014790.1.24",
          "form": "조가 결혼하지 않았다는 결말",
          "original_form": "조가 결혼하지 않았다는 결말",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100014790.1.25",
          "form": "로리와 이어지지 않았다는 것도요",
          "original_form": "로리와 이어지지 않았다는 것도요",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100014790.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100014790.1.27",
          "form": "역시 관전포인트가 통하는...",
          "original_form": "역시 관전포인트가 통하는...",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100014790.1.28",
          "form": "로리랑 이어지지 않은 거 너무 좋고",
          "original_form": "로리랑 이어지지 않은 거 너무 좋고",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100014790.1.29",
          "form": "결혼 안 한 것도 좋아요",
          "original_form": "결혼 안 한 것도 좋아요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100014790.1.30",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100014790.1.31",
          "form": "너무너무 외롭다고 말했을 때",
          "original_form": "너무너무 외롭다고 말했을 때",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100014790.1.32",
          "form": "저는 그게 로맨스를 원한다는 말로 들리진 않았어요",
          "original_form": "저는 그게 로맨스를 원한다는 말로 들리진 않았어요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100014790.1.33",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100014790.1.34",
          "form": "그 외로움은 로맨스가 아니란말이에요",
          "original_form": "그 외로움은 로맨스가 아니란말이에요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100014790.1.35",
          "form": "조의 상황에서는 충분히 외로움이 올만한 상황이었잖아요",
          "original_form": "조의 상황에서는 충분히 외로움이 올만한 상황이었잖아요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100014790.1.36",
          "form": "환경이 계속 바뀌고있고",
          "original_form": "환경이 계속 바뀌고있고",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100014790.1.37",
          "form": "맞아요 자매들이 다 떠나가고",
          "original_form": "맞아요 자매들이 다 떠나가고",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100014790.1.38",
          "form": "하 진짜...관전포인트 같아서 너무 기뻐요....",
          "original_form": "하 진짜...관전포인트 같아서 너무 기뻐요....",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100014790.1.39",
          "form": "어떻게 보면 조만 그대로였던..",
          "original_form": "어떻게 보면 조만 그대로였던..",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100014790.1.40",
          "form": "저도요 저 이런 얘기 통하는 사람 처음 봐요",
          "original_form": "저도요 저 이런 얘기 통하는 사람 처음 봐요",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100014790.1.41",
          "form": "제가 진짜진짜 좋아하는데",
          "original_form": "제가 진짜진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100014790.1.42",
          "form": "저만큼 작은아씨들을 좋아하는 친구는 없더라구요",
          "original_form": "저만큼 작은아씨들을 좋아하는 친구는 없더라구요",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100014790.1.43",
          "form": "그래서 이런 얘기 나눠본 적이 없어요",
          "original_form": "그래서 이런 얘기 나눠본 적이 없어요",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100014790.1.44",
          "form": "헉 진짜로요 진짜",
          "original_form": "헉 진짜로요 진짜",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100014790.1.45",
          "form": "조의 외로움을 다들 결혼? 연애? 그걸로 생각하는 거예요",
          "original_form": "조의 외로움을 다들 결혼? 연애? 그걸로 생각하는 거예요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100014790.1.46",
          "form": "맞아요 근데 그게 아니란 말이에요,,",
          "original_form": "맞아요 근데 그게 아니란 말이에요,,",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100014790.1.47",
          "form": "근데 저기서 오는 외로움은 그게 아니라 조만 그대로고 다 떠나간 상황이잖아요\r\n언니는 결혼 베스의 죽음 에이미의 파리행 그리고... 로리의 마음변호ㅓ",
          "original_form": "근데 저기서 오는 외로움은 그게 아니라 조만 그대로고 다 떠나간 상황이잖아요\r\n언니는 결혼 베스의 죽음 에이미의 파리행 그리고... 로리의 마음변호ㅓ",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100014790.1.48",
          "form": "당연히 조가 외로울 수 밖에 없는데도 그걸 연애라고 치부하는게 너무 싫었어요",
          "original_form": "당연히 조가 외로울 수 밖에 없는데도 그걸 연애라고 치부하는게 너무 싫었어요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100014790.1.49",
          "form": "하ㅠ",
          "original_form": "하ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100014790.1.50",
          "form": "그죠 진짜",
          "original_form": "그죠 진짜",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100014790.1.51",
          "form": "저랑 생각 완전 일치하시는",
          "original_form": "저랑 생각 완전 일치하시는",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100014790.1.52",
          "form": "진짜 대박이다..",
          "original_form": "진짜 대박이다..",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100014790.1.53",
          "form": "그냥 입버릇처럼 하는 말인데",
          "original_form": "그냥 입버릇처럼 하는 말인데",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100014790.1.54",
          "form": "조처럼 살고싶다는 말 해요",
          "original_form": "조처럼 살고싶다는 말 해요",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100014790.1.55",
          "form": "진짜로 말하는 건 아니고.. sns에 ㅋㅋㅋ",
          "original_form": "진짜로 말하는 건 아니고.. sns에 ㅋㅋㅋ",
