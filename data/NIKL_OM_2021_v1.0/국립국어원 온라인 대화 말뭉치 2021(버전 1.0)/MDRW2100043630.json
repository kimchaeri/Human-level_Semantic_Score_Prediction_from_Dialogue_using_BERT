      "utterance": [
        {
          "id": "MDRW2100043630.1.1",
          "form": "유툽같은거보시나요",
          "original_form": "유툽같은거보시나요",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043630.1.2",
          "form": "네 봐요. 동물 채널이나 예능클립이요",
          "original_form": "네 봐요. 동물 채널이나 예능클립이요",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043630.1.3",
          "form": "세상에 망나니개가 그리많은지몰랐네요",
          "original_form": "세상에 망나니개가 그리많은지몰랐네요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043630.1.4",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043630.1.5",
          "form": "못된개가 그리많은지몰랐네요",
          "original_form": "못된개가 그리많은지몰랐네요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043630.1.6",
          "form": "나쁜말 보내지말라고뜨네요ㅋㅋ",
          "original_form": "나쁜말 보내지말라고뜨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043630.1.7",
          "form": "ㅋㅋㅋㅋ개훌륭 보세요?",
          "original_form": "ㅋㅋㅋㅋ개훌륭 보세요?",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043630.1.8",
          "form": "최근에 관심있어서 몰아서봤어요",
          "original_form": "최근에 관심있어서 몰아서봤어요",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043630.1.9",
          "form": "개키우고싶어서봤는데",
          "original_form": "개키우고싶어서봤는데",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043630.1.10",
          "form": "개키우고싶은사람은",
          "original_form": "개키우고싶은사람은",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043630.1.11",
          "form": "보면안되겠더라구요",
          "original_form": "보면안되겠더라구요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043630.1.12",
          "form": "ㅋㅋㅋ준비를 잘 해야 키울 수 있을것 같더라구요",
          "original_form": "ㅋㅋㅋ준비를 잘 해야 키울 수 있을것 같더라구요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043630.1.13",
          "form": "그쵸 개훌륭 보세요?",
          "original_form": "그쵸 개훌륭 보세요?",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043630.1.14",
          "form": "네 가끔 보고있어요",
          "original_form": "네 가끔 보고있어요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043630.1.15",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043630.1.16",
          "form": "금쪽이같은것도 자주보구요",
          "original_form": "금쪽이같은것도 자주보구요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043630.1.17",
          "form": "금쪽이는 클립으로 뜨는 거만 봐요",
          "original_form": "금쪽이는 클립으로 뜨는 거만 봐요",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043630.1.18",
          "form": "애들 혼내고 소리지르는거 보기 힘들더라구요",
          "original_form": "애들 혼내고 소리지르는거 보기 힘들더라구요",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043630.1.19",
          "form": "아 저는 약간 직업이 그쪽이라서",
          "original_form": "아 저는 약간 직업이 그쪽이라서",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043630.1.20",
          "form": "맨날 보는애들중에 저런애들이있어서",
          "original_form": "맨날 보는애들중에 저런애들이있어서",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043630.1.21",
          "form": "걔들을 좀 이해해볼까 싶어서 봤어요ㅋㅋ",
          "original_form": "걔들을 좀 이해해볼까 싶어서 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043630.1.22",
          "form": "이해는안되지만",
          "original_form": "이해는안되지만",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043630.1.23",
          "form": "엄마들 공감은 좀 되더라구요",
          "original_form": "엄마들 공감은 좀 되더라구요",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043630.1.24",
          "form": "육아하는거 넘 힘들어보이긴 하더라구요ㅋㅋ",
          "original_form": "육아하는거 넘 힘들어보이긴 하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043630.1.25",
          "form": "그쵸 전 육아는 안해봐서",
          "original_form": "그쵸 전 육아는 안해봐서",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043630.1.26",
          "form": "그런 모습보면 진짜 혼을 쏙뺄거같은데",
          "original_form": "그런 모습보면 진짜 혼을 쏙뺄거같은데",
          "speaker_id": "1",
          "time": "20210907 20:27"
        }
      ]
    }
