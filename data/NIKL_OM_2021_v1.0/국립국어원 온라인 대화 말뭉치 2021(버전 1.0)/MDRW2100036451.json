      "utterance": [
        {
          "id": "MDRW2100036451.1.1",
          "form": "집안일은 많이 하시나요?",
          "original_form": "집안일은 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036451.1.2",
          "form": "자주는 안하고 몰아서 하는 타입이에요 ㅎ",
          "original_form": "자주는 안하고 몰아서 하는 타입이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036451.1.3",
          "form": "저도요ㅋ 너무 싫어욧",
          "original_form": "저도요ㅋ 너무 싫어욧",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036451.1.4",
          "form": "저녁 먹은 설거지가 아직 설거지통에 쌓여 있어요 ㅎ",
          "original_form": "저녁 먹은 설거지가 아직 설거지통에 쌓여 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036451.1.5",
          "form": "앗 동지에요ㅋ",
          "original_form": "앗 동지에요ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036451.1.6",
          "form": "name1님도 아직 설거지가 쌓여 있으신가봐요 ㅎ",
          "original_form": "&name1&님도 아직 설거지가 쌓여 있으신가봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036451.1.7",
          "form": "내일 할래여ㅋㅋ",
          "original_form": "내일 할래여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036451.1.8",
          "form": "저두 ㅎㅎ 너무 귀찮아요",
          "original_form": "저두 ㅎㅎ 너무 귀찮아요",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036451.1.9",
          "form": "설거지 로봇 있었으면ㅠ",
          "original_form": "설거지 로봇 있었으면ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036451.1.10",
          "form": "ㅋㅋ 식기세척기가 요즘 잘 씻기긴 한다고 하더라고요",
          "original_form": "ㅋㅋ 식기세척기가 요즘 잘 씻기긴 한다고 하더라고요",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036451.1.11",
          "form": "그거있는데 넣다뺐다 귀찮네요ㅋ",
          "original_form": "그거있는데 넣다뺐다 귀찮네요ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100036451.1.12",
          "form": "아~식세기 있으시군요! 잘 안쓰시나요?",
          "original_form": "아~식세기 있으시군요! 잘 안쓰시나요?",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100036451.1.13",
          "form": "쓰긴하는데 그것도 귀찮은ㅠ",
          "original_form": "쓰긴하는데 그것도 귀찮은ㅠ",
