      "utterance": [
        {
          "id": "MDRW2100030493.1.1",
          "form": "요즘 넷플릭스로 보는거 있어요?",
          "original_form": "요즘 넷플릭스로 보는거 있어요?",
          "speaker_id": "1",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030493.1.2",
          "form": "넷플릭스 잘 안 보긴 하는데",
          "original_form": "넷플릭스 잘 안 보긴 하는데",
          "speaker_id": "2",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030493.1.3",
          "form": "슬의생2 완결나면 보려고 기다리는 중이에요",
          "original_form": "슬의생2 완결나면 보려고 기다리는 중이에요",
          "speaker_id": "2",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030493.1.4",
          "form": "오 슬의생1은 보셨어요?",
          "original_form": "오 슬의생1은 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030493.1.5",
          "form": "네 그건 완결나고 정주행 했어요",
          "original_form": "네 그건 완결나고 정주행 했어요",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030493.1.6",
          "form": "슬의생 보셨어요?",
          "original_form": "슬의생 보셨어요?",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030493.1.7",
          "form": "네 저는 넷플 뜨자마자 보는편이에요!",
          "original_form": "네 저는 넷플 뜨자마자 보는편이에요!",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030493.1.8",
          "form": "우와 요즘은 뭐 보세요?",
          "original_form": "우와 요즘은 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030493.1.9",
          "form": "요즘은 드라마 괴물 봤어요",
          "original_form": "요즘은 드라마 괴물 봤어요",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030493.1.10",
          "form": "이 드라마 아세요?",
          "original_form": "이 드라마 아세요?",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030493.1.11",
          "form": "이름은 들어봤는데 안 봤어요!",
          "original_form": "이름은 들어봤는데 안 봤어요!",
          "speaker_id": "2",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030493.1.12",
          "form": "여진구 나오는 거죠",
          "original_form": "여진구 나오는 거죠",
          "speaker_id": "2",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030493.1.13",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030493.1.14",
          "form": "이것도 재미있더라고요",
          "original_form": "이것도 재미있더라고요",
          "speaker_id": "1",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030493.1.15",
          "form": "재밌다는 얘기는 들었는데",
          "original_form": "재밌다는 얘기는 들었는데",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030493.1.16",
          "form": "저 여진구 나오는 건 왕이된남잔가 그거 먼저 보고 싶어서",
          "original_form": "저 여진구 나오는 건 왕이된남잔가 그거 먼저 보고 싶어서",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030493.1.17",
          "form": "이거 보셨어요?",
          "original_form": "이거 보셨어요?",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030493.1.18",
          "form": "전 아직 그거 안봤어요",
          "original_form": "전 아직 그거 안봤어요",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030493.1.19",
          "form": "드라마에요 영화에요??",
          "original_form": "드라마에요 영화에요??",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030493.1.20",
          "form": "드라마요!",
          "original_form": "드라마요!",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030493.1.21",
          "form": "넷플릭스에 얼마전에 올라왔더라구요",
          "original_form": "넷플릭스에 얼마전에 올라왔더라구요",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030493.1.22",
          "form": "아 곧 봐야겠어요!!",
          "original_form": "아 곧 봐야겠어요!!",
          "speaker_id": "1",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030493.1.23",
          "form": "좋아하는 드라마 장르 있어요?",
          "original_form": "좋아하는 드라마 장르 있어요?",
          "speaker_id": "1",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030493.1.24",
          "form": "드라마 진짜 잘 안 봐서 장르는 잘 안 가리고",
          "original_form": "드라마 진짜 잘 안 봐서 장르는 잘 안 가리고",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030493.1.25",
          "form": "그냥 재밌는 거 보는 것 같아요",
          "original_form": "그냥 재밌는 거 보는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030493.1.26",
          "form": "제일 재밌게 본 건 동백꽃 필 무렵인가 그거였어요",
          "original_form": "제일 재밌게 본 건 동백꽃 필 무렵인가 그거였어요",
          "speaker_id": "2",
          "time": "20210904 02:16"
        }
      ]
    }
