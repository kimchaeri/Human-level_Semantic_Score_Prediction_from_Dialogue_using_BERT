      "utterance": [
        {
          "id": "MDRW2100040822.1.1",
          "form": "그리고 백신 맞아도 모르나 걸릴 수 있다 던데요",
          "original_form": "그리고 백신 맞아도 모르나 걸릴 수 있다 던데요",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040822.1.2",
          "form": "네 걸리는데 중증으로는 안간다 그러더라고요",
          "original_form": "네 걸리는데 중증으로는 안간다 그러더라고요",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040822.1.3",
          "form": "백신접종 해도 조심해야죠",
          "original_form": "백신접종 해도 조심해야죠",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040822.1.4",
          "form": "코로나 걸려도 운이고 백신 맞아도 운이고",
          "original_form": "코로나 걸려도 운이고 백신 맞아도 운이고",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040822.1.5",
          "form": "난감한 병 이네요",
          "original_form": "난감한 병 이네요",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040822.1.6",
          "form": "네네 코로나 치료제 나올때까진",
          "original_form": "네네 코로나 치료제 나올때까진",
          "speaker_id": "2",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100040822.1.7",
          "form": "계속 조심해야할듯",
          "original_form": "계속 조심해야할듯",
          "speaker_id": "2",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100040822.1.8",
          "form": "이런 병을 퍼뜨려 놓고",
          "original_form": "이런 병을 퍼뜨려 놓고",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100040822.1.9",
          "form": "중국 욕하면 혐오라고 하는게 말이 되나요",
          "original_form": "중국 욕하면 혐오라고 하는게 말이 되나요",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100040822.1.10",
          "form": "코로나 이렇게 오래 갈지 몰랐는데",
          "original_form": "코로나 이렇게 오래 갈지 몰랐는데",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100040822.1.11",
          "form": "장사하는 분들 힘들 거 같더라고요",
          "original_form": "장사하는 분들 힘들 거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100040822.1.12",
          "form": "근데 또 코로나 때문에 엄청 잘 되는데도 있으니까",
          "original_form": "근데 또 코로나 때문에 엄청 잘 되는데도 있으니까",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100040822.1.13",
          "form": "어이 없죠 안타깝구요",
          "original_form": "어이 없죠 안타깝구요",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100040822.1.14",
          "form": "네 얼른 코로나가 잠잠해지고",
          "original_form": "네 얼른 코로나가 잠잠해지고",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040822.1.15",
          "form": "일상생활도 돌아오면 좋겠어요",
          "original_form": "일상생활도 돌아오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040822.1.16",
          "form": "그러니까요 내년에는 가능할까요",
          "original_form": "그러니까요 내년에는 가능할까요",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040822.1.17",
          "form": "거의 2년이 됐잖아요",
          "original_form": "거의 2년이 됐잖아요",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040822.1.18",
          "form": "내년엔 코로나 없는 세상이 되면 좋겠어요",
          "original_form": "내년엔 코로나 없는 세상이 되면 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100040822.1.19",
          "form": "정말요 풀어나가 사라져서",
          "original_form": "정말요 풀어나가 사라져서",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100040822.1.20",
          "form": "코로나가",
          "original_form": "코로나가",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100040822.1.21",
          "form": "마스크 좀 빼고 싶어요",
          "original_form": "마스크 좀 빼고 싶어요",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100040822.1.22",
          "form": "네 마스크 쓰니까 피부트러블도 생기고",
          "original_form": "네 마스크 쓰니까 피부트러블도 생기고",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100040822.1.23",
          "form": "저는 사무실에서도 내내 써야해서",
          "original_form": "저는 사무실에서도 내내 써야해서",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100040822.1.24",
          "form": "힘들더라고요ㅜ",
          "original_form": "힘들더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100040822.1.25",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "1",
          "time": "20210905 09:08"
        }
      ]
    }
  ]
