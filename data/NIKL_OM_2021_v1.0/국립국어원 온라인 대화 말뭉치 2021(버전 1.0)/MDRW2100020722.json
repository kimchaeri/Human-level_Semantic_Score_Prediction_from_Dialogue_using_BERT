      "utterance": [
        {
          "id": "MDRW2100020722.1.1",
          "form": "무슨맛이 느껴지세요",
          "original_form": "무슨맛이 느껴지세요",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020722.1.2",
          "form": "뭔가 특이한 향이 나는데",
          "original_form": "뭔가 특이한 향이 나는데",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020722.1.3",
          "form": "설명하기 힘드네요",
          "original_form": "설명하기 힘드네요",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020722.1.4",
          "form": "약간 전 카레같은데 이상한맛",
          "original_form": "약간 전 카레같은데 이상한맛",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.5",
          "form": "저는 해물이 아닌 모든 것에 관대해요",
          "original_form": "저는 해물이 아닌 모든 것에 관대해요",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.6",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.7",
          "form": "카레처럼 향이 확 쎈느낌",
          "original_form": "카레처럼 향이 확 쎈느낌",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.8",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.9",
          "form": "근데 뒤에 뭔가 카레아닌향신료맛",
          "original_form": "근데 뒤에 뭔가 카레아닌향신료맛",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.10",
          "form": "샤브샤브는 좋아하시나요",
          "original_form": "샤브샤브는 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.11",
          "form": "훠궈나",
          "original_form": "훠궈나",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.12",
          "form": "당연하죠 샤브샤브만요",
          "original_form": "당연하죠 샤브샤브만요",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.13",
          "form": "양고기 안먹어봤어요",
          "original_form": "양고기 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020722.1.14",
          "form": "엇 저두요ㅋㅋㅋ 훠궈는 별로더라구요",
          "original_form": "엇 저두요ㅋㅋㅋ 훠궈는 별로더라구요",
          "speaker_id": "1",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020722.1.15",
          "form": "훠거 무슨맛이에요",
          "original_form": "훠거 무슨맛이에요",
          "speaker_id": "2",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020722.1.16",
          "form": "마라탕이랑도 또 다른데",
          "original_form": "마라탕이랑도 또 다른데",
          "speaker_id": "1",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020722.1.17",
          "form": "지나갈 때 마다 양냄새? 되게 강하게나서",
          "original_form": "지나갈 때 마다 양냄새? 되게 강하게나서",
          "speaker_id": "2",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020722.1.18",
          "form": "샤브샤브에 밍밍한 향신료가 들어가있어요",
          "original_form": "샤브샤브에 밍밍한 향신료가 들어가있어요",
          "speaker_id": "1",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020722.1.19",
          "form": "으음...",
          "original_form": "으음...",
          "speaker_id": "2",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020722.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:40"
        },
        {
          "id": "MDRW2100020722.1.21",
          "form": "그 향만으로도 진입장벽이 생기죠",
          "original_form": "그 향만으로도 진입장벽이 생기죠",
          "speaker_id": "1",
