        },
        {
          "id": "MDRW2100030616.1.2",
          "form": "날씨 겁나좋아요",
          "original_form": "날씨 겁나좋아요",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030616.1.3",
          "form": "여긴 서울입니당",
          "original_form": "여긴 서울입니당",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030616.1.4",
          "form": "대구요",
          "original_form": "대구요",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030616.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100030616.1.6",
          "form": "여긴 하늘도 퍼렇고 여행가기 딱좋은 날씨네요",
          "original_form": "여긴 하늘도 퍼렇고 여행가기 딱좋은 날씨네요",
          "speaker_id": "2",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030616.1.7",
          "form": "부럽다요",
          "original_form": "부럽다요",
          "speaker_id": "1",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030616.1.8",
          "form": "대구는 미녀가 많다던데",
          "original_form": "대구는 미녀가 많다던데",
          "speaker_id": "2",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030616.1.9",
          "form": "사실입니까",
          "original_form": "사실입니까",
          "speaker_id": "2",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030616.1.10",
          "form": "소문이요",
          "original_form": "소문이요",
          "speaker_id": "1",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030616.1.11",
          "form": "소문이었군요",
          "original_form": "소문이었군요",
          "speaker_id": "2",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030616.1.12",
          "form": "이번여름 대구는 어땠나요",
          "original_form": "이번여름 대구는 어땠나요",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030616.1.13",
          "form": "불지옥이여",
          "original_form": "불지옥이여",
          "speaker_id": "1",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030616.1.14",
          "form": "디아블로한편 찍었군",
          "original_form": "디아블로한편 찍었군",
          "speaker_id": "2",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030616.1.15",
          "form": "오늘은 집콕합니까",
          "original_form": "오늘은 집콕합니까",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030616.1.16",
          "form": "아마도요",
          "original_form": "아마도요",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030616.1.17",
          "form": "그래도 나가서 바람도쐬고 하세요",
          "original_form": "그래도 나가서 바람도쐬고 하세요",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030616.1.18",
