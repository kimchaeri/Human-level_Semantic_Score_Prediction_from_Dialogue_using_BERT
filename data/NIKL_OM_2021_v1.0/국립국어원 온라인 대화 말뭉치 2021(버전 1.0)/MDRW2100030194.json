      "utterance": [
        {
          "id": "MDRW2100030194.1.1",
          "form": "네 다양한게 좋아야",
          "original_form": "네 다양한게 좋아야",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.2",
          "form": "후식으로 메론수박",
          "original_form": "후식으로 메론수박",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.3",
          "form": "튀긴건강ᆢ",
          "original_form": "튀긴건강ᆢ",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.4",
          "form": "먹고있어요",
          "original_form": "먹고있어요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.5",
          "form": "침나오네요",
          "original_form": "침나오네요",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.6",
          "form": "맥주도마셔요",
          "original_form": "맥주도마셔요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.7",
          "form": "푸짐하군요 기다린보람",
          "original_form": "푸짐하군요 기다린보람",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.8",
          "form": "생맥이진리요",
          "original_form": "생맥이진리요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.9",
          "form": "와 살쪄요",
          "original_form": "와 살쪄요",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.10",
          "form": "감귤시럽도 주네요",
          "original_form": "감귤시럽도 주네요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030194.1.11",
          "form": "배둘레햄 아시나요",
          "original_form": "배둘레햄 아시나요",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.12",
          "form": "탄산수에 타먹음 좋겠네요",
          "original_form": "탄산수에 타먹음 좋겠네요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.13",
          "form": "알지요",
          "original_form": "알지요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.14",
          "form": "예전부ㅌ동거중이에요",
          "original_form": "예전부ㅌ동거중이에요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.15",
          "form": "없던적이없어요",
          "original_form": "없던적이없어요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.16",
          "form": "헤어지기힘든가요",
          "original_form": "헤어지기힘든가요",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.17",
          "form": "평생 같이살생각이에요",
          "original_form": "평생 같이살생각이에요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.18",
          "form": "메론철인가요",
          "original_form": "메론철인가요",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.19",
          "form": "수박이 철이죠",
          "original_form": "수박이 철이죠",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030194.1.20",
          "form": "접목한듯요",
          "original_form": "접목한듯요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        }
      ]
