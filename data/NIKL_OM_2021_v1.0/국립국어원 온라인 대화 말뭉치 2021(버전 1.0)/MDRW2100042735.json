      "utterance": [
        {
          "id": "MDRW2100042735.1.1",
          "form": "아 뭐 보면서 하면 낫겟네요",
          "original_form": "아 뭐 보면서 하면 낫겟네요",
          "speaker_id": "1",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042735.1.2",
          "form": "글고 마늘 많이까면 손이랑 눈이 맵지않나요",
          "original_form": "글고 마늘 많이까면 손이랑 눈이 맵지않나요",
          "speaker_id": "1",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042735.1.3",
          "form": "아예 그냥 자리를 잡습니다.",
          "original_form": "아예 그냥 자리를 잡습니다.",
          "speaker_id": "2",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042735.1.4",
          "form": "니트릴 장갑 끼고 해요",
          "original_form": "니트릴 장갑 끼고 해요",
          "speaker_id": "2",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042735.1.5",
          "form": "장갑ㅋㅋㅋㅋㅋ",
          "original_form": "장갑ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042735.1.6",
          "form": "마늘까기 공장장의 마음으로 하시는군요",
          "original_form": "마늘까기 공장장의 마음으로 하시는군요",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042735.1.7",
          "form": "코스트코에 손가락 사이즈별로 파는 회색 니트릴 장갑을 사서",
          "original_form": "코스트코에 손가락 사이즈별로 파는 회색 니트릴 장갑을 사서",
          "speaker_id": "2",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042735.1.8",
          "form": "그거 은근 쓸모가 좋아요 ㅎㅎ",
          "original_form": "그거 은근 쓸모가 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042735.1.9",
          "form": "손에 딱 맞으니까 마늘을 깝니다.",
          "original_form": "손에 딱 맞으니까 마늘을 깝니다.",
          "speaker_id": "2",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042735.1.10",
          "form": "저도 사야겟어요 장갑낄 생각을 못햇네요",
          "original_form": "저도 사야겟어요 장갑낄 생각을 못햇네요",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042735.1.11",
          "form": "비닐장갑같은걸론 엄두를 못냇거든요",
          "original_form": "비닐장갑같은걸론 엄두를 못냇거든요",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042735.1.12",
          "form": "작은 칼과 니트릴장갑과 그릇 두 개와 모니터 한 개",
          "original_form": "작은 칼과 니트릴장갑과 그릇 두 개와 모니터 한 개",
          "speaker_id": "2",
          "time": "20210907 08:09"
        },
        {
          "id": "MDRW2100042735.1.13",
          "form": "아 마늘 ㅎ",
          "original_form": "아 마늘 ㅎ",
          "speaker_id": "2",
          "time": "20210907 08:09"
        },
        {
          "id": "MDRW2100042735.1.14",
          "form": "알리오 올리오 하기 무서운 데는 다 이유가 있죠 ㅋㅋ",
          "original_form": "알리오 올리오 하기 무서운 데는 다 이유가 있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:09"
        },
        {
          "id": "MDRW2100042735.1.15",
          "form": "마늘이 순식간에 사라져서 ㅋㅋ",
          "original_form": "마늘이 순식간에 사라져서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:09"
        },
        {
          "id": "MDRW2100042735.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 08:09"
        },
        {
          "id": "MDRW2100042735.1.17",
          "form": "열심히 깐 마늘 순식간에 증발..",
          "original_form": "열심히 깐 마늘 순식간에 증발..",
          "speaker_id": "1",
          "time": "20210907 08:10"
        },
        {
          "id": "MDRW2100042735.1.18",
          "form": "여름엔 다 빻아서 큐브로 얼려놓고 쓰거든요.",
          "original_form": "여름엔 다 빻아서 큐브로 얼려놓고 쓰거든요.",
          "speaker_id": "2",
          "time": "20210907 08:10"
        },
        {
          "id": "MDRW2100042735.1.19",
          "form": "아직은 모부가 하시지만 햇마늘은 거의 제가 까는데",
          "original_form": "아직은 모부가 하시지만 햇마늘은 거의 제가 까는데",
          "speaker_id": "2",
          "time": "20210907 08:11"
        },
        {
          "id": "MDRW2100042735.1.20",
          "form": "아오 마늘 깔 땐 인 트리트먼트 시즌3을 봅니다.",
          "original_form": "아오 마늘 깔 땐 인 트리트먼트 시즌3을 봅니다.",
          "speaker_id": "2",
          "time": "20210907 08:11"
        },
        {
          "id": "MDRW2100042735.1.21",
          "form": "비닐장갑 너무 위험하죠 ㄷㄷ",
          "original_form": "비닐장갑 너무 위험하죠 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100042735.1.22",
          "form": "맞아요 빻는것도 진짜귀찮아요",
          "original_form": "맞아요 빻는것도 진짜귀찮아요",
          "speaker_id": "1",
          "time": "20210907 08:13"
        },
        {
          "id": "MDRW2100042735.1.23",
          "form": "그 마늘 빻는 요리기구 보셧나요? 한 알씩 다지게 해주는게 잇더라구요",
          "original_form": "그 마늘 빻는 요리기구 보셧나요? 한 알씩 다지게 해주는게 잇더라구요",
          "speaker_id": "1",
          "time": "20210907 08:13"
        },
        {
          "id": "MDRW2100042735.1.24",
          "form": "대고 누르는 거요?",
          "original_form": "대고 누르는 거요?",
          "speaker_id": "2",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100042735.1.25",
          "form": "부메랑처럼 생긴 건가요",
          "original_form": "부메랑처럼 생긴 건가요",
          "speaker_id": "2",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100042735.1.26",
          "form": "그 한알씩 넣고 누르는 건데 되게 편해보엿어요!",
          "original_form": "그 한알씩 넣고 누르는 건데 되게 편해보엿어요!",
          "speaker_id": "1",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100042735.1.27",
          "form": "설거지는 귀찮겟지만..",
          "original_form": "설거지는 귀찮겟지만..",
          "speaker_id": "1",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100042735.1.28",
          "form": "오오 찾아봐야겠어요 .. 저는 그 때 그 때 막자 사발에 했거든요.",
          "original_form": "오오 찾아봐야겠어요 .. 저는 그 때 그 때 막자 사발에 했거든요.",
