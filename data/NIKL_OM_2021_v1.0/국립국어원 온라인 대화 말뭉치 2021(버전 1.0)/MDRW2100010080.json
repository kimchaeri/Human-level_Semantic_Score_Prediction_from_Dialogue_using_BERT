      "utterance": [
        {
          "id": "MDRW2100010080.1.1",
          "form": "요즘엔 카페가 정말 많더라고요",
          "original_form": "요즘엔 카페가 정말 많더라고요",
          "speaker_id": "2",
          "time": "20210823 14:01"
        },
        {
          "id": "MDRW2100010080.1.2",
          "form": "카페 많죠 회사건물 1층에 커피숍이 열개는되는거같아요",
          "original_form": "카페 많죠 회사건물 1층에 커피숍이 열개는되는거같아요",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100010080.1.3",
          "form": "바로 옆에 붙어있어서 경쟁치열",
          "original_form": "바로 옆에 붙어있어서 경쟁치열",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100010080.1.4",
          "form": "커피좋아하세요?",
          "original_form": "커피좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010080.1.5",
          "form": "네 직접 내려먹기도 해요ㅋㅋ",
          "original_form": "네 직접 내려먹기도 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010080.1.6",
          "form": "좋아한다기 보다 중독이에요ㅠ",
          "original_form": "좋아한다기 보다 중독이에요ㅠ",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010080.1.7",
          "form": "저도 밥먹고 필스코스요",
          "original_form": "저도 밥먹고 필스코스요",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010080.1.8",
          "form": "아메리카노 드시나요?",
          "original_form": "아메리카노 드시나요?",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010080.1.9",
          "form": "네 거의 대부분 아메리카노입니다ㅋㅋ",
          "original_form": "네 거의 대부분 아메리카노입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010080.1.10",
          "form": "라떼류는 텁텁해서 개인적으로 안 좋아해요",
          "original_form": "라떼류는 텁텁해서 개인적으로 안 좋아해요",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010080.1.11",
          "form": "전 달달커피요",
          "original_form": "전 달달커피요",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010080.1.12",
          "form": "아메드시는분들은 다그말씀하시더라구요",
          "original_form": "아메드시는분들은 다그말씀하시더라구요",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010080.1.13",
          "form": "이디야 토피넛 라떼 진짜 달던데",
          "original_form": "이디야 토피넛 라떼 진짜 달던데",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010080.1.14",
          "form": "달달이는 체력회복용ㅋㅋ",
          "original_form": "달달이는 체력회복용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010080.1.15",
          "form": "ㅎㅎ 저 주로 마끼아또아님 바닐라라떼",
          "original_form": "ㅎㅎ 저 주로 마끼아또아님 바닐라라떼",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010080.1.16",
          "form": "물을 잘 안마셔서 아메도 잘 안넘어가요",
          "original_form": "물을 잘 안마셔서 아메도 잘 안넘어가요",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010080.1.17",
          "form": "쓰기만하고",
          "original_form": "쓰기만하고",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010080.1.18",
          "form": "달달이는 진짜",
          "original_form": "달달이는 진짜",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010080.1.19",
          "form": "온몸으로 원할 때가 가끔 있어요ㅋㅋ",
          "original_form": "온몸으로 원할 때가 가끔 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010080.1.20",
          "form": "당떨어질때ㅎ",
          "original_form": "당떨어질때ㅎ",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010080.1.21",
          "form": "말씀 들으니 갑자기 땡기네요ㅎㅎ",
          "original_form": "말씀 들으니 갑자기 땡기네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010080.1.22",
          "form": "전 라떼시럽넣어서 한잔 사왔어요ㅎ",
          "original_form": "전 라떼시럽넣어서 한잔 사왔어요ㅎ",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100010080.1.23",
          "form": "대화제출시 안되네요ㅡㅡ 동일주제라근가",
          "original_form": "대화제출시 안되네요ㅡㅡ 동일주제라근가",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010080.1.24",
          "form": "무슨 커피숍 제일 좋아하세요? 역시 스벅?",
          "original_form": "무슨 커피숍 제일 좋아하세요? 역시 스벅?",
          "speaker_id": "2",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010080.1.25",
          "form": "스벅이 카드할인이 돼서 ㅋ",
          "original_form": "스벅이 카드할인이 돼서 ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010080.1.26",
          "form": "점식먹고왔어요",
          "original_form": "점식먹고왔어요",
          "speaker_id": "1",
          "time": "20210825 12:39"
        }
      ]
    }
