      "utterance": [
        {
          "id": "MDRW2100027895.1.1",
          "form": "최근에 사신 물건 있으신가요?",
          "original_form": "최근에 사신 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027895.1.2",
          "form": "저는최근에 옷 샀어요..!",
          "original_form": "저는최근에 옷 샀어요..!",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027895.1.3",
          "form": "옷 사시는거 좋아하세요?",
          "original_form": "옷 사시는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027895.1.4",
          "form": "사이즈 실패 몇 번 하고 구경만 하고 있어요ㅋㅋ",
          "original_form": "사이즈 실패 몇 번 하고 구경만 하고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027895.1.5",
          "form": "헐 저두 얼마전에 산거 사이즈 실패했어요..ㅠ",
          "original_form": "헐 저두 얼마전에 산거 사이즈 실패했어요..ㅠ",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027895.1.6",
          "form": "프라사이즈라며..!",
          "original_form": "프라사이즈라며..!",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027895.1.7",
          "form": "프리 사이즈는 말이 프리지 걍 44 같아요",
          "original_form": "프리 사이즈는 말이 프리지 걍 44 같아요",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027895.1.8",
          "form": "그쵸..ㅠㅠ 저도 살이 많이 찌긴했는데",
          "original_form": "그쵸..ㅠㅠ 저도 살이 많이 찌긴했는데",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027895.1.9",
          "form": "프리사이즈라는 말이 너무 잔인해요",
          "original_form": "프리사이즈라는 말이 너무 잔인해요",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027895.1.10",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027895.1.11",
          "form": "무슨 옷 사셨어요?",
          "original_form": "무슨 옷 사셨어요?",
          "speaker_id": "1",
          "time": "20210902 16:14"
        },
        {
          "id": "MDRW2100027895.1.12",
          "form": "치마랑 윗옷 하나 샀어요",
          "original_form": "치마랑 윗옷 하나 샀어요",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027895.1.13",
          "form": "근데 절대.. 안맞음..",
          "original_form": "근데 절대.. 안맞음..",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027895.1.14",
          "form": "치마는 들어가지도 않아요.^_ㅠ",
          "original_form": "치마는 들어가지도 않아요.^_ㅠ",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027895.1.15",
          "form": "아앗ㅠㅠㅠ",
          "original_form": "아앗ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027895.1.16",
          "form": "환불해야겠네요ㅠㅠ",
          "original_form": "환불해야겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027895.1.17",
          "form": "앗 근데 아시죠..",
          "original_form": "앗 근데 아시죠..",
          "speaker_id": "2",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027895.1.18",
          "form": "지하상가에서 현금으로 사면.. 환불도 안되는거..",
          "original_form": "지하상가에서 현금으로 사면.. 환불도 안되는거..",
          "speaker_id": "2",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027895.1.19",
          "form": "대체 무슨 자신감이었는지..",
          "original_form": "대체 무슨 자신감이었는지..",
          "speaker_id": "2",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027895.1.20",
          "form": "자연스럽게 인터넷 구매라고 생각했는데 환불 안 되는 구매셨군요....ㅇㅅㅠ",
          "original_form": "자연스럽게 인터넷 구매라고 생각했는데 환불 안 되는 구매셨군요....ㅇㅅㅠ",
          "speaker_id": "1",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027895.1.21",
          "form": "당근당근 하세요ㅜㅜ",
          "original_form": "당근당근 하세요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027895.1.22",
          "form": "한국에서 약간 덩치있으면 정말 인터넷 구매는..",
          "original_form": "한국에서 약간 덩치있으면 정말 인터넷 구매는..",
          "speaker_id": "2",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027895.1.23",
          "form": "정말 어려운거 같아요.. ㅠ ㅠ 저는 살도 있는데 키도 평균보다 약간 커서..ㅠㅠ",
          "original_form": "정말 어려운거 같아요.. ㅠ ㅠ 저는 살도 있는데 키도 평균보다 약간 커서..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027895.1.24",
          "form": "쉽지않은거같아요",
          "original_form": "쉽지않은거같아요",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027895.1.25",
          "form": "사실 제 목표가 살빼서 그때 산 옷을 입으려규 하는데",
          "original_form": "사실 제 목표가 살빼서 그때 산 옷을 입으려규 하는데",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027895.1.26",
          "form": "당근이 나을가요..? ㅎㅎ",
          "original_form": "당근이 나을가요..? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027895.1.27",
          "form": "목표용 옷이었다면 둬도 괜찮죠ㅋㅋㅋㅋㅋ",
          "original_form": "목표용 옷이었다면 둬도 괜찮죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027895.1.28",
          "form": "의도한 바는 아니었지만..",
          "original_form": "의도한 바는 아니었지만..",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027895.1.29",
          "form": "이렇게 된 김에 목표용 옷으로.. 두려고요..",
          "original_form": "이렇게 된 김에 목표용 옷으로.. 두려고요..",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
