      "utterance": [
        {
          "id": "MDRW2100024383.1.1",
          "form": "운동하실때 식단관리도 함께 하셧나요",
          "original_form": "운동하실때 식단관리도 함께 하셧나요",
          "speaker_id": "1",
          "time": "20210831 06:07"
        },
        {
          "id": "MDRW2100024383.1.2",
          "form": "아니요 먹는건 못참아서요",
          "original_form": "아니요 먹는건 못참아서요",
          "speaker_id": "2",
          "time": "20210831 11:13"
        },
        {
          "id": "MDRW2100024383.1.3",
          "form": "뭐 좋아하시나요?^^",
          "original_form": "뭐 좋아하시나요?^^",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024383.1.4",
          "form": "국밥종류좋아해요! 특히 뼈해장국!",
          "original_form": "국밥종류좋아해요! 특히 뼈해장국!",
          "speaker_id": "2",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100024383.1.5",
          "form": "아 좋네요 ㅎ 저도 국밥좋아해요 순대국밥 콩나물국밥",
          "original_form": "아 좋네요 ㅎ 저도 국밥좋아해요 순대국밥 콩나물국밥",
          "speaker_id": "1",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100024383.1.6",
          "form": "오오 좋죠ㅋㅋㅋ 근데 국밥도좋아하는데 파스타를 더잘먹이요ㅋㅋㅋ",
          "original_form": "오오 좋죠ㅋㅋㅋ 근데 국밥도좋아하는데 파스타를 더잘먹이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024383.1.7",
          "form": "파스타는 뭐 좋아하세용",
          "original_form": "파스타는 뭐 좋아하세용",
          "speaker_id": "1",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024383.1.8",
          "form": "로제요! 좀매콤한거? 파스타좋아하세요?",
          "original_form": "로제요! 좀매콤한거? 파스타좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100024383.1.9",
          "form": "네 점심에도 파스타 먹었어요 ^^",
          "original_form": "네 점심에도 파스타 먹었어요 ^^",
          "speaker_id": "1",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024383.1.10",
          "form": "부럽네요 저희쪽 회사는 없어서 가끔 먹어요",
          "original_form": "부럽네요 저희쪽 회사는 없어서 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100024383.1.11",
          "form": "그럼 주로 점심엔 뭘 드시나요?",
          "original_form": "그럼 주로 점심엔 뭘 드시나요?",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024383.1.12",
          "form": "음료나 에너지바 정도? 먹어요 오후출근이라서요",
          "original_form": "음료나 에너지바 정도? 먹어요 오후출근이라서요",
          "speaker_id": "2",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100024383.1.13",
          "form": "와 소식하시네요 ㅠ 배 안고프시나요",
          "original_form": "와 소식하시네요 ㅠ 배 안고프시나요",
          "speaker_id": "1",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100024383.1.14",
          "form": "고픈데 먹을시간이 없더보니ㅋㅋㅋ",
          "original_form": "고픈데 먹을시간이 없더보니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100024383.1.15",
          "form": "잠자는게 더중요해서요",
          "original_form": "잠자는게 더중요해서요",
          "speaker_id": "2",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100024383.1.16",
