      "utterance": [
        {
          "id": "MDRW2100036239.1.1",
          "form": "네 계피향도 솔솔",
          "original_form": "네 계피향도 솔솔",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036239.1.2",
          "form": "고소하고 쫀득하고",
          "original_form": "고소하고 쫀득하고",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036239.1.3",
          "form": "원래 츄러스 좋아하거든요 ㅋㅋㅋ",
          "original_form": "원래 츄러스 좋아하거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036239.1.4",
          "form": "으어어 ㅋㅋㅋ",
          "original_form": "으어어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036239.1.5",
          "form": "저 한때 츄러스에 꽂혀서 한시간 걸어서 츄러스 사온적 있어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 한때 츄러스에 꽂혀서 한시간 걸어서 츄러스 사온적 있어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036239.1.6",
          "form": "동생한테 산책하자고 핑계대면서 ㅋㅋㅋ 이쪽으로 가볼까? 목적지 조종하면서 ㅋㅋ",
          "original_form": "동생한테 산책하자고 핑계대면서 ㅋㅋㅋ 이쪽으로 가볼까? 목적지 조종하면서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100036239.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ동생이 과연 전혀 모르고 따라갓을까요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ동생이 과연 전혀 모르고 따라갓을까요??",
          "speaker_id": "2",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100036239.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ제가 살짝 억지 좀 부리고...ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ제가 살짝 억지 좀 부리고...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100036239.1.9",
          "form": "저희집 근처에는 츄러스 파는곳이 없어요ㅠㅠ",
          "original_form": "저희집 근처에는 츄러스 파는곳이 없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100036239.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ앗 동생도 내심 츄러스 먹고싶어서 모르는척 따라간건줄 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앗 동생도 내심 츄러스 먹고싶어서 모르는척 따라간건줄 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036239.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ전혀.. 그 때 제가 진짜 꽂혀있었어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전혀.. 그 때 제가 진짜 꽂혀있었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100036239.1.12",
          "form": "영화보러가도 츄러스먹고",
          "original_form": "영화보러가도 츄러스먹고",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100036239.1.13",
          "form": "그전엔 츄러스 별로 안좋아했거든요!",
          "original_form": "그전엔 츄러스 별로 안좋아했거든요!",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100036239.1.14",
          "form": "그땐 왜그리 맛있던지..",
          "original_form": "그땐 왜그리 맛있던지..",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036239.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 저도 그런적잇어요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 저도 그런적잇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036239.1.16",
          "form": "전 찹쌀도넛에 꽂혀서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 찹쌀도넛에 꽂혀서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036239.1.17",
          "form": "원래 찹쌀도넛을 그렇게 좋아하는 편이 아니엇거든요",
          "original_form": "원래 찹쌀도넛을 그렇게 좋아하는 편이 아니엇거든요",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036239.1.18",
          "form": "그런데 어느순간 너무 맛잇어서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런데 어느순간 너무 맛잇어서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036239.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 그것도 엄청 쫄깃쫄깃하죠!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그것도 엄청 쫄깃쫄깃하죠!!",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036239.1.20",
          "form": "찹쌀도넛 안에 팥들어있는것도 좋아하세여??",
          "original_form": "찹쌀도넛 안에 팥들어있는것도 좋아하세여??",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036239.1.21",
          "form": "네 팥 그닥 안좋아하는데 찹쌀도넛 안의 팥은 맛잇어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 팥 그닥 안좋아하는데 찹쌀도넛 안의 팥은 맛잇어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100036239.1.22",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100036239.1.23",
          "form": "저도 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100036239.1.24",
          "form": "저는 단팥빵도 좋아하고~ 찹쌀도넛안에 찹쌀이랑 팥도 맛있더라구요",
          "original_form": "저는 단팥빵도 좋아하고~ 찹쌀도넛안에 찹쌀이랑 팥도 맛있더라구요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100036239.1.25",
          "form": "쫄깃쫄깃",
          "original_form": "쫄깃쫄깃",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100036239.1.26",
          "form": "진짜 찹쌀빵은 누가 먼저 만들었는지..",
          "original_form": "진짜 찹쌀빵은 누가 먼저 만들었는지..",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100036239.1.27",
          "form": "전 원래 팥빵읗 그렇게 좋아하는 편이 아니엇는데",
          "original_form": "전 원래 팥빵읗 그렇게 좋아하는 편이 아니엇는데",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036239.1.28",
          "form": "팥빵 맛잇는 집은 진짜 맛잇더라구요",
          "original_form": "팥빵 맛잇는 집은 진짜 맛잇더라구요",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036239.1.29",
          "form": "제가 맛없는 팥빵들을 먹어왓던 거엿어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 맛없는 팥빵들을 먹어왓던 거엿어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036239.1.30",
          "form": "이성당 팥빵 먹어보고 깜짝 놀랏어요",
          "original_form": "이성당 팥빵 먹어보고 깜짝 놀랏어요",
          "speaker_id": "2",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100036239.1.31",
          "form": "ㅋㅋㅋㅋㅋ맞아요!!! 저 진짜 맛없는 단팥빵 먹어봤는데..",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요!!! 저 진짜 맛없는 단팥빵 먹어봤는데..",
