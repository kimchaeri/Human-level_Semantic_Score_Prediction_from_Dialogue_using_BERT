      "utterance": [
        {
          "id": "MDRW2100046647.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211103 23:26"
        },
        {
          "id": "MDRW2100046647.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211103 23:26"
        },
        {
          "id": "MDRW2100046647.1.3",
          "form": "웹툰 좋아하세요?",
          "original_form": "웹툰 좋아하세요?",
          "speaker_id": "2",
          "time": "20211103 23:26"
        },
        {
          "id": "MDRW2100046647.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20211103 23:30"
        },
        {
          "id": "MDRW2100046647.1.5",
          "form": "애니는 좋아합니다",
          "original_form": "애니는 좋아합니다",
          "speaker_id": "1",
          "time": "20211103 23:30"
        },
        {
          "id": "MDRW2100046647.1.6",
          "form": "어떤 애니 좋아하세요~? ㅎㅎ",
          "original_form": "어떤 애니 좋아하세요~? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 23:30"
        },
        {
          "id": "MDRW2100046647.1.7",
          "form": "귀멸의칼날 좋아합니다",
          "original_form": "귀멸의칼날 좋아합니다",
          "speaker_id": "1",
          "time": "20211103 23:30"
        },
        {
          "id": "MDRW2100046647.1.8",
          "form": "오 넷플릭스에서 본 것 같기도 하네요!",
          "original_form": "오 넷플릭스에서 본 것 같기도 하네요!",
          "speaker_id": "2",
          "time": "20211103 23:31"
        },
        {
          "id": "MDRW2100046647.1.9",
          "form": "무슨 내용이에요~?",
          "original_form": "무슨 내용이에요~?",
          "speaker_id": "2",
          "time": "20211103 23:31"
        },
        {
          "id": "MDRW2100046647.1.10",
          "form": "또깨비가와 인간이 공존하는데 도깨비가 인간을 잡아먹어서 사람들이 도깨비를 없애버리는 만화입니다",
          "original_form": "또깨비가와 인간이 공존하는데 도깨비가 인간을 잡아먹어서 사람들이 도깨비를 없애버리는 만화입니다",
          "speaker_id": "1",
          "time": "20211103 23:32"
        },
        {
          "id": "MDRW2100046647.1.11",
          "form": "헉 ㄷㄷ",
          "original_form": "헉 ㄷㄷ",
          "speaker_id": "2",
          "time": "20211103 23:33"
        },
        {
          "id": "MDRW2100046647.1.12",
          "form": "지금 검색해봤는데 영화로도 나오나봐요",
          "original_form": "지금 검색해봤는데 영화로도 나오나봐요",
          "speaker_id": "2",
          "time": "20211103 23:33"
        },
        {
          "id": "MDRW2100046647.1.13",
          "form": "극장판 애니로 최근에 나왔네요",
          "original_form": "극장판 애니로 최근에 나왔네요",
          "speaker_id": "1",
          "time": "20211103 23:34"
        },
        {
          "id": "MDRW2100046647.1.14",
          "form": "큰 기대없이 보면 재미있네요",
          "original_form": "큰 기대없이 보면 재미있네요",
          "speaker_id": "1",
          "time": "20211103 23:34"
        },
        {
          "id": "MDRW2100046647.1.15",
          "form": "저도 한번 봐야겠어요!",
          "original_form": "저도 한번 봐야겠어요!",
          "speaker_id": "2",
          "time": "20211103 23:35"
        },
        {
          "id": "MDRW2100046647.1.16",
          "form": "애니는 원피스랑 나루토 봤었는데 요즘엔 잘 안보게 되더라구요 ㅋㅋ",
          "original_form": "애니는 원피스랑 나루토 봤었는데 요즘엔 잘 안보게 되더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 23:36"
        },
        {
          "id": "MDRW2100046647.1.17",
          "form": "나루토는 이야기가 산으로 가고",
          "original_form": "나루토는 이야기가 산으로 가고",
          "speaker_id": "1",
          "time": "20211103 23:36"
        },
        {
          "id": "MDRW2100046647.1.18",
          "form": "원피스는 진도가 느려서 지루하죠",
          "original_form": "원피스는 진도가 느려서 지루하죠",
          "speaker_id": "1",
          "time": "20211103 23:37"
        },
        {
          "id": "MDRW2100046647.1.19",
          "form": "네 ㅋㅋㅋ 그래서 다 하차했어요!",
          "original_form": "네 ㅋㅋㅋ 그래서 다 하차했어요!",
          "speaker_id": "2",
          "time": "20211103 23:37"
        },
        {
          "id": "MDRW2100046647.1.20",
          "form": "유튭에서 짱구 종종 봐요",
          "original_form": "유튭에서 짱구 종종 봐요",
          "speaker_id": "2",
          "time": "20211103 23:37"
        },
        {
          "id": "MDRW2100046647.1.21",
          "form": "짱구는 호불호가 없이 다 좋아하죠",
          "original_form": "짱구는 호불호가 없이 다 좋아하죠",
          "speaker_id": "1",
          "time": "20211103 23:39"
        },
        {
          "id": "MDRW2100046647.1.22",
          "form": "틀어놓고 밥 먹기 딱 좋아요! ㅋㅋㅋ",
          "original_form": "틀어놓고 밥 먹기 딱 좋아요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 23:39"
        }
      ]
    }
