      "utterance": [
        {
          "id": "MDRW2100012652.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012652.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012652.1.3",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012652.1.4",
          "form": "네 저는 본가에서 강아지 한마리키우고있어요",
          "original_form": "네 저는 본가에서 강아지 한마리키우고있어요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100012652.1.5",
          "form": "키우고싶은 반려동물있으신가요?",
          "original_form": "키우고싶은 반려동물있으신가요?",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100012652.1.6",
          "form": "전 예전에 강아지 키웠어서 고양이도 키워보고 싶어요",
          "original_form": "전 예전에 강아지 키웠어서 고양이도 키워보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100012652.1.7",
          "form": "어떤 종이에요?",
          "original_form": "어떤 종이에요?",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100012652.1.8",
          "form": "따로 종은없고 그냥 믹스견이에요",
          "original_form": "따로 종은없고 그냥 믹스견이에요",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100012652.1.9",
          "form": "저도 고양이 한번 키워보고싶네요",
          "original_form": "저도 고양이 한번 키워보고싶네요",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100012652.1.10",
          "form": "믹스견 너무 귀여워요",
          "original_form": "믹스견 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100012652.1.11",
          "form": "마자요 저는 믹스가 더 귀여운거같아요",
          "original_form": "마자요 저는 믹스가 더 귀여운거같아요",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100012652.1.12",
          "form": "저 자취하는집에서도 키우고싶은데",
          "original_form": "저 자취하는집에서도 키우고싶은데",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100012652.1.13",
          "form": "저 일하러가면 혼자있어야하니까 그것땜에 못키우겠더라구요",
          "original_form": "저 일하러가면 혼자있어야하니까 그것땜에 못키우겠더라구요",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100012652.1.14",
          "form": "그러게요ㅜ 개들이 혼자 있을때 많이 우울해 한다더라구요",
          "original_form": "그러게요ㅜ 개들이 혼자 있을때 많이 우울해 한다더라구요",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100012652.1.15",
          "form": "고양이는 산책도 안시켜줘도 되고 혼자있는것도 좋아해서 키우긴 더 쉬울거 같아요",
          "original_form": "고양이는 산책도 안시켜줘도 되고 혼자있는것도 좋아해서 키우긴 더 쉬울거 같아요",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100012652.1.16",
          "form": "마자요 고양이는 혼자서 알아서 잘 지내는거같아서\n그런걱정은 안해도될거같더라구요",
          "original_form": "마자요 고양이는 혼자서 알아서 잘 지내는거같아서\n그런걱정은 안해도될거같더라구요",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100012652.1.17",
          "form": "맞아요 근데 보통 강아지 산책 매일 시키는 편이셨나요?",
          "original_form": "맞아요 근데 보통 강아지 산책 매일 시키는 편이셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100012652.1.18",
          "form": "아니요ㅠ 밖에서 키우는개인데\n산책 자주는 못시켜줬어요\n밖에서 키워도 산책 자주시켜주는게 좋다그러던데ㅠ",
          "original_form": "아니요ㅠ 밖에서 키우는개인데\n산책 자주는 못시켜줬어요\n밖에서 키워도 산책 자주시켜주는게 좋다그러던데ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100012652.1.19",
          "form": "앗  그렇구나 저도 키웠을때 산책 자주 못시켜줘서 많이 미안했어요ㅜ",
          "original_form": "앗  그렇구나 저도 키웠을때 산책 자주 못시켜줘서 많이 미안했어요ㅜ",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100012652.1.20",
          "form": "개들은 새로운 냄새 맡으면서 스트레스 푼다고 하죠?",
          "original_form": "개들은 새로운 냄새 맡으면서 스트레스 푼다고 하죠?",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100012652.1.21",
          "form": "저희개는 꽃냄새 좋아했었어요",
          "original_form": "저희개는 꽃냄새 좋아했었어요",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100012652.1.22",
          "form": "우와 강아지들도 꽃냄새 좋아하는군요\n귀엽네요ㅠ",
          "original_form": "우와 강아지들도 꽃냄새 좋아하는군요\n귀엽네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100012652.1.23",
          "form": "저희개는 가끔 풀도 뜯어먹고 그러더라구요ㅋㅋ",
          "original_form": "저희개는 가끔 풀도 뜯어먹고 그러더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:23"
        }
      ]
