        {
          "id": "MDRW2100013124.1.1",
          "form": "겜 좋아하시나요",
          "original_form": "겜 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100013124.1.2",
          "form": "네 게임 좋아해요",
          "original_form": "네 게임 좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013124.1.3",
          "form": "어떤게임 하시나요",
          "original_form": "어떤게임 하시나요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013124.1.4",
          "form": "오버워치도 오래했었구요",
          "original_form": "오버워치도 오래했었구요",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013124.1.5",
          "form": "스팀게임도 좋아하고 스위치게임도 해요",
          "original_form": "스팀게임도 좋아하고 스위치게임도 해요",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013124.1.6",
          "form": "오 옵치저도 했었어요",
          "original_form": "오 옵치저도 했었어요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013124.1.7",
          "form": "주캐가 뭐 셨나요?",
          "original_form": "주캐가 뭐 셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013124.1.8",
          "form": "솔저76요 ㅎㅎ",
          "original_form": "솔저76요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100013124.1.9",
          "form": "딜러하셨구나!",
          "original_form": "딜러하셨구나!",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100013124.1.10",
          "form": "저는 탱커주로해요",
          "original_form": "저는 탱커주로해요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100013124.1.11",
          "form": "오오 요즘도하시나요",
          "original_form": "오오 요즘도하시나요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100013124.1.12",
          "form": "요즘은 안하는데 한두달전만해도 했어요",
          "original_form": "요즘은 안하는데 한두달전만해도 했어요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100013124.1.13",
          "form": "요즘도 잘나가나요 옵치",
          "original_form": "요즘도 잘나가나요 옵치",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100013124.1.14",
          "form": "아앗....",
          "original_form": "아앗....",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100013124.1.15",
          "form": "아니요...",
          "original_form": "아니요...",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100013124.1.16",
          "form": "망겜 소리 듣고있어요",
          "original_form": "망겜 소리 듣고있어요",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100013124.1.17",
          "form": "업데이트가 2년째 거의 하나도 없어서요",
          "original_form": "업데이트가 2년째 거의 하나도 없어서요",
