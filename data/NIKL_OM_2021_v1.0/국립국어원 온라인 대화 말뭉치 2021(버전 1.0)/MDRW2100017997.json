      "utterance": [
        {
          "id": "MDRW2100017997.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017997.1.2",
          "form": "오늘 비왔나용?",
          "original_form": "오늘 비왔나용?",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017997.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017997.1.4",
          "form": "인천 지방은 흐리기만 하고 비는 안 왔습니다.",
          "original_form": "인천 지방은 흐리기만 하고 비는 안 왔습니다.",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017997.1.5",
          "form": "name1님 사시는 곳은 비가 왔나요?",
          "original_form": "&name1&님 사시는 곳은 비가 왔나요?",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017997.1.6",
          "form": "제가 사는곳은 안왔네요!",
          "original_form": "제가 사는곳은 안왔네요!",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017997.1.7",
          "form": "아침에 조금 흐리고",
          "original_form": "아침에 조금 흐리고",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017997.1.8",
          "form": "낮부턴 햇빛 나더라고요 ㅎㅎ",
          "original_form": "낮부턴 햇빛 나더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017997.1.9",
          "form": "태풍도 큰 피해 없이 지나가고 다행입니다ㅎ",
          "original_form": "태풍도 큰 피해 없이 지나가고 다행입니다ㅎ",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017997.1.10",
          "form": "습도가 높긴 한데 그래도 기온은 좀 떨어진 거 같아요.",
          "original_form": "습도가 높긴 한데 그래도 기온은 좀 떨어진 거 같아요.",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017997.1.11",
          "form": "비가 오고 나서 그런거같아요!",
          "original_form": "비가 오고 나서 그런거같아요!",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017997.1.12",
          "form": "내일도 비소식있다고 하던데 ㅠ.ㅠ",
          "original_form": "내일도 비소식있다고 하던데 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017997.1.13",
          "form": "일기 예보 찾아보니 밤부터 내일 오전에 비 소식 있네요.",
          "original_form": "일기 예보 찾아보니 밤부터 내일 오전에 비 소식 있네요.",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017997.1.14",
          "form": "다음주 화수목도 온다고 하더라고요 이 지역은",
          "original_form": "다음주 화수목도 온다고 하더라고요 이 지역은",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017997.1.15",
          "form": "가을장마인가봐용 ㅠㅠ",
          "original_form": "가을장마인가봐용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017997.1.16",
          "form": "장마는 있는 듯 없는 듯 지나가더니 9월 다 돼 비가 많이 오네요ㅜㅜ",
          "original_form": "장마는 있는 듯 없는 듯 지나가더니 9월 다 돼 비가 많이 오네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017997.1.17",
          "form": "맞아요 늦장마라니 ㅠㅠ",
          "original_form": "맞아요 늦장마라니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017997.1.18",
          "form": "집에 있을 때 비가 오면 견딜만 한데 출퇴근 때 비 오면 넘 괴롭더라고요;",
          "original_form": "집에 있을 때 비가 오면 견딜만 한데 출퇴근 때 비 오면 넘 괴롭더라고요;",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017997.1.19",
          "form": "신발 축축해지는 거 넘 힘들어요;",
          "original_form": "신발 축축해지는 거 넘 힘들어요;",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017997.1.20",
          "form": "아하..ㅠㅠ",
          "original_form": "아하..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017997.1.21",
          "form": "비 오는 날씨 좋아하시나요?",
          "original_form": "비 오는 날씨 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017997.1.22",
          "form": "저는 그닥 안좋아해요",
          "original_form": "저는 그닥 안좋아해요",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017997.1.23",
          "form": "다만 밤에 비가 오는 경우면",
          "original_form": "다만 밤에 비가 오는 경우면",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017997.1.24",
          "form": "좀 마음이 편안해져서",
          "original_form": "좀 마음이 편안해져서",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017997.1.25",
          "form": "밤에 오는 비 한정으론 좋아하는거같아요 ㅎㅎ",
          "original_form": "밤에 오는 비 한정으론 좋아하는거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017997.1.26",
          "form": "ㅎㅎ 저도 밤에 빗소리 듣고 있음 잔잔하게 좋더라고요.",
          "original_form": "ㅎㅎ 저도 밤에 빗소리 듣고 있음 잔잔하게 좋더라고요.",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017997.1.27",
          "form": "외출할 땐 너무 싫지만요;;;",
          "original_form": "외출할 땐 너무 싫지만요;;;",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017997.1.28",
          "form": "맞아요 낮에는 어우..",
          "original_form": "맞아요 낮에는 어우..",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100017997.1.29",
          "form": "신발 바지 다 젖고 ㅠㅠ",
          "original_form": "신발 바지 다 젖고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:22"
        }
      ]
