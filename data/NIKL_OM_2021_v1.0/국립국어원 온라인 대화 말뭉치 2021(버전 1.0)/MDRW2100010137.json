      "utterance": [
        {
          "id": "MDRW2100010137.1.1",
          "form": "도서 뭐여 이번에 재희재욱재현 리커버 버전 나옵니다",
          "original_form": "도서 뭐여 이번에 재희재욱재현 리커버 버전 나옵니다",
          "speaker_id": "2",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010137.1.2",
          "form": "그건뭐임?",
          "original_form": "그건뭐임?",
          "speaker_id": "1",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010137.1.3",
          "form": "미안하다 재인 재욱 재훈이었다",
          "original_form": "미안하다 재인 재욱 재훈이었다",
          "speaker_id": "2",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010137.1.4",
          "form": "책이야?",
          "original_form": "책이야?",
          "speaker_id": "1",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010137.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010137.1.6",
          "form": "정세랑 작가 책이야",
          "original_form": "정세랑 작가 책이야",
          "speaker_id": "2",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010137.1.7",
          "form": "장르가뭔데ㅡ?",
          "original_form": "장르가뭔데ㅡ?",
          "speaker_id": "1",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010137.1.8",
          "form": "판타지?",
          "original_form": "판타지?",
          "speaker_id": "1",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010137.1.9",
          "form": "정세랑 김초엽 작가 책은 일단 재밌음 읽으셈",
          "original_form": "정세랑 김초엽 작가 책은 일단 재밌음 읽으셈",
          "speaker_id": "2",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010137.1.10",
          "form": "SF? 판타지 그런거임",
          "original_form": "SF? 판타지 그런거임",
          "speaker_id": "2",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010137.1.11",
          "form": "우연히 초능력을 가지게 된 삼남먀 이애기",
          "original_form": "우연히 초능력을 가지게 된 삼남먀 이애기",
          "speaker_id": "2",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010137.1.12",
          "form": "리커버면",
          "original_form": "리커버면",
          "speaker_id": "1",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010137.1.13",
          "form": "나왔던거다시",
          "original_form": "나왔던거다시",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010137.1.14",
          "form": "편집해서",
          "original_form": "편집해서",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010137.1.15",
          "form": "내는거임?",
          "original_form": "내는거임?",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010137.1.16",
          "form": "책 표지만 바꿔서 다시 내는거야",
          "original_form": "책 표지만 바꿔서 다시 내는거야",
          "speaker_id": "2",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010137.1.17",
          "form": "아 초능력 좀 끌리긴하네",
          "original_form": "아 초능력 좀 끌리긴하네",
          "speaker_id": "1",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.18",
          "form": "보건 교사 안은영도 리커버 버전으로 나왔었어",
          "original_form": "보건 교사 안은영도 리커버 버전으로 나왔었어",
          "speaker_id": "2",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.19",
          "form": "ㅇㅇㅇ 읽어보셈",
          "original_form": "ㅇㅇㅇ 읽어보셈",
          "speaker_id": "2",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.20",
          "form": "보건교사안은영",
          "original_form": "보건교사안은영",
          "speaker_id": "1",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.21",
          "form": "원래",
          "original_form": "원래",
          "speaker_id": "1",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.22",
          "form": "책이었어?",
          "original_form": "책이었어?",
          "speaker_id": "1",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.23",
          "form": "아 얍얍",
          "original_form": "아 얍얍",
          "speaker_id": "2",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.24",
          "form": "정세랑 작가 원작",
          "original_form": "정세랑 작가 원작",
          "speaker_id": "2",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010137.1.25",
