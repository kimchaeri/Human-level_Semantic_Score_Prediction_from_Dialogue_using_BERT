      "utterance": [
        {
          "id": "MDRW2100023662.1.1",
          "form": "아뇨! 넷플 볼거 너무너무 많아소",
          "original_form": "아뇨! 넷플 볼거 너무너무 많아소",
          "speaker_id": "2",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100023662.1.2",
          "form": "하루종일 그거만 볼거같아서 안봐요ㅠ",
          "original_form": "하루종일 그거만 볼거같아서 안봐요ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023662.1.3",
          "form": "진짜 보고싶으면 친구있을때 친구꺼로 보고ㅋㅋ",
          "original_form": "진짜 보고싶으면 친구있을때 친구꺼로 보고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023662.1.4",
          "form": "악ㅋㅋㅋ맞아요 하루금방갈거같아요ㄱㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋ맞아요 하루금방갈거같아요ㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023662.1.5",
          "form": "앜ㅋㅋㅋ저 지금",
          "original_form": "앜ㅋㅋㅋ저 지금",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023662.1.6",
          "form": "런닝맨보는데",
          "original_form": "런닝맨보는데",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023662.1.7",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023662.1.8",
          "form": "어제꺼보셔요?ㅋㅋㅋ",
          "original_form": "어제꺼보셔요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:22"
        },
        {
          "id": "MDRW2100023662.1.9",
          "form": "네네 보셨어요????",
          "original_form": "네네 보셨어요????",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023662.1.10",
          "form": "지루한 장면이 1도없어...대박..",
          "original_form": "지루한 장면이 1도없어...대박..",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023662.1.11",
          "form": "ㅋㅋㅋㅋㅋ진짜너무재밌어요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ진짜너무재밌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100023662.1.12",
          "form": "진짜 너무 피곤했는데 너무웃겨서 다 잊었어요ㅋㅋㅋ",
          "original_form": "진짜 너무 피곤했는데 너무웃겨서 다 잊었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023662.1.13",
          "form": "그리고 원래도 멋지고 이뻤지만",
          "original_form": "그리고 원래도 멋지고 이뻤지만",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023662.1.14",
          "form": "지효언니 존멋 ㅜㅜ",
          "original_form": "지효언니 존멋 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023662.1.15",
          "form": "ㅠㅡㅠ 진짜존멋ㅜㅜ",
          "original_form": "ㅠㅡㅠ 진짜존멋ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100023662.1.16",
          "form": "이쁘고멋지고다함ㅋㄱ쿠ㅜ",
          "original_form": "이쁘고멋지고다함ㅋㄱ쿠ㅜ",
          "speaker_id": "1",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100023662.1.17",
          "form": "드라마 보는거 뭐 있어요?!",
          "original_form": "드라마 보는거 뭐 있어요?!",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100023662.1.18",
          "form": "슬의요!  이번주결방이죠...?ㅜㅜ",
          "original_form": "슬의요!  이번주결방이죠...?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100023662.1.19",
          "form": "네ㅜㅜ 결방되게 많이하는느낌...",
          "original_form": "네ㅜㅜ 결방되게 많이하는느낌...",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100023662.1.20",
          "form": "ㅠㅡㅠ..아쉬워요주1횐데결방을하면...",
          "original_form": "ㅠㅡㅠ..아쉬워요주1횐데결방을하면...",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100023662.1.21",
          "form": "그리고 지금 풀어야할 이야기가 산더민데....",
          "original_form": "그리고 지금 풀어야할 이야기가 산더민데....",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100023662.1.22",
          "form": "종방까지 2회 남았던거같은데",
          "original_form": "종방까지 2회 남았던거같은데",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100023662.1.23",
          "form": "그러게요...   요새 되게 교훈주려구",
          "original_form": "그러게요...   요새 되게 교훈주려구",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100023662.1.24",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100023662.1.25",
          "form": "그런장면많이넣고",
          "original_form": "그런장면많이넣고",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100023662.1.26",
          "form": "커플진도나가는건없구ㅋㅋㅋ",
          "original_form": "커플진도나가는건없구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100023662.1.27",
          "form": "어어맞아요",
          "original_form": "어어맞아요",
          "speaker_id": "2",
          "time": "20210830 23:12"
        }
      ]
    }
