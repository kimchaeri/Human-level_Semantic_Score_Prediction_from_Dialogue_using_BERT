      "utterance": [
        {
          "id": "MDRW2100025561.1.1",
          "form": "최근에 구입하거나 구입할 예정인 상품이 있으신가요?",
          "original_form": "최근에 구입하거나 구입할 예정인 상품이 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100025561.1.2",
          "form": "저~ 최근에 팀장님 차사서 선물 샀어요 ㅎ",
          "original_form": "저~ 최근에 팀장님 차사서 선물 샀어요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025561.1.3",
          "form": "팀장님이랑 친하신가봐요ㅎㅎ",
          "original_form": "팀장님이랑 친하신가봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025561.1.4",
          "form": "전 믹서기 구매해서 기다리는 중이에요",
          "original_form": "전 믹서기 구매해서 기다리는 중이에요",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025561.1.5",
          "form": "ㅋ...그냥 살아남으려고... 어쩔수 없이...",
          "original_form": "ㅋ...그냥 살아남으려고... 어쩔수 없이...",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100025561.1.6",
          "form": "오 믹서기!",
          "original_form": "오 믹서기!",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100025561.1.7",
          "form": "어디꺼가좋나요?",
          "original_form": "어디꺼가좋나요?",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100025561.1.8",
          "form": "한일전자요!!!",
          "original_form": "한일전자요!!!",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100025561.1.9",
          "form": "전에 쓰던 브랜드인데 튼튼하고 잘 갈려요 ㅋㅋㅋ",
          "original_form": "전에 쓰던 브랜드인데 튼튼하고 잘 갈려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100025561.1.10",
          "form": "오 한일 좋죠!",
          "original_form": "오 한일 좋죠!",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100025561.1.11",
          "form": "저희 집도 한일꺼 잘 써요",
          "original_form": "저희 집도 한일꺼 잘 써요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100025561.1.12",
          "form": "선풍기랑 온수매트도 한일껀데 괜찮은거같아여",
          "original_form": "선풍기랑 온수매트도 한일껀데 괜찮은거같아여",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100025561.1.13",
          "form": "그니까요 ㅎㅎㅎ 은근 가전제품이 잘나오는거 같아요",
          "original_form": "그니까요 ㅎㅎㅎ 은근 가전제품이 잘나오는거 같아요",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100025561.1.14",
          "form": "그리고 자외서차단제도 보고 있어요 ㅋㅋ 살때가 되어서ㅋㅋㅋㅋ",
          "original_form": "그리고 자외서차단제도 보고 있어요 ㅋㅋ 살때가 되어서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100025561.1.15",
          "form": "오 원래 뭐쓰세요? 저는 썬크림 유목민이라...",
          "original_form": "오 원래 뭐쓰세요? 저는 썬크림 유목민이라...",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025561.1.16",
          "form": "저 바이오더마 선크림을 쓰고 있어요",
          "original_form": "저 바이오더마 선크림을 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025561.1.17",
          "form": "로션같이 묽고 부드러워서 쓰기 편해요",
          "original_form": "로션같이 묽고 부드러워서 쓰기 편해요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100025561.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100025561.1.19",
          "form": "좋네요저 그런거 좋아요",
          "original_form": "좋네요저 그런거 좋아요",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100025561.1.20",
          "form": "얼굴에 기름이 많아서",
          "original_form": "얼굴에 기름이 많아서",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100025561.1.21",
          "form": "순하기도 해서 피부타입 상관없이 쓰기 좋은거 같아요",
          "original_form": "순하기도 해서 피부타입 상관없이 쓰기 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100025561.1.22",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100025561.1.23",
          "form": "지금꺼 다쓰면 써봐야겠어요",
          "original_form": "지금꺼 다쓰면 써봐야겠어요",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100025561.1.24",
          "form": "지금껀 간지럽더라고요",
          "original_form": "지금껀 간지럽더라고요",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100025561.1.25",
          "form": "화장품이 간지러우면 안맞는건데.. ㅠ",
          "original_form": "화장품이 간지러우면 안맞는건데.. ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100025561.1.26",
          "form": "그냥 버리기도 그렇죠.. 저도 그런적 많아요",
          "original_form": "그냥 버리기도 그렇죠.. 저도 그런적 많아요",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100025561.1.27",
          "form": "예전이라면 막 버렸을텐데",
          "original_form": "예전이라면 막 버렸을텐데",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025561.1.28",
          "form": "요즘 돈 생각에 그냥 아끼자는식으로 쓰고있어요ㅠ",
          "original_form": "요즘 돈 생각에 그냥 아끼자는식으로 쓰고있어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025561.1.29",
          "form": "마스크를 제외한 부위에만 조금만 바르는게 좋을거 같아요 ㅎ",
          "original_form": "마스크를 제외한 부위에만 조금만 바르는게 좋을거 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100025561.1.30",
          "form": "아니면 계절이 문제일 수도 있으니까 좀 선선해지면 괜찮을 수 도 있어요..",
          "original_form": "아니면 계절이 문제일 수도 있으니까 좀 선선해지면 괜찮을 수 도 있어요..",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100025561.1.31",
          "form": "아하 그럴수도 있겠네요",
          "original_form": "아하 그럴수도 있겠네요",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100025561.1.32",
          "form": "제가 땀이 많아서 더 그럴수도 있을것같아요",
          "original_form": "제가 땀이 많아서 더 그럴수도 있을것같아요",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100025561.1.33",
          "form": "더 선선해지면 봐야겠어여",
          "original_form": "더 선선해지면 봐야겠어여",
          "speaker_id": "2",
          "time": "20210901 22:00"
        }
      ]
    }
