      "utterance": [
        {
          "id": "MDRW2100019568.1.1",
          "form": "게임인가요 이번엔!",
          "original_form": "게임인가요 이번엔!",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019568.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019568.1.3",
          "form": "롤!!!!!좋아해요",
          "original_form": "롤!!!!!좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019568.1.4",
          "form": "왕 롤 재미있나요?",
          "original_form": "왕 롤 재미있나요?",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019568.1.5",
          "form": "네네네!!!!",
          "original_form": "네네네!!!!",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019568.1.6",
          "form": "저 롤은 유튜브로 영상은 봤는데 안 해봤어요ㅋㅋㅋ",
          "original_form": "저 롤은 유튜브로 영상은 봤는데 안 해봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019568.1.7",
          "form": "아아 그럼 무슨게임하세요???!!",
          "original_form": "아아 그럼 무슨게임하세요???!!",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019568.1.8",
          "form": "저 핸드폰게임만해요",
          "original_form": "저 핸드폰게임만해요",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019568.1.9",
          "form": "아아 모하세어??!!",
          "original_form": "아아 모하세어??!!",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019568.1.10",
          "form": "마이리틀포레스트요ㅎㅎ",
          "original_form": "마이리틀포레스트요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019568.1.11",
          "form": "아아안 그거 하는사람은 봤어여!",
          "original_form": "아아안 그거 하는사람은 봤어여!",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019568.1.12",
          "form": "오 그래요? 재밌어요!",
          "original_form": "오 그래요? 재밌어요!",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019568.1.13",
          "form": "오옹ㅇ 다른거는여??!",
          "original_form": "오옹ㅇ 다른거는여??!",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019568.1.14",
          "form": "쿠키런 킹덤이요!",
          "original_form": "쿠키런 킹덤이요!",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019568.1.15",
          "form": "아아아!!!",
          "original_form": "아아아!!!",
          "speaker_id": "2",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019568.1.16",
          "form": "뭔지알아여",
          "original_form": "뭔지알아여",
          "speaker_id": "2",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019568.1.17",
          "form": "점점 현질해야해서 질리긴해요ㅋㅋ",
          "original_form": "점점 현질해야해서 질리긴해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019568.1.18",
          "form": "게임할 때 현질하세요?",
          "original_form": "게임할 때 현질하세요?",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019568.1.19",
          "form": "음..저는 안해여!",
          "original_form": "음..저는 안해여!",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019568.1.20",
          "form": "그걸로 맛있는거 사먹자는 주의라서",
          "original_form": "그걸로 맛있는거 사먹자는 주의라서",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019568.1.21",
          "form": "무과금으로 하시는구나",
          "original_form": "무과금으로 하시는구나",
          "speaker_id": "1",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019568.1.22",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019568.1.23",
          "form": "무과금유저입니단",
          "original_form": "무과금유저입니단",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019568.1.24",
          "form": "ㅋㅋㅋ 저도 무과금으로 하다가 맨날 충동결제하고 후회해요..",
          "original_form": "ㅋㅋㅋ 저도 무과금으로 하다가 맨날 충동결제하고 후회해요..",
          "speaker_id": "1",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019568.1.25",
          "form": "아잌ㅋㅋㅋㅋ 어느정도 하셨어여??",
          "original_form": "아잌ㅋㅋㅋㅋ 어느정도 하셨어여??",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019568.1.26",
          "form": "킹덤에 15만원정도 질렀어요ㅎ",
          "original_form": "킹덤에 15만원정도 질렀어요ㅎ",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019568.1.27",
          "form": "요즘에 쿠키런킹덤 조금 질려서 후회중입니다",
          "original_form": "요즘에 쿠키런킹덤 조금 질려서 후회중입니다",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019568.1.28",
          "form": "곧 지울거같아서..",
          "original_form": "곧 지울거같아서..",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019568.1.29",
          "form": "아하...그람 후회가 될수도 ㅠㅠ",
          "original_form": "아하...그람 후회가 될수도 ㅠㅠ",
          "speaker_id": "2",
