      "utterance": [
        {
          "id": "MDRW2100040588.1.1",
          "form": "피자요. 근처에 새로 생기기도 했구요",
          "original_form": "피자요. 근처에 새로 생기기도 했구요",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040588.1.2",
          "form": "어떤 피자인가요",
          "original_form": "어떤 피자인가요",
          "speaker_id": "1",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040588.1.3",
          "form": "파파존스요. 한번도 안 먹어봤거든요.",
          "original_form": "파파존스요. 한번도 안 먹어봤거든요.",
          "speaker_id": "2",
          "time": "20210905 13:04"
        },
        {
          "id": "MDRW2100040588.1.4",
          "form": "저도 안먹어봤는데",
          "original_form": "저도 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040588.1.5",
          "form": "맛 어떤가요",
          "original_form": "맛 어떤가요",
          "speaker_id": "1",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040588.1.6",
          "form": "딴데보다 좀 짭조름한거 같아여ㅋㅋ",
          "original_form": "딴데보다 좀 짭조름한거 같아여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040588.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ오호",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오호",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100040588.1.8",
          "form": "거기 햄버거도 팔지 않나요?",
          "original_form": "거기 햄버거도 팔지 않나요?",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100040588.1.9",
          "form": "글쎄요? 햄버거는 모르겠네요",
          "original_form": "글쎄요? 햄버거는 모르겠네요",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100040588.1.10",
          "form": "ㅋㅋㅋㅋㅋ 아하 파는 줄 알았어요",
          "original_form": "ㅋㅋㅋㅋㅋ 아하 파는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040588.1.11",
          "form": "어떤 피자.드셨어요?",
          "original_form": "어떤 피자.드셨어요?",
          "speaker_id": "1",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040588.1.12",
          "form": "슈퍼 파파스요",
          "original_form": "슈퍼 파파스요",
          "speaker_id": "2",
          "time": "20210905 19:26"
        },
        {
          "id": "MDRW2100040588.1.13",
          "form": "그냥 콤비네이션 피자 같아요",
          "original_form": "그냥 콤비네이션 피자 같아요",
          "speaker_id": "2",
          "time": "20210905 19:26"
        },
        {
          "id": "MDRW2100040588.1.14",
          "form": "피자.. 너뮤 먹고싶네요ㅋㅋㅋㅋ",
          "original_form": "피자.. 너뮤 먹고싶네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100040588.1.15",
          "form": "ㅋㅋㅋ무슨 피자 좋아하세요?",
          "original_form": "ㅋㅋㅋ무슨 피자 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100040588.1.16",
          "form": "전 고구마요",
          "original_form": "전 고구마요",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040588.1.17",
          "form": "아니면.불고기요,!!!",
          "original_form": "아니면.불고기요,!!!",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040588.1.18",
          "form": "맛있죠ㅋㅋ 고구마 크러스트도 좋아하세여?",
          "original_form": "맛있죠ㅋㅋ 고구마 크러스트도 좋아하세여?",
          "speaker_id": "2",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100040588.1.19",
          "form": "ㅋㅋㅋ 넵",
          "original_form": "ㅋㅋㅋ 넵",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100040588.1.20",
          "form": "하지만.치즈를 더 좋아해요",
          "original_form": "하지만.치즈를 더 좋아해요",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100040588.1.21",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100040588.1.22",
          "form": "어떤 크러스트를 더 좋아흐시나요?",
