      "utterance": [
        {
          "id": "MDRW2100026342.1.1",
          "form": "파즐이면 핸드폰 게임인가 봐요?",
          "original_form": "파즐이면 핸드폰 게임인가 봐요?",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026342.1.2",
          "form": "퍼즐",
          "original_form": "퍼즐",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026342.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026342.1.4",
          "form": "밖에서 시간 죽일때 퍼즐게임해요",
          "original_form": "밖에서 시간 죽일때 퍼즐게임해요",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026342.1.5",
          "form": "rpg는 배터리가 너무 빨리닳더라고요",
          "original_form": "rpg는 배터리가 너무 빨리닳더라고요",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026342.1.6",
          "form": "그러게요 원신 이런거 해봤는데",
          "original_form": "그러게요 원신 이런거 해봤는데",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026342.1.7",
          "form": "폰이 너무 뜨거워져요",
          "original_form": "폰이 너무 뜨거워져요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026342.1.8",
          "form": "집이나 충전할 곳이 있는곳이면 괜찮은데",
          "original_form": "집이나 충전할 곳이 있는곳이면 괜찮은데",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026342.1.9",
          "form": "밖에서 하기에는 용량도 부담되고 그런면은 있는거 같아요",
          "original_form": "밖에서 하기에는 용량도 부담되고 그런면은 있는거 같아요",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026342.1.10",
          "form": "아 배그 해보셨나요?",
          "original_form": "아 배그 해보셨나요?",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026342.1.11",
          "form": "저 배그 보는건 좋아하는데",
          "original_form": "저 배그 보는건 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026342.1.12",
          "form": "간이 작아서",
          "original_form": "간이 작아서",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026342.1.13",
          "form": "뒤에서 사람 나오면 기절한다는 ㅋㅋㅋ",
          "original_form": "뒤에서 사람 나오면 기절한다는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026342.1.14",
          "form": "아 맞아요ㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026342.1.15",
          "form": "진짜 뒤에서 나올까봐",
          "original_form": "진짜 뒤에서 나올까봐",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026342.1.16",
          "form": "너무 쫄면서 해요",
          "original_form": "너무 쫄면서 해요",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026342.1.17",
          "form": "그래서 저는 맨날 어디에 숨어서있다가 치킨먹은적도 있어요",
          "original_form": "그래서 저는 맨날 어디에 숨어서있다가 치킨먹은적도 있어요",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026342.1.18",
          "form": "운이 좋네요 ㅋㅋㅋㅋ 저도 그런거하면 도망다니다 끝나요 ㅠㅠ",
          "original_form": "운이 좋네요 ㅋㅋㅋㅋ 저도 그런거하면 도망다니다 끝나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026342.1.19",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026342.1.20",
          "form": "도망다니다가 총맞고",
          "original_form": "도망다니다가 총맞고",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026342.1.21",
          "form": "숨어있었는데 뒤에서 총맞고",
          "original_form": "숨어있었는데 뒤에서 총맞고",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026342.1.22",
          "form": "새가슴에게는 위험한 게임 ㅋㅋㅋ",
          "original_form": "새가슴에게는 위험한 게임 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026342.1.23",
          "form": "진짜 사람만날때마다",
          "original_form": "진짜 사람만날때마다",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026342.1.24",
          "form": "심장마비올거같아요",
          "original_form": "심장마비올거같아요",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026342.1.25",
          "form": "스팀게임 재밌는거 많아 보이는데 컴퓨터가 없어요 ㅠㅠ",
          "original_form": "스팀게임 재밌는거 많아 보이는데 컴퓨터가 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026342.1.26",
          "form": "컴퓨터로 하려면",
          "original_form": "컴퓨터로 하려면",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026342.1.27",
          "form": "게이밍 컴퓨터가 너무 비싸요...",
          "original_form": "게이밍 컴퓨터가 너무 비싸요...",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026342.1.28",
          "form": "그러게요 ㅜㅜ 저는 심즈 좋아하는데",
          "original_form": "그러게요 ㅜㅜ 저는 심즈 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026342.1.29",
          "form": "저는 심즈 좋아하는데 못한지 오래에요 ㅜㅜㅋㅋ",
