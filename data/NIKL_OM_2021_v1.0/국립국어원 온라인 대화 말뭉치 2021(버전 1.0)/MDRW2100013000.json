        {
          "id": "MDRW2100013000.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100013000.1.3",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100013000.1.4",
          "form": "아뇨 고양이 키워보고싶어요",
          "original_form": "아뇨 고양이 키워보고싶어요",
          "speaker_id": "1",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100013000.1.5",
          "form": "아ㅎ 전 강아지 키우고 있어요",
          "original_form": "아ㅎ 전 강아지 키우고 있어요",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100013000.1.6",
          "form": "한번도 키워본 적이 없으신가요?",
          "original_form": "한번도 키워본 적이 없으신가요?",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100013000.1.7",
          "form": "네 없습니다ㅎㅎ",
          "original_form": "네 없습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100013000.1.8",
          "form": "고양이는 무슨 종 키워보고 싶으세요?",
          "original_form": "고양이는 무슨 종 키워보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100013000.1.9",
          "form": "종 까지는 잘몰라요",
          "original_form": "종 까지는 잘몰라요",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100013000.1.10",
          "form": "아 하긴 고양이 이즈 뭔들이지요",
          "original_form": "아 하긴 고양이 이즈 뭔들이지요",
          "speaker_id": "2",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100013000.1.11",
          "form": "눈이 너무 귀여워요",
          "original_form": "눈이 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100013000.1.12",
          "form": "댕냥이들은 천사지요",
          "original_form": "댕냥이들은 천사지요",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013000.1.13",
          "form": "놀라도 귀엽고 졸라도 귀엽고ㅎ",
          "original_form": "놀라도 귀엽고 졸라도 귀엽고ㅎ",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013000.1.14",
          "form": "울음소리도 귀여워요ㅠ",
          "original_form": "울음소리도 귀여워요ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013000.1.15",
          "form": "가족들이 키우는 것을 반대하나요?",
          "original_form": "가족들이 키우는 것을 반대하나요?",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013000.1.16",
          "form": "네 조금 그래요ㅠ",
          "original_form": "네 조금 그래요ㅠ",
          "speaker_id": "1",
