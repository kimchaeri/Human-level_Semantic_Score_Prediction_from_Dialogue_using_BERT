      "utterance": [
        {
          "id": "MDRW2100012126.1.1",
          "form": "어떤 계절 좋아하세용",
          "original_form": "어떤 계절 좋아하세용",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012126.1.2",
          "form": "저는 가을이요!",
          "original_form": "저는 가을이요!",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012126.1.3",
          "form": "name2님은여?",
          "original_form": "&name2&님은여?",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012126.1.4",
          "form": "저도 가을이에요 ㅎ",
          "original_form": "저도 가을이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012126.1.5",
          "form": "다른 계절은 흠",
          "original_form": "다른 계절은 흠",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012126.1.6",
          "form": "봄은 미세먼지나 황사만 없으면 딱 좋을텐데...",
          "original_form": "봄은 미세먼지나 황사만 없으면 딱 좋을텐데...",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012126.1.7",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012126.1.8",
          "form": "근데 그것들이 너무 타격이....",
          "original_form": "근데 그것들이 너무 타격이....",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012126.1.9",
          "form": "정말요!!!!",
          "original_form": "정말요!!!!",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012126.1.10",
          "form": "이번에 비그치고 나면 꽤 쌀쌀해지겠어요",
          "original_form": "이번에 비그치고 나면 꽤 쌀쌀해지겠어요",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012126.1.11",
          "form": "네!! 지금도 좋더라구요",
          "original_form": "네!! 지금도 좋더라구요",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012126.1.12",
          "form": "비오는거 좋아하세요?",
          "original_form": "비오는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012126.1.13",
          "form": "보는 것만 좋아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "보는 것만 좋아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012126.1.14",
          "form": "저는 햇빛나는 날씨를 좋아해요",
          "original_form": "저는 햇빛나는 날씨를 좋아해요",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012126.1.15",
          "form": "저두요 근데 적당한 햇빛이요",
          "original_form": "저두요 근데 적당한 햇빛이요",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012126.1.16",
          "form": "맞아요ㅋㅋ음.. 말하자면 선선한 가을날씨에 어느 아침햇살...?",
          "original_form": "맞아요ㅋㅋ음.. 말하자면 선선한 가을날씨에 어느 아침햇살...?",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012126.1.17",
          "form": "뭔지 알아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭔지 알아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012126.1.18",
          "form": "모두 좋아하는 날씨네여",
          "original_form": "모두 좋아하는 날씨네여",
          "speaker_id": "2",
