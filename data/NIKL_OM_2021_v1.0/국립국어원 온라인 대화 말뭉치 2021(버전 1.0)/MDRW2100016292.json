      "utterance": [
        {
          "id": "MDRW2100016292.1.1",
          "form": "동호회해봣어요?",
          "original_form": "동호회해봣어요?",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100016292.1.2",
          "form": "전 직장내에서만 동호회해봤어요. name2님은요??",
          "original_form": "전 직장내에서만 동호회해봤어요. &name2&님은요??",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100016292.1.3",
          "form": "기타 동호회 들라했는데코로나가 터져서 ㅜ",
          "original_form": "기타 동호회 들라했는데코로나가 터져서 ㅜ",
          "speaker_id": "2",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100016292.1.4",
          "form": "못했어요",
          "original_form": "못했어요",
          "speaker_id": "2",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100016292.1.5",
          "form": "아쉬워요ㅠㅠ 저도 기타 배워보고싶군오",
          "original_form": "아쉬워요ㅠㅠ 저도 기타 배워보고싶군오",
          "speaker_id": "1",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100016292.1.6",
          "form": "기타 너무 있어보여서",
          "original_form": "기타 너무 있어보여서",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100016292.1.7",
          "form": "배우기싶어요 분위기있어서 ㅎㅎ",
          "original_form": "배우기싶어요 분위기있어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100016292.1.8",
          "form": "다른건 뭐배우고싶어요?",
          "original_form": "다른건 뭐배우고싶어요?",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100016292.1.9",
          "form": "제 친구가 스윙댄스 배우는데 재밌다고 하더라구요! 근데 전 몸치라서 못따라갈거같기도...ㅎㅎ",
          "original_form": "제 친구가 스윙댄스 배우는데 재밌다고 하더라구요! 근데 전 몸치라서 못따라갈거같기도...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100016292.1.10",
          "form": "헐 스윙댄스 재밌겠어요 ㅎㅎ",
          "original_form": "헐 스윙댄스 재밌겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100016292.1.11",
          "form": "저도 몸치라 운동못해요 ㅜ",
          "original_form": "저도 몸치라 운동못해요 ㅜ",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100016292.1.12",
          "form": "그쵸? 탭댄스 같은 것도 배워보고 싶고 ㅎㅎ 주로 댄스네요",
          "original_form": "그쵸? 탭댄스 같은 것도 배워보고 싶고 ㅎㅎ 주로 댄스네요",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100016292.1.13",
          "form": "춤추는거 재밌더라고요",
          "original_form": "춤추는거 재밌더라고요",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100016292.1.14",
          "form": "저도 방송댄스?",
          "original_form": "저도 방송댄스?",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100016292.1.15",
          "form": "배울때 재밌었어요",
          "original_form": "배울때 재밌었어요",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100016292.1.16",
          "form": "오 저도 배워봤어요 ㅋㅋㅋ 재밌긴한데 힘들더라구요",
          "original_form": "오 저도 배워봤어요 ㅋㅋㅋ 재밌긴한데 힘들더라구요",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100016292.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100016292.1.18",
          "form": "ㄱㅋㅋㅋㅋ 힘들었어요",
          "original_form": "ㄱㅋㅋㅋㅋ 힘들었어요",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100016292.1.19",
          "form": "ㅋㅋ전 한 달 배우고 그만뒀어요....",
          "original_form": "ㅋㅋ전 한 달 배우고 그만뒀어요....",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100016292.1.20",
          "form": "ㅋㅋㅋㅋ아 저랑 똑같아요",
          "original_form": "ㅋㅋㅋㅋ아 저랑 똑같아요",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
