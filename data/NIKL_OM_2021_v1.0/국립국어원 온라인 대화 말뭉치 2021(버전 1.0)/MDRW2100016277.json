      "utterance": [
        {
          "id": "MDRW2100016277.1.1",
          "form": "여행은 좋아하시나요",
          "original_form": "여행은 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016277.1.2",
          "form": "좋아하는 편이에요",
          "original_form": "좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016277.1.3",
          "form": "name1은 가고싶은 여행지 있으신가요",
          "original_form": "&name1&은 가고싶은 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016277.1.4",
          "form": "일단 이번 주말에 제주도 가기로 했엉ㅅ요",
          "original_form": "일단 이번 주말에 제주도 가기로 했엉ㅅ요",
          "speaker_id": "1",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016277.1.5",
          "form": "코로나 끝나면 외국 가고 싶네요 ㅎㅎ",
          "original_form": "코로나 끝나면 외국 가고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016277.1.6",
          "form": "제주도 너무 좋네요",
          "original_form": "제주도 너무 좋네요",
          "speaker_id": "2",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016277.1.7",
          "form": "제주도 가서 뭐 하실 예정이에요",
          "original_form": "제주도 가서 뭐 하실 예정이에요",
          "speaker_id": "2",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016277.1.8",
          "form": "힐링을 주제로... 뚜벅이 여행 하려고요 제주도 가보셧나요",
          "original_form": "힐링을 주제로... 뚜벅이 여행 하려고요 제주도 가보셧나요",
          "speaker_id": "1",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016277.1.9",
          "form": "저 예전에 수학여행으로 1번 가봤어요",
          "original_form": "저 예전에 수학여행으로 1번 가봤어요",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016277.1.10",
          "form": "뚜벅이 좋죠 갈곳은 정해두셨나요",
          "original_form": "뚜벅이 좋죠 갈곳은 정해두셨나요",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016277.1.11",
          "form": "함덕쪽으로 가려고요 ㅎㅎ 여행가서 느긋하게 돌아다니는 편인가요 아님 바쁘게 이것저것 다 보려는 스타일이세요",
          "original_form": "함덕쪽으로 가려고요 ㅎㅎ 여행가서 느긋하게 돌아다니는 편인가요 아님 바쁘게 이것저것 다 보려는 스타일이세요",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016277.1.12",
          "form": "저는 느긋한거요",
          "original_form": "저는 느긋한거요",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016277.1.13",
          "form": "그냥 가면가고 아니면 말고 스타일이라",
          "original_form": "그냥 가면가고 아니면 말고 스타일이라",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016277.1.14",
          "form": "name1은 느긋한 스타일이시죠?",
          "original_form": "&name1&은 느긋한 스타일이시죠?",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016277.1.15",
          "form": "오 맞아요 ㅋㅋㅋ 어떻게 아셨죠 ㅋㅋㅋㅋ",
          "original_form": "오 맞아요 ㅋㅋㅋ 어떻게 아셨죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016277.1.16",
          "form": "광고에서 많이 나오기도하고",
          "original_form": "광고에서 많이 나오기도하고",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016277.1.17",
          "form": "차트순위권에도 계속 있었잖아요",
          "original_form": "차트순위권에도 계속 있었잖아요",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016277.1.18",
          "form": "아 죄송해요",
          "original_form": "아 죄송해요",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016277.1.19",
          "form": "ㅋㅋㅋ 이해해요",
          "original_form": "ㅋㅋㅋ 이해해요",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016277.1.20",
          "form": "다른분과 헷갈렸어요",
          "original_form": "다른분과 헷갈렸어요",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016277.1.21",
          "form": "네 저도 그럴 때 많아요",
          "original_form": "네 저도 그럴 때 많아요",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016277.1.22",
          "form": "그냥 뚜벅이 여행즐기신다그래서",
          "original_form": "그냥 뚜벅이 여행즐기신다그래서",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016277.1.23",
          "form": "그럴 것  같았어요",
          "original_form": "그럴 것  같았어요",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016277.1.24",
          "form": "산이랑 바다 중엔 어디가 좋으세요",
          "original_form": "산이랑 바다 중엔 어디가 좋으세요",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016277.1.25",
          "form": "전 산..?",
          "original_form": "전 산..?",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016277.1.26",
          "form": "산에서 오는 기운이 좋아오",
          "original_form": "산에서 오는 기운이 좋아오",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016277.1.27",
          "form": "name1은 산이 좋으세요 바다가 좋으세요",
          "original_form": "&name1&은 산이 좋으세요 바다가 좋으세요",
