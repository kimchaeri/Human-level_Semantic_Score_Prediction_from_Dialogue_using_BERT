      "utterance": [
        {
          "id": "MDRW2100047248.1.1",
          "form": "큰애가 동생봐줄 나이네요",
          "original_form": "큰애가 동생봐줄 나이네요",
          "speaker_id": "1",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100047248.1.2",
          "form": "네 ㅋ근데 자기친구들 하고 노는걸더좋아하니ㅋㅋ 학원외 시간에는 잘없고..",
          "original_form": "네 ㅋ근데 자기친구들 하고 노는걸더좋아하니ㅋㅋ 학원외 시간에는 잘없고..",
          "speaker_id": "2",
          "time": "20210826 01:19"
        },
        {
          "id": "MDRW2100047248.1.3",
          "form": "오면 티비보고ㅋㅋ",
          "original_form": "오면 티비보고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:19"
        },
        {
          "id": "MDRW2100047248.1.4",
          "form": "음 원래 또래랑 교류하고싶죠 ㅋㅋ",
          "original_form": "음 원래 또래랑 교류하고싶죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100047248.1.5",
          "form": "세살은 최신 트랜드를 모르니깐ㅋㅋㅋ",
          "original_form": "세살은 최신 트랜드를 모르니깐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100047248.1.6",
          "form": "최신 트랜드까지ㅋㅋㅋ",
          "original_form": "최신 트랜드까지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047248.1.7",
          "form": "세살은 유투브나 게임 모르잖아요 ㅋㅋ",
          "original_form": "세살은 유투브나 게임 모르잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100047248.1.8",
          "form": "아휴 유투브는 몰라도 보는건ㅋㅋ잘봐요",
          "original_form": "아휴 유투브는 몰라도 보는건ㅋㅋ잘봐요",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100047248.1.9",
          "form": "ㅋㅋ 아 글쿤요 상어가족보나요?",
          "original_form": "ㅋㅋ 아 글쿤요 상어가족보나요?",
          "speaker_id": "1",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100047248.1.10",
          "form": "너무 좋아하죠ㅡ율동따라하면서",
          "original_form": "너무 좋아하죠ㅡ율동따라하면서",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100047248.1.11",
          "form": "아유 이쁘네 리듬타네 ㅎㅎ",
          "original_form": "아유 이쁘네 리듬타네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100047248.1.12",
          "form": "제법 따라해요~",
          "original_form": "제법 따라해요~",
          "speaker_id": "2",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100047248.1.13",
          "form": "흥이 많은 아이네요 ㅋㅋ",
          "original_form": "흥이 많은 아이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100047248.1.14",
          "form": "제가봐도 그래요 잘 춰요ㅋㅋ쪼만한게",
          "original_form": "제가봐도 그래요 잘 춰요ㅋㅋ쪼만한게",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100047248.1.15",
          "form": "ㅋㅋㅋ 연예계 큰물로 나가야겠네요 준비해두세요",
          "original_form": "ㅋㅋㅋ 연예계 큰물로 나가야겠네요 준비해두세요",
