      "utterance": [
        {
          "id": "MDRW2100010359.1.1",
          "form": "최근에 보신 영화 있으세요?",
          "original_form": "최근에 보신 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010359.1.2",
          "form": "미나리봤어요",
          "original_form": "미나리봤어요",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010359.1.3",
          "form": "재미없던데 ㅎㅎ",
          "original_form": "재미없던데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010359.1.4",
          "form": "아 저도 미나리 봤는데 생각보다는 재밌더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 미나리 봤는데 생각보다는 재밌더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010359.1.5",
          "form": "그런 작품성있는 영화랑은 잘 안 맞다고 생각했는데 의외로 괜찮았던ㅋㅋㅋ",
          "original_form": "그런 작품성있는 영화랑은 잘 안 맞다고 생각했는데 의외로 괜찮았던ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010359.1.6",
          "form": "기생충은 재밌게 봤어요 ㅎ의미가있어서요",
          "original_form": "기생충은 재밌게 봤어요 ㅎ의미가있어서요",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010359.1.7",
          "form": "미나리는 어렵게느꼈나봐요",
          "original_form": "미나리는 어렵게느꼈나봐요",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010359.1.8",
          "form": "저는 최근에 모가디슈 봤는데 재밌더라구요!!",
          "original_form": "저는 최근에 모가디슈 봤는데 재밌더라구요!!",
          "speaker_id": "1",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010359.1.9",
          "form": "잘 만든 영화라는 생각이 들었어요",
          "original_form": "잘 만든 영화라는 생각이 들었어요",
          "speaker_id": "1",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010359.1.10",
          "form": "오 보고싶너요",
          "original_form": "오 보고싶너요",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010359.1.11",
          "form": "씽크홀도 재밌다던데",
          "original_form": "씽크홀도 재밌다던데",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010359.1.12",
          "form": "싱크홀도 재밌어보여요!",
          "original_form": "싱크홀도 재밌어보여요!",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100010359.1.13",
          "form": "역시 여름에는 블록버스터가 재밌죠",
          "original_form": "역시 여름에는 블록버스터가 재밌죠",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100010359.1.14",
          "form": "네 코믹도 최고죠",
          "original_form": "네 코믹도 최고죠",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100010359.1.15",
          "form": "그래도 작년보다는 재밌는 영화 개봉을 많이하는거 같아요",
          "original_form": "그래도 작년보다는 재밌는 영화 개봉을 많이하는거 같아요",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100010359.1.16",
          "form": "작년엔 기억도 안나네요 ㅎㅎ",
          "original_form": "작년엔 기억도 안나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100010359.1.17",
          "form": "인생 영화 있으신가요??",
          "original_form": "인생 영화 있으신가요??",
          "speaker_id": "1",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100010359.1.18",
          "form": "이터널션샤인 너무 잘만든거같아요",
          "original_form": "이터널션샤인 너무 잘만든거같아요",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100010359.1.19",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100010359.1.20",
          "form": "저는 타이타닉 좋아해서 여러번 봤어요!",
          "original_form": "저는 타이타닉 좋아해서 여러번 봤어요!",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100010359.1.21",
          "form": "해리포터 시리즈도 질리도록 봤구요ㅋㅋㅋㅋ",
          "original_form": "해리포터 시리즈도 질리도록 봤구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100010359.1.22",
          "form": "해리포터는 처음나올때 정말 신기했었죠",
          "original_form": "해리포터는 처음나올때 정말 신기했었죠",
          "speaker_id": "2",
          "time": "20210825 06:14"
        }
      ]
    }
