          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000420.1.4",
          "form": "비안온다고했는디",
          "original_form": "비안온다고했는디",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000420.1.5",
          "form": "소나기 ㅡ.ㅡ",
          "original_form": "소나기 ㅡ.ㅡ",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000420.1.6",
          "form": "기상청 일해라!!!!!",
          "original_form": "기상청 일해라!!!!!",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000420.1.7",
          "form": "근데 지금은 또 안와",
          "original_form": "근데 지금은 또 안와",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000420.1.8",
          "form": "어이없셈",
          "original_form": "어이없셈",
          "speaker_id": "1",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000420.1.9",
          "form": "ㅋㅋㅋㅋㅋ밖에있을때만 비오네",
          "original_form": "ㅋㅋㅋㅋㅋ밖에있을때만 비오네",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000420.1.10",
          "form": "이번주부터 쭉 비온다던데",
          "original_form": "이번주부터 쭉 비온다던데",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000420.1.11",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000420.1.12",
          "form": "면허 따러가야하는데",
          "original_form": "면허 따러가야하는데",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000420.1.13",
          "form": "비오는거 너무 싫다..",
          "original_form": "비오는거 너무 싫다..",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000420.1.14",
          "form": "여보 비올때 나가는거 싫어하자너",
          "original_form": "여보 비올때 나가는거 싫어하자너",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000420.1.15",
          "form": "ㅋㅋㅋㅋㅋ강제 외출금지",
          "original_form": "ㅋㅋㅋㅋㅋ강제 외출금지",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000420.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ인정",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ인정",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000420.1.17",
          "form": "못나가겠다",
          "original_form": "못나가겠다",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000420.1.18",
          "form": "그래도 요즘은",
          "original_form": "그래도 요즘은",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000420.1.19",
          "form": "배달이 잘돼서 다행",
          "original_form": "배달이 잘돼서 다행",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000420.1.20",
          "form": "안나가도 크게 문제없으니까",
          "original_form": "안나가도 크게 문제없으니까",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000420.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ그치",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그치",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000420.1.22",
          "form": "응응 ㅋㅌㅌㅋ",
          "original_form": "응응 ㅋㅌㅌㅋ",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000420.1.23",
          "form": "그리고 선선해서",
          "original_form": "그리고 선선해서",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000420.1.24",
          "form": "나가더라도 별로 안더워서 좋아",
          "original_form": "나가더라도 별로 안더워서 좋아",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000420.1.25",
          "form": "날이 많이 풀렸긴하지",
          "original_form": "날이 많이 풀렸긴하지",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000420.1.26",
          "form": "확실히 더운거는 좀 가신듯",
          "original_form": "확실히 더운거는 좀 가신듯",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000420.1.27",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000420.1.28",
          "form": "곧 가을인가봐",
          "original_form": "곧 가을인가봐",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000420.1.29",
          "form": "가을도 지나고 빨리 겨울오면 좋겠다",
          "original_form": "가을도 지나고 빨리 겨울오면 좋겠다",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000420.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000420.1.31",
          "form": "딸기먹을수있삼",
          "original_form": "딸기먹을수있삼",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000420.1.32",
          "form": "겨울이 좋아?",
          "original_form": "겨울이 좋아?",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000420.1.33",
          "form": "ㅋㅌㅋㅋㅇㅈ",
          "original_form": "ㅋㅌㅋㅋㅇㅈ",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000420.1.34",
