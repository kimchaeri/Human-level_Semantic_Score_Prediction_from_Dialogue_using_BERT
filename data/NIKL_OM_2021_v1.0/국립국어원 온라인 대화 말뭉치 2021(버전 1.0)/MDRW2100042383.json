      "utterance": [
        {
          "id": "MDRW2100042383.1.1",
          "form": "네네 간단히 고구마랑 라떼에 마들렌을 먹었어요",
          "original_form": "네네 간단히 고구마랑 라떼에 마들렌을 먹었어요",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042383.1.2",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042383.1.3",
          "form": "넹 제육이요 ㅋㅋㅋ",
          "original_form": "넹 제육이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042383.1.4",
          "form": "마들렌 카페서요?",
          "original_form": "마들렌 카페서요?",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042383.1.5",
          "form": "아니요 어제 동네 빵집에서 샀어요ㅋㅋㅋ",
          "original_form": "아니요 어제 동네 빵집에서 샀어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042383.1.6",
          "form": "마싯겟다아 라떼는용",
          "original_form": "마싯겟다아 라떼는용",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042383.1.7",
          "form": "라떼는 카페에서 테이크아웃이죠.. 비가 와서 따아요️",
          "original_form": "라떼는 카페에서 테이크아웃이죠.. 비가 와서 따아요{emoji:☺}️",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042383.1.8",
          "form": "제육은 조리하신거세요?",
          "original_form": "제육은 조리하신거세요?",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042383.1.9",
          "form": "엄마가 싸서줫어용 해무라고  ㅋㅋㅋㅋ",
          "original_form": "엄마가 싸서줫어용 해무라고  ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042383.1.10",
          "form": "어제 어머님이 주신거가요.. 완전 맛있었을거 같네요",
          "original_form": "어제 어머님이 주신거가요.. 완전 맛있었을거 같네요",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042383.1.11",
          "form": "오늘 간식은 드셨나요?",
          "original_form": "오늘 간식은 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042383.1.12",
          "form": "네네 ㅋㅋ 아이스크림  먹엇어요!",
          "original_form": "네네 ㅋㅋ 아이스크림  먹엇어요!",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042383.1.13",
          "form": "배라",
          "original_form": "배라",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042383.1.14",
          "form": "배라 어떤맛을 좋아하시나요?",
          "original_form": "배라 어떤맛을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042383.1.15",
          "form": "오 민초와 체리쥬빌이라니..",
          "original_form": "오 민초와 체리쥬빌이라니..",
          "speaker_id": "2",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100042383.1.16",
          "form": "레인보우샤벳.. 상큼한거 좋아하시는군요",
          "original_form": "레인보우샤벳.. 상큼한거 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100042383.1.17",
          "form": "넹 name2님은요 ㅋㅋ",
          "original_form": "넹 &name2&님은요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042383.1.18",
          "form": "저는 민초/엄외계/아몬드봉봉이요ㅋㅋㅋㅋ",
          "original_form": "저는 민초/엄외계/아몬드봉봉이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042383.1.19",
          "form": "달달한 아이스 좋아해요",
          "original_form": "달달한 아이스 좋아해요",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042383.1.20",
