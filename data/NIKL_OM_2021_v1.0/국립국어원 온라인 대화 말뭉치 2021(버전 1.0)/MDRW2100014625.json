      "utterance": [
        {
          "id": "MDRW2100014625.1.1",
          "form": "나 네이버쇼핑에서 배 샀당",
          "original_form": "나 네이버쇼핑에서 배 샀당",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014625.1.2",
          "form": "배를?",
          "original_form": "배를?",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014625.1.3",
          "form": "얼마나?",
          "original_form": "얼마나?",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014625.1.4",
          "form": "3키로 샀옹 ㅋㅋ",
          "original_form": "3키로 샀옹 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014625.1.5",
          "form": "3키로 몇과수로?",
          "original_form": "3키로 몇과수로?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014625.1.6",
          "form": "3개에서 7개 준다든데 4개 왔엉",
          "original_form": "3개에서 7개 준다든데 4개 왔엉",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014625.1.7",
          "form": "3키로 4개면 배 제법 크겠당",
          "original_form": "3키로 4개면 배 제법 크겠당",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014625.1.8",
          "form": "크진않더라구 원래 배 큰거만 봐서 긍가",
          "original_form": "크진않더라구 원래 배 큰거만 봐서 긍가",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014625.1.9",
          "form": "작지도 않긴한데",
          "original_form": "작지도 않긴한데",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014625.1.10",
          "form": "배가 안큰데 4개 3키로면 과육이 단단하니 무거운갑다",
          "original_form": "배가 안큰데 4개 3키로면 과육이 단단하니 무거운갑다",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014625.1.11",
          "form": "먹어봄?",
          "original_form": "먹어봄?",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014625.1.12",
          "form": "아니 아직 ㅋㅋ 맛있었음 좋겠어",
          "original_form": "아니 아직 ㅋㅋ 맛있었음 좋겠어",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014625.1.13",
          "form": "이따 먹어봄 맛 후기도 알려줘ㅎ",
          "original_form": "이따 먹어봄 맛 후기도 알려줘ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014625.1.14",
          "form": "ㅇㅇ 먹어보고 알려줄게~",
          "original_form": "ㅇㅇ 먹어보고 알려줄게~",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014625.1.15",
          "form": "뿌리볼륨 집게도 샀당",
          "original_form": "뿌리볼륨 집게도 샀당",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014625.1.16",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014625.1.17",
          "form": "뭔지 알것같은ㅋㅋ",
          "original_form": "뭔지 알것같은ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014625.1.18",
          "form": "효과봄?",
          "original_form": "효과봄?",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014625.1.19",
          "form": "난 정전기포 샀는데ㅋㅋ",
          "original_form": "난 정전기포 샀는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014625.1.20",
          "form": "인자 청소 좀 해볼까하고",
          "original_form": "인자 청소 좀 해볼까하고",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014625.1.21",
          "form": "오 정전기포 괜차나",
          "original_form": "오 정전기포 괜차나",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014625.1.22",
          "form": "근데 이게 만든지 삼년지나면 정전기 잘 안 나나보더라",
          "original_form": "근데 이게 만든지 삼년지나면 정전기 잘 안 나나보더라",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014625.1.23",
          "form": "엇!",
          "original_form": "엇!",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014625.1.24",
          "form": "맞나?",
          "original_form": "맞나?",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014625.1.25",
          "form": "낸 그냥 썼는데",
          "original_form": "낸 그냥 썼는데",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014625.1.26",
          "form": "함 봐야겠다",
          "original_form": "함 봐야겠다",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014625.1.27",
          "form": "안뜾은거 좀된거 뒤지면 있을거라",
          "original_form": "안뜾은거 좀된거 뒤지면 있을거라",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014625.1.28",
          "form": "궁금해졓다ㅋ성능차이",
          "original_form": "궁금해졓다ㅋ성능차이",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014625.1.29",
          "form": "오 좀된거 부터 써야겠네",
          "original_form": "오 좀된거 부터 써야겠네",
          "speaker_id": "1",
          "time": "20210825 20:32"
        }
      ]
    }
  ]
