      "utterance": [
        {
          "id": "MDRW2100003945.1.1",
          "form": "지금 하는 일이 힘드네요",
          "original_form": "지금 하는 일이 힘드네요",
          "speaker_id": "1",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003945.1.2",
          "form": "안녕하세요 name1님 ㅜ 어떤 부분이 힘드신가요? 적성에 안맞으세요?",
          "original_form": "안녕하세요 &name1&님 ㅜ 어떤 부분이 힘드신가요? 적성에 안맞으세요?",
          "speaker_id": "2",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003945.1.3",
          "form": "아니요 그냥 근로소득이 작아서요ㅋ",
          "original_form": "아니요 그냥 근로소득이 작아서요ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003945.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 그러시구나 그러면 힘드실수도 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그러시구나 그러면 힘드실수도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003945.1.5",
          "form": "전 곧 학교 졸업해서",
          "original_form": "전 곧 학교 졸업해서",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003945.1.6",
          "form": "저도 돈잘버는 직업 없나 열심히 고민중이네요 ",
          "original_form": "저도 돈잘버는 직업 없나 열심히 고민중이네요 ",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003945.1.7",
          "form": "오호 어떤일 하실 생각이세요",
          "original_form": "오호 어떤일 하실 생각이세요",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003945.1.8",
          "form": "개발자쪽으로 생각하고있어요! 지금 전공이라서요",
          "original_form": "개발자쪽으로 생각하고있어요! 지금 전공이라서요",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003945.1.9",
          "form": "오 전공이 뭐에요?",
          "original_form": "오 전공이 뭐에요?",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003945.1.10",
          "form": "요즘 개발자가 대세",
          "original_form": "요즘 개발자가 대세",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003945.1.11",
          "form": "소프트웨어요!",
          "original_form": "소프트웨어요!",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003945.1.12",
          "form": "name1님은 어떤일하세요?",
          "original_form": "&name1&님은 어떤일하세요?",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003945.1.13",
          "form": "저는 교육쪽이요ㅋ",
          "original_form": "저는 교육쪽이요ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003945.1.14",
          "form": "헉 ㅋㅋㅋㅋㅋ 직접 다른 사람들 교육하시는거에요??",
          "original_form": "헉 ㅋㅋㅋㅋㅋ 직접 다른 사람들 교육하시는거에요??",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003945.1.15",
          "form": "네 뭐 그런 분야입니당ㅋ",
          "original_form": "네 뭐 그런 분야입니당ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003945.1.16",
          "form": "그럼 되게 말씀 잘하시겠다 ㅋㅋㅋ",
          "original_form": "그럼 되게 말씀 잘하시겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003945.1.17",
          "form": "저는 딴사람한테 뭐 갈켜주는거 너무 기빨려요 ㅓ ㅋㅋ",
          "original_form": "저는 딴사람한테 뭐 갈켜주는거 너무 기빨려요 ㅓ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100003945.1.18",
          "form": "저도ㅜㅜ 그냥 별 수 없이 하는거죵ㅋ",
          "original_form": "저도ㅜㅜ 그냥 별 수 없이 하는거죵ㅋ",
