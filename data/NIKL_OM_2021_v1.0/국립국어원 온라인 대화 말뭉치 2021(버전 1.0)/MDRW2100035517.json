      "utterance": [
        {
          "id": "MDRW2100035517.1.1",
          "form": "아뇨 택배시켰어요",
          "original_form": "아뇨 택배시켰어요",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100035517.1.2",
          "form": "제가 대구 살진않고",
          "original_form": "제가 대구 살진않고",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035517.1.3",
          "form": "엄마가 라디오에서 들으셨대서",
          "original_form": "엄마가 라디오에서 들으셨대서",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035517.1.4",
          "form": "저는 대구살아요 ㅋㅋ",
          "original_form": "저는 대구살아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035517.1.5",
          "form": "근데 그 떡집 이름 봤어요",
          "original_form": "근데 그 떡집 이름 봤어요",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035517.1.6",
          "form": "저희엄마도 영양떡 그집에서 사서 몇개주셔서 맛봤어요 ㅎ",
          "original_form": "저희엄마도 영양떡 그집에서 사서 몇개주셔서 맛봤어요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100035517.1.7",
          "form": "아하 뭔가 이미 나름 유명한집 이군요 ㅋㅋ",
          "original_form": "아하 뭔가 이미 나름 유명한집 이군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100035517.1.8",
          "form": "네 대구에서는 맛집인가봐요",
          "original_form": "네 대구에서는 맛집인가봐요",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100035517.1.9",
          "form": "싸긴하네요",
          "original_form": "싸긴하네요",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035517.1.10",
          "form": "천원씩이라니",
          "original_form": "천원씩이라니",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035517.1.11",
          "form": "하나씩 포장되어있죠?",
          "original_form": "하나씩 포장되어있죠?",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035517.1.12",
          "form": "네네 맞아요 개별포장",
          "original_form": "네네 맞아요 개별포장",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035517.1.13",
          "form": "그리고 그냥 랩포장 아니라  들고다니기좋고요 ㅋㅋ",
          "original_form": "그리고 그냥 랩포장 아니라  들고다니기좋고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035517.1.14",
          "form": "저는 지난번에 기지떡인가 시켰는데",
          "original_form": "저는 지난번에 기지떡인가 시켰는데",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100035517.1.15",
          "form": "개별포장이라 좋다고시켰더니",
          "original_form": "개별포장이라 좋다고시켰더니",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100035517.1.16",
          "form": "맛이...없더라구요",
          "original_form": "맛이...없더라구요",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100035517.1.17",
          "form": "후기는 좋던데 왜 내입맛엔 안맞는지",
          "original_form": "후기는 좋던데 왜 내입맛엔 안맞는지",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100035517.1.18",
          "form": "자끄 먹는 이야기만해서 배고파지시는거아니예요?ㅎ",
          "original_form": "자끄 먹는 이야기만해서 배고파지시는거아니예요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100035517.1.19",
          "form": "후기는 후기일뿐 결국 내입맛에 맞아야하는거같아요 ㅋㅋ",
          "original_form": "후기는 후기일뿐 결국 내입맛에 맞아야하는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100035517.1.20",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100035517.1.21",
          "form": "알바생들이였는지 ㅜㅠ",
          "original_form": "알바생들이였는지 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100035517.1.22",
          "form": "요새 블로그에리뷰 이런거로 돈받는사람 많으니까요ㅜㅜ",
          "original_form": "요새 블로그에리뷰 이런거로 돈받는사람 많으니까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100035517.1.23",
          "form": "아 방금 꼬북칩 먹었어요 ㅋㄲ",
          "original_form": "아 방금 꼬북칩 먹었어요 ㅋㄲ",
          "speaker_id": "2",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100035517.1.24",
          "form": "꼬북칩 맛나쥬 ㅋ",
          "original_form": "꼬북칩 맛나쥬 ㅋ",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100035517.1.25",
          "form": "오늘은 망원동 티라미슈가 먹고싶어요 ㅋㅋ",
          "original_form": "오늘은 망원동 티라미슈가 먹고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035517.1.26",
          "form": "관리중이 더 먹고싶은게 많아지는듯해요",
          "original_form": "관리중이 더 먹고싶은게 많아지는듯해요",
          "speaker_id": "1",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100035517.1.27",
          "form": "저는 아까 쑥절편 사려다가 ㅜㅠ",
          "original_form": "저는 아까 쑥절편 사려다가 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100035517.1.28",
          "form": "품절당해서 갑자기 못사니 간절해지네요",
          "original_form": "품절당해서 갑자기 못사니 간절해지네요",
          "speaker_id": "1",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100035517.1.29",
          "form": "오늘 관리1일차라서 잘 참아보려고요ㅜ",
          "original_form": "오늘 관리1일차라서 잘 참아보려고요ㅜ",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100035517.1.30",
          "form": "추석전까지는 ㅜㅜ진짜 열심히조절하려고요",
          "original_form": "추석전까지는 ㅜㅜ진짜 열심히조절하려고요",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100035517.1.31",
          "form": "아 추석전까지",
          "original_form": "아 추석전까지",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100035517.1.32",
          "form": "데드라인이 있으시군요 ㅎ",
          "original_form": "데드라인이 있으시군요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 17:37"
        }
      ]
