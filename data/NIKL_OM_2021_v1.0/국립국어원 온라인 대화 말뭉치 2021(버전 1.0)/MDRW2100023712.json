      "utterance": [
        {
          "id": "MDRW2100023712.1.1",
          "form": "군대 보안법 쉽게 생각하다가",
          "original_form": "군대 보안법 쉽게 생각하다가",
          "speaker_id": "2",
          "time": "20210830 15:19"
        },
        {
          "id": "MDRW2100023712.1.2",
          "form": "울면서 영창가는데",
          "original_form": "울면서 영창가는데",
          "speaker_id": "2",
          "time": "20210830 15:19"
        },
        {
          "id": "MDRW2100023712.1.3",
          "form": "그 친구도 아마 그러지 않았을까..",
          "original_form": "그 친구도 아마 그러지 않았을까..",
          "speaker_id": "2",
          "time": "20210830 15:19"
        },
        {
          "id": "MDRW2100023712.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:19"
        },
        {
          "id": "MDRW2100023712.1.5",
          "form": "안가고 제대로제대햇든데요",
          "original_form": "안가고 제대로제대햇든데요",
          "speaker_id": "1",
          "time": "20210830 15:19"
        },
        {
          "id": "MDRW2100023712.1.6",
          "form": "뻑하면 외출",
          "original_form": "뻑하면 외출",
          "speaker_id": "1",
          "time": "20210830 15:20"
        },
        {
          "id": "MDRW2100023712.1.7",
          "form": "공군 해군은 1달에 1일씩 쌓여서 외출나오죠ㅋㅋ",
          "original_form": "공군 해군은 1달에 1일씩 쌓여서 외출나오죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:20"
        },
        {
          "id": "MDRW2100023712.1.8",
          "form": "기무부대가 나와서 뜯어보면",
          "original_form": "기무부대가 나와서 뜯어보면",
          "speaker_id": "2",
          "time": "20210830 15:20"
        },
        {
          "id": "MDRW2100023712.1.9",
          "form": "다 걸리는데 운좋네",
          "original_form": "다 걸리는데 운좋네",
          "speaker_id": "2",
          "time": "20210830 15:20"
        },
        {
          "id": "MDRW2100023712.1.10",
          "form": "운이좋앗죠 진짜",
          "original_form": "운이좋앗죠 진짜",
          "speaker_id": "1",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100023712.1.11",
          "form": "걔는꿀빨앗어요 ㅋㅋㅋㅋ",
          "original_form": "걔는꿀빨앗어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100023712.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:22"
        },
        {
          "id": "MDRW2100023712.1.13",
          "form": "진짜 군대에서 할거없어서 책읽고 운동하고 노래배우고 그랬는데",
          "original_form": "진짜 군대에서 할거없어서 책읽고 운동하고 노래배우고 그랬는데",
          "speaker_id": "2",
          "time": "20210830 15:22"
        },
        {
          "id": "MDRW2100023712.1.14",
          "form": "요샌 폰있으니 뭐",
          "original_form": "요샌 폰있으니 뭐",
          "speaker_id": "2",
          "time": "20210830 15:22"
        },
        {
          "id": "MDRW2100023712.1.15",
          "form": "심심할 겨를도 없을듯요",
          "original_form": "심심할 겨를도 없을듯요",
          "speaker_id": "2",
          "time": "20210830 15:22"
        },
        {
          "id": "MDRW2100023712.1.16",
          "form": "전남친 하두배달음식먹어서 군대에서 몸무게 최대치찍음요",
          "original_form": "전남친 하두배달음식먹어서 군대에서 몸무게 최대치찍음요",
          "speaker_id": "1",
          "time": "20210830 15:23"
        },
        {
          "id": "MDRW2100023712.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:23"
        },
        {
          "id": "MDRW2100023712.1.18",
          "form": "신기한케이스 진짜 걔얘기듣고 군대아무것도아닌거같은 느낌",
          "original_form": "신기한케이스 진짜 걔얘기듣고 군대아무것도아닌거같은 느낌",
          "speaker_id": "1",
          "time": "20210830 15:23"
        },
        {
          "id": "MDRW2100023712.1.19",
          "form": "그 사람이 그런 자리를 운좋게 간거죠",
          "original_form": "그 사람이 그런 자리를 운좋게 간거죠",
          "speaker_id": "2",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100023712.1.20",
          "form": "근데 공군 대도시 근무하는 사람들 그런애들 많았음",
          "original_form": "근데 공군 대도시 근무하는 사람들 그런애들 많았음",
          "speaker_id": "2",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100023712.1.21",
          "form": "걔는 강릉 ㅋㅋㅋㅋㅋ",
          "original_form": "걔는 강릉 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100023712.1.22",
          "form": "와.. 친구 강릉가고싶어했는데",
          "original_form": "와.. 친구 강릉가고싶어했는데",
          "speaker_id": "2",
          "time": "20210830 15:27"
        },
        {
          "id": "MDRW2100023712.1.23",
          "form": "군무원 친구가 공군이거든요",
          "original_form": "군무원 친구가 공군이거든요",
          "speaker_id": "2",
          "time": "20210830 15:27"
        },
        {
          "id": "MDRW2100023712.1.24",
          "form": "과가 항공이라서",
          "original_form": "과가 항공이라서",
          "speaker_id": "2",
          "time": "20210830 15:27"
        },
        {
          "id": "MDRW2100023712.1.25",
          "form": "아진짜요? ㅋㅋㅋㅋ",
          "original_form": "아진짜요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:27"
        },
        {
          "id": "MDRW2100023712.1.26",
          "form": "쟂수없게 예천으로 떨어져서",
          "original_form": "쟂수없게 예천으로 떨어져서",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.27",
          "form": "8월 휴가때 걔 보러감요",
          "original_form": "8월 휴가때 걔 보러감요",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.28",
          "form": "와 제친구 기상단인데",
          "original_form": "와 제친구 기상단인데",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.29",
          "form": "거기서 새똥도 맞고",
          "original_form": "거기서 새똥도 맞고",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.30",
          "form": "친구도 기상단이에요?",
          "original_form": "친구도 기상단이에요?",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.31",
          "form": "기상단이 뭡니까?",
          "original_form": "기상단이 뭡니까?",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.32",
          "form": "친구한테말하면 바로알껄요 ㅋㅋㅋㅋ",
          "original_form": "친구한테말하면 바로알껄요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.33",
          "form": "16전투비행단이던데",
          "original_form": "16전투비행단이던데",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.34",
          "form": "아 아닠ㅋㅋㅋ",
          "original_form": "아 아닠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.35",
          "form": "군무원이요",
          "original_form": "군무원이요",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.36",
          "form": "사진 찍은거에 그렇게 써있었음",
          "original_form": "사진 찍은거에 그렇게 써있었음",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.37",
          "form": "기상단이 뭐지",
          "original_form": "기상단이 뭐지",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.38",
          "form": "물어봄 ㄱㄷ",
          "original_form": "물어봄 ㄱㄷ",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023712.1.39",
          "form": "ㅇㅋ ㄱㄱ",
          "original_form": "ㅇㅋ ㄱㄱ",
          "speaker_id": "1",
          "time": "20210830 15:29"
        },
        {
          "id": "MDRW2100023712.1.40",
          "form": "바쁜듯",
          "original_form": "바쁜듯",
          "speaker_id": "2",
          "time": "20210830 15:31"
        },
        {
          "id": "MDRW2100023712.1.41",
          "form": "거긴 뭐하는덴데요?",
          "original_form": "거긴 뭐하는덴데요?",
          "speaker_id": "2",
          "time": "20210830 15:32"
        },
        {
          "id": "MDRW2100023712.1.42",
          "form": "알아봣자 뭐하겟어요 ㅋㅋㅋㅋㅋ",
          "original_form": "알아봣자 뭐하겟어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:32"
        },
        {
          "id": "MDRW2100023712.1.43",
          "form": "기밀이라 말못해요",
          "original_form": "기밀이라 말못해요",
          "speaker_id": "1",
          "time": "20210830 15:32"
        },
        {
          "id": "MDRW2100023712.1.44",
          "form": "아 이걸 안속네",
          "original_form": "아 이걸 안속네",
          "speaker_id": "2",
          "time": "20210830 15:33"
        },
        {
          "id": "MDRW2100023712.1.45",
          "form": "아무튼 기밀이군요",
          "original_form": "아무튼 기밀이군요",
          "speaker_id": "2",
          "time": "20210830 15:33"
        },
        {
          "id": "MDRW2100023712.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:33"
        },
        {
          "id": "MDRW2100023712.1.47",
          "form": "제 직업이면 모르겟는데 친구직업인데 발설했다가는,,",
          "original_form": "제 직업이면 모르겟는데 친구직업인데 발설했다가는,,",
          "speaker_id": "1",
          "time": "20210830 15:33"
        },
        {
          "id": "MDRW2100023712.1.48",
          "form": "잘 모른답디다 아는 형이 장교로 일해서 듣긴 했다는데",
          "original_form": "잘 모른답디다 아는 형이 장교로 일해서 듣긴 했다는데",
          "speaker_id": "2",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100023712.1.49",
          "form": "근데 군대 은근 잘 아시네요 신기하네",
          "original_form": "근데 군대 은근 잘 아시네요 신기하네",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100023712.1.50",
          "form": "이정도로 아시는 분은 첨임",
          "original_form": "이정도로 아시는 분은 첨임",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100023712.1.51",
          "form": "아 그래요?ㅋㅋㅋㅋㅋ",
          "original_form": "아 그래요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100023712.1.52",
          "form": "뭘잘아알아요 아무것도모른ㄴ데",
          "original_form": "뭘잘아알아요 아무것도모른ㄴ데",
          "speaker_id": "1",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100023712.1.53",
          "form": "이정도만 해도 뭐",
          "original_form": "이정도만 해도 뭐",
          "speaker_id": "2",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100023712.1.54",
          "form": "군필여고생 가능",
          "original_form": "군필여고생 가능",
          "speaker_id": "2",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100023712.1.55",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023712.1.56",
          "form": "남친군대보내서 그런듯요",
          "original_form": "남친군대보내서 그런듯요",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023712.1.57",
          "form": "두번보냈거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "두번보냈거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023712.1.58",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100023712.1.59",
          "form": "고무신 두번이나",
          "original_form": "고무신 두번이나",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100023712.1.60",
          "form": "일말상초가 고비라고 매번 그랬는데",
          "original_form": "일말상초가 고비라고 매번 그랬는데",
          "speaker_id": "2",
          "time": "20210830 15:42"
        },
        {
          "id": "MDRW2100023712.1.61",
          "form": "깐건 아니죠?",
          "original_form": "깐건 아니죠?",
          "speaker_id": "2",
          "time": "20210830 15:42"
        },
        {
          "id": "MDRW2100023712.1.62",
          "form": "보내고 다른 사람이랑 잘됐다던지...",
          "original_form": "보내고 다른 사람이랑 잘됐다던지...",
          "speaker_id": "2",
          "time": "20210830 15:42"
        },
