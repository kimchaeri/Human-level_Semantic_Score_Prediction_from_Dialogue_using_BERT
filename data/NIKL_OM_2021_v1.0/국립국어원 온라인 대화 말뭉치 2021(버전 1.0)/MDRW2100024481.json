      "utterance": [
        {
          "id": "MDRW2100024481.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100024481.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100024481.1.3",
          "form": "반려동물 키우고 계신가요?",
          "original_form": "반려동물 키우고 계신가요?",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100024481.1.4",
          "form": "아니요 키우는동물이 없네요\r\n어떤거 키우세요?",
          "original_form": "아니요 키우는동물이 없네요\r\n어떤거 키우세요?",
          "speaker_id": "2",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100024481.1.5",
          "form": "저도 안 키우고 있어요",
          "original_form": "저도 안 키우고 있어요",
          "speaker_id": "1",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100024481.1.6",
          "form": "관리할 자신이 없어서...",
          "original_form": "관리할 자신이 없어서...",
          "speaker_id": "1",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100024481.1.7",
          "form": "귀엽기는 한데 막상 키운다고 하면 손이 많이 가겠죠",
          "original_form": "귀엽기는 한데 막상 키운다고 하면 손이 많이 가겠죠",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100024481.1.8",
          "form": "강아지 키우고 싶었는데 털 많이 빠진다는 소리 듣고 포기했어요",
          "original_form": "강아지 키우고 싶었는데 털 많이 빠진다는 소리 듣고 포기했어요",
          "speaker_id": "1",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100024481.1.9",
          "form": "동물털은 심하긴하더라고요\r\n가끔 친구네 가면 그건 어쩔수없나봐요",
          "original_form": "동물털은 심하긴하더라고요\r\n가끔 친구네 가면 그건 어쩔수없나봐요",
          "speaker_id": "2",
          "time": "20210831 11:29"
        },
        {
          "id": "MDRW2100024481.1.10",
          "form": "친구분이 키우는 동물이 있으신가봐요",
          "original_form": "친구분이 키우는 동물이 있으신가봐요",
          "speaker_id": "1",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100024481.1.11",
          "form": "강아지 키웠거든요 지금은 없지만요\r\n반려동물이 가는거 보는것도 힘들어서 더 안키우고 싶긴해요",
          "original_form": "강아지 키웠거든요 지금은 없지만요\r\n반려동물이 가는거 보는것도 힘들어서 더 안키우고 싶긴해요",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024481.1.12",
          "form": "아하... 같이 오래 지냈다면 보내기 힘들죠... 결국은 가족이었으니까요",
          "original_form": "아하... 같이 오래 지냈다면 보내기 힘들죠... 결국은 가족이었으니까요",
          "speaker_id": "1",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024481.1.13",
          "form": "맞아요 진짜 힘들어하더라고요",
          "original_form": "맞아요 진짜 힘들어하더라고요",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024481.1.14",
          "form": "그럼에도 불구하고 새로운 친구 데려오는 분도 있으시더라구요",
          "original_form": "그럼에도 불구하고 새로운 친구 데려오는 분도 있으시더라구요",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024481.1.15",
          "form": "그만큼 이쁜거겟져\r\n키우는 사람들은 꾸준히 키우는거 같아여",
          "original_form": "그만큼 이쁜거겟져\r\n키우는 사람들은 꾸준히 키우는거 같아여",
          "speaker_id": "2",
          "time": "20210831 11:34"
        }
      ]
