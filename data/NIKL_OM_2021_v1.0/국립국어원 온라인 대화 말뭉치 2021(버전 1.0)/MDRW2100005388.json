      "utterance": [
        {
          "id": "MDRW2100005388.1.1",
          "form": "혹시 어떤음식 좋아하세요?",
          "original_form": "혹시 어떤음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005388.1.2",
          "form": "저는 치킨이랑 피자 좋아해요",
          "original_form": "저는 치킨이랑 피자 좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005388.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005388.1.4",
          "form": "아 저는 라면같은 면종류 되게 좋아해요",
          "original_form": "아 저는 라면같은 면종류 되게 좋아해요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005388.1.5",
          "form": "면 좋아하세요?",
          "original_form": "면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005388.1.6",
          "form": "앗 저는 그렇게 좋아하진않는데",
          "original_form": "앗 저는 그렇게 좋아하진않는데",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005388.1.7",
          "form": "간단하게 먹기좋아서 자주 먹는거같아요",
          "original_form": "간단하게 먹기좋아서 자주 먹는거같아요",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005388.1.8",
          "form": "오늘도 점심에 컵라면 먹었어요 ㅎㅎ",
          "original_form": "오늘도 점심에 컵라면 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005388.1.9",
          "form": "아 그러면 해외가서 드신음식중에 좋아하는거 있으세요?",
          "original_form": "아 그러면 해외가서 드신음식중에 좋아하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005388.1.10",
          "form": "앗 저는 일본가서 규카츠를 먹어봤는데",
          "original_form": "앗 저는 일본가서 규카츠를 먹어봤는데",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005388.1.11",
          "form": "그게 되게 맛있었어요! 타코야키랑요!ㅎㅎ",
          "original_form": "그게 되게 맛있었어요! 타코야키랑요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005388.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005388.1.13",
          "form": "아 저는 해외 못나가봐서...",
          "original_form": "아 저는 해외 못나가봐서...",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005388.1.14",
          "form": "내년이나 내후년에 일본 정말 가고싶어요",
          "original_form": "내년이나 내후년에 일본 정말 가고싶어요",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005388.1.15",
          "form": "일본 가셔서 라멘도 드셔보셨어요?",
          "original_form": "일본 가셔서 라멘도 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005388.1.16",
          "form": "앗 그렇군요ㅠㅠ 지금은 코로나라 너무 아쉬우시겠어요",
          "original_form": "앗 그렇군요ㅠㅠ 지금은 코로나라 너무 아쉬우시겠어요",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005388.1.17",
          "form": "네네 라멘도 먹어봤어요!",
          "original_form": "네네 라멘도 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005388.1.18",
          "form": "근데 라멘 맛은 여기서 먹는거나 비슷비슷했던거같아요ㅎㅎ",
          "original_form": "근데 라멘 맛은 여기서 먹는거나 비슷비슷했던거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005388.1.19",
          "form": "라멘중에 어떤라멘이 제일 맛있었어요?",
          "original_form": "라멘중에 어떤라멘이 제일 맛있었어요?",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005388.1.20",
          "form": "저희동네는 라멘집이 하나라 많이 못먹어보네요",
          "original_form": "저희동네는 라멘집이 하나라 많이 못먹어보네요",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005388.1.21",
          "form": "전 입맛이 무난한 편이라",
          "original_form": "전 입맛이 무난한 편이라",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005388.1.22",
          "form": "항상 돈코츠 라멘 이런거 먹었어요!",
          "original_form": "항상 돈코츠 라멘 이런거 먹었어요!",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005388.1.23",
          "form": "앗 동네에 라멘집이 있으신가요? 부러워요ㅠㅠ",
          "original_form": "앗 동네에 라멘집이 있으신가요? 부러워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005388.1.24",
          "form": "원래 없었는데 올해 하나 생겼어요",
          "original_form": "원래 없었는데 올해 하나 생겼어요",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005388.1.25",
          "form": "근데 메뉴가 돈코츠랑 탄탄멘? 그거 두개밖에 없어요..",
          "original_form": "근데 메뉴가 돈코츠랑 탄탄멘? 그거 두개밖에 없어요..",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005388.1.26",
          "form": "헉.... 완전 메뉴 두개로 승부보는 가게인가봐요",
          "original_form": "헉.... 완전 메뉴 두개로 승부보는 가게인가봐요",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005388.1.27",
          "form": "사장님이 자신감이 있으신가봐요",
          "original_form": "사장님이 자신감이 있으신가봐요",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005388.1.28",
          "form": "name1님은 배달음식중에는 좋아하는 음식 있으세요?",
          "original_form": "&name1&님은 배달음식중에는 좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005388.1.29",
          "form": "저는 분식이나 그런게 되게 좋더라구요",
          "original_form": "저는 분식이나 그런게 되게 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005388.1.30",
          "form": "야채같은건 안좋아해서..",
          "original_form": "야채같은건 안좋아해서..",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005388.1.31",
          "form": "앗 저도 떡볶이 좋아해요!",
          "original_form": "앗 저도 떡볶이 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005388.1.32",
          "form": "배떡에 로제떡볶이",
          "original_form": "배떡에 로제떡볶이",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005388.1.33",
          "form": "참 맛있더라구요 ㅎㅎ",
          "original_form": "참 맛있더라구요 ㅎㅎ",
