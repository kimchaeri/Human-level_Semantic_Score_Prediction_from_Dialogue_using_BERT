      "utterance": [
        {
          "id": "MDRW2100038385.1.1",
          "form": "마자요 !",
          "original_form": "마자요 !",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038385.1.2",
          "form": "글구 샹궈는 더 비싸잖아요 양은 적은거같은데 ㅋㅋ",
          "original_form": "글구 샹궈는 더 비싸잖아요 양은 적은거같은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038385.1.3",
          "form": "국물로 뿔리지를 못하니까",
          "original_form": "국물로 뿔리지를 못하니까",
          "speaker_id": "2",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038385.1.4",
          "form": "샹궈는 더 얼얼하기도 하고요",
          "original_form": "샹궈는 더 얼얼하기도 하고요",
          "speaker_id": "2",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038385.1.5",
          "form": "맞아요 ㅎㅎ 더 맵기도 하죠",
          "original_form": "맞아요 ㅎㅎ 더 맵기도 하죠",
          "speaker_id": "1",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100038385.1.6",
          "form": "그러면 재료중에 뭐 젤 좋아하세요?",
          "original_form": "그러면 재료중에 뭐 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100038385.1.7",
          "form": "배추, 숙주, 중국당면, 푸주, 완자요 ㅎㅎ",
          "original_form": "배추, 숙주, 중국당면, 푸주, 완자요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100038385.1.8",
          "form": "파란 향채는 안좋아해요 ㅎㅎ",
          "original_form": "파란 향채는 안좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100038385.1.9",
          "form": "엇 저두 건두부 푸주 젤 좋아해여 ㅎㅎ",
          "original_form": "엇 저두 건두부 푸주 젤 좋아해여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100038385.1.10",
          "form": "옥수수면도 좋아하구요 !!",
          "original_form": "옥수수면도 좋아하구요 !!",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100038385.1.11",
          "form": "푸주 진짜 맛있죠",
          "original_form": "푸주 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100038385.1.12",
          "form": "마자요ㅜㅜ식감이 좋습니다",
          "original_form": "마자요ㅜㅜ식감이 좋습니다",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100038385.1.13",
          "form": "저는 분모자는 싫어해요 ㅋㅋㅋ",
          "original_form": "저는 분모자는 싫어해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100038385.1.14",
          "form": "푸주 마트에서 팔면 좋겠어요",
          "original_form": "푸주 마트에서 팔면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038385.1.15",
          "form": "전 좋아하는데",
          "original_form": "전 좋아하는데",
          "speaker_id": "2",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038385.1.16",
          "form": "국물에 오래두면 좀 그렇지만",
          "original_form": "국물에 오래두면 좀 그렇지만",
          "speaker_id": "2",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038385.1.17",
          "form": "너무 떡같아서 ㅜㅜ 아 맞아요",
          "original_form": "너무 떡같아서 ㅜㅜ 아 맞아요",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038385.1.18",
          "form": "퍼지더라구여 ㅋㅋㅋ",
          "original_form": "퍼지더라구여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100038385.1.19",
          "form": "네, 흐물텅해지구ㅠ",
          "original_form": "네, 흐물텅해지구ㅠ",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038385.1.20",
          "form": "하 오늘 저녁은 마라탕입니다.",
          "original_form": "하 오늘 저녁은 마라탕입니다.",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038385.1.21",
          "form": "넘 먹고싶네요 ㅋㅋ",
          "original_form": "넘 먹고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038385.1.22",
          "form": "호오, 참 묘하게 중독되는 음식이긴 하단말이에요",
          "original_form": "호오, 참 묘하게 중독되는 음식이긴 하단말이에요",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100038385.1.23",
          "form": "전 근데 마라탕도 마라탕인데 거기 채소들 따로 팔 생각은 없는지 묻고싶어요",
          "original_form": "전 근데 마라탕도 마라탕인데 거기 채소들 따로 팔 생각은 없는지 묻고싶어요",
          "speaker_id": "2",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038385.1.24",
          "form": "아 ㅋㅋㅋㄱㅋㄱ 마자요 ㅋㅋㄲ",
          "original_form": "아 ㅋㅋㅋㄱㅋㄱ 마자요 ㅋㅋㄲ",
