      "utterance": [
        {
          "id": "MDRW2100041663.1.1",
          "form": "어제 쇼핑했어요",
          "original_form": "어제 쇼핑했어요",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041663.1.2",
          "form": "님 경기도에",
          "original_form": "님 경기도에",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041663.1.3",
          "form": "사시면",
          "original_form": "사시면",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041663.1.4",
          "form": "경기도 청년들한테",
          "original_form": "경기도 청년들한테",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041663.1.5",
          "form": "사시나?",
          "original_form": "사시나?",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041663.1.6",
          "form": "도서상품권 3만원어치를 준대요",
          "original_form": "도서상품권 3만원어치를 준대요",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041663.1.7",
          "form": "????",
          "original_form": "????",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041663.1.8",
          "form": "이거 말해주고 싶었다",
          "original_form": "이거 말해주고 싶었다",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041663.1.9",
          "form": "아 그거 봤는데",
          "original_form": "아 그거 봤는데",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041663.1.10",
          "form": "사이트터졌다면서요",
          "original_form": "사이트터졌다면서요",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041663.1.11",
          "form": "터진 건 아닌듯한데",
          "original_form": "터진 건 아닌듯한데",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041663.1.12",
          "form": "대기시간이 1시간이더라",
          "original_form": "대기시간이 1시간이더라",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041663.1.13",
          "form": "난 새벽에 햇어",
          "original_form": "난 새벽에 햇어",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041663.1.14",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041663.1.15",
          "form": "전독시 2부 나온대요",
          "original_form": "전독시 2부 나온대요",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100041663.1.16",
          "form": "ㅇㅁㅇ?? 2부??",
          "original_form": "ㅇㅁㅇ?? 2부??",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041663.1.17",
          "form": "종이책? 이북??",
          "original_form": "종이책? 이북??",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041663.1.18",
          "form": "종이책은 ^^..이북이겟지",
          "original_form": "종이책은 ^^..이북이겟지",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041663.1.19",
          "form": "종이책은 하반기 준비중이고",
          "original_form": "종이책은 하반기 준비중이고",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041663.1.20",
          "form": "2부는 이북이구",
          "original_form": "2부는 이북이구",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041663.1.21",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041663.1.22",
          "form": "삼만원 타내서 탈탈 털어야지",
          "original_form": "삼만원 타내서 탈탈 털어야지",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041663.1.23",
          "form": "아무튼 이북 편해 조아",
          "original_form": "아무튼 이북 편해 조아",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041663.1.24",
          "form": "난... 종이책을 더 사고싶어요",
          "original_form": "난... 종이책을 더 사고싶어요",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100041663.1.25",
          "form": "전독시가 아니더라두",
          "original_form": "전독시가 아니더라두",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100041663.1.26",
          "form": "흠 그러고보니",
          "original_form": "흠 그러고보니",
          "speaker_id": "2",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100041663.1.27",
          "form": "위대한 수업에 나오는 선생님들 책을 사도 좋을 것 같군요",
          "original_form": "위대한 수업에 나오는 선생님들 책을 사도 좋을 것 같군요",
          "speaker_id": "2",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100041663.1.28",
          "form": "ㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋ 더 첫수업이 넘 인상깊었다 리-다는 무엇인가",
          "original_form": "ㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋ 더 첫수업이 넘 인상깊었다 리-다는 무엇인가",
          "speaker_id": "1",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100041663.1.29",
          "form": "후후..총균쇠만 사도",
          "original_form": "후후..총균쇠만 사도",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100041663.1.30",
          "form": "이득일거같아여",
          "original_form": "이득일거같아여",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100041663.1.31",
          "form": "안삿나요",
          "original_form": "안삿나요",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100041663.1.32",
          "form": "이북으론 잇는데",
          "original_form": "이북으론 잇는데",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100041663.1.33",
          "form": "종이론 업어",
          "original_form": "종이론 업어",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100041663.1.34",
          "form": "글고 나 솔직히",
          "original_form": "글고 나 솔직히",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100041663.1.35",
          "form": "사피엔스 안읽엇어",
          "original_form": "사피엔스 안읽엇어",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100041663.1.36",
          "form": "(양심고백",
          "original_form": "(양심고백",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100041663.1.37",
          "form": "아니 모 안읽을수도 있죠ㅋㅋㅋㅋㅋ",
          "original_form": "아니 모 안읽을수도 있죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100041663.1.38",
          "form": "난 님이 책으로 갖고 있을 줄 알고",
          "original_form": "난 님이 책으로 갖고 있을 줄 알고",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100041663.1.39",
          "form": "물어본거였어욬ㅋㅋㅋ",
          "original_form": "물어본거였어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100041663.1.40",
          "form": "유발 선생님",
          "original_form": "유발 선생님",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100041663.1.41",
          "form": "ㅈㅅ함미다..",
          "original_form": "ㅈㅅ함미다..",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100041663.1.42",
          "form": "그치만 사피엔스는 왠지",
          "original_form": "그치만 사피엔스는 왠지",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100041663.1.43",
          "form": "정이 안갓어",
          "original_form": "정이 안갓어",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100041663.1.44",
          "form": "그럴 수 잇지",
          "original_form": "그럴 수 잇지",
          "speaker_id": "1",
          "time": "20210907 15:24"
        }
      ]
