      "utterance": [
        {
          "id": "MDRW2100007776.1.1",
          "form": "좋아하는 음식이나 메뉴가있으신가요",
          "original_form": "좋아하는 음식이나 메뉴가있으신가요",
          "speaker_id": "1",
          "time": "20210822 10:37"
        },
        {
          "id": "MDRW2100007776.1.2",
          "form": "초밥 좋아해요ㅎㅎ 뭐좋아하세요?",
          "original_form": "초밥 좋아해요ㅎㅎ 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 10:38"
        },
        {
          "id": "MDRW2100007776.1.3",
          "form": "저는 피자요",
          "original_form": "저는 피자요",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007776.1.4",
          "form": "빵vs밥?",
          "original_form": "빵vs밥?",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007776.1.5",
          "form": "저는 밥파에요ㅎㅎ",
          "original_form": "저는 밥파에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007776.1.6",
          "form": "저는 빵파인데",
          "original_form": "저는 빵파인데",
          "speaker_id": "1",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007776.1.7",
          "form": "밀가루 끊으려는데 힘드네요ㅜ",
          "original_form": "밀가루 끊으려는데 힘드네요ㅜ",
          "speaker_id": "1",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007776.1.8",
          "form": "탄수화물 줄여야하는데 맛있는데 어떡해요ㅎㅎ",
          "original_form": "탄수화물 줄여야하는데 맛있는데 어떡해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007776.1.9",
          "form": "합법마약 3종이 설탕 밀가루 쌀이래요 ㅋㅋ",
          "original_form": "합법마약 3종이 설탕 밀가루 쌀이래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007776.1.10",
          "form": "ㅋㅋ풀만 먹을수가 없는데ㅠㅜ점심 뭐드세요?",
          "original_form": "ㅋㅋ풀만 먹을수가 없는데ㅠㅜ점심 뭐드세요?",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007776.1.11",
          "form": "저는 삼각김밥 오랜만에사봤어요",
          "original_form": "저는 삼각김밥 오랜만에사봤어요",
          "speaker_id": "1",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007776.1.12",
          "form": "오 컵라면이랑 먹어야하는데ㅎㅎ",
          "original_form": "오 컵라면이랑 먹어야하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007776.1.13",
          "form": "편의점 상품권이있어서 썼어요",
          "original_form": "편의점 상품권이있어서 썼어요",
          "speaker_id": "1",
          "time": "20210822 10:46"
        },
        {
          "id": "MDRW2100007776.1.14",
          "form": "2개붙은거 1900원하네요 ㅋㅋ",
          "original_form": "2개붙은거 1900원하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007776.1.15",
          "form": "물가가 장난이 아니에요ㅎㅎ",
          "original_form": "물가가 장난이 아니에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007776.1.16",
          "form": "깨알같이 핸드폰할인도 받았네요",
          "original_form": "깨알같이 핸드폰할인도 받았네요",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
