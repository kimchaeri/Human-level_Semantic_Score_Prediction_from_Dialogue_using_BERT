        {
          "id": "MDRW2100037715.1.1",
          "form": "name1 님은 자주 보는 유튜브 있으세요?",
          "original_form": "&name1& 님은 자주 보는 유튜브 있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037715.1.2",
          "form": "예능 클립이요",
          "original_form": "예능 클립이요",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037715.1.3",
          "form": "고양이 영상도 보구요",
          "original_form": "고양이 영상도 보구요",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037715.1.4",
          "form": "좋아하는 예능 있으세요?",
          "original_form": "좋아하는 예능 있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100037715.1.5",
          "form": "런닝맨이요ㅋㅋ",
          "original_form": "런닝맨이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037715.1.6",
          "form": "아 런닝맨 레전드 클립 많죠ㅋㅋ",
          "original_form": "아 런닝맨 레전드 클립 많죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037715.1.7",
          "form": "옛날 예능도 자주 봐요",
          "original_form": "옛날 예능도 자주 봐요",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100037715.1.8",
          "form": "무한도전 지금 봐도 재밌더라구요",
          "original_form": "무한도전 지금 봐도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100037715.1.9",
          "form": "티비 예능도 자주 보세요?",
          "original_form": "티비 예능도 자주 보세요?",
          "speaker_id": "2",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100037715.1.10",
          "form": "네 tvn 예능 자주 보는거 같네요",
          "original_form": "네 tvn 예능 자주 보는거 같네요",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100037715.1.11",
          "form": "보시는거 있나요?",
          "original_form": "보시는거 있나요?",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100037715.1.12",
          "form": "아뇨 전 티비 잘 안 봐요",
          "original_form": "아뇨 전 티비 잘 안 봐요",
          "speaker_id": "2",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100037715.1.13",
          "form": "tvn이면 유퀴즈 같은거요?",
          "original_form": "tvn이면 유퀴즈 같은거요?",
          "speaker_id": "2",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100037715.1.14",
          "form": "네 유퀴즈도 봐요",
          "original_form": "네 유퀴즈도 봐요",
          "speaker_id": "1",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100037715.1.15",
          "form": "전 최근에 유퀴즈에 여자양궁선수들 나온 거 봤어요 선수들 너무 멋있더라고요",
          "original_form": "전 최근에 유퀴즈에 여자양궁선수들 나온 거 봤어요 선수들 너무 멋있더라고요",
          "speaker_id": "2",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100037715.1.16",
          "form": "네 요새 올림픽 선수들 나와요",
          "original_form": "네 요새 올림픽 선수들 나와요",
          "speaker_id": "1",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100037715.1.17",
          "form": "어젠 배구팀 선수들 나왔어요",
          "original_form": "어젠 배구팀 선수들 나왔어요",
          "speaker_id": "1",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100037715.1.18",
          "form": "맞아요 영상 짧은 것만 봤었어요",
          "original_form": "맞아요 영상 짧은 것만 봤었어요",
          "speaker_id": "2",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100037715.1.19",
          "form": "저번에 황정민님 인터뷰도 재밌더라구요",
          "original_form": "저번에 황정민님 인터뷰도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100037715.1.20",
          "form": "유퀴즈에 황정민 배우도 나왔었어요?",
          "original_form": "유퀴즈에 황정민 배우도 나왔었어요?",
