      "utterance": [
        {
          "id": "MDRW2100044734.1.1",
          "form": "저도 죽순은 처음이에요ㅎㅎ",
          "original_form": "저도 죽순은 처음이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044734.1.2",
          "form": "식감이 있나요?",
          "original_form": "식감이 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044734.1.3",
          "form": "물컹물컹? ㅋㅋㅋㅋ",
          "original_form": "물컹물컹? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044734.1.4",
          "form": "물컹한섬유질? ㅋㅋ",
          "original_form": "물컹한섬유질? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044734.1.5",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044734.1.6",
          "form": "궁금해지네요",
          "original_form": "궁금해지네요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044734.1.7",
          "form": "나중에 먹을때 넣어봐야겠어요",
          "original_form": "나중에 먹을때 넣어봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044734.1.8",
          "form": "마라탕 배달로 드시나요?",
          "original_form": "마라탕 배달로 드시나요?",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044734.1.9",
          "form": "아니요 ㅋㅋ전 배달은 안시켜요.... 포장해먹습니다 ㅋㅋ",
          "original_form": "아니요 ㅋㅋ전 배달은 안시켜요.... 포장해먹습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044734.1.10",
          "form": "오 집근처에 있나요?",
          "original_form": "오 집근처에 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044734.1.11",
          "form": "네 ㅋㅋㅋ 2분?3분? 거리에있습니다",
          "original_form": "네 ㅋㅋㅋ 2분?3분? 거리에있습니다",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044734.1.12",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044734.1.13",
          "form": "좋은 위치에 거주하시네요",
          "original_form": "좋은 위치에 거주하시네요",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044734.1.14",
          "form": "대신 서브웨이가 너무 멉니다. ...",
          "original_form": "대신 서브웨이가 너무 멉니다. ...",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044734.1.15",
          "form": "저희 지역엔 서브웨이도 없어요ㅠㅠ",
          "original_form": "저희 지역엔 서브웨이도 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044734.1.16",
          "form": "안타까운 위로의 말씀을 전합니다... 서브웨이도 참 맛있는데말이죠..",
          "original_form": "안타까운 위로의 말씀을 전합니다... 서브웨이도 참 맛있는데말이죠..",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044734.1.17",
          "form": "서울갈때만 먹고있어요...",
          "original_form": "서울갈때만 먹고있어요...",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044734.1.18",
          "form": "뭐 귀한거라고 서브웨이를...ㅠㅠ",
          "original_form": "뭐 귀한거라고 서브웨이를...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044734.1.19",
          "form": "왠지 주위에 맛집 많이 있을거 같아요!!",
          "original_form": "왠지 주위에 맛집 많이 있을거 같아요!!",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044734.1.20",
          "form": "별로없습니다 죄다 체인점이죠",
          "original_form": "별로없습니다 죄다 체인점이죠",
