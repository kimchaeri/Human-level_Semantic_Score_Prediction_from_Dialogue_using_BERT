      "utterance": [
        {
          "id": "MDRW2100008775.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008775.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008775.1.3",
          "form": "애완동물 키우시나요?",
          "original_form": "애완동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008775.1.4",
          "form": "아뇨 ㅋㅋ저도 안키워요",
          "original_form": "아뇨 ㅋㅋ저도 안키워요",
          "speaker_id": "1",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008775.1.5",
          "form": "어릴때도 안키워보셧나요?",
          "original_form": "어릴때도 안키워보셧나요?",
          "speaker_id": "1",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008775.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100008775.1.7",
          "form": "한번도요ㅠㅠ",
          "original_form": "한번도요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100008775.1.8",
          "form": "키우고싶은동물 있나요?",
          "original_form": "키우고싶은동물 있나요?",
          "speaker_id": "2",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100008775.1.9",
          "form": "저도 키우고싶은 생각은없는데",
          "original_form": "저도 키우고싶은 생각은없는데",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100008775.1.10",
          "form": "귀엽게 생긴 강아지들 보는건 좋아해요 ㅋㅋ",
          "original_form": "귀엽게 생긴 강아지들 보는건 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100008775.1.11",
          "form": "오 강아지좋죠",
          "original_form": "오 강아지좋죠",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100008775.1.12",
          "form": "저는 강아지너무키우고싶어요ㅠㅠ",
          "original_form": "저는 강아지너무키우고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100008775.1.13",
          "form": "아진짜요? 생각이 있으신가봐용 ?!",
          "original_form": "아진짜요? 생각이 있으신가봐용 ?!",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100008775.1.14",
          "form": "네네 원래 워낙",
          "original_form": "네네 원래 워낙",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100008775.1.15",
          "form": "좋아해서요ㅎㅎ",
          "original_form": "좋아해서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100008775.1.16",
          "form": "고양이는용 ?",
          "original_form": "고양이는용 ?",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100008775.1.17",
          "form": "고양이도",
          "original_form": "고양이도",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100008775.1.18",
          "form": "너무귀엽죠~",
          "original_form": "너무귀엽죠~",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100008775.1.19",
          "form": "동물 다 좋아하실거같아용 !!",
          "original_form": "동물 다 좋아하실거같아용 !!",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100008775.1.20",
          "form": "뱀 이런건",
          "original_form": "뱀 이런건",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100008775.1.21",
          "form": "싫어한답니다ㅠ",
          "original_form": "싫어한답니다ㅠ",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100008775.1.22",
          "form": "ㅋㅋㅋㅋㅋ귀여운거 좋아하시는구나 !",
          "original_form": "ㅋㅋㅋㅋㅋ귀여운거 좋아하시는구나 !",
