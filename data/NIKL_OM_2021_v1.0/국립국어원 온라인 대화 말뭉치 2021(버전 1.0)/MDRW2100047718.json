      "utterance": [
        {
          "id": "MDRW2100047718.1.1",
          "form": "모두의 마블 아시나요?",
          "original_form": "모두의 마블 아시나요?",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100047718.1.2",
          "form": "네 알죠",
          "original_form": "네 알죠",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100047718.1.3",
          "form": "그거 재밌어요",
          "original_form": "그거 재밌어요",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100047718.1.4",
          "form": "예전에 많이 한 것 같아요ㅎㅎ",
          "original_form": "예전에 많이 한 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:06"
        },
        {
          "id": "MDRW2100047718.1.5",
          "form": "ㅋㅋㅋ 저도요..",
          "original_form": "ㅋㅋㅋ 저도요..",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047718.1.6",
          "form": "그거 주사위 돌리는 거",
          "original_form": "그거 주사위 돌리는 거",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047718.1.7",
          "form": "너무 중독적이에요..",
          "original_form": "너무 중독적이에요..",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047718.1.8",
          "form": "6,6 나올 때가 가장 기분이 좋아요ㅋㅋㅋ",
          "original_form": "6,6 나올 때가 가장 기분이 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100047718.1.9",
          "form": "아 정말요",
          "original_form": "아 정말요",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100047718.1.10",
          "form": "짜릿하죸ㅋㅋㅋㅋㅋㅋ",
          "original_form": "짜릿하죸ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100047718.1.11",
          "form": "신의손? 맵에서 그거 들어올려서 제 랜마에 걸리게 하는 것도 좋았어요ㅋㅋ",
          "original_form": "신의손? 맵에서 그거 들어올려서 제 랜마에 걸리게 하는 것도 좋았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100047718.1.12",
          "form": "으와 그건 안해봤어요ㅋㅋ",
          "original_form": "으와 그건 안해봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100047718.1.13",
          "form": "그것도 오래전이라 이제는 또 어떤 맵이 나왔는지 모르겠네요",
          "original_form": "그것도 오래전이라 이제는 또 어떤 맵이 나왔는지 모르겠네요",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100047718.1.14",
          "form": "지금은 좀 망하지 않았을까요..",
          "original_form": "지금은 좀 망하지 않았을까요..",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100047718.1.15",
          "form": "아마도요...ㅎ 이제 다른? 게임도 냈구요",
          "original_form": "아마도요...ㅎ 이제 다른? 게임도 냈구요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100047718.1.16",
          "form": "헉 무슨 게임이요??!",
          "original_form": "헉 무슨 게임이요??!",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100047718.1.17",
          "form": "제2의 나라? 그거 넷마블 아닌가여??",
          "original_form": "제2의 나라? 그거 넷마블 아닌가여??",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100047718.1.18",
          "form": "제2의 나라 넷마블로 알고있어요!",
          "original_form": "제2의 나라 넷마블로 알고있어요!",
          "speaker_id": "1",
