      "utterance": [
        {
          "id": "MDRW2100019139.1.1",
          "form": "과일 상태가 좋은가보네",
          "original_form": "과일 상태가 좋은가보네",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019139.1.2",
          "form": "스위티오 바나나라 맛있어",
          "original_form": "스위티오 바나나라 맛있어",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019139.1.3",
          "form": "요즘 바나나는 얼마나 해?",
          "original_form": "요즘 바나나는 얼마나 해?",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019139.1.4",
          "form": "가격이 많이 올랐어 요즘엔 5천원 정도야",
          "original_form": "가격이 많이 올랐어 요즘엔 5천원 정도야",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019139.1.5",
          "form": "한참 쌀 때는 3천원대였는데 ㅎㅎ",
          "original_form": "한참 쌀 때는 3천원대였는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019139.1.6",
          "form": "ㅇㅇ 양도 좀 줄고 가격도 많이 올랐어",
          "original_form": "ㅇㅇ 양도 좀 줄고 가격도 많이 올랐어",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019139.1.7",
          "form": "그래도 바나나만한 간식이 없지 ㅎㅎ",
          "original_form": "그래도 바나나만한 간식이 없지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019139.1.8",
          "form": "고구마랑 바나나 간식으로 완전 최고지",
          "original_form": "고구마랑 바나나 간식으로 완전 최고지",
          "speaker_id": "1",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019139.1.9",
          "form": "애들이랑 많이 먹어 ㅎㅎ",
          "original_form": "애들이랑 많이 먹어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019139.1.10",
          "form": "신랑도 먹어야지 ㅋ",
          "original_form": "신랑도 먹어야지 ㅋ",
          "speaker_id": "1",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019139.1.11",
          "form": "난 다이어트 중이잖아",
          "original_form": "난 다이어트 중이잖아",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019139.1.12",
          "form": "과일은 괜찮지 않나?",
          "original_form": "과일은 괜찮지 않나?",
          "speaker_id": "1",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019139.1.13",
          "form": "과일도 많이 먹으면 살쪄 ㅋㅋ",
          "original_form": "과일도 많이 먹으면 살쪄 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019139.1.14",
          "form": "앞으로 바나나는 트레이더스에서 사야겠어 ㅎㅎ",
          "original_form": "앞으로 바나나는 트레이더스에서 사야겠어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:37"
