      "utterance": [
        {
          "id": "MDRW2100036735.1.1",
          "form": "클라이밍하고 어떤지",
          "original_form": "클라이밍하고 어떤지",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100036735.1.2",
          "form": "알려주세요ㅋㅋ!",
          "original_form": "알려주세요ㅋㅋ!",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100036735.1.3",
          "form": "또 앓아누울 것 같긴 한데 후기 남기겠습니다 ㅋㅋㅋ",
          "original_form": "또 앓아누울 것 같긴 한데 후기 남기겠습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036735.1.4",
          "form": "ㅋㅋㅋㅋ네.. 아마 팔이",
          "original_form": "ㅋㅋㅋㅋ네.. 아마 팔이",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036735.1.5",
          "form": "대박 아프지 않을까여ㅋㅋㅌ",
          "original_form": "대박 아프지 않을까여ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036735.1.6",
          "form": "팔이 젤 아플거같긴해요 ㅋㅋㅋ 클라이밍선수들 팔근육 대박이더라구요",
          "original_form": "팔이 젤 아플거같긴해요 ㅋㅋㅋ 클라이밍선수들 팔근육 대박이더라구요",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100036735.1.7",
          "form": "멋있어요 \n... 근육 짱짱",
          "original_form": "멋있어요 \n... 근육 짱짱",
          "speaker_id": "2",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100036735.1.8",
          "form": "그런거 보면 저도 뽐뿌가 와요 ㅋㅋㅋ",
          "original_form": "그런거 보면 저도 뽐뿌가 와요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036735.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ공감이용...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ공감이용...",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036735.1.10",
          "form": "저도 그렇게 근육을 만들고싶어요",
          "original_form": "저도 그렇게 근육을 만들고싶어요",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036735.1.11",
          "form": "저도요 ㅋㅋㅋㅋ 근데 꾸준히 운동하기 쉽지않고",
          "original_form": "저도요 ㅋㅋㅋㅋ 근데 꾸준히 운동하기 쉽지않고",
          "speaker_id": "1",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100036735.1.12",
          "form": "진심이요 ㅠ",
          "original_form": "진심이요 ㅠ",
