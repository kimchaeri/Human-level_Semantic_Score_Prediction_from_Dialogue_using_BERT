      "utterance": [
        {
          "id": "MDRW2100043144.1.1",
          "form": "니 동생 케이크 고르라고해",
          "original_form": "니 동생 케이크 고르라고해",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043144.1.2",
          "form": "벌써? 이렇게 빨리 고르낰ㅋ",
          "original_form": "벌써? 이렇게 빨리 고르낰ㅋ",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043144.1.3",
          "form": "야 니케이크 너무 달기만하고",
          "original_form": "야 니케이크 너무 달기만하고",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043144.1.4",
          "form": "맛없어",
          "original_form": "맛없어",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043144.1.5",
          "form": "헐 맛있거든?",
          "original_form": "헐 맛있거든?",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043144.1.6",
          "form": "초코초코한데 많이 안달다고",
          "original_form": "초코초코한데 많이 안달다고",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043144.1.7",
          "form": "달더만",
          "original_form": "달더만",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043144.1.8",
          "form": "니 동생껀 맛있는걸로 고르자",
          "original_form": "니 동생껀 맛있는걸로 고르자",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043144.1.9",
          "form": "ㅡㅡ커피빈 가고 싶다니까 못가게하고",
          "original_form": "ㅡㅡ커피빈 가고 싶다니까 못가게하고",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043144.1.10",
          "form": "저런소리한다",
          "original_form": "저런소리한다",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043144.1.11",
          "form": "카페 좀 찾아봐",
          "original_form": "카페 좀 찾아봐",
          "speaker_id": "2",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043144.1.12",
          "form": "나 케이크 사려고 찾아본데 있는데",
          "original_form": "나 케이크 사려고 찾아본데 있는데",
          "speaker_id": "1",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043144.1.13",
          "form": "카톡보냄",
          "original_form": "카톡보냄",
          "speaker_id": "1",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043144.1.14",
          "form": "오 뭐가 많네",
          "original_form": "오 뭐가 많네",
          "speaker_id": "2",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043144.1.15",
          "form": "그지? 케이크 종류 많더라",
          "original_form": "그지? 케이크 종류 많더라",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043144.1.16",
          "form": "니동생보고 골라보라고해",
          "original_form": "니동생보고 골라보라고해",
          "speaker_id": "2",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043144.1.17",
          "form": "ㅇㅇ근데 걔는 우리보고 골르라고할걸",
          "original_form": "ㅇㅇ근데 걔는 우리보고 골르라고할걸",
          "speaker_id": "1",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043144.1.18",
          "form": "뭘하든 먹기만 젤 많이 먹을듯ㅋㅋ",
          "original_form": "뭘하든 먹기만 젤 많이 먹을듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043144.1.19",
          "form": "요즘 통통해졌어",
          "original_form": "요즘 통통해졌어",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043144.1.20",
          "form": "잘 모르겠는데",
          "original_form": "잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043144.1.21",
          "form": "아니야 포동포동 살 많이 올랐더라",
          "original_form": "아니야 포동포동 살 많이 올랐더라",
