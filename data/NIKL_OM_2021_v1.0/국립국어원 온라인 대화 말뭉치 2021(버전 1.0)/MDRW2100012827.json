      "utterance": [
        {
          "id": "MDRW2100012827.1.1",
          "form": "군대어떻게생각하세요?",
          "original_form": "군대어떻게생각하세요?",
          "speaker_id": "1",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012827.1.2",
          "form": "부조리의 집합체요",
          "original_form": "부조리의 집합체요",
          "speaker_id": "2",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012827.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012827.1.4",
          "form": "사실 안가봐서",
          "original_form": "사실 안가봐서",
          "speaker_id": "1",
          "time": "20210825 02:09"
        },
        {
          "id": "MDRW2100012827.1.5",
          "form": "잘은 모르겠네요",
          "original_form": "잘은 모르겠네요",
          "speaker_id": "1",
          "time": "20210825 02:09"
        },
        {
          "id": "MDRW2100012827.1.6",
          "form": "저도 안가봤어요",
          "original_form": "저도 안가봤어요",
          "speaker_id": "2",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012827.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012827.1.8",
          "form": "요즘 폰사용이",
          "original_form": "요즘 폰사용이",
          "speaker_id": "1",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012827.1.9",
          "form": "자유롭다하더라고요",
          "original_form": "자유롭다하더라고요",
          "speaker_id": "1",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012827.1.10",
          "form": "ㅋㅋ 좋아졌네요",
          "original_form": "ㅋㅋ 좋아졌네요",
          "speaker_id": "2",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012827.1.11",
          "form": "월급도",
          "original_form": "월급도",
          "speaker_id": "1",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012827.1.12",
          "form": "올랐대요",
          "original_form": "올랐대요",
          "speaker_id": "1",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012827.1.13",
          "form": "맞아요 한달에 40인가?",
          "original_form": "맞아요 한달에 40인가?",
          "speaker_id": "2",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012827.1.14",
          "form": "올랐던데요",
          "original_form": "올랐던데요",
          "speaker_id": "2",
          "time": "20210825 02:13"
        },
        {
          "id": "MDRW2100012827.1.15",
          "form": "코로나 격리 시설",
          "original_form": "코로나 격리 시설",
          "speaker_id": "2",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012827.1.16",
          "form": "급식보셨나요",
          "original_form": "급식보셨나요",
          "speaker_id": "2",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012827.1.17",
          "form": "60이라지않았나요?",
          "original_form": "60이라지않았나요?",
          "speaker_id": "1",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012827.1.18",
          "form": "급식이왜요?!",
          "original_form": "급식이왜요?!",
          "speaker_id": "1",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012827.1.19",
          "form": "완전 부실하던데요",
          "original_form": "완전 부실하던데요",
          "speaker_id": "2",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012827.1.20",
          "form": "남자들이 여자도 군대가야한다는 주장에 대해서",
          "original_form": "남자들이 여자도 군대가야한다는 주장에 대해서",
          "speaker_id": "2",
          "time": "20210825 02:32"
        },
        {
          "id": "MDRW2100012827.1.21",
          "form": "어떻게 생각하세요?",
          "original_form": "어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20210825 02:32"
        },
        {
          "id": "MDRW2100012827.1.22",
          "form": "ㅋㅋㅋ 주장이 받아들여진다면",
          "original_form": "ㅋㅋㅋ 주장이 받아들여진다면",
          "speaker_id": "1",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012827.1.23",
          "form": "가야겠죠",
          "original_form": "가야겠죠",
          "speaker_id": "1",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012827.1.24",
          "form": "받아들여진다면! 말이예요ㅎ",
          "original_form": "받아들여진다면! 말이예요ㅎ",
          "speaker_id": "1",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012827.1.25",
          "form": "그럴일이없겠지만",
          "original_form": "그럴일이없겠지만",
          "speaker_id": "1",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012827.1.26",
          "form": "군대하면 무슨 생각이 먼저 떠오르세요?",
          "original_form": "군대하면 무슨 생각이 먼저 떠오르세요?",
          "speaker_id": "2",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100012827.1.27",
          "form": "음..딱히 별생각은없네요",
          "original_form": "음..딱히 별생각은없네요",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012827.1.28",
          "form": "괴롭힘이런건",
          "original_form": "괴롭힘이런건",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012827.1.29",
          "form": "오바같긴해요",
          "original_form": "오바같긴해요",
          "speaker_id": "1",
          "time": "20210825 02:43"
        }
      ]
    }
  ]
