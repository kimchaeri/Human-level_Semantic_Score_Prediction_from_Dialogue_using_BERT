        },
        {
          "id": "MDRW2100025047.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025047.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025047.1.4",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025047.1.5",
          "form": "아뇨  저녁은 거의 안먹어요",
          "original_form": "아뇨  저녁은 거의 안먹어요",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025047.1.6",
          "form": "늦은 점심을 먹기 때문에",
          "original_form": "늦은 점심을 먹기 때문에",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025047.1.7",
          "form": "그렇군요\r\n점심을 보통 몇시에 드시나요?",
          "original_form": "그렇군요\r\n점심을 보통 몇시에 드시나요?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025047.1.8",
          "form": "3~4시에 먹어요",
          "original_form": "3~4시에 먹어요",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025047.1.9",
          "form": "일부로 그 시간대 드시는건가요?",
          "original_form": "일부로 그 시간대 드시는건가요?",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025047.1.10",
          "form": "그건 아닌데... 어쩌다보니",
          "original_form": "그건 아닌데... 어쩌다보니",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025047.1.11",
          "form": "아침을 10시경에 먹어요",
          "original_form": "아침을 10시경에 먹어요",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025047.1.12",
          "form": "어쩔땐 하루 한끼만 먹을때도",
          "original_form": "어쩔땐 하루 한끼만 먹을때도",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025047.1.13",
          "form": "그렇군요 그럼 한 끼에 많이 드시는 편인가요?",
          "original_form": "그렇군요 그럼 한 끼에 많이 드시는 편인가요?",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025047.1.14",
          "form": "아뇨  보통사람들 양이에여",
          "original_form": "아뇨  보통사람들 양이에여",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025047.1.15",
          "form": "젊을때(?)는 라면에 밥을 말아 먹었는데  요즘은 라면만 먹어요",
          "original_form": "젊을때(?)는 라면에 밥을 말아 먹었는데  요즘은 라면만 먹어요",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025047.1.16",
          "form": "양이 줄었어요",
          "original_form": "양이 줄었어요",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025047.1.17",
          "form": "안먹어도 배고픈줄 모르겠더라구요",
          "original_form": "안먹어도 배고픈줄 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025047.1.18",
          "form": "활동량이 많지 않아서인듯",
          "original_form": "활동량이 많지 않아서인듯",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025047.1.19",
          "form": "애 키울땐  먹어도 허기졌거든요  정신없이  대충   밥을 먹으니까",
          "original_form": "애 키울땐  먹어도 허기졌거든요  정신없이  대충   밥을 먹으니까",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025047.1.20",
          "form": "그렇군요 근데 그렇게 드시면 힘이 딸리거나 어지럽거나 하지는 않으시나요??",
          "original_form": "그렇군요 근데 그렇게 드시면 힘이 딸리거나 어지럽거나 하지는 않으시나요??",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025047.1.21",
          "form": "그렇지는 않아요  나도 내가 이렇게 변할줄 몰랐네요 ㅋ",
          "original_form": "그렇지는 않아요  나도 내가 이렇게 변할줄 몰랐네요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025047.1.22",
          "form": "그러시군요 신기하네요",
          "original_form": "그러시군요 신기하네요",
          "speaker_id": "1",
          "time": "20210831 22:57"
        }
      ]
    }
