      "utterance": [
        {
          "id": "MDRW2100041249.1.1",
          "form": "여긴 흐리더라구요",
          "original_form": "여긴 흐리더라구요",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041249.1.2",
          "form": "어제는 쨍쩅해서 좋았는데",
          "original_form": "어제는 쨍쩅해서 좋았는데",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041249.1.3",
          "form": "거긴 비안왔나요?",
          "original_form": "거긴 비안왔나요?",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100041249.1.4",
          "form": "네.  여긴 해쨍쨍~  했어요",
          "original_form": "네.  여긴 해쨍쨍~  했어요",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100041249.1.5",
          "form": "파란하늘에 흰구름",
          "original_form": "파란하늘에 흰구름",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100041249.1.6",
          "form": "어제가 여기 하늘이 그랬어요",
          "original_form": "어제가 여기 하늘이 그랬어요",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100041249.1.7",
          "form": "그래서 오늘 애들 옷을 얇게 입혀서 나갔는데",
          "original_form": "그래서 오늘 애들 옷을 얇게 입혀서 나갔는데",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100041249.1.8",
          "form": "갑자기 비도 약간오고...애가 춥다고..ㅠㅜ",
          "original_form": "갑자기 비도 약간오고...애가 춥다고..ㅠㅜ",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100041249.1.9",
          "form": "약간 춥게 느꼈겠네요",
          "original_form": "약간 춥게 느꼈겠네요",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041249.1.10",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041249.1.11",
          "form": "물감놀이 하는 카페 델고 갔는데...\r\n밖에서 물고기 잡기도 하는것도 있었거든요",
          "original_form": "물감놀이 하는 카페 델고 갔는데...\r\n밖에서 물고기 잡기도 하는것도 있었거든요",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041249.1.12",
          "form": "아침저녁으론 너무 쌀쌀해요",
          "original_form": "아침저녁으론 너무 쌀쌀해요",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041249.1.13",
          "form": "애들 신났겠네요",
          "original_form": "애들 신났겠네요",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041249.1.14",
          "form": "애가 추워해서 일찍 들어왔어요 ㅎ",
          "original_form": "애가 추워해서 일찍 들어왔어요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041249.1.15",
          "form": "제대로 못놀아서 아숩 ㅎ",
          "original_form": "제대로 못놀아서 아숩 ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041249.1.16",
          "form": "ㅎ 전 여기 5일장 서서 잠깐구경갔다왔어요",
          "original_form": "ㅎ 전 여기 5일장 서서 잠깐구경갔다왔어요",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041249.1.17",
          "form": "글게요. 금방오셨음 아쉽겠네요",
          "original_form": "글게요. 금방오셨음 아쉽겠네요",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041249.1.18",
          "form": "진짜 아침저녁으론 이제 쌀쌀해져서",
          "original_form": "진짜 아침저녁으론 이제 쌀쌀해져서",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041249.1.19",
          "form": "옷 맞춰입기도 애매해요 ㅎ",
          "original_form": "옷 맞춰입기도 애매해요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041249.1.20",
          "form": "그니깐요.",
          "original_form": "그니깐요.",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041249.1.21",
          "form": "옷정리는 하셨어요?",
          "original_form": "옷정리는 하셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041249.1.22",
          "form": "해야하는데...  대강 뒤져입어야죠~~",
          "original_form": "해야하는데...  대강 뒤져입어야죠~~",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041249.1.23",
          "form": "저도 해야하는데 ㅎㅎ",
          "original_form": "저도 해야하는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041249.1.24",
          "form": "자꾸 미루고 있어요 ㅎ",
          "original_form": "자꾸 미루고 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041249.1.25",
          "form": "리빙박스에서 꺼내입다가 겨울올듯요 ㅎ",
          "original_form": "리빙박스에서 꺼내입다가 겨울올듯요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041249.1.26",
          "form": "ㅎㅎ 가을짧으니 얼렁 추워지길 기다려야겠어요",
          "original_form": "ㅎㅎ 가을짧으니 얼렁 추워지길 기다려야겠어요",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041249.1.27",
          "form": "귀찮네요^^",
          "original_form": "귀찮네요^^",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041249.1.28",
          "form": "ㅎㅎㅎ 저는 애들 좀 재우러 가야할듯요 ㅎ",
          "original_form": "ㅎㅎㅎ 저는 애들 좀 재우러 가야할듯요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041249.1.29",
          "form": "재우고 올께요 같이 잠들지도 모르겠지만...ㅋ",
          "original_form": "재우고 올께요 같이 잠들지도 모르겠지만...ㅋ",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041249.1.30",
