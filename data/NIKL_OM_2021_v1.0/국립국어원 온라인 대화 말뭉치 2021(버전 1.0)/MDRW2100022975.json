      "utterance": [
        {
          "id": "MDRW2100022975.1.1",
          "form": "출근하기싫엉",
          "original_form": "출근하기싫엉",
          "speaker_id": "1",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022975.1.2",
          "form": "나도... 월요일엔 유독 일이 많은 느낌이안",
          "original_form": "나도... 월요일엔 유독 일이 많은 느낌이안",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022975.1.3",
          "form": "넌 무슨 일 하는데",
          "original_form": "넌 무슨 일 하는데",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022975.1.4",
          "form": "마자 넌 또 투잡이잖아 ㅋㅋㅋㅋㅋ 학생+알바",
          "original_form": "마자 넌 또 투잡이잖아 ㅋㅋㅋㅋㅋ 학생+알바",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022975.1.5",
          "form": "난 개발자징",
          "original_form": "난 개발자징",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022975.1.6",
          "form": "일 자체는 재밌는 편인데",
          "original_form": "일 자체는 재밌는 편인데",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022975.1.7",
          "form": "쳇..ㅋㅋㅋㅋㅋ 난 니가 앉아서 키보드 두드리는 사람인줄",
          "original_form": "쳇..ㅋㅋㅋㅋㅋ 난 니가 앉아서 키보드 두드리는 사람인줄",
          "speaker_id": "2",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022975.1.8",
          "form": "키보드 두드리긴 함 ㅋㅋㅋㅋㅋ",
          "original_form": "키보드 두드리긴 함 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022975.1.9",
          "form": "부울함수 알려줭",
          "original_form": "부울함수 알려줭",
          "speaker_id": "2",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022975.1.10",
          "form": "나 그런거 몰랑.......",
          "original_form": "나 그런거 몰랑.......",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022975.1.11",
          "form": "넌 일 할만해?",
          "original_form": "넌 일 할만해?",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022975.1.12",
          "form": "지금다니는데 일한지 얼마나됐지?",
          "original_form": "지금다니는데 일한지 얼마나됐지?",
          "speaker_id": "1",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022975.1.13",
          "form": "음... 이쪽 콜센터는 기업들 대상이라서 그런지 진상은 거의 없구",
          "original_form": "음... 이쪽 콜센터는 기업들 대상이라서 그런지 진상은 거의 없구",
          "speaker_id": "2",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022975.1.14",
          "form": "난 곧 한달..",
          "original_form": "난 곧 한달..",
          "speaker_id": "2",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022975.1.15",
          "form": "오 한달이면 배우고 적응하느라 죽어날땐데",
          "original_form": "오 한달이면 배우고 적응하느라 죽어날땐데",
          "speaker_id": "1",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022975.1.16",
          "form": "가끔 화가 난 손님이 전화할때도 있는데 이유가 이해가 가서 별로 화도 안 나더라",
          "original_form": "가끔 화가 난 손님이 전화할때도 있는데 이유가 이해가 가서 별로 화도 안 나더라",
          "speaker_id": "2",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022975.1.17",
          "form": "맞아.. 맨날 나 쭈그리야..",
          "original_form": "맞아.. 맨날 나 쭈그리야..",
          "speaker_id": "2",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022975.1.18",
          "form": "쭈굴쭈굴..전화업무 무서워 ㅠ",
          "original_form": "쭈굴쭈굴..전화업무 무서워 ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022975.1.19",
          "form": "ㅠㅠ 맨날 몰라서 힘드려",
          "original_form": "ㅠㅠ 맨날 몰라서 힘드려",
          "speaker_id": "2",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022975.1.20",
          "form": "모르는건 하다보면 늘겠징 ㅋㅋ",
          "original_form": "모르는건 하다보면 늘겠징 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022975.1.21",
          "form": "사수님을 귀찮게 해",
          "original_form": "사수님을 귀찮게 해",
          "speaker_id": "1",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100022975.1.22",
          "form": "이거 뭐에요 저거 뭐에요 나도 맨날물어봄 ㅋㅋㅋ",
          "original_form": "이거 뭐에요 저거 뭐에요 나도 맨날물어봄 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100022975.1.23",
          "form": "한달이면 아직 애기잖어ㅋㅋㅋㅋㅋㅋ",
          "original_form": "한달이면 아직 애기잖어ㅋㅋㅋㅋㅋㅋ",
