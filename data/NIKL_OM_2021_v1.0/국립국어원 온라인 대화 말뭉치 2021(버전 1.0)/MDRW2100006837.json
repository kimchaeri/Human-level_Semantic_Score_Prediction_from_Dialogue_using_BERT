      "utterance": [
        {
          "id": "MDRW2100006837.1.1",
          "form": "안녕하세요 오늘 날씨 어떠신가여",
          "original_form": "안녕하세요 오늘 날씨 어떠신가여",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006837.1.2",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006837.1.3",
          "form": "거기는",
          "original_form": "거기는",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006837.1.4",
          "form": "여기도조아요",
          "original_form": "여기도조아요",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100006837.1.5",
          "form": "비왔음?",
          "original_form": "비왔음?",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100006837.1.6",
          "form": "요즘 긴팔 입어여?",
          "original_form": "요즘 긴팔 입어여?",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100006837.1.7",
          "form": "네 왔다가 지금 맑아여",
          "original_form": "네 왔다가 지금 맑아여",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100006837.1.8",
          "form": "아직안입어요",
          "original_form": "아직안입어요",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006837.1.9",
          "form": "거기는?",
          "original_form": "거기는?",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006837.1.10",
          "form": "오 저는 입어여",
          "original_form": "오 저는 입어여",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006837.1.11",
          "form": "바람은 부나요?",
          "original_form": "바람은 부나요?",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006837.1.12",
          "form": "초가을같아요",
          "original_form": "초가을같아요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006837.1.13",
          "form": "여름탈피",
          "original_form": "여름탈피",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006837.1.14",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006837.1.15",
          "form": "맞아요 좋아요 하늘맑네요",
          "original_form": "맞아요 좋아요 하늘맑네요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006837.1.16",
          "form": "봄여름겨울가을 머가조으세요",
          "original_form": "봄여름겨울가을 머가조으세요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006837.1.17",
          "form": "봄 가을",
          "original_form": "봄 가을",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006837.1.18",
          "form": "님은",
          "original_form": "님은",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006837.1.19",
          "form": "저는 여름빼고!",
          "original_form": "저는 여름빼고!",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006837.1.20",
          "form": "선선한날씨좋아하시나보군요",
          "original_form": "선선한날씨좋아하시나보군요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006837.1.21",
          "form": "추우면 움츠러듬요",
          "original_form": "추우면 움츠러듬요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006837.1.22",
          "form": "보일러를킵시다..! 겨울조아요 낭만있자나요",
          "original_form": "보일러를킵시다..! 겨울조아요 낭만있자나요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006837.1.23",
          "form": "눈은싫어요?",
          "original_form": "눈은싫어요?",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006837.1.24",
          "form": "눈은 조흠",
          "original_form": "눈은 조흠",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006837.1.25",
          "form": "추우면 힘듬",
          "original_form": "추우면 힘듬",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006837.1.26",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006837.1.27",
          "form": "맞아요추운거싫죠 패쪼를 입읍시다..!",
          "original_form": "맞아요추운거싫죠 패쪼를 입읍시다..!",
          "speaker_id": "1",
          "time": "20210908 10:52"
        }
      ]
