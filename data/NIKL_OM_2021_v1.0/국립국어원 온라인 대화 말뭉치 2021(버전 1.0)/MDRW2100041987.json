      },
      "utterance": [
        {
          "id": "MDRW2100041987.1.1",
          "form": "아빠 술좀 덜마시시면 좋을텐데 ㅠ",
          "original_form": "아빠 술좀 덜마시시면 좋을텐데 ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041987.1.2",
          "form": "말을 해도 안듣는다 내가 많이 말하지",
          "original_form": "말을 해도 안듣는다 내가 많이 말하지",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041987.1.3",
          "form": "제가 말해도 안들으시는거보니 알콜중독이신듯",
          "original_form": "제가 말해도 안들으시는거보니 알콜중독이신듯",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041987.1.4",
          "form": "완조니 병원가셔야함",
          "original_form": "완조니 병원가셔야함",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041987.1.5",
          "form": "아빠는 간기능도 안떨어진대니",
          "original_form": "아빠는 간기능도 안떨어진대니",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041987.1.6",
          "form": "간기능 예전부터 안좋으셨을거같은데",
          "original_form": "간기능 예전부터 안좋으셨을거같은데",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041987.1.7",
          "form": "술도 못마시시는걸 ㅠ 왜 계속 마시시고",
          "original_form": "술도 못마시시는걸 ㅠ 왜 계속 마시시고",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041987.1.8",
          "form": "엄마도 지방간이래 많이 안마셨는데",
          "original_form": "엄마도 지방간이래 많이 안마셨는데",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041987.1.9",
          "form": "술마시지마세요ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ 걱정돼",
          "original_form": "술마시지마세요ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ 걱정돼",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041987.1.10",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041987.1.11",
          "form": "그래도 술 많이 안마셨는데도 그러네",
          "original_form": "그래도 술 많이 안마셨는데도 그러네",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041987.1.12",
          "form": "몸이 원래 안좋으신가부다 걱정이에요",
          "original_form": "몸이 원래 안좋으신가부다 걱정이에요",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041987.1.13",
          "form": "그래도 조금씩 운동 해보려고",
          "original_form": "그래도 조금씩 운동 해보려고",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041987.1.14",
          "form": "좋아요 ㅠ 건강하셔야돼",
          "original_form": "좋아요 ㅠ 건강하셔야돼",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041987.1.15",
          "form": "운동하면 좀 건강해지겠지",
          "original_form": "운동하면 좀 건강해지겠지",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041987.1.16",
          "form": "제가 집에 두고간거 있잖아요",
          "original_form": "제가 집에 두고간거 있잖아요",
          "speaker_id": "2",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041987.1.17",
          "form": "너 운동 안하고 놔둔거??운동좀해",
          "original_form": "너 운동 안하고 놔둔거??운동좀해",
          "speaker_id": "1",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041987.1.18",
          "form": "참나 왜 얘기가 글루가요",
          "original_form": "참나 왜 얘기가 글루가요",
          "speaker_id": "2",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041987.1.19",
          "form": "아깝게 사놓고 쓰지도 않고",
          "original_form": "아깝게 사놓고 쓰지도 않고",
          "speaker_id": "1",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041987.1.20",
          "form": "암튼 ㅡㅡ 그 요가매크랑 폼롤러로 유튜브 검색해보세요",
          "original_form": "암튼 ㅡㅡ 그 요가매크랑 폼롤러로 유튜브 검색해보세요",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041987.1.21",
          "form": "검색하면 뭐 나와?",
          "original_form": "검색하면 뭐 나와?",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041987.1.22",
          "form": "땅끄부부 홈트레이닝 이런거 검색하면 많이 나올걸요",
          "original_form": "땅끄부부 홈트레이닝 이런거 검색하면 많이 나올걸요",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041987.1.23",
          "form": "홈트레이닝 공간 넓어야하는거 아니냐",
          "original_form": "홈트레이닝 공간 넓어야하는거 아니냐",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041987.1.24",
          "form": "아니에요 좁아도 돼요 ㅋㅋㅋ 요가매트크기로",
          "original_form": "아니에요 좁아도 돼요 ㅋㅋㅋ 요가매트크기로",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041987.1.25",
          "form": "줄넘기로 살빼고싶다",
          "original_form": "줄넘기로 살빼고싶다",
          "speaker_id": "2",
          "time": "20210906 18:55"
        }
      ]
    }
  ]
