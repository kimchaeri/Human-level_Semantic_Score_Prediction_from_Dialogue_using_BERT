      "utterance": [
        {
          "id": "MDRW2100009118.1.1",
          "form": "전 커피 음료는 안사먹는데 새로나온 아이스크림 블렌딩은 사먹어 봐야겠어요",
          "original_form": "전 커피 음료는 안사먹는데 새로나온 아이스크림 블렌딩은 사먹어 봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.2",
          "form": "오 대단하세요",
          "original_form": "오 대단하세요",
          "speaker_id": "2",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.3",
          "form": "여긴 별맛집 입니다",
          "original_form": "여긴 별맛집 입니다",
          "speaker_id": "2",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.4",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.5",
          "form": "별맛집 인정이요",
          "original_form": "별맛집 인정이요",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.6",
          "form": "저앞에분 아포카토 시켰는데 급 눈이가서 ㅋㅋ",
          "original_form": "저앞에분 아포카토 시켰는데 급 눈이가서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.7",
          "form": "먹고팠어요",
          "original_form": "먹고팠어요",
          "speaker_id": "2",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.8",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009118.1.9",
          "form": "아까 말씀하신 쿠키머핀은 머핀이름이 쿠키머핀이예요?",
          "original_form": "아까 말씀하신 쿠키머핀은 머핀이름이 쿠키머핀이예요?",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009118.1.10",
          "form": "기억이 ㅜㅜ쿠키블루베리머핀",
          "original_form": "기억이 ㅜㅜ쿠키블루베리머핀",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009118.1.11",
          "form": "이였어요",
          "original_form": "이였어요",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009118.1.12",
          "form": "블루베리 좋아해서요",
          "original_form": "블루베리 좋아해서요",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009118.1.13",
          "form": "위가 쿠키라 바삭",
          "original_form": "위가 쿠키라 바삭",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009118.1.14",
          "form": "저도 한번 먹어봐야겠어요",
          "original_form": "저도 한번 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009118.1.15",
          "form": "진짜 안에 잼가득이요",
          "original_form": "진짜 안에 잼가득이요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009118.1.16",
          "form": "팡터져요",
          "original_form": "팡터져요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009118.1.17",
          "form": "와 기대되요",
          "original_form": "와 기대되요",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009118.1.18",
          "form": "네 앙 맛나요",
          "original_form": "네 앙 맛나요",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009118.1.19",
          "form": "제가 새콤이 좋아해요",
          "original_form": "제가 새콤이 좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009118.1.20",
          "form": "블루베리도 박혀있어요",
          "original_form": "블루베리도 박혀있어요",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009118.1.21",
          "form": "맨날커피만 드라이브스루로 픽업해가거든요 ㅎ",
          "original_form": "맨날커피만 드라이브스루로 픽업해가거든요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009118.1.22",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009118.1.23",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009118.1.24",
          "form": "가야 보고 사는데",
          "original_form": "가야 보고 사는데",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009118.1.25",
          "form": "매장안에서 봐야해요",
          "original_form": "매장안에서 봐야해요",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009118.1.26",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009118.1.27",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009118.1.28",
          "form": "그러면 고르는재미도 있는데 말이죠",
          "original_form": "그러면 고르는재미도 있는데 말이죠",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009118.1.29",
          "form": "스벅은 드라이브스루도 줄이 줄이 ㅎㅎ",
          "original_form": "스벅은 드라이브스루도 줄이 줄이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009118.1.30",
          "form": "장난 아니죠",
          "original_form": "장난 아니죠",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009118.1.31",
          "form": "진짜 어쩔땐 그냥 가고싶을때도 있어요",
          "original_form": "진짜 어쩔땐 그냥 가고싶을때도 있어요",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009118.1.32",
          "form": "근데 매장마다",
          "original_form": "근데 매장마다",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009118.1.33",
          "form": "제품이 좀달라요",
          "original_form": "제품이 좀달라요",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009118.1.34",
          "form": "머핀도 오늘 다른데 가서 먹었어요",
          "original_form": "머핀도 오늘 다른데 가서 먹었어요",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009118.1.35",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009118.1.36",
          "form": "네 다르던데요",
          "original_form": "네 다르던데요",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009118.1.37",
          "form": "저희 스벅에선 못봤어요",
          "original_form": "저희 스벅에선 못봤어요",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009118.1.38",
          "form": "몇군데 가봐야겠네요 ㅎ",
          "original_form": "몇군데 가봐야겠네요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009118.1.39",
          "form": "시간따라 나가는게 달라서 제가 간시간에 없었을지도 모르겠네요",
          "original_form": "시간따라 나가는게 달라서 제가 간시간에 없었을지도 모르겠네요",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009118.1.40",
          "form": "저는 이제 애들 취침시간이라 재우러가야겠네요 ㅎㅎ",
          "original_form": "저는 이제 애들 취침시간이라 재우러가야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009118.1.41",
          "form": "네 맛난 디저트도 낼드시고",
          "original_form": "네 맛난 디저트도 낼드시고",
          "speaker_id": "2",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009118.1.42",
          "form": "굿밤되세요",
          "original_form": "굿밤되세요",
          "speaker_id": "2",
          "time": "20210822 21:33"
        }
      ]
    }
  ]
