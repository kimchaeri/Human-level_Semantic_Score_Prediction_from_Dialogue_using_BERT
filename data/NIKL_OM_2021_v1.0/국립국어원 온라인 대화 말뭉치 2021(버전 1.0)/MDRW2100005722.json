      "utterance": [
        {
          "id": "MDRW2100005722.1.1",
          "form": "중고거래 해보셨나요?",
          "original_form": "중고거래 해보셨나요?",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100005722.1.2",
          "form": "당근마켓으로 몇번 판매한적 있어요",
          "original_form": "당근마켓으로 몇번 판매한적 있어요",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100005722.1.3",
          "form": "저도 당근마켓 써요",
          "original_form": "저도 당근마켓 써요",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100005722.1.4",
          "form": "물건을 사기보다 판매하기 위해 사용하죠",
          "original_form": "물건을 사기보다 판매하기 위해 사용하죠",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100005722.1.5",
          "form": "저도 주로 판매용으로",
          "original_form": "저도 주로 판매용으로",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100005722.1.6",
          "form": "집에 짐 많은걸 안좋아해서 물건을 잘 안사는 편이에요",
          "original_form": "집에 짐 많은걸 안좋아해서 물건을 잘 안사는 편이에요",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100005722.1.7",
          "form": "안쓰는거 있다 싶으면 중고처분 하든지 분리배출 하든지 해요",
          "original_form": "안쓰는거 있다 싶으면 중고처분 하든지 분리배출 하든지 해요",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100005722.1.8",
          "form": "어떤거 팔아보셨어요?",
          "original_form": "어떤거 팔아보셨어요?",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100005722.1.9",
          "form": "미리미리 분리 정리를 잘하는 타입이시군요",
          "original_form": "미리미리 분리 정리를 잘하는 타입이시군요",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005722.1.10",
          "form": "현명하시네요~",
          "original_form": "현명하시네요~",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005722.1.11",
          "form": "최근에 비디오플레이어 팔았어요",
          "original_form": "최근에 비디오플레이어 팔았어요",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005722.1.12",
          "form": "그렇구나 저는 안마기랑 커피머신을 팔았어요",
          "original_form": "그렇구나 저는 안마기랑 커피머신을 팔았어요",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100005722.1.13",
          "form": "아 그리고 tv도 팔았었어요",
          "original_form": "아 그리고 tv도 팔았었어요",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100005722.1.14",
          "form": "쏠쏠하게 용돈생기는 기분이라 좋아요",
          "original_form": "쏠쏠하게 용돈생기는 기분이라 좋아요",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100005722.1.15",
          "form": "온라인쇼핑하시나요?",
          "original_form": "온라인쇼핑하시나요?",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100005722.1.16",
          "form": "저는 온라인으로 뭘 못사요 할줄 몰라서",
          "original_form": "저는 온라인으로 뭘 못사요 할줄 몰라서",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005722.1.17",
          "form": "오프라인으로 시장이나 마트에서 장 봅니다",
          "original_form": "오프라인으로 시장이나 마트에서 장 봅니다",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005722.1.18",
          "form": "가전도 매장가서 사고",
          "original_form": "가전도 매장가서 사고",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005722.1.19",
          "form": "저는 혼자 생활할때는",
          "original_form": "저는 혼자 생활할때는",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100005722.1.20",
          "form": "신선식품만 마트이용하고",
          "original_form": "신선식품만 마트이용하고",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100005722.1.21",
          "form": "온라인으로 물건 구입했거든요",
          "original_form": "온라인으로 물건 구입했거든요",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005722.1.22",
          "form": "그런데 결혼 후에는 아내따라 마트가고 백화점가서 오프라인으로 구매하는 경우가 많아졌어요",
          "original_form": "그런데 결혼 후에는 아내따라 마트가고 백화점가서 오프라인으로 구매하는 경우가 많아졌어요",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005722.1.23",
          "form": "같이 장보는 재미도 쏠쏠하죠",
          "original_form": "같이 장보는 재미도 쏠쏠하죠",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100005722.1.24",
          "form": "저야 뭐 할줄 몰라서 온라인 쇼핑을 안하는 거지만요 ㅎㅎ",
          "original_form": "저야 뭐 할줄 몰라서 온라인 쇼핑을 안하는 거지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100005722.1.25",
          "form": "맞아요 혼자 장보기는 귀찮은데",
          "original_form": "맞아요 혼자 장보기는 귀찮은데",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005722.1.26",
          "form": "같이가면 재미있어요",
          "original_form": "같이가면 재미있어요",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005722.1.27",
          "form": "그렇죠 의논해가며 살질말지 결정하고",
          "original_form": "그렇죠 의논해가며 살질말지 결정하고",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005722.1.28",
          "form": "장보면서 이런저런 얘기하면 시간 보내는 것 좋더라고요",
          "original_form": "장보면서 이런저런 얘기하면 시간 보내는 것 좋더라고요",
          "speaker_id": "1",
          "time": "20210907 16:37"
        }
      ]
