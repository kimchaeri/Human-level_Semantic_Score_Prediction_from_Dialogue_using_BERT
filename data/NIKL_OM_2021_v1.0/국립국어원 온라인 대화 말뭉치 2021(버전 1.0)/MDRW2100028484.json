      "utterance": [
        {
          "id": "MDRW2100028484.1.1",
          "form": "너가 말한 대로 환풍기 켜놓을게",
          "original_form": "너가 말한 대로 환풍기 켜놓을게",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028484.1.2",
          "form": "네 화장실에 곰팡이 안슬게 하려면 환풍기 트는게 좋아요!!",
          "original_form": "네 화장실에 곰팡이 안슬게 하려면 환풍기 트는게 좋아요!!",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028484.1.3",
          "form": "내일도 비오려나",
          "original_form": "내일도 비오려나",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028484.1.4",
          "form": "일기예보 봐봐",
          "original_form": "일기예보 봐봐",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028484.1.5",
          "form": "오는거같은데... 지금도 오고 있으니까요 ㅠ",
          "original_form": "오는거같은데... 지금도 오고 있으니까요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028484.1.6",
          "form": "아마 내일도 오지 않을까요",
          "original_form": "아마 내일도 오지 않을까요",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028484.1.7",
          "form": "보고 와봐",
          "original_form": "보고 와봐",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028484.1.8",
          "form": "흐리고 한때 비래요",
          "original_form": "흐리고 한때 비래요",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028484.1.9",
          "form": "오긴 오나봐요",
          "original_form": "오긴 오나봐요",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028484.1.10",
          "form": "빨래 돌리지 말아야겠다",
          "original_form": "빨래 돌리지 말아야겠다",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028484.1.11",
          "form": "건조기 하나 사시면 좋을텐데 ㅠㅠ",
          "original_form": "건조기 하나 사시면 좋을텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028484.1.12",
          "form": "그것도 다 돈이야",
          "original_form": "그것도 다 돈이야",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028484.1.13",
          "form": "그건 그렇지만~ 건조기 사면 좋아요",
          "original_form": "그건 그렇지만~ 건조기 사면 좋아요",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028484.1.14",
          "form": "뽀송뽀송한 수건 만나볼수 있다구요",
          "original_form": "뽀송뽀송한 수건 만나볼수 있다구요",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028484.1.15",
          "form": "보일러 틀면 된다",
          "original_form": "보일러 틀면 된다",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028484.1.16",
          "form": "비오는 날에 빨래 말리면 쉰내나는데 ㅠㅠ",
          "original_form": "비오는 날에 빨래 말리면 쉰내나는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028484.1.17",
          "form": "뭔가 식초?? 넣으면 좋다고 들은거같기도",
          "original_form": "뭔가 식초?? 넣으면 좋다고 들은거같기도",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028484.1.18",
          "form": "더 냄새나지 않겠니??",
          "original_form": "더 냄새나지 않겠니??",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028484.1.19",
          "form": "오늘도 비오더라구요",
          "original_form": "오늘도 비오더라구요",
