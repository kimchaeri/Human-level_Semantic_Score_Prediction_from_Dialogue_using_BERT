      "utterance": [
        {
          "id": "MDRW2100044655.1.1",
          "form": "ㅋㅋㅋㅋㅋ 그렇죠, 로봇 만들어서 탈출하는 것도 판타지고",
          "original_form": "ㅋㅋㅋㅋㅋ 그렇죠, 로봇 만들어서 탈출하는 것도 판타지고",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044655.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044655.1.3",
          "form": "근데 그래서 재밌는거겠죠",
          "original_form": "근데 그래서 재밌는거겠죠",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044655.1.4",
          "form": "안될걸 되게 하니까 재밌어요ㅋㅋㅋㅋ",
          "original_form": "안될걸 되게 하니까 재밌어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044655.1.5",
          "form": "그쵸ㅋㅋㅋㅋㅋ\n아, 빨리 스파이더맨 보고 싶어요\n전작 스파이디들도 나온다데요",
          "original_form": "그쵸ㅋㅋㅋㅋㅋ\n아, 빨리 스파이더맨 보고 싶어요\n전작 스파이디들도 나온다데요",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044655.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044655.1.7",
          "form": "헐헐",
          "original_form": "헐헐",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044655.1.8",
          "form": "꼭 봐야겠는데요",
          "original_form": "꼭 봐야겠는데요",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044655.1.9",
          "form": "다 나와요?",
          "original_form": "다 나와요?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044655.1.10",
          "form": "그렇대요!! 완전 어썸하지 않나요...짱기대중",
          "original_form": "그렇대요!! 완전 어썸하지 않나요...짱기대중",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044655.1.11",
          "form": "미쳤네요 후하 명작탄생일까요",
          "original_form": "미쳤네요 후하 명작탄생일까요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044655.1.12",
          "form": "두근두근합니다\n샹치랑 이터널스는 보실 거예요?",
          "original_form": "두근두근합니다\n샹치랑 이터널스는 보실 거예요?",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044655.1.13",
          "form": "샹치는 별 생각 없고 이터널스는 약간 고민 중입니당",
          "original_form": "샹치는 별 생각 없고 이터널스는 약간 고민 중입니당",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044655.1.14",
          "form": "하임님은요?",
          "original_form": "하임님은요?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044655.1.15",
          "form": "저도욬ㅋㅋㅋ 샹치는 안 끌리고 마동석이랑 졸리 땜에 이터널스는 궁금",
          "original_form": "저도욬ㅋㅋㅋ 샹치는 안 끌리고 마동석이랑 졸리 땜에 이터널스는 궁금",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044655.1.16",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044655.1.17",
          "form": "하임님 영화 보실때 스크린 크기 중요하게 샹각하세요?",
          "original_form": "하임님 영화 보실때 스크린 크기 중요하게 샹각하세요?",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044655.1.18",
          "form": "저는 그건 크게 신경 안써요ㅋㅋㅋ 좀 둔하다고 해야되나 알못이라서\n금송님은요?",
          "original_form": "저는 그건 크게 신경 안써요ㅋㅋㅋ 좀 둔하다고 해야되나 알못이라서\n금송님은요?",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044655.1.19",
          "form": "저두요!ㅋㅋㅋㅋ 근데 그거 되게 중요하게 생각하시는 분들도 많길래 여쭤봤어요ㅎㅎ",
          "original_form": "저두요!ㅋㅋㅋㅋ 근데 그거 되게 중요하게 생각하시는 분들도 많길래 여쭤봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044655.1.20",
          "form": "오, 그렇군영ㅋㅋㅋ 저는 뭐 눈에 들어오면 오케이입니다",
          "original_form": "오, 그렇군영ㅋㅋㅋ 저는 뭐 눈에 들어오면 오케이입니다",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044655.1.21",
          "form": "저도 그래요ㅋㅋㅋㅋ",
          "original_form": "저도 그래요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        }
      ]
