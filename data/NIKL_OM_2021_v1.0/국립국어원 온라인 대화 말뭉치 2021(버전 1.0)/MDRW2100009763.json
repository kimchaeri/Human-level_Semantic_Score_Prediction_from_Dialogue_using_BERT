      "utterance": [
        {
          "id": "MDRW2100009763.1.1",
          "form": "알바다녀와서 김밥먹었어요.ㅋ",
          "original_form": "알바다녀와서 김밥먹었어요.ㅋ",
          "speaker_id": "2",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009763.1.2",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009763.1.3",
          "form": "소고기요",
          "original_form": "소고기요",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009763.1.4",
          "form": "소고기 좋은고기 드셨네요ㅋㅋ",
          "original_form": "소고기 좋은고기 드셨네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100009763.1.5",
          "form": "오늘은 특별히 ㅎ",
          "original_form": "오늘은 특별히 ㅎ",
          "speaker_id": "1",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100009763.1.6",
          "form": "저녁은 뭐드세요?",
          "original_form": "저녁은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100009763.1.7",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100009763.1.8",
          "form": "저는 어제 찜닭먹었어요ㅋ",
          "original_form": "저는 어제 찜닭먹었어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 07:24"
        },
        {
          "id": "MDRW2100009763.1.9",
          "form": "피곤해서 일찍잤네요ㅋ",
          "original_form": "피곤해서 일찍잤네요ㅋ",
          "speaker_id": "2",
          "time": "20210824 07:25"
        },
        {
          "id": "MDRW2100009763.1.10",
          "form": "맛나죠",
          "original_form": "맛나죠",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100009763.1.11",
          "form": "네네 ㅋ점심도 찜닭이네요",
          "original_form": "네네 ㅋ점심도 찜닭이네요",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100009763.1.12",
          "form": "점심 뭐드세요?",
          "original_form": "점심 뭐드세요?",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100009763.1.13",
          "form": "한식이여",
          "original_form": "한식이여",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100009763.1.14",
          "form": "ㅋㅋ 저는 오리고기먹었네요ㅋㅋ",
          "original_form": "ㅋㅋ 저는 오리고기먹었네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100009763.1.15",
          "form": "너무 맛있었어요ㅋ",
          "original_form": "너무 맛있었어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100009763.1.16",
          "form": "오리 부드럽죠",
          "original_form": "오리 부드럽죠",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100009763.1.17",
          "form": "네 부드러워요.ㅋ",
          "original_form": "네 부드러워요.ㅋ",
