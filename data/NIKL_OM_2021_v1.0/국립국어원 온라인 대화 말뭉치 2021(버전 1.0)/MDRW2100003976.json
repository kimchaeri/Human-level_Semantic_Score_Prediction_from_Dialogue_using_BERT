      "utterance": [
        {
          "id": "MDRW2100003976.1.1",
          "form": "엘지광고가 많이 나오네",
          "original_form": "엘지광고가 많이 나오네",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003976.1.2",
          "form": "전자제품은 난 엘지가 더 낫드라",
          "original_form": "전자제품은 난 엘지가 더 낫드라",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003976.1.3",
          "form": "언니가 좀 있다가 티비 산다고 하더라",
          "original_form": "언니가 좀 있다가 티비 산다고 하더라",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003976.1.4",
          "form": "우리는 동생 때문에 삼성 샀는데 샘은 엘지사라",
          "original_form": "우리는 동생 때문에 삼성 샀는데 샘은 엘지사라",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003976.1.5",
          "form": "전지현도 너무 이쁘게 나온다",
          "original_form": "전지현도 너무 이쁘게 나온다",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003976.1.6",
          "form": "원래 이쁘자나",
          "original_form": "원래 이쁘자나",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003976.1.7",
          "form": "단발도 정말 잘 어울리네",
          "original_form": "단발도 정말 잘 어울리네",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003976.1.8",
          "form": "뭔들 안 어울릴까?",
          "original_form": "뭔들 안 어울릴까?",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100003976.1.9",
          "form": "그러게 다 이뻐서 착용한게 다 품절되나보다",
          "original_form": "그러게 다 이뻐서 착용한게 다 품절되나보다",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100003976.1.10",
          "form": "엄청나게 돈 모았겠어",
          "original_form": "엄청나게 돈 모았겠어",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100003976.1.11",
          "form": "광고 게약금으로만해도 빌딩 사겠다",
          "original_form": "광고 게약금으로만해도 빌딩 사겠다",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100003976.1.12",
          "form": "빌딩을 현금으로 샀다고 하드라고",
          "original_form": "빌딩을 현금으로 샀다고 하드라고",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100003976.1.13",
          "form": "금방 돈을 모으는듯하다 ",
          "original_form": "금방 돈을 모으는듯하다 ",
