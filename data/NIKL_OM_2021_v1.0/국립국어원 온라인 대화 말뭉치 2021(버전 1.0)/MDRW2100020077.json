      "utterance": [
        {
          "id": "MDRW2100020077.1.1",
          "form": "맞아요 광고 엄청 늘더라고요 ^.ㅠ",
          "original_form": "맞아요 광고 엄청 늘더라고요 ^.ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020077.1.2",
          "form": "그럼 다른 음원 사이트는 안 쓰시는 건가요?",
          "original_form": "그럼 다른 음원 사이트는 안 쓰시는 건가요?",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020077.1.3",
          "form": "멜론같은!",
          "original_form": "멜론같은!",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020077.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020077.1.5",
          "form": "제가 해외음악도 많이 들어서",
          "original_form": "제가 해외음악도 많이 들어서",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020077.1.6",
          "form": "플로 잠깐 써봤는데 생각보다 노래가 얼마 없더라구요",
          "original_form": "플로 잠깐 써봤는데 생각보다 노래가 얼마 없더라구요",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020077.1.7",
          "form": "헉 그렇구나...! 유튜브가 그래도 제일 많죠?!",
          "original_form": "헉 그렇구나...! 유튜브가 그래도 제일 많죠?!",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020077.1.8",
          "form": "name2 해외음악 뭐 들으세요!?",
          "original_form": "&name2& 해외음악 뭐 들으세요!?",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020077.1.9",
          "form": "저도 추천해주세요!",
          "original_form": "저도 추천해주세요!",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020077.1.10",
          "form": "저 요즘 추억의 음악 듣고있어요 ㅋㅋㅋ",
          "original_form": "저 요즘 추억의 음악 듣고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:24"
        },
        {
          "id": "MDRW2100020077.1.11",
          "form": "약간 싸이월드 감성 00년대 노래들이요 ㅋㅋㅋ",
          "original_form": "약간 싸이월드 감성 00년대 노래들이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:24"
        },
        {
          "id": "MDRW2100020077.1.12",
          "form": "앗 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요 그런 노래들 들으면",
          "original_form": "앗 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요 그런 노래들 들으면",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020077.1.13",
          "form": "괜히 몽글몽글 하더라고요",
          "original_form": "괜히 몽글몽글 하더라고요",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020077.1.14",
          "form": "그 y 처럼!",
          "original_form": "그 y 처럼!",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020077.1.15",
          "form": "그쵸 그시절 감성이 있는거같아요 ㅋㅋㅋㅋ",
          "original_form": "그쵸 그시절 감성이 있는거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020077.1.16",
          "form": "혹시 싫어하는 음악 장르도 있으세요?",
          "original_form": "혹시 싫어하는 음악 장르도 있으세요?",
          "speaker_id": "1",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100020077.1.17",
          "form": "저 랩이랑 락은 별로 안좋아해요",
          "original_form": "저 랩이랑 락은 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020077.1.18",
          "form": "name1은 싫어하는 장르 있으신가요?",
          "original_form": "&name1&은 싫어하는 장르 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020077.1.19",
          "form": "저도 랩 싫어해요 ㅠ !",
          "original_form": "저도 랩 싫어해요 ㅠ !",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020077.1.20",
          "form": "중2병 걸렸을 때 한창 듣다가 ^^..",
          "original_form": "중2병 걸렸을 때 한창 듣다가 ^^..",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020077.1.21",
          "form": "고등학겨 때 정신차렸었어요",
          "original_form": "고등학겨 때 정신차렸었어요",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020077.1.22",
          "form": "앗 ㅋㅋㅋㅋ 그때는 다들 그장르 한번씩 찍먹 해보는 시기인거 같아요 ㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋ 그때는 다들 그장르 한번씩 찍먹 해보는 시기인거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100020077.1.23",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ 지금 와서 생각해보면",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ 지금 와서 생각해보면",
          "speaker_id": "1",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020077.1.24",
          "form": "너무... 부끄러운 ^.ㅠ",
          "original_form": "너무... 부끄러운 ^.ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020077.1.25",
          "form": "name2 좋아하는 가수 있으신가요?!",
          "original_form": "&name2& 좋아하는 가수 있으신가요?!",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020077.1.26",
          "form": "저 최근에 씨야 노래 다시 듣고있어요!",
          "original_form": "저 최근에 씨야 노래 다시 듣고있어요!",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020077.1.27",
          "form": "헐!!! 그 사랑의 인사?",
          "original_form": "헐!!! 그 사랑의 인사?",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020077.1.28",
          "form": "부른 그룹 맞죠!",
          "original_form": "부른 그룹 맞죠!",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
