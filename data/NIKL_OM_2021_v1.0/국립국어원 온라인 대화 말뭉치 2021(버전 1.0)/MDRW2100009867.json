        {
          "id": "MDRW2100009867.1.1",
          "form": "동호회나 봉사모임 경험 있으신가요",
          "original_form": "동호회나 봉사모임 경험 있으신가요",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100009867.1.2",
          "form": "양로원봉사나",
          "original_form": "양로원봉사나",
          "speaker_id": "2",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100009867.1.3",
          "form": "백패킹동호회요!",
          "original_form": "백패킹동호회요!",
          "speaker_id": "2",
          "time": "20210823 14:09"
        },
        {
          "id": "MDRW2100009867.1.4",
          "form": "와 백패킹이면 등산인가요 캠핑인가요",
          "original_form": "와 백패킹이면 등산인가요 캠핑인가요",
          "speaker_id": "1",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100009867.1.5",
          "form": "둘다에요ㅎ",
          "original_form": "둘다에요ㅎ",
          "speaker_id": "2",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100009867.1.6",
          "form": "캠핑에가깝지만?",
          "original_form": "캠핑에가깝지만?",
          "speaker_id": "2",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100009867.1.7",
          "form": "오호 그럼 산에서 주무시고 오시는건가요?",
          "original_form": "오호 그럼 산에서 주무시고 오시는건가요?",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100009867.1.8",
          "form": "넹 1박2일요",
          "original_form": "넹 1박2일요",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100009867.1.9",
          "form": "힘드시진 않나요? 넘 멋지셔요^^",
          "original_form": "힘드시진 않나요? 넘 멋지셔요^^",
          "speaker_id": "1",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100009867.1.10",
          "form": "재미있더라구요ㅎ",
          "original_form": "재미있더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100009867.1.11",
          "form": "산에는 텐트 칠 장소가 따로 마련되어 있나요?",
          "original_form": "산에는 텐트 칠 장소가 따로 마련되어 있나요?",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009867.1.12",
          "form": "아주 가파른산말고 텐트칠만한 평지잇는곳을 가요",
          "original_form": "아주 가파른산말고 텐트칠만한 평지잇는곳을 가요",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009867.1.13",
          "form": "보툥 화장실은 있나요? 저도 캠핑 종종 가는데 그런 시설이 중요하더라구요^^;",
          "original_form": "보툥 화장실은 있나요? 저도 캠핑 종종 가는데 그런 시설이 중요하더라구요^^;",
          "speaker_id": "1",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100009867.1.14",
          "form": "없어요..",
          "original_form": "없어요..",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100009867.1.15",
          "form": "아 저는 어렵겠네요 ㅠㅠㅋㅋ",
          "original_form": "아 저는 어렵겠네요 ㅠㅠㅋㅋ",
