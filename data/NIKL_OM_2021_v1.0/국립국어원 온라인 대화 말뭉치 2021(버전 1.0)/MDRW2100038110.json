      "utterance": [
        {
          "id": "MDRW2100038110.1.1",
          "form": "ㅋㅋ지금하고계신가요?",
          "original_form": "ㅋㅋ지금하고계신가요?",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038110.1.2",
          "form": "네 그래서 대화하다가 에프를 더 돌려서 그런가",
          "original_form": "네 그래서 대화하다가 에프를 더 돌려서 그런가",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038110.1.3",
          "form": "왜 수플레 케이크가 바삭하죠",
          "original_form": "왜 수플레 케이크가 바삭하죠",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038110.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038110.1.5",
          "form": "흠 ㅜㅜㅜ수플레는 언제 고수가되지 ㅋㅋㅋ 그래도맛은 괜찮지않나요",
          "original_form": "흠 ㅜㅜㅜ수플레는 언제 고수가되지 ㅋㅋㅋ 그래도맛은 괜찮지않나요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038110.1.6",
          "form": "네 밀가루가 60그램 들어가는 주제에",
          "original_form": "네 밀가루가 60그램 들어가는 주제에",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038110.1.7",
          "form": "버터 60에 설탕 60이 들어가는 레시피니",
          "original_form": "버터 60에 설탕 60이 들어가는 레시피니",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038110.1.8",
          "form": "맛이 없음 혼내줄거에요",
          "original_form": "맛이 없음 혼내줄거에요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038110.1.9",
          "form": "ㅋㅋㅋㅋ 저도먹구싶네요",
          "original_form": "ㅋㅋㅋㅋ 저도먹구싶네요",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038110.1.10",
          "form": "조아요 제가 딱딱해진 껍질만 떼서 드릴께요 ㅎㅎㅎ",
          "original_form": "조아요 제가 딱딱해진 껍질만 떼서 드릴께요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038110.1.11",
          "form": "농담이에요 ㅋㅋ",
          "original_form": "농담이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038110.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038110.1.13",
          "form": "안엔 갠찬나요",
          "original_form": "안엔 갠찬나요",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038110.1.14",
          "form": "아직 안식어서 틀에서 빨 수가 없어요",
          "original_form": "아직 안식어서 틀에서 빨 수가 없어요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038110.1.15",
          "form": "안식었을때 막 두들겨서 빼면",
          "original_form": "안식었을때 막 두들겨서 빼면",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038110.1.16",
          "form": "아아아 ㅋㅋㅋㅋ",
          "original_form": "아아아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038110.1.17",
          "form": "머랭을 많이 취어서 넣는거자나여",
          "original_form": "머랭을 많이 취어서 넣는거자나여",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038110.1.18",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038110.1.19",
          "form": "틀은 사신건가요",
          "original_form": "틀은 사신건가요",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038110.1.20",
          "form": "그래서 식음쪼그라들어용",
          "original_form": "그래서 식음쪼그라들어용",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038110.1.21",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038110.1.22",
          "form": "ㅋㅋ 오븐욕심 안나시나요",
          "original_form": "ㅋㅋ 오븐욕심 안나시나요",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038110.1.23",
          "form": "욕심입니다 ㅋㅋ",
          "original_form": "욕심입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038110.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038110.1.25",
          "form": "스콘 구워보세용 쉽고맛나요",
          "original_form": "스콘 구워보세용 쉽고맛나요",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038110.1.26",
          "form": "아 뭐가 덜갔네요",
          "original_form": "아 뭐가 덜갔네요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038110.1.27",
          "form": "내가 안사고",
          "original_form": "내가 안사고",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038110.1.28",
          "form": "남이 사주면 좋겠다 정도의",
          "original_form": "남이 사주면 좋겠다 정도의",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
