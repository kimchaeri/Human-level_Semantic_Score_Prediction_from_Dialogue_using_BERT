      "utterance": [
        {
          "id": "MDRW2100046738.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 11:57"
        },
        {
          "id": "MDRW2100046738.1.2",
          "form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "original_form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "speaker_id": "2",
          "time": "20211104 11:57"
        },
        {
          "id": "MDRW2100046738.1.3",
          "form": "네 맛점하시고 카톡주세요^^",
          "original_form": "네 맛점하시고 카톡주세요^^",
          "speaker_id": "1",
          "time": "20211105 00:04"
        },
        {
          "id": "MDRW2100046738.1.4",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 13:18"
        },
        {
          "id": "MDRW2100046738.1.5",
          "form": "네 먹었어요ㅎㅎ",
          "original_form": "네 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 13:19"
        },
        {
          "id": "MDRW2100046738.1.6",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20211104 13:19"
        },
        {
          "id": "MDRW2100046738.1.7",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "1",
          "time": "20211104 13:19"
        },
        {
          "id": "MDRW2100046738.1.8",
          "form": "저는 덮밥 먹고왔어요!",
          "original_form": "저는 덮밥 먹고왔어요!",
          "speaker_id": "2",
          "time": "20211104 13:23"
        },
        {
          "id": "MDRW2100046738.1.9",
          "form": "뭐 드셨어요~?",
          "original_form": "뭐 드셨어요~?",
          "speaker_id": "2",
          "time": "20211104 13:23"
        },
        {
          "id": "MDRW2100046738.1.10",
          "form": "저는 제육볶음먹었습니다ㅎㅎ",
          "original_form": "저는 제육볶음먹었습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 13:25"
        },
        {
          "id": "MDRW2100046738.1.11",
          "form": "어떤 덮밥드셨나요?",
          "original_form": "어떤 덮밥드셨나요?",
          "speaker_id": "1",
          "time": "20211104 13:26"
        },
        {
          "id": "MDRW2100046738.1.12",
          "form": "앗 ㅎㅎ 직장인들 최애 메뉴 드셨네요 ㅎㅎ",
          "original_form": "앗 ㅎㅎ 직장인들 최애 메뉴 드셨네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:34"
        },
        {
          "id": "MDRW2100046738.1.13",
          "form": "전 대창덮밥 먹었어요! 오랜만에 기름칠 ㅎㅎ",
          "original_form": "전 대창덮밥 먹었어요! 오랜만에 기름칠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:34"
        },
        {
          "id": "MDRW2100046738.1.14",
          "form": "대창덮밥 맛있죠ㅎㅎ",
          "original_form": "대창덮밥 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 13:35"
        },
        {
          "id": "MDRW2100046738.1.15",
          "form": "주변에 맛있는음식점이많으신가요?",
          "original_form": "주변에 맛있는음식점이많으신가요?",
          "speaker_id": "1",
          "time": "20211104 13:36"
        },
        {
          "id": "MDRW2100046738.1.16",
          "form": "네! 음식점 꽤 많아서",
          "original_form": "네! 음식점 꽤 많아서",
          "speaker_id": "2",
          "time": "20211104 13:47"
        },
        {
          "id": "MDRW2100046738.1.17",
          "form": "매일 새로운 음식점 탐험 다니고 있습니다 ㅎㅎ",
          "original_form": "매일 새로운 음식점 탐험 다니고 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:47"
        },
        {
          "id": "MDRW2100046738.1.18",
          "form": "부럽네요 저희회사쪽은 메뉴가 한정적이라 갈곳이 없거든요ㅠ",
          "original_form": "부럽네요 저희회사쪽은 메뉴가 한정적이라 갈곳이 없거든요ㅠ",
          "speaker_id": "1",
          "time": "20211104 13:48"
        },
        {
          "id": "MDRW2100046738.1.19",
          "form": "저도 예전에 다니던 회사는",
          "original_form": "저도 예전에 다니던 회사는",
          "speaker_id": "2",
          "time": "20211104 14:04"
        },
        {
          "id": "MDRW2100046738.1.20",
          "form": "엄청 고립되어있어서 밥 먹는게 일이었어요 ㅠㅠ",
          "original_form": "엄청 고립되어있어서 밥 먹는게 일이었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 14:04"
        },
        {
          "id": "MDRW2100046738.1.21",
          "form": "밥집고르는것도 하나의 재미겠어요ㅠㅠ",
          "original_form": "밥집고르는것도 하나의 재미겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20211104 14:05"
        },
        {
          "id": "MDRW2100046738.1.22",
          "form": "네! 점심 먹는 낙으로 살아갑니다 ㅎㅎㅎㅎ",
          "original_form": "네! 점심 먹는 낙으로 살아갑니다 ㅎㅎㅎㅎ",
