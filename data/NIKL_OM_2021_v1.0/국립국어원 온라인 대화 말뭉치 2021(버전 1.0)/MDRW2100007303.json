      "utterance": [
        {
          "id": "MDRW2100007303.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007303.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007303.1.3",
          "form": "저는 야구 좋아하는데 혹시 야구 좋아하세요?!",
          "original_form": "저는 야구 좋아하는데 혹시 야구 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007303.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007303.1.5",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007303.1.6",
          "form": "야구는 간단한 룰만 알아요",
          "original_form": "야구는 간단한 룰만 알아요",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007303.1.7",
          "form": "응원하는 팀 있으세요?",
          "original_form": "응원하는 팀 있으세요?",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007303.1.8",
          "form": "저는 삼성라이온즈 응원해요!",
          "original_form": "저는 삼성라이온즈 응원해요!",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007303.1.9",
          "form": "아! 삼성은 그래도 잘하는 팀이죠?",
          "original_form": "아! 삼성은 그래도 잘하는 팀이죠?",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007303.1.10",
          "form": "한화나 기아는 주변 지인인 팬들이 욕하는 거 많이 들었는데",
          "original_form": "한화나 기아는 주변 지인인 팬들이 욕하는 거 많이 들었는데",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007303.1.11",
          "form": "감사합니다 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "감사합니다 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.12",
          "form": "삼성을 잘하는팀으로 기억해주셔서... 감사합니다...",
          "original_form": "삼성을 잘하는팀으로 기억해주셔서... 감사합니다...",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.14",
          "form": "직관도 다니시나요?",
          "original_form": "직관도 다니시나요?",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.15",
          "form": "코로나 전엔 종종 다니다가 요새는 구장이 좀 멀어서",
          "original_form": "코로나 전엔 종종 다니다가 요새는 구장이 좀 멀어서",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.16",
          "form": "안 가고 있어요ㅠㅠ",
          "original_form": "안 가고 있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.17",
          "form": "응원가 불러야하는데! 실시간으로 사람들 화내는거 들어야하는데!!",
          "original_form": "응원가 불러야하는데! 실시간으로 사람들 화내는거 들어야하는데!!",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.18",
          "form": "아 코로나때문에 확실히 어렵겠네요.",
          "original_form": "아 코로나때문에 확실히 어렵겠네요.",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007303.1.19",
          "form": "그럼 이번 올림픽때도 야구 보셨어요?",
          "original_form": "그럼 이번 올림픽때도 야구 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007303.1.20",
          "form": "올림픽 야구... 봤어요",
          "original_form": "올림픽 야구... 봤어요",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007303.1.21",
          "form": "ㅎㅎ전 기사로만 접했는데 조금 부진했던 것 같더라구요",
          "original_form": "ㅎㅎ전 기사로만 접했는데 조금 부진했던 것 같더라구요",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007303.1.22",
          "form": "맞아요... 그건 어쩔 수 없지",
          "original_form": "맞아요... 그건 어쩔 수 없지",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007303.1.23",
          "form": "하지만 감독이 정말 못했어요...",
          "original_form": "하지만 감독이 정말 못했어요...",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007303.1.24",
          "form": "저도 스포츠 직관은 가끔 가봤는데 야구 배구 농구 가봤어요",
          "original_form": "저도 스포츠 직관은 가끔 가봤는데 야구 배구 농구 가봤어요",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007303.1.25",
          "form": "다른 것도 가보셨어요?",
          "original_form": "다른 것도 가보셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007303.1.26",
          "form": "저도 배구 농구 가봤어요!",
          "original_form": "저도 배구 농구 가봤어요!",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007303.1.27",
          "form": "오 배구 농구도 보면 재밌더라구요",
          "original_form": "오 배구 농구도 보면 재밌더라구요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007303.1.28",
          "form": "룰 몰라도 즐기기 쉽고",
          "original_form": "룰 몰라도 즐기기 쉽고",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007303.1.29",
          "form": "그쵸! 뭔가 가볍게? 직관가기엔",
          "original_form": "그쵸! 뭔가 가볍게? 직관가기엔",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007303.1.30",
          "form": "요즘 여자배구 인기가 좋아서 나중에 여배 경기도 가보고싶은데 티켓팅 어렵겠죠?",
          "original_form": "요즘 여자배구 인기가 좋아서 나중에 여배 경기도 가보고싶은데 티켓팅 어렵겠죠?",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007303.1.31",
          "form": "바로바로 점수 나는 스포츠들이 좋은거 같아요",
          "original_form": "바로바로 점수 나는 스포츠들이 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007303.1.32",
          "form": "여배 티켓팅 조금 빡셀거 같은 감 있어요ㅋㅋㅋㅋㅋ",
          "original_form": "여배 티켓팅 조금 빡셀거 같은 감 있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007303.1.33",
          "form": "아 맞아요! 그리고 턴도 짧고! 축구같은 건 길어서 잘 못봐요ㅜㅜ",
          "original_form": "아 맞아요! 그리고 턴도 짧고! 축구같은 건 길어서 잘 못봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007303.1.34",
          "form": "근데 전 가까운데에 배구 경기장이 없어서..ㅠㅠ",
          "original_form": "근데 전 가까운데에 배구 경기장이 없어서..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007303.1.35",
          "form": "저도 제가 사는 곳 말고 수원인가 경기장 가본 거 같아요",
          "original_form": "저도 제가 사는 곳 말고 수원인가 경기장 가본 거 같아요",
          "speaker_id": "2",
          "time": "20210822 01:26"
        }
      ]
    }
