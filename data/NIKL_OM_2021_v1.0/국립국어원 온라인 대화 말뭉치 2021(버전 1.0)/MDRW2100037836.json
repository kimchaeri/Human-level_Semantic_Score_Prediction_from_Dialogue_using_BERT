      "utterance": [
        {
          "id": "MDRW2100037836.1.1",
          "form": "하고싶은 운동 있으세요?",
          "original_form": "하고싶은 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037836.1.2",
          "form": "실내운동인게 좋네요",
          "original_form": "실내운동인게 좋네요",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037836.1.3",
          "form": "저는운동 안좋아하는데",
          "original_form": "저는운동 안좋아하는데",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037836.1.4",
          "form": "좋아해보려고요ㅜㅜ",
          "original_form": "좋아해보려고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037836.1.5",
          "form": "걷는거? 그건 잘해요 ㅋㅋ",
          "original_form": "걷는거? 그건 잘해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100037836.1.6",
          "form": "그쵸 ㅜㅜㅜ 이제 운동 안하면 생명의 위협이..",
          "original_form": "그쵸 ㅜㅜㅜ 이제 운동 안하면 생명의 위협이..",
          "speaker_id": "1",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100037836.1.7",
          "form": "걷는거 재밌어요!!",
          "original_form": "걷는거 재밌어요!!",
          "speaker_id": "1",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100037836.1.8",
          "form": "근데 이걸로 막다이어트가되지는 않더라고요ㅜ",
          "original_form": "근데 이걸로 막다이어트가되지는 않더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100037836.1.9",
          "form": "2만보씩 걸었는덷ᆢ",
          "original_form": "2만보씩 걸었는덷ᆢ",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100037836.1.10",
          "form": "데도",
          "original_form": "데도",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100037836.1.11",
          "form": "2만보면 엄청 많은거아닌가요 ㄷㄷ",
          "original_form": "2만보면 엄청 많은거아닌가요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100037836.1.12",
          "form": "ㅋㅋ근데 더 살쪘어요",
          "original_form": "ㅋㅋ근데 더 살쪘어요",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037836.1.13",
          "form": "잘먹어서 ㅋㅋ",
          "original_form": "잘먹어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037836.1.14",
          "form": "입맛이 도셨구나 ㅋㅋㅋㅋ",
          "original_form": "입맛이 도셨구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037836.1.15",
          "form": "건강해지셨겠네요",
          "original_form": "건강해지셨겠네요",
          "speaker_id": "1",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037836.1.16",
          "form": "음 건강도 나빠졌어요 ㅋㄱ",
          "original_form": "음 건강도 나빠졌어요 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037836.1.17",
          "form": "왜냐면 ㅋㅋㅋ",
          "original_form": "왜냐면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037836.1.18",
          "form": "헉 왜요",
          "original_form": "헉 왜요",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037836.1.19",
          "form": "일단 걷기전 프라페 1리터짜리사서 마시면서걸음",
          "original_form": "일단 걷기전 프라페 1리터짜리사서 마시면서걸음",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037836.1.20",
          "form": "ㅋㅋㅋㅋㅋ일리텈ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ일리텈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037836.1.21",
          "form": "마치고 햄버거 버거킹 세일하는거 2개세트먹음",
          "original_form": "마치고 햄버거 버거킹 세일하는거 2개세트먹음",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037836.1.22",
          "form": "후식 1리터 버블티 ㅋㅋ",
          "original_form": "후식 1리터 버블티 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037836.1.23",
          "form": "개망함 ㅋㅋ",
          "original_form": "개망함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100037836.1.24",
          "form": "2 l를 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "2 l를 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100037836.1.25",
          "form": "걷기가 입맛이 엄청 돋궈지나본데욬ㅋㅋㅋ",
          "original_form": "걷기가 입맛이 엄청 돋궈지나본데욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100037836.1.26",
          "form": "그리고 돈도 엄청 썼어요ㅋㅋ",
          "original_form": "그리고 돈도 엄청 썼어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100037836.1.27",
          "form": "나중엔 버거킹에서 킹오더 저인거알고 걍줌 ㄷㄷ",
          "original_form": "나중엔 버거킹에서 킹오더 저인거알고 걍줌 ㄷㄷ",
