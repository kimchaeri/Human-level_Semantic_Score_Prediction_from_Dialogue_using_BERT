      "utterance": [
        {
          "id": "MDRW2100037448.1.1",
          "form": "어떤음식 좋아하세요?",
          "original_form": "어떤음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100037448.1.2",
          "form": "전 대부분을 좋아하는데 요즘계절엔 훠궈요",
          "original_form": "전 대부분을 좋아하는데 요즘계절엔 훠궈요",
          "speaker_id": "1",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100037448.1.3",
          "form": "훠궈는 어떤음식인가요?",
          "original_form": "훠궈는 어떤음식인가요?",
          "speaker_id": "2",
          "time": "20210901 07:23"
        },
        {
          "id": "MDRW2100037448.1.4",
          "form": "중국식 샤브샤브요ㅋㅋ",
          "original_form": "중국식 샤브샤브요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100037448.1.5",
          "form": "오! 마라탕이랑 다른거죠??!",
          "original_form": "오! 마라탕이랑 다른거죠??!",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100037448.1.6",
          "form": "마라탕이랑 비슷해요!",
          "original_form": "마라탕이랑 비슷해요!",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100037448.1.7",
          "form": "다만 제가 익혀서 건더기만 건져 먹어요 ㅋㅋ",
          "original_form": "다만 제가 익혀서 건더기만 건져 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100037448.1.8",
          "form": "아항~ 안먹어본거라 궁금하네요ㅋㅋ",
          "original_form": "아항~ 안먹어본거라 궁금하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100037448.1.9",
          "form": "처음 시작은 무한리필집으로 해보세요",
          "original_form": "처음 시작은 무한리필집으로 해보세요",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100037448.1.10",
          "form": "맘대로 골라먹을 수 있어서 좋아요",
          "original_form": "맘대로 골라먹을 수 있어서 좋아요",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100037448.1.11",
          "form": "오오.. 건두부?가 맛있다고들 하더라구요~?!",
          "original_form": "오오.. 건두부?가 맛있다고들 하더라구요~?!",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037448.1.12",
          "form": "그건 마라탕에도 들어가자나요",
          "original_form": "그건 마라탕에도 들어가자나요",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037448.1.13",
          "form": "재료고르는 마라탕집에 꼭 있던데여 마라탕의 겅두부를 조아하시나요?",
          "original_form": "재료고르는 마라탕집에 꼭 있던데여 마라탕의 겅두부를 조아하시나요?",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037448.1.14",
          "form": "저는 한번도 못먹어봐서요ㅠㅜ",
          "original_form": "저는 한번도 못먹어봐서요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037448.1.15",
          "form": "훠궈랑 마라탕이랑 비슷하다셔서 안에 들어가는것도 비슷한줄 알았어욥",
          "original_form": "훠궈랑 마라탕이랑 비슷하다셔서 안에 들어가는것도 비슷한줄 알았어욥",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100037448.1.16",
          "form": "마라탕 재료를 직접 안고르세여?",
          "original_form": "마라탕 재료를 직접 안고르세여?",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100037448.1.17",
          "form": "셀프로 재료뜨고무게 다는 식의 마라탕집에선 그렇게 해요",
          "original_form": "셀프로 재료뜨고무게 다는 식의 마라탕집에선 그렇게 해요",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037448.1.18",
          "form": "아예 안먹어 본거여서요!",
          "original_form": "아예 안먹어 본거여서요!",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037448.1.19",
          "form": "그렇군욤",
          "original_form": "그렇군욤",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037448.1.20",
          "form": "혹시 담에 재료를 셀프로 뜨는 마라탕집에가면",
          "original_form": "혹시 담에 재료를 셀프로 뜨는 마라탕집에가면",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037448.1.21",
          "form": "건두부도 넣어보세요 아마 물에 푹 불려놨을거에요",
          "original_form": "건두부도 넣어보세요 아마 물에 푹 불려놨을거에요",
          "speaker_id": "1",
          "time": "20210901 21:43"
        }
      ]
    }
