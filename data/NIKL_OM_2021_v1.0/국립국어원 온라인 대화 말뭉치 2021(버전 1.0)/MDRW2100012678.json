      "utterance": [
        {
          "id": "MDRW2100012678.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100012678.1.2",
          "form": "너 동물 보호단체에 관심있니",
          "original_form": "너 동물 보호단체에 관심있니",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100012678.1.3",
          "form": "안녕 오랜만이야",
          "original_form": "안녕 오랜만이야",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.4",
          "form": "관심없구나.. 나는 있어",
          "original_form": "관심없구나.. 나는 있어",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.5",
          "form": "응 후원은 안하지만",
          "original_form": "응 후원은 안하지만",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.6",
          "form": "너는 카라라는 단체 알아?",
          "original_form": "너는 카라라는 단체 알아?",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.7",
          "form": "가끔 기사같은거 나면 읽어봐",
          "original_form": "가끔 기사같은거 나면 읽어봐",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.8",
          "form": "나는 가끔 해피빈이 생기면 기부를 해",
          "original_form": "나는 가끔 해피빈이 생기면 기부를 해",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.9",
          "form": "응응 알아!",
          "original_form": "응응 알아!",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.10",
          "form": "오 기사도 읽어보는구나",
          "original_form": "오 기사도 읽어보는구나",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.11",
          "form": "아맞다 나두 해피빈 기부해",
          "original_form": "아맞다 나두 해피빈 기부해",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.12",
          "form": "나는 동물 보호단체가 좋아",
          "original_form": "나는 동물 보호단체가 좋아",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100012678.1.13",
          "form": "맞아 동물들을 보호하는건",
          "original_form": "맞아 동물들을 보호하는건",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.14",
          "form": "근데 기부를 할 때에 무슨 인식개선교육비 어쩌구 들어가면 기부하기가 꺼려져",
          "original_form": "근데 기부를 할 때에 무슨 인식개선교육비 어쩌구 들어가면 기부하기가 꺼려져",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.15",
          "form": "정말 좋은 일이야",
          "original_form": "정말 좋은 일이야",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.16",
          "form": "중간에 돈을 가로챌 거 같아",
          "original_form": "중간에 돈을 가로챌 거 같아",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.17",
          "form": "그건뭐야? 인간한테 주는건가?",
          "original_form": "그건뭐야? 인간한테 주는건가?",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.18",
          "form": "교육비 없이도 충분히 활동할 수 있어",
          "original_form": "교육비 없이도 충분히 활동할 수 있어",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.19",
          "form": "응 그런 거 같아",
          "original_form": "응 그런 거 같아",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.20",
          "form": "무슨 캠페인 비용 이런것도 별로 안좋아해",
          "original_form": "무슨 캠페인 비용 이런것도 별로 안좋아해",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.21",
          "form": "그러게 나는 동물에게 후원하는건데",
          "original_form": "그러게 나는 동물에게 후원하는건데",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.22",
          "form": "밥도 알아서 사먹길...",
          "original_form": "밥도 알아서 사먹길...",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.23",
          "form": "봉사단체들이 자금을 투명하게 썼으면 해",
          "original_form": "봉사단체들이 자금을 투명하게 썼으면 해",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100012678.1.24",
          "form": "너 지역 비혼동호회에대해 알라?",
          "original_form": "너 지역 비혼동호회에대해 알라?",
          "speaker_id": "2",
          "time": "20210831 22:37"
        }
      ]
    }
  ]
