      "utterance": [
        {
          "id": "MDRW2100041546.1.1",
          "form": "출근했나??",
          "original_form": "출근했나??",
          "speaker_id": "1",
          "time": "20210906 10:09"
        },
        {
          "id": "MDRW2100041546.1.2",
          "form": "내일 진짜 안 되겠나?? ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "내일 진짜 안 되겠나?? ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 10:10"
        },
        {
          "id": "MDRW2100041546.1.3",
          "form": "그래요 언니 날 찾지 마요ㅋㅋㅋㅋㅋ",
          "original_form": "그래요 언니 날 찾지 마요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:34"
        },
        {
          "id": "MDRW2100041546.1.4",
          "form": "지금도 나 혼자 땀에 쩔어 있음ㅋㅋㅋㅋㅋㅋ",
          "original_form": "지금도 나 혼자 땀에 쩔어 있음ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:34"
        },
        {
          "id": "MDRW2100041546.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㄱㄱ 어후 디다이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㄱㄱ 어후 디다이",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041546.1.6",
          "form": "알았다 슬프네 ㅠ",
          "original_form": "알았다 슬프네 ㅠ",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041546.1.7",
          "form": "날씨가 갑자기 추워져서 손님도 윽시 많네여ㅠ",
          "original_form": "날씨가 갑자기 추워져서 손님도 윽시 많네여ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041546.1.8",
          "form": "그래서 일이 진도가 안나 감;",
          "original_form": "그래서 일이 진도가 안나 감;",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041546.1.9",
          "form": "아 손님도 많구만",
          "original_form": "아 손님도 많구만",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041546.1.10",
          "form": "바쁘노",
          "original_form": "바쁘노",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041546.1.11",
          "form": "name2 언니 시험 끝나면",
          "original_form": "&name2& 언니 시험 끝나면",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041546.1.12",
          "form": "목요일 날 날 날 잡아서 하루 열심히 놀자",
          "original_form": "목요일 날 날 날 잡아서 하루 열심히 놀자",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041546.1.13",
          "form": "그래요 그래요ㅋㅋㅋㅋㅋ",
          "original_form": "그래요 그래요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041546.1.14",
          "form": "하루 알차게 놉시다ㅋㅋㅋ",
          "original_form": "하루 알차게 놉시다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041546.1.15",
          "form": "지금 깔딱고개ㅋㅋㅋ",
          "original_form": "지금 깔딱고개ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041546.1.16",
          "form": "ㅋㅋㅋㅋㅋ 열심히 하자 좀",
          "original_form": "ㅋㅋㅋㅋㅋ 열심히 하자 좀",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041546.1.17",
          "form": "닌 주말에도 말이 없고 이사람아",
          "original_form": "닌 주말에도 말이 없고 이사람아",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041546.1.18",
          "form": "어 언니 나 주말에도 진짜 힘들었어요ㅋㅋㅋㅋ",
          "original_form": "어 언니 나 주말에도 진짜 힘들었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041546.1.19",
          "form": "첫째 옷하고 다 작아져서",
          "original_form": "첫째 옷하고 다 작아져서",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041546.1.20",
          "form": "산다고 얼마나 돌아다녔는지ㅠ",
          "original_form": "산다고 얼마나 돌아다녔는지ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041546.1.21",
          "form": "아 쇼핑하러 돌아다녔나?",
          "original_form": "아 쇼핑하러 돌아다녔나?",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041546.1.22",
          "form": "그래 쇼핑은 힘들지",
          "original_form": "그래 쇼핑은 힘들지",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041546.1.23",
          "form": "나도 어제 여름옷 원꺼 세일 하길래",
          "original_form": "나도 어제 여름옷 원꺼 세일 하길래",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
