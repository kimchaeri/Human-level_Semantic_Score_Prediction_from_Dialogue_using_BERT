      "utterance": [
        {
          "id": "MDRW2100036785.1.1",
          "form": "롯데리아도 맛있더라구요",
          "original_form": "롯데리아도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100036785.1.2",
          "form": "비싼거는 맛있었어요 상추도 쌩쌩하구요",
          "original_form": "비싼거는 맛있었어요 상추도 쌩쌩하구요",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100036785.1.3",
          "form": "롯데리아도 좋죠 가까이에 없어서",
          "original_form": "롯데리아도 좋죠 가까이에 없어서",
          "speaker_id": "2",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100036785.1.4",
          "form": "잘안가게되긴 해도  괜찮았어요",
          "original_form": "잘안가게되긴 해도  괜찮았어요",
          "speaker_id": "2",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100036785.1.5",
          "form": "온라인에선 맥날이 좋다하는데 그 여론이랑 좀 다르더라구요",
          "original_form": "온라인에선 맥날이 좋다하는데 그 여론이랑 좀 다르더라구요",
          "speaker_id": "1",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100036785.1.6",
          "form": "그죠 뭐 예전에 롯데리아가 한참 맛없던 때가있어서",
          "original_form": "그죠 뭐 예전에 롯데리아가 한참 맛없던 때가있어서",
          "speaker_id": "2",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100036785.1.7",
          "form": "그 이미지가 오래가나 싶어요",
          "original_form": "그 이미지가 오래가나 싶어요",
          "speaker_id": "2",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100036785.1.8",
          "form": "그런가봐요 롯데리아 = 일본 이 프레임도 있구요",
          "original_form": "그런가봐요 롯데리아 = 일본 이 프레임도 있구요",
          "speaker_id": "1",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100036785.1.9",
          "form": "아 맞네요 그런 인식도 있겠다",
          "original_form": "아 맞네요 그런 인식도 있겠다",
          "speaker_id": "2",
          "time": "20210831 12:19"
        },
        {
          "id": "MDRW2100036785.1.10",
          "form": "맥날에선 어떤거 주로드세요?",
          "original_form": "맥날에선 어떤거 주로드세요?",
          "speaker_id": "2",
          "time": "20210831 12:19"
        },
        {
          "id": "MDRW2100036785.1.11",
          "form": "요즘엔 슈비버거 좋아해요 ㅎㅎ",
          "original_form": "요즘엔 슈비버거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:19"
        },
        {
          "id": "MDRW2100036785.1.12",
          "form": "슈비버거맛있죠 요즘은 양이조금 준거 같지만",
          "original_form": "슈비버거맛있죠 요즘은 양이조금 준거 같지만",
          "speaker_id": "2",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036785.1.13",
          "form": "그래도 든든하구요",
          "original_form": "그래도 든든하구요",
          "speaker_id": "2",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036785.1.14",
          "form": "저도제일좋아해요",
          "original_form": "저도제일좋아해요",
          "speaker_id": "2",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036785.1.15",
          "form": "어머 반가워요 소고기랑 새우랑 같이있어서 맛있었어요",
          "original_form": "어머 반가워요 소고기랑 새우랑 같이있어서 맛있었어요",
          "speaker_id": "1",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036785.1.16",
          "form": "그죠 소스도 어울리게 맛있구요",
          "original_form": "그죠 소스도 어울리게 맛있구요",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100036785.1.17",
          "form": "네네 저도 그 소스맛있더라구요",
          "original_form": "네네 저도 그 소스맛있더라구요",
          "speaker_id": "1",
          "time": "20210831 12:23"
        },
        {
          "id": "MDRW2100036785.1.18",
          "form": "요즘도 감튀안나오나요?",
          "original_form": "요즘도 감튀안나오나요?",
          "speaker_id": "2",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100036785.1.19",
          "form": "흠 2주전 주문하는 기계에 써있긴 하더라구요 근데 저희는 안주는게 아니고 맥너겟으로 바꿔준다 정도열어요",
          "original_form": "흠 2주전 주문하는 기계에 써있긴 하더라구요 근데 저희는 안주는게 아니고 맥너겟으로 바꿔준다 정도열어요",
