      "utterance": [
        {
          "id": "MDRW2100040273.1.1",
          "form": "헉 언니 졸업시험족보 이후로",
          "original_form": "헉 언니 졸업시험족보 이후로",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040273.1.2",
          "form": "채팅 못봤어요",
          "original_form": "채팅 못봤어요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040273.1.3",
          "form": "앜ㅋㅋㅋㅋ 네네",
          "original_form": "앜ㅋㅋㅋㅋ 네네",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040273.1.4",
          "form": "아직 그렇게 친하진 않지만",
          "original_form": "아직 그렇게 친하진 않지만",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040273.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040273.1.6",
          "form": "더 친해질 예정이라고 썼었어요!!!",
          "original_form": "더 친해질 예정이라고 썼었어요!!!",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040273.1.7",
          "form": "좋은분들이네요",
          "original_form": "좋은분들이네요",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040273.1.8",
          "form": "쟝교수님네 원생분들",
          "original_form": "쟝교수님네 원생분들",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040273.1.9",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040273.1.10",
          "form": "맞아요,, 헤헤 진짜 대학원 들어오니까",
          "original_form": "맞아요,, 헤헤 진짜 대학원 들어오니까",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040273.1.11",
          "form": "다른 연구실에 친구 있는게",
          "original_form": "다른 연구실에 친구 있는게",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040273.1.12",
          "form": "진짜 좋은거더라구요ㅠ",
          "original_form": "진짜 좋은거더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040273.1.13",
          "form": "저도 그러고싶어요!!",
          "original_form": "저도 그러고싶어요!!",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040273.1.14",
          "form": "쟝교수님랩 원생분들은",
          "original_form": "쟝교수님랩 원생분들은",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040273.1.15",
          "form": "어떻게 알게된건가요?",
          "original_form": "어떻게 알게된건가요?",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040273.1.16",
          "form": "이게 좀 복잡한데",
          "original_form": "이게 좀 복잡한데",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040273.1.17",
          "form": "제가 지난 학기에 조교장도 아닌데 안전점검을 맡았거든요",
          "original_form": "제가 지난 학기에 조교장도 아닌데 안전점검을 맡았거든요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040273.1.18",
          "form": "이게 쟝 교수님이 실수로 저를 안전관리담당자로 지정해놓으셔서 ㅎ....",
          "original_form": "이게 쟝 교수님이 실수로 저를 안전관리담당자로 지정해놓으셔서 ㅎ....",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040273.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040273.1.20",
          "form": "저는 돈도 못받고",
          "original_form": "저는 돈도 못받고",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040273.1.21",
          "form": "원래 조교장이 그런거 하라고 30 만원 더 받는건데",
          "original_form": "원래 조교장이 그런거 하라고 30 만원 더 받는건데",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040273.1.22",
          "form": "헐 안돼!!!",
          "original_form": "헐 안돼!!!",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040273.1.23",
          "form": "저는 그냥 남 좋은 일 한거죠",
          "original_form": "저는 그냥 남 좋은 일 한거죠",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040273.1.24",
          "form": "ㅋㅋㅋㅋ 그래서 저 진짜 화났었어요",
          "original_form": "ㅋㅋㅋㅋ 그래서 저 진짜 화났었어요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040273.1.25",
          "form": "저희 조교장한테 말을 해봐도",
          "original_form": "저희 조교장한테 말을 해봐도",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040273.1.26",
          "form": "대답이 없고",
          "original_form": "대답이 없고",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040273.1.27",
          "form": "그냥 신경쓰지말라고만 하더라구요 ㅎ",
          "original_form": "그냥 신경쓰지말라고만 하더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040273.1.28",
          "form": "하튼 그래서 그게 항상 불만이었는데.. 그 쟝교수님 랩실 원생한테 혹시 이거 어떻게 된건지 아냐고 물어보니까 그냥 쟝 교수님 실수인거같다고....",
          "original_form": "하튼 그래서 그게 항상 불만이었는데.. 그 쟝교수님 랩실 원생한테 혹시 이거 어떻게 된건지 아냐고 물어보니까 그냥 쟝 교수님 실수인거같다고....",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040273.1.29",
          "form": "그래서 같이 밥먹는 계기가 생긴건가요???",
          "original_form": "그래서 같이 밥먹는 계기가 생긴건가요???",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100040273.1.30",
          "form": "네네 사실 처음에 그 원생분이 저를 찾으셨거든요",
          "original_form": "네네 사실 처음에 그 원생분이 저를 찾으셨거든요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040273.1.31",
          "form": "지난학기 안전관리담당자한테 물어볼게 있어서",
          "original_form": "지난학기 안전관리담당자한테 물어볼게 있어서",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040273.1.32",
          "form": "저를 찾은건데",
          "original_form": "저를 찾은건데",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040273.1.33",
          "form": "연락 닿아서 이야기하다가",
          "original_form": "연락 닿아서 이야기하다가",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040273.1.34",
          "form": "밥 한 번 먹자 이야기 나왔는데 바로 다음날 약속 잡았어욬ㅋㅋㅋ 저랑 원래 친한 김현우 교수님 랩실 원생이랑 같이요!",
          "original_form": "밥 한 번 먹자 이야기 나왔는데 바로 다음날 약속 잡았어욬ㅋㅋㅋ 저랑 원래 친한 김현우 교수님 랩실 원생이랑 같이요!",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040273.1.35",
          "form": "오오오 짱이에오ㆍ",
          "original_form": "오오오 짱이에오ㆍ",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100040273.1.36",
          "form": "우연한 기회에 좋은 인연생겼네요",
          "original_form": "우연한 기회에 좋은 인연생겼네요",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100040273.1.37",
          "form": "최고에요!!!",
          "original_form": "최고에요!!!",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100040273.1.38",
          "form": "맞아요 헤헤",
          "original_form": "맞아요 헤헤",
          "speaker_id": "2",
          "time": "20210904 19:27"
        },
        {
          "id": "MDRW2100040273.1.39",
          "form": "가연씨도 대학원을 가든 회사를 가든",
          "original_form": "가연씨도 대학원을 가든 회사를 가든",
          "speaker_id": "2",
          "time": "20210904 19:27"
        },
        {
          "id": "MDRW2100040273.1.40",
          "form": "친구 많이 사귀게 되실거예요",
          "original_form": "친구 많이 사귀게 되실거예요",
          "speaker_id": "2",
          "time": "20210904 19:28"
        }
      ]
