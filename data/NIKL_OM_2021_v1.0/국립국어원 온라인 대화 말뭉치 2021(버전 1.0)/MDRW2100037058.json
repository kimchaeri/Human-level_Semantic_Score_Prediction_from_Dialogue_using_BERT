      "utterance": [
        {
          "id": "MDRW2100037058.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100037058.1.2",
          "form": "안녕하세요! 지금 저녁 먹고 있어요~ 식사하셨나요??",
          "original_form": "안녕하세요! 지금 저녁 먹고 있어요~ 식사하셨나요??",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100037058.1.3",
          "form": "저도 좀 전에 먹었어요",
          "original_form": "저도 좀 전에 먹었어요",
          "speaker_id": "1",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100037058.1.4",
          "form": "뭐 드시고 계신가요?",
          "original_form": "뭐 드시고 계신가요?",
          "speaker_id": "1",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100037058.1.5",
          "form": "그냥 집에 있던 반찬이랑 먹고 있어요 ㅋㅋ",
          "original_form": "그냥 집에 있던 반찬이랑 먹고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100037058.1.6",
          "form": "김이랑 동그랑땡이랑",
          "original_form": "김이랑 동그랑땡이랑",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100037058.1.7",
          "form": "동그랑땡 맛있죠! 오랜만에 들으니까 저도 먹고 싶네요ㅎㅎ",
          "original_form": "동그랑땡 맛있죠! 오랜만에 들으니까 저도 먹고 싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100037058.1.8",
          "form": "직접 만드신 건가요?",
          "original_form": "직접 만드신 건가요?",
          "speaker_id": "1",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100037058.1.9",
          "form": "아뇨 ㅋㅋ 마트에서 산거에요",
          "original_form": "아뇨 ㅋㅋ 마트에서 산거에요",
          "speaker_id": "2",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100037058.1.10",
          "form": "아하 동그랑땡 저도 명절에나 해서 먹어요ㅋㅋㅋ",
          "original_form": "아하 동그랑땡 저도 명절에나 해서 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100037058.1.11",
          "form": "사먹는게 편하고 좋아요ㅎㅎ",
          "original_form": "사먹는게 편하고 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100037058.1.12",
          "form": "맞아요 요리는 남이 해준 게 제일 맛있어요 ㅋㅋ",
          "original_form": "맞아요 요리는 남이 해준 게 제일 맛있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100037058.1.13",
          "form": "저는 오늘 소고기 미역국이랑 먹었어요",
          "original_form": "저는 오늘 소고기 미역국이랑 먹었어요",
          "speaker_id": "1",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100037058.1.14",
          "form": "미역국 좋아하시나요?",
          "original_form": "미역국 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100037058.1.15",
          "form": "오 미역국 좋아해요 특히 소고기 넣은 거 진짜 좋아합니다.",
          "original_form": "오 미역국 좋아해요 특히 소고기 넣은 거 진짜 좋아합니다.",
          "speaker_id": "2",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100037058.1.16",
          "form": "소고기 미역국이랑 김치 밥 이렇게만 있어도 두그릇은 먹을 수 있을 것 같아요",
          "original_form": "소고기 미역국이랑 김치 밥 이렇게만 있어도 두그릇은 먹을 수 있을 것 같아요",
          "speaker_id": "2",
          "time": "20210831 18:42"
        },
        {
          "id": "MDRW2100037058.1.17",
          "form": "저도 홍합보다 소고기 미역국 더 좋아해요ㅎㅎ",
          "original_form": "저도 홍합보다 소고기 미역국 더 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:42"
        },
        {
          "id": "MDRW2100037058.1.18",
          "form": "완전 맛있죠~ 하얀 쌀밥이면 더 맛있어요ㅎㅎ",
          "original_form": "완전 맛있죠~ 하얀 쌀밥이면 더 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100037058.1.19",
          "form": "맞아요!! 하지만 잡곡밥이어도 좋아요 ㅋㄱㄱ",
          "original_form": "맞아요!! 하지만 잡곡밥이어도 좋아요 ㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100037058.1.20",
          "form": "요즘에 현미밥을 해먹는데",
          "original_form": "요즘에 현미밥을 해먹는데",
          "speaker_id": "2",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100037058.1.21",
          "form": "포만감이 오래가는 것 같아서 좋더라구요",
          "original_form": "포만감이 오래가는 것 같아서 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100037058.1.22",
          "form": "맞아요 확실히 포만감이 더 오래가는 거 같아요",
          "original_form": "맞아요 확실히 포만감이 더 오래가는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 18:45"
        }
      ]
    }
