      "utterance": [
        {
          "id": "MDRW2100003212.1.1",
          "form": "name2님이 제일 좋아하는 맛집은 어떤 곳인지 궁금해요!",
          "original_form": "&name2&님이 제일 좋아하는 맛집은 어떤 곳인지 궁금해요!",
          "speaker_id": "1",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003212.1.2",
          "form": "저는 짬뽕을 잘하는 중국집을 좋아해요!",
          "original_form": "저는 짬뽕을 잘하는 중국집을 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003212.1.3",
          "form": "좋아하시는 맛집이 있나요?",
          "original_form": "좋아하시는 맛집이 있나요?",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003212.1.4",
          "form": "저는 딱 제일 좋아하는 맛집은 없는 거 같아요",
          "original_form": "저는 딱 제일 좋아하는 맛집은 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003212.1.5",
          "form": "예전에 되게 좋아하던 치킨집이 있었는데 거기가 없어졌어요ㅠㅠ",
          "original_form": "예전에 되게 좋아하던 치킨집이 있었는데 거기가 없어졌어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003212.1.6",
          "form": "아 그러면 아쉽죠",
          "original_form": "아 그러면 아쉽죠",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003212.1.7",
          "form": "뭔가 추억도 같이 사라지는 것 같잖아요",
          "original_form": "뭔가 추억도 같이 사라지는 것 같잖아요",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003212.1.8",
          "form": "그쵸 진짜 열심히 갔었거든요",
          "original_form": "그쵸 진짜 열심히 갔었거든요",
          "speaker_id": "1",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003212.1.9",
          "form": "맛있었는데 너무 아쉬워요",
          "original_form": "맛있었는데 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003212.1.10",
          "form": "저도 좋아하는 맥주집 있는데",
          "original_form": "저도 좋아하는 맥주집 있는데",
          "speaker_id": "2",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003212.1.11",
          "form": "갈때마다 손님이 없어서 항상 망하지말라고 기도해요ㅎㅎ",
          "original_form": "갈때마다 손님이 없어서 항상 망하지말라고 기도해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003212.1.12",
          "form": "헉 맞아요 왜 이런 곳을 몰라주지 한다니까요",
          "original_form": "헉 맞아요 왜 이런 곳을 몰라주지 한다니까요",
          "speaker_id": "1",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003212.1.13",
          "form": "근데 소문은 넘 많이는 안났으면 좋겠구ㅋㅋㅋ",
          "original_form": "근데 소문은 넘 많이는 안났으면 좋겠구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003212.1.14",
          "form": "거긴 무슨 안주가 맛있어요?",
          "original_form": "거긴 무슨 안주가 맛있어요?",
          "speaker_id": "1",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003212.1.15",
          "form": "맥주집은 감튀죠!!",
          "original_form": "맥주집은 감튀죠!!",
          "speaker_id": "2",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100003212.1.16",
          "form": "소스가 많아서 좋아요",
          "original_form": "소스가 많아서 좋아요",
          "speaker_id": "2",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100003212.1.17",
          "form": "저도 감튀 좋아해요!",
          "original_form": "저도 감튀 좋아해요!",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003212.1.18",
          "form": "나쵸도 좋아요",
          "original_form": "나쵸도 좋아요",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003212.1.19",
          "form": "나쵸 맛있죠 바삭바삭",
          "original_form": "나쵸 맛있죠 바삭바삭",
          "speaker_id": "2",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003212.1.20",
          "form": "맥주는 좋아하는 종류 있으세요?",
          "original_form": "맥주는 좋아하는 종류 있으세요?",
          "speaker_id": "2",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003212.1.21",
          "form": "저 맥주는 다 좋아해요",
          "original_form": "저 맥주는 다 좋아해요",
          "speaker_id": "1",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003212.1.22",
          "form": "얼음생맥 사랑합니다..",
          "original_form": "얼음생맥 사랑합니다..",
          "speaker_id": "1",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003212.1.23",
          "form": "캔으로 된 건 데스페라도스 좋아해요",
          "original_form": "캔으로 된 건 데스페라도스 좋아해요",
          "speaker_id": "1",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003212.1.24",
          "form": "와 저도 데스페라도스 좋아해요!!",
          "original_form": "와 저도 데스페라도스 좋아해요!!",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003212.1.25",
          "form": "맥주중에 도수가 높은편이라 맛있더라구요",
          "original_form": "맥주중에 도수가 높은편이라 맛있더라구요",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003212.1.26",
          "form": "오 맞아요 그리고 감튀랑 너무 잘어울려요",
          "original_form": "오 맞아요 그리고 감튀랑 너무 잘어울려요",
          "speaker_id": "1",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003212.1.27",
          "form": "요새 곰표맥주도 맛있더라구요",
          "original_form": "요새 곰표맥주도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003212.1.28",
          "form": "아 그거 아직 못먹어봤어요",
          "original_form": "아 그거 아직 못먹어봤어요",
