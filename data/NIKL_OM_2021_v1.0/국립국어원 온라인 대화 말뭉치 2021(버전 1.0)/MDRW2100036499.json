      "utterance": [
        {
          "id": "MDRW2100036499.1.1",
          "form": "음 아이허브면 영양제 파는 곳이지?",
          "original_form": "음 아이허브면 영양제 파는 곳이지?",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036499.1.2",
          "form": "그럼 영양제??",
          "original_form": "그럼 영양제??",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036499.1.3",
          "form": "내가 말한 적이 있던가?",
          "original_form": "내가 말한 적이 있던가?",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036499.1.4",
          "form": "음 거의 맞아",
          "original_form": "음 거의 맞아",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036499.1.5",
          "form": "아니 처음 듣는 이야기야",
          "original_form": "아니 처음 듣는 이야기야",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036499.1.6",
          "form": "에너지바를 샀어",
          "original_form": "에너지바를 샀어",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.7",
          "form": "에너지바 먹어본 적 있어?",
          "original_form": "에너지바 먹어본 적 있어?",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.8",
          "form": "아? 그것도 영양제인가?",
          "original_form": "아? 그것도 영양제인가?",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.9",
          "form": "나는 거의 닥터유 제품만 먹었어",
          "original_form": "나는 거의 닥터유 제품만 먹었어",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.10",
          "form": "아니 식사대용으로 먹을거야",
          "original_form": "아니 식사대용으로 먹을거야",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.11",
          "form": "거기 간식종류도 많이 팔아",
          "original_form": "거기 간식종류도 많이 팔아",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.12",
          "form": "음 나도 가끔 밥대신 먹은적있어",
          "original_form": "음 나도 가끔 밥대신 먹은적있어",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.13",
          "form": "감자칩 이런것도 팔고... 다 팔아",
          "original_form": "감자칩 이런것도 팔고... 다 팔아",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.14",
          "form": "밥먹기 귀찮을때",
          "original_form": "밥먹기 귀찮을때",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.15",
          "form": "나도 아침에 먹으려고 샀어",
          "original_form": "나도 아침에 먹으려고 샀어",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.16",
          "form": "세일하더라",
          "original_form": "세일하더라",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.17",
          "form": "갑자칩더?? 세상에 올리브영같다",
          "original_form": "갑자칩더?? 세상에 올리브영같다",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.18",
          "form": "12개에 7달러엮어",
          "original_form": "12개에 7달러엮어",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.19",
          "form": "근데 미국직구라 엄청 짜대",
          "original_form": "근데 미국직구라 엄청 짜대",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.20",
          "form": "흠 그럼 9천원 정도인가?",
          "original_form": "흠 그럼 9천원 정도인가?",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.21",
          "form": "케틀칩 살까말까 고밍했어",
          "original_form": "케틀칩 살까말까 고밍했어",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.22",
          "form": "짠거 좋아",
          "original_form": "짠거 좋아",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.23",
          "form": "케틀칩은 뭐니??",
          "original_form": "케틀칩은 뭐니??",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.24",
          "form": "응 근데 또 할인응 해주더라",
          "original_form": "응 근데 또 할인응 해주더라",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036499.1.25",
          "form": "감자칩이야",
          "original_form": "감자칩이야",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036499.1.26",
          "form": "프링글스같은 칩 이룸이야",
          "original_form": "프링글스같은 칩 이룸이야",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036499.1.27",
          "form": "나 최근에 우산 잃어버렸다고 했잖아",
          "original_form": "나 최근에 우산 잃어버렸다고 했잖아",
          "speaker_id": "2",
          "time": "20210830 23:52"
        }
      ]
    }
