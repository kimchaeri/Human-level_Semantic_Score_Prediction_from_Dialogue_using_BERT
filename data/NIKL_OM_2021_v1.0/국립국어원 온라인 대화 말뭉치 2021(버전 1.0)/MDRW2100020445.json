      "utterance": [
        {
          "id": "MDRW2100020445.1.1",
          "form": "일단 복장 갖춰서 일해야하는데",
          "original_form": "일단 복장 갖춰서 일해야하는데",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.2",
          "form": "복장도 너무 불편하고",
          "original_form": "복장도 너무 불편하고",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.3",
          "form": "옷공장이 힘들었다고 하실줄 ㅋㅋㅋ",
          "original_form": "옷공장이 힘들었다고 하실줄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.4",
          "form": "특히 굽있는 구두신고 종일",
          "original_form": "특히 굽있는 구두신고 종일",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.5",
          "form": "아 복장!",
          "original_form": "아 복장!",
          "speaker_id": "2",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.6",
          "form": "서있으니까 다리아프고 허리아프고",
          "original_form": "서있으니까 다리아프고 허리아프고",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.7",
          "form": "아 구두신고 일해야하는구나",
          "original_form": "아 구두신고 일해야하는구나",
          "speaker_id": "2",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.8",
          "form": "그리고 갑질 엄청 심해요",
          "original_form": "그리고 갑질 엄청 심해요",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.9",
          "form": "너무하네요 ㅜㅜ",
          "original_form": "너무하네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.10",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.11",
          "form": "머리망해야해서 머리는 삔때문에 찌르고",
          "original_form": "머리망해야해서 머리는 삔때문에 찌르고",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.12",
          "form": "저도 구두는 하루종일 신었지만 거의 앉아있어서",
          "original_form": "저도 구두는 하루종일 신었지만 거의 앉아있어서",
          "speaker_id": "2",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.13",
          "form": "아휴 난리도 아니었어요ㅋㅋㅋ",
          "original_form": "아휴 난리도 아니었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.15",
          "form": "넘 부러워요ㅠㅠ",
          "original_form": "넘 부러워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:01"
        },
        {
          "id": "MDRW2100020445.1.16",
          "form": "맞아 서비스직은 그런 보여지는 게 정말",
          "original_form": "맞아 서비스직은 그런 보여지는 게 정말",
          "speaker_id": "2",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020445.1.17",
          "form": "그래도 제일 오래한건 편의점 알바 제일 오래했어요",
          "original_form": "그래도 제일 오래한건 편의점 알바 제일 오래했어요",
          "speaker_id": "1",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020445.1.18",
          "form": "손님 많은 곳에서 했나요",
          "original_form": "손님 많은 곳에서 했나요",
          "speaker_id": "2",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020445.1.19",
          "form": "주택가여서 없을 줄 알았는데.. 많았어요",
          "original_form": "주택가여서 없을 줄 알았는데.. 많았어요",
          "speaker_id": "1",
          "time": "20210828 06:02"
        }
      ]
