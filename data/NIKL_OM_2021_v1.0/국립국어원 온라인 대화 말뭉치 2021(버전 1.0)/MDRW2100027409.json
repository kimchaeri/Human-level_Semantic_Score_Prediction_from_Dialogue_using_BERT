      "utterance": [
        {
          "id": "MDRW2100027409.1.1",
          "form": "도시락싸와서 만두랑 볶음밥이요! ㅋㅋ",
          "original_form": "도시락싸와서 만두랑 볶음밥이요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027409.1.2",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027409.1.3",
          "form": "저는 찜닭에 콩나물국 먹었어요!",
          "original_form": "저는 찜닭에 콩나물국 먹었어요!",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027409.1.4",
          "form": "매콤하니 맛있더라고요",
          "original_form": "매콤하니 맛있더라고요",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027409.1.5",
          "form": "아 완전 맛난 조합이네요!",
          "original_form": "아 완전 맛난 조합이네요!",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027409.1.6",
          "form": "저도 뜨끈한 국물 먹고싶네요",
          "original_form": "저도 뜨끈한 국물 먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027409.1.7",
          "form": "그쵸 ㅎ오늘 메뉴 맘에들어요",
          "original_form": "그쵸 ㅎ오늘 메뉴 맘에들어요",
          "speaker_id": "1",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027409.1.8",
          "form": "아ㅠ흑 라면국물이라도",
          "original_form": "아ㅠ흑 라면국물이라도",
          "speaker_id": "1",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027409.1.9",
          "form": "두번 리필할 메뉴예요!!!",
          "original_form": "두번 리필할 메뉴예요!!!",
          "speaker_id": "2",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027409.1.10",
          "form": "얼큰 뜨끈!!!",
          "original_form": "얼큰 뜨끈!!!",
          "speaker_id": "2",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027409.1.11",
          "form": "ㅋㅋ하지만 참았어요",
          "original_form": "ㅋㅋ하지만 참았어요",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027409.1.12",
          "form": "소화안될까봐",
          "original_form": "소화안될까봐",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027409.1.13",
          "form": "이따 커피한잔 똭 마실까해요",
          "original_form": "이따 커피한잔 똭 마실까해요",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027409.1.14",
          "form": "아 아아드시나요??",
          "original_form": "아 아아드시나요??",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027409.1.15",
          "form": "네~ 카누 타먹었어요 ㅎ",
          "original_form": "네~ 카누 타먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027409.1.16",
          "form": "줌 교육이라 과자도 먹고있어요",
          "original_form": "줌 교육이라 과자도 먹고있어요",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027409.1.17",
          "form": "뽀또",
          "original_form": "뽀또",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027409.1.18",
          "form": "ㅋㅋ전 아까 마카롱에 디카페인카누 먹었어요",
          "original_form": "ㅋㅋ전 아까 마카롱에 디카페인카누 먹었어요",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027409.1.19",
          "form": "소다맛마카롱 좀 구리네요ㅜ 겉도는 맛",
          "original_form": "소다맛마카롱 좀 구리네요ㅜ 겉도는 맛",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027409.1.20",
          "form": "그래요? 소다는 맛있는데!ㅠ",
          "original_form": "그래요? 소다는 맛있는데!ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027409.1.21",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027409.1.22",
          "form": "전 흑당버블티 먹고있어요!!!!!!",
          "original_form": "전 흑당버블티 먹고있어요!!!!!!",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027409.1.23",
          "form": "아 어디꺼요???",
          "original_form": "아 어디꺼요???",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027409.1.24",
          "form": "음 이 지역에만 있는 카페라 모르실거에요ㅠ",
          "original_form": "음 이 지역에만 있는 카페라 모르실거에요ㅠ",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100027409.1.25",
          "form": "저녁드셨나요!",
          "original_form": "저녁드셨나요!",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100027409.1.26",
          "form": "네 옥수수랑 과일 먹었어요",
          "original_form": "네 옥수수랑 과일 먹었어요",
          "speaker_id": "2",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100027409.1.27",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100027409.1.28",
          "form": "오 간단히 드셨군요 !",
          "original_form": "오 간단히 드셨군요 !",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100027409.1.29",
          "form": "저는 밥먹고 만두먹고있어요",
          "original_form": "저는 밥먹고 만두먹고있어요",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100027409.1.30",
          "form": "밥먹고 만두는 따로 드시는거예요??? 비비고요?",
          "original_form": "밥먹고 만두는 따로 드시는거예요??? 비비고요?",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100027409.1.31",
          "form": "네네 ㅎ 비비고 만두 따로 먹었어요!",
          "original_form": "네네 ㅎ 비비고 만두 따로 먹었어요!",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100027409.1.32",
          "form": "맛있네요 올만에 먹으니깐요",
          "original_form": "맛있네요 올만에 먹으니깐요",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100027409.1.33",
          "form": "4개만 먹었어요",
          "original_form": "4개만 먹었어요",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100027409.1.34",
          "form": "ㅋㅋ넘 자주 먹음 물리고 간만에 먹음 환상!",
          "original_form": "ㅋㅋ넘 자주 먹음 물리고 간만에 먹음 환상!",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100027409.1.35",
