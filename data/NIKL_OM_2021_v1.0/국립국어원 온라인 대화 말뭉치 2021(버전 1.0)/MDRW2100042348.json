          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.2",
          "form": "넌 운동하고나서",
          "original_form": "넌 운동하고나서",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.3",
          "form": "종아리 어떻게풀어??",
          "original_form": "종아리 어떻게풀어??",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.4",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.5",
          "form": "종아리?",
          "original_form": "종아리?",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.6",
          "form": "폼롤러해?",
          "original_form": "폼롤러해?",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.8",
          "form": "난 운동하고나면 유독 종아리가 뭉치고 아픔",
          "original_form": "난 운동하고나면 유독 종아리가 뭉치고 아픔",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.9",
          "form": "자세가 이상해서글ㄴ가",
          "original_form": "자세가 이상해서글ㄴ가",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042348.1.10",
          "form": "어떤 운동하고??",
          "original_form": "어떤 운동하고??",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042348.1.11",
          "form": "걍 홈트할때도 그렇고",
          "original_form": "걍 홈트할때도 그렇고",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042348.1.12",
          "form": "오늘 런데이 30분뛰고왔는데",
          "original_form": "오늘 런데이 30분뛰고왔는데",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042348.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042348.1.14",
          "form": "오 굿굿",
          "original_form": "오 굿굿",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042348.1.15",
          "form": "종아리가 너무아파서ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "종아리가 너무아파서ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042348.1.16",
          "form": "그거 보통",
          "original_form": "그거 보통",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042348.1.17",
          "form": "운동전에 다리 잘 안풀고 하면 그렇다던데",
          "original_form": "운동전에 다리 잘 안풀고 하면 그렇다던데",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042348.1.18",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042348.1.19",
          "form": "대충 야매로 스트레칭하고 가긴햇는데",
          "original_form": "대충 야매로 스트레칭하고 가긴햇는데",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042348.1.20",
          "form": "운동전에도 스타레칭 했는데 그래?",
          "original_form": "운동전에도 스타레칭 했는데 그래?",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042348.1.21",
          "form": "스트레칭 뭐 했어??",
          "original_form": "스트레칭 뭐 했어??",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042348.1.22",
          "form": "응...ㅋㅋㅋㅋㅋ",
          "original_form": "응...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042348.1.23",
          "form": "학교 체육시간때 하는거처럼",
          "original_form": "학교 체육시간때 하는거처럼",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042348.1.24",
          "form": "/ㅣ 이자세로",
          "original_form": "/ㅣ 이자세로",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042348.1.25",
          "form": "다리 쭉쭉펴는??",
          "original_form": "다리 쭉쭉펴는??",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042348.1.26",
          "form": "그런거 함",
          "original_form": "그런거 함",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042348.1.27",
          "form": "아아 그거 하지 말고",
          "original_form": "아아 그거 하지 말고",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042348.1.28",
          "form": "계단 같은곳에",
          "original_form": "계단 같은곳에",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042348.1.29",
          "form": "발 앞쪽만 올려서",
          "original_form": "발 앞쪽만 올려서",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042348.1.30",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042348.1.31",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042348.1.32",
          "form": "좀 올라섰다 내렸다 하는거",
          "original_form": "좀 올라섰다 내렸다 하는거",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042348.1.33",
          "form": "일단 지금 폼롤러로 조지는중",
          "original_form": "일단 지금 폼롤러로 조지는중",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042348.1.34",
          "form": "그것도 좋지",
          "original_form": "그것도 좋지",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042348.1.35",
          "form": "폼롤러 진짜 너무 아프지 않아",
          "original_form": "폼롤러 진짜 너무 아프지 않아",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042348.1.36",
          "form": "ㅇㅇㄱ ㅐ아파ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㄱ ㅐ아파ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:50"
        }
      ]
