      "utterance": [
        {
          "id": "MDRW2100018524.1.1",
          "form": "펫우유 비싸요",
          "original_form": "펫우유 비싸요",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018524.1.2",
          "form": "저유당밀크 줍니다",
          "original_form": "저유당밀크 줍니다",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018524.1.3",
          "form": "지금 우유 먹고 와서 트름하네요?",
          "original_form": "지금 우유 먹고 와서 트름하네요?",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018524.1.4",
          "form": "우리 강아지 매너 어디 갔죠?",
          "original_form": "우리 강아지 매너 어디 갔죠?",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018524.1.5",
          "form": "아ㅋㅋㅋㅈ정말 개 하는 짓이 너무 웃겨요",
          "original_form": "아ㅋㅋㅋㅈ정말 개 하는 짓이 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018524.1.6",
          "form": "우리집 애는 베개에 입 닦아요",
          "original_form": "우리집 애는 베개에 입 닦아요",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018524.1.7",
          "form": "자기 혼자 깔끔함ㅎ",
          "original_form": "자기 혼자 깔끔함ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018524.1.8",
          "form": "아 맞아요 자기 혼자 깔끔",
          "original_form": "아 맞아요 자기 혼자 깔끔",
          "speaker_id": "2",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018524.1.9",
          "form": "목욕은 싫어하면서",
          "original_form": "목욕은 싫어하면서",
          "speaker_id": "2",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018524.1.10",
          "form": "목욕하기 싫어서 혼자서 닦겠다고 하는건지",
          "original_form": "목욕하기 싫어서 혼자서 닦겠다고 하는건지",
          "speaker_id": "2",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018524.1.11",
          "form": "ㅎㅎ 아니 뭐 이렇게 비슷한가요ㅎㅎ",
          "original_form": "ㅎㅎ 아니 뭐 이렇게 비슷한가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018524.1.12",
          "form": "그러면서 젖은 수건에 얼굴 부벼요",
          "original_form": "그러면서 젖은 수건에 얼굴 부벼요",
          "speaker_id": "1",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018524.1.13",
          "form": "좋아서 그런건지 싫어서 그러는지",
          "original_form": "좋아서 그런건지 싫어서 그러는지",
          "speaker_id": "1",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018524.1.14",
          "form": "그건 저희 냄새 자기 몸에 묻히는 거래요ㅎㅎ",
          "original_form": "그건 저희 냄새 자기 몸에 묻히는 거래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018524.1.15",
          "form": "그러면 샴푸냄새가 좋은 걸까요",
          "original_form": "그러면 샴푸냄새가 좋은 걸까요",
          "speaker_id": "1",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018524.1.16",
          "form": "전 세수하려는 줄ㅎ",
          "original_form": "전 세수하려는 줄ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018524.1.17",
          "form": "영역표시의 개념이요ㅎㅎ",
          "original_form": "영역표시의 개념이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018524.1.18",
          "form": "아니 이미 다 애들 영역이구만ㅎ",
          "original_form": "아니 이미 다 애들 영역이구만ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018524.1.19",
          "form": "지금 우리집 아기는 우유 엎고 튀었네요",
          "original_form": "지금 우리집 아기는 우유 엎고 튀었네요",
          "speaker_id": "1",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018524.1.20",
          "form": "우리가 자기 무리라서ㅋㅋ 냄새 비슷하게 만드려고ㅎㅎ",
          "original_form": "우리가 자기 무리라서ㅋㅋ 냄새 비슷하게 만드려고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018524.1.21",
          "form": "아 그집도 우유 먹는 중이군요",
          "original_form": "아 그집도 우유 먹는 중이군요",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018524.1.22",
          "form": "네 우리는 어려서부터 사람우유 먹였어요",
          "original_form": "네 우리는 어려서부터 사람우유 먹였어요",
          "speaker_id": "1",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018524.1.23",
          "form": "탈은 안 나나요?",
          "original_form": "탈은 안 나나요?",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018524.1.24",
          "form": "어려서부터 먹어서인지 몸에 잘 받아요",
          "original_form": "어려서부터 먹어서인지 몸에 잘 받아요",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018524.1.25",
          "form": "강아지가 못먹는 음식도 있나요",
          "original_form": "강아지가 못먹는 음식도 있나요",
          "speaker_id": "1",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018524.1.26",
          "form": "다행이네요ㅜㅜ 저희애들도 좋아하는데 탈이 안 나서 너무 좋더라구요",
          "original_form": "다행이네요ㅜㅜ 저희애들도 좋아하는데 탈이 안 나서 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 07:49"
        }
      ]
