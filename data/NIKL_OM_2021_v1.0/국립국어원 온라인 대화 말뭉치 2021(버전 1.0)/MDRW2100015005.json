        },
        {
          "id": "MDRW2100015005.1.2",
          "form": "나도!!!!! 우리언제간다냐...",
          "original_form": "나도!!!!! 우리언제간다냐...",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100015005.1.3",
          "form": "어디제일가고싶어!!!!",
          "original_form": "어디제일가고싶어!!!!",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100015005.1.4",
          "form": "나 경주랑 통영",
          "original_form": "나 경주랑 통영",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100015005.1.5",
          "form": "난 다녀온데 또 가는게 좋더라 ㅋ",
          "original_form": "난 다녀온데 또 가는게 좋더라 ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100015005.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.7",
          "form": "그거랑 별 쏟아지는 곳",
          "original_form": "그거랑 별 쏟아지는 곳",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.8",
          "form": "별쏟ㅇㆍ지는곳진짜이쁘더라",
          "original_form": "별쏟ㅇㆍ지는곳진짜이쁘더라",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.9",
          "form": "내가치는것들자꾸나쁜말이래",
          "original_form": "내가치는것들자꾸나쁜말이래",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.10",
          "form": "합천가야하나",
          "original_form": "합천가야하나",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.11",
          "form": "안반데기 너무 멀어",
          "original_form": "안반데기 너무 멀어",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.12",
          "form": "너 대체 뭐라고 하는거니 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너 대체 뭐라고 하는거니 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㄲㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄲㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.14",
          "form": "띄어쓰기를 너무 안했나..흑흑",
          "original_form": "띄어쓰기를 너무 안했나..흑흑",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015005.1.15",
          "form": "서울언제가징으흑",
          "original_form": "서울언제가징으흑",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015005.1.16",
          "form": "그런가보다",
          "original_form": "그런가보다",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015005.1.17",
          "form": "서울왜",
          "original_form": "서울왜",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015005.1.18",
          "form": "대만은또언제가본담",
          "original_form": "대만은또언제가본담",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015005.1.19",
          "form": "콘서트?",
          "original_form": "콘서트?",
