        {
          "id": "MDRW2100035915.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035915.1.2",
          "form": "이제 곧 하려구요!",
          "original_form": "이제 곧 하려구요!",
          "speaker_id": "2",
          "time": "20210830 11:24"
        },
        {
          "id": "MDRW2100035915.1.3",
          "form": "뭐먹을지 고밍중입니다",
          "original_form": "뭐먹을지 고밍중입니다",
          "speaker_id": "2",
          "time": "20210830 11:24"
        },
        {
          "id": "MDRW2100035915.1.4",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210830 11:24"
        },
        {
          "id": "MDRW2100035915.1.5",
          "form": "저는 아침으로 든든하게 전복죽 먹었답니당 ㅎㅎ",
          "original_form": "저는 아침으로 든든하게 전복죽 먹었답니당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035915.1.6",
          "form": "헉 보양식을 드셨네요",
          "original_form": "헉 보양식을 드셨네요",
          "speaker_id": "2",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035915.1.7",
          "form": "어디 몸 안좋으신가요 죽이라니ㅜㅜ",
          "original_form": "어디 몸 안좋으신가요 죽이라니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 11:27"
        },
        {
          "id": "MDRW2100035915.1.8",
          "form": "앜ㅋㅋㅋㅋㅋ 안타깝게도(?) 멀쩡합니당ㅋㅋㅋㅋ 그냥 아침으로 종종 죽 먹어주고있어요 든든하니좋그든요~",
          "original_form": "앜ㅋㅋㅋㅋㅋ 안타깝게도(?) 멀쩡합니당ㅋㅋㅋㅋ 그냥 아침으로 종종 죽 먹어주고있어요 든든하니좋그든요~",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035915.1.9",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035915.1.10",
          "form": "좋은방법이네요 그것두",
          "original_form": "좋은방법이네요 그것두",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035915.1.11",
          "form": "맞아요~ㅎㅎ",
          "original_form": "맞아요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035915.1.12",
          "form": "혹시 배라좋아하시나요?",
          "original_form": "혹시 배라좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035915.1.13",
          "form": "약간 선물로 자주받아가지구",
          "original_form": "약간 선물로 자주받아가지구",
          "speaker_id": "2",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100035915.1.14",
          "form": "받으면 먹어요!",
          "original_form": "받으면 먹어요!",
          "speaker_id": "2",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100035915.1.15",
          "form": "오! 3픽 어떻게되시나요?",
          "original_form": "오! 3픽 어떻게되시나요?",
          "speaker_id": "1",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100035915.1.16",
          "form": "전 레샤 이상한나라의솜사탕 팝핑 그거입니다",
          "original_form": "전 레샤 이상한나라의솜사탕 팝핑 그거입니다",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100035915.1.17",
