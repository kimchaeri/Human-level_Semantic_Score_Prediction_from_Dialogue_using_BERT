      "utterance": [
        {
          "id": "MDRW2100001777.1.1",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100001777.1.2",
          "form": "결혼에 대해 어떻게 생각하세요!!",
          "original_form": "결혼에 대해 어떻게 생각하세요!!",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100001777.1.3",
          "form": "결혼이라..",
          "original_form": "결혼이라..",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100001777.1.4",
          "form": "진짜 사랑하는 사람과 남은 일생을 보내는거...?",
          "original_form": "진짜 사랑하는 사람과 남은 일생을 보내는거...?",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100001777.1.5",
          "form": "너무 엄청나네요..",
          "original_form": "너무 엄청나네요..",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100001777.1.6",
          "form": "아직 멀었죠...",
          "original_form": "아직 멀었죠...",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100001777.1.7",
          "form": "혹시 결혼 하 셨 나 요?",
          "original_form": "혹시 결혼 하 셨 나 요?",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100001777.1.8",
          "form": "아니요 저도 아직 안했어요",
          "original_form": "아니요 저도 아직 안했어요",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100001777.1.9",
          "form": "근데 결혼을 하는게 좋을지 고민이 되더라구요",
          "original_form": "근데 결혼을 하는게 좋을지 고민이 되더라구요",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100001777.1.10",
          "form": "저한테도 먼 이야기지만요",
          "original_form": "저한테도 먼 이야기지만요",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100001777.1.11",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100001777.1.12",
          "form": "저는 하고는 싶어요!!",
          "original_form": "저는 하고는 싶어요!!",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100001777.1.13",
          "form": "아직까지는..",
          "original_form": "아직까지는..",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100001777.1.14",
          "form": "결혼해서 행복하게 사는 사람들 보면 하고싶고",
          "original_form": "결혼해서 행복하게 사는 사람들 보면 하고싶고",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100001777.1.15",
          "form": "왔다갔다하네요!!!",
          "original_form": "왔다갔다하네요!!!",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100001777.1.16",
          "form": "친구분중에 결혼하신분들 많나요..?",
          "original_form": "친구분중에 결혼하신분들 많나요..?",
          "speaker_id": "2",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100001777.1.17",
          "form": "아니요!!!! 그냥 연예인들 결혼 이렁거요 ㅎ ㅎ..하하",
          "original_form": "아니요!!!! 그냥 연예인들 결혼 이렁거요 ㅎ ㅎ..하하",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001777.1.18",
          "form": "앜ㅋㅋ ㅋ 글쿤요!",
          "original_form": "앜ㅋㅋ ㅋ 글쿤요!",
          "speaker_id": "2",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001777.1.19",
          "form": "연예계 좀 고인 남자분들보면 맨날 결혼 하지마세요 ㅋㅋㅋㅋㅋㅎㅋ",
          "original_form": "연예계 좀 고인 남자분들보면 맨날 결혼 하지마세요 ㅋㅋㅋㅋㅋㅎㅋ",
          "speaker_id": "2",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001777.1.20",
          "form": "이러던데 ㅋㅋㅋ",
          "original_form": "이러던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001777.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러니까요.. ㅎ흠...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러니까요.. ㅎ흠...",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001777.1.22",
          "form": "아주 많이 고민되는 거 같아요",
          "original_form": "아주 많이 고민되는 거 같아요",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001777.1.23",
          "form": "혹시 막 소개팅 같은거 나가보셨어요?",
          "original_form": "혹시 막 소개팅 같은거 나가보셨어요?",
          "speaker_id": "2",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001777.1.24",
          "form": "아니요 소개팅은 그 어색한 느낌이 너무 .....!",
          "original_form": "아니요 소개팅은 그 어색한 느낌이 너무 .....!",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001777.1.25",
          "form": "아으 저도 그럴거 같아요..",
          "original_form": "아으 저도 그럴거 같아요..",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001777.1.26",
          "form": "과미팅도 친구가 찾던데.. 저는 어색할거같아서",
          "original_form": "과미팅도 친구가 찾던데.. 저는 어색할거같아서",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001777.1.27",
          "form": "무섭더라구요",
          "original_form": "무섭더라구요",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001777.1.28",
          "form": "ㅋㅋㅎㅋㅋ",
          "original_form": "ㅋㅋㅎㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001777.1.29",
          "form": "ㅋㅋ ㅋㅋㅋㅋ 대학 다니면 원래 과미팅하고 많이한다잖아요!",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋ 대학 다니면 원래 과미팅하고 많이한다잖아요!",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001777.1.30",
          "form": "전 아직.. 부끄럽더라고요 ㅋㅋㅋ",
          "original_form": "전 아직.. 부끄럽더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001777.1.31",
          "form": "소개팅도 비슷하지 않나요?",
          "original_form": "소개팅도 비슷하지 않나요?",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001777.1.32",
          "form": "같은느낌이죠 ㅎ 저도 그래서 안해요 ㅎ",
          "original_form": "같은느낌이죠 ㅎ 저도 그래서 안해요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001777.1.33",
          "form": "역시 자연스러운 만남을 추구하시나요?",
          "original_form": "역시 자연스러운 만남을 추구하시나요?",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001777.1.34",
          "form": "저는 그렇거든요",
          "original_form": "저는 그렇거든요",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001777.1.35",
          "form": "ㅋㅋㅋㅎㅋ",
          "original_form": "ㅋㅋㅋㅎㅋ",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001777.1.36",
          "form": "네ㅋㅋㅋㅋㅋㅋ 자연스러운 만남이 좋죠",
          "original_form": "네ㅋㅋㅋㅋㅋㅋ 자연스러운 만남이 좋죠",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001777.1.37",
          "form": "그러면서 운명을 바라능....",
          "original_form": "그러면서 운명을 바라능....",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001777.1.38",
          "form": "혹시 그럼..",
          "original_form": "혹시 그럼..",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001777.1.39",
          "form": "클럽.. 같은데 가 보셨나요..?",
          "original_form": "클럽.. 같은데 가 보셨나요..?",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001777.1.40",
          "form": "클럽이요? 그런 비슷항 곳은 가봤지만 찐인 곳은 안가봤어요",
          "original_form": "클럽이요? 그런 비슷항 곳은 가봤지만 찐인 곳은 안가봤어요",
          "speaker_id": "1",
          "time": "20210819 21:50"
