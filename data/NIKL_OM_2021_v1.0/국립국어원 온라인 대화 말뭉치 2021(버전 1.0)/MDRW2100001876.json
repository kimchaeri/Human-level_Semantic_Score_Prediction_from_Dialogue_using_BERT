      "utterance": [
        {
          "id": "MDRW2100001876.1.1",
          "form": "여행은 좋아하시나요??",
          "original_form": "여행은 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001876.1.2",
          "form": "여행 다니는거 좋아하는데",
          "original_form": "여행 다니는거 좋아하는데",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001876.1.3",
          "form": "흥미 느낀지 얼마 안되고 코로나가 생겨서 자주 못가고 있네요ㅠㅠ",
          "original_form": "흥미 느낀지 얼마 안되고 코로나가 생겨서 자주 못가고 있네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001876.1.4",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001876.1.5",
          "form": "네~ 좋아해요ㅠ근데 요즘에는 코로나때문에ㅠㅠ",
          "original_form": "네~ 좋아해요ㅠ근데 요즘에는 코로나때문에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001876.1.6",
          "form": "마지막으로 다녀온 여행이 어디신가요?",
          "original_form": "마지막으로 다녀온 여행이 어디신가요?",
          "speaker_id": "2",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001876.1.7",
          "form": "얼마전에 애들이 너무심심해해서 포항에 스파있는 곳으로 다녀왔어요",
          "original_form": "얼마전에 애들이 너무심심해해서 포항에 스파있는 곳으로 다녀왔어요",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001876.1.8",
          "form": "헐 스파ㅠㅠㅠ",
          "original_form": "헐 스파ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001876.1.9",
          "form": "저도 너무 가고싶네요ㅠㅠ",
          "original_form": "저도 너무 가고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001876.1.10",
          "form": "포항 관광지 구경은 안하셨나요?",
          "original_form": "포항 관광지 구경은 안하셨나요?",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001876.1.11",
          "form": "네ㅠㅠ 코로나때문에 안에서만ㅠ죽도시장만 잠시 들렀다가",
          "original_form": "네ㅠㅠ 코로나때문에 안에서만ㅠ죽도시장만 잠시 들렀다가",
          "speaker_id": "1",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001876.1.12",
          "form": "아쉬우셨겠어요ㅠㅠ",
          "original_form": "아쉬우셨겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001876.1.13",
          "form": "포항에도 볼거리 많을거 같은데ㅠㅠ",
          "original_form": "포항에도 볼거리 많을거 같은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001876.1.14",
          "form": "바다 발도 한번 못 담궈보고ㅠ",
          "original_form": "바다 발도 한번 못 담궈보고ㅠ",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001876.1.15",
          "form": "ㅠㅠㅠㅠ아쉽네요",
          "original_form": "ㅠㅠㅠㅠ아쉽네요",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001876.1.16",
          "form": "기억에 남는 여행지는 어디신가요?",
          "original_form": "기억에 남는 여행지는 어디신가요?",
          "speaker_id": "2",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001876.1.17",
          "form": "신혼여행이요ㅋㅋ 정말제대로 힐링으로 다녀왔는데 그뒤로는 애가있으니 힐링위주는ㅠㅠ",
          "original_form": "신혼여행이요ㅋㅋ 정말제대로 힐링으로 다녀왔는데 그뒤로는 애가있으니 힐링위주는ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001876.1.18",
          "form": "오 어디로 다녀오셨나요?",
          "original_form": "오 어디로 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001876.1.19",
          "form": "푸켓이요~ㅎ",
          "original_form": "푸켓이요~ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001876.1.20",
          "form": "오 푸켓도 휴양지인가요?",
          "original_form": "오 푸켓도 휴양지인가요?",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001876.1.21",
          "form": "관광도있고 마사지있고 볼거리도 꽤있었어요",
          "original_form": "관광도있고 마사지있고 볼거리도 꽤있었어요",
          "speaker_id": "1",
          "time": "20210819 22:51"
        }
      ]
    }
