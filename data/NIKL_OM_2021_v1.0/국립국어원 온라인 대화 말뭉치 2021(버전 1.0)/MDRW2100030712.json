      "utterance": [
        {
          "id": "MDRW2100030712.1.1",
          "form": "ㅋㅋㅋㅋ맞아요..그래서 점심많이먹어요",
          "original_form": "ㅋㅋㅋㅋ맞아요..그래서 점심많이먹어요",
          "speaker_id": "1",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100030712.1.2",
          "form": "근데 저녁도 많이먹어요..^^",
          "original_form": "근데 저녁도 많이먹어요..^^",
          "speaker_id": "1",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100030712.1.3",
          "form": "하루에 두끼면",
          "original_form": "하루에 두끼면",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030712.1.4",
          "form": "1.5인분씩먹어서",
          "original_form": "1.5인분씩먹어서",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030712.1.5",
          "form": "하루 3인분 맞춰줘야한다구요!",
          "original_form": "하루 3인분 맞춰줘야한다구요!",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030712.1.6",
          "form": "ㅋㅋㅋㅋㅋ맞죠맞죠",
          "original_form": "ㅋㅋㅋㅋㅋ맞죠맞죠",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100030712.1.7",
          "form": "지당한 말씀이십니다",
          "original_form": "지당한 말씀이십니다",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100030712.1.8",
          "form": "아 근데 운동하는걸 너무 귀찮아해서 문제예요 ㅠㅠ 퇴근하면 힘들다는 핑계로",
          "original_form": "아 근데 운동하는걸 너무 귀찮아해서 문제예요 ㅠㅠ 퇴근하면 힘들다는 핑계로",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100030712.1.9",
          "form": "저도 운동너무 귀찮아요....",
          "original_form": "저도 운동너무 귀찮아요....",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100030712.1.10",
          "form": "어릴때 운동해두라는데",
          "original_form": "어릴때 운동해두라는데",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100030712.1.11",
          "form": "근력운동 너무 귀찮습니다..",
          "original_form": "근력운동 너무 귀찮습니다..",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100030712.1.12",
          "form": "맨날 앉아만 잇고 먹기만하니까",
          "original_form": "맨날 앉아만 잇고 먹기만하니까",
          "speaker_id": "1",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100030712.1.13",
          "form": "살이 넘 쪄요..",
          "original_form": "살이 넘 쪄요..",
          "speaker_id": "1",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100030712.1.14",
          "form": "난 출퇴근하면 빠질 줄 알았어요...",
          "original_form": "난 출퇴근하면 빠질 줄 알았어요...",
          "speaker_id": "1",
          "time": "20210904 18:42"
        },
        {
          "id": "MDRW2100030712.1.15",
          "form": "ㅠㅠ 힘든건 더 힘든데 살이 찌는 매직....",
          "original_form": "ㅠㅠ 힘든건 더 힘든데 살이 찌는 매직....",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100030712.1.16",
          "form": "내말이요..",
          "original_form": "내말이요..",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100030712.1.17",
          "form": "억울합니다",
          "original_form": "억울합니다",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100030712.1.18",
          "form": "직업이 교사라고 하셨었나요??",
          "original_form": "직업이 교사라고 하셨었나요??",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100030712.1.19",
          "form": "아니요 ㅋㅋㅋ 직장인입니다",
          "original_form": "아니요 ㅋㅋㅋ 직장인입니다",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100030712.1.20",
          "form": "앗 헷갈렸네요ㅋㅋ",
          "original_form": "앗 헷갈렸네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100030712.1.21",
          "form": "어떤 직종이세요??",
          "original_form": "어떤 직종이세요??",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100030712.1.22",
          "form": "사무직인데 cs팀에서 일해요!",
          "original_form": "사무직인데 cs팀에서 일해요!",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100030712.1.23",
          "form": "헐 멋져요ㅋㅋ",
          "original_form": "헐 멋져요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100030712.1.24",
          "form": "제 성향이랑은 반대의 직종이에요",
          "original_form": "제 성향이랑은 반대의 직종이에요",
