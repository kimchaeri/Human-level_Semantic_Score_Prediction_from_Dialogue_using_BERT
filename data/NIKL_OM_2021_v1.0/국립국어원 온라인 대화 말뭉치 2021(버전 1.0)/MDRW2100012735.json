      "utterance": [
        {
          "id": "MDRW2100012735.1.1",
          "form": "결혼을 꼭 하고싶으세요?",
          "original_form": "결혼을 꼭 하고싶으세요?",
          "speaker_id": "2",
          "time": "20210825 00:24"
        },
        {
          "id": "MDRW2100012735.1.2",
          "form": "네  결혼보단 애는 한명 낳고싶어서ㅋ",
          "original_form": "네  결혼보단 애는 한명 낳고싶어서ㅋ",
          "speaker_id": "1",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100012735.1.3",
          "form": "여자에게 임신은 축복이죠ㅎ",
          "original_form": "여자에게 임신은 축복이죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100012735.1.4",
          "form": "근데 포기할것도많아요ㅎ",
          "original_form": "근데 포기할것도많아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100012735.1.5",
          "form": "아이에게 내 모든걸 줘야해요ㅎㅎ",
          "original_form": "아이에게 내 모든걸 줘야해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.6",
          "form": "아ㅎㅎㅎ",
          "original_form": "아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.7",
          "form": "넘 얘기가 깊숙히ㅋ",
          "original_form": "넘 얘기가 깊숙히ㅋ",
          "speaker_id": "1",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.8",
          "form": "뭐 상관없지 않나요ㅎㅎㅎㅎ",
          "original_form": "뭐 상관없지 않나요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.9",
          "form": "친구보니 그런거같아요",
          "original_form": "친구보니 그런거같아요",
          "speaker_id": "1",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.10",
          "form": "내삶이  없는",
          "original_form": "내삶이  없는",
          "speaker_id": "1",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.11",
          "form": "어짜피 name1이나 저나ㅎ 모으는사람이니ㅎㅎ",
          "original_form": "어짜피 &name1&이나 저나ㅎ 모으는사람이니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.12",
          "form": "맞아요ㅠ 어느정도 포기해야해요",
          "original_form": "맞아요ㅠ 어느정도 포기해야해요",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012735.1.13",
          "form": "남들하는건 그래도 다해봐야되지 않나 싶기도하고",
          "original_form": "남들하는건 그래도 다해봐야되지 않나 싶기도하고",
          "speaker_id": "1",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100012735.1.14",
          "form": "평범하게 사는게 진짜힘든듯요",
          "original_form": "평범하게 사는게 진짜힘든듯요",
          "speaker_id": "1",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100012735.1.15",
          "form": "남들하는거 안하면 어때서요?ㅎㅎ",
          "original_form": "남들하는거 안하면 어때서요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012735.1.16",
          "form": "따지는거 아니에요ㅎㅎㅎ",
          "original_form": "따지는거 아니에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012735.1.17",
          "form": "제가 옆에서 본 아내는",
          "original_form": "제가 옆에서 본 아내는",
          "speaker_id": "2",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012735.1.18",
          "form": "여자의 삶을 포기한 엄마라서 그래요",
          "original_form": "여자의 삶을 포기한 엄마라서 그래요",
          "speaker_id": "2",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012735.1.19",
          "form": "name1도 그랬으면 좋겟어요ㅎ만약에 결혼을 하더라두요ㅎㅎ",
          "original_form": "&name1&도 그랬으면 좋겟어요ㅎ만약에 결혼을 하더라두요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012735.1.20",
          "form": "애기있음그렇게되는거같아요 내이름불러주는이 없고ㅎ",
          "original_form": "애기있음그렇게되는거같아요 내이름불러주는이 없고ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012735.1.21",
          "form": "누구엄마로 살아야되니",
          "original_form": "누구엄마로 살아야되니",
          "speaker_id": "1",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012735.1.22",
          "form": "저라도 불러드릴까요? name1엄마~~ 엄마~ 엄마~",
          "original_form": "저라도 불러드릴까요? &name1&엄마~~ 엄마~ 엄마~",
          "speaker_id": "2",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012735.1.23",
          "form": "ㅎㅎㅎ 그냥 흘러가는대로 냅두세요ㅎ",
          "original_form": "ㅎㅎㅎ 그냥 흘러가는대로 냅두세요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012735.1.24",
          "form": "언젠가 찾아올꺼에요ㅎ 그게 한순간에 오니 항상 신중하시구요ㅎㄹ",
          "original_form": "언젠가 찾아올꺼에요ㅎ 그게 한순간에 오니 항상 신중하시구요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012735.1.25",
          "form": "아뇨ㅋ 엄마가 되고싶은건아니고ㅋ",
          "original_form": "아뇨ㅋ 엄마가 되고싶은건아니고ㅋ",
          "speaker_id": "1",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100012735.1.26",
          "form": "이러다 독거노인 되지싶은데 ㅋ키",
          "original_form": "이러다 독거노인 되지싶은데 ㅋ키",
          "speaker_id": "1",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100012735.1.27",
          "form": "ㅎㅎ 에이 설마요ㅎ",
          "original_form": "ㅎㅎ 에이 설마요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012735.1.28",
          "form": "내가지금까지 대화한걸로 봤을땐ㅎ",
          "original_form": "내가지금까지 대화한걸로 봤을땐ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012735.1.29",
          "form": "name1은 충분히 사랑받을 자격있어요ㅎㅎ",
          "original_form": "&name1&은 충분히 사랑받을 자격있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012735.1.30",
          "form": "좋은 사람인데 주위에서 못알아주는거에요ㅎㅎ",
          "original_form": "좋은 사람인데 주위에서 못알아주는거에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012735.1.31",
          "form": "그리말해주니 감사해요ㅎㅎ",
          "original_form": "그리말해주니 감사해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012735.1.32",
          "form": "희망을 가져보죠~",
          "original_form": "희망을 가져보죠~",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012735.1.33",
          "form": "듀오가입안하고",
          "original_form": "듀오가입안하고",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012735.1.34",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012735.1.35",
          "form": "늦었는데 어여주무세요",
          "original_form": "늦었는데 어여주무세요",
