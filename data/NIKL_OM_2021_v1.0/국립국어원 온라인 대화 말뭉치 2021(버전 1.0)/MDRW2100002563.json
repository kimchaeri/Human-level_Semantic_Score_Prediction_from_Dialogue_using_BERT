      "utterance": [
        {
          "id": "MDRW2100002563.1.1",
          "form": "얼굴이 통통상",
          "original_form": "얼굴이 통통상",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002563.1.2",
          "form": "성병이라니!!",
          "original_form": "성병이라니!!",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002563.1.3",
          "form": "그럼 걸리지길 다행이군",
          "original_form": "그럼 걸리지길 다행이군",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002563.1.4",
          "form": "자재 쪽으로 여자들 이상한 소문 많이 들었는데",
          "original_form": "자재 쪽으로 여자들 이상한 소문 많이 들었는데",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002563.1.5",
          "form": "헛소리겠구나 하다보며너",
          "original_form": "헛소리겠구나 하다보며너",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002563.1.6",
          "form": "그여자 관련된?",
          "original_form": "그여자 관련된?",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002563.1.7",
          "form": "나중에 겪어보면 다 사실",
          "original_form": "나중에 겪어보면 다 사실",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002563.1.8",
          "form": "대부분 가쉽 여자들",
          "original_form": "대부분 가쉽 여자들",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002563.1.9",
          "form": "무슨 소문들있어?",
          "original_form": "무슨 소문들있어?",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002563.1.10",
          "form": "걍",
          "original_form": "걍",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002563.1.11",
          "form": "남자 밝힌다고",
          "original_form": "남자 밝힌다고",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002563.1.12",
          "form": "그런것들",
          "original_form": "그런것들",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002563.1.13",
          "form": "레에즈 바이",
          "original_form": "레에즈 바이",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002563.1.14",
          "form": "사람이 500 이였으니",
          "original_form": "사람이 500 이였으니",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002563.1.15",
          "form": "내 밑에만 2-300명이고",
          "original_form": "내 밑에만 2-300명이고",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002563.1.16",
          "form": "95%가 여자고",
          "original_form": "95%가 여자고",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002563.1.17",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002563.1.18",
          "form": "너 좋은곳에서 일했구나",
          "original_form": "너 좋은곳에서 일했구나",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002563.1.19",
          "form": "여자 많은 곳 개힘듬",
          "original_form": "여자 많은 곳 개힘듬",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002563.1.20",
          "form": "특히 관리자면",
          "original_form": "특히 관리자면",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002563.1.21",
          "form": "맨날 파벌 만들어서  싸우고",
          "original_form": "맨날 파벌 만들어서  싸우고",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002563.1.22",
          "form": "고오자질함",
          "original_form": "고오자질함",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002563.1.23",
          "form": "아하ㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002563.1.24",
          "form": "회사 여자들 만나본적있어?",
          "original_form": "회사 여자들 만나본적있어?",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002563.1.25",
          "form": "이미 유부남인데?",
          "original_form": "이미 유부남인데?",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002563.1.26",
          "form": "친한 애들이랑만 종종 술먹었지",
          "original_form": "친한 애들이랑만 종종 술먹었지",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002563.1.27",
          "form": "서브관리자나 데이터 치는 애들",
          "original_form": "서브관리자나 데이터 치는 애들",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002563.1.28",
          "form": "술먹고 뭐했어",
          "original_form": "술먹고 뭐했어",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002563.1.29",
          "form": "단둘이 마셨어?",
          "original_form": "단둘이 마셨어?",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002563.1.30",
          "form": "놉 단둘이 마실일은 없지",
          "original_form": "놉 단둘이 마실일은 없지",
          "speaker_id": "2",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002563.1.31",
          "form": "에이",
          "original_form": "에이",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002563.1.32",
