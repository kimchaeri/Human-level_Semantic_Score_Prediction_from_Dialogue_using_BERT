      "utterance": [
        {
          "id": "MDRW2100043905.1.1",
          "form": "개강하니까 너무 바빠!",
          "original_form": "개강하니까 너무 바빠!",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043905.1.2",
          "form": "1학기 때 너무 놀아서 더 힘들어ㅋㅋㅋ",
          "original_form": "1학기 때 너무 놀아서 더 힘들어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043905.1.3",
          "form": "마자!",
          "original_form": "마자!",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043905.1.4",
          "form": "너무 바빠!",
          "original_form": "너무 바빠!",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043905.1.5",
          "form": "1학기에 들은 거 상위 과목 2개 있는데",
          "original_form": "1학기에 들은 거 상위 과목 2개 있는데",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043905.1.6",
          "form": "복습 시급해ㅋㅋㅋㅋ",
          "original_form": "복습 시급해ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043905.1.7",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043905.1.8",
          "form": "너는 상위과목이나 2과목 없어?",
          "original_form": "너는 상위과목이나 2과목 없어?",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.9",
          "form": "난 문접 배우는 중",
          "original_form": "난 문접 배우는 중",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.10",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.11",
          "form": "없어 아직",
          "original_form": "없어 아직",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.12",
          "form": "문접은 뭐야?",
          "original_form": "문접은 뭐야?",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.13",
          "form": "문법..",
          "original_form": "문법..",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.14",
          "form": "오타",
          "original_form": "오타",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.15",
          "form": "헿",
          "original_form": "헿",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043905.1.16",
          "form": "아하ㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.17",
          "form": "나 한국어 문법이랑",
          "original_form": "나 한국어 문법이랑",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.18",
          "form": "음운론이",
          "original_form": "음운론이",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.19",
          "form": "있어",
          "original_form": "있어",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.20",
          "form": "오 국어 문법이구나",
          "original_form": "오 국어 문법이구나",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.21",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.22",
          "form": "필수 과목이야?",
          "original_form": "필수 과목이야?",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.23",
          "form": "아하하",
          "original_form": "아하하",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.24",
          "form": "하나는 교필",
          "original_form": "하나는 교필",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043905.1.25",
          "form": "하나는 전공선택",
          "original_form": "하나는 전공선택",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043905.1.26",
          "form": "근데 말이 선택이지",
          "original_form": "근데 말이 선택이지",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043905.1.27",
          "form": "글쿠만",
          "original_form": "글쿠만",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043905.1.28",
