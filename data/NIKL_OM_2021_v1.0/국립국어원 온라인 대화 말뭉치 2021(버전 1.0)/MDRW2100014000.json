      "utterance": [
        {
          "id": "MDRW2100014000.1.1",
          "form": "이제 날씨가 너무 추워졌어",
          "original_form": "이제 날씨가 너무 추워졌어",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100014000.1.2",
          "form": "진짜 비도 엄청와",
          "original_form": "진짜 비도 엄청와",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100014000.1.3",
          "form": "창문열고자면 감기걸릴것같아",
          "original_form": "창문열고자면 감기걸릴것같아",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100014000.1.4",
          "form": "가을이오려나",
          "original_form": "가을이오려나",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.5",
          "form": "여기는 오늘도 비가 많이오더라",
          "original_form": "여기는 오늘도 비가 많이오더라",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.6",
          "form": "처서가 지나서 그런가봐",
          "original_form": "처서가 지나서 그런가봐",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.7",
          "form": "처서지났어??",
          "original_form": "처서지났어??",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.8",
          "form": "항상 날씨 잘 챙겨보고 따뜻하게 입어",
          "original_form": "항상 날씨 잘 챙겨보고 따뜻하게 입어",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.9",
          "form": "그러네..",
          "original_form": "그러네..",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.10",
          "form": "웅 처서 지났어",
          "original_form": "웅 처서 지났어",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.11",
          "form": "감기걸리지않게 조심해",
          "original_form": "감기걸리지않게 조심해",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.12",
          "form": "이제 추석쯤되면 쌀쌀하려나",
          "original_form": "이제 추석쯤되면 쌀쌀하려나",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014000.1.13",
          "form": "너도 감기조심해 환절기가 제일 위험해",
          "original_form": "너도 감기조심해 환절기가 제일 위험해",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014000.1.14",
          "form": "마스크 꼭 하렴",
          "original_form": "마스크 꼭 하렴",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014000.1.15",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014000.1.16",
          "form": "추워지면 감기가 제일 걱정이야",
          "original_form": "추워지면 감기가 제일 걱정이야",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014000.1.17",
          "form": "한번 걸리면 너무 아파서..",
          "original_form": "한번 걸리면 너무 아파서..",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014000.1.18",
          "form": "너도 항상 조심해",
          "original_form": "너도 항상 조심해",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014000.1.19",
          "form": "그나저나",
          "original_form": "그나저나",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100014000.1.20",
          "form": "그래 10월부터는 긴팔입어야할거같아",
          "original_form": "그래 10월부터는 긴팔입어야할거같아",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100014000.1.21",
          "form": "내일 조금 맑고 또 비온대",
          "original_form": "내일 조금 맑고 또 비온대",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100014000.1.22",
          "form": "그렇구만 싫다..",
          "original_form": "그렇구만 싫다..",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100014000.1.23",
          "form": "오늘도 비오네요",
          "original_form": "오늘도 비오네요",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100014000.1.24",
          "form": "그곳은요?",
          "original_form": "그곳은요?",
          "speaker_id": "1",
          "time": "20210827 09:56"
        }
      ]
