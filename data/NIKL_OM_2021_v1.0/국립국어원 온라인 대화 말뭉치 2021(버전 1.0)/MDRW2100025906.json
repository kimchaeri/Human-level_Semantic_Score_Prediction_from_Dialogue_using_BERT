      "utterance": [
        {
          "id": "MDRW2100025906.1.1",
          "form": "윙봉 좋아",
          "original_form": "윙봉 좋아",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025906.1.2",
          "form": "윙봉 나온대? 회사식당에서",
          "original_form": "윙봉 나온대? 회사식당에서",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025906.1.3",
          "form": "응 저녁",
          "original_form": "응 저녁",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100025906.1.4",
          "form": "이제 다  먹었어ㅎ",
          "original_form": "이제 다  먹었어ㅎ",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100025906.1.5",
          "form": "먹고나니 짜서 아이스크림 먹는중ㅎ",
          "original_form": "먹고나니 짜서 아이스크림 먹는중ㅎ",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100025906.1.6",
          "form": "ㅋㅋ 맛있겠네 ㅋㅋ 나도 어제 남은 배스킨 조금 먹었다 아까",
          "original_form": "ㅋㅋ 맛있겠네 ㅋㅋ 나도 어제 남은 배스킨 조금 먹었다 아까",
          "speaker_id": "1",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100025906.1.7",
          "form": "체리쥬빌레 오랜만에 넣었더니 그것만 잘 안먹더라 다들",
          "original_form": "체리쥬빌레 오랜만에 넣었더니 그것만 잘 안먹더라 다들",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100025906.1.8",
          "form": "체쥬는 이제 안 넣으려고 ㅜㅜ",
          "original_form": "체쥬는 이제 안 넣으려고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100025906.1.9",
          "form": "너무달아서 긍가",
          "original_form": "너무달아서 긍가",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100025906.1.10",
          "form": "아몬드봉봉 젤조아",
          "original_form": "아몬드봉봉 젤조아",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100025906.1.11",
          "form": "난 단지모르겠던데 엄마아빠는 달다 하더라",
          "original_form": "난 단지모르겠던데 엄마아빠는 달다 하더라",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100025906.1.12",
          "form": "아몬드봉봉 맛있지",
          "original_form": "아몬드봉봉 맛있지",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100025906.1.13",
          "form": "난 엄외가 젤 맛있어",
          "original_form": "난 엄외가 젤 맛있어",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100025906.1.14",
          "form": "난 아몬드봉봉 최애",
          "original_form": "난 아몬드봉봉 최애",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100025906.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100025906.1.16",
          "form": "담엔 아몬드봉봉이나 엄외 두번 담아야지",
          "original_form": "담엔 아몬드봉봉이나 엄외 두번 담아야지",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100025906.1.17",
          "form": "역시 먹던게 맛있어",
          "original_form": "역시 먹던게 맛있어",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100025906.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025906.1.19",
          "form": "좋아하는거만 먹게돼",
          "original_form": "좋아하는거만 먹게돼",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025906.1.20",
          "form": "와 나방금 대구 육처넌뷔폐 유튜브에서 보고왔는데 대박이다",
          "original_form": "와 나방금 대구 육처넌뷔폐 유튜브에서 보고왔는데 대박이다",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025906.1.21",
          "form": "난 피스타치오 아몬드2랑 그린티2, 아몬드봉봉1,  바람과함께사라지다1 요래 담아ㅎ",
          "original_form": "난 피스타치오 아몬드2랑 그린티2, 아몬드봉봉1,  바람과함께사라지다1 요래 담아ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100025906.1.22",
          "form": "피스타치오 나도 이번에 오랜만에 담아서 먹었어 ㅋㅋ",
          "original_form": "피스타치오 나도 이번에 오랜만에 담아서 먹었어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100025906.1.23",
          "form": "아몬드있어서 좋아",
          "original_form": "아몬드있어서 좋아",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025906.1.24",
          "form": "피스타치오만 있었음 이정도로 좋진않았을거같아",
          "original_form": "피스타치오만 있었음 이정도로 좋진않았을거같아",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025906.1.25",
          "form": "고추장맛이 맛있다고 라이브에서 팔던 사람이 그러던데",
          "original_form": "고추장맛이 맛있다고 라이브에서 팔던 사람이 그러던데",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100025906.1.26",
