      "utterance": [
        {
          "id": "MDRW2100044380.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044380.1.2",
          "form": "네 좋아해요! 딱 규칙적으로 하는 건 없는데",
          "original_form": "네 좋아해요! 딱 규칙적으로 하는 건 없는데",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044380.1.3",
          "form": "땀 흘리고나면 상쾌하더라고요",
          "original_form": "땀 흘리고나면 상쾌하더라고요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044380.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044380.1.5",
          "form": "저도 좋아요!",
          "original_form": "저도 좋아요!",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044380.1.6",
          "form": "땀이 흐를때까지 하는거!!",
          "original_form": "땀이 흐를때까지 하는거!!",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044380.1.7",
          "form": "맞아요! 딱 그 힘든 거 넘어가면 기분이 진짜 좋더라고요",
          "original_form": "맞아요! 딱 그 힘든 거 넘어가면 기분이 진짜 좋더라고요",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044380.1.8",
          "form": "꾸준히 하시는 운동 있으신가요?",
          "original_form": "꾸준히 하시는 운동 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044380.1.9",
          "form": "홈트랑 런닝이요",
          "original_form": "홈트랑 런닝이요",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044380.1.10",
          "form": "와 대단하시네요 저는 꾸준히가 너무 힘들더라고요ㅋㅋㅋ",
          "original_form": "와 대단하시네요 저는 꾸준히가 너무 힘들더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100044380.1.11",
          "form": "저도 요즘은ㅋㅋ",
          "original_form": "저도 요즘은ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100044380.1.12",
          "form": "저는 요즘 운동을 아무것도 못 하고 있어요ㅜ",
          "original_form": "저는 요즘 운동을 아무것도 못 하고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100044380.1.13",
          "form": "홈트 이주에 한번하는듯요ㅋㅋ",
          "original_form": "홈트 이주에 한번하는듯요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100044380.1.14",
          "form": "ㅋㅋㅋㅋ그래도 꾸준히 하는 게 대단하세요",
          "original_form": "ㅋㅋㅋㅋ그래도 꾸준히 하는 게 대단하세요",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100044380.1.15",
          "form": "배가 너무 불러서해요ㅋㅋ",
          "original_form": "배가 너무 불러서해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100044380.1.16",
          "form": "저도 가끔 몸 고장났다 싶으면 스트레칭하고 그래요ㅋㅋ",
          "original_form": "저도 가끔 몸 고장났다 싶으면 스트레칭하고 그래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100044380.1.17",
          "form": "폼롤러 추천이요!!",
          "original_form": "폼롤러 추천이요!!",
          "speaker_id": "1",
