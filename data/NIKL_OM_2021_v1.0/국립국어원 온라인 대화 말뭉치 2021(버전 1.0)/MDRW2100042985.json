      "utterance": [
        {
          "id": "MDRW2100042985.1.1",
          "form": "비가 오네요 ㅠ",
          "original_form": "비가 오네요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042985.1.2",
          "form": "그러게요 오늘 꽤 많이 오더라구요ㅠ",
          "original_form": "그러게요 오늘 꽤 많이 오더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042985.1.3",
          "form": "오늘 출근 하셧어요?",
          "original_form": "오늘 출근 하셧어요?",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100042985.1.4",
          "form": "네 출근 했죠! 오늘 쉬시나요?",
          "original_form": "네 출근 했죠! 오늘 쉬시나요?",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100042985.1.5",
          "form": "네네 잠깐 외출 햇다가",
          "original_form": "네네 잠깐 외출 햇다가",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100042985.1.6",
          "form": "비가 마니 와서 들어왓어요 ㅠㅠ",
          "original_form": "비가 마니 와서 들어왓어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100042985.1.7",
          "form": "비 엄청오죠ㅜ",
          "original_form": "비 엄청오죠ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100042985.1.8",
          "form": "맑은 날이 거의 없네요 요새\r\n주말에만 좀 맑고ㅠ",
          "original_form": "맑은 날이 거의 없네요 요새\r\n주말에만 좀 맑고ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100042985.1.9",
          "form": "네네 ㅠㅠ 외출이라도 하고싶은데",
          "original_form": "네네 ㅠㅠ 외출이라도 하고싶은데",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100042985.1.10",
          "form": "우산 부터 펴야해요 ㅋㅋ",
          "original_form": "우산 부터 펴야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100042985.1.11",
          "form": "마자요ㅠ",
          "original_form": "마자요ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100042985.1.12",
          "form": "저도  나가서 좀 걷고싶은데ㅠ 계속 비가와세ㅠ",
          "original_form": "저도  나가서 좀 걷고싶은데ㅠ 계속 비가와세ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100042985.1.13",
          "form": "요즘 부츠 많이 신드라구요!",
          "original_form": "요즘 부츠 많이 신드라구요!",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100042985.1.14",
          "form": "마자요 레인부츠 많이들 사더라구요",
          "original_form": "마자요 레인부츠 많이들 사더라구요",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100042985.1.15",
          "form": "하나있으면 좋긴할거같아요",
          "original_form": "하나있으면 좋긴할거같아요",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100042985.1.16",
          "form": "양말 젖는게 너무 싫어서 ㅋㅋㅋㅋ",
          "original_form": "양말 젖는게 너무 싫어서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042985.1.17",
          "form": "우산도 큰게 있드라구요",
          "original_form": "우산도 큰게 있드라구요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042985.1.18",
          "form": "마자요 저두요  신발이랑 양말 젖는거 너무 싫어요ㅠ",
          "original_form": "마자요 저두요  신발이랑 양말 젖는거 너무 싫어요ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042985.1.19",
          "form": "아하 완전 왕우산도 있군요",
          "original_form": "아하 완전 왕우산도 있군요",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042985.1.20",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042985.1.21",
          "form": "골프장 우산..?",
          "original_form": "골프장 우산..?",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042985.1.22",
          "form": "같은 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "같은 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042985.1.23",
          "form": "아 마자요 그게 진짜 크죠ㅋㅋ",
          "original_form": "아 마자요 그게 진짜 크죠ㅋㅋ",
