      "utterance": [
        {
          "id": "MDRW2100030787.1.1",
          "form": "맞아요 맨날 사귀지 말란말이야",
          "original_form": "맞아요 맨날 사귀지 말란말이야",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030787.1.2",
          "form": "재판을 해도 사귀고 전쟁이 나도 사귀고ㅋㅋㅋㅋ",
          "original_form": "재판을 해도 사귀고 전쟁이 나도 사귀고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030787.1.3",
          "form": "장르물은 로맨스 없이 만드는게 더 재미밌는거 같아요",
          "original_form": "장르물은 로맨스 없이 만드는게 더 재미밌는거 같아요",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030787.1.4",
          "form": "ㅋㅋㅋㅋㅋ맞아요 그냥 썸 정도면 적당히 재밌는 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 그냥 썸 정도면 적당히 재밌는 것 같아요",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030787.1.5",
          "form": "나중에 외전으로나 풀어줬으면",
          "original_form": "나중에 외전으로나 풀어줬으면",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030787.1.6",
          "form": "맞아요 로맨스는 외전으로나 풀어주고",
          "original_form": "맞아요 로맨스는 외전으로나 풀어주고",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030787.1.7",
          "form": "근데 요즘 장르물은 다들 잘만들더라고요",
          "original_form": "근데 요즘 장르물은 다들 잘만들더라고요",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030787.1.8",
          "form": "그러니까요 비밀의숲도 로맨스 별로 없죠",
          "original_form": "그러니까요 비밀의숲도 로맨스 별로 없죠",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030787.1.9",
          "form": "아 맞아요 비밀의 숲도 로맨스가 거의 없어요ㅋㅋ",
          "original_form": "아 맞아요 비밀의 숲도 로맨스가 거의 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030787.1.10",
          "form": "요즘은 장르물에 로맨스 있는거 찾기가 더 힘들더라고요",
          "original_form": "요즘은 장르물에 로맨스 있는거 찾기가 더 힘들더라고요",
          "speaker_id": "2",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030787.1.11",
          "form": "너무 좋은 것 같아요",
          "original_form": "너무 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030787.1.12",
          "form": "어떤 장르 주로 보세요?",
          "original_form": "어떤 장르 주로 보세요?",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030787.1.13",
          "form": "전 요즘 장르물에 빠졌어요",
          "original_form": "전 요즘 장르물에 빠졌어요",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030787.1.14",
          "form": "님은 어떤 장르 보세요?",
          "original_form": "님은 어떤 장르 보세요?",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030787.1.15",
          "form": "저는 진짜 아무거나 재밌어보이는 거 보는 편인데 자주 안 봐서...",
          "original_form": "저는 진짜 아무거나 재밌어보이는 거 보는 편인데 자주 안 봐서...",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030787.1.16",
          "form": "제일 최근에는 뭐 보셨어요?",
          "original_form": "제일 최근에는 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030787.1.17",
          "form": "라이프 온 마스 봤어요",
          "original_form": "라이프 온 마스 봤어요",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030787.1.18",
          "form": "라온마 들어보셨어요?",
          "original_form": "라온마 들어보셨어요?",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030787.1.19",
          "form": "우와 아뇨 이건 진짜 처음 들어요 재밌나요??",
          "original_form": "우와 아뇨 이건 진짜 처음 들어요 재밌나요??",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030787.1.20",
          "form": "정경호 나오는 장르물인데 이것도 꽤 재미있답니다ㅋㅋㅋ",
          "original_form": "정경호 나오는 장르물인데 이것도 꽤 재미있답니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100030787.1.21",
          "form": "거의 정경호 브이로그급으로 정경호가 많이 나와요ㅋㅋㅋㅋ",
          "original_form": "거의 정경호 브이로그급으로 정경호가 많이 나와요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100030787.1.22",
          "form": "아ㅋㅋㅋ큐ㅠ 정경호 좋아하세요??",
          "original_form": "아ㅋㅋㅋ큐ㅠ 정경호 좋아하세요??",
          "speaker_id": "1",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100030787.1.23",
          "form": "이 드라마 보고 좋아졌어요ㅋㅋㅋ",
          "original_form": "이 드라마 보고 좋아졌어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100030787.1.24",
          "form": "정경호 되게 매력적인 것 같아요",
          "original_form": "정경호 되게 매력적인 것 같아요",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100030787.1.25",
          "form": "저도 슬의생 보고 완전 반했던ㅋㅋㅋ",
          "original_form": "저도 슬의생 보고 완전 반했던ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100030787.1.26",
          "form": "정경호 연기도 잘하는데 키도 크고...",
          "original_form": "정경호 연기도 잘하는데 키도 크고...",
          "speaker_id": "2",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100030787.1.27",
          "form": "정경호 좋으시면 라온마 봐보시는거 추천해요!!",
          "original_form": "정경호 좋으시면 라온마 봐보시는거 추천해요!!",
          "speaker_id": "2",
          "time": "20210904 17:33"
        },
        {
          "id": "MDRW2100030787.1.28",
          "form": "나중에 한번 봐봐야겠어요..!",
          "original_form": "나중에 한번 봐봐야겠어요..!",
          "speaker_id": "1",
          "time": "20210904 17:35"
        }
      ]
