        {
          "id": "MDRW2100028583.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028583.1.2",
          "form": "본집에서 강아지 키워요",
          "original_form": "본집에서 강아지 키워요",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028583.1.3",
          "form": "전 구피요",
          "original_form": "전 구피요",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028583.1.4",
          "form": "귀여운 반려동물 키우시네요ㅎㅎ",
          "original_form": "귀여운 반려동물 키우시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028583.1.5",
          "form": "물관리가힘들죠",
          "original_form": "물관리가힘들죠",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028583.1.6",
          "form": "그러니까요 부지런하시네용",
          "original_form": "그러니까요 부지런하시네용",
          "speaker_id": "2",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100028583.1.7",
          "form": "금방 물때랑 이끼 끼던데ㅜㅜ",
          "original_form": "금방 물때랑 이끼 끼던데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100028583.1.8",
          "form": "갈기 귀찮은데",
          "original_form": "갈기 귀찮은데",
          "speaker_id": "1",
          "time": "20210903 07:25"
        },
        {
          "id": "MDRW2100028583.1.9",
          "form": "병 안생기게 하려면 어쩔 수 없죠ㅋ",
          "original_form": "병 안생기게 하려면 어쩔 수 없죠ㅋ",
          "speaker_id": "2",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100028583.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100028583.1.11",
          "form": "강아지도 관리 안해주면 피부병걸리고.. 반려동물 키우는게 쉽지않더라구요",
          "original_form": "강아지도 관리 안해주면 피부병걸리고.. 반려동물 키우는게 쉽지않더라구요",
          "speaker_id": "2",
          "time": "20210903 07:29"
        },
        {
          "id": "MDRW2100028583.1.12",
          "form": "케어가 힘들어요",
          "original_form": "케어가 힘들어요",
          "speaker_id": "1",
          "time": "20210903 07:30"
        },
        {
          "id": "MDRW2100028583.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 07:30"
        },
        {
          "id": "MDRW2100028583.1.14",
          "form": "간식, 사료도 챙겨줘야하구",
          "original_form": "간식, 사료도 챙겨줘야하구",
          "speaker_id": "2",
          "time": "20210903 07:31"
        },
