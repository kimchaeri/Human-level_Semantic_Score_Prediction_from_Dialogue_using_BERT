      "utterance": [
        {
          "id": "MDRW2100031852.1.1",
          "form": "헉 5천원이나 할인돼요?",
          "original_form": "헉 5천원이나 할인돼요?",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031852.1.2",
          "form": "네 내일까지!",
          "original_form": "네 내일까지!",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031852.1.3",
          "form": "저두 낼 굽네나 먹을까봐요",
          "original_form": "저두 낼 굽네나 먹을까봐요",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031852.1.4",
          "form": "네 저도 낼 또 먹을려고요 ㅋㅋ",
          "original_form": "네 저도 낼 또 먹을려고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031852.1.5",
          "form": "5천원 할인이면 먹어야죠",
          "original_form": "5천원 할인이면 먹어야죠",
          "speaker_id": "2",
          "time": "20210905 02:55"
        },
        {
          "id": "MDRW2100031852.1.6",
          "form": "전 볼케이노!",
          "original_form": "전 볼케이노!",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.7",
          "form": "볼케이노 좋아하세요?",
          "original_form": "볼케이노 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.8",
          "form": "네 매운게 좋아요",
          "original_form": "네 매운게 좋아요",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.9",
          "form": "치밥각이네요 ㅋㅋ",
          "original_form": "치밥각이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.11",
          "form": "밥반찬으로 ㅎㅎ",
          "original_form": "밥반찬으로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.12",
          "form": "네 양도 적어서",
          "original_form": "네 양도 적어서",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.13",
          "form": "밥 필수에요",
          "original_form": "밥 필수에요",
          "speaker_id": "1",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.14",
          "form": "네 ㅎㅎ 덕분에 먹겠네요",
          "original_form": "네 ㅎㅎ 덕분에 먹겠네요",
          "speaker_id": "2",
          "time": "20210905 02:56"
        },
        {
          "id": "MDRW2100031852.1.15",
          "form": "저는 고바 고민중 ㅎㅎ",
          "original_form": "저는 고바 고민중 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 02:57"
        },
        {
          "id": "MDRW2100031852.1.16",
          "form": "고바?",
          "original_form": "고바?",
          "speaker_id": "2",
          "time": "20210905 02:57"
        },
        {
          "id": "MDRW2100031852.1.17",
