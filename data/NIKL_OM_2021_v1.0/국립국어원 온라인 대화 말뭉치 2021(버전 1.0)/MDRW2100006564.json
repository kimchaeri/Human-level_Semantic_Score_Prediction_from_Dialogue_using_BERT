      "utterance": [
        {
          "id": "MDRW2100006564.1.1",
          "form": "시카고피자 맛있다던데 아직 못 먹어봤어요",
          "original_form": "시카고피자 맛있다던데 아직 못 먹어봤어요",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006564.1.2",
          "form": "혹시 드셔보셨어요?",
          "original_form": "혹시 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006564.1.3",
          "form": "아 치즈많이 든거죠??",
          "original_form": "아 치즈많이 든거죠??",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006564.1.4",
          "form": "예전에 레스토랑에서",
          "original_form": "예전에 레스토랑에서",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006564.1.5",
          "form": "근데 썩ㅋㅋ",
          "original_form": "근데 썩ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006564.1.6",
          "form": "두툼하니 맛있다고 하던데 별로 인가요?",
          "original_form": "두툼하니 맛있다고 하던데 별로 인가요?",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006564.1.7",
          "form": "토핑 많은게 더 좋아요!",
          "original_form": "토핑 많은게 더 좋아요!",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006564.1.8",
          "form": "어떤 토핑 좋아하세요",
          "original_form": "어떤 토핑 좋아하세요",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006564.1.9",
          "form": "새우랑 고구마요!",
          "original_form": "새우랑 고구마요!",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006564.1.10",
          "form": "오..저도 좋아하는거네요.",
          "original_form": "오..저도 좋아하는거네요.",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006564.1.11",
          "form": "저는 파인애플도 맛있더라구요",
          "original_form": "저는 파인애플도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006564.1.12",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006564.1.13",
          "form": "하와이언어쩌구 좋아요!",
          "original_form": "하와이언어쩌구 좋아요!",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006564.1.14",
          "form": "파인애플 그냥 먹으면 신데 구우면 달아져서 좋아요",
          "original_form": "파인애플 그냥 먹으면 신데 구우면 달아져서 좋아요",
          "speaker_id": "2",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006564.1.15",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006564.1.16",
          "form": "사람들이 왜 싫어하는지ㅜ",
          "original_form": "사람들이 왜 싫어하는지ㅜ",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100006564.1.17",
          "form": "치즈는 별로 안 좋아 하시나요?",
          "original_form": "치즈는 별로 안 좋아 하시나요?",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100006564.1.18",
          "form": "파인애플 사람들이 싫어하나요?^^",
          "original_form": "파인애플 사람들이 싫어하나요?^^",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100006564.1.19",
          "form": "치즈좋아해요",
          "original_form": "치즈좋아해요",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006564.1.20",
          "form": "사람들이 썩...",
          "original_form": "사람들이 썩...",
