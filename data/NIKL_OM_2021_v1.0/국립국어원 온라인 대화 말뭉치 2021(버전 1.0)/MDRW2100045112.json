      "utterance": [
        {
          "id": "MDRW2100045112.1.1",
          "form": "안에 씨앗이 있나요?",
          "original_form": "안에 씨앗이 있나요?",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045112.1.2",
          "form": "빨간 과일 아니예요!",
          "original_form": "빨간 과일 아니예요!",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045112.1.3",
          "form": "안에 씨앗이 있나요?",
          "original_form": "안에 씨앗이 있나요?",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045112.1.4",
          "form": "네 씨앗이 있긴 해요",
          "original_form": "네 씨앗이 있긴 해요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045112.1.5",
          "form": "명목상 있어요",
          "original_form": "명목상 있어요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045112.1.6",
          "form": "노란색인가요?",
          "original_form": "노란색인가요?",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045112.1.7",
          "form": "아니요 노란색 과일은 아니예요",
          "original_form": "아니요 노란색 과일은 아니예요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045112.1.8",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045112.1.9",
          "form": "주로 단맛이 나나요?",
          "original_form": "주로 단맛이 나나요?",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045112.1.10",
          "form": "네 달기도 하고 새콤해요",
          "original_form": "네 달기도 하고 새콤해요",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045112.1.11",
          "form": "새콤한 이미지가 더 강하답니다",
          "original_form": "새콤한 이미지가 더 강하답니다",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045112.1.12",
          "form": "주로 겨울에 나나요?",
          "original_form": "주로 겨울에 나나요?",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045112.1.13",
          "form": "주로 여름에 쑥쑥 자라요",
          "original_form": "주로 여름에 쑥쑥 자라요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045112.1.14",
          "form": "따뜻한 곳에서 많이 나는 과일이예요 ㅎㅎ",
          "original_form": "따뜻한 곳에서 많이 나는 과일이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045112.1.15",
          "form": "국내에서는 재배를 안하나요?",
          "original_form": "국내에서는 재배를 안하나요?",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045112.1.16",
          "form": "국내 일부에서도 재배를 해요! 좋은 질문이예요",
          "original_form": "국내 일부에서도 재배를 해요! 좋은 질문이예요",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045112.1.17",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045112.1.18",
          "form": "전혀 감을 못잡고 잇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전혀 감을 못잡고 잇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045112.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045112.1.20",
          "form": "껍질을 벗겨먹나요 깎아먹나요?",
          "original_form": "껍질을 벗겨먹나요 깎아먹나요?",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045112.1.21",
          "form": "무조건 깎아먹어요!",
          "original_form": "무조건 깎아먹어요!",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045112.1.22",
          "form": "껍질 못 먹어요",
          "original_form": "껍질 못 먹어요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045112.1.23",
          "form": "껍질을 먹는다면 혓바닥이 괴로울 것이예요",
          "original_form": "껍질을 먹는다면 혓바닥이 괴로울 것이예요",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045112.1.24",
          "form": "힌트 다시 정리해보자면",
          "original_form": "힌트 다시 정리해보자면",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045112.1.25",
          "form": "껍질 못 먹어요! 껍질을 먹는다면 혓바닥이 괴로울 것이예요 ㅋㅋㅋㅋ",
