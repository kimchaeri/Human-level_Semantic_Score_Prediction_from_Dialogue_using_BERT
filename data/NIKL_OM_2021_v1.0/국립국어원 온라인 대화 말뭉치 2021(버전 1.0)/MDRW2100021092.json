      "utterance": [
        {
          "id": "MDRW2100021092.1.1",
          "form": "저녁 뭐 먹을까",
          "original_form": "저녁 뭐 먹을까",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021092.1.2",
          "form": "뭐먹지",
          "original_form": "뭐먹지",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021092.1.3",
          "form": "어제 얘기한 볶음밥?",
          "original_form": "어제 얘기한 볶음밥?",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021092.1.4",
          "form": "음 그래도 되고",
          "original_form": "음 그래도 되고",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021092.1.5",
          "form": "어제 먹은 부대찌개는 또 사먹을 것 같아?",
          "original_form": "어제 먹은 부대찌개는 또 사먹을 것 같아?",
          "speaker_id": "2",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021092.1.6",
          "form": "모르겠네",
          "original_form": "모르겠네",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021092.1.7",
          "form": "귀찮을때는 먹을 수도 있는데",
          "original_form": "귀찮을때는 먹을 수도 있는데",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021092.1.8",
          "form": "무겁고 직접 만든게 더 맛있긴함",
          "original_form": "무겁고 직접 만든게 더 맛있긴함",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021092.1.9",
          "form": "ㅇㅇ마자 그렇더라",
          "original_form": "ㅇㅇ마자 그렇더라",
          "speaker_id": "2",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021092.1.10",
          "form": "비닐도 많이 나왔제",
          "original_form": "비닐도 많이 나왔제",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021092.1.11",
          "form": "그렇지 아무래도",
          "original_form": "그렇지 아무래도",
          "speaker_id": "2",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021092.1.12",
          "form": "사골국물이었으면 더 나았을듯",
          "original_form": "사골국물이었으면 더 나았을듯",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021092.1.13",
          "form": "ㅇㅇ육수 선택할 수 있음 좋았을텐데",
          "original_form": "ㅇㅇ육수 선택할 수 있음 좋았을텐데",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021092.1.14",
          "form": "맞어맞어",
          "original_form": "맞어맞어",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021092.1.15",
          "form": "볶음밥엔 스팸 넣을거랬나?",
          "original_form": "볶음밥엔 스팸 넣을거랬나?",
          "speaker_id": "2",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021092.1.16",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021092.1.17",
          "form": "맛있었당",
          "original_form": "맛있었당",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021092.1.18",
          "form": "근데 역시 스팸은 그냥 구워서 밥이랑 먹는게 짜세같음",
          "original_form": "근데 역시 스팸은 그냥 구워서 밥이랑 먹는게 짜세같음",
          "speaker_id": "2",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021092.1.19",
          "form": "ㅋㅋㅋ쪼끔 남은 스팸은 구워먹어",
          "original_form": "ㅋㅋㅋ쪼끔 남은 스팸은 구워먹어",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021092.1.20",
          "form": "내일 아점은 뭐먹지",
          "original_form": "내일 아점은 뭐먹지",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021092.1.21",
          "form": "샐러드 채소 올꺼야",
          "original_form": "샐러드 채소 올꺼야",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021092.1.22",
          "form": "남은 샐러드채소는 버렸나",
          "original_form": "남은 샐러드채소는 버렸나",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021092.1.23",
          "form": "ㅇㅇ오늘 버렸어",
          "original_form": "ㅇㅇ오늘 버렸어",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021092.1.24",
          "form": "잘했어",
          "original_form": "잘했어",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021092.1.25",
          "form": "그럼 내일 아점을 샐러드 먹자고?",
          "original_form": "그럼 내일 아점을 샐러드 먹자고?",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100021092.1.26",
          "form": "내일 보고",
          "original_form": "내일 보고",
          "speaker_id": "1",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021092.1.27",
          "form": "내일 저녁은 땡기는거 있나",
          "original_form": "내일 저녁은 땡기는거 있나",
          "speaker_id": "2",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021092.1.28",
          "form": "딱히 생각나는건 없음",
          "original_form": "딱히 생각나는건 없음",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021092.1.29",
          "form": "누가 메뉴 다 짜주고 맨날 뚝딱 만들어주면 좋겠다",
          "original_form": "누가 메뉴 다 짜주고 맨날 뚝딱 만들어주면 좋겠다",
          "speaker_id": "2",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100021092.1.30",
          "form": "내가 하는건 뭐나올까 두근거리다가 맛있게 먹는거지",
          "original_form": "내가 하는건 뭐나올까 두근거리다가 맛있게 먹는거지",
          "speaker_id": "2",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100021092.1.31",
          "form": "ㅋㅋㅋ그러게ㅋㅋ 기대했는데 싫어하는거 나올수도 있잖아",
          "original_form": "ㅋㅋㅋ그러게ㅋㅋ 기대했는데 싫어하는거 나올수도 있잖아",
          "speaker_id": "1",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100021092.1.32",
          "form": "우동이라든지ㅎㅎ",
          "original_form": "우동이라든지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100021092.1.33",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021092.1.34",
          "form": "우동도 존맛탱이면 뭐,.",
          "original_form": "우동도 존맛탱이면 뭐,.",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021092.1.35",
          "form": "아예 안먹는건 아니니까",
          "original_form": "아예 안먹는건 아니니까",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021092.1.36",
          "form": "ㅋㅋ그래",
          "original_form": "ㅋㅋ그래",
          "speaker_id": "1",
