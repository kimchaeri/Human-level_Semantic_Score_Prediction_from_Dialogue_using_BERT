        },
        {
          "id": "MDRW2100008070.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008070.1.11",
          "form": "외계인 침공 뭐 이런 느낌이였는데 ㅎㅎㅎ",
          "original_form": "외계인 침공 뭐 이런 느낌이였는데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008070.1.12",
          "form": "넷플릭스는 보세요??",
          "original_form": "넷플릭스는 보세요??",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008070.1.13",
          "form": "아뇨.. 저 ott 안써요ㅠㅜ",
          "original_form": "아뇨.. 저 ott 안써요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008070.1.14",
          "form": "티빙 가입하고 싶기는해요!",
          "original_form": "티빙 가입하고 싶기는해요!",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008070.1.15",
          "form": "여고추리반 보고 싶어서 ㅋㅋㅋ",
          "original_form": "여고추리반 보고 싶어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008070.1.16",
          "form": "여고추리반!!",
          "original_form": "여고추리반!!",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
