      "utterance": [
        {
          "id": "MDRW2100025334.1.1",
          "form": "안녕하세요 종교가있으신가요",
          "original_form": "안녕하세요 종교가있으신가요",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025334.1.2",
          "form": "저는 무교에요~",
          "original_form": "저는 무교에요~",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025334.1.3",
          "form": "저도 무교예요",
          "original_form": "저도 무교예요",
          "speaker_id": "2",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025334.1.4",
          "form": "교회 가보신적 있나요?",
          "original_form": "교회 가보신적 있나요?",
          "speaker_id": "2",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025334.1.5",
          "form": "네 어릴때 문상준다고 해서 가본적있어요",
          "original_form": "네 어릴때 문상준다고 해서 가본적있어요",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025334.1.6",
          "form": "가본적있으신가요?",
          "original_form": "가본적있으신가요?",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025334.1.7",
          "form": "저는 과자받으러 가봤어요",
          "original_form": "저는 과자받으러 가봤어요",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025334.1.8",
          "form": "역시 다들 한번쯤 뭐 받으러 가보게되는거 같아요ㅋㅋㅋ",
          "original_form": "역시 다들 한번쯤 뭐 받으러 가보게되는거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025334.1.9",
          "form": "여기 그런분 많더라구요",
          "original_form": "여기 그런분 많더라구요",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025334.1.10",
          "form": "ㅋㅋㅋ 그러니까요 저도 간식 너무 좋아서  간적 많아요",
          "original_form": "ㅋㅋㅋ 그러니까요 저도 간식 너무 좋아서  간적 많아요",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025334.1.11",
          "form": "절은 가즈본적있너요?",
          "original_form": "절은 가즈본적있너요?",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025334.1.12",
          "form": "절은 부모님따라서 몇번가기도하구요",
          "original_form": "절은 부모님따라서 몇번가기도하구요",
          "speaker_id": "2",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100025334.1.13",
          "form": "등산가다가 한두번 가봤어요",
          "original_form": "등산가다가 한두번 가봤어요",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025334.1.14",
          "form": "저도 등산으로 가보고",
          "original_form": "저도 등산으로 가보고",
          "speaker_id": "1",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025334.1.15",
          "form": "탬플스테이??그걸로 가봤어요",
          "original_form": "탬플스테이??그걸로 가봤어요",
          "speaker_id": "1",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025334.1.16",
          "form": "탬플 스테이 가보셨구나",
          "original_form": "탬플 스테이 가보셨구나",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025334.1.17",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025334.1.18",
          "form": "저는 나름 의미있었던거같아요",
          "original_form": "저는 나름 의미있었던거같아요",
          "speaker_id": "1",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025334.1.19",
          "form": "단무지로 그릇닦아 먹는게",
          "original_form": "단무지로 그릇닦아 먹는게",
          "speaker_id": "1",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025334.1.20",
          "form": "아직도 기억에 남을 만큼요..",
          "original_form": "아직도 기억에 남을 만큼요..",
          "speaker_id": "1",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025334.1.21",
          "form": "그러셨구나 거기간친구는 조용하게. 생각정리하기도 좋고 힐링된다고하더라구요",
          "original_form": "그러셨구나 거기간친구는 조용하게. 생각정리하기도 좋고 힐링된다고하더라구요",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025334.1.22",
          "form": "맞아요 나름 힐링되더라구요",
          "original_form": "맞아요 나름 힐링되더라구요",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025334.1.23",
          "form": "명상시간이있다보니",
          "original_form": "명상시간이있다보니",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025334.1.24",
          "form": "저를되돌아게되었어요",
          "original_form": "저를되돌아게되었어요",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025334.1.25",
          "form": "절은 산속에 보통있어서 풍경도 참 좋아요",
          "original_form": "절은 산속에 보통있어서 풍경도 참 좋아요",
          "speaker_id": "2",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025334.1.26",
          "form": "초록초록해서 조터라구요!",
          "original_form": "초록초록해서 조터라구요!",
          "speaker_id": "1",
          "time": "20210901 11:40"
        }
      ]
    }
  ]
}
