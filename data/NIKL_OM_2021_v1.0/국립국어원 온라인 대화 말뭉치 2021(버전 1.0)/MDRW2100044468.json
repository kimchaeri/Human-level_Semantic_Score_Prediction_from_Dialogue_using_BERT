      "utterance": [
        {
          "id": "MDRW2100044468.1.1",
          "form": "차차차 1화부터 볼껴?",
          "original_form": "차차차 1화부터 볼껴?",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044468.1.2",
          "form": "아니 굳이 그렇게까지는 안볼듯ㅋㅋ",
          "original_form": "아니 굳이 그렇게까지는 안볼듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044468.1.3",
          "form": "지나가다 볼거없음 봐야지ㅋㅋ",
          "original_form": "지나가다 볼거없음 봐야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044468.1.4",
          "form": "그럼 스우파부터 봐 ㅇㅅㅇ",
          "original_form": "그럼 스우파부터 봐 ㅇㅅㅇ",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044468.1.5",
          "form": "ㅋㅋㅋ스우파 영업 장난 아니시네요ㅋㅋ",
          "original_form": "ㅋㅋㅋ스우파 영업 장난 아니시네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044468.1.6",
          "form": "ㅋㅋㅋ 나 끝까지 영업하는거 알지? 보라구^^ㅋㅋ",
          "original_form": "ㅋㅋㅋ 나 끝까지 영업하는거 알지? 보라구^^ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044468.1.7",
          "form": "아님 디피? 엄청 추천은 아니지만..",
          "original_form": "아님 디피? 엄청 추천은 아니지만..",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044468.1.8",
          "form": "근데 너가 추천한거는 거의 재밌었으니까 믿고 봐볼게",
          "original_form": "근데 너가 추천한거는 거의 재밌었으니까 믿고 봐볼게",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044468.1.9",
          "form": "언제가 될지는 모르겠지만",
          "original_form": "언제가 될지는 모르겠지만",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044468.1.10",
          "form": "아니 말이 앞뒤가 너무 극단적이네요ㅋㅋㅋ",
          "original_form": "아니 말이 앞뒤가 너무 극단적이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044468.1.11",
          "form": "ㅋㅋㅋㅋ내가 채널 돌리다가 나와야 한번 보는데",
          "original_form": "ㅋㅋㅋㅋ내가 채널 돌리다가 나와야 한번 보는데",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044468.1.12",
          "form": "아직 채널돌릴때 안나오더라구..",
          "original_form": "아직 채널돌릴때 안나오더라구..",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044468.1.13",
          "form": "재방 안하나? 많이 할거같은데",
          "original_form": "재방 안하나? 많이 할거같은데",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044468.1.14",
          "form": "너 tving 없어?",
          "original_form": "너 tving 없어?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044468.1.15",
          "form": "그니까 많이 할거같은데",
          "original_form": "그니까 많이 할거같은데",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044468.1.16",
          "form": "안보이네",
          "original_form": "안보이네",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044468.1.17",
          "form": "티빙 있지만",
          "original_form": "티빙 있지만",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044468.1.18",
          "form": "찾아보기에는 아직 끌리지않아서요..지나가다라도 봐야 찾아보죠",
          "original_form": "찾아보기에는 아직 끌리지않아서요..지나가다라도 봐야 찾아보죠",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044468.1.19",
          "form": "이런 내가 편성표 찾아서 보내줘야겠다",
          "original_form": "이런 내가 편성표 찾아서 보내줘야겠다",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044468.1.20",
          "form": "ㅋㅋㅋ아니야 그맛이 아니라구",
          "original_form": "ㅋㅋㅋ아니야 그맛이 아니라구",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044468.1.21",
          "form": "지나가다 봤는데 어! 이거 좀 재밌는데 \r\n이래야한다구!",
