      "utterance": [
        {
          "id": "MDRW2100023777.1.1",
          "form": "별료인 색은 없어?",
          "original_form": "별료인 색은 없어?",
          "speaker_id": "2",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100023777.1.2",
          "form": "별로",
          "original_form": "별로",
          "speaker_id": "2",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100023777.1.3",
          "form": "난 노란 계열 별로 안 좋아해",
          "original_form": "난 노란 계열 별로 안 좋아해",
          "speaker_id": "1",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100023777.1.4",
          "form": "난 갈색 넘 많이 해봐서",
          "original_form": "난 갈색 넘 많이 해봐서",
          "speaker_id": "2",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100023777.1.5",
          "form": "좀 질리는구나?",
          "original_form": "좀 질리는구나?",
          "speaker_id": "1",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100023777.1.6",
          "form": "응 질려서",
          "original_form": "응 질려서",
          "speaker_id": "2",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100023777.1.7",
          "form": "그럼 그냥 검은 머리가 좋아?",
          "original_form": "그럼 그냥 검은 머리가 좋아?",
          "speaker_id": "1",
          "time": "20210830 17:15"
        },
        {
          "id": "MDRW2100023777.1.8",
          "form": "어차피 나중엔 염색 못하니까",
          "original_form": "어차피 나중엔 염색 못하니까",
          "speaker_id": "2",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100023777.1.9",
          "form": "검정색 고정될거 같아",
          "original_form": "검정색 고정될거 같아",
          "speaker_id": "2",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100023777.1.10",
          "form": "미리하면 좋지",
          "original_form": "미리하면 좋지",
          "speaker_id": "1",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100023777.1.11",
          "form": "할 수 있을때",
          "original_form": "할 수 있을때",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023777.1.12",
          "form": "미리 하려고 ㅎ",
          "original_form": "미리 하려고 ㅎ",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023777.1.13",
          "form": "그치 그러면 좋지",
          "original_form": "그치 그러면 좋지",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023777.1.14",
          "form": "나도 빨간색 해봐야 하나..?ㅋ",
          "original_form": "나도 빨간색 해봐야 하나..?ㅋ",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023777.1.15",
          "form": "나는 빨간색 나름 잘 어울렸었어",
          "original_form": "나는 빨간색 나름 잘 어울렸었어",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023777.1.16",
          "form": "그럼 나도 해볼까?",
          "original_form": "그럼 나도 해볼까?",
          "speaker_id": "2",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100023777.1.17",
          "form": "밝은 빨강 어두운 빨강?",
          "original_form": "밝은 빨강 어두운 빨강?",
          "speaker_id": "1",
