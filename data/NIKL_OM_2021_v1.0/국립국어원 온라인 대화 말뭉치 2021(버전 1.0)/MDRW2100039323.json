      "utterance": [
        {
          "id": "MDRW2100039323.1.1",
          "form": "한두시간은 하는거 같아요",
          "original_form": "한두시간은 하는거 같아요",
          "speaker_id": "2",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100039323.1.2",
          "form": "그 겜도 레벨 같은게 있나요?",
          "original_form": "그 겜도 레벨 같은게 있나요?",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100039323.1.3",
          "form": "레벨은 있는데 크게 상관있진 않아요",
          "original_form": "레벨은 있는데 크게 상관있진 않아요",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100039323.1.4",
          "form": "게임을 오래했다 정도죠",
          "original_form": "게임을 오래했다 정도죠",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100039323.1.5",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039323.1.6",
          "form": "말씀하신 그 하얀 고양이 그 게임 말고도 다른 것도 많이 즐기는 편이세요?",
          "original_form": "말씀하신 그 하얀 고양이 그 게임 말고도 다른 것도 많이 즐기는 편이세요?",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039323.1.7",
          "form": "롤 해요",
          "original_form": "롤 해요",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039323.1.8",
          "form": "역사와 전통의 롤",
          "original_form": "역사와 전통의 롤",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039323.1.9",
          "form": "문외한인 저도 들어봤죠",
          "original_form": "문외한인 저도 들어봤죠",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039323.1.10",
          "form": "무슨 캐릭터로 플레이 하세요?",
          "original_form": "무슨 캐릭터로 플레이 하세요?",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039323.1.11",
          "form": "쓰레쉬 주로 해요",
          "original_form": "쓰레쉬 주로 해요",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039323.1.12",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039323.1.13",
          "form": "롤도 프로선수같은게 있나요?",
          "original_form": "롤도 프로선수같은게 있나요?",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039323.1.14",
          "form": "옛날 스타크래프트 처럼?",
          "original_form": "옛날 스타크래프트 처럼?",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039323.1.15",
          "form": "네ㅎㅎ 곧있음 세계대회도 해요",
          "original_form": "네ㅎㅎ 곧있음 세계대회도 해요",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039323.1.16",
          "form": "그것도 한국이 최강자인가요?",
          "original_form": "그것도 한국이 최강자인가요?",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039323.1.17",
          "form": "네ㅋㅋ 작년에도 한국이 우승했어요",
          "original_form": "네ㅋㅋ 작년에도 한국이 우승했어요",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039323.1.18",
          "form": "오오 자랑스러운 한민족ㅋㅋ",
          "original_form": "오오 자랑스러운 한민족ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039323.1.19",
          "form": "다른 나라 팀에도 한국인이 많더라구요",
          "original_form": "다른 나라 팀에도 한국인이 많더라구요",
          "speaker_id": "2",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039323.1.20",
          "form": "우왕 자부심이 차오르네요",
          "original_form": "우왕 자부심이 차오르네요",
          "speaker_id": "1",
          "time": "20210903 17:52"
        }
