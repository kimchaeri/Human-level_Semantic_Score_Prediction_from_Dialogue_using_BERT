      "utterance": [
        {
          "id": "MDRW2100039375.1.1",
          "form": "티알피지",
          "original_form": "티알피지",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039375.1.2",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039375.1.3",
          "form": "예상대로군",
          "original_form": "예상대로군",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039375.1.4",
          "form": "이번에 하는 티알피지는",
          "original_form": "이번에 하는 티알피지는",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039375.1.5",
          "form": "몇 명이 모이니",
          "original_form": "몇 명이 모이니",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039375.1.6",
          "form": "4명이서 한단다",
          "original_form": "4명이서 한단다",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039375.1.7",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039375.1.8",
          "form": "생각보다는 적은 숫자군",
          "original_form": "생각보다는 적은 숫자군",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039375.1.9",
          "form": "한번 할 때 4시간 3시간인가",
          "original_form": "한번 할 때 4시간 3시간인가",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039375.1.10",
          "form": "평일엔 할 수 있는 시간이 4시간 정도지",
          "original_form": "평일엔 할 수 있는 시간이 4시간 정도지",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039375.1.11",
          "form": "아 너는 연가를 내도",
          "original_form": "아 너는 연가를 내도",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100039375.1.12",
          "form": "다른 사람들이 4시간 이상이 안되겠구나",
          "original_form": "다른 사람들이 4시간 이상이 안되겠구나",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100039375.1.13",
          "form": "4시간으로 한판에 끝나?",
          "original_form": "4시간으로 한판에 끝나?",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039375.1.14",
          "form": "아니 한 학년을 한달 동안 해",
          "original_form": "아니 한 학년을 한달 동안 해",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039375.1.15",
          "form": "학교야....?",
          "original_form": "학교야....?",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039375.1.16",
          "form": "게임스토리가 학교가 배경인가보네",
          "original_form": "게임스토리가 학교가 배경인가보네",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039375.1.17",
          "form": "해리포터 배경이야",
          "original_form": "해리포터 배경이야",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039375.1.18",
          "form": "호그와트 미스터리라고 폰 게임 스토리를 좀 개변해서 만든 거",
          "original_form": "호그와트 미스터리라고 폰 게임 스토리를 좀 개변해서 만든 거",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039375.1.19",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100039375.1.20",
          "form": "재미있겠다....",
          "original_form": "재미있겠다....",
          "speaker_id": "2",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100039375.1.21",
          "form": "4명이면",
          "original_form": "4명이면",
          "speaker_id": "2",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100039375.1.22",
          "form": "넌 어디 기숙사에 배정 받았니",
          "original_form": "넌 어디 기숙사에 배정 받았니",
          "speaker_id": "2",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100039375.1.23",
          "form": "그리핀도르",
          "original_form": "그리핀도르",
          "speaker_id": "1",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100039375.1.24",
          "form": "오 성격검사해서 받은 거니",
          "original_form": "오 성격검사해서 받은 거니",
          "speaker_id": "2",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039375.1.25",
          "form": "그냥 랜덤하게 해서 받은 거니",
          "original_form": "그냥 랜덤하게 해서 받은 거니",
          "speaker_id": "2",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039375.1.26",
          "form": "내가 정한거야 ㅋㅋ",
          "original_form": "내가 정한거야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039375.1.27",
          "form": "ㅋㅋㅋㅋ그랬군",
          "original_form": "ㅋㅋㅋㅋ그랬군",
          "speaker_id": "2",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039375.1.28",
          "form": "어울리는 걸 배정됬길래",
          "original_form": "어울리는 걸 배정됬길래",
          "speaker_id": "2",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039375.1.29",
          "form": "물어보았다",
          "original_form": "물어보았다",
          "speaker_id": "2",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039375.1.30",
          "form": "어차피 내 캐릭터가 배정되는 거니까",
          "original_form": "어차피 내 캐릭터가 배정되는 거니까",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039375.1.31",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210903 17:48"
        }
      ]
    }
