      "utterance": [
        {
          "id": "MDRW2100026861.1.1",
          "form": "집안일 하실때 가장힘든것은?",
          "original_form": "집안일 하실때 가장힘든것은?",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026861.1.2",
          "form": "화장실청소?",
          "original_form": "화장실청소?",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026861.1.3",
          "form": "음 설거지요 ㅋㅋㅋ",
          "original_form": "음 설거지요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026861.1.4",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026861.1.5",
          "form": "요리잘하시니깐",
          "original_form": "요리잘하시니깐",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026861.1.6",
          "form": "요리하는것 좋은데 피곤하면  치우기가 싫어요",
          "original_form": "요리하는것 좋은데 피곤하면  치우기가 싫어요",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026861.1.7",
          "form": "많이나오시는편이에요?",
          "original_form": "많이나오시는편이에요?",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026861.1.8",
          "form": "아아ㅎ",
          "original_form": "아아ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026861.1.9",
          "form": "피곤하면그렇죠ㅍ",
          "original_form": "피곤하면그렇죠ㅍ",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026861.1.10",
          "form": "많이 앙나와도 그냥 배부르면 귀찮을때가 ㅎㅎ",
          "original_form": "많이 앙나와도 그냥 배부르면 귀찮을때가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026861.1.11",
          "form": "근데 일하고오면 치우기 싫잖아요ㅎ 남편분이 안도와주세요?",
          "original_form": "근데 일하고오면 치우기 싫잖아요ㅎ 남편분이 안도와주세요?",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026861.1.12",
          "form": "음 육아가사 독박이에요 일이 바빠서",
          "original_form": "음 육아가사 독박이에요 일이 바빠서",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026861.1.13",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026861.1.14",
          "form": "그러시구나ㅎ 그래도 일바쁜게 좋잖아요",
          "original_form": "그러시구나ㅎ 그래도 일바쁜게 좋잖아요",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026861.1.15",
          "form": "음 글쵸 뭐 ㅎㅎ",
          "original_form": "음 글쵸 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026861.1.16",
          "form": "그만큼 연봉도 높으실꺼고ㅎ",
          "original_form": "그만큼 연봉도 높으실꺼고ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026861.1.17",
          "form": "일이많다는거는 능력이되신다는거니깐 ㅎㅎ",
          "original_form": "일이많다는거는 능력이되신다는거니깐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026861.1.18",
          "form": "중소기업이  비슷비슷하죺ㅎㅎ",
          "original_form": "중소기업이  비슷비슷하죺ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026861.1.19",
          "form": "저 다시태어나면",
          "original_form": "저 다시태어나면",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026861.1.20",
          "form": "여성으로ㅎㅎ",
          "original_form": "여성으로ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026861.1.21",
          "form": "설겆이 엄청 잘해요ㅎ 청소도ㅎ 육아도 잘할 자신있음요ㅎ",
          "original_form": "설겆이 엄청 잘해요ㅎ 청소도ㅎ 육아도 잘할 자신있음요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026861.1.22",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026861.1.23",
          "form": "전 그냥  뭐가됏든  자유롭게 살고 싶네요 ㅋ",
          "original_form": "전 그냥  뭐가됏든  자유롭게 살고 싶네요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026861.1.24",
          "form": "ㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026861.1.25",
          "form": "너무독박육아를하셔서 그런가보다ㅎ",
          "original_form": "너무독박육아를하셔서 그런가보다ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026861.1.26",
          "form": "육아하는분들의 답이 거의 이거에요 ㅋ",
          "original_form": "육아하는분들의 답이 거의 이거에요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026861.1.27",
          "form": "ㅎㅎㅎ 전 아이랑 평생살고싶어요ㅎ",
          "original_form": "ㅎㅎㅎ 전 아이랑 평생살고싶어요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026861.1.28",
          "form": "여자가 되고싶다는것도",
          "original_form": "여자가 되고싶다는것도",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026861.1.29",
          "form": "ㅎㅎㅎㅎ네넹",
          "original_form": "ㅎㅎㅎㅎ네넹",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026861.1.30",
          "form": "돈걱정없이",
          "original_form": "돈걱정없이",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026861.1.31",
          "form": "아이랑같이 있는 시간이 기니깐ㅎㅎ",
          "original_form": "아이랑같이 있는 시간이 기니깐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:49"
        }
      ]
