      "utterance": [
        {
          "id": "MDRW2100045109.1.1",
          "form": "제가 몇번해봤는데 이상하게 간을 못보겠어요ㅜ",
          "original_form": "제가 몇번해봤는데 이상하게 간을 못보겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045109.1.2",
          "form": "ㅠㅠ 간이 젤 어려워요ㅠㅠ",
          "original_form": "ㅠㅠ 간이 젤 어려워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045109.1.3",
          "form": "그쵸ㅜㅜ 제가한건 다 괜찮은것같은데 다른사람들이 아니래여..",
          "original_form": "그쵸ㅜㅜ 제가한건 다 괜찮은것같은데 다른사람들이 아니래여..",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045109.1.4",
          "form": "앗.....ㅠㅠㅠ다른사람의 입맛이 정확하져ㅠ",
          "original_form": "앗.....ㅠㅠㅠ다른사람의 입맛이 정확하져ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045109.1.5",
          "form": "그러니까요..그래서 시켜먹어용ㅋㅋㅋ",
          "original_form": "그러니까요..그래서 시켜먹어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045109.1.6",
          "form": "맛있으니까ㅋㅋ",
          "original_form": "맛있으니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045109.1.7",
          "form": "그쳐 ㅋㅋㅋㅋㅋ 정답",
          "original_form": "그쳐 ㅋㅋㅋㅋㅋ 정답",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045109.1.8",
          "form": "아침은 뭘로드실거에요?ㅋㅋ",
          "original_form": "아침은 뭘로드실거에요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045109.1.9",
          "form": "저 모카빵이랑 아메요 !",
          "original_form": "저 모카빵이랑 아메요 !",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045109.1.10",
          "form": "헐 완전 부럽",
          "original_form": "헐 완전 부럽",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045109.1.11",
          "form": "지금집에 모카빵이 있다는 얘기",
          "original_form": "지금집에 모카빵이 있다는 얘기",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045109.1.12",
          "form": "있어요 반틈! 신랑이, ㅠ 뜯어 먹고 남은거",
          "original_form": "있어요 반틈! 신랑이, ㅠ 뜯어 먹고 남은거",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045109.1.13",
          "form": "앗 그래도 반틈남기셨다니 사랑이네요",
          "original_form": "앗 그래도 반틈남기셨다니 사랑이네요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045109.1.14",
          "form": "오늘 어쩐일로 안 먹더라구요 더..ㅋㅋ",
          "original_form": "오늘 어쩐일로 안 먹더라구요 더..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045109.1.15",
          "form": "저 줄려고 남긴거 아니고 잊은듯해여",
          "original_form": "저 줄려고 남긴거 아니고 잊은듯해여",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045109.1.16",
          "form": "ㅋㅋㅋ모카빵이랑 아메라니ㅜㅜ",
          "original_form": "ㅋㅋㅋ모카빵이랑 아메라니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045109.1.17",
          "form": "커피번이 땡기네요",
          "original_form": "커피번이 땡기네요",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045109.1.18",
          "form": "어제 아침엔 번 !",
          "original_form": "어제 아침엔 번 !",
          "speaker_id": "1",
          "time": "20210908 01:01"
        }
