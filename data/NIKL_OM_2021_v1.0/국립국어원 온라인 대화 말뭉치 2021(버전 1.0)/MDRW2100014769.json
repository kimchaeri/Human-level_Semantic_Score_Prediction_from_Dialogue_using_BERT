      "utterance": [
        {
          "id": "MDRW2100014769.1.1",
          "form": "비 많이 온다",
          "original_form": "비 많이 온다",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014769.1.2",
          "form": "소리 들림?",
          "original_form": "소리 들림?",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014769.1.3",
          "form": "요즘 날씨가 별로야",
          "original_form": "요즘 날씨가 별로야",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014769.1.4",
          "form": "천둥도친다",
          "original_form": "천둥도친다",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014769.1.5",
          "form": "비 언제 그칠까",
          "original_form": "비 언제 그칠까",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014769.1.6",
          "form": "번개도 엄청쳐",
          "original_form": "번개도 엄청쳐",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014769.1.7",
          "form": "빨리 그쳤음 좋겠다",
          "original_form": "빨리 그쳤음 좋겠다",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014769.1.8",
          "form": "지금은 많이 그쳤어",
          "original_form": "지금은 많이 그쳤어",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014769.1.9",
          "form": "비 끝나면 가을된다",
          "original_form": "비 끝나면 가을된다",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014769.1.10",
          "form": "선선해짐 좋겠다",
          "original_form": "선선해짐 좋겠다",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014769.1.11",
          "form": "가을 좋음",
          "original_form": "가을 좋음",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014769.1.12",
          "form": "난겨울이 좋아",
          "original_form": "난겨울이 좋아",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014769.1.13",
          "form": "부산도 눈이 왔으면 좋겠어",
          "original_form": "부산도 눈이 왔으면 좋겠어",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014769.1.14",
          "form": "겨울엔 먹을게 많아서 좋음",
          "original_form": "겨울엔 먹을게 많아서 좋음",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014769.1.15",
          "form": "겨울에 먹을게 뭐있는데?",
          "original_form": "겨울에 먹을게 뭐있는데?",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014769.1.16",
          "form": "붕어빵 군고구마 다 좋음",
          "original_form": "붕어빵 군고구마 다 좋음",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014769.1.17",
          "form": "요즘 길거리음식없어",
          "original_form": "요즘 길거리음식없어",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014769.1.18",
          "form": "길거리 음식이 없다고?",
          "original_form": "길거리 음식이 없다고?",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014769.1.19",
          "form": "길에서 잘 안보이잖아",
          "original_form": "길에서 잘 안보이잖아",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014769.1.20",
          "form": "난 호두과자 보긴함",
          "original_form": "난 호두과자 보긴함",
          "speaker_id": "2",
          "time": "20210825 20:45"
        }
      ]
