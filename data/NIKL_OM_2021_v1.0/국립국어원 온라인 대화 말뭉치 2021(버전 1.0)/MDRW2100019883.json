      "utterance": [
        {
          "id": "MDRW2100019883.1.1",
          "form": "저녁으로 보쌈이랑 막국수 먹었어요",
          "original_form": "저녁으로 보쌈이랑 막국수 먹었어요",
          "speaker_id": "2",
          "time": "20210828 01:41"
        },
        {
          "id": "MDRW2100019883.1.2",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210828 01:41"
        },
        {
          "id": "MDRW2100019883.1.3",
          "form": "프렌차시즈 시켜드신건가요",
          "original_form": "프렌차시즈 시켜드신건가요",
          "speaker_id": "1",
          "time": "20210828 01:41"
        },
        {
          "id": "MDRW2100019883.1.4",
          "form": "집 가는 길에 포장해왔어요!",
          "original_form": "집 가는 길에 포장해왔어요!",
          "speaker_id": "2",
          "time": "20210828 01:43"
        },
        {
          "id": "MDRW2100019883.1.5",
          "form": "포장 너무 좋네요",
          "original_form": "포장 너무 좋네요",
          "speaker_id": "1",
          "time": "20210828 01:43"
        },
        {
          "id": "MDRW2100019883.1.6",
          "form": "막국수는 물막국수였나요 비빔막국수였나요",
          "original_form": "막국수는 물막국수였나요 비빔막국수였나요",
          "speaker_id": "1",
          "time": "20210828 01:43"
        },
        {
          "id": "MDRW2100019883.1.7",
          "form": "비빔이요! 쟁반국수 느낌의",
          "original_form": "비빔이요! 쟁반국수 느낌의",
          "speaker_id": "2",
          "time": "20210828 01:45"
        },
        {
          "id": "MDRW2100019883.1.8",
          "form": "비빔이라니 너무 좋네요",
          "original_form": "비빔이라니 너무 좋네요",
          "speaker_id": "1",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100019883.1.9",
          "form": "맛있었나요",
          "original_form": "맛있었나요",
          "speaker_id": "1",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100019883.1.10",
          "form": "매우 만족스럽게 먹었어요ㅎㅎ 비빔 좋아하시나요?",
          "original_form": "매우 만족스럽게 먹었어요ㅎㅎ 비빔 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 01:47"
        },
        {
          "id": "MDRW2100019883.1.11",
          "form": "네 비빔 좋아해ㅛ",
          "original_form": "네 비빔 좋아해ㅛ",
          "speaker_id": "1",
          "time": "20210828 01:47"
        },
        {
          "id": "MDRW2100019883.1.12",
          "form": "매콤한 매력이 있어서 좋더라구요",
          "original_form": "매콤한 매력이 있어서 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 01:47"
        },
        {
          "id": "MDRW2100019883.1.13",
          "form": "저도 고깃집에서도 무조건 비냉 시켜요! 좀 더 맛이 진하죠ㅌㅋ",
          "original_form": "저도 고깃집에서도 무조건 비냉 시켜요! 좀 더 맛이 진하죠ㅌㅋ",
          "speaker_id": "2",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100019883.1.14",
          "form": "맞아요 드실 때 계란도 갇이 드시나요",
          "original_form": "맞아요 드실 때 계란도 갇이 드시나요",
          "speaker_id": "1",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100019883.1.15",
          "form": "같이 드시나요",
          "original_form": "같이 드시나요",
          "speaker_id": "1",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100019883.1.16",
          "form": "네네! 예전에는 노른자를 소스게 섞어 먹었는데 요즘은 입에 같이 넣고 먹어요ㅎㅎ",
          "original_form": "네네! 예전에는 노른자를 소스게 섞어 먹었는데 요즘은 입에 같이 넣고 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100019883.1.17",
          "form": "그렇게 먹으면 완전맛있겠어요",
          "original_form": "그렇게 먹으면 완전맛있겠어요",
          "speaker_id": "1",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100019883.1.18",
          "form": "노른자의 맛이 비빔면과 잘 어울리니까",
          "original_form": "노른자의 맛이 비빔면과 잘 어울리니까",
          "speaker_id": "1",
          "time": "20210828 01:51"
        },
        {
          "id": "MDRW2100019883.1.19",
          "form": "맞아여ㅋㅋㅋ 약간 떡볶이랑도 비슷하죠",
          "original_form": "맞아여ㅋㅋㅋ 약간 떡볶이랑도 비슷하죠",
          "speaker_id": "2",
          "time": "20210828 01:51"
        }
