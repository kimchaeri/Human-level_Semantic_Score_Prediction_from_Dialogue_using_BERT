      "utterance": [
        {
          "id": "MDRW2100043097.1.1",
          "form": "먼지 간식 뭐먹어",
          "original_form": "먼지 간식 뭐먹어",
          "speaker_id": "1",
          "time": "20210907 14:13"
        },
        {
          "id": "MDRW2100043097.1.2",
          "form": "처방식캔",
          "original_form": "처방식캔",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.3",
          "form": "그건가능?",
          "original_form": "그건가능?",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.4",
          "form": "사료..츄르",
          "original_form": "사료..츄르",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.5",
          "form": "아니 안맞아서 끊기로",
          "original_form": "아니 안맞아서 끊기로",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.6",
          "form": "사료 두종류 먹이는중",
          "original_form": "사료 두종류 먹이는중",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.7",
          "form": "어우 빡세네",
          "original_form": "어우 빡세네",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.8",
          "form": "짱비싸 한포에 오마넌임",
          "original_form": "짱비싸 한포에 오마넌임",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.9",
          "form": "잘먹긴해?",
          "original_form": "잘먹긴해?",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.10",
          "form": "헐 몇키로",
          "original_form": "헐 몇키로",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.11",
          "form": "새거조아해",
          "original_form": "새거조아해",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043097.1.12",
          "form": "작던데...",
          "original_form": "작던데...",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043097.1.13",
          "form": "2키롬가",
          "original_form": "2키롬가",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043097.1.14",
          "form": "그렂듯",
          "original_form": "그렂듯",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043097.1.15",
          "form": "비싼데..노묘니까..",
          "original_form": "비싼데..노묘니까..",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043097.1.16",
          "form": "ㅠ..",
          "original_form": "ㅠ..",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043097.1.17",
          "form": "너네애들 털 안빠짐??",
          "original_form": "너네애들 털 안빠짐??",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043097.1.18",
          "form": "겁나빠져",
          "original_form": "겁나빠져",
          "speaker_id": "1",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043097.1.19",
          "form": "name3 이중모",
          "original_form": "&name3& 이중모",
          "speaker_id": "1",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043097.1.20",
          "form": "나도죽을거같다",
          "original_form": "나도죽을거같다",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043097.1.21",
          "form": "울name3도 시니어먹여야하나",
          "original_form": "울&name3&도 시니어먹여야하나",
          "speaker_id": "1",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043097.1.22",
          "form": "발에 굴러다녀",
          "original_form": "발에 굴러다녀",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
