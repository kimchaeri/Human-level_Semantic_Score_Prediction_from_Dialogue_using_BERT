      "utterance": [
        {
          "id": "MDRW2100002795.1.1",
          "form": "넌 요즘 보는 방송 있어?",
          "original_form": "넌 요즘 보는 방송 있어?",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002795.1.2",
          "form": "유튜브나",
          "original_form": "유튜브나",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002795.1.3",
          "form": "나 유튜브로 게임이나 홀로라이브 보고 있어",
          "original_form": "나 유튜브로 게임이나 홀로라이브 보고 있어",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002795.1.4",
          "form": "홀로라이브가 뭐야?",
          "original_form": "홀로라이브가 뭐야?",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002795.1.5",
          "form": "혹시 미쿠나 키즈나 아이 알아?",
          "original_form": "혹시 미쿠나 키즈나 아이 알아?",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002795.1.6",
          "form": "애니메이션 캐릭터인가?",
          "original_form": "애니메이션 캐릭터인가?",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002795.1.7",
          "form": "버츄얼 유튜버도 못들어봤겠네?",
          "original_form": "버츄얼 유튜버도 못들어봤겠네?",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002795.1.8",
          "form": "사람이 직접 얼굴로 방송을 하는게 아니라 캐릭터를 만들어서 그 캐릭터를 얼굴 삼아 방송 하는걸 말하는거야",
          "original_form": "사람이 직접 얼굴로 방송을 하는게 아니라 캐릭터를 만들어서 그 캐릭터를 얼굴 삼아 방송 하는걸 말하는거야",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002795.1.9",
          "form": "아 그건 들어봤어",
          "original_form": "아 그건 들어봤어",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002795.1.10",
          "form": "어떤 남자가 여자 캐릭터로 버츄얼 유튜거 하다가",
          "original_form": "어떤 남자가 여자 캐릭터로 버츄얼 유튜거 하다가",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002795.1.11",
          "form": "캐릭터 풀려서 남자 얼굴 나간거",
          "original_form": "캐릭터 풀려서 남자 얼굴 나간거",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002795.1.12",
          "form": "아 나도 그거 들어봤어",
          "original_form": "아 나도 그거 들어봤어",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002795.1.13",
          "form": "대참사",
          "original_form": "대참사",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002795.1.14",
          "form": "진짜 대참사야...",
          "original_form": "진짜 대참사야...",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002795.1.15",
          "form": "버츄얼은 개인 규모고",
          "original_form": "버츄얼은 개인 규모고",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002795.1.16",
          "form": "홀로 라이브는 회사 규모야",
          "original_form": "홀로 라이브는 회사 규모야",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002795.1.17",
          "form": "우와 신기하다",
          "original_form": "우와 신기하다",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002795.1.18",
          "form": "버츄얼을 회사에서 하는거야?",
          "original_form": "버츄얼을 회사에서 하는거야?",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002795.1.19",
          "form": "버츄얼은 혼자서 하는거야!",
          "original_form": "버츄얼은 혼자서 하는거야!",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002795.1.20",
          "form": "홀로는 매니저도 있고 그룹이 따로 있어",
          "original_form": "홀로는 매니저도 있고 그룹이 따로 있어",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002795.1.21",
          "form": "회사에서 데뷔까지 시켜줘",
          "original_form": "회사에서 데뷔까지 시켜줘",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002795.1.22",
          "form": "장난아니네",
          "original_form": "장난아니네",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002795.1.23",
          "form": "데뷔라니 멋지다",
          "original_form": "데뷔라니 멋지다",
          "speaker_id": "1",
          "time": "20210820 15:58"
        }
      ]
    }
