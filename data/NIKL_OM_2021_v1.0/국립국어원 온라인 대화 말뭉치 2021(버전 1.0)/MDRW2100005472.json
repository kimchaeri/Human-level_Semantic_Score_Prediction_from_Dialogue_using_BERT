      "utterance": [
        {
          "id": "MDRW2100005472.1.1",
          "form": "최근에 뭘 구매했어?",
          "original_form": "최근에 뭘 구매했어?",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005472.1.2",
          "form": "풀무원에서 출시한 치즈폭포시카고 샀어!",
          "original_form": "풀무원에서 출시한 치즈폭포시카고 샀어!",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005472.1.3",
          "form": "치즈가 흘러내릴정도로 많다고해서 샀는데 속은 것 같아!",
          "original_form": "치즈가 흘러내릴정도로 많다고해서 샀는데 속은 것 같아!",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005472.1.4",
          "form": "헐!!!!!! 완전 맛있을 것 같아",
          "original_form": "헐!!!!!! 완전 맛있을 것 같아",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005472.1.5",
          "form": "그런 것도 있어? 이건 진짜 알아봐야겠네",
          "original_form": "그런 것도 있어? 이건 진짜 알아봐야겠네",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005472.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ 검색해봐야지 이따",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 검색해봐야지 이따",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005472.1.7",
          "form": "아마 홀린듯이 장바구니에 담게 될거야 ㅋㅋㅋㅋ 그리고 나처럼 말할걸! 속았구만!!!",
          "original_form": "아마 홀린듯이 장바구니에 담게 될거야 ㅋㅋㅋㅋ 그리고 나처럼 말할걸! 속았구만!!!",
          "speaker_id": "2",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005472.1.8",
          "form": "엥 진자?? 안흘러내려?ㅠㅠㅠ",
          "original_form": "엥 진자?? 안흘러내려?ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005472.1.9",
          "form": "으앙 뭐야 기대했는데",
          "original_form": "으앙 뭐야 기대했는데",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005472.1.10",
          "form": "먹어 본 냉동피자 중에는 치즈가 많은 편이긴 했는데",
          "original_form": "먹어 본 냉동피자 중에는 치즈가 많은 편이긴 했는데",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.11",
          "form": "도우를 너무 잘 만든걸까",
          "original_form": "도우를 너무 잘 만든걸까",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.12",
          "form": "응응 나지금 진지해졌어",
          "original_form": "응응 나지금 진지해졌어",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.13",
          "form": "안 흘러내리더라 ㅋㅋㅋㅋㅋ",
          "original_form": "안 흘러내리더라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.14",
          "form": "오뚜기 피자 보다 별로야?",
          "original_form": "오뚜기 피자 보다 별로야?",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.15",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 탈락이네..",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 탈락이네..",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.16",
          "form": "오뚜기 보다는 나은 것 같아",
          "original_form": "오뚜기 보다는 나은 것 같아",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.17",
          "form": "오뚜기가 너무 별로였어 나는ㅋㅋㅋㅋ",
          "original_form": "오뚜기가 너무 별로였어 나는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005472.1.18",
          "form": "그렇구나.. 내가 보는 유튜버는 오뚜기가 낫다고 하던데 사람마다 다른가봐!",
          "original_form": "그렇구나.. 내가 보는 유튜버는 오뚜기가 낫다고 하던데 사람마다 다른가봐!",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005472.1.19",
          "form": "풀무원 피자는 되게되게 두꺼운 고르곤졸라 피자 같아 ㅋㅋㅋㅋ",
          "original_form": "풀무원 피자는 되게되게 두꺼운 고르곤졸라 피자 같아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005472.1.20",
          "form": "되게 달달한 치즈피자...?",
          "original_form": "되게 달달한 치즈피자...?",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005472.1.21",
          "form": "다른 맛도 있었는데 그걸 먹어볼 걸 그랬나벼 ㅋㅋㅋ",
          "original_form": "다른 맛도 있었는데 그걸 먹어볼 걸 그랬나벼 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005472.1.22",
          "form": "에잉 그냥 사먹을게",
          "original_form": "에잉 그냥 사먹을게",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005472.1.23",
          "form": "냉동 피자는 믿지 않으려고 해 ㅎㅎㅎㅎ",
          "original_form": "냉동 피자는 믿지 않으려고 해 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:55"
