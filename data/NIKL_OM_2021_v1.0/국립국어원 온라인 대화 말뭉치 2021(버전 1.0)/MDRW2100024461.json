      "utterance": [
        {
          "id": "MDRW2100024461.1.1",
          "form": "ㅜㅠ 한번만 사용가능하다니",
          "original_form": "ㅜㅠ 한번만 사용가능하다니",
          "speaker_id": "1",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024461.1.2",
          "form": "넘 아까워요!!",
          "original_form": "넘 아까워요!!",
          "speaker_id": "1",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024461.1.3",
          "form": "캣잎?! 같은것도 주나요?",
          "original_form": "캣잎?! 같은것도 주나요?",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024461.1.4",
          "form": "hahaha라는 유튜브 채널을 보는데",
          "original_form": "hahaha라는 유튜브 채널을 보는데",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024461.1.5",
          "form": "고양이들이 엄청 좋아하더라구요!",
          "original_form": "고양이들이 엄청 좋아하더라구요!",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024461.1.6",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024461.1.7",
          "form": "넹!! 전에 풀 심는거 실패해서 가루로 구입했어오ㅡㅋㅋ",
          "original_form": "넹!! 전에 풀 심는거 실패해서 가루로 구입했어오ㅡㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024461.1.8",
          "form": "ㅋㅋㅋ 가루로 하면 어떻게 주나요..?",
          "original_form": "ㅋㅋㅋ 가루로 하면 어떻게 주나요..?",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024461.1.9",
          "form": "그냥 뿌려두나요?",
          "original_form": "그냥 뿌려두나요?",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024461.1.10",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024461.1.11",
          "form": "넹! 장난감에 뿌리거나",
          "original_form": "넹! 장난감에 뿌리거나",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024461.1.12",
          "form": "좋아하는 이불에 뿌리면 뒹굴거려용",
          "original_form": "좋아하는 이불에 뿌리면 뒹굴거려용",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024461.1.13",
          "form": "캣잎볼은 품에 간직하더라구요ㅋㅋㅋ",
          "original_form": "캣잎볼은 품에 간직하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024461.1.14",
          "form": "아ㅜㅠㅠ 넘 귀여울 것 같아요ㅜㅠ",
          "original_form": "아ㅜㅠㅠ 넘 귀여울 것 같아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100024461.1.15",
          "form": "고양이들이 왜 그렇게 좋아하는 걸까요..?",
          "original_form": "고양이들이 왜 그렇게 좋아하는 걸까요..?",
          "speaker_id": "1",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100024461.1.16",
          "form": "궁금해요ㅋㅋㅋ",
          "original_form": "궁금해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100024461.1.17",
          "form": "그러니까요.! 마약같은 향이라던데ㅋㅋㅋㅋ 역시.. 마약은 동물도 어쩔 수 없나봐요",
          "original_form": "그러니까요.! 마약같은 향이라던데ㅋㅋㅋㅋ 역시.. 마약은 동물도 어쩔 수 없나봐요",
          "speaker_id": "2",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024461.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024461.1.19",
          "form": "진짜 정신을 못차리는게 넘 귀여워욬ㅋㅋㅋ",
          "original_form": "진짜 정신을 못차리는게 넘 귀여워욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024461.1.20",
          "form": "추르",
          "original_form": "추르",
          "speaker_id": "1",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024461.1.21",
          "form": "도 주시나요?",
          "original_form": "도 주시나요?",
          "speaker_id": "1",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024461.1.22",
          "form": "맞아용ㅋㅋㅋㅋ",
          "original_form": "맞아용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024461.1.23",
          "form": "추르",
          "original_form": "추르",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024461.1.24",
          "form": "환장",
          "original_form": "환장",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024461.1.25",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024461.1.26",
          "form": "봉지만 꺼내도 달라고 난리에요",
          "original_form": "봉지만 꺼내도 달라고 난리에요",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100024461.1.27",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100024461.1.28",
          "form": "넘 귀엽네요ㅜㅠ",
          "original_form": "넘 귀엽네요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100024461.1.29",
          "form": "막 사고치고 그러진 않나요?",
          "original_form": "막 사고치고 그러진 않나요?",
          "speaker_id": "1",
          "time": "20210831 10:59"
        },
        {
          "id": "MDRW2100024461.1.30",
          "form": "보니까 호기심이 많아서",
          "original_form": "보니까 호기심이 많아서",
          "speaker_id": "1",
          "time": "20210831 10:59"
        },
        {
          "id": "MDRW2100024461.1.31",
          "form": "여기저기 망가뜨리고 돌아다니는 고양이들이 많던데요ㅎㅎㅎ",
          "original_form": "여기저기 망가뜨리고 돌아다니는 고양이들이 많던데요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:59"
        },
        {
          "id": "MDRW2100024461.1.32",
          "form": "넹! 성격이 온순해서 그런지 괜찮아요!",
          "original_form": "넹! 성격이 온순해서 그런지 괜찮아요!",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100024461.1.33",
          "form": "벽지 뜯고 그런다던데 그런것도 없었어요!",
          "original_form": "벽지 뜯고 그런다던데 그런것도 없었어요!",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100024461.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024461.1.35",
          "form": "다행이네요!!!",
          "original_form": "다행이네요!!!",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024461.1.36",
          "form": "고양이 얌전하신가봐요ㅎㅋㅋ",
          "original_form": "고양이 얌전하신가봐요ㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024461.1.37",
          "form": "넹ㅋㅋ 말이 많고 얌전해용ㅋㅋㅋ",
          "original_form": "넹ㅋㅋ 말이 많고 얌전해용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:29"
        },
        {
          "id": "MDRW2100024461.1.38",
          "form": "진짜 보여드리고 싶네여",
          "original_form": "진짜 보여드리고 싶네여",
          "speaker_id": "2",
          "time": "20210831 11:29"
        },
        {
          "id": "MDRW2100024461.1.39",
          "form": "강아지도 좋아하세요?",
          "original_form": "강아지도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 11:29"
        },
        {
          "id": "MDRW2100024461.1.40",
          "form": "네!+!",
          "original_form": "네!+!",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100024461.1.41",
          "form": "동물은 다 좋아해요ㅎㅎ",
          "original_form": "동물은 다 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100024461.1.42",
          "form": "키우지는 못하지만 ㅜㅠ",
          "original_form": "키우지는 못하지만 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100024461.1.43",
          "form": "동생이 털알러지가 있어요ㅠ",
