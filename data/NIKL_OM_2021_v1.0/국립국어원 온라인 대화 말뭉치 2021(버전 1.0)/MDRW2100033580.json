      "utterance": [
        {
          "id": "MDRW2100033580.1.1",
          "form": "윙잇 쿠폰줬나봐요",
          "original_form": "윙잇 쿠폰줬나봐요",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100033580.1.2",
          "form": "지금 택배왔는데 어제 저녁때 네이버 라방으로산게왔네요",
          "original_form": "지금 택배왔는데 어제 저녁때 네이버 라방으로산게왔네요",
          "speaker_id": "2",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100033580.1.3",
          "form": "네~~~ 2만원에 만원 득템이네요^^",
          "original_form": "네~~~ 2만원에 만원 득템이네요^^",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100033580.1.4",
          "form": "저번에 3만원에 만원줘서 버렸거든요",
          "original_form": "저번에 3만원에 만원줘서 버렸거든요",
          "speaker_id": "1",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100033580.1.5",
          "form": "저도요~",
          "original_form": "저도요~",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100033580.1.6",
          "form": "회사동생주니 좋다고 쓰던데",
          "original_form": "회사동생주니 좋다고 쓰던데",
          "speaker_id": "1",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100033580.1.7",
          "form": "자주구매하는지",
          "original_form": "자주구매하는지",
          "speaker_id": "1",
          "time": "20210824 15:27"
        },
        {
          "id": "MDRW2100033580.1.8",
          "form": "ㅎㅎ 전 50퍼아님 이젠 비싼듯~~~",
          "original_form": "ㅎㅎ 전 50퍼아님 이젠 비싼듯~~~",
          "speaker_id": "2",
          "time": "20210824 15:27"
        },
        {
          "id": "MDRW2100033580.1.9",
          "form": "마자요 ㅎ 제값주고 이제 못사겠다는",
          "original_form": "마자요 ㅎ 제값주고 이제 못사겠다는",
          "speaker_id": "1",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100033580.1.10",
          "form": "아워홈도 좋은가요? 많이들사네요",
          "original_form": "아워홈도 좋은가요? 많이들사네요",
          "speaker_id": "1",
          "time": "20210824 15:29"
        },
        {
          "id": "MDRW2100033580.1.11",
          "form": "차이있는거같아요",
          "original_form": "차이있는거같아요",
          "speaker_id": "2",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100033580.1.12",
          "form": "아워홈도없고 번개도없고~~",
          "original_form": "아워홈도없고 번개도없고~~",
          "speaker_id": "2",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100033580.1.13",
          "form": "뭐파는지 몰겠어서",
          "original_form": "뭐파는지 몰겠어서",
          "speaker_id": "1",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100033580.1.14",
          "form": "글게요",
          "original_form": "글게요",
          "speaker_id": "2",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100033580.1.15",
          "form": "그냥 윙잇만 해야겠어욪",
          "original_form": "그냥 윙잇만 해야겠어욪",
          "speaker_id": "1",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100033580.1.16",
          "form": "쥬스같은거 사던데",
          "original_form": "쥬스같은거 사던데",
          "speaker_id": "1",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100033580.1.17",
          "form": "우딜하고싶은데 배송비쿠폰이 활성화가 안되네요. 만원이상 무로던데...",
          "original_form": "우딜하고싶은데 배송비쿠폰이 활성화가 안되네요. 만원이상 무로던데...",
          "speaker_id": "2",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100033580.1.18",
          "form": "주말만 좀비쿠폰이였다고 그러던데요",
          "original_form": "주말만 좀비쿠폰이였다고 그러던데요",
          "speaker_id": "1",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100033580.1.19",
          "form": "오늘부터 말일까지 매일인지는 모르겠지만 배달비0원해서 있어요",
          "original_form": "오늘부터 말일까지 매일인지는 모르겠지만 배달비0원해서 있어요",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100033580.1.20",
          "form": "픽업해야 괜찮은데",
          "original_form": "픽업해야 괜찮은데",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100033580.1.21",
          "form": "그니깐요. 배송비있음 넘 비싸요~~",
          "original_form": "그니깐요. 배송비있음 넘 비싸요~~",
