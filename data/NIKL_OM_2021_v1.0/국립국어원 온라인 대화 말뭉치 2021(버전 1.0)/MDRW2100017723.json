      "utterance": [
        {
          "id": "MDRW2100017723.1.1",
          "form": "제주도도 가고싶다",
          "original_form": "제주도도 가고싶다",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017723.1.2",
          "form": "풀빌라 빌려서",
          "original_form": "풀빌라 빌려서",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017723.1.3",
          "form": "놀고",
          "original_form": "놀고",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017723.1.4",
          "form": "나도!",
          "original_form": "나도!",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017723.1.5",
          "form": "동남아도 좋고!",
          "original_form": "동남아도 좋고!",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017723.1.6",
          "form": "휴양지!",
          "original_form": "휴양지!",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017723.1.7",
          "form": "세부나 괌 하와이 이런 데 가고 싶어",
          "original_form": "세부나 괌 하와이 이런 데 가고 싶어",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017723.1.8",
          "form": "완전 좋아",
          "original_form": "완전 좋아",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017723.1.9",
          "form": "해양 스포츠 즐기고 싶어",
          "original_form": "해양 스포츠 즐기고 싶어",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017723.1.10",
          "form": "서핑 해보고싶어",
          "original_form": "서핑 해보고싶어",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017723.1.11",
          "form": "나도!",
          "original_form": "나도!",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017723.1.12",
          "form": "재밌을 거 같아",
          "original_form": "재밌을 거 같아",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017723.1.13",
          "form": "그러다가 물먹고",
          "original_form": "그러다가 물먹고",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017723.1.14",
          "form": "근데 난 일단 수영부터 배워야 함",
          "original_form": "근데 난 일단 수영부터 배워야 함",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017723.1.15",
          "form": "바다에 풍덩",
          "original_form": "바다에 풍덩",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017723.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017723.1.17",
          "form": "질질 끌려올 각이다",
          "original_form": "질질 끌려올 각이다",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017723.1.18",
          "form": "빨리 코로나 끝났으면 좋겠어 나 수영 배워야 해",
          "original_form": "빨리 코로나 끝났으면 좋겠어 나 수영 배워야 해",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017723.1.19",
          "form": "나도..",
          "original_form": "나도..",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017723.1.20",
          "form": "수영 배우고싶어",
          "original_form": "수영 배우고싶어",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017723.1.21",
          "form": "놀고싶은 욕구가 물공포증을 이겼음",
          "original_form": "놀고싶은 욕구가 물공포증을 이겼음",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017723.1.22",
          "form": "다음에 같이 다닐 수 있으면 수영장 같이 다니자ㅠ",
          "original_form": "다음에 같이 다닐 수 있으면 수영장 같이 다니자ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017723.1.23",
          "form": "좋아좋아!!",
          "original_form": "좋아좋아!!",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017723.1.24",
          "form": "얍",
