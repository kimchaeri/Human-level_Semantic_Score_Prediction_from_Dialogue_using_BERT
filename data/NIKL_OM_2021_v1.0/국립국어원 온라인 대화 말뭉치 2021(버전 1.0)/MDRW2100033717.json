      "utterance": [
        {
          "id": "MDRW2100033717.1.1",
          "form": "이상형이 어떻게 돼요?",
          "original_form": "이상형이 어떻게 돼요?",
          "speaker_id": "1",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100033717.1.2",
          "form": "이상형 다정하고 자기할일 잘하는 사람이요",
          "original_form": "이상형 다정하고 자기할일 잘하는 사람이요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033717.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033717.1.4",
          "form": "저도요 착한사람",
          "original_form": "저도요 착한사람",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033717.1.5",
          "form": "그리고 잘생긴 사람이요 크크",
          "original_form": "그리고 잘생긴 사람이요 크크",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033717.1.6",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033717.1.7",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033717.1.8",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033717.1.9",
          "form": "네 연애중이예요",
          "original_form": "네 연애중이예요",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033717.1.10",
          "form": "name1님은 동갑 연하 연상중에 누가 좋으세요?",
          "original_form": "&name1&님은 동갑 연하 연상중에 누가 좋으세요?",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033717.1.11",
          "form": "전 연하요 ~~~~",
          "original_form": "전 연하요 ~~~~",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033717.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033717.1.13",
          "form": "동갑이요",
          "original_form": "동갑이요",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033717.1.14",
          "form": "오호 지금 연애중이신 분이 동갑인가요?",
          "original_form": "오호 지금 연애중이신 분이 동갑인가요?",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033717.1.15",
          "form": "네! 여태 연상 연하 다만나봤는데",
          "original_form": "네! 여태 연상 연하 다만나봤는데",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033717.1.16",
          "form": "저는 동갑이 좋아요",
          "original_form": "저는 동갑이 좋아요",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033717.1.17",
          "form": "오호 사람마다 취향이 다르니까요!",
          "original_form": "오호 사람마다 취향이 다르니까요!",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033717.1.18",
          "form": "그럼요! 사람 100명있으면 100가지 취향이 있죠",
          "original_form": "그럼요! 사람 100명있으면 100가지 취향이 있죠",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033717.1.19",
          "form": "맞아요 ㅋㅋㅋ ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033717.1.20",
          "form": "연예인으로 이상형이 있나요?",
          "original_form": "연예인으로 이상형이 있나요?",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033717.1.21",
          "form": "제가 연예인에 관심이 없어서...",
          "original_form": "제가 연예인에 관심이 없어서...",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033717.1.22",
          "form": "생각나는 사람이 없어요ㅋㅋㅋ",
          "original_form": "생각나는 사람이 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033717.1.23",
          "form": "name1님은요? 엔시티 멤버인가요?",
