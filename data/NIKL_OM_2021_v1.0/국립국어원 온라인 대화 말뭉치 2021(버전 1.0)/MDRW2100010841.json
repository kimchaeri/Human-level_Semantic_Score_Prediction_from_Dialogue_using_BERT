      "utterance": [
        {
          "id": "MDRW2100010841.1.1",
          "form": "그냥 아무생각없이 봐야한다. 그래야 재밌게 볼수 있다",
          "original_form": "그냥 아무생각없이 봐야한다. 그래야 재밌게 볼수 있다",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010841.1.2",
          "form": "이제 티비 뭐 보는데",
          "original_form": "이제 티비 뭐 보는데",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010841.1.3",
          "form": "티비 안보고 방에 왔다",
          "original_form": "티비 안보고 방에 왔다",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010841.1.4",
          "form": "너는 내운명하네 ㅋㅋ",
          "original_form": "너는 내운명하네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010841.1.5",
          "form": "아빠 영화 보는거 같네",
          "original_form": "아빠 영화 보는거 같네",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010841.1.6",
          "form": "우리아빠도 영화 좋아하신다 ㅋㅋ 티비를 안끄신다",
          "original_form": "우리아빠도 영화 좋아하신다 ㅋㅋ 티비를 안끄신다",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010841.1.7",
          "form": "영화 봤던거 또 보고 또 보고 ㅋ",
          "original_form": "영화 봤던거 또 보고 또 보고 ㅋ",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010841.1.8",
          "form": "나도 내가 좋아하는거 보고 또 보고한다.",
          "original_form": "나도 내가 좋아하는거 보고 또 보고한다.",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010841.1.9",
          "form": "근데 재밌는건 또 봐도 재밌음",
          "original_form": "근데 재밌는건 또 봐도 재밌음",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010841.1.10",
          "form": "엉 나중에 다음화면에 뭐 나올지 안다",
          "original_form": "엉 나중에 다음화면에 뭐 나올지 안다",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010841.1.11",
          "form": "알아도 재밌으니깐ㅋ 도깨비 봐도 봐도 재밌음",
          "original_form": "알아도 재밌으니깐ㅋ 도깨비 봐도 봐도 재밌음",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010841.1.12",
          "form": "난 한편도 안봤는데 ㅋㅋ 빨간 목도리만 안다",
          "original_form": "난 한편도 안봤는데 ㅋㅋ 빨간 목도리만 안다",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010841.1.13",
          "form": "진짜 재밌다. 강추ㅋ",
          "original_form": "진짜 재밌다. 강추ㅋ",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010841.1.14",
          "form": "다른 사람 다 재밌다고 하는 드라마 영화 다 안보게 되더라",
          "original_form": "다른 사람 다 재밌다고 하는 드라마 영화 다 안보게 되더라",
          "speaker_id": "2",
          "time": "20210823 22:28"
        }
