        },
        {
          "id": "MDRW2100047498.1.26",
          "form": "잌ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "잌ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100047498.1.27",
          "form": "물가는 것도 일이잖아요 ㅠㅠㅋㅋㅋ",
          "original_form": "물가는 것도 일이잖아요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100047498.1.28",
          "form": "산책매일가는것보다야",
          "original_form": "산책매일가는것보다야",
          "speaker_id": "1",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100047498.1.29",
