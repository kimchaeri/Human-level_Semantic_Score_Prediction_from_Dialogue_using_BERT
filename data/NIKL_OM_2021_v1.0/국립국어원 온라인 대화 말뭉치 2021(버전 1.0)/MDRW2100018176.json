      "utterance": [
        {
          "id": "MDRW2100018176.1.1",
          "form": "졸업하니.좋은게",
          "original_form": "졸업하니.좋은게",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018176.1.2",
          "form": "시험안보고",
          "original_form": "시험안보고",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018176.1.3",
          "form": "레포트안쓰고 ㅋㅋ",
          "original_form": "레포트안쓰고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018176.1.4",
          "form": "아.. 진짜 세상 너무 부러워요",
          "original_form": "아.. 진짜 세상 너무 부러워요",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018176.1.5",
          "form": "사실 전 편입하려고 휴학 중인데",
          "original_form": "사실 전 편입하려고 휴학 중인데",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018176.1.6",
          "form": "학교 다닐 때 시험 레포트에 밤을 샌 날이",
          "original_form": "학교 다닐 때 시험 레포트에 밤을 샌 날이",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018176.1.7",
          "form": "헐 ㅋ 저도 편입했어요",
          "original_form": "헐 ㅋ 저도 편입했어요",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018176.1.8",
          "form": "수두룩했죠..",
          "original_form": "수두룩했죠..",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018176.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018176.1.10",
          "form": "선배님이시다",
          "original_form": "선배님이시다",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018176.1.11",
          "form": "학과는 정하셨어요?",
          "original_form": "학과는 정하셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018176.1.12",
          "form": "편입을할거라면 무조건..취업잘되는과로 고르시길....ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "편입을할거라면 무조건..취업잘되는과로 고르시길....ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.13",
          "form": "전 미디어커뮤니케이션, 신방과 쪽을 원하는데",
          "original_form": "전 미디어커뮤니케이션, 신방과 쪽을 원하는데",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.14",
          "form": "사싱 제가 편입 재수라.. 작년에.거기.넣었다가 경쟁률이 터져서",
          "original_form": "사싱 제가 편입 재수라.. 작년에.거기.넣었다가 경쟁률이 터져서",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.15",
          "form": "저는 심리학과였거든요",
          "original_form": "저는 심리학과였거든요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.16",
          "form": "떨어졌었거든요",
          "original_form": "떨어졌었거든요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.17",
          "form": "졸업하니 무쓸모네요",
          "original_form": "졸업하니 무쓸모네요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그 전 학과도 갗은 학과였나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그 전 학과도 갗은 학과였나요?",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.20",
          "form": "아뇨 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.21",
          "form": "전적대는 경영이였어요",
          "original_form": "전적대는 경영이였어요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018176.1.22",
          "form": "아 경영 나쁘지 않은데.. 경영도 경쟁률이 엄청나죠",
          "original_form": "아 경영 나쁘지 않은데.. 경영도 경쟁률이 엄청나죠",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018176.1.23",
          "form": "일반편입이에요?",
          "original_form": "일반편입이에요?",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018176.1.24",
          "form": "그래서 저도 이번에 경쟁률 적은 곳을 넣을까 하다가도 하고 싶은 건 너무 확실해서",
          "original_form": "그래서 저도 이번에 경쟁률 적은 곳을 넣을까 하다가도 하고 싶은 건 너무 확실해서",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018176.1.25",
          "form": "네네 일반편입이요",
          "original_form": "네네 일반편입이요",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018176.1.26",
          "form": "지금전공이랑 같은과",
          "original_form": "지금전공이랑 같은과",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018176.1.27",
          "form": "쓰시는건가요 ㅎㅎ",
          "original_form": "쓰시는건가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:06"
        }
      ]
    }
