      "utterance": [
        {
          "id": "MDRW2100015728.1.1",
          "form": "재테크 커뮤 하시나여?",
          "original_form": "재테크 커뮤 하시나여?",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015728.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015728.1.3",
          "form": "해요!",
          "original_form": "해요!",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015728.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015728.1.5",
          "form": "저는 유튜브 보는정도요.. \r\n어디어디 들어가시나요?",
          "original_form": "저는 유튜브 보는정도요.. \r\n어디어디 들어가시나요?",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015728.1.6",
          "form": "저는 그냥 네이버 카페랑 다음 카페요",
          "original_form": "저는 그냥 네이버 카페랑 다음 카페요",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015728.1.7",
          "form": "유튜브로는 길어서 못보겠더라고요ㅠㅠ",
          "original_form": "유튜브로는 길어서 못보겠더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015728.1.8",
          "form": "다른 커뮤니키는 안하세요?",
          "original_form": "다른 커뮤니키는 안하세요?",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015728.1.9",
          "form": "그때그때 검색해서 들어가시는거에요? \r\n추천있음 알려주세여..",
          "original_form": "그때그때 검색해서 들어가시는거에요? \r\n추천있음 알려주세여..",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015728.1.10",
          "form": "전 유머사이트 주로 봐요",
          "original_form": "전 유머사이트 주로 봐요",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015728.1.11",
          "form": "아뇨ㅠㅠ가입해놓구요!",
          "original_form": "아뇨ㅠㅠ가입해놓구요!",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015728.1.12",
          "form": "어떤 유머사이트 이용하세요?",
          "original_form": "어떤 유머사이트 이용하세요?",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015728.1.13",
          "form": "웃대랑 오유는 망해서 안가구요..",
          "original_form": "웃대랑 오유는 망해서 안가구요..",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015728.1.14",
          "form": "루리웹 베스트도 눈팅하고..",
          "original_form": "루리웹 베스트도 눈팅하고..",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015728.1.15",
          "form": "보통 눈팅하는게 많으시죠?",
          "original_form": "보통 눈팅하는게 많으시죠?",
          "speaker_id": "2",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015728.1.16",
          "form": "저두요ㅎㅎ",
          "original_form": "저두요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015728.1.17",
          "form": "정모 같은건 하세요??",
          "original_form": "정모 같은건 하세요??",
          "speaker_id": "2",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015728.1.18",
          "form": "네.. 그정도는 아니에여..",
          "original_form": "네.. 그정도는 아니에여..",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015728.1.19",
          "form": "ㅈ테크는 오프도 많죠?",
          "original_form": "ㅈ테크는 오프도 많죠?",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015728.1.20",
          "form": "저는 근데 정모 많이 안좋아해서요ㅎㅎ",
          "original_form": "저는 근데 정모 많이 안좋아해서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015728.1.21",
          "form": "재태크에 관심이 많으신가봐요!",
          "original_form": "재태크에 관심이 많으신가봐요!",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015728.1.22",
          "form": "아니요...ㅋㅋㅋ 님 닉이 부자라서.. ㅋㅋㅋ",
          "original_form": "아니요...ㅋㅋㅋ 님 닉이 부자라서.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015728.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015728.1.24",
          "form": "부자가 되고싶은 마음에",
          "original_form": "부자가 되고싶은 마음에",
