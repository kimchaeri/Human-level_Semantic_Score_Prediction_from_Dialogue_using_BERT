        },
        {
          "id": "MDRW2100015249.1.3",
          "form": "와 오졌다 외길인생",
          "original_form": "와 오졌다 외길인생",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015249.1.4",
          "form": "난 고민도 안해요 오로지 요거트만 담거든요^^",
          "original_form": "난 고민도 안해요 오로지 요거트만 담거든요^^",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015249.1.5",
          "form": "우리 서로 지조있네요",
          "original_form": "우리 서로 지조있네요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015249.1.6",
          "form": "ㄹㅇ 요거트 최애신가바",
          "original_form": "ㄹㅇ 요거트 최애신가바",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015249.1.7",
          "form": "요거트 먹어봐요",
          "original_form": "요거트 먹어봐요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.8",
          "form": "그 혹시 카페가서도",
          "original_form": "그 혹시 카페가서도",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.9",
          "form": "요거트 프라푸치노 이런거 드시나요",
          "original_form": "요거트 프라푸치노 이런거 드시나요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.10",
          "form": "아니요 카페가서는 안먹어요",
          "original_form": "아니요 카페가서는 안먹어요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.11",
          "form": "사람이 간지가있지",
          "original_form": "사람이 간지가있지",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.12",
          "form": "오 배라만 좋아하세요?!",
          "original_form": "오 배라만 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.13",
          "form": "쓰읍..",
          "original_form": "쓰읍..",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.14",
          "form": "오직 배라에서만 요거트",
          "original_form": "오직 배라에서만 요거트",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.15",
          "form": "핏 위해서 슬랙스 입는다는 것부터 알아봤어야..",
          "original_form": "핏 위해서 슬랙스 입는다는 것부터 알아봤어야..",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.16",
          "form": "가오맨이시구나",
          "original_form": "가오맨이시구나",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.17",
          "form": "그럼 아아인가요",
          "original_form": "그럼 아아인가요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.18",
          "form": "얼죽아 회원입니다",
          "original_form": "얼죽아 회원입니다",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015249.1.19",
          "form": "얼죽아 회원이신가요",
          "original_form": "얼죽아 회원이신가요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015249.1.20",
          "form": "당근이죠",
          "original_form": "당근이죠",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015249.1.21",
          "form": "따듯한거에서는 재떨이맛나요",
          "original_form": "따듯한거에서는 재떨이맛나요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015249.1.22",
          "form": "아 ㅇㅈ",
          "original_form": "아 ㅇㅈ",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015249.1.23",
          "form": "우리 2센티 가까워져봐요",
          "original_form": "우리 2센티 가까워져봐요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015249.1.24",
          "form": "오케이.",
          "original_form": "오케이.",
          "speaker_id": "2",
          "time": "20210825 22:26"
