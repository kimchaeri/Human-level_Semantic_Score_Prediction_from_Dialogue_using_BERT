      "utterance": [
        {
          "id": "MDRW2100022656.1.1",
          "form": "점심식사는 하셨나요!?",
          "original_form": "점심식사는 하셨나요!?",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022656.1.2",
          "form": "아니요 늦게 일어나서 아직 안 했어요 ㅋㅋㅋㅋ 슬슬 배고파서 그냥 집에 있는 걸로 대충 먹으려구요 ㅎㅎ name1은 점심 드셨나요~?",
          "original_form": "아니요 늦게 일어나서 아직 안 했어요 ㅋㅋㅋㅋ 슬슬 배고파서 그냥 집에 있는 걸로 대충 먹으려구요 ㅎㅎ &name1&은 점심 드셨나요~?",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022656.1.3",
          "form": "배고프겠어요ㅠㅠ 점저? 네용ㅋㅋ",
          "original_form": "배고프겠어요ㅠㅠ 점저? 네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022656.1.4",
          "form": "네! 저는 칼국수랑 냉면 먹었습니다!!ㅎㅎ",
          "original_form": "네! 저는 칼국수랑 냉면 먹었습니다!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022656.1.5",
          "form": "와악 부럽네요 맛있으셨겠어요!!",
          "original_form": "와악 부럽네요 맛있으셨겠어요!!",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022656.1.6",
          "form": "만두까지 먹어줘야 할 것 같은 조합이네요 ㅋㅋㅋ",
          "original_form": "만두까지 먹어줘야 할 것 같은 조합이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022656.1.7",
          "form": "ㅋㅋㅋㅋ오오 역쉬!!",
          "original_form": "ㅋㅋㅋㅋ오오 역쉬!!",
          "speaker_id": "1",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100022656.1.8",
          "form": "만두도 먹었습니다...ㅎㅎ",
          "original_form": "만두도 먹었습니다...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100022656.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 역시",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 역시",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022656.1.10",
          "form": "먹을줄 아시는군요",
          "original_form": "먹을줄 아시는군요",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022656.1.11",
          "form": "약간 국룰?인가요?!ㅋㅋㅋ",
          "original_form": "약간 국룰?인가요?!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100022656.1.12",
          "form": "저녁은 진짜 오랜만에 소고기먹었눈데 정말 맛있었어용!!",
          "original_form": "저녁은 진짜 오랜만에 소고기먹었눈데 정말 맛있었어용!!",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100022656.1.13",
          "form": "캬 넘 부럽네요",
          "original_form": "캬 넘 부럽네요",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100022656.1.14",
          "form": "저는 귀찮아서 시리얼 먹었어요 ㅋㅋㅋㅋ",
          "original_form": "저는 귀찮아서 시리얼 먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100022656.1.15",
          "form": "근데 성에 안차서 배달 시킬지말지 고민되네요 ㅎㅎ..",
          "original_form": "근데 성에 안차서 배달 시킬지말지 고민되네요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100022656.1.16",
          "form": "시리얼 맛있는데 저녁으로 먹으면 배 안고프세오ㅠ?",
          "original_form": "시리얼 맛있는데 저녁으로 먹으면 배 안고프세오ㅠ?",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100022656.1.17",
          "form": "아앜ㅋㅋㅋ 역시!!",
          "original_form": "아앜ㅋㅋㅋ 역시!!",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100022656.1.18",
          "form": "저는 시리얼만 먹으면 못참을것가탕요ㅠ",
          "original_form": "저는 시리얼만 먹으면 못참을것가탕요ㅠ",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100022656.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 귀찮아서 먹을거 찾다가 대충..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 귀찮아서 먹을거 찾다가 대충..",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100022656.1.20",
          "form": "시리얼은 간식이긴 하죠 ㅎㅎ",
          "original_form": "시리얼은 간식이긴 하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100022656.1.21",
          "form": "ㅋㅋㅋ요즘 날씨가 밥하기 귀찮은 날씨에용ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ요즘 날씨가 밥하기 귀찮은 날씨에용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100022656.1.22",
          "form": "좀 처진다고 해어되나ㅜ?!",
          "original_form": "좀 처진다고 해어되나ㅜ?!",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100022656.1.23",
          "form": "그쵸 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100022656.1.24",
          "form": "하아~~",
          "original_form": "하아~~",
