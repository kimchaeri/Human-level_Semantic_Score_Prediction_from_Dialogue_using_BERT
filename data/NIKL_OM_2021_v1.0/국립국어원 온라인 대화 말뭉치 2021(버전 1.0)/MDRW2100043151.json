      "utterance": [
        {
          "id": "MDRW2100043151.1.1",
          "form": "집안일잘하시나요",
          "original_form": "집안일잘하시나요",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043151.1.2",
          "form": "청소는 진짜 잘해요",
          "original_form": "청소는 진짜 잘해요",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043151.1.3",
          "form": "근데 요리는 진짜 못해요",
          "original_form": "근데 요리는 진짜 못해요",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043151.1.4",
          "form": "못한다고 할까 대충 먹어요 그냥 ㅋㅋㅋ 요리잘하세요?",
          "original_form": "못한다고 할까 대충 먹어요 그냥 ㅋㅋㅋ 요리잘하세요?",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043151.1.5",
          "form": "ㅎㅎ저는요리는안하고",
          "original_form": "ㅎㅎ저는요리는안하고",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043151.1.6",
          "form": "조리정도해요..",
          "original_form": "조리정도해요..",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043151.1.7",
          "form": "샐러드요런거ㅎㅎ",
          "original_form": "샐러드요런거ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043151.1.8",
          "form": "샐러드도 은근 손 많이가잖아요! 채소 다듬고 보관하는게 그렇게 귀찮더라구요 ㅋㅋ",
          "original_form": "샐러드도 은근 손 많이가잖아요! 채소 다듬고 보관하는게 그렇게 귀찮더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043151.1.9",
          "form": "ㅎㅎ맞아요~~",
          "original_form": "ㅎㅎ맞아요~~",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043151.1.10",
          "form": "토핑근데많이올려먹어서좋아요",
          "original_form": "토핑근데많이올려먹어서좋아요",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043151.1.11",
          "form": "어떤 토핑 좋아하세요?",
          "original_form": "어떤 토핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043151.1.12",
          "form": "저는요거트랑과일이요",
          "original_form": "저는요거트랑과일이요",
          "speaker_id": "1",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043151.1.13",
          "form": "오 과일 저도 좋아하는데 건과일도 좋아하세요?",
          "original_form": "오 과일 저도 좋아하는데 건과일도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043151.1.14",
          "form": "망고같은거요~",
          "original_form": "망고같은거요~",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043151.1.15",
          "form": "아 건망고 너무 맛있어요 ㄷㄷ",
          "original_form": "아 건망고 너무 맛있어요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043151.1.16",
          "form": "또뭐좋아하세요",
          "original_form": "또뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043151.1.17",
          "form": "과일은 껍질 쉽게까지는거 좋아해요 ㅋㅋㅋ",
          "original_form": "과일은 껍질 쉽게까지는거 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043151.1.18",
