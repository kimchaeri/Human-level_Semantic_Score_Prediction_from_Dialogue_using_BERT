      "utterance": [
        {
          "id": "MDRW2100031360.1.1",
          "form": "저 갑자기 솜사탕이 먹고싶어요",
          "original_form": "저 갑자기 솜사탕이 먹고싶어요",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031360.1.2",
          "form": "솜사탕이요?",
          "original_form": "솜사탕이요?",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031360.1.3",
          "form": "네 놀이공원 같은데서 파는거요",
          "original_form": "네 놀이공원 같은데서 파는거요",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031360.1.4",
          "form": "아 갑자기 왜요?",
          "original_form": "아 갑자기 왜요?",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031360.1.5",
          "form": "유투브에서 솜사탕 만드는 영상을 봤더니 ㅎㅎ",
          "original_form": "유투브에서 솜사탕 만드는 영상을 봤더니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031360.1.6",
          "form": "아 저는 솜사탕은 안좋아해요",
          "original_form": "아 저는 솜사탕은 안좋아해요",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.7",
          "form": "헉 왜요",
          "original_form": "헉 왜요",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.8",
          "form": "음..너무 달아서..?",
          "original_form": "음..너무 달아서..?",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.9",
          "form": "그럼 놀이공원 음식중에서 뭘 좋아하시나요?",
          "original_form": "그럼 놀이공원 음식중에서 뭘 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.10",
          "form": "저는 츄러스 좋아해요 ㅎㅎ",
          "original_form": "저는 츄러스 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.11",
          "form": "아 저도 츄러스 좋아해요",
          "original_form": "아 저도 츄러스 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.12",
          "form": "츄러스도 달지만..",
          "original_form": "츄러스도 달지만..",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.13",
          "form": "맞아요!ㅋㅋㅋ",
          "original_form": "맞아요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031360.1.14",
          "form": "시나몬 맛을 좋아해서 맛있어요",
