      "utterance": [
        {
          "id": "MDRW2100044886.1.1",
          "form": "영화 어떤 장르 좋아하세요?",
          "original_form": "영화 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044886.1.2",
          "form": "저 판타지 sf요!",
          "original_form": "저 판타지 sf요!",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044886.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044886.1.4",
          "form": "오 저두요 ㅎㅎ",
          "original_form": "오 저두요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044886.1.5",
          "form": "어떤 영화 재밌게 보셨어요?",
          "original_form": "어떤 영화 재밌게 보셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044886.1.6",
          "form": "저 인셉션이요!",
          "original_form": "저 인셉션이요!",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044886.1.7",
          "form": "자각몽을 자주꿔서 진짜 감명깊게 잘봤어요 ㅋㅋㅋㅋ",
          "original_form": "자각몽을 자주꿔서 진짜 감명깊게 잘봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044886.1.8",
          "form": "명작이라곤 하는데 안봐봤어요... 한번봐야겠네요",
          "original_form": "명작이라곤 하는데 안봐봤어요... 한번봐야겠네요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044886.1.9",
          "form": "저는 메이즈러너 재밌게봤어요!",
          "original_form": "저는 메이즈러너 재밌게봤어요!",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044886.1.10",
          "form": "메이즈러너 저도잼게 봤어요 ㅋㅋㅋㅋ긴장감넘치고",
          "original_form": "메이즈러너 저도잼게 봤어요 ㅋㅋㅋㅋ긴장감넘치고",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044886.1.11",
          "form": "뭐라구 보내셨죠..? ㅜㅜ",
          "original_form": "뭐라구 보내셨죠..? ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044886.1.12",
          "form": "렉 걸렸는데 대화내용이 없어져버렸어요..",
          "original_form": "렉 걸렸는데 대화내용이 없어져버렸어요..",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044886.1.13",
          "form": "헉아이구ㅜ",
          "original_form": "헉아이구ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044886.1.14",
          "form": "메이즈러너 저도재밌게봤었다고했어요 ㅎㅎ긴장감넘치구",
          "original_form": "메이즈러너 저도재밌게봤었다고했어요 ㅎㅎ긴장감넘치구",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044886.1.15",
          "form": "감사해요 ㅍㅍ",
          "original_form": "감사해요 ㅍㅍ",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044886.1.16",
          "form": "맞아요 긴장감 넘치게봐서 .. 진짜 가끔 재탕해요 ㅋ",
          "original_form": "맞아요 긴장감 넘치게봐서 .. 진짜 가끔 재탕해요 ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044886.1.17",
          "form": "책도 봤었구요 ㅎㅎ",
          "original_form": "책도 봤었구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044886.1.18",
          "form": "오 책은어때요? 보통책이더잼나다고하던데",
          "original_form": "오 책은어때요? 보통책이더잼나다고하던데",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044886.1.19",
          "form": "맞아요 책이 스토리도 더 있고 재밌더라구요 ㅋㅋ 한번 읽어보시는 것도 좋은 것 같아툐",
          "original_form": "맞아요 책이 스토리도 더 있고 재밌더라구요 ㅋㅋ 한번 읽어보시는 것도 좋은 것 같아툐",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044886.1.20",
          "form": "상상하게되고 묘사가 좋아서 그런가봐요",
          "original_form": "상상하게되고 묘사가 좋아서 그런가봐요",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044886.1.21",
          "form": "유투브 보시는거 있나여",
          "original_form": "유투브 보시는거 있나여",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044886.1.22",
          "form": "저는 먹방 요리레시피 예능클립 많이 보규있어요",
          "original_form": "저는 먹방 요리레시피 예능클립 많이 보규있어요",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044886.1.23",
          "form": "어떤거 보시나요?",
          "original_form": "어떤거 보시나요?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044886.1.24",
          "form": "전 해쭈 예랑가랑 예능클립 일케보는거같아요",
          "original_form": "전 해쭈 예랑가랑 예능클립 일케보는거같아요",
          "speaker_id": "1",
          "time": "20210908 00:29"
