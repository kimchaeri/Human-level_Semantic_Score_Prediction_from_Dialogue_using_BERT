      "utterance": [
        {
          "id": "MDRW2100032764.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032764.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032764.1.3",
          "form": "전 싱글인데 연애중이신가여",
          "original_form": "전 싱글인데 연애중이신가여",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032764.1.4",
          "form": "아니여 ㅋㅋ",
          "original_form": "아니여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032764.1.5",
          "form": "ㅋㅋ 글쿤요 전 결혼생각도 없어서",
          "original_form": "ㅋㅋ 글쿤요 전 결혼생각도 없어서",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032764.1.6",
          "form": "코시국이라 데이트 소개팅 쉽지 않을 것 같어요",
          "original_form": "코시국이라 데이트 소개팅 쉽지 않을 것 같어요",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032764.1.7",
          "form": "그래도 하는 사람은 잘 하던걸요 ㅋㅋ",
          "original_form": "그래도 하는 사람은 잘 하던걸요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032764.1.8",
          "form": "회사나...주변에 없나여?",
          "original_form": "회사나...주변에 없나여?",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032764.1.9",
          "form": "네 관심이 없어서 ㅋㅋㅋ",
          "original_form": "네 관심이 없어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:42"
        },
        {
          "id": "MDRW2100032764.1.10",
          "form": "앗 ㅋㅋ그럼 이상형도 없으신가여?",
          "original_form": "앗 ㅋㅋ그럼 이상형도 없으신가여?",
          "speaker_id": "1",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032764.1.11",
          "form": "아 이상형은 흠",
          "original_form": "아 이상형은 흠",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032764.1.12",
          "form": "생각 좀 해볼게여",
          "original_form": "생각 좀 해볼게여",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032764.1.13",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032764.1.14",
          "form": "흠..저도 크게 생각해본 적은 없는데...",
          "original_form": "흠..저도 크게 생각해본 적은 없는데...",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032764.1.15",
          "form": "대화가 잘 통해야겠죠",
          "original_form": "대화가 잘 통해야겠죠",
          "speaker_id": "2",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032764.1.16",
          "form": "암튼 ㅎㅎ",
          "original_form": "암튼 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032764.1.17",
          "form": "ㅋㅋㅋ그렇죠",
          "original_form": "ㅋㅋㅋ그렇죠",
          "speaker_id": "1",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032764.1.18",
          "form": "성격이 안맞으면 노답이죠",
          "original_form": "성격이 안맞으면 노답이죠",
          "speaker_id": "1",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032764.1.19",
          "form": "어떻게 비혼? 관을 갖게 되엇는지 물어봐도 될가요?..ㅎㅎ",
          "original_form": "어떻게 비혼? 관을 갖게 되엇는지 물어봐도 될가요?..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100032764.1.20",
          "form": "해야된다는 생각이 없는거구요",
          "original_form": "해야된다는 생각이 없는거구요",
          "speaker_id": "2",
          "time": "20210905 23:48"
        },
        {
          "id": "MDRW2100032764.1.21",
          "form": "비혼주의 이건 아니예여",
          "original_form": "비혼주의 이건 아니예여",
          "speaker_id": "2",
