        },
        {
          "id": "MDRW2100045943.1.2",
          "form": "초코송이 좋아하니",
          "original_form": "초코송이 좋아하니",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045943.1.3",
          "form": "좋아하지",
          "original_form": "좋아하지",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045943.1.4",
          "form": "일본에 같은 과자가 있다던데",
          "original_form": "일본에 같은 과자가 있다던데",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.5",
          "form": "정말? 몰랐어",
          "original_form": "정말? 몰랐어",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.6",
          "form": "그러게 말이야",
          "original_form": "그러게 말이야",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.7",
          "form": "초코송이 맛 다양한거 알아?",
          "original_form": "초코송이 맛 다양한거 알아?",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.8",
          "form": "이번에 민트맛 나왔지?",
          "original_form": "이번에 민트맛 나왔지?",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.9",
          "form": "먹었는데 맛있더라",
          "original_form": "먹었는데 맛있더라",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.10",
          "form": "딸기맛도 있었어",
          "original_form": "딸기맛도 있었어",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.11",
          "form": "딸기우유 맛이려나",
          "original_form": "딸기우유 맛이려나",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.12",
          "form": "달달하니 맛있던데",
          "original_form": "달달하니 맛있던데",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045943.1.13",
          "form": "일본엔 팥맛도 있대",
          "original_form": "일본엔 팥맛도 있대",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
