      "utterance": [
        {
          "id": "MDRW2100010772.1.1",
          "form": "알바해보셨어요?",
          "original_form": "알바해보셨어요?",
          "speaker_id": "1",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100010772.1.2",
          "form": "당근이죠 ㅎ",
          "original_form": "당근이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010772.1.3",
          "form": "과자공장도 해봤어요",
          "original_form": "과자공장도 해봤어요",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010772.1.4",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010772.1.5",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010772.1.6",
          "form": "과자공장어때요?",
          "original_form": "과자공장어때요?",
          "speaker_id": "1",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010772.1.7",
          "form": "따끈한 프렌치파이랑",
          "original_form": "따끈한 프렌치파이랑",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.8",
          "form": "오예스랑 엄청맛있어요",
          "original_form": "오예스랑 엄청맛있어요",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.9",
          "form": "해태였어요",
          "original_form": "해태였어요",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.10",
          "form": "헐 방금만든 프렌치파이..",
          "original_form": "헐 방금만든 프렌치파이..",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.11",
          "form": "상상만해도...침",
          "original_form": "상상만해도...침",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.12",
          "form": "침이 ㅋㅋ",
          "original_form": "침이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.13",
          "form": "늘어지더라고요",
          "original_form": "늘어지더라고요",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.14",
          "form": "무슨일하는거예요??",
          "original_form": "무슨일하는거예요??",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.15",
          "form": "헐..너무맛나겠다",
          "original_form": "헐..너무맛나겠다",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.16",
          "form": "오예스는 상자에 담고",
          "original_form": "오예스는 상자에 담고",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010772.1.17",
          "form": "프렌치파이는 딸기잼 안발린거 걸러내고",
          "original_form": "프렌치파이는 딸기잼 안발린거 걸러내고",
          "speaker_id": "2",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010772.1.18",
          "form": "버리는것도 많더라고요 아까워요",
          "original_form": "버리는것도 많더라고요 아까워요",
          "speaker_id": "2",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010772.1.19",
          "form": "헐..버리는거 주워와서 먹고싶어요",
          "original_form": "헐..버리는거 주워와서 먹고싶어요",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010772.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010772.1.21",
          "form": "과자공장 알바한번쯤해보고싶은데요",
          "original_form": "과자공장 알바한번쯤해보고싶은데요",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010772.1.22",
          "form": "쌀푸대로 넘쳐나서 주워먹어도 끝이없더라고요",
          "original_form": "쌀푸대로 넘쳐나서 주워먹어도 끝이없더라고요",
          "speaker_id": "2",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010772.1.23",
          "form": "식사도 엄청맛있어요",
          "original_form": "식사도 엄청맛있어요",
          "speaker_id": "2",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010772.1.24",
          "form": "한번해보세요",
          "original_form": "한번해보세요",
          "speaker_id": "2",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010772.1.25",
          "form": "와 그만큼이나 남나요??대박",
          "original_form": "와 그만큼이나 남나요??대박",
          "speaker_id": "1",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010772.1.26",
          "form": "한번해볼까요ㅋㅋ",
          "original_form": "한번해볼까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010772.1.27",
          "form": "몸만있으면되는건가여",
          "original_form": "몸만있으면되는건가여",
          "speaker_id": "1",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010772.1.28",
          "form": "넹 가까우면 셔틀도있어서 좋아요",
          "original_form": "넹 가까우면 셔틀도있어서 좋아요",
          "speaker_id": "2",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010772.1.29",
          "form": "근데 일이없어서 일주일에 한두번불러주더라고요 ㅜ",
          "original_form": "근데 일이없어서 일주일에 한두번불러주더라고요 ㅜ",
          "speaker_id": "2",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010772.1.30",
          "form": "엥 그래요?ㅠㅠ 정기적으로 일이있는게 아니군요..",
          "original_form": "엥 그래요?ㅠㅠ 정기적으로 일이있는게 아니군요..",
          "speaker_id": "1",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010772.1.31",
          "form": "아쉽아쉽",
          "original_form": "아쉽아쉽",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010772.1.32",
          "form": "알바라 그런가봐요 ㅜ",
          "original_form": "알바라 그런가봐요 ㅜ",
          "speaker_id": "2",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010772.1.33",
          "form": "집근처에 과자공장 있으면 바로해볼텐데말이져",
          "original_form": "집근처에 과자공장 있으면 바로해볼텐데말이져",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010772.1.34",
          "form": "알바말고 견학은간적있어요",
          "original_form": "알바말고 견학은간적있어요",
          "speaker_id": "1",
          "time": "20210823 21:44"
        }
      ]
    }
