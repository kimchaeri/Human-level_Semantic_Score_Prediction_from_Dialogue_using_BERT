      "utterance": [
        {
          "id": "MDRW2100027110.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027110.1.2",
          "form": "전 식혜 좋아요",
          "original_form": "전 식혜 좋아요",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100027110.1.3",
          "form": "저도 식혜 엄청 좋아해요",
          "original_form": "저도 식혜 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100027110.1.4",
          "form": "잣 띄워 먹는거 좋아하세요?",
          "original_form": "잣 띄워 먹는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100027110.1.5",
          "form": "잣은 수정과 아닌가유",
          "original_form": "잣은 수정과 아닌가유",
          "speaker_id": "2",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100027110.1.6",
          "form": "식혜에 띄워먹어도 맛있어요",
          "original_form": "식혜에 띄워먹어도 맛있어요",
          "speaker_id": "1",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100027110.1.7",
          "form": "헉 그렇게는 안 먹어봤어요ㅋ",
          "original_form": "헉 그렇게는 안 먹어봤어요ㅋ",
          "speaker_id": "2",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100027110.1.8",
          "form": "저만 그렇게 먹는거 같긴 해요",
          "original_form": "저만 그렇게 먹는거 같긴 해요",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100027110.1.9",
          "form": "전 커피도 좋아해요.온리 아아",
          "original_form": "전 커피도 좋아해요.온리 아아",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100027110.1.10",
          "form": "극강 아아파이시군요",
          "original_form": "극강 아아파이시군요",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100027110.1.11",
          "form": "저는 쓴것만 아니면 다 마셔요",
          "original_form": "저는 쓴것만 아니면 다 마셔요",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100027110.1.12",
          "form": "헛 그럼 아아 안드시겠네요. 전 카누만ㅋ",
          "original_form": "헛 그럼 아아 안드시겠네요. 전 카누만ㅋ",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100027110.1.13",
          "form": "아아도 시럽 많이 타면 마셔요",
          "original_form": "아아도 시럽 많이 타면 마셔요",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027110.1.14",
          "form": "달달한 것만 드시는군요ㅋ",
          "original_form": "달달한 것만 드시는군요ㅋ",
          "speaker_id": "2",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027110.1.15",
          "form": "쓴건 진짜 안 맞아요ㅠ",
          "original_form": "쓴건 진짜 안 맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100027110.1.16",
          "form": "음 저도 쓴 맛은 싫은데.. 아아는 쓴 느낌이 없달까요",
          "original_form": "음 저도 쓴 맛은 싫은데.. 아아는 쓴 느낌이 없달까요",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027110.1.17",
          "form": "그런가요 제가 잘못 먹었나봐요",
          "original_form": "그런가요 제가 잘못 먹었나봐요",
          "speaker_id": "1",
          "time": "20210902 10:17"
        }
