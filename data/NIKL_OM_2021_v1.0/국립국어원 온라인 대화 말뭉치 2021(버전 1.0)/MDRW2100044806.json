        {
          "id": "MDRW2100044806.1.1",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044806.1.2",
          "form": "어떤 거 사셨어요",
          "original_form": "어떤 거 사셨어요",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044806.1.3",
          "form": "오설록 티 샀어요",
          "original_form": "오설록 티 샀어요",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044806.1.4",
          "form": "아 찾아 가셨군요",
          "original_form": "아 찾아 가셨군요",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044806.1.5",
          "form": "차 사셨군요",
          "original_form": "차 사셨군요",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044806.1.6",
          "form": "네 환절기라 목이 칼칼 한 거 같아서",
          "original_form": "네 환절기라 목이 칼칼 한 거 같아서",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044806.1.7",
          "form": "내 지금이 제일 못 간게 많이 걸려 시간이 시키죠",
          "original_form": "내 지금이 제일 못 간게 많이 걸려 시간이 시키죠",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044806.1.8",
          "form": "목감기 많이 걸",
          "original_form": "목감기 많이 걸",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044806.1.9",
          "form": "오타가 많이 나네요",
          "original_form": "오타가 많이 나네요",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044806.1.10",
          "form": "괜찮아요 저도 오타가 많이 나는 걸요",
          "original_form": "괜찮아요 저도 오타가 많이 나는 걸요",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044806.1.11",
          "form": "목이 많이 아플 수도 있으니 음료를 따뜻하게 해서 드세요",
          "original_form": "목이 많이 아플 수도 있으니 음료를 따뜻하게 해서 드세요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044806.1.12",
          "form": "네 그래야겠어요",
          "original_form": "네 그래야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044806.1.13",
          "form": "유자차나 꿀 물도 좋아요",
          "original_form": "유자차나 꿀 물도 좋아요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044806.1.14",
          "form": "유자차나 꿀 물도 좋아 해요 크크크",
          "original_form": "유자차나 꿀 물도 좋아 해요 크크크",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044806.1.15",
          "form": "자주 드시나요",
          "original_form": "자주 드시나요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044806.1.16",
          "form": "네 좋아해서 여름에도 먹었어요",
