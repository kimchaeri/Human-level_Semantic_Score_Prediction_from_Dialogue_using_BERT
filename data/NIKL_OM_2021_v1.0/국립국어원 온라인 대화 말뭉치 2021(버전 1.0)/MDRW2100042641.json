      "utterance": [
        {
          "id": "MDRW2100042641.1.1",
          "form": "식단조절은 참 어려워요",
          "original_form": "식단조절은 참 어려워요",
          "speaker_id": "1",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042641.1.2",
          "form": "해를 많이 쬐는것도 좋대요",
          "original_form": "해를 많이 쬐는것도 좋대요",
          "speaker_id": "2",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042641.1.3",
          "form": "아 햇빛이요?!!",
          "original_form": "아 햇빛이요?!!",
          "speaker_id": "1",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042641.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042641.1.5",
          "form": "낮에 산책하는게 아토피에 도움이 될거에요",
          "original_form": "낮에 산책하는게 아토피에 도움이 될거에요",
          "speaker_id": "2",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042641.1.6",
          "form": "ㅠ감사해요",
          "original_form": "ㅠ감사해요",
          "speaker_id": "1",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042641.1.7",
          "form": "햇빛이라니ㅠ",
          "original_form": "햇빛이라니ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042641.1.8",
          "form": "ㅎㅎ그래도 이제 많이 선선해졌으니 한번 시도해 보세요",
          "original_form": "ㅎㅎ그래도 이제 많이 선선해졌으니 한번 시도해 보세요",
          "speaker_id": "2",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042641.1.9",
          "form": "네ㅠ그래볼게요!",
          "original_form": "네ㅠ그래볼게요!",
          "speaker_id": "1",
          "time": "20210907 00:14"
        },
        {
          "id": "MDRW2100042641.1.10",
          "form": "혹시 네일아트하세요?",
          "original_form": "혹시 네일아트하세요?",
          "speaker_id": "1",
          "time": "20210907 00:14"
        },
        {
          "id": "MDRW2100042641.1.11",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 00:14"
        },
        {
          "id": "MDRW2100042641.1.12",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210907 00:14"
        },
        {
          "id": "MDRW2100042641.1.13",
          "form": "저도 안해요",
          "original_form": "저도 안해요",
          "speaker_id": "1",
          "time": "20210907 00:15"
        },
        {
          "id": "MDRW2100042641.1.14",
          "form": "네일아트가손톱건강에 안좋다는것같더라고요",
          "original_form": "네일아트가손톱건강에 안좋다는것같더라고요",
          "speaker_id": "1",
          "time": "20210907 00:15"
        },
        {
          "id": "MDRW2100042641.1.15",
          "form": "아 그런가요 ?",
          "original_form": "아 그런가요 ?",
          "speaker_id": "2",
          "time": "20210907 00:16"
        },
        {
          "id": "MDRW2100042641.1.16",
          "form": "네 위급상황일땐",
          "original_form": "네 위급상황일땐",
          "speaker_id": "1",
          "time": "20210907 00:16"
        },
        {
          "id": "MDRW2100042641.1.17",
          "form": "손톱으로 건강 뭘 확인할수있다더라고요",
          "original_form": "손톱으로 건강 뭘 확인할수있다더라고요",
          "speaker_id": "1",
          "time": "20210907 00:16"
        },
        {
          "id": "MDRW2100042641.1.18",
          "form": "아 그런게 있군요",
          "original_form": "아 그런게 있군요",
          "speaker_id": "2",
          "time": "20210907 00:17"
        },
        {
          "id": "MDRW2100042641.1.19",
          "form": "앞으로도 안해야겠네요",
          "original_form": "앞으로도 안해야겠네요",
          "speaker_id": "2",
          "time": "20210907 00:17"
        },
        {
          "id": "MDRW2100042641.1.20",
          "form": "네네!  비추입니다",
          "original_form": "네네!  비추입니다",
          "speaker_id": "1",
