          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035475.1.4",
          "form": "저두요ㅠ 간단하게 먹을지 고민이네여ㅠ",
          "original_form": "저두요ㅠ 간단하게 먹을지 고민이네여ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035475.1.5",
          "form": "왜 간단하게드실생각을,,,?",
          "original_form": "왜 간단하게드실생각을,,,?",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035475.1.6",
          "form": "저녁은 넘치도록,,",
          "original_form": "저녁은 넘치도록,,",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035475.1.7",
          "form": "ㅋㅋㅋㅋ 그것도 그렇네요, 배부르게 먹을 메뉴를 고민해겠어요 ㅣㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그것도 그렇네요, 배부르게 먹을 메뉴를 고민해겠어요 ㅣㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035475.1.8",
          "form": "저는 피자나 돈까스",
          "original_form": "저는 피자나 돈까스",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035475.1.9",
          "form": "먹고싶어요",
          "original_form": "먹고싶어요",
          "speaker_id": "1",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035475.1.10",
          "form": "저는 김치찌개나 햄버거요ㅠ",
          "original_form": "저는 김치찌개나 햄버거요ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035475.1.11",
          "form": "저  점심 김찌먹었어요",
          "original_form": "저  점심 김찌먹었어요",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100035475.1.12",
          "form": "김치찌개는 언제나 옳죠",
          "original_form": "김치찌개는 언제나 옳죠",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100035475.1.13",
          "form": "저녁도 김찌먹어야겠어요",
          "original_form": "저녁도 김찌먹어야겠어요",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100035475.1.14",
          "form": "일주일동안 먹어도 안 질릴 것 같아요ㅋㅋ",
