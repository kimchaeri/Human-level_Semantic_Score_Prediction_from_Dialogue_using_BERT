      "utterance": [
        {
          "id": "MDRW2100000811.1.1",
          "form": "저녁에 불고기 덮밥먹으려했는데 메뉴바뀌었어요",
          "original_form": "저녁에 불고기 덮밥먹으려했는데 메뉴바뀌었어요",
          "speaker_id": "1",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000811.1.2",
          "form": "애들이 배달음식먹고 싶다고 순살치킨시켰어요",
          "original_form": "애들이 배달음식먹고 싶다고 순살치킨시켰어요",
          "speaker_id": "1",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000811.1.3",
          "form": "오우 맛나겠어요 ㅎㅎ",
          "original_form": "오우 맛나겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000811.1.4",
          "form": "주로 어디서 시켜드세요",
          "original_form": "주로 어디서 시켜드세요",
          "speaker_id": "2",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000811.1.5",
          "form": "저흰 치킨배달은  비비큐는 황금올리브시키구요",
          "original_form": "저흰 치킨배달은  비비큐는 황금올리브시키구요",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000811.1.6",
          "form": "양념은 멕시카나 땡초치킨 배달시켜요",
          "original_form": "양념은 멕시카나 땡초치킨 배달시켜요",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000811.1.7",
          "form": "저희는 두마리 치킨 주로 시켜먹고요",
          "original_form": "저희는 두마리 치킨 주로 시켜먹고요",
          "speaker_id": "2",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000811.1.8",
          "form": "비비큐 황금올리브 주로 많이 시켜먹는데요",
          "original_form": "비비큐 황금올리브 주로 많이 시켜먹는데요",
          "speaker_id": "2",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000811.1.9",
          "form": "요즘 가족들 수가 적어서 한마리 시키면 이틀에 먹는것 같아요",
          "original_form": "요즘 가족들 수가 적어서 한마리 시키면 이틀에 먹는것 같아요",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000811.1.10",
          "form": "밥반찬으로 먹는거라 땡초치킨순살을 자주 주문하게 되는거 같아요",
          "original_form": "밥반찬으로 먹는거라 땡초치킨순살을 자주 주문하게 되는거 같아요",
          "speaker_id": "1",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000811.1.11",
          "form": "고기를  많이 못먹어서 아쉬워요",
          "original_form": "고기를  많이 못먹어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000811.1.12",
          "form": "땡초치킨순살은 샐러드도 같이 오나요?",
          "original_form": "땡초치킨순살은 샐러드도 같이 오나요?",
          "speaker_id": "2",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000811.1.13",
          "form": "맵기가 딱 제 스타일이라",
          "original_form": "맵기가 딱 제 스타일이라",
          "speaker_id": "1",
          "time": "20210819 19:44"
        },
        {
          "id": "MDRW2100000811.1.14",
          "form": "매운 통닭 잘드시나봐요?",
          "original_form": "매운 통닭 잘드시나봐요?",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000811.1.15",
          "form": "저는 매운것 거의 못먹고요",
          "original_form": "저는 매운것 거의 못먹고요",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000811.1.16",
          "form": "가끔은 매운게 좋아요",
          "original_form": "가끔은 매운게 좋아요",
          "speaker_id": "1",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000811.1.17",
          "form": "달달한 치킨맛 주로 먹습니다",
          "original_form": "달달한 치킨맛 주로 먹습니다",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000811.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000811.1.19",
          "form": "맥주는 한잔 드세요?",
          "original_form": "맥주는 한잔 드세요?",
          "speaker_id": "2",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000811.1.20",
          "form": "저는 술은 못먹어서요",
          "original_form": "저는 술은 못먹어서요",
          "speaker_id": "2",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000811.1.21",
          "form": "피자는 어디것 맛나요?",
          "original_form": "피자는 어디것 맛나요?",
          "speaker_id": "2",
          "time": "20210819 19:47"
        },
        {
          "id": "MDRW2100000811.1.22",
          "form": "지금 맥주에 치맥 피맥하고 있어요",
          "original_form": "지금 맥주에 치맥 피맥하고 있어요",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000811.1.23",
          "form": "도미노피자요",
          "original_form": "도미노피자요",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000811.1.24",
          "form": "저도요 도미노피자 최고최고예요",
          "original_form": "저도요 도미노피자 최고최고예요",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000811.1.25",
          "form": "세일도 자주해서",
          "original_form": "세일도 자주해서",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000811.1.26",
          "form": "주로 포장해와서요",
          "original_form": "주로 포장해와서요",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000811.1.27",
          "form": "그쳐 블랙쉬림프피자가 제일 좋아요",
          "original_form": "그쳐 블랙쉬림프피자가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000811.1.28",
          "form": "저희두 세일할때 포장해와요",
          "original_form": "저희두 세일할때 포장해와요",
          "speaker_id": "1",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000811.1.29",
          "form": "오우 저랑 비슷비슷해요",
          "original_form": "오우 저랑 비슷비슷해요",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000811.1.30",
          "form": "40프로 세일 자주해서 완전 좋은것 같아요",
          "original_form": "40프로 세일 자주해서 완전 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000811.1.31",
          "form": "현장할인 맞죠?",
          "original_form": "현장할인 맞죠?",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000811.1.32",
          "form": "찜닭은 인동 찜닭이!!! 매콤하고 달달한 맛이 맛나다고요 ^^",
          "original_form": "찜닭은 인동 찜닭이!!! 매콤하고 달달한 맛이 맛나다고요 ^^",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000811.1.33",
          "form": "찜닭 직접 자주만들어 드시죠?",
          "original_form": "찜닭 직접 자주만들어 드시죠?",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000811.1.34",
          "form": "워낙 솜씨가 좋으시니깐요",
          "original_form": "워낙 솜씨가 좋으시니깐요",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000811.1.35",
          "form": "찜닭 식재 주로 뭐 넣어세요?",
          "original_form": "찜닭 식재 주로 뭐 넣어세요?",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000811.1.36",
          "form": "감자 당면 파 양파 ?",
          "original_form": "감자 당면 파 양파 ?",
          "speaker_id": "2",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100000811.1.37",
          "form": "안동찜닭 좋져",
          "original_form": "안동찜닭 좋져",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000811.1.38",
          "form": "감자 당면 파 양파 버섯넣어요",
          "original_form": "감자 당면 파 양파 버섯넣어요",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000811.1.39",
          "form": "가끔 만들어 먹는데요 ㅎㅎ",
          "original_form": "가끔 만들어 먹는데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000811.1.40",
          "form": "매번 맛차이가 나요 ㅎ",
          "original_form": "매번 맛차이가 나요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000811.1.41",
          "form": "어떨땐 맛나고요",
          "original_form": "어떨땐 맛나고요",
          "speaker_id": "2",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000811.1.42",
          "form": "닭 싱싱한 차이?",
          "original_form": "닭 싱싱한 차이?",
          "speaker_id": "2",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000811.1.43",
          "form": "졸여지는 시간??",
          "original_form": "졸여지는 시간??",
          "speaker_id": "1",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000811.1.44",
          "form": "닭이 신선한가 차이두 없지않아있어요",
          "original_form": "닭이 신선한가 차이두 없지않아있어요",
          "speaker_id": "1",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000811.1.45",
          "form": "매번 차이가 좀 나서요 ㅎ",
          "original_form": "매번 차이가 좀 나서요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000811.1.46",
          "form": "닭도리탕이랑 찜닭 차이가 뭔가요?",
          "original_form": "닭도리탕이랑 찜닭 차이가 뭔가요?",
          "speaker_id": "2",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000811.1.47",
          "form": "그쳐 저두  항상 고민되는 질문이요",
          "original_form": "그쳐 저두  항상 고민되는 질문이요",
          "speaker_id": "1",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000811.1.48",
          "form": "찜닭은 국물이 없는거라고 하던데 맞나요",
          "original_form": "찜닭은 국물이 없는거라고 하던데 맞나요",
          "speaker_id": "1",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000811.1.49",
          "form": "저는 찜닭이라도 이야기하는데요",
          "original_form": "저는 찜닭이라도 이야기하는데요",
          "speaker_id": "2",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000811.1.50",
          "form": "동네마다 지역마다 다른가봐요?",
          "original_form": "동네마다 지역마다 다른가봐요?",
          "speaker_id": "2",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000811.1.51",
          "form": "닭도리탕 아하 국물이 있어서 탕?",
          "original_form": "닭도리탕 아하 국물이 있어서 탕?",
          "speaker_id": "2",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000811.1.52",
          "form": "부르기 나름인거 같아요",
          "original_form": "부르기 나름인거 같아요",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000811.1.53",
          "form": "오늘에서야 명확하게 알게되었네요 ㅎㅎ",
          "original_form": "오늘에서야 명확하게 알게되었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000811.1.54",
          "form": "덕분에 알게되었어요",
          "original_form": "덕분에 알게되었어요",
