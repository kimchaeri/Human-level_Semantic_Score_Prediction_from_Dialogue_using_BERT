      "utterance": [
        {
          "id": "MDRW2100028078.1.1",
          "form": "군단장 대회에서 1등해서 표창을 받았고 백지휴가증을 받았는데",
          "original_form": "군단장 대회에서 1등해서 표창을 받았고 백지휴가증을 받았는데",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028078.1.2",
          "form": "그 알죠?",
          "original_form": "그 알죠?",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028078.1.3",
          "form": "백지휴가!!!!????",
          "original_form": "백지휴가!!!!????",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028078.1.4",
          "form": "백지수표처럼?",
          "original_form": "백지수표처럼?",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028078.1.5",
          "form": "멀 1등햇어요 근데",
          "original_form": "멀 1등햇어요 근데",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028078.1.6",
          "form": "그런게잇어요?",
          "original_form": "그런게잇어요?",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028078.1.7",
          "form": "포반장교육대에서",
          "original_form": "포반장교육대에서",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028078.1.8",
          "form": "이론을 겸비하고",
          "original_form": "이론을 겸비하고",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.9",
          "form": "실전 업무능력을 배양해서",
          "original_form": "실전 업무능력을 배양해서",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.10",
          "form": "시험도 치고했져",
          "original_form": "시험도 치고했져",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.11",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.12",
          "form": "거기서 백지휴가를 받았는데",
          "original_form": "거기서 백지휴가를 받았는데",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.13",
          "form": "그거1등하셧군",
          "original_form": "그거1등하셧군",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.14",
          "form": "근데용 ?",
          "original_form": "근데용 ?",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.15",
          "form": "제가 한달썼는데",
          "original_form": "제가 한달썼는데",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028078.1.16",
          "form": "와 그게가능한가요",
          "original_form": "와 그게가능한가요",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100028078.1.17",
          "form": "행보관 아세요?",
          "original_form": "행보관 아세요?",
          "speaker_id": "2",
          "time": "20210902 17:29"
        },
