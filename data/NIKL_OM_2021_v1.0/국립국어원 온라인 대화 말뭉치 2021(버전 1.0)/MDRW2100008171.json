        },
        {
          "id": "MDRW2100008171.1.2",
          "form": "인터넷 쇼핑은 주로 어디서 하나요",
          "original_form": "인터넷 쇼핑은 주로 어디서 하나요",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.3",
          "form": "네이버요",
          "original_form": "네이버요",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.4",
          "form": "네이버페이모아야돼",
          "original_form": "네이버페이모아야돼",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.5",
          "form": "전 11번가요",
          "original_form": "전 11번가요",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.6",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.7",
          "form": "네이버페이 모으면 쏠쏠하지",
          "original_form": "네이버페이 모으면 쏠쏠하지",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.8",
          "form": "그냥 가끔 쿠폰쓰면 싸요",
          "original_form": "그냥 가끔 쿠폰쓰면 싸요",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.9",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008171.1.10",
          "form": "쿠팡안씀?",
          "original_form": "쿠팡안씀?",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008171.1.11",
          "form": "쿠팡은 잘 안씀",
          "original_form": "쿠팡은 잘 안씀",
          "speaker_id": "1",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008171.1.12",
          "form": "불편해",
          "original_form": "불편해",
          "speaker_id": "1",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008171.1.13",
          "form": "로켓배송좋은데",
          "original_form": "로켓배송좋은데",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008171.1.14",
          "form": "쿠팡은 직구할 때 쓰면 좋아요",
          "original_form": "쿠팡은 직구할 때 쓰면 좋아요",
          "speaker_id": "1",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008171.1.15",
          "form": "쿠팡직구 영양제좋아",
          "original_form": "쿠팡직구 영양제좋아",
          "speaker_id": "2",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100008171.1.16",
          "form": "...?",
          "original_form": "...?",
          "speaker_id": "1",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100008171.1.17",
          "form": "배송이 빨라서 좋음",
          "original_form": "배송이 빨라서 좋음",
          "speaker_id": "1",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100008171.1.18",
          "form": "이번에 당근에서 중고거래 하려고 했는데",
          "original_form": "이번에 당근에서 중고거래 하려고 했는데",
