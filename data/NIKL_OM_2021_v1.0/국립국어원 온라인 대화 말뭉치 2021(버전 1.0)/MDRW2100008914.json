        {
          "id": "MDRW2100008914.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008914.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008914.1.3",
          "form": "근데 못간지 너무 오래됐어요ㅜ",
          "original_form": "근데 못간지 너무 오래됐어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008914.1.4",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008914.1.5",
          "form": "최근 어디 다녀오셨어요?",
          "original_form": "최근 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008914.1.6",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "1",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100008914.1.7",
          "form": "저는 부산 갈까했는데",
          "original_form": "저는 부산 갈까했는데",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100008914.1.8",
          "form": "포기했어요ㅜ",
          "original_form": "포기했어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100008914.1.9",
          "form": "부산심하죠?",
          "original_form": "부산심하죠?",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100008914.1.10",
          "form": "4단계라길래 괜히 갔다가 사달날까 싶더라고요",
          "original_form": "4단계라길래 괜히 갔다가 사달날까 싶더라고요",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008914.1.11",
          "form": "네 겁나여",
          "original_form": "네 겁나여",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100008914.1.12",
          "form": "전 그래서 그냥 동네 걸어요 ㅋㅋ",
          "original_form": "전 그래서 그냥 동네 걸어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100008914.1.13",
          "form": "어디 가고 싶은데 있어요?",
          "original_form": "어디 가고 싶은데 있어요?",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100008914.1.14",
          "form": "해외라면 가까운 일본이라도 가고싶어요 ㅋㅋ",
          "original_form": "해외라면 가까운 일본이라도 가고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100008914.1.15",
          "form": "방사능 위험 은요",
          "original_form": "방사능 위험 은요",
