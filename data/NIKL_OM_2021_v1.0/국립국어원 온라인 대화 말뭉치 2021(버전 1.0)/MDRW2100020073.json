      "utterance": [
        {
          "id": "MDRW2100020073.1.1",
          "form": "저녁식사하셨어요?",
          "original_form": "저녁식사하셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020073.1.2",
          "form": "이제 대하랑 고구마철이라 기대됩니다",
          "original_form": "이제 대하랑 고구마철이라 기대됩니다",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020073.1.3",
          "form": "아...대하 너무 좋아했었는데",
          "original_form": "아...대하 너무 좋아했었는데",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020073.1.4",
          "form": "저녁은 배가고프지 않아서 먹지 않았어요",
          "original_form": "저녁은 배가고프지 않아서 먹지 않았어요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020073.1.5",
          "form": "결혼하고 아이낳고는 체질이ㅇ바껴서 ㅜㅠ",
          "original_form": "결혼하고 아이낳고는 체질이ㅇ바껴서 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020073.1.6",
          "form": "새우먹으니 알러지반응이 나더라구요",
          "original_form": "새우먹으니 알러지반응이 나더라구요",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020073.1.7",
          "form": "이젠 새우넣은 순두부도 못먹어요",
          "original_form": "이젠 새우넣은 순두부도 못먹어요",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020073.1.8",
          "form": "아이고ㅠㅠ 아쉽네요",
          "original_form": "아이고ㅠㅠ 아쉽네요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020073.1.9",
          "form": "신랑이 대하로 저 꼬셨는디 ㅋ",
          "original_form": "신랑이 대하로 저 꼬셨는디 ㅋ",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020073.1.10",
          "form": "넘어가셨군요 호호",
          "original_form": "넘어가셨군요 호호",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020073.1.11",
          "form": "새우까주는 모습에 ㅋㅋ",
          "original_form": "새우까주는 모습에 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020073.1.12",
          "form": "며칠전에 고구마를 샀는데",
          "original_form": "며칠전에 고구마를 샀는데",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020073.1.13",
          "form": "고구마랑 감자를 좋아하는데 곧 고구마 철이니까 단고구마 먹을 수 있지않나 싶어요",
          "original_form": "고구마랑 감자를 좋아하는데 곧 고구마 철이니까 단고구마 먹을 수 있지않나 싶어요",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020073.1.14",
          "form": "너무 맛이 없더라구요",
          "original_form": "너무 맛이 없더라구요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020073.1.15",
          "form": "아직 아닌건지...ㅜㅠ\n저걸 어찌 다먹을지 걱정이예요",
          "original_form": "아직 아닌건지...ㅜㅠ\n저걸 어찌 다먹을지 걱정이예요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020073.1.16",
          "form": "생크림이랑 조금 섞어드시는건 어떠세요?",
          "original_form": "생크림이랑 조금 섞어드시는건 어떠세요?",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020073.1.17",
          "form": "겨울엔 고구마죠!!",
          "original_form": "겨울엔 고구마죠!!",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020073.1.18",
          "form": "앗..,그런방법이 있어요?",
          "original_form": "앗..,그런방법이 있어요?",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020073.1.19",
          "form": "그러고보니 아웃백에서 메뉴로 나오는거아닌가요?",
          "original_form": "그러고보니 아웃백에서 메뉴로 나오는거아닌가요?",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020073.1.20",
          "form": "아웃백 안가본지 오래라 모르겠어요",
          "original_form": "아웃백 안가본지 오래라 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020073.1.21",
          "form": "그생각을 못했네요",
          "original_form": "그생각을 못했네요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020073.1.22",
          "form": "생크림이랑 고구마랑 섞어서 먹어봤는데 고구마 케이크 느낌이라 좋았어요",
          "original_form": "생크림이랑 고구마랑 섞어서 먹어봤는데 고구마 케이크 느낌이라 좋았어요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020073.1.23",
          "form": "생크림팁 고마워요",
          "original_form": "생크림팁 고마워요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        }
      ]
    }
