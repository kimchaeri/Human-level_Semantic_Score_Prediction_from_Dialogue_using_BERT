      "utterance": [
        {
          "id": "MDRW2100032644.1.1",
          "form": "아침은 간단히 시리얼 먹었어요",
          "original_form": "아침은 간단히 시리얼 먹었어요",
          "speaker_id": "2",
          "time": "20210906 08:30"
        },
        {
          "id": "MDRW2100032644.1.2",
          "form": "일어나셨나용",
          "original_form": "일어나셨나용",
          "speaker_id": "2",
          "time": "20210906 08:30"
        },
        {
          "id": "MDRW2100032644.1.3",
          "form": "전 아침 못먹었네요",
          "original_form": "전 아침 못먹었네요",
          "speaker_id": "1",
          "time": "20210906 11:11"
        },
        {
          "id": "MDRW2100032644.1.4",
          "form": "아ㅜ",
          "original_form": "아ㅜ",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100032644.1.5",
          "form": "저는 운동가기전이라 먹었네요ㅠ",
          "original_form": "저는 운동가기전이라 먹었네요ㅠ",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100032644.1.6",
          "form": "원래 출근할땐 못먹지만",
          "original_form": "원래 출근할땐 못먹지만",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100032644.1.7",
          "form": "곧 점심이네요",
          "original_form": "곧 점심이네요",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100032644.1.8",
          "form": "점심은 맛있는거 드셔요!!",
          "original_form": "점심은 맛있는거 드셔요!!",
          "speaker_id": "2",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100032644.1.9",
          "form": "네 지금점심 뭐 먹을지 고민중이예요",
          "original_form": "네 지금점심 뭐 먹을지 고민중이예요",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100032644.1.10",
          "form": "오~ 뭐 고민중이세요?",
          "original_form": "오~ 뭐 고민중이세요?",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100032644.1.11",
          "form": "게살파스타먹었어요",
          "original_form": "게살파스타먹었어요",
          "speaker_id": "1",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100032644.1.12",
          "form": "배불배불",
          "original_form": "배불배불",
          "speaker_id": "1",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100032644.1.13",
          "form": "name2님은 뭐드셨어요?",
          "original_form": "&name2&님은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100032644.1.14",
          "form": "와 ㅋㅋㅋ 맛있겠네요!!!",
          "original_form": "와 ㅋㅋㅋ 맛있겠네요!!!",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100032644.1.15",
          "form": "저는 닭꼬치먹고 롤케이크 조금 먹었어요ㅋ 조합이 안맞죠",
          "original_form": "저는 닭꼬치먹고 롤케이크 조금 먹었어요ㅋ 조합이 안맞죠",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100032644.1.16",
          "form": "술안주와 디저트 ㅋㅋ",
          "original_form": "술안주와 디저트 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100032644.1.17",
          "form": "음료는?",
          "original_form": "음료는?",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100032644.1.18",
          "form": "커피먹었어요ㅋㅋㅋㅋㅋ아아~",
          "original_form": "커피먹었어요ㅋㅋㅋㅋㅋ아아~",
          "speaker_id": "2",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100032644.1.19",
          "form": "올만에 밖에서 일 좀 보고왔더니 지치더라고용",
          "original_form": "올만에 밖에서 일 좀 보고왔더니 지치더라고용",
          "speaker_id": "2",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100032644.1.20",
          "form": "마자요 안하다 외근나가면 피곤하더라구요",
          "original_form": "마자요 안하다 외근나가면 피곤하더라구요",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100032644.1.21",
          "form": "시간은 진짜 빨리가는데",
          "original_form": "시간은 진짜 빨리가는데",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100032644.1.22",
          "form": "그쵸 ㅎ 시간은 훅훅 가요",
          "original_form": "그쵸 ㅎ 시간은 훅훅 가요",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100032644.1.23",
          "form": "오늘 병가내서 병원갔다가 밀린일봤거든요 ㅎ",
          "original_form": "오늘 병가내서 병원갔다가 밀린일봤거든요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100032644.1.24",
          "form": "아까 비왔다고 하던데",
          "original_form": "아까 비왔다고 하던데",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100032644.1.25",
          "form": "비 안맞으셨죠?",
          "original_form": "비 안맞으셨죠?",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100032644.1.26",
          "form": "네네 ㅎ",
          "original_form": "네네 ㅎ",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100032644.1.27",
          "form": "근데 오늘 비를 맞네요 ㅎㅎ",
          "original_form": "근데 오늘 비를 맞네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100032644.1.28",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "2",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100032644.1.29",
          "form": "? 오늘은 죙일 올 듯한데",
          "original_form": "? 오늘은 죙일 올 듯한데",
          "speaker_id": "1",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100032644.1.30",
          "form": "그쵸 내일까지 온다더라고용",
          "original_form": "그쵸 내일까지 온다더라고용",
          "speaker_id": "2",
          "time": "20210907 11:04"
        },
        {
          "id": "MDRW2100032644.1.31",
          "form": "점심은 뭐 드실건가요~?",
          "original_form": "점심은 뭐 드실건가요~?",
          "speaker_id": "2",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100032644.1.32",
          "form": "칼국수에 만두?",
          "original_form": "칼국수에 만두?",
          "speaker_id": "1",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100032644.1.33",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100032644.1.34",
          "form": "비오는날에 딱 맞는 메뉴네요 크으",
          "original_form": "비오는날에 딱 맞는 메뉴네요 크으",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100032644.1.35",
          "form": "외근이있어서 아침을 먹고오려했는데 못먹었더니 기운이 없네여 ㅜ",
          "original_form": "외근이있어서 아침을 먹고오려했는데 못먹었더니 기운이 없네여 ㅜ",
