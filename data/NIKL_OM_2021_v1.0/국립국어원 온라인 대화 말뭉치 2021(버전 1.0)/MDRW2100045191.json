      "utterance": [
        {
          "id": "MDRW2100045191.1.1",
          "form": "어제 뭐 드셨나요",
          "original_form": "어제 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045191.1.2",
          "form": "저는 아침은 아아 먹었습니다",
          "original_form": "저는 아침은 아아 먹었습니다",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045191.1.3",
          "form": "저는 그냥 굶네요",
          "original_form": "저는 그냥 굶네요",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045191.1.4",
          "form": "헉 ... 점심은 드셨지요",
          "original_form": "헉 ... 점심은 드셨지요",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045191.1.5",
          "form": "구내식당에서 먹은듯요",
          "original_form": "구내식당에서 먹은듯요",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045191.1.6",
          "form": "저는 도시락ㄴ이요",
          "original_form": "저는 도시락ㄴ이요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045191.1.7",
          "form": "무슨 반찬드셨나요",
          "original_form": "무슨 반찬드셨나요",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045191.1.8",
          "form": "친구 어머님 반찬 싸갔네요",
          "original_form": "친구 어머님 반찬 싸갔네요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045191.1.9",
          "form": "연근등",
          "original_form": "연근등",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045191.1.10",
          "form": "저녁은 머드셨나요",
          "original_form": "저녁은 머드셨나요",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045191.1.11",
          "form": "간단하게 새우 볶음밥 ㅎㅎ",
          "original_form": "간단하게 새우 볶음밥 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045191.1.12",
          "form": "저는 치킨시킬까하다",
          "original_form": "저는 치킨시킬까하다",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045191.1.13",
          "form": "그냥 라면으로 떼운..",
          "original_form": "그냥 라면으로 떼운..",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045191.1.14",
          "form": "저녁은 잘드셔야죠 ㅜㅜ",
          "original_form": "저녁은 잘드셔야죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045191.1.15",
          "form": "그래야하는데 귀찮네요",
          "original_form": "그래야하는데 귀찮네요",
          "speaker_id": "1",
