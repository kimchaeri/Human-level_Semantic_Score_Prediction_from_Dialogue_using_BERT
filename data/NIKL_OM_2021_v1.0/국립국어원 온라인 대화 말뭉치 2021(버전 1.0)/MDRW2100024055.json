      "utterance": [
        {
          "id": "MDRW2100024055.1.1",
          "form": "안녕하세요? 식사는 하셨나요?",
          "original_form": "안녕하세요? 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024055.1.2",
          "form": "저녁 식사는 아주 간다하개 햏어요!!",
          "original_form": "저녁 식사는 아주 간다하개 햏어요!!",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024055.1.3",
          "form": "저녁애 막있는거 드셨너요?",
          "original_form": "저녁애 막있는거 드셨너요?",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024055.1.4",
          "form": "저도 간단히 해서 사실 할 말은 별로 없어요 ㅋㅋ",
          "original_form": "저도 간단히 해서 사실 할 말은 별로 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100024055.1.5",
          "form": "오늘은 아침에 복숭아 한개 먹고 바로 나왔고",
          "original_form": "오늘은 아침에 복숭아 한개 먹고 바로 나왔고",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100024055.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210831 13:02"
        },
        {
          "id": "MDRW2100024055.1.7",
          "form": "점심은 어떤거 드셨어요?",
          "original_form": "점심은 어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100024055.1.8",
          "form": "저는 마라탕 먹었습니다!",
          "original_form": "저는 마라탕 먹었습니다!",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100024055.1.9",
          "form": "와 저 마라탕을 참 좋아해요",
          "original_form": "와 저 마라탕을 참 좋아해요",
          "speaker_id": "1",
          "time": "20210831 20:47"
        },
        {
          "id": "MDRW2100024055.1.10",
          "form": "전 샌드위치를 먹었어요",
          "original_form": "전 샌드위치를 먹었어요",
          "speaker_id": "1",
          "time": "20210831 20:47"
        },
        {
          "id": "MDRW2100024055.1.11",
          "form": "마라탕을 먹고 싶네요 저렴하고 고기야채 다있고",
          "original_form": "마라탕을 먹고 싶네요 저렴하고 고기야채 다있고",
          "speaker_id": "1",
          "time": "20210831 20:47"
        },
        {
          "id": "MDRW2100024055.1.12",
          "form": "저도 마라탕 진짜 좋아해요",
          "original_form": "저도 마라탕 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024055.1.13",
          "form": "마라탕이 좋으세요 마라샹궈가 좋으세요?",
          "original_form": "마라탕이 좋으세요 마라샹궈가 좋으세요?",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024055.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024055.1.15",
          "form": "아,, 전 샹궈도 좋지만 탕은 건져서 매운 양념을 씻어먹음 되는데",
          "original_form": "아,, 전 샹궈도 좋지만 탕은 건져서 매운 양념을 씻어먹음 되는데",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024055.1.16",
          "form": "샹궈는 좀 매워요",
          "original_form": "샹궈는 좀 매워요",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024055.1.17",
          "form": "헐 전 샹궈를 더 좋아해요!",
          "original_form": "헐 전 샹궈를 더 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100024055.1.18",
          "form": "맵찔이지만 샹궈를 더 사랑해요",
          "original_form": "맵찔이지만 샹궈를 더 사랑해요",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100024055.1.19",
          "form": "아 그러시군요 전 샹궈를 좋아는 하는데,, 음,, 그래도 여러시 모이면 샹궈를 갈것 같긴 해요 마라탕은 혼자서 먹을 수 ㅇ있으니까요",
          "original_form": "아 그러시군요 전 샹궈를 좋아는 하는데,, 음,, 그래도 여러시 모이면 샹궈를 갈것 같긴 해요 마라탕은 혼자서 먹을 수 ㅇ있으니까요",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024055.1.20",
          "form": "저 잠시만 설거지 좀 하고 올께요 ㅋㅋ 죄송",
          "original_form": "저 잠시만 설거지 좀 하고 올께요 ㅋㅋ 죄송",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024055.1.21",
          "form": "앗 그럼 저도 얼른 샤워를..!!",
          "original_form": "앗 그럼 저도 얼른 샤워를..!!",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024055.1.22",
          "form": "샹궈에선 어떤 재료를 좋아하시나요?",
          "original_form": "샹궈에선 어떤 재료를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 23:23"
        },
        {
          "id": "MDRW2100024055.1.23",
          "form": "전 샹궈서는 고기보다도 감자같은 소박한 재료가 은근 맛있었어요",
          "original_form": "전 샹궈서는 고기보다도 감자같은 소박한 재료가 은근 맛있었어요",
          "speaker_id": "1",
          "time": "20210831 23:23"
        },
        {
          "id": "MDRW2100024055.1.24",
          "form": "헐 저두요 감자 젤 좋어해요",
          "original_form": "헐 저두요 감자 젤 좋어해요",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100024055.1.25",
          "form": "아니면 라면추가해서 드셔보셨어요?",
          "original_form": "아니면 라면추가해서 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100024055.1.26",
          "form": "전 라면은 좀 배차는 느낌이구 집에도 많아서 잘 안먹구",
          "original_form": "전 라면은 좀 배차는 느낌이구 집에도 많아서 잘 안먹구",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100024055.1.27",
          "form": "감자가 다른 고기나 소세지 맛배여서 포실포실해진걸",
          "original_form": "감자가 다른 고기나 소세지 맛배여서 포실포실해진걸",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100024055.1.28",
          "form": "잘먹습니다ㅋㅋㅋ",
          "original_form": "잘먹습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100024055.1.29",
          "form": "아하! 저도 비슷한 느낌을 좋라해요!!",
          "original_form": "아하! 저도 비슷한 느낌을 좋라해요!!",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100024055.1.30",
          "form": "오늫은 어떤거 드셨엉ㅅ?",
          "original_form": "오늫은 어떤거 드셨엉ㅅ?",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100024055.1.31",
          "form": "저는 오늘 인도커리 먹었는데",
          "original_form": "저는 오늘 인도커리 먹었는데",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100024055.1.32",
          "form": "사진을 못보내는게 한이네요 ㅠㅠ",
          "original_form": "사진을 못보내는게 한이네요 ㅠㅠ",
