      "utterance": [
        {
          "id": "MDRW2100024145.1.1",
          "form": "지금은 군대가있져?",
          "original_form": "지금은 군대가있져?",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024145.1.2",
          "form": "요즘 박보검 군대 갔죠?",
          "original_form": "요즘 박보검 군대 갔죠?",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024145.1.3",
          "form": "엌ㅋㅋ그래서 최근에 잠깐 갈아탔습니다.",
          "original_form": "엌ㅋㅋ그래서 최근에 잠깐 갈아탔습니다.",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024145.1.4",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.5",
          "form": "최근엔 누구요",
          "original_form": "최근엔 누구요",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.6",
          "form": "남주혁이요 ㅜㅜ",
          "original_form": "남주혁이요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.7",
          "form": "아 남주혁두!!",
          "original_form": "아 남주혁두!!",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.8",
          "form": "느낌이 깔끔하고 좋아요",
          "original_form": "느낌이 깔끔하고 좋아요",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.9",
          "form": "저 원래 남주혁 배우님 관심 없었거든요?",
          "original_form": "저 원래 남주혁 배우님 관심 없었거든요?",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.10",
          "form": "넹 ㅋㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.11",
          "form": "뭐 잘생긴건 알고 있었는데 거리감 느꼈어요",
          "original_form": "뭐 잘생긴건 알고 있었는데 거리감 느꼈어요",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.13",
          "form": "근데 스타트업 딱 보는데!!",
          "original_form": "근데 스타트업 딱 보는데!!",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.14",
          "form": "성격이 막 멍뭉미 같아가지고 귀여워요",
          "original_form": "성격이 막 멍뭉미 같아가지고 귀여워요",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.15",
          "form": "오 ㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024145.1.16",
          "form": "저는 보건교사 안은영에서 좀 귀엽더라구요",
          "original_form": "저는 보건교사 안은영에서 좀 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.17",
          "form": "근데 뭐 캐릭터 설정이겠지만 프로그래밍 코딩 잘하는 연출도 한몫 했네요 ㅋㅋㅋㅋ",
          "original_form": "근데 뭐 캐릭터 설정이겠지만 프로그래밍 코딩 잘하는 연출도 한몫 했네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.18",
          "form": "아 거기서도 남주혁 나와요?",
          "original_form": "아 거기서도 남주혁 나와요?",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.19",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.20",
          "form": "주연? 조연?",
          "original_form": "주연? 조연?",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.21",
          "form": "한문쌤으러 주연으러 나와요",
          "original_form": "한문쌤으러 주연으러 나와요",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.22",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.23",
          "form": "검색해보니까 6부작밖에 안되네요",
          "original_form": "검색해보니까 6부작밖에 안되네요",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.24",
          "form": "정유미가 가장 주연이지만",
          "original_form": "정유미가 가장 주연이지만",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.25",
          "form": "네 시즌2 나올거에요",
          "original_form": "네 시즌2 나올거에요",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024145.1.26",
          "form": "저 원래 이런 괴상한 이름 달린 작품 안보는데",
          "original_form": "저 원래 이런 괴상한 이름 달린 작품 안보는데",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024145.1.27",
          "form": "남주혁 나온다고 하니 꼭 볼래요",
          "original_form": "남주혁 나온다고 하니 꼭 볼래요",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024145.1.28",
          "form": "ㅋㅋㅋㅋㅋ내용도 좀 괴상해요",
