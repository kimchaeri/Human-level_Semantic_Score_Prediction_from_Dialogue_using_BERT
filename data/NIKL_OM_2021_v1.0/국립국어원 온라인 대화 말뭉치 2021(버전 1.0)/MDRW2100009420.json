      "utterance": [
        {
          "id": "MDRW2100009420.1.1",
          "form": "전시회 가보셨어요?ㅎ",
          "original_form": "전시회 가보셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009420.1.2",
          "form": "저 분리수거좀 하고요 ㅎ 이따다시 얘기해요ㅋ",
          "original_form": "저 분리수거좀 하고요 ㅎ 이따다시 얘기해요ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009420.1.3",
          "form": "네네~^^ㅎ 언능 다녀오세요ㅎㅎ",
          "original_form": "네네~^^ㅎ 언능 다녀오세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009420.1.4",
          "form": "저 전시회 몇번 가보긴 했는데 넘 오래전이라ㅋ",
          "original_form": "저 전시회 몇번 가보긴 했는데 넘 오래전이라ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009420.1.5",
          "form": "왜요?ㅎㅎ",
          "original_form": "왜요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009420.1.6",
          "form": "오래전이라는게?ㄹ",
          "original_form": "오래전이라는게?ㄹ",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009420.1.7",
          "form": "전 오늘 갔다왔어요ㅎㅎ 딸이랑ㄹㄹ",
          "original_form": "전 오늘 갔다왔어요ㅎㅎ 딸이랑ㄹㄹ",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009420.1.8",
          "form": "오 딸이 볼만한 전시회가 있나요?",
          "original_form": "오 딸이 볼만한 전시회가 있나요?",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009420.1.9",
          "form": "도립미술관갔어요ㅎㅎ",
          "original_form": "도립미술관갔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009420.1.10",
          "form": "무슨전시회였나요",
          "original_form": "무슨전시회였나요",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009420.1.11",
          "form": "가서 전시회는 관심이업고 색칠공부만 하고왔어요ㅎㅎ",
          "original_form": "가서 전시회는 관심이업고 색칠공부만 하고왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009420.1.12",
          "form": "그날의 기억이었던가?",
          "original_form": "그날의 기억이었던가?",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009420.1.13",
          "form": "전시회를 갔는데 기억이없어요ㅎㅎㅎ 딸이 안간다고... 그래서ㅎㅎ",
          "original_form": "전시회를 갔는데 기억이없어요ㅎㅎㅎ 딸이 안간다고... 그래서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009420.1.14",
          "form": "아ㅎ 오래전이라 기억이 안난단얘기였어요",
          "original_form": "아ㅎ 오래전이라 기억이 안난단얘기였어요",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009420.1.15",
          "form": "샤갈전이랑",
          "original_form": "샤갈전이랑",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009420.1.16",
          "form": "클림튼 봤던거같은데",
          "original_form": "클림튼 봤던거같은데",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009420.1.17",
          "form": "우와ㅎㅎ 역시 지식인ㅎㅎ",
          "original_form": "우와ㅎㅎ 역시 지식인ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009420.1.18",
          "form": "어쩌다 본거예요ㅋ",
          "original_form": "어쩌다 본거예요ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009420.1.19",
          "form": "요즘 아이때문에 전시관이나 박물관 가는 걸 좋아하게됬어요ㅎ",
          "original_form": "요즘 아이때문에 전시관이나 박물관 가는 걸 좋아하게됬어요ㅎ",
