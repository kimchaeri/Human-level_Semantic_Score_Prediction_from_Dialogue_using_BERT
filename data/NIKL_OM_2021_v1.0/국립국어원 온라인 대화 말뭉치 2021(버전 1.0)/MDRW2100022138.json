      "utterance": [
        {
          "id": "MDRW2100022138.1.1",
          "form": "즐겨보는 컨텐츠 있으신가요",
          "original_form": "즐겨보는 컨텐츠 있으신가요",
          "speaker_id": "1",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100022138.1.2",
          "form": "전 주로 드라마 좋아해요~",
          "original_form": "전 주로 드라마 좋아해요~",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.3",
          "form": "저도 드라마좋아해요",
          "original_form": "저도 드라마좋아해요",
          "speaker_id": "1",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.4",
          "form": "요즘은 보는 드라마가 다 끝나서 속상해요ㅠㅠ",
          "original_form": "요즘은 보는 드라마가 다 끝나서 속상해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.5",
          "form": "즐겨보는 드라마있으셨나요",
          "original_form": "즐겨보는 드라마있으셨나요",
          "speaker_id": "1",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.6",
          "form": "오 최근에 어떤드라마 보셨나요?",
          "original_form": "오 최근에 어떤드라마 보셨나요?",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.7",
          "form": "약간 잔잔한 드라마 좋아해요",
          "original_form": "약간 잔잔한 드라마 좋아해요",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.8",
          "form": "고구마가 많지 않은?",
          "original_form": "고구마가 많지 않은?",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.9",
          "form": "저 요즘 막장.. 많이 봤..",
          "original_form": "저 요즘 막장.. 많이 봤..",
          "speaker_id": "1",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022138.1.10",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022138.1.11",
          "form": "ㅋㅋㅋ 라켓소년단보셨나요",
          "original_form": "ㅋㅋㅋ 라켓소년단보셨나요",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022138.1.12",
          "form": "제가 최근에 본 잔잔하고 힐링되는.. 유일한 드라마가 저거뿐이라",
          "original_form": "제가 최근에 본 잔잔하고 힐링되는.. 유일한 드라마가 저거뿐이라",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022138.1.13",
          "form": "엇 맞아요 ㅋㅋㅋㅋ 라켓소년단 재밌게 봤죠",
          "original_form": "엇 맞아요 ㅋㅋㅋㅋ 라켓소년단 재밌게 봤죠",
          "speaker_id": "2",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022138.1.14",
          "form": "막장이라하면 펜트하우스 맞나요? ㅎㅎ",
          "original_form": "막장이라하면 펜트하우스 맞나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022138.1.15",
          "form": "보셨구나",
          "original_form": "보셨구나",
          "speaker_id": "1",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022138.1.16",
          "form": "어 그것도 있고",
          "original_form": "어 그것도 있고",
          "speaker_id": "1",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022138.1.17",
          "form": "임성한 드라마.. 결혼작사작곡이혼이라고..",
          "original_form": "임성한 드라마.. 결혼작사작곡이혼이라고..",
          "speaker_id": "1",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022138.1.18",
          "form": "앜ㅋㅋㅋ 혹시나 했는데 맞았네요 ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ 혹시나 했는데 맞았네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.19",
          "form": "아시나요ㅋㅋㅋㅋㅋㅋ 아이고.. 뭔가 민망하네요",
          "original_form": "아시나요ㅋㅋㅋㅋㅋㅋ 아이고.. 뭔가 민망하네요",
          "speaker_id": "1",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.20",
          "form": "맞아요 그것도 장난아니게 자극적이고 재밌다고 들었어요 ㅋㅋㅋ",
          "original_form": "맞아요 그것도 장난아니게 자극적이고 재밌다고 들었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.21",
          "form": "저 막장을 그렇게 좋아하는 사람은 아닌데ㅋㅋㅋ주위에서 보라고 재밌다고 그래서ㅋㅋㅋ",
          "original_form": "저 막장을 그렇게 좋아하는 사람은 아닌데ㅋㅋㅋ주위에서 보라고 재밌다고 그래서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.22",
          "form": "아니에요 ㅋㅋㅋ 요즘 하는 막장은 거기에 광자매까지 추가하면 장난 아닌 것 같아요",
          "original_form": "아니에요 ㅋㅋㅋ 요즘 하는 막장은 거기에 광자매까지 추가하면 장난 아닌 것 같아요",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.23",
          "form": "광자매는 아직 안봤는데",
          "original_form": "광자매는 아직 안봤는데",
          "speaker_id": "1",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.24",
          "form": "그것도 난리납니까",
          "original_form": "그것도 난리납니까",
          "speaker_id": "1",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.25",
          "form": "앜ㅋㅋㅋ 보다보면 재밌죠 ㅋㅋㅋ 그맛에 보는 걸요 막장은",
          "original_form": "앜ㅋㅋㅋ 보다보면 재밌죠 ㅋㅋㅋ 그맛에 보는 걸요 막장은",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022138.1.26",
          "form": "그냥 그건 좀 얘기가 산으로가요 ㅋㅋㅋ\r\n앞의 그 2개랑은 좀 결이 다른 막장이에요",
          "original_form": "그냥 그건 좀 얘기가 산으로가요 ㅋㅋㅋ\r\n앞의 그 2개랑은 좀 결이 다른 막장이에요",
          "speaker_id": "2",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100022138.1.27",
          "form": "아.. 다시 말하지만 저 막장을 그렇게 좋아하는 사람이 아닙니다..(언행불일치)",
          "original_form": "아.. 다시 말하지만 저 막장을 그렇게 좋아하는 사람이 아닙니다..(언행불일치)",
          "speaker_id": "1",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100022138.1.28",
          "form": "절대 안보시는걸 추천합니다.. 빅재미도 없어요 ㅋㅋㅋ",
          "original_form": "절대 안보시는걸 추천합니다.. 빅재미도 없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100022138.1.29",
          "form": "약간 드라마가 왔다갔다 하는군요 보지않겠습니다",
          "original_form": "약간 드라마가 왔다갔다 하는군요 보지않겠습니다",
          "speaker_id": "1",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100022138.1.30",
          "form": "아니에욬ㅋㅋ 막장 좋아하실 수도 있죠~",
          "original_form": "아니에욬ㅋㅋ 막장 좋아하실 수도 있죠~",
          "speaker_id": "2",
          "time": "20210829 09:16"
        }
      ]
