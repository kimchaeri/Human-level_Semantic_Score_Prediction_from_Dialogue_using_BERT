      "utterance": [
        {
          "id": "MDRW2100013898.1.1",
          "form": "싸이월드 로그인해봤어?",
          "original_form": "싸이월드 로그인해봤어?",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013898.1.2",
          "form": "다시 열었어요?",
          "original_form": "다시 열었어요?",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013898.1.3",
          "form": "싸이월드 오픈한다고 홈피 뜨지않았어?",
          "original_form": "싸이월드 오픈한다고 홈피 뜨지않았어?",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013898.1.4",
          "form": "로그인하면 도토리 보상해준다고 했는데",
          "original_form": "로그인하면 도토리 보상해준다고 했는데",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013898.1.5",
          "form": "헐 도토리여?",
          "original_form": "헐 도토리여?",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013898.1.6",
          "form": "아이디 기억안나는디",
          "original_form": "아이디 기억안나는디",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013898.1.7",
          "form": "언니 열어봤어요?",
          "original_form": "언니 열어봤어요?",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013898.1.8",
          "form": "아니 분명히 나 도토리 몇개 남은걸로 알고있었는데 없더라구",
          "original_form": "아니 분명히 나 도토리 몇개 남은걸로 알고있었는데 없더라구",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013898.1.9",
          "form": "도토리 없어졌나 개당 100원",
          "original_form": "도토리 없어졌나 개당 100원",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013898.1.10",
          "form": "도토리 백원밖에 안됐어?",
          "original_form": "도토리 백원밖에 안됐어?",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013898.1.11",
          "form": "얼마안되네ㅋㅋ",
          "original_form": "얼마안되네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013898.1.12",
          "form": "ㅋㅋㅋㅋㅋ 언니 그래도 그땐 비싼 100원 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 언니 그래도 그땐 비싼 100원 ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013898.1.13",
          "form": "아 그런가? ㅋㅋㅋ",
          "original_form": "아 그런가? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013898.1.14",
          "form": "싸이월드 오픈하면 빨리가서 미니월드 사진 삭제해야지",
          "original_form": "싸이월드 오픈하면 빨리가서 미니월드 사진 삭제해야지",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013898.1.15",
          "form": "싸이월드 함 열어봐야것네요  예전사진이 있으려나",
          "original_form": "싸이월드 함 열어봐야것네요  예전사진이 있으려나",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013898.1.16",
          "form": "ㅋㅋㄲㄲㅋ",
          "original_form": "ㅋㅋㄲㄲㅋ",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013898.1.17",
          "form": "와 완전 흑역사 잔치",
          "original_form": "와 완전 흑역사 잔치",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013898.1.18",
          "form": "큰일이야",
          "original_form": "큰일이야",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013898.1.19",
          "form": "그리운 사진들도 있겠지만 대부분 흑역사일것같다",
          "original_form": "그리운 사진들도 있겠지만 대부분 흑역사일것같다",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013898.1.20",
          "form": "공유합시다",
          "original_form": "공유합시다",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013898.1.21",
          "form": "보여주세요",
          "original_form": "보여주세요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013898.1.22",
          "form": "네? 그걸 굳이ㅋㅋㅋ",
          "original_form": "네? 그걸 굳이ㅋㅋㅋ",
          "speaker_id": "1",
