        },
        {
          "id": "MDRW2100041996.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041996.1.5",
          "form": "네네 학교 다녀요",
          "original_form": "네네 학교 다녀요",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041996.1.6",
          "form": "아 학교 재미있나요? ㅋㅋ",
          "original_form": "아 학교 재미있나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041996.1.7",
          "form": "아니요! 힘들어요",
          "original_form": "아니요! 힘들어요",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041996.1.8",
          "form": "ㅜㅜ 남녀공학이에요?",
          "original_form": "ㅜㅜ 남녀공학이에요?",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041996.1.9",
          "form": "아 대학교 다녀요",
          "original_form": "아 대학교 다녀요",
          "speaker_id": "2",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041996.1.10",
          "form": "전 여대다녔었어요ㄱㅋ",
          "original_form": "전 여대다녔었어요ㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041996.1.11",
          "form": "남녀공학 노잼인가요? ㅜㅋㅋ",
          "original_form": "남녀공학 노잼인가요? ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041996.1.12",
          "form": "재미없어요..",
          "original_form": "재미없어요..",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041996.1.13",
          "form": "ㅜㅋㅋ 전 고등학교랑 대학이랑 비슷ㅋㅋ",
          "original_form": "ㅜㅋㅋ 전 고등학교랑 대학이랑 비슷ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041996.1.14",
          "form": "남학생이 공개고백해서 괜히 힘들게 하는 일도 있었어요",
          "original_form": "남학생이 공개고백해서 괜히 힘들게 하는 일도 있었어요",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041996.1.15",
          "form": "저때도 학교앞에서 기다리고ㅋㅋ",
          "original_form": "저때도 학교앞에서 기다리고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041996.1.16",
          "form": "본인이 당하신거예요?ㅜㅠ",
          "original_form": "본인이 당하신거예요?ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041996.1.17",
          "form": "제 친구가 당했었어요ㅠ",
          "original_form": "제 친구가 당했었어요ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100041996.1.18",
          "form": "그 남학생이 고백 안 받아줬다고 학교 앞 담벼락에 라커칠로 욕 써놓은 일도 있었어요..",
          "original_form": "그 남학생이 고백 안 받아줬다고 학교 앞 담벼락에 라커칠로 욕 써놓은 일도 있었어요..",
          "speaker_id": "2",
