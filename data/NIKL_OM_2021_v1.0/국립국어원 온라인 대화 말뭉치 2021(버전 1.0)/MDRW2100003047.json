      "utterance": [
        {
          "id": "MDRW2100003047.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210820 17:55"
        },
        {
          "id": "MDRW2100003047.1.2",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210820 17:56"
        },
        {
          "id": "MDRW2100003047.1.3",
          "form": "골든리트리버키워요",
          "original_form": "골든리트리버키워요",
          "speaker_id": "2",
          "time": "20210820 17:57"
        },
        {
          "id": "MDRW2100003047.1.4",
          "form": "래브라도리트리버도요",
          "original_form": "래브라도리트리버도요",
          "speaker_id": "2",
          "time": "20210820 17:57"
        },
        {
          "id": "MDRW2100003047.1.5",
          "form": "우와 ..",
          "original_form": "우와 ..",
          "speaker_id": "1",
          "time": "20210820 17:58"
        },
        {
          "id": "MDRW2100003047.1.6",
          "form": "저는 강아지를 원래 무서워했는데",
          "original_form": "저는 강아지를 원래 무서워했는데",
          "speaker_id": "1",
          "time": "20210820 17:58"
        },
        {
          "id": "MDRW2100003047.1.7",
          "form": "요즘엔 귀여워서 관심은 가더라구요",
          "original_form": "요즘엔 귀여워서 관심은 가더라구요",
          "speaker_id": "1",
          "time": "20210820 17:58"
        },
        {
          "id": "MDRW2100003047.1.8",
          "form": "종류는 잘몰라여 ..",
          "original_form": "종류는 잘몰라여 ..",
          "speaker_id": "1",
          "time": "20210820 17:58"
        },
        {
          "id": "MDRW2100003047.1.9",
          "form": "쪼꼬미들좋아해요?",
          "original_form": "쪼꼬미들좋아해요?",
          "speaker_id": "2",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003047.1.10",
          "form": "저희는 주택에 마당이 있어서 다 대형견이예요 진돗개도있었는데 얼마전에 나이들어서 죽었어요",
          "original_form": "저희는 주택에 마당이 있어서 다 대형견이예요 진돗개도있었는데 얼마전에 나이들어서 죽었어요",
          "speaker_id": "2",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100003047.1.11",
          "form": "ㅠㅠ 마당에서 진돗개키우면 넘좋겠어영",
          "original_form": "ㅠㅠ 마당에서 진돗개키우면 넘좋겠어영",
          "speaker_id": "1",
          "time": "20210820 19:16"
        },
        {
          "id": "MDRW2100003047.1.12",
          "form": "뛰어다니고 놀기도 좋아요",
          "original_form": "뛰어다니고 놀기도 좋아요",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003047.1.13",
          "form": "대형견은 산책을 어마어마 하게 해줘야되는데",
          "original_form": "대형견은 산책을 어마어마 하게 해줘야되는데",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003047.1.14",
          "form": "마당 울타리에서 놀 수 있으니까",
          "original_form": "마당 울타리에서 놀 수 있으니까",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003047.1.15",
          "form": "먹는건 다 잘먹어요 ??",
          "original_form": "먹는건 다 잘먹어요 ??",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003047.1.16",
          "form": "아니요 야채는 싫어해요ㅋㅋ",
          "original_form": "아니요 야채는 싫어해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100003047.1.17",
          "form": "같이줘도 야채만 빼고 먹어요",
          "original_form": "같이줘도 야채만 빼고 먹어요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100003047.1.18",
          "form": "ㅋㅋㅋㅋㅋ사람같네요",
          "original_form": "ㅋㅋㅋㅋㅋ사람같네요",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100003047.1.19",
          "form": "고기좋아하고 영양제도 먹어요",
          "original_form": "고기좋아하고 영양제도 먹어요",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100003047.1.20",
          "form": "다른 좋아하는 동물있어요?",
          "original_form": "다른 좋아하는 동물있어요?",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100003047.1.21",
          "form": "우와 .. 흠 토끼? 귀여워요 어릴때 키웟어요",
          "original_form": "우와 .. 흠 토끼? 귀여워요 어릴때 키웟어요",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100003047.1.22",
          "form": "토끼 냄새엄청나요",
          "original_form": "토끼 냄새엄청나요",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100003047.1.23",
          "form": "충격이었는데",
          "original_form": "충격이었는데",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100003047.1.24",
          "form": "ㅋㅋㅋㅋ맞아요 그래도 보면 귀엽더라구요",
          "original_form": "ㅋㅋㅋㅋ맞아요 그래도 보면 귀엽더라구요",
