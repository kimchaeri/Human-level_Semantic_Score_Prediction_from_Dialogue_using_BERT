      "utterance": [
        {
          "id": "MDRW2100041533.1.1",
          "form": "맞아 아침마다 너무 추워",
          "original_form": "맞아 아침마다 너무 추워",
          "speaker_id": "2",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100041533.1.2",
          "form": "오늘 뭐입고 갔니",
          "original_form": "오늘 뭐입고 갔니",
          "speaker_id": "2",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100041533.1.3",
          "form": "나 반팔에 여름청긴바지 ㅋㅋㅋㅋ",
          "original_form": "나 반팔에 여름청긴바지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 09:26"
        },
        {
          "id": "MDRW2100041533.1.4",
          "form": "시원해지긴 했는데 아직 가을옷 입을 정도는 아닌거 같아서",
          "original_form": "시원해지긴 했는데 아직 가을옷 입을 정도는 아닌거 같아서",
          "speaker_id": "1",
          "time": "20210906 09:26"
        },
        {
          "id": "MDRW2100041533.1.5",
          "form": "ㅋㅋㅋㅋ그러니까 그게 애매해",
          "original_form": "ㅋㅋㅋㅋ그러니까 그게 애매해",
          "speaker_id": "2",
          "time": "20210906 09:52"
        },
        {
          "id": "MDRW2100041533.1.6",
          "form": "아직 사무실에 에어컨 트는 사람들도 있지?",
          "original_form": "아직 사무실에 에어컨 트는 사람들도 있지?",
          "speaker_id": "2",
          "time": "20210906 09:52"
        },
        {
          "id": "MDRW2100041533.1.7",
          "form": "ㅇㅇ 우린 아직 항시 틀어놔",
          "original_form": "ㅇㅇ 우린 아직 항시 틀어놔",
          "speaker_id": "1",
          "time": "20210906 10:00"
        },
        {
          "id": "MDRW2100041533.1.8",
          "form": "너무추워.. 난아침마다 에어컨끄는게",
          "original_form": "너무추워.. 난아침마다 에어컨끄는게",
          "speaker_id": "2",
          "time": "20210906 10:01"
        },
        {
          "id": "MDRW2100041533.1.9",
          "form": "일과야ㅋㅋㅋㅋ",
          "original_form": "일과야ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 10:01"
        },
        {
          "id": "MDRW2100041533.1.10",
          "form": "ㅋㅋㅋㅋㅋ 꺼도 다른 사람이 안켜?",
          "original_form": "ㅋㅋㅋㅋㅋ 꺼도 다른 사람이 안켜?",
          "speaker_id": "1",
          "time": "20210906 10:02"
        },
        {
          "id": "MDRW2100041533.1.11",
          "form": "맨날키더라 근데",
          "original_form": "맨날키더라 근데",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041533.1.12",
          "form": "에어컨이 여섯대거든",
          "original_form": "에어컨이 여섯대거든",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041533.1.13",
          "form": "내자리주변만 꺼서",
          "original_form": "내자리주변만 꺼서",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041533.1.14",
          "form": "잘안들키는날도 있음 ㅋㅋㅋ",
          "original_form": "잘안들키는날도 있음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041533.1.15",
          "form": "너흰 프로그램때문에",
          "original_form": "너흰 프로그램때문에",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041533.1.16",
          "form": "불가피하게 켜놓는거지?",
          "original_form": "불가피하게 켜놓는거지?",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041533.1.17",
          "form": "헐 ㄷㄷ 사무실이 커서 그런가",
          "original_form": "헐 ㄷㄷ 사무실이 커서 그런가",
          "speaker_id": "1",
          "time": "20210906 10:27"
        },
        {
          "id": "MDRW2100041533.1.18",
          "form": "뭐 일단은ㅋㅋㅋ",
          "original_form": "뭐 일단은ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100041533.1.19",
          "form": "난오늘 긴팔입었어야했는데 ㅡㅡ",
          "original_form": "난오늘 긴팔입었어야했는데 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100041533.1.20",
          "form": "우린 딱히 프로그램이라기보단 ㅋㅋㅋ 플머만 좀 더위타고",
          "original_form": "우린 딱히 프로그램이라기보단 ㅋㅋㅋ 플머만 좀 더위타고",
          "speaker_id": "1",
          "time": "20210906 10:41"
        },
        {
          "id": "MDRW2100041533.1.21",
          "form": "그냥 출근때는 낮게 해놨다가 시간 좀 지나면 온도 높여",
          "original_form": "그냥 출근때는 낮게 해놨다가 시간 좀 지나면 온도 높여",
          "speaker_id": "1",
          "time": "20210906 10:42"
        },
        {
          "id": "MDRW2100041533.1.22",
          "form": "그게막 겨울에도 계속되는거야?",
          "original_form": "그게막 겨울에도 계속되는거야?",
          "speaker_id": "2",
          "time": "20210906 10:51"
        },
        {
          "id": "MDRW2100041533.1.23",
          "form": "미쳤는데..",
          "original_form": "미쳤는데..",
          "speaker_id": "2",
          "time": "20210906 10:51"
        },
        {
          "id": "MDRW2100041533.1.24",
          "form": "ㄴㄴㄴㄴ 겨울에는 히터 틀겠지",
          "original_form": "ㄴㄴㄴㄴ 겨울에는 히터 틀겠지",
          "speaker_id": "1",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100041533.1.25",
          "form": "ㅋㅋㅋㅋㅋ그렇지",
          "original_form": "ㅋㅋㅋㅋㅋ그렇지",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100041533.1.26",
          "form": "살고봐야지",
          "original_form": "살고봐야지",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100041533.1.27",
          "form": "선선한데 비오려고하니까",
          "original_form": "선선한데 비오려고하니까",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100041533.1.28",
          "form": "좀 짜증나는군",
          "original_form": "좀 짜증나는군",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100041533.1.29",
          "form": "요새 날이 맑은 날이 잘 없어",
          "original_form": "요새 날이 맑은 날이 잘 없어",
          "speaker_id": "1",
          "time": "20210906 11:29"
        }
      ]
