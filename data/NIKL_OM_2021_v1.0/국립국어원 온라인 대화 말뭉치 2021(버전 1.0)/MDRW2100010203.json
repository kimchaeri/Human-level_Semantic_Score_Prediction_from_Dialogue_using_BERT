      "utterance": [
        {
          "id": "MDRW2100010203.1.1",
          "form": "둘다 타는데 추위를 더 타요",
          "original_form": "둘다 타는데 추위를 더 타요",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010203.1.2",
          "form": "name1님은요ㅣ???",
          "original_form": "&name1&님은요ㅣ???",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010203.1.3",
          "form": "저는 더위를 더 많이 타요!",
          "original_form": "저는 더위를 더 많이 타요!",
          "speaker_id": "1",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010203.1.4",
          "form": "이번 여름 너무 덥지 않았나요!",
          "original_form": "이번 여름 너무 덥지 않았나요!",
          "speaker_id": "1",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100010203.1.5",
          "form": "네 재택근무 하는데 너무 더웠어요ㅜㅜ",
          "original_form": "네 재택근무 하는데 너무 더웠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 15:43"
        },
        {
          "id": "MDRW2100010203.1.6",
          "form": "어떻게 버티셨나요",
          "original_form": "어떻게 버티셨나요",
          "speaker_id": "2",
          "time": "20210823 15:43"
        },
        {
          "id": "MDRW2100010203.1.7",
          "form": "이번엔 못 참고 낮에는 에어컨 틀었어요ㅜㅜ 외출도 자제하고",
          "original_form": "이번엔 못 참고 낮에는 에어컨 틀었어요ㅜㅜ 외출도 자제하고",
          "speaker_id": "1",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010203.1.8",
          "form": "에어컨 없이 못 버티겠더라고요",
          "original_form": "에어컨 없이 못 버티겠더라고요",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010203.1.9",
          "form": "그쵸 진짜 더워서ㅜㅜ",
          "original_form": "그쵸 진짜 더워서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010203.1.10",
          "form": "빨리 가을 되면 좋겠네요..",
          "original_form": "빨리 가을 되면 좋겠네요..",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010203.1.11",
          "form": "그레도 이번 장마 끝나면 가을이네요",
          "original_form": "그레도 이번 장마 끝나면 가을이네요",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010203.1.12",
          "form": "슬슬 가을 옷을 꺼내야 할 거 같아요",
          "original_form": "슬슬 가을 옷을 꺼내야 할 거 같아요",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010203.1.13",
          "form": "네~~~가을에는 주로 어떤옷 입으세요?",
          "original_form": "네~~~가을에는 주로 어떤옷 입으세요?",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010203.1.14",
          "form": "긴팔 셔츠나 남방 좋아해요 바지는 슬랙스 자주 입어요",
          "original_form": "긴팔 셔츠나 남방 좋아해요 바지는 슬랙스 자주 입어요",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010203.1.15",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010203.1.16",
          "form": "저도요~~~저랑 똑같군요",
          "original_form": "저도요~~~저랑 똑같군요",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010203.1.17",
          "form": "ㅋㅋ가을 남방이 이상하게 많더라고요",
          "original_form": "ㅋㅋ가을 남방이 이상하게 많더라고요",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010203.1.18",
          "form": "무난하고 좋은 거 같아요",
          "original_form": "무난하고 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010203.1.19",
          "form": "적당히 따뜻하고",
          "original_form": "적당히 따뜻하고",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010203.1.20",
          "form": "네 가을엔 남방이죠~~~",
          "original_form": "네 가을엔 남방이죠~~~",
          "speaker_id": "2",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010203.1.21",
          "form": "가을이 코디하기도 좋아요",
          "original_form": "가을이 코디하기도 좋아요",
          "speaker_id": "1",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010203.1.22",
          "form": "레이어드 하기도 좋고",
          "original_form": "레이어드 하기도 좋고",
          "speaker_id": "1",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010203.1.23",
          "form": "여름에는 그냥 최대한 얇게 입고",
          "original_form": "여름에는 그냥 최대한 얇게 입고",
          "speaker_id": "1",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010203.1.24",
          "form": "겨울엔 그냥 롱패딩 입어서ㅋㅋㅋ",
          "original_form": "겨울엔 그냥 롱패딩 입어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010203.1.25",
          "form": "네ㅋㅋㅋ 간단하게 겉에 걸치면 되니까요",
          "original_form": "네ㅋㅋㅋ 간단하게 겉에 걸치면 되니까요",
          "speaker_id": "2",
          "time": "20210823 17:45"
        }
      ]
    }
