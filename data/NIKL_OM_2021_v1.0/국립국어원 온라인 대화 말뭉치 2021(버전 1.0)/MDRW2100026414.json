      "utterance": [
        {
          "id": "MDRW2100026414.1.1",
          "form": "오늘 머리잘랐어요",
          "original_form": "오늘 머리잘랐어요",
          "speaker_id": "1",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026414.1.2",
          "form": "머리잘랐어요오늘 !",
          "original_form": "머리잘랐어요오늘 !",
          "speaker_id": "1",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026414.1.3",
          "form": "오오 저는 한 이주전에 잘랐습니다",
          "original_form": "오오 저는 한 이주전에 잘랐습니다",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026414.1.4",
          "form": "얼마나 자르셨나요ㅎㅎ",
          "original_form": "얼마나 자르셨나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026414.1.5",
          "form": "저는 여름에 단발했다가 거지존이었는데 다시 짧게요",
          "original_form": "저는 여름에 단발했다가 거지존이었는데 다시 짧게요",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026414.1.6",
          "form": "저랑 똑같으시네요 ㅎㅎ 거지존을 못참고 맨날 단발해요",
          "original_form": "저랑 똑같으시네요 ㅎㅎ 거지존을 못참고 맨날 단발해요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026414.1.7",
          "form": "오 단발이시구나",
          "original_form": "오 단발이시구나",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026414.1.8",
          "form": "전 10년만에 단발 첨 햇어요",
          "original_form": "전 10년만에 단발 첨 햇어요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026414.1.9",
          "form": "오 그러시구나 편하시죠 ㅎㅎ",
          "original_form": "오 그러시구나 편하시죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026414.1.10",
          "form": "네 머리감을때 최고",
          "original_form": "네 머리감을때 최고",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026414.1.11",
          "form": "말릴때도 좋아여 ㅎㅎ",
          "original_form": "말릴때도 좋아여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026414.1.12",
          "form": "근데오늘은 너무 짧게돼서 슬퍼요",
          "original_form": "근데오늘은 너무 짧게돼서 슬퍼요",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026414.1.13",
          "form": "ㅠㅠ 괜찮아요 금방 자랄거에요",
          "original_form": "ㅠㅠ 괜찮아요 금방 자랄거에요",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026414.1.14",
          "form": "넹 ㅠㅠ 머리는.자라니까",
          "original_form": "넹 ㅠㅠ 머리는.자라니까",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026414.1.15",
          "form": "마자요 ㅎㅎ",
          "original_form": "마자요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026414.1.16",
          "form": "똑단발이신가여??",
          "original_form": "똑단발이신가여??",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026414.1.17",
          "form": "네지금 똑단발됐어요",
          "original_form": "네지금 똑단발됐어요",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026414.1.18",
          "form": "시크를 추구했는데 순박 단발 됐어여 ㅋ",
          "original_form": "시크를 추구했는데 순박 단발 됐어여 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026414.1.19",
          "form": "ㅋㅋㅋㅋㅋ요즘에는 똑단발이 세련되보이던데ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ요즘에는 똑단발이 세련되보이던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026414.1.20",
          "form": "그쵸 하지만 순박해졌어요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "그쵸 하지만 순박해졌어요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026414.1.21",
          "form": "ㅠㅠㅋㅋ 알죠 그 미묘한차이 ㅎㅎ",
          "original_form": "ㅠㅠㅋㅋ 알죠 그 미묘한차이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026414.1.22",
          "form": "그쵸 슬픕니당ㅋㅋ",
          "original_form": "그쵸 슬픕니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026414.1.23",
          "form": "옷으로 시크를 추구해여겠어요",
          "original_form": "옷으로 시크를 추구해여겠어요",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026414.1.24",
          "form": "마자여 옷으로 해결할수이써여 ㅎㅎ",
          "original_form": "마자여 옷으로 해결할수이써여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:12"
        }
      ]
    }
  ]
