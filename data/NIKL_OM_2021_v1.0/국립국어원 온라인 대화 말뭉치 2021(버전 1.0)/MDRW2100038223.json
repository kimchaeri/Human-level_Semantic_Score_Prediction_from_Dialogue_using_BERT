      "utterance": [
        {
          "id": "MDRW2100038223.1.1",
          "form": "난 감우 성능",
          "original_form": "난 감우 성능",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038223.1.2",
          "form": "썩 맘에들진",
          "original_form": "썩 맘에들진",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038223.1.3",
          "form": "않던데",
          "original_form": "않던데",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038223.1.4",
          "form": "성능 사기캐라는데는",
          "original_form": "성능 사기캐라는데는",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038223.1.5",
          "form": "이견이 없더라구",
          "original_form": "이견이 없더라구",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038223.1.6",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038223.1.7",
          "form": "깡딜이 센건가?",
          "original_form": "깡딜이 센건가?",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038223.1.8",
          "form": "난 얼음 원소 묻히는게",
          "original_form": "난 얼음 원소 묻히는게",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038223.1.9",
          "form": "너무 빡세서",
          "original_form": "너무 빡세서",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038223.1.10",
          "form": "그리고 컨트롤이",
          "original_form": "그리고 컨트롤이",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038223.1.11",
          "form": "손 좀 탈것같아서",
          "original_form": "손 좀 탈것같아서",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038223.1.12",
          "form": "구린편으로 분류했는데",
          "original_form": "구린편으로 분류했는데",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038223.1.13",
          "form": "ㅇㅇ 나도 내가 활캐 사용에 능숙한 편이 아니라",
          "original_form": "ㅇㅇ 나도 내가 활캐 사용에 능숙한 편이 아니라",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.14",
          "form": "이게... 좋다고...?",
          "original_form": "이게... 좋다고...?",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.15",
          "form": "하는 입장이긴 한데",
          "original_form": "하는 입장이긴 한데",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.16",
          "form": "실전에서 쓰면",
          "original_form": "실전에서 쓰면",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.17",
          "form": "스킬 쿨타임 거의 없이",
          "original_form": "스킬 쿨타임 거의 없이",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.18",
          "form": "빙결을 풀로 돌ㄹ릴 수 있어서",
          "original_form": "빙결을 풀로 돌ㄹ릴 수 있어서",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.19",
          "form": "쿨타임 짧긴 했어",
          "original_form": "쿨타임 짧긴 했어",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.20",
          "form": "사기캐 소리 듣나봐",
          "original_form": "사기캐 소리 듣나봐",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038223.1.21",
          "form": "근데 뒤로 빼는 스킬이라",
          "original_form": "근데 뒤로 빼는 스킬이라",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038223.1.22",
          "form": "흠.....",
          "original_form": "흠.....",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038223.1.23",
          "form": "그리고 내가",
          "original_form": "그리고 내가",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038223.1.24",
          "form": "시간차로 터지는걸",
          "original_form": "시간차로 터지는걸",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038223.1.25",
          "form": "쓰기 좀 힘들어해",
          "original_form": "쓰기 좀 힘들어해",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038223.1.26",
          "form": "시간차로 터지는거?",
          "original_form": "시간차로 터지는거?",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038223.1.27",
          "form": "감우 스킬이",
          "original_form": "감우 스킬이",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038223.1.28",
          "form": "그거 아니었나",
          "original_form": "그거 아니었나",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038223.1.29",
          "form": "쓰면 그 자리에 얼음연꽃 생기고 뒤로 훅 빼고",
          "original_form": "쓰면 그 자리에 얼음연꽃 생기고 뒤로 훅 빼고",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038223.1.30",
          "form": "얼음 연꽃 조금 뒤에 터지고",
          "original_form": "얼음 연꽃 조금 뒤에 터지고",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038223.1.31",
          "form": "ㅇㅇ 그럴걸?",
          "original_form": "ㅇㅇ 그럴걸?",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038223.1.32",
          "form": "그리고 궁쓰면 평타도 원소스킬되고",
          "original_form": "그리고 궁쓰면 평타도 원소스킬되고",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038223.1.33",
          "form": "그거 몹이 있을자리에 딱 터뜨리는거",
          "original_form": "그거 몹이 있을자리에 딱 터뜨리는거",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038223.1.34",
          "form": "좀 빡세지 않아?",
          "original_form": "좀 빡세지 않아?",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038223.1.35",
          "form": "난 그때 잠깐 썼을때",
          "original_form": "난 그때 잠깐 썼을때",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038223.1.36",
          "form": "그게 맘대로 안돼서",
          "original_form": "그게 맘대로 안돼서",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038223.1.37",
          "form": "난 오래돼서 기억이 잘 안나긴하는데",
          "original_form": "난 오래돼서 기억이 잘 안나긴하는데",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038223.1.38",
          "form": "명중률 그닥 신경 안쓴듯",
          "original_form": "명중률 그닥 신경 안쓴듯",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.39",
          "form": "그때 썼던곳은 얕은 연못이라 좀만 써도 얼어서 그나마 나았는데",
          "original_form": "그때 썼던곳은 얕은 연못이라 좀만 써도 얼어서 그나마 나았는데",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.40",
          "form": "범위라",
          "original_form": "범위라",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.41",
          "form": "평지에서 쓰라면 좀 힘들것같아",
          "original_form": "평지에서 쓰라면 좀 힘들것같아",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.42",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.43",
          "form": "그리고 단일몹한테 쓰는것도",
          "original_form": "그리고 단일몹한테 쓰는것도",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.44",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.45",
          "form": "벤티같은 몹몰이 서포터 하나 끼고",
          "original_form": "벤티같은 몹몰이 서포터 하나 끼고",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038223.1.46",
          "form": "써야하나보다",
          "original_form": "써야하나보다",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038223.1.47",
          "form": "그건 괜찮을듯",
          "original_form": "그건 괜찮을듯",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038223.1.48",
          "form": "카즈하는",
          "original_form": "카즈하는",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038223.1.49",
          "form": "하위호환이라...",
          "original_form": "하위호환이라...",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038223.1.50",
          "form": "벤티+물서폿하고 쓰면",
          "original_form": "벤티+물서폿하고 쓰면",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038223.1.51",
          "form": "괜찮겠다",
          "original_form": "괜찮겠다",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038223.1.52",
          "form": "비슷하게 몹몰이 기능이 있긴 한대",
          "original_form": "비슷하게 몹몰이 기능이 있긴 한대",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038223.1.53",
          "form": "얘네가 되게 웃긴게",
          "original_form": "얘네가 되게 웃긴게",
          "speaker_id": "1",
          "time": "20210902 01:17"
        }
      ]
    }
