      "utterance": [
        {
          "id": "MDRW2100044256.1.1",
          "form": "여름옷은 또 세탁해서",
          "original_form": "여름옷은 또 세탁해서",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044256.1.2",
          "form": "접어서 넣을생각하니",
          "original_form": "접어서 넣을생각하니",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044256.1.3",
          "form": "머리가아프네요ㅋㅋ",
          "original_form": "머리가아프네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044256.1.4",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044256.1.5",
          "form": "계절 바뀔 때 옷정리하는 것 은근히 큰일",
          "original_form": "계절 바뀔 때 옷정리하는 것 은근히 큰일",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044256.1.6",
          "form": "맞아요 또 요즘날씨가",
          "original_form": "맞아요 또 요즘날씨가",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044256.1.7",
          "form": "왓다갔다하니",
          "original_form": "왓다갔다하니",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044256.1.8",
          "form": "어느 타이밍에 넣을지도 고민이구요",
          "original_form": "어느 타이밍에 넣을지도 고민이구요",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044256.1.9",
          "form": "네 옷정리하기 난감해요",
          "original_form": "네 옷정리하기 난감해요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044256.1.10",
          "form": "이불도 바꿔야하는데요",
          "original_form": "이불도 바꿔야하는데요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044256.1.11",
          "form": "여름이불 넣으셨나요?",
          "original_form": "여름이불 넣으셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044256.1.12",
          "form": "네 솜이불로 바꿨어요",
          "original_form": "네 솜이불로 바꿨어요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044256.1.13",
          "form": "근데 여름이불도 아직 옆에 두고 자요",
          "original_form": "근데 여름이불도 아직 옆에 두고 자요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044256.1.14",
          "form": "저는 어제 추웠어서 얇은 솜이불을 꺼냈어요",
          "original_form": "저는 어제 추웠어서 얇은 솜이불을 꺼냈어요",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044256.1.15",
          "form": "ㅋㅋ자다가 더우면 바꾸시려구요?",
          "original_form": "ㅋㅋ자다가 더우면 바꾸시려구요?",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044256.1.16",
          "form": "ㅋㅋ맞아요",
          "original_form": "ㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044256.1.17",
          "form": "사실 여름이불 세탁해서 넣는것도 일이구요",
          "original_form": "사실 여름이불 세탁해서 넣는것도 일이구요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044256.1.18",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044256.1.19",
          "form": "맘의 준비가 필요해요",
          "original_form": "맘의 준비가 필요해요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044256.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044256.1.21",
          "form": "선풍기 넣으셨어요?",
          "original_form": "선풍기 넣으셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044256.1.22",
          "form": "전아직있어요ㅋㅋ",
          "original_form": "전아직있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044256.1.23",
          "form": "저도 지금 선풍기 틀었어요",
          "original_form": "저도 지금 선풍기 틀었어요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044256.1.24",
          "form": "ㅋㅋ저도 선풍기는 쉽게 포기하지못해요",
          "original_form": "ㅋㅋ저도 선풍기는 쉽게 포기하지못해요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044256.1.25",
          "form": "머리말리기 편해서요ㅋㅋ",
          "original_form": "머리말리기 편해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044256.1.26",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044256.1.27",
          "form": "그라고 더워질때가 있어서",
          "original_form": "그라고 더워질때가 있어서",
          "speaker_id": "1",
          "time": "20210907 22:30"
        }
      ]
    }
