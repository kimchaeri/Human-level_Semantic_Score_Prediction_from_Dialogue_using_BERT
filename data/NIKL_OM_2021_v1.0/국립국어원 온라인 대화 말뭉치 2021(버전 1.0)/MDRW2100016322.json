      "utterance": [
        {
          "id": "MDRW2100016322.1.1",
          "form": "오늘 날씨가 참 화창하네요",
          "original_form": "오늘 날씨가 참 화창하네요",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016322.1.2",
          "form": "해가 쨍쨍하네요",
          "original_form": "해가 쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016322.1.3",
          "form": "네네 정말 오랜만에 날씨가 참 좋아요",
          "original_form": "네네 정말 오랜만에 날씨가 참 좋아요",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016322.1.4",
          "form": "벌써 가을이 왔네요",
          "original_form": "벌써 가을이 왔네요",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016322.1.5",
          "form": "맞아요 시간이 정말 빨라요! 금새 겨울도 올것 같아요",
          "original_form": "맞아요 시간이 정말 빨라요! 금새 겨울도 올것 같아요",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100016322.1.6",
          "form": "가을옷 꺼내셨나요?",
          "original_form": "가을옷 꺼내셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016322.1.7",
          "form": "얇은 자켓만 꺼내놨네요 쌀쌀할때 입으려구",
          "original_form": "얇은 자켓만 꺼내놨네요 쌀쌀할때 입으려구",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016322.1.8",
          "form": "좋아하는 계절있나요",
          "original_form": "좋아하는 계절있나요",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016322.1.9",
          "form": "저는 봄 가을이 좋더라구요?",
          "original_form": "저는 봄 가을이 좋더라구요?",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016322.1.10",
          "form": "좋아하시는 계절이 있나요?",
          "original_form": "좋아하시는 계절이 있나요?",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016322.1.11",
          "form": "여름이요!",
          "original_form": "여름이요!",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016322.1.12",
          "form": "오 여름을 좋아하시는 이유가 있나요?",
          "original_form": "오 여름을 좋아하시는 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016322.1.13",
          "form": "추운게 싫어요",
          "original_form": "추운게 싫어요",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016322.1.14",
          "form": "아 그러시구나 저는 더운것보단 추운게 낫더라구요!",
          "original_form": "아 그러시구나 저는 더운것보단 추운게 낫더라구요!",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016322.1.15",
          "form": "땀 흘리는게 싫어서",
          "original_form": "땀 흘리는게 싫어서",
          "speaker_id": "1",
          "time": "20210826 12:13"
        }
      ]
