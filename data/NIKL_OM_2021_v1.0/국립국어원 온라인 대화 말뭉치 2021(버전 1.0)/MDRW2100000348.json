      "utterance": [
        {
          "id": "MDRW2100000348.1.1",
          "form": "하이 name2~ 여름휴가계획 있어?",
          "original_form": "하이 &name2&~ 여름휴가계획 있어?",
          "speaker_id": "1",
          "time": "20210819 15:28"
        },
        {
          "id": "MDRW2100000348.1.2",
          "form": "안녕! 집에서 에어컨틀고 넷플릭스 볼 거야.",
          "original_form": "안녕! 집에서 에어컨틀고 넷플릭스 볼 거야.",
          "speaker_id": "2",
          "time": "20210819 15:29"
        },
        {
          "id": "MDRW2100000348.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210819 15:29"
        },
        {
          "id": "MDRW2100000348.1.4",
          "form": "와 좋겠다 그런게바로 여름휴가지",
          "original_form": "와 좋겠다 그런게바로 여름휴가지",
          "speaker_id": "1",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000348.1.5",
          "form": "나는 어디 놀러갈까 생각중인데 아직 못정했어",
          "original_form": "나는 어디 놀러갈까 생각중인데 아직 못정했어",
          "speaker_id": "1",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000348.1.6",
          "form": "재작년 휴가 때는 강릉으로 놀러 갔었어!",
          "original_form": "재작년 휴가 때는 강릉으로 놀러 갔었어!",
          "speaker_id": "2",
          "time": "20210819 15:31"
        },
        {
          "id": "MDRW2100000348.1.7",
          "form": "제주도도 갔었고..! 그리운 여름휴가!",
          "original_form": "제주도도 갔었고..! 그리운 여름휴가!",
          "speaker_id": "2",
          "time": "20210819 15:31"
        },
        {
          "id": "MDRW2100000348.1.8",
          "form": "나는 5월초에 다녀왔어 강릉",
          "original_form": "나는 5월초에 다녀왔어 강릉",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000348.1.9",
          "form": "7월초에는 캐비다녀왔어 ㅎ 사람없어서 좋드라",
          "original_form": "7월초에는 캐비다녀왔어 ㅎ 사람없어서 좋드라",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000348.1.10",
          "form": "10월말에 제주도가려고 ㅎㅎㅎ헤헤",
          "original_form": "10월말에 제주도가려고 ㅎㅎㅎ헤헤",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000348.1.11",
          "form": "우와 여기저기 많이 갔네!",
          "original_form": "우와 여기저기 많이 갔네!",
          "speaker_id": "2",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000348.1.12",
          "form": "돈벌어서 뭐하겠어~",
          "original_form": "돈벌어서 뭐하겠어~",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000348.1.13",
          "form": "강릉 너무 좋지 않아?",
          "original_form": "강릉 너무 좋지 않아?",
          "speaker_id": "2",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000348.1.14",
          "form": "너무좋아 살고싶어 ㅠㅠ",
          "original_form": "너무좋아 살고싶어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000348.1.15",
          "form": "내 직장상사가 강릉사람인데 전에비해서 놀러온사람너무많다고 복잡해져서 별루라고하더라",
          "original_form": "내 직장상사가 강릉사람인데 전에비해서 놀러온사람너무많다고 복잡해져서 별루라고하더라",
          "speaker_id": "1",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000348.1.16",
          "form": "거기 주민들은 그렇게 생각할 수도 있겠다",
          "original_form": "거기 주민들은 그렇게 생각할 수도 있겠다",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000348.1.17",
          "form": "마치 북촌한옥마을에 사는 느낌..?",
          "original_form": "마치 북촌한옥마을에 사는 느낌..?",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000348.1.18",
          "form": "나한테는 집인데 사람들은 구경오는 곳",
          "original_form": "나한테는 집인데 사람들은 구경오는 곳",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000348.1.19",
          "form": "아 나같아도 시끄러워서 너무싫을거같아",
          "original_form": "아 나같아도 시끄러워서 너무싫을거같아",
          "speaker_id": "1",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000348.1.20",
          "form": "누가 지켜보는 기분,,",
          "original_form": "누가 지켜보는 기분,,",
          "speaker_id": "1",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000348.1.21",
          "form": "그렇지만 강릉은 정말 매력적인 곳!",
          "original_form": "그렇지만 강릉은 정말 매력적인 곳!",
          "speaker_id": "2",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000348.1.22",
          "form": "난 산보다는 바다거든!",
          "original_form": "난 산보다는 바다거든!",
          "speaker_id": "2",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000348.1.23",
          "form": "나도! 난 무조건 바다!",
          "original_form": "나도! 난 무조건 바다!",
          "speaker_id": "1",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000348.1.24",
          "form": "산은 벌레가 너무많아서 싫엌ㅋㅋㅋ",
          "original_form": "산은 벌레가 너무많아서 싫엌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000348.1.25",
          "form": "윽 나도 벌레....싫어",
          "original_form": "윽 나도 벌레....싫어",
          "speaker_id": "2",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000348.1.26",
          "form": "그리고 정상에 오르기 전에는 약간 답답한 느낌",
          "original_form": "그리고 정상에 오르기 전에는 약간 답답한 느낌",
          "speaker_id": "2",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000348.1.27",
          "form": "맞아 ㅠㅠ 난 외국여행 너무가고싶어 ㅠㅠ",
          "original_form": "맞아 ㅠㅠ 난 외국여행 너무가고싶어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000348.1.28",
          "form": "비행기....기내식....그리운 느낌",
          "original_form": "비행기....기내식....그리운 느낌",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000348.1.29",
          "form": "어디 제일 가고 싶어?",
          "original_form": "어디 제일 가고 싶어?",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000348.1.30",
          "form": "나 원래 작년 3월에 대만비행기표 끊어놓고 여행계획 다 짜고있는데 취소된거있찌,,,",
          "original_form": "나 원래 작년 3월에 대만비행기표 끊어놓고 여행계획 다 짜고있는데 취소된거있찌,,,",
          "speaker_id": "1",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000348.1.31",
          "form": "대만!",
          "original_form": "대만!",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000348.1.32",
          "form": "먹으러 가는 대만!!",
          "original_form": "먹으러 가는 대만!!",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000348.1.33",
          "form": "아직 못 가봤는데 그렇게 맛있는 게 많다며?",
          "original_form": "아직 못 가봤는데 그렇게 맛있는 게 많다며?",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000348.1.34",
          "form": "그니까,,, 나도 엄청기대했는데 2월말에 취소되더라고 그때부터 코로나 갑자기 시작되는바람에 ㅠㅠㅠ눈물나",
          "original_form": "그니까,,, 나도 엄청기대했는데 2월말에 취소되더라고 그때부터 코로나 갑자기 시작되는바람에 ㅠㅠㅠ눈물나",
          "speaker_id": "1",
          "time": "20210819 15:39"
        }
      ]
    }
