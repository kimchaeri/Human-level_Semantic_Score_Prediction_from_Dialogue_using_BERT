      "utterance": [
        {
          "id": "MDRW2100028012.1.1",
          "form": "계속영업당하는 ㅋㅋ",
          "original_form": "계속영업당하는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100028012.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 고개 휙 틀면 또 다른 사람이 영업하구...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 고개 휙 틀면 또 다른 사람이 영업하구...",
          "speaker_id": "2",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100028012.1.3",
          "form": "영업의 연속입니다..",
          "original_form": "영업의 연속입니다..",
          "speaker_id": "2",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100028012.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100028012.1.5",
          "form": "애니도좋아하신다구",
          "original_form": "애니도좋아하신다구",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100028012.1.6",
          "form": "했죠?? 뭐좋아하세요?",
          "original_form": "했죠?? 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100028012.1.7",
          "form": "저 원피스부터 시작해서 웬만한 애니는 다 봤어요!",
          "original_form": "저 원피스부터 시작해서 웬만한 애니는 다 봤어요!",
          "speaker_id": "2",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100028012.1.8",
          "form": "저는 애니는잘몰라서 ㅋㅋ",
          "original_form": "저는 애니는잘몰라서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028012.1.9",
          "form": "친구들이 보는건좀알아요",
          "original_form": "친구들이 보는건좀알아요",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028012.1.10",
          "form": "고등학교다닐때",
          "original_form": "고등학교다닐때",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028012.1.11",
          "form": "하이큐?좋아하더라구요",
          "original_form": "하이큐?좋아하더라구요",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028012.1.12",
          "form": "하이큐 ! 재밌어요! 시간 나실 때 보는 거 추천... 이렇게 트위터마냥 영업하기",
          "original_form": "하이큐 ! 재밌어요! 시간 나실 때 보는 거 추천... 이렇게 트위터마냥 영업하기",
          "speaker_id": "2",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100028012.1.13",
          "form": "ㅋㅋㄱㅋㄱ 애니보는사람들도",
          "original_form": "ㅋㅋㄱㅋㄱ 애니보는사람들도",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028012.1.14",
          "form": "정모같은거하나요??",
          "original_form": "정모같은거하나요??",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028012.1.15",
          "form": "네! 저는 안 한지 좀 오래되긴 했는데 뭐... 덕질 투어라던지... 그냥 오프 뛴다던지 해요!",
          "original_form": "네! 저는 안 한지 좀 오래되긴 했는데 뭐... 덕질 투어라던지... 그냥 오프 뛴다던지 해요!",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028012.1.16",
          "form": "덕질투어면 일본으로가나요 ??!",
          "original_form": "덕질투어면 일본으로가나요 ??!",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028012.1.17",
          "form": "아뇨! 그냥 콜라보 카페 가거나 애니메이트? 이런 곳 가거나 굿즈샵 가고 그러는 거 같아요!",
          "original_form": "아뇨! 그냥 콜라보 카페 가거나 애니메이트? 이런 곳 가거나 굿즈샵 가고 그러는 거 같아요!",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028012.1.18",
          "form": "아아! 그런식의 덕질이군요ㅋㅋ",
          "original_form": "아아! 그런식의 덕질이군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028012.1.19",
          "form": "덕질정모요 ㅋㅋ",
          "original_form": "덕질정모요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028012.1.20",
          "form": "연뮤는 그냥 극장가면",
          "original_form": "연뮤는 그냥 극장가면",
          "speaker_id": "1",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028012.1.21",
          "form": "트위터사람들이 맨날있는",
          "original_form": "트위터사람들이 맨날있는",
          "speaker_id": "1",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028012.1.22",
          "form": "앜ㅋㅋㅋㅋㅋ 아 진짜요?! 막 저번에 봤던 얼굴들이구만. 이런건가요?",
          "original_form": "앜ㅋㅋㅋㅋㅋ 아 진짜요?! 막 저번에 봤던 얼굴들이구만. 이런건가요?",
          "speaker_id": "2",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100028012.1.23",
          "form": "그런것도있어요 ㅋㅋ 얼굴을외운적도있고",
          "original_form": "그런것도있어요 ㅋㅋ 얼굴을외운적도있고",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028012.1.24",
          "form": "트위터하면서 극장에서인사해요~! 해서",
          "original_form": "트위터하면서 극장에서인사해요~! 해서",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028012.1.25",
          "form": "인사하고 친해져서 자주보는것도있구요 ㅋㅋ",
          "original_form": "인사하고 친해져서 자주보는것도있구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028012.1.26",
          "form": "같은배우덕질하면아무래도",
          "original_form": "같은배우덕질하면아무래도",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028012.1.27",
          "form": "아! 넘 신기해요!",
          "original_form": "아! 넘 신기해요!",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028012.1.28",
          "form": "같은공연을자주보니까 !!",
          "original_form": "같은공연을자주보니까 !!",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028012.1.29",
          "form": "그렇게 많이 친해진것같아요 ㅋㅋ",
          "original_form": "그렇게 많이 친해진것같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:19"
        }
      ]
    }
