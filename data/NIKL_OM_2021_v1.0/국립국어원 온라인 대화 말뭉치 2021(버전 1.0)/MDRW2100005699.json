      "utterance": [
        {
          "id": "MDRW2100005699.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005699.1.2",
          "form": "대화가능하실까요?",
          "original_form": "대화가능하실까요?",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005699.1.3",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005699.1.4",
          "form": "티비 뭐보고계세요?",
          "original_form": "티비 뭐보고계세요?",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005699.1.5",
          "form": "저 티비안본지 10년이넘었어요 ㅋㅋ",
          "original_form": "저 티비안본지 10년이넘었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005699.1.6",
          "form": "어쩌다 뉴스만 보네요",
          "original_form": "어쩌다 뉴스만 보네요",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005699.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005699.1.8",
          "form": "오 진짜요? 전 집에오면 티비부터 틀어요ㅎ",
          "original_form": "오 진짜요? 전 집에오면 티비부터 틀어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005699.1.9",
          "form": "뉴스도 보고 예능도 보고",
          "original_form": "뉴스도 보고 예능도 보고",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005699.1.10",
          "form": "전 뉴스도 유투브로보고 유투브만 보네요 ㅋㅋ",
          "original_form": "전 뉴스도 유투브로보고 유투브만 보네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005699.1.11",
          "form": "예능요새 뭐있나요?",
          "original_form": "예능요새 뭐있나요?",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005699.1.12",
          "form": "요즘 전참시랑 나혼자산다 유퀴즈 이런거봐요",
          "original_form": "요즘 전참시랑 나혼자산다 유퀴즈 이런거봐요",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005699.1.13",
          "form": "그것이알고싶다도 좋아해요",
          "original_form": "그것이알고싶다도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005699.1.14",
          "form": "유튜브로 뉴스만 보세요?",
          "original_form": "유튜브로 뉴스만 보세요?",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005699.1.15",
          "form": "네 그냥 피드에뜨는거 위주로",
          "original_form": "네 그냥 피드에뜨는거 위주로",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005699.1.16",
          "form": "유투버도 종종봐요",
          "original_form": "유투버도 종종봐요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005699.1.17",
          "form": "이상한유투버 최근에 봤는데",
          "original_form": "이상한유투버 최근에 봤는데",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005699.1.18",
          "form": "정신이 이상해질거같았어요 ㅋㅋㅋ",
          "original_form": "정신이 이상해질거같았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005699.1.19",
          "form": "어떤데요?ㅎㅎ",
          "original_form": "어떤데요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005699.1.20",
          "form": "카광 이라는 유투버인데",
          "original_form": "카광 이라는 유투버인데",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005699.1.21",
          "form": "여장을하고 남자를낚는영상을찍어요 ㅋㅋㅋㅋ",
          "original_form": "여장을하고 남자를낚는영상을찍어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005699.1.22",
          "form": "헐 방송에 내보내도 되는거예요?",
          "original_form": "헐 방송에 내보내도 되는거예요?",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005699.1.23",
          "form": "몰카같은건가요",
          "original_form": "몰카같은건가요",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005699.1.24",
          "form": "유투버는 방송심의가없으니까 다 되나봐요 진짜",
          "original_form": "유투버는 방송심의가없으니까 다 되나봐요 진짜",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005699.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005699.1.26",
          "form": "그런게 많은가봐요",
          "original_form": "그런게 많은가봐요",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005699.1.27",
          "form": "구독자수늘릴려고",
          "original_form": "구독자수늘릴려고",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005699.1.28",
          "form": "네 이상한 유투버 많아요",
          "original_form": "네 이상한 유투버 많아요",
          "speaker_id": "1",
          "time": "20210822 00:04"
        }
      ]
    }
