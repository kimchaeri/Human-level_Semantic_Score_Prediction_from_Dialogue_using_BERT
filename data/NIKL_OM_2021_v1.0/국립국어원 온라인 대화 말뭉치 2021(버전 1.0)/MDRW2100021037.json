      "utterance": [
        {
          "id": "MDRW2100021037.1.1",
          "form": "요즘 컵밥 얼마 정도 해요?",
          "original_form": "요즘 컵밥 얼마 정도 해요?",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100021037.1.2",
          "form": "2천원쯤 할걸요",
          "original_form": "2천원쯤 할걸요",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100021037.1.3",
          "form": "컵밥 하나에 2천원 밖에 안해요?",
          "original_form": "컵밥 하나에 2천원 밖에 안해요?",
          "speaker_id": "1",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100021037.1.4",
          "form": "네 그쯤 해요",
          "original_form": "네 그쯤 해요",
          "speaker_id": "2",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021037.1.5",
          "form": "생각보다 저렴하네요..",
          "original_form": "생각보다 저렴하네요..",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021037.1.6",
          "form": "햇반 하나만해도 거의 천원 넘어가지 않아요?",
          "original_form": "햇반 하나만해도 거의 천원 넘어가지 않아요?",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021037.1.7",
          "form": "쿠팡  가면 생각보다 저렴해요ㅎㅎ",
          "original_form": "쿠팡  가면 생각보다 저렴해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021037.1.8",
          "form": "천오백원짜리도 있고",
          "original_form": "천오백원짜리도 있고",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021037.1.9",
          "form": "아 여러개 한꺼번에 사야 저렴한가보네요",
          "original_form": "아 여러개 한꺼번에 사야 저렴한가보네요",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100021037.1.10",
          "form": "마트 같은데에선 할인하면 3천원 좀 넘더라구요",
          "original_form": "마트 같은데에선 할인하면 3천원 좀 넘더라구요",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100021037.1.11",
          "form": "네 저는 보통 쿠팡에서 대량으로 사요",
          "original_form": "네 저는 보통 쿠팡에서 대량으로 사요",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100021037.1.12",
          "form": "컵반 여러가지 맛이 있던데 어떤거 추천해요?",
          "original_form": "컵반 여러가지 맛이 있던데 어떤거 추천해요?",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100021037.1.13",
          "form": "낙지콩나물비빔밥이 맛있어요",
          "original_form": "낙지콩나물비빔밥이 맛있어요",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021037.1.14",
          "form": "헐 추천 믿고 꼭 그거 먹어볼게요",
          "original_form": "헐 추천 믿고 꼭 그거 먹어볼게요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021037.1.15",
          "form": "가격도 착하니 괜찮네요",
          "original_form": "가격도 착하니 괜찮네요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021037.1.16",
          "form": "그거 하나만 먹어도 충분히 배불러요?",
          "original_form": "그거 하나만 먹어도 충분히 배불러요?",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021037.1.17",
          "form": "저는 배가 불렀는데 친구는 조금 부족하다고 하더라구요",
          "original_form": "저는 배가 불렀는데 친구는 조금 부족하다고 하더라구요",
          "speaker_id": "2",
