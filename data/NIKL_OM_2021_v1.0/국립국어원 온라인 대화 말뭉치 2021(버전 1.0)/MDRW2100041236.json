        },
        {
          "id": "MDRW2100041236.1.2",
          "form": "좋아하는 음식이 있나용?",
          "original_form": "좋아하는 음식이 있나용?",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041236.1.3",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041236.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041236.1.5",
          "form": "저는 곱창 막창 이런거요!",
          "original_form": "저는 곱창 막창 이런거요!",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041236.1.6",
          "form": "기름진음식 좋아하시나봐요",
          "original_form": "기름진음식 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041236.1.7",
          "form": "엇 그런건 아닌데 그게 맛있엉요.,,ㅋㅋㅋ",
          "original_form": "엇 그런건 아닌데 그게 맛있엉요.,,ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041236.1.8",
          "form": "그럼 주로 뭐에 찍어드시나요?",
          "original_form": "그럼 주로 뭐에 찍어드시나요?",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041236.1.9",
          "form": "김치에 먹어요ㅋㅋ! 아님 그냥 생으로 먹구요",
          "original_form": "김치에 먹어요ㅋㅋ! 아님 그냥 생으로 먹구요",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041236.1.10",
          "form": "그럼 그 고소한 맛을 좋아하시나봐요",
          "original_form": "그럼 그 고소한 맛을 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100041236.1.11",
          "form": "앗 그런가봐요ㅎㅎ",
          "original_form": "앗 그런가봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100041236.1.12",
          "form": "그냥 맛있어요ㅋㅋㅋ",
          "original_form": "그냥 맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100041236.1.13",
          "form": "떡볶이는 어디 떡볶이 좋아하세요?",
          "original_form": "떡볶이는 어디 떡볶이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100041236.1.14",
          "form": "저는 엽기떡볶이요",
          "original_form": "저는 엽기떡볶이요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041236.1.15",
          "form": "오 매운맛 좋아하시나봐여",
          "original_form": "오 매운맛 좋아하시나봐여",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041236.1.16",
          "form": "아니에요 그냥 엽떡 맛을 좋아하는거에요ㅋㅋㅋ",
          "original_form": "아니에요 그냥 엽떡 맛을 좋아하는거에요ㅋㅋㅋ",
