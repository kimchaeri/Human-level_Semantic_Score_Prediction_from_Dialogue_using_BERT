      "utterance": [
        {
          "id": "MDRW2100016411.1.1",
          "form": "그럭저럭 재밌음",
          "original_form": "그럭저럭 재밌음",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016411.1.2",
          "form": "근데 진심 대박이야",
          "original_form": "근데 진심 대박이야",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016411.1.3",
          "form": "그거때문에",
          "original_form": "그거때문에",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016411.1.4",
          "form": "넷플릭스",
          "original_form": "넷플릭스",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016411.1.5",
          "form": "할까고민중인데",
          "original_form": "할까고민중인데",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016411.1.6",
          "form": "뭐가?",
          "original_form": "뭐가?",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016411.1.7",
          "form": "진짜 바뀌는 걸 너무 잘보여줌",
          "original_form": "진짜 바뀌는 걸 너무 잘보여줌",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016411.1.8",
          "form": "자세히나와?",
          "original_form": "자세히나와?",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016411.1.9",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016411.1.10",
          "form": "오 좀 끌리는데",
          "original_form": "오 좀 끌리는데",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016411.1.11",
          "form": "그런영화보면",
          "original_form": "그런영화보면",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016411.1.12",
          "form": "끝에항상평등이런식으로끝나서",
          "original_form": "끝에항상평등이런식으로끝나서",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016411.1.13",
          "form": "좀힘빠졌었는데",
          "original_form": "좀힘빠졌었는데",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016411.1.14",
          "form": "이건결말도괜찬ㄹ음?",
          "original_form": "이건결말도괜찬ㄹ음?",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016411.1.15",
          "form": "오 아니 근데 결말이 조금 아쉽긴 함",
          "original_form": "오 아니 근데 결말이 조금 아쉽긴 함",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.16",
          "form": "대충 내용 스포말고",
          "original_form": "대충 내용 스포말고",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.17",
          "form": "어떤식으로아쉬움",
          "original_form": "어떤식으로아쉬움",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.18",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.19",
          "form": "그걸 말하려면 스포야",
          "original_form": "그걸 말하려면 스포야",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.21",
          "form": "대충",
          "original_form": "대충",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.22",
          "form": "뭔가결말이",
          "original_form": "뭔가결말이",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.23",
          "form": "제대로안남?",
          "original_form": "제대로안남?",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016411.1.24",
          "form": "결말은 제대로 남",
          "original_form": "결말은 제대로 남",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016411.1.25",
          "form": "근데 약간 다른 식이어도 되지 않았나 하는 아쉬움",
          "original_form": "근데 약간 다른 식이어도 되지 않았나 하는 아쉬움",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016411.1.26",
          "form": "아 대충 무슨 말인지 알겠음",
          "original_form": "아 대충 무슨 말인지 알겠음",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016411.1.27",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016411.1.28",
          "form": "완전 잘 맞는 영화 찾기가 힘든듯",
          "original_form": "완전 잘 맞는 영화 찾기가 힘든듯",
          "speaker_id": "1",
          "time": "20210826 12:43"
        }
      ]
    }
