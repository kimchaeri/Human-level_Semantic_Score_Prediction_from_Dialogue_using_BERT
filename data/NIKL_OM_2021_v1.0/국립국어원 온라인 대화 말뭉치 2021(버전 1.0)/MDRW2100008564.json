      "utterance": [
        {
          "id": "MDRW2100008564.1.1",
          "form": "오 토스만보기",
          "original_form": "오 토스만보기",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008564.1.2",
          "form": "쏠쏠한가요?",
          "original_form": "쏠쏠한가요?",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008564.1.3",
          "form": "매일 1만보걸으면 100원 벌어요",
          "original_form": "매일 1만보걸으면 100원 벌어요",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008564.1.4",
          "form": "아 저 어제 선풍기샀어요 ㅋㅋ 여름 다 지난 지금 ㅋㅋ",
          "original_form": "아 저 어제 선풍기샀어요 ㅋㅋ 여름 다 지난 지금 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008564.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ왜 지금 사셨어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ왜 지금 사셨어요",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008564.1.6",
          "form": "고장났나요?",
          "original_form": "고장났나요?",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008564.1.7",
          "form": "전부터 사려고 했는데",
          "original_form": "전부터 사려고 했는데",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008564.1.8",
          "form": "집에 있는게 고장이 안나서",
          "original_form": "집에 있는게 고장이 안나서",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008564.1.9",
          "form": "매번 고장나면 사야지하다가",
          "original_form": "매번 고장나면 사야지하다가",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008564.1.10",
          "form": "어제는 도저히 소리가 시끄러워서 ㅋㅋ 샀어요ㅋㅋ",
          "original_form": "어제는 도저히 소리가 시끄러워서 ㅋㅋ 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008564.1.11",
          "form": "아이고 ㅋㅋㅋㅋㅋ",
          "original_form": "아이고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008564.1.12",
          "form": "조금더 빨리사면 좋았을텐데",
          "original_form": "조금더 빨리사면 좋았을텐데",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008564.1.13",
          "form": "그래오 오늘은 좀 더워서",
          "original_form": "그래오 오늘은 좀 더워서",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008564.1.14",
          "form": "딱 좋내요 ㅎㅎ",
          "original_form": "딱 좋내요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008564.1.15",
          "form": "아직 안왔어요 진짜 왜 급한건 꼭 주말에 살까여 ㅋㅣㅣ",
          "original_form": "아직 안왔어요 진짜 왜 급한건 꼭 주말에 살까여 ㅋㅣㅣ",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008564.1.16",
          "form": "아니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008564.1.17",
          "form": "어떻게 웃으면 안되는데 너무 웃갸뇨 ㅋㅋㅋㅋㅋ",
          "original_form": "어떻게 웃으면 안되는데 너무 웃갸뇨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008564.1.18",
          "form": "근데 다들 그렇죠 뭐",
          "original_form": "근데 다들 그렇죠 뭐",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008564.1.19",
          "form": "항상 살건 주말에 생각나고..",
          "original_form": "항상 살건 주말에 생각나고..",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008564.1.20",
          "form": "그리고 꼭 사놓고 외지나간 낭군 기다리듯 오매불망 기다리고 ㅋㅋ",
          "original_form": "그리고 꼭 사놓고 외지나간 낭군 기다리듯 오매불망 기다리고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008564.1.21",
          "form": "아니 그럴바에야 며칠전에 사면 될것을 ㅋㅋ",
          "original_form": "아니 그럴바에야 며칠전에 사면 될것을 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008564.1.22",
          "form": "그게 제일 어려운걸요...",
          "original_form": "그게 제일 어려운걸요...",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008564.1.23",
          "form": "맨날 준비물 급하게 사가는 아이처럼..",
          "original_form": "맨날 준비물 급하게 사가는 아이처럼..",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008564.1.24",
          "form": "그아이는 자라... 똑같이 컷다..",
          "original_form": "그아이는 자라... 똑같이 컷다..",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008564.1.25",
          "form": "지금 날씨가 딱 에어컨켜긴아깝고 선풍기쓸시즌같아요 ㅋㅋ",
          "original_form": "지금 날씨가 딱 에어컨켜긴아깝고 선풍기쓸시즌같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008564.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008564.1.27",
          "form": "에어컨은 아깝죠",
          "original_form": "에어컨은 아깝죠",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008564.1.28",
          "form": "땀은 안나는 정도의 날씨라",
          "original_form": "땀은 안나는 정도의 날씨라",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008564.1.29",
          "form": "저는 쿨토시 열심히 썼어요",
          "original_form": "저는 쿨토시 열심히 썼어요",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008564.1.30",
          "form": "시원하더라고요",
          "original_form": "시원하더라고요",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008564.1.31",
          "form": "근데 습하긴습해요 ㅋㅋ",
          "original_form": "근데 습하긴습해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008564.1.32",
          "form": "오 저도 좀 쓸걸 그랬어요",
          "original_form": "오 저도 좀 쓸걸 그랬어요",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008564.1.33",
          "form": "그거말고는 큐앤큐 손목시계? 사서 열심히 쓰고있네요",
          "original_form": "그거말고는 큐앤큐 손목시계? 사서 열심히 쓰고있네요",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008564.1.34",
          "form": "오 뭐 특별한 손목시계인가요?",
          "original_form": "오 뭐 특별한 손목시계인가요?",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008564.1.35",
          "form": "그냥 싼건데 우레탄 밴드에 야광되고 아 시계판? 시계머리가 커서 숫자가 잘보여요",
          "original_form": "그냥 싼건데 우레탄 밴드에 야광되고 아 시계판? 시계머리가 커서 숫자가 잘보여요",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008564.1.36",
          "form": "오오 신기하내요",
          "original_form": "오오 신기하내요",
          "speaker_id": "1",
          "time": "20210822 17:31"
        }
      ]
    }
  ]
