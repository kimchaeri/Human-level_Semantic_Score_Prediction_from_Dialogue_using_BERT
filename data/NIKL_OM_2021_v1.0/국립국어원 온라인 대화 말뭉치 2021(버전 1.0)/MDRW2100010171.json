      "utterance": [
        {
          "id": "MDRW2100010171.1.1",
          "form": "예전에히가시노책",
          "original_form": "예전에히가시노책",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010171.1.2",
          "form": "좋아했었는데",
          "original_form": "좋아했었는데",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010171.1.3",
          "form": "요즘다시보면",
          "original_form": "요즘다시보면",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010171.1.4",
          "form": "못보겠음",
          "original_form": "못보겠음",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010171.1.5",
          "form": "아 그래? 왜??",
          "original_form": "아 그래? 왜??",
          "speaker_id": "2",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010171.1.6",
          "form": "너무 그런 내용이 많아",
          "original_form": "너무 그런 내용이 많아",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010171.1.7",
          "form": "막 여자는 사랑을 받아야만 하는 존재고",
          "original_form": "막 여자는 사랑을 받아야만 하는 존재고",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010171.1.8",
          "form": "집안일만하고",
          "original_form": "집안일만하고",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010171.1.9",
          "form": "이런거",
          "original_form": "이런거",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010171.1.10",
          "form": "아 맞아 그런 내용 진심",
          "original_form": "아 맞아 그런 내용 진심",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010171.1.11",
          "form": "못 보겠지",
          "original_form": "못 보겠지",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010171.1.12",
          "form": "그분여성관진짜",
          "original_form": "그분여성관진짜",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010171.1.13",
          "form": "참",
          "original_form": "참",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010171.1.14",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010171.1.15",
          "form": "일작가 특유인가 하면 그렇지도 않음",
          "original_form": "일작가 특유인가 하면 그렇지도 않음",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010171.1.16",
          "form": "ㅇㅇㅇㅇ맞음",
          "original_form": "ㅇㅇㅇㅇ맞음",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010171.1.17",
          "form": "얼마전에경제책봤었는데",
          "original_form": "얼마전에경제책봤었는데",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010171.1.18",
          "form": "2000년대초반책이긴함",
          "original_form": "2000년대초반책이긴함",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010171.1.19",
          "form": "근데 미국 남작가였는데",
          "original_form": "근데 미국 남작가였는데",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010171.1.20",
          "form": "꼭 돈보다 여성이런 행복을 누려야된다고",
          "original_form": "꼭 돈보다 여성이런 행복을 누려야된다고",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010171.1.21",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010171.1.22",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010171.1.23",
          "form": "좀 그런 내용이 많았음",
          "original_form": "좀 그런 내용이 많았음",
          "speaker_id": "1",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010171.1.24",
          "form": "그래서여성작가책을찾게되는듯",
          "original_form": "그래서여성작가책을찾게되는듯",
          "speaker_id": "1",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010171.1.25",
          "form": "시대가 변하면서 그런 내용이 적어지는 추세라는게 너무 좋아",
          "original_form": "시대가 변하면서 그런 내용이 적어지는 추세라는게 너무 좋아",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010171.1.26",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010171.1.27",
          "form": "문체나 표현으로 어떤 성별이 썼는지 구별되는게 너무 웃김",
          "original_form": "문체나 표현으로 어떤 성별이 썼는지 구별되는게 너무 웃김",
          "speaker_id": "2",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100010171.1.28",
          "form": "사실 안웃김",
          "original_form": "사실 안웃김",
          "speaker_id": "2",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100010171.1.29",
          "form": "ㅇㅇㅇ그니까 근대소설?",
          "original_form": "ㅇㅇㅇ그니까 근대소설?",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100010171.1.30",
          "form": "무ㅜ라그러지",
          "original_form": "무ㅜ라그러지",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100010171.1.31",
          "form": "운수좋은날같은",
          "original_form": "운수좋은날같은",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100010171.1.32",
          "form": "거",
          "original_form": "거",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100010171.1.33",
          "form": "ㅇㅇ 근대 소설",
          "original_form": "ㅇㅇ 근대 소설",
          "speaker_id": "2",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010171.1.34",
          "form": "한국근대소설은진짜못봐주겠더라",
          "original_form": "한국근대소설은진짜못봐주겠더라",
          "speaker_id": "1",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010171.1.35",
          "form": "시도그렇고",
          "original_form": "시도그렇고",
          "speaker_id": "1",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010171.1.36",
          "form": "그런걸왜교과서에넣는지모르겠음",
          "original_form": "그런걸왜교과서에넣는지모르겠음",
          "speaker_id": "1",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010171.1.37",
          "form": "아 맞아 진짜",
          "original_form": "아 맞아 진짜",
          "speaker_id": "2",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010171.1.38",
          "form": "뭔 의미가 있는지 모르겠고",
          "original_form": "뭔 의미가 있는지 모르겠고",
          "speaker_id": "2",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010171.1.39",
          "form": "ㅇㅇㅇ운수좋은날도그냥",
          "original_form": "ㅇㅇㅇ운수좋은날도그냥",
          "speaker_id": "1",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010171.1.40",
          "form": "가정폭력범내용이잖아",
          "original_form": "가정폭력범내용이잖아",
          "speaker_id": "1",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010171.1.41",
          "form": "맞아 나 구거 처음 보고 충격받았잖아",
          "original_form": "맞아 나 구거 처음 보고 충격받았잖아",
          "speaker_id": "2",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010171.1.42",
          "form": "아내가 아픈데 왜 때려",
          "original_form": "아내가 아픈데 왜 때려",
          "speaker_id": "2",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010171.1.43",
          "form": "그니까 고등학교땐가중학굔가",
          "original_form": "그니까 고등학교땐가중학굔가",
          "speaker_id": "1",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010171.1.44",
          "form": "배웠는데",
          "original_form": "배웠는데",
          "speaker_id": "1",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010171.1.45",
          "form": "그걸츤데레어쩌고하면서",
          "original_form": "그걸츤데레어쩌고하면서",
          "speaker_id": "1",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100010171.1.46",
          "form": "올려쳐주는거보고",
          "original_form": "올려쳐주는거보고",
          "speaker_id": "1",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100010171.1.47",
          "form": "내가비정상인줄알았어",
          "original_form": "내가비정상인줄알았어",
          "speaker_id": "1",
          "time": "20210823 15:14"
        }
      ]
