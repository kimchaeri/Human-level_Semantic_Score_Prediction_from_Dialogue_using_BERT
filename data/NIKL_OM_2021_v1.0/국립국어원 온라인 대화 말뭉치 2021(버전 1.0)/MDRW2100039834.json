      "utterance": [
        {
          "id": "MDRW2100039834.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039834.1.2",
          "form": "네 방금 먹고 왔어요~",
          "original_form": "네 방금 먹고 왔어요~",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039834.1.3",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100039834.1.4",
          "form": "무슨 메뉴로 드셨나요?",
          "original_form": "무슨 메뉴로 드셨나요?",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100039834.1.5",
          "form": "소불고기랑 깻잎먹었어요",
          "original_form": "소불고기랑 깻잎먹었어요",
          "speaker_id": "1",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100039834.1.6",
          "form": "와 맛있는 식사였군요!",
          "original_form": "와 맛있는 식사였군요!",
          "speaker_id": "2",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100039834.1.7",
          "form": "직접 해먹은건가요?",
          "original_form": "직접 해먹은건가요?",
          "speaker_id": "2",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100039834.1.8",
          "form": "엄마가 만들어주셨어요",
          "original_form": "엄마가 만들어주셨어요",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100039834.1.9",
          "form": "제가 하진않구요",
          "original_form": "제가 하진않구요",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100039834.1.10",
          "form": "그러셨군요",
          "original_form": "그러셨군요",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100039834.1.11",
          "form": "깻잎을 좋아하시나요 상추를 좋아하시나요?",
          "original_form": "깻잎을 좋아하시나요 상추를 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100039834.1.12",
          "form": "상추가 조금더 좋아요 ㅎㅎ",
          "original_form": "상추가 조금더 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100039834.1.13",
          "form": "그러시군요 ㅎㅎ",
          "original_form": "그러시군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100039834.1.14",
          "form": "배달음식은 주로 뭐 좋아하시나요?",
          "original_form": "배달음식은 주로 뭐 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100039834.1.15",
          "form": "피자좋아요 ㅎㅎ 뭐 배달자주하세요?",
          "original_form": "피자좋아요 ㅎㅎ 뭐 배달자주하세요?",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100039834.1.16",
          "form": "피자 저도 좋아해요 ㅎㅎ",
          "original_form": "피자 저도 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100039834.1.17",
          "form": "피자가 은근 매력 있어요",
          "original_form": "피자가 은근 매력 있어요",
          "speaker_id": "1",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100039834.1.18",
          "form": "비싸지만요 ㅎㅎ",
