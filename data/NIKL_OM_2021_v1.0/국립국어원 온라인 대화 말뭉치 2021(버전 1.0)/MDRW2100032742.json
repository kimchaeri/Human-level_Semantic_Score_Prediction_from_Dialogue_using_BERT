      "utterance": [
        {
          "id": "MDRW2100032742.1.1",
          "form": "코로나 끝나면 어디로 여행가고싶어",
          "original_form": "코로나 끝나면 어디로 여행가고싶어",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032742.1.2",
          "form": "해외가고싶심니다 ㅠㅜ",
          "original_form": "해외가고싶심니다 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032742.1.3",
          "form": "원래 크게 관심은없기도햇지만",
          "original_form": "원래 크게 관심은없기도햇지만",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032742.1.4",
          "form": "해외 어디루 ㅋㅋ",
          "original_form": "해외 어디루 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032742.1.5",
          "form": "제대로가본적도없고해섴ㅋㅋㅋ",
          "original_form": "제대로가본적도없고해섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032742.1.6",
          "form": "스위스 넘 가고싶다",
          "original_form": "스위스 넘 가고싶다",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032742.1.7",
          "form": "스위스 크...진짜 얼마나 예쁠지 넘 궁금",
          "original_form": "스위스 크...진짜 얼마나 예쁠지 넘 궁금",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032742.1.8",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032742.1.9",
          "form": "근데 그냥 제주도 오래놀러가고싶긴함ㅋㅋㅋㅋㅋ",
          "original_form": "근데 그냥 제주도 오래놀러가고싶긴함ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032742.1.10",
          "form": "ㅋㅋㅋㅋㅋ제주도 한달살이 이런거?",
          "original_form": "ㅋㅋㅋㅋㅋ제주도 한달살이 이런거?",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032742.1.11",
          "form": "예전에 그거 꽂혀서 막 제주도 펜션 알바 찾아보고 그랬는데 ㅋㅋ",
          "original_form": "예전에 그거 꽂혀서 막 제주도 펜션 알바 찾아보고 그랬는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032742.1.12",
          "form": "ㅋㅋㅋㅋㅋ아 한달살기 이런거",
          "original_form": "ㅋㅋㅋㅋㅋ아 한달살기 이런거",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032742.1.13",
          "form": "진짜 제주도살규싶다",
          "original_form": "진짜 제주도살규싶다",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032742.1.14",
          "form": "난 한달말고 2주정도...",
          "original_form": "난 한달말고 2주정도...",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032742.1.15",
          "form": "근데 살려면 차가 있어야할듯 ㅋㅋ큐ㅠㅠ",
          "original_form": "근데 살려면 차가 있어야할듯 ㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032742.1.16",
          "form": "맞닼ㅋㅋㅋㅋㅋ차는 진짜 잇우야할듯 ㅜㅜ",
          "original_form": "맞닼ㅋㅋㅋㅋㅋ차는 진짜 잇우야할듯 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032742.1.17",
          "form": "차있으면 여행편하게 다닐거가텨",
          "original_form": "차있으면 여행편하게 다닐거가텨",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032742.1.18",
          "form": "ㅇㅇ 저번에 차없이 그냥 택시 잘 타긴했지만",
          "original_form": "ㅇㅇ 저번에 차없이 그냥 택시 잘 타긴했지만",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032742.1.19",
          "form": "그래도 콜 잘 안잡히거나 그러면 좀...",
          "original_form": "그래도 콜 잘 안잡히거나 그러면 좀...",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032742.1.20",
          "form": "마자...그렇겟다",
          "original_form": "마자...그렇겟다",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032742.1.21",
          "form": "제주여행가자..제주도 가곱네 ㅜㅜ",
          "original_form": "제주여행가자..제주도 가곱네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032742.1.22",
          "form": "차있으면 근교에 여행도 편하게 잘갈듯 ㅠㅜ",
          "original_form": "차있으면 근교에 여행도 편하게 잘갈듯 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032742.1.23",
          "form": "ㅜㅜ 맞음...차있으면 일단 뭔가 범위가 확 넓어지지",
          "original_form": "ㅜㅜ 맞음...차있으면 일단 뭔가 범위가 확 넓어지지",
          "speaker_id": "1",
          "time": "20210905 23:09"
        }
      ]
    }
  ]
