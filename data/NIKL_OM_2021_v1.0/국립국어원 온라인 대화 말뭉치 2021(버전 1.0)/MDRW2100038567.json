      "utterance": [
        {
          "id": "MDRW2100038567.1.1",
          "form": "저는 반 민초파에요 ....",
          "original_form": "저는 반 민초파에요 ....",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038567.1.2",
          "form": "좋아하시나봐요!",
          "original_form": "좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038567.1.3",
          "form": "헉 네",
          "original_form": "헉 네",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100038567.1.4",
          "form": "전 좋아해요",
          "original_form": "전 좋아해요",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100038567.1.5",
          "form": "민초 아예 안드세요?",
          "original_form": "민초 아예 안드세요?",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100038567.1.6",
          "form": "억지로 먹으려면 먹는데...제돈주고 사먹진 않는ㅜㅜ",
          "original_form": "억지로 먹으려면 먹는데...제돈주고 사먹진 않는ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038567.1.7",
          "form": "입맛에 맞으세여!?!!",
          "original_form": "입맛에 맞으세여!?!!",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038567.1.8",
          "form": "네 전 요새는 보통이였는데",
          "original_form": "네 전 요새는 보통이였는데",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038567.1.9",
          "form": "배라 먹고 빠졌습니다..!",
          "original_form": "배라 먹고 빠졌습니다..!",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038567.1.10",
          "form": "아앗..그러시군요!! 취존해요ㅋㅋㅋ",
          "original_form": "아앗..그러시군요!! 취존해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038567.1.11",
          "form": "전 너무 지나치게 상쾌한맛이 별로인거같아요ㅠㅠ",
          "original_form": "전 너무 지나치게 상쾌한맛이 별로인거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038567.1.12",
          "form": "아 뭔지 알거 같아요 ㅎ",
          "original_form": "아 뭔지 알거 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038567.1.13",
          "form": "그럼 하와이안피자는",
          "original_form": "그럼 하와이안피자는",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038567.1.14",
          "form": "어떠세요?",
          "original_form": "어떠세요?",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038567.1.15",
          "form": "그건 좋아해요ㅎㅎㅎ파인애플 달달하니 맛나죵",
          "original_form": "그건 좋아해요ㅎㅎㅎ파인애플 달달하니 맛나죵",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038567.1.16",
          "form": "저랑 반대네요",
          "original_form": "저랑 반대네요",
          "speaker_id": "1",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100038567.1.17",
          "form": "전 그거 안좋아해요!!",
          "original_form": "전 그거 안좋아해요!!",
          "speaker_id": "1",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100038567.1.18",
          "form": "아앗ㅋㅋㅋ그러시군여!??달달한 피자는 별로신가봐요",
          "original_form": "아앗ㅋㅋㅋ그러시군여!??달달한 피자는 별로신가봐요",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100038567.1.19",
          "form": "파인애플은 그냥 꾸어먹어도 넘 맛나는데",
          "original_form": "파인애플은 그냥 꾸어먹어도 넘 맛나는데",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100038567.1.20",
          "form": "파인애플은 구운게 별로에요ㅎ",
          "original_form": "파인애플은 구운게 별로에요ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
