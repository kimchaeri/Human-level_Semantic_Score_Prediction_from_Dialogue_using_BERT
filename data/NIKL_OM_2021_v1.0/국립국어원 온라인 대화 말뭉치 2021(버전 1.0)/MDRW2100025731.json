      "utterance": [
        {
          "id": "MDRW2100025731.1.1",
          "form": "낼이랑 모레는 흐리기만하고 비는 안온대",
          "original_form": "낼이랑 모레는 흐리기만하고 비는 안온대",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025731.1.2",
          "form": "안그래도 어플보니 오늘만 오네 ㅋㅋㅋ",
          "original_form": "안그래도 어플보니 오늘만 오네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025731.1.3",
          "form": "이제 비 좀 그친거 같네ㅋㅋ",
          "original_form": "이제 비 좀 그친거 같네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025731.1.4",
          "form": "많이는 안 오는데 계속 오네 춥다 ㅋx",
          "original_form": "많이는 안 오는데 계속 오네 춥다 ㅋx",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100025731.1.5",
          "form": "응 나도 요즘 계속 추워서 전기매트 틀고 잔다ㅋㅋ",
          "original_form": "응 나도 요즘 계속 추워서 전기매트 틀고 잔다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100025731.1.6",
          "form": "아직 그 정도는 아니지만 ㅋㅋㅋ 가을 잠옷 사고싶네",
          "original_form": "아직 그 정도는 아니지만 ㅋㅋㅋ 가을 잠옷 사고싶네",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025731.1.7",
          "form": "난 집에선 그냥 아무거나 주워입는다ㅋㅋㅋ",
          "original_form": "난 집에선 그냥 아무거나 주워입는다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025731.1.8",
          "form": "반팔 반바지 춥넹",
          "original_form": "반팔 반바지 춥넹",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100025731.1.9",
          "form": "그래서 나도 요즘 긴옷 주워입는다ㅋ",
          "original_form": "그래서 나도 요즘 긴옷 주워입는다ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100025731.1.10",
          "form": "나도 긴옷 입어야겠다 ㅋㅋㅋ",
          "original_form": "나도 긴옷 입어야겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100025731.1.11",
          "form": "이젠 창문도 닫고잔다ㅋㅋ 찬바람 무서웡ㅋㅋ",
          "original_form": "이젠 창문도 닫고잔다ㅋㅋ 찬바람 무서웡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100025731.1.12",
          "form": "난 원래 꽁꽁 닫고 잠 ㅋㅋㅋ",
          "original_form": "난 원래 꽁꽁 닫고 잠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100025731.1.13",
          "form": "한여름에도?",
          "original_form": "한여름에도?",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100025731.1.14",
          "form": "응 집이 1층이니께 ㅋㅋㅋ",
          "original_form": "응 집이 1층이니께 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100025731.1.15",
          "form": "난 자취할때도 3층 살때도 창문 무조건 잠그고 잤는데",
          "original_form": "난 자취할때도 3층 살때도 창문 무조건 잠그고 잤는데",
          "speaker_id": "1",
