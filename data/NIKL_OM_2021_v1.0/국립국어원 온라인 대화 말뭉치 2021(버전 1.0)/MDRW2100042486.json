      "utterance": [
        {
          "id": "MDRW2100042486.1.1",
          "form": "저렇게 말하는 사람들이 있으니까",
          "original_form": "저렇게 말하는 사람들이 있으니까",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.2",
          "form": "선생님들이",
          "original_form": "선생님들이",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.3",
          "form": "맞아요 좋게 해줄라 해도",
          "original_form": "맞아요 좋게 해줄라 해도",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.4",
          "form": "굳이 굳이 사진을 찍어주거나",
          "original_form": "굳이 굳이 사진을 찍어주거나",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.5",
          "form": "잘하려고 안하는 거 같다",
          "original_form": "잘하려고 안하는 거 같다",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.6",
          "form": "또 말나 오니까",
          "original_form": "또 말나 오니까",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.7",
          "form": "근데 애가 나올 수도 있고",
          "original_form": "근데 애가 나올 수도 있고",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.8",
          "form": "안 나올 수도 있지",
          "original_form": "안 나올 수도 있지",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.9",
          "form": "그냥 애초부터 안할라고 하는 거죠",
          "original_form": "그냥 애초부터 안할라고 하는 거죠",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.10",
          "form": "그게 도대체 왜 중요한지",
          "original_form": "그게 도대체 왜 중요한지",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.11",
          "form": "알 수가 없다",
          "original_form": "알 수가 없다",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.13",
          "form": "내같아도 하기 싫겠다",
          "original_form": "내같아도 하기 싫겠다",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042486.1.14",
          "form": "나도 엄마지만 엄마들 맞추기 참 힘든ㅋㅋㅋ",
          "original_form": "나도 엄마지만 엄마들 맞추기 참 힘든ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.15",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.16",
          "form": "그 앞전에 그랬었대요",
          "original_form": "그 앞전에 그랬었대요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.17",
          "form": "그럴 거면 학교에 씨씨티브이를 설치하지",
          "original_form": "그럴 거면 학교에 씨씨티브이를 설치하지",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.18",
          "form": "그래서 본인은 카메라도 자기가 사고",
          "original_form": "그래서 본인은 카메라도 자기가 사고",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.19",
          "form": "헐 카메라까지",
          "original_form": "헐 카메라까지",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.20",
          "form": "시간 시간 들여가면서 편집하고 올리고 하는데",
          "original_form": "시간 시간 들여가면서 편집하고 올리고 하는데",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.21",
          "form": "그런 말 들으니까 너무 속상하더라면 선",
          "original_form": "그런 말 들으니까 너무 속상하더라면 선",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.22",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.23",
          "form": "안하는 선생님이 아홉 명일 텐데",
          "original_form": "안하는 선생님이 아홉 명일 텐데",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.24",
          "form": "이 선생님은 진짜 진심 임ㅋㅋㅋㅋ",
          "original_form": "이 선생님은 진짜 진심 임ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.25",
          "form": "사진 찍어주는 선생님 그 선생님 밖에 없는 거 아니야??",
          "original_form": "사진 찍어주는 선생님 그 선생님 밖에 없는 거 아니야??",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042486.1.26",
          "form": "그래 그 선생님은 진짜 진심이다",
          "original_form": "그래 그 선생님은 진짜 진심이다",
          "speaker_id": "2",
          "time": "20210906 22:25"
        }
      ]
    }
