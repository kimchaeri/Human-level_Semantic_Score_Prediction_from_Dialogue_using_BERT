      "utterance": [
        {
          "id": "MDRW2100013489.1.1",
          "form": "요즘은 매일매일 비가 내리는듯해요",
          "original_form": "요즘은 매일매일 비가 내리는듯해요",
          "speaker_id": "2",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100013489.1.2",
          "form": "습해요ㅠㅠ",
          "original_form": "습해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100013489.1.3",
          "form": "그러게요 태풍이 아직도 있으려나",
          "original_form": "그러게요 태풍이 아직도 있으려나",
          "speaker_id": "1",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100013489.1.4",
          "form": "정말 제습기를 매일 풀가동하네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "정말 제습기를 매일 풀가동하네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100013489.1.5",
          "form": "얼마전에 제습기를 샀는데 요즘 잘 샀다고 생각하는 것중 하나에요",
          "original_form": "얼마전에 제습기를 샀는데 요즘 잘 샀다고 생각하는 것중 하나에요",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100013489.1.6",
          "form": "물이 콸콸 나오죠?",
          "original_form": "물이 콸콸 나오죠?",
          "speaker_id": "1",
          "time": "20210828 09:51"
        },
        {
          "id": "MDRW2100013489.1.7",
          "form": "네ㅋㅋㅋ몇시간이면 그 큰 물통이 다 차더라구요",
          "original_form": "네ㅋㅋㅋ몇시간이면 그 큰 물통이 다 차더라구요",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100013489.1.8",
          "form": "습하지만 않아도 덜 찝찝하니깐ㅋㅋㅋ",
          "original_form": "습하지만 않아도 덜 찝찝하니깐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100013489.1.9",
          "form": "오늘도 선선한하루였네요",
          "original_form": "오늘도 선선한하루였네요",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100013489.1.10",
          "form": "점점 추워지는듯해요",
          "original_form": "점점 추워지는듯해요",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100013489.1.11",
          "form": "오늘은 긴팔입고 출근했네요",
          "original_form": "오늘은 긴팔입고 출근했네요",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100013489.1.12",
          "form": "가디건이 딱필요해요 ㅎ 가을이네요",
          "original_form": "가디건이 딱필요해요 ㅎ 가을이네요",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100013489.1.13",
          "form": "맞아요! 내일부터 또 비온다는데",
          "original_form": "맞아요! 내일부터 또 비온다는데",
          "speaker_id": "2",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100013489.1.14",
          "form": "비 그치고나면 진짜 추워질거같아요",
          "original_form": "비 그치고나면 진짜 추워질거같아요",
          "speaker_id": "2",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100013489.1.15",
          "form": "마음도 쓸쓸해지는 가을이오네요",
          "original_form": "마음도 쓸쓸해지는 가을이오네요",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100013489.1.16",
          "form": "오늘은 비가 많이온대요",
          "original_form": "오늘은 비가 많이온대요",
          "speaker_id": "2",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100013489.1.17",
          "form": "그러게요 호우주의보라니 ㅎㄷㄷ",
          "original_form": "그러게요 호우주의보라니 ㅎㄷㄷ",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100013489.1.18",
          "form": "진짜 많이오네요",
          "original_form": "진짜 많이오네요",
          "speaker_id": "2",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100013489.1.19",
          "form": "출근길에 장화를 신을까 고민하다가 신었는데 아주 잘한 선택이었어요",
          "original_form": "출근길에 장화를 신을까 고민하다가 신었는데 아주 잘한 선택이었어요",
          "speaker_id": "2",
          "time": "20210901 00:53"
        }
      ]
