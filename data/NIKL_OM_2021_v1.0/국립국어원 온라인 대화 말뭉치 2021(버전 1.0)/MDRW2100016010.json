      "utterance": [
        {
          "id": "MDRW2100016010.1.1",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210826 07:32"
        },
        {
          "id": "MDRW2100016010.1.2",
          "form": "아뇨! 아침은 여덟시에먹어요",
          "original_form": "아뇨! 아침은 여덟시에먹어요",
          "speaker_id": "1",
          "time": "20210826 07:32"
        },
        {
          "id": "MDRW2100016010.1.3",
          "form": "아침 메뉴는 정하셨나요",
          "original_form": "아침 메뉴는 정하셨나요",
          "speaker_id": "2",
          "time": "20210826 07:32"
        },
        {
          "id": "MDRW2100016010.1.4",
          "form": "아마 오늘아침은 미역국밥 먹을거같아요!",
          "original_form": "아마 오늘아침은 미역국밥 먹을거같아요!",
          "speaker_id": "1",
          "time": "20210826 07:33"
        },
        {
          "id": "MDRW2100016010.1.5",
          "form": "미역국 너무 맛있겠다",
          "original_form": "미역국 너무 맛있겠다",
          "speaker_id": "2",
          "time": "20210826 07:33"
        },
        {
          "id": "MDRW2100016010.1.6",
          "form": "끓여드시는건가요 인스턴트 드시는건가요",
          "original_form": "끓여드시는건가요 인스턴트 드시는건가요",
          "speaker_id": "2",
          "time": "20210826 07:33"
        },
        {
          "id": "MDRW2100016010.1.7",
          "form": "맛있어요~ 어제먹다남은거 끓이는거라 국물더시원할거같아요 ㅎㅎ",
          "original_form": "맛있어요~ 어제먹다남은거 끓이는거라 국물더시원할거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 07:34"
        },
        {
          "id": "MDRW2100016010.1.8",
          "form": "만드신거구나",
          "original_form": "만드신거구나",
          "speaker_id": "2",
          "time": "20210826 07:34"
        },
        {
          "id": "MDRW2100016010.1.9",
          "form": "참 직접만든거에요",
          "original_form": "참 직접만든거에요",
          "speaker_id": "1",
          "time": "20210826 07:34"
        },
        {
          "id": "MDRW2100016010.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 07:34"
        },
        {
          "id": "MDRW2100016010.1.11",
          "form": "맞아요 국은 끓이면 끓일수록 맛나더라구요",
          "original_form": "맞아요 국은 끓이면 끓일수록 맛나더라구요",
          "speaker_id": "2",
          "time": "20210826 07:34"
        },
        {
          "id": "MDRW2100016010.1.12",
          "form": "특히 미역국은 미역에서 나오는 맛이 더 감칠맛을 더해주고요",
          "original_form": "특히 미역국은 미역에서 나오는 맛이 더 감칠맛을 더해주고요",
          "speaker_id": "2",
          "time": "20210826 07:34"
        },
        {
          "id": "MDRW2100016010.1.13",
          "form": "뭐 넣고 끓이셨나요,",
          "original_form": "뭐 넣고 끓이셨나요,",
          "speaker_id": "2",
          "time": "20210826 07:35"
        },
        {
          "id": "MDRW2100016010.1.14",
          "form": "맞아요! 저두 첫번째한것보다 두세번째끓인거 더 좋아해요~",
          "original_form": "맞아요! 저두 첫번째한것보다 두세번째끓인거 더 좋아해요~",
          "speaker_id": "1",
          "time": "20210826 07:35"
        },
        {
          "id": "MDRW2100016010.1.15",
          "form": "전 미역국은 무조건 소고기요",
          "original_form": "전 미역국은 무조건 소고기요",
          "speaker_id": "1",
          "time": "20210826 07:35"
        },
        {
          "id": "MDRW2100016010.1.16",
          "form": "맞아요 소고기넣은게 적당히 기름지고 맛있더라구요",
          "original_form": "맞아요 소고기넣은게 적당히 기름지고 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 07:35"
        },
        {
          "id": "MDRW2100016010.1.17",
          "form": "그쵸! 생선보다는 소고기가 더 맛나요",
          "original_form": "그쵸! 생선보다는 소고기가 더 맛나요",
          "speaker_id": "1",
          "time": "20210826 07:36"
        },
        {
          "id": "MDRW2100016010.1.18",
          "form": "반찬은 뭐 같이 드시나요",
          "original_form": "반찬은 뭐 같이 드시나요",
          "speaker_id": "2",
          "time": "20210826 07:36"
        }
      ]
