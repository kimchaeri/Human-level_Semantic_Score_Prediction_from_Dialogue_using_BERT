      "utterance": [
        {
          "id": "MDRW2100007488.1.1",
          "form": "비 왔어요?",
          "original_form": "비 왔어요?",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007488.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007488.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007488.1.4",
          "form": "여긴 구멍 뚫린줄",
          "original_form": "여긴 구멍 뚫린줄",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007488.1.5",
          "form": "진짜 미친듯이 퍼붓더라구요",
          "original_form": "진짜 미친듯이 퍼붓더라구요",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007488.1.6",
          "form": "태풍온다하더러고요",
          "original_form": "태풍온다하더러고요",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007488.1.7",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007488.1.8",
          "form": "아니 여름 폭염 어제같은데",
          "original_form": "아니 여름 폭염 어제같은데",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.10",
          "form": "태풍이래요 이게",
          "original_form": "태풍이래요 이게",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.11",
          "form": "누가 가을장마래서 장마인줄",
          "original_form": "누가 가을장마래서 장마인줄",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.12",
          "form": "엇 장마인가",
          "original_form": "엇 장마인가",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.13",
          "form": "진짜 더웠던거 엊그제같은데",
          "original_form": "진짜 더웠던거 엊그제같은데",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.14",
          "form": "그니까요 이제 제습기만 틀어야하는 때가 왔어요",
          "original_form": "그니까요 이제 제습기만 틀어야하는 때가 왔어요",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.15",
          "form": "그럼 장마&태풍이라고 해요",
          "original_form": "그럼 장마&태풍이라고 해요",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.16",
          "form": "겨울이 무서워요",
          "original_form": "겨울이 무서워요",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.17",
          "form": "현자시네요",
          "original_form": "현자시네요",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.18",
          "form": "어떤점이요?",
          "original_form": "어떤점이요?",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007488.1.19",
          "form": "겨울 추워서?",
          "original_form": "겨울 추워서?",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007488.1.20",
          "form": "아니면 음..",
          "original_form": "아니면 음..",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007488.1.21",
          "form": "눈이 싫으신건가",
          "original_form": "눈이 싫으신건가",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007488.1.22",
          "form": "엇 아뇨 추워서요",
          "original_form": "엇 아뇨 추워서요",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007488.1.23",
          "form": "혹시 겨울 좋아하시나요",
          "original_form": "혹시 겨울 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007488.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007488.1.25",
          "form": "여름보다는 나아요",
          "original_form": "여름보다는 나아요",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007488.1.26",
          "form": "전 더운게 더 싫어서ㅠ",
          "original_form": "전 더운게 더 싫어서ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007488.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007488.1.28",
          "form": "저도 여름보다는 겨울이 좋아욬ㅋㅋㅋ",
          "original_form": "저도 여름보다는 겨울이 좋아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007488.1.29",
          "form": "그럼 눈이 좋아요 비가 좋아요",
          "original_form": "그럼 눈이 좋아요 비가 좋아요",
          "speaker_id": "1",
          "time": "20210822 02:32"
        }
      ]
    }
