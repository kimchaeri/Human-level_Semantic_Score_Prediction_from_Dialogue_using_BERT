      "utterance": [
        {
          "id": "MDRW2100018840.1.1",
          "form": "사랑으로 좀 키워봐라",
          "original_form": "사랑으로 좀 키워봐라",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018840.1.2",
          "form": "때리지 말고",
          "original_form": "때리지 말고",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018840.1.3",
          "form": "우리 신랑이 주인 무는 거는 맞아야 된다고",
          "original_form": "우리 신랑이 주인 무는 거는 맞아야 된다고",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018840.1.4",
          "form": "나는 요즘 옆에 가지도 않아요",
          "original_form": "나는 요즘 옆에 가지도 않아요",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018840.1.5",
          "form": "밥도 첫 찌가 주고",
          "original_form": "밥도 첫 찌가 주고",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018840.1.6",
          "form": "나랑 안 맞아요",
          "original_form": "나랑 안 맞아요",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018840.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ 그래 애 키우기도 힘든데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ 그래 애 키우기도 힘든데",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.8",
          "form": "반려동물은 힘들다",
          "original_form": "반려동물은 힘들다",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.9",
          "form": "우리집에 아직도 고양이\r\n사료있음",
          "original_form": "우리집에 아직도 고양이\r\n사료있음",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.10",
          "form": "전에 그 초등학교 고양이",
          "original_form": "전에 그 초등학교 고양이",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.11",
          "form": "버려야 되는데 버리지 못하고 있음",
          "original_form": "버려야 되는데 버리지 못하고 있음",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.12",
          "form": "중학교에 고양이 요즘 안 보이던데",
          "original_form": "중학교에 고양이 요즘 안 보이던데",
          "speaker_id": "1",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.13",
          "form": "아 요즘 그 초등학교는 안가요",
          "original_form": "아 요즘 그 초등학교는 안가요",
          "speaker_id": "1",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.14",
          "form": "어 이제는 안가 고양이 없어서",
          "original_form": "어 이제는 안가 고양이 없어서",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.15",
          "form": "거기 가면 계속 고양이 생각나 가지고",
          "original_form": "거기 가면 계속 고양이 생각나 가지고",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018840.1.16",
          "form": "아직도 우리 투 그거 고양이 보고 싶다고 계속 얘기해",
          "original_form": "아직도 우리 투 그거 고양이 보고 싶다고 계속 얘기해",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018840.1.17",
          "form": "그래도 키우자고는 안하네요",
          "original_form": "그래도 키우자고는 안하네요",
          "speaker_id": "1",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018840.1.18",
          "form": "키우면 언니가 더 잘해줄 듯",
          "original_form": "키우면 언니가 더 잘해줄 듯",
          "speaker_id": "1",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018840.1.19",
          "form": "키우자고 계속 얘기하지",
          "original_form": "키우자고 계속 얘기하지",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018840.1.20",
          "form": "근데 일단 원투 키우기도 힘들어서",
          "original_form": "근데 일단 원투 키우기도 힘들어서",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018840.1.21",
          "form": "나중에 투한테 너 초등학교 가면",
          "original_form": "나중에 투한테 너 초등학교 가면",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018840.1.22",
          "form": "스스로  잘할 수 있을 때 키워보자고 얘기했음",
          "original_form": "스스로  잘할 수 있을 때 키워보자고 얘기했음",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018840.1.23",
          "form": "그쯤엔 키워도 괜찮을 듯",
          "original_form": "그쯤엔 키워도 괜찮을 듯",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018840.1.24",
          "form": "애들이 강아지 데리고 산책 나오고 하는 거 보면",
          "original_form": "애들이 강아지 데리고 산책 나오고 하는 거 보면",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018840.1.25",
          "form": "뭔가 좋아 보임",
          "original_form": "뭔가 좋아 보임",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018840.1.26",
          "form": "그치만 나도 키우라면 고양이가  아을 든요",
          "original_form": "그치만 나도 키우라면 고양이가  아을 든요",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018840.1.27",
          "form": "마치 애들이 강아지 데리고 산책 나오는 거 보면",
          "original_form": "마치 애들이 강아지 데리고 산책 나오는 거 보면",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.28",
          "form": "얼굴에 뭔가가 안정감이 있음",
          "original_form": "얼굴에 뭔가가 안정감이 있음",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.29",
          "form": "어제 후배 랑도 그 강아지 데리고 나오는 애들 얘기 했는데",
          "original_form": "어제 후배 랑도 그 강아지 데리고 나오는 애들 얘기 했는데",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.30",
          "form": "얼굴이 편안해 보임",
          "original_form": "얼굴이 편안해 보임",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.31",
          "form": "맞아요 동물을 사랑할 수 있어야",
          "original_form": "맞아요 동물을 사랑할 수 있어야",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.32",
          "form": "사람도 사랑할 수 있음",
          "original_form": "사람도 사랑할 수 있음",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.33",
          "form": "난 누구도 사랑할 수 없음",
          "original_form": "난 누구도 사랑할 수 없음",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.35",
          "form": "ㅋㅋㅋㅋㅋㄱㄱ 아놔",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱ 아놔",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.36",
          "form": "대화에 흐름은 끊지 마라",
          "original_form": "대화에 흐름은 끊지 마라",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.37",
          "form": "지금 딱 좋았잖아",
          "original_form": "지금 딱 좋았잖아",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018840.1.38",
          "form": "훈훈하게 이어가고 있는데",
          "original_form": "훈훈하게 이어가고 있는데",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018840.1.39",
          "form": "근데 나도 애들 크면 한 번은 생각해 볼 거 같아요",
          "original_form": "근데 나도 애들 크면 한 번은 생각해 볼 거 같아요",
          "speaker_id": "1",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018840.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋ 뻥치지마라",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 뻥치지마라",
          "speaker_id": "2",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018840.1.41",
          "form": "생각은 해 볼 수 있잖아요",
          "original_form": "생각은 해 볼 수 있잖아요",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018840.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018840.1.43",
