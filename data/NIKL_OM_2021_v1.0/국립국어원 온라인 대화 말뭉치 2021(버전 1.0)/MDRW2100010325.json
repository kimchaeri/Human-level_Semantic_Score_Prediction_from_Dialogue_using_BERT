      "utterance": [
        {
          "id": "MDRW2100010325.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010325.1.2",
          "form": "키우진 않지만 고양이관심많아요!",
          "original_form": "키우진 않지만 고양이관심많아요!",
          "speaker_id": "1",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010325.1.3",
          "form": "저도 키우지 않아서 .. 나중에 여건이 되면 키우고 싶은 마음은 있어요!",
          "original_form": "저도 키우지 않아서 .. 나중에 여건이 되면 키우고 싶은 마음은 있어요!",
          "speaker_id": "2",
          "time": "20210823 16:58"
        },
        {
          "id": "MDRW2100010325.1.4",
          "form": "저도요..! name2카를 키울수있다면 좋을텐데",
          "original_form": "저도요..! &name2&카를 키울수있다면 좋을텐데",
          "speaker_id": "1",
          "time": "20210823 17:01"
        },
        {
          "id": "MDRW2100010325.1.5",
          "form": "쿼카 한번이라도 봤으면 좋겠네요 ..ㅠㅠ",
          "original_form": "쿼카 한번이라도 봤으면 좋겠네요 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010325.1.6",
          "form": "햄스터 좋아하세요? 예전에 햄스터는 키워본적이 있어요",
          "original_form": "햄스터 좋아하세요? 예전에 햄스터는 키워본적이 있어요",
          "speaker_id": "2",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010325.1.7",
          "form": "영상으로는 많이봤어요!",
          "original_form": "영상으로는 많이봤어요!",
          "speaker_id": "1",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010325.1.8",
          "form": "진짜 입안가득 음식먹나용ㅋㅋㅋ",
          "original_form": "진짜 입안가득 음식먹나용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010325.1.9",
          "form": "네네 볼주머니에 가득 넣더라구요ㅋㅋ",
          "original_form": "네네 볼주머니에 가득 넣더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010325.1.10",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "2",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010325.1.11",
          "form": "진짜 그렇군요ㅋㅋㅋㅋ",
          "original_form": "진짜 그렇군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010325.1.12",
          "form": "쪼그매서 더 귀여워요!",
          "original_form": "쪼그매서 더 귀여워요!",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010325.1.13",
          "form": "맞아요 .. 제가 키우던 햄스터는 펄햄스터여서",
          "original_form": "맞아요 .. 제가 키우던 햄스터는 펄햄스터여서",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010325.1.14",
          "form": "털이 하얀색에 발바닥이 핑크색이었어요!",
          "original_form": "털이 하얀색에 발바닥이 핑크색이었어요!",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010325.1.15",
          "form": "헐 귀여워요!!!",
          "original_form": "헐 귀여워요!!!",
          "speaker_id": "1",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010325.1.16",
          "form": "핑크색도는 부분이 진짜 포인트에요ㅋㅋ",
          "original_form": "핑크색도는 부분이 진짜 포인트에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010325.1.17",
          "form": "그니까요 ! 발바닥이 너무 귀엽더라구요ㅎㅎ",
          "original_form": "그니까요 ! 발바닥이 너무 귀엽더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010325.1.18",
          "form": "올망졸망한 눈도 너무 귀여워요!",
          "original_form": "올망졸망한 눈도 너무 귀여워요!",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010325.1.19",
          "form": "초롱초롱한 눈도 귀여워요!",
          "original_form": "초롱초롱한 눈도 귀여워요!",
          "speaker_id": "1",
