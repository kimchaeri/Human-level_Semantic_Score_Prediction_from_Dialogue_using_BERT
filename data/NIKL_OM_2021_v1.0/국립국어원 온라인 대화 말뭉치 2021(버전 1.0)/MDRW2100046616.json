      "utterance": [
        {
          "id": "MDRW2100046616.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 08:30"
        },
        {
          "id": "MDRW2100046616.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 09:10"
        },
        {
          "id": "MDRW2100046616.1.3",
          "form": "운동하시는거 좋아하세요?",
          "original_form": "운동하시는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20211104 09:10"
        },
        {
          "id": "MDRW2100046616.1.4",
          "form": "좋아하는건 아니지만 건강을 위해 하루에 1시간정도 운동해요",
          "original_form": "좋아하는건 아니지만 건강을 위해 하루에 1시간정도 운동해요",
          "speaker_id": "1",
          "time": "20211104 09:12"
        },
        {
          "id": "MDRW2100046616.1.5",
          "form": "와 하루에 1시간씩 매일하시는거세요?",
          "original_form": "와 하루에 1시간씩 매일하시는거세요?",
          "speaker_id": "2",
          "time": "20211104 09:13"
        },
        {
          "id": "MDRW2100046616.1.6",
          "form": "엄청 부지런하시네요!! 저는 일주일에 1시간도 겨우 하는데 ㅎㅎ..",
          "original_form": "엄청 부지런하시네요!! 저는 일주일에 1시간도 겨우 하는데 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20211104 09:14"
        },
        {
          "id": "MDRW2100046616.1.7",
          "form": "네 근처 공원에서 러닝이랑 걷기해요",
          "original_form": "네 근처 공원에서 러닝이랑 걷기해요",
          "speaker_id": "1",
          "time": "20211104 09:14"
        },
        {
          "id": "MDRW2100046616.1.8",
          "form": "아 런데이 같은거 하시는건가요?",
          "original_form": "아 런데이 같은거 하시는건가요?",
          "speaker_id": "2",
          "time": "20211104 09:14"
        },
        {
          "id": "MDRW2100046616.1.9",
          "form": "대단하시다 매일매일이라니 ㅠㅠ",
          "original_form": "대단하시다 매일매일이라니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 09:15"
        },
        {
          "id": "MDRW2100046616.1.10",
          "form": "어플같은건 안하고 그냥 해요",
          "original_form": "어플같은건 안하고 그냥 해요",
          "speaker_id": "1",
          "time": "20211104 09:15"
        },
        {
          "id": "MDRW2100046616.1.11",
          "form": "아하 저는",
          "original_form": "아하 저는",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046616.1.12",
          "form": "러닝은 정말 한 번도",
          "original_form": "러닝은 정말 한 번도",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046616.1.13",
          "form": "해본 적이 없는데",
          "original_form": "해본 적이 없는데",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046616.1.14",
          "form": "초보라면.. 몇 분 뛰고",
          "original_form": "초보라면.. 몇 분 뛰고",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046616.1.15",
          "form": "몇 분 걷고",
          "original_form": "몇 분 걷고",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046616.1.16",
          "form": "하는게 좋을까요?",
          "original_form": "하는게 좋을까요?",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046616.1.17",
          "form": "저도 초보에요 길게 해봤자 30분정도에요ㅋ",
          "original_form": "저도 초보에요 길게 해봤자 30분정도에요ㅋ",
          "speaker_id": "1",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046616.1.18",
          "form": "한 번에 30분 뛰시는거에요..?",
          "original_form": "한 번에 30분 뛰시는거에요..?",
          "speaker_id": "2",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046616.1.19",
          "form": "그게 초보라니 저는..",
          "original_form": "그게 초보라니 저는..",
          "speaker_id": "2",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046616.1.20",
          "form": "찌그러져있겠습니닷..",
          "original_form": "찌그러져있겠습니닷..",
          "speaker_id": "2",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046616.1.21",
          "form": "매일 그런건 아니고 15분정도 뛰고 걷고해요 너무 빨리 달리지도 않구요",
          "original_form": "매일 그런건 아니고 15분정도 뛰고 걷고해요 너무 빨리 달리지도 않구요",
          "speaker_id": "1",
          "time": "20211104 09:18"
        },
        {
          "id": "MDRW2100046616.1.22",
          "form": "저도 차근차근 해봐야겠어요",
          "original_form": "저도 차근차근 해봐야겠어요",
          "speaker_id": "2",
          "time": "20211104 09:18"
        },
        {
          "id": "MDRW2100046616.1.23",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "2",
          "time": "20211104 09:18"
        },
        {
          "id": "MDRW2100046616.1.24",
          "form": "러닝하면",
          "original_form": "러닝하면",
          "speaker_id": "2",
          "time": "20211104 09:18"
        },
        {
          "id": "MDRW2100046616.1.25",
          "form": "체력이 달라지는게",
          "original_form": "체력이 달라지는게",
          "speaker_id": "2",
          "time": "20211104 09:18"
        },
        {
          "id": "MDRW2100046616.1.26",
          "form": "느껴지시나요? 궁금궁금",
          "original_form": "느껴지시나요? 궁금궁금",
          "speaker_id": "2",
          "time": "20211104 09:19"
        },
        {
          "id": "MDRW2100046616.1.27",
          "form": "네 근데 유산소만 하는거라..근력운동을 하긴해야하는데",
          "original_form": "네 근데 유산소만 하는거라..근력운동을 하긴해야하는데",
          "speaker_id": "1",
          "time": "20211104 09:19"
        },
        {
          "id": "MDRW2100046616.1.28",
          "form": "하고싶지가 않네요..ㅋㅋ",
          "original_form": "하고싶지가 않네요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 09:19"
        },
        {
          "id": "MDRW2100046616.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하 근력운동이 중요하다곤 하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하 근력운동이 중요하다곤 하는데",
          "speaker_id": "2",
          "time": "20211104 09:20"
        },
        {
          "id": "MDRW2100046616.1.30",
          "form": "하면 욕나오죠 ㅋㅅㅋ",
          "original_form": "하면 욕나오죠 ㅋㅅㅋ",
          "speaker_id": "2",
          "time": "20211104 09:20"
        },
        {
          "id": "MDRW2100046616.1.31",
          "form": "그래도 유산소라도 꾸준히 하시는게",
          "original_form": "그래도 유산소라도 꾸준히 하시는게",
          "speaker_id": "2",
          "time": "20211104 09:20"
        },
        {
          "id": "MDRW2100046616.1.32",
          "form": "아무것도 안하는거보다는... 훨씬 좋죠 ㅎㅎ",
          "original_form": "아무것도 안하는거보다는... 훨씬 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 09:20"
        },
        {
          "id": "MDRW2100046616.1.33",
          "form": "심장 건강에는 유산소가 필수라고 하던데 ㅎ.ㅎ\r\n",
          "original_form": "심장 건강에는 유산소가 필수라고 하던데 ㅎ.ㅎ\r\n",
          "speaker_id": "2",
