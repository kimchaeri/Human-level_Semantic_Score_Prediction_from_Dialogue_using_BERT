      "utterance": [
        {
          "id": "MDRW2100010341.1.1",
          "form": "서점가면 어느코너 들리시나요",
          "original_form": "서점가면 어느코너 들리시나요",
          "speaker_id": "1",
          "time": "20210823 17:01"
        },
        {
          "id": "MDRW2100010341.1.2",
          "form": "소설코너 많이 보는거같아요ㅎㅎ 요즘엔 경제서적도 볼까 생각만 하고..",
          "original_form": "소설코너 많이 보는거같아요ㅎㅎ 요즘엔 경제서적도 볼까 생각만 하고..",
          "speaker_id": "2",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010341.1.3",
          "form": "와 실용적이시네요",
          "original_form": "와 실용적이시네요",
          "speaker_id": "1",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010341.1.4",
          "form": "저는 소설이나 에세이 구경하구 팬시 구경해요ㅎㅎㅎ",
          "original_form": "저는 소설이나 에세이 구경하구 팬시 구경해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010341.1.5",
          "form": "아 팬시 저도 당연히 들리죠ㅎㅎ 이쁜게 너무 많아요",
          "original_form": "아 팬시 저도 당연히 들리죠ㅎㅎ 이쁜게 너무 많아요",
          "speaker_id": "2",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010341.1.6",
          "form": "서점어제갔는데 사람이 별로없더래요",
          "original_form": "서점어제갔는데 사람이 별로없더래요",
          "speaker_id": "1",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010341.1.7",
          "form": "신간도 잘 안들어오구 슬펐어요",
          "original_form": "신간도 잘 안들어오구 슬펐어요",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010341.1.8",
          "form": "요즘 코로나때문에 힘든가봐요",
          "original_form": "요즘 코로나때문에 힘든가봐요",
          "speaker_id": "2",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010341.1.9",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010341.1.10",
          "form": "요즘 또 이북이 잘된다고 하구..",
          "original_form": "요즘 또 이북이 잘된다고 하구..",
          "speaker_id": "2",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010341.1.11",
          "form": "코로나라서 그런가봐요 도서관도 문닫고",
          "original_form": "코로나라서 그런가봐요 도서관도 문닫고",
          "speaker_id": "1",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010341.1.12",
          "form": "이북은 리더기 쓰시는거 있으세요?",
          "original_form": "이북은 리더기 쓰시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010341.1.13",
          "form": "네 저 크레마 쓰는데 잘안봐요ㅎㅎ",
          "original_form": "네 저 크레마 쓰는데 잘안봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010341.1.14",
          "form": "당근마켓에서 구해보려구요",
          "original_form": "당근마켓에서 구해보려구요",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010341.1.15",
          "form": "오 크레마 좋나요?",
          "original_form": "오 크레마 좋나요?",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010341.1.16",
          "form": "아 사실 추천은 안해요ㅠㅠ그냥 책이 나아요ㅋㅋ",
          "original_form": "아 사실 추천은 안해요ㅠㅠ그냥 책이 나아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010341.1.17",
          "form": "ㅜㅜ아 그런가요..",
          "original_form": "ㅜㅜ아 그런가요..",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010341.1.18",
          "form": "제가 이사를 자주다니니까 책이 짐되더라구요",
          "original_form": "제가 이사를 자주다니니까 책이 짐되더라구요",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010341.1.19",
          "form": "네 근데 또 사람마다 다르니까..",
          "original_form": "네 근데 또 사람마다 다르니까..",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010341.1.20",
          "form": "저도 그래서 산건데..후회가ㅋㅋ",
          "original_form": "저도 그래서 산건데..후회가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010341.1.21",
          "form": "저도 종이책이 좋은데",
          "original_form": "저도 종이책이 좋은데",
          "speaker_id": "1",
          "time": "20210823 17:11"
        }
      ]
    }
