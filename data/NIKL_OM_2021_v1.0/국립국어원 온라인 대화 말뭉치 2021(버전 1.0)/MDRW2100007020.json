      "utterance": [
        {
          "id": "MDRW2100007020.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007020.1.2",
          "form": "요즘 코로나라 여행가기어려운데ㅠㅠ 최근에 가신적있으신가용",
          "original_form": "요즘 코로나라 여행가기어려운데ㅠㅠ 최근에 가신적있으신가용",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007020.1.3",
          "form": "최근....저 2년 전이요 ㅋㅋ",
          "original_form": "최근....저 2년 전이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007020.1.4",
          "form": "언제 여행하셨어요?",
          "original_form": "언제 여행하셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007020.1.5",
          "form": "국내여행 포함하면 이번달초에 다녀왔어요ㅎㅎ",
          "original_form": "국내여행 포함하면 이번달초에 다녀왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007020.1.6",
          "form": "오 어디요?",
          "original_form": "오 어디요?",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007020.1.7",
          "form": "인천이요! 송도에서 호캉스하고왔어요ㅎㅎ",
          "original_form": "인천이요! 송도에서 호캉스하고왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007020.1.8",
          "form": "오 편하게 쉬셨겠네요 ㅋ 호캉스 해본 적 없는데 추천할만 하신가요?",
          "original_form": "오 편하게 쉬셨겠네요 ㅋ 호캉스 해본 적 없는데 추천할만 하신가요?",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007020.1.9",
          "form": "네 뷰 좋은곳에서 편하게 쉬니까 좋더라구용",
          "original_form": "네 뷰 좋은곳에서 편하게 쉬니까 좋더라구용",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007020.1.10",
          "form": "아 한번 가봐야겠어요",
          "original_form": "아 한번 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007020.1.11",
          "form": "코로나 길어지니 답답해요",
          "original_form": "코로나 길어지니 답답해요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007020.1.12",
          "form": "맞아요ㅠㅠ 해외 넘 가고싶어요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ 해외 넘 가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007020.1.13",
          "form": "2년전에 가셨던곳은 어디였어요?",
          "original_form": "2년전에 가셨던곳은 어디였어요?",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007020.1.14",
          "form": "파리갔었어요",
          "original_form": "파리갔었어요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007020.1.15",
          "form": "그땐 금방 다른 여행지도 갈 수 있을 줄 알았는데",
          "original_form": "그땐 금방 다른 여행지도 갈 수 있을 줄 알았는데",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007020.1.16",
          "form": "파리 좋죠..! 저도 짧게있어서 넘 아쉬웠지만 그래두 좋았어요",
          "original_form": "파리 좋죠..! 저도 짧게있어서 넘 아쉬웠지만 그래두 좋았어요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007020.1.17",
          "form": "그러게요 코로나전까지는 열심히 다녔었는데ㅠ",
          "original_form": "그러게요 코로나전까지는 열심히 다녔었는데ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007020.1.18",
          "form": "또 가고 싶어요 언제가 될지",
          "original_form": "또 가고 싶어요 언제가 될지",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007020.1.19",
          "form": "내년에는 갈 수 있으면ㅠㅠ",
          "original_form": "내년에는 갈 수 있으면ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        }
      ]
