      "utterance": [
        {
          "id": "MDRW2100003250.1.1",
          "form": "name2님 학교 다니시나요?",
          "original_form": "&name2&님 학교 다니시나요?",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003250.1.2",
          "form": "네 대학다녀요",
          "original_form": "네 대학다녀요",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003250.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003250.1.4",
          "form": "오 저는 졸업하고 일하고 있어요",
          "original_form": "오 저는 졸업하고 일하고 있어요",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003250.1.5",
          "form": "요새 수강신청 하더라구요",
          "original_form": "요새 수강신청 하더라구요",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003250.1.6",
          "form": "네 수강신청 이번주에 했어요",
          "original_form": "네 수강신청 이번주에 했어요",
          "speaker_id": "2",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003250.1.7",
          "form": "우와 장바구니 식으로 되어있어요?",
          "original_form": "우와 장바구니 식으로 되어있어요?",
          "speaker_id": "1",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003250.1.8",
          "form": "저희학굔 아니었어서 하나하나 눌렀었어요",
          "original_form": "저희학굔 아니었어서 하나하나 눌렀었어요",
          "speaker_id": "1",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003250.1.9",
          "form": "저희도 누르는 식이었어요",
          "original_form": "저희도 누르는 식이었어요",
          "speaker_id": "2",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003250.1.10",
          "form": "약간 실패했어요",
          "original_form": "약간 실패했어요",
          "speaker_id": "2",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003250.1.11",
          "form": "저도 매번 원하던 과목 완벽하게 성공한 적은 없는거같아요",
          "original_form": "저도 매번 원하던 과목 완벽하게 성공한 적은 없는거같아요",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003250.1.12",
          "form": "어려웠어요",
          "original_form": "어려웠어요",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003250.1.13",
          "form": "이번에 빠르게 눌렀는데도",
          "original_form": "이번에 빠르게 눌렀는데도",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003250.1.14",
          "form": "원하는 교수님은 다 나갔더라고요",
          "original_form": "원하는 교수님은 다 나갔더라고요",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003250.1.15",
          "form": "인기많은 수업은 진짜 힘들더라구요ㅠㅠ",
          "original_form": "인기많은 수업은 진짜 힘들더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003250.1.16",
          "form": "같은 등록금 내고 다니는데 듣고싶은 과목을 못듣고ㅠㅠ",
          "original_form": "같은 등록금 내고 다니는데 듣고싶은 과목을 못듣고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003250.1.17",
          "form": "그러니까요ㅜㅜ",
          "original_form": "그러니까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003250.1.18",
          "form": "그나마 원하는 시간은 골랐어요",
          "original_form": "그나마 원하는 시간은 골랐어요",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003250.1.19",
          "form": "우와 다행이에요",
          "original_form": "우와 다행이에요",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003250.1.20",
          "form": "공강도 있어요?",
          "original_form": "공강도 있어요?",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003250.1.21",
          "form": "공강 만들기 싫어서 공강 거의 없게 계획했어요",
          "original_form": "공강 만들기 싫어서 공강 거의 없게 계획했어요",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003250.1.22",
          "form": "다행히 시간은 다 성공해서 공강 거의 없어요",
          "original_form": "다행히 시간은 다 성공해서 공강 거의 없어요",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003250.1.23",
          "form": "와 좋네요!",
          "original_form": "와 좋네요!",
          "speaker_id": "1",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003250.1.24",
          "form": "저 예전에 수강신청 실패해서 과제 진짜 많은 과목 걸렸었는데",
          "original_form": "저 예전에 수강신청 실패해서 과제 진짜 많은 과목 걸렸었는데",
          "speaker_id": "1",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003250.1.25",
          "form": "너무 힘들었어요",
          "original_form": "너무 힘들었어요",
          "speaker_id": "1",
          "time": "20210820 23:30"
        }
      ]
    }
