      "utterance": [
        {
          "id": "MDRW2100020617.1.1",
          "form": "안녕하세요 유튜브 보세요?",
          "original_form": "안녕하세요 유튜브 보세요?",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020617.1.2",
          "form": "안녕하세요! 유튜브 많이 보죠ㅎㅎ",
          "original_form": "안녕하세요! 유튜브 많이 보죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020617.1.3",
          "form": "어제도 보다가 잤습니다",
          "original_form": "어제도 보다가 잤습니다",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020617.1.4",
          "form": "name1님은 어떤 영상 많이 보세요??",
          "original_form": "&name1&님은 어떤 영상 많이 보세요??",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020617.1.5",
          "form": "아하ㅋㅋㅋ 저도 프리미엄 가입했어용",
          "original_form": "아하ㅋㅋㅋ 저도 프리미엄 가입했어용",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020617.1.6",
          "form": "저는 동물영상 주로 봐요",
          "original_form": "저는 동물영상 주로 봐요",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020617.1.7",
          "form": "동물농장 애청자입니당",
          "original_form": "동물농장 애청자입니당",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020617.1.8",
          "form": "동물농장! 저도 가끔 보는데 영상편집이 넘 귀엽더라구요ㅎㅎ",
          "original_form": "동물농장! 저도 가끔 보는데 영상편집이 넘 귀엽더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020617.1.9",
          "form": "저는 주로 고양이 영상 많이 봐요!",
          "original_form": "저는 주로 고양이 영상 많이 봐요!",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020617.1.10",
          "form": "고양이 좋아하시나봐용",
          "original_form": "고양이 좋아하시나봐용",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020617.1.11",
          "form": "메탈남 보세요??",
          "original_form": "메탈남 보세요??",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020617.1.12",
          "form": "앗! 메탈남님은 처음 들어봤네요 이번에 봐야겠어요! 저는 주로 하하하님 봐요ㅎㅎ",
          "original_form": "앗! 메탈남님은 처음 들어봤네요 이번에 봐야겠어요! 저는 주로 하하하님 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020617.1.13",
          "form": "하하하님 완전 유명하시더라구요ㅎㅎㅎ",
          "original_form": "하하하님 완전 유명하시더라구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020617.1.14",
          "form": "키우시는 고양이들도 다 너무 이쁘고",
          "original_form": "키우시는 고양이들도 다 너무 이쁘고",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020617.1.15",
          "form": "사랑받은 티가 팍팍 나요",
          "original_form": "사랑받은 티가 팍팍 나요",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020617.1.16",
          "form": "맞아요! 정을 주지 않겠다하시면서 누구보다 정을 많이 주시니..",
          "original_form": "맞아요! 정을 주지 않겠다하시면서 누구보다 정을 많이 주시니..",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020617.1.17",
          "form": "넘 귀엽습니다..",
          "original_form": "넘 귀엽습니다..",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020617.1.18",
          "form": "저는 강아지 채널",
          "original_form": "저는 강아지 채널",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020617.1.19",
          "form": "오복이도 보는데",
          "original_form": "오복이도 보는데",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020617.1.20",
          "form": "말티즈거든용",
          "original_form": "말티즈거든용",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020617.1.21",
          "form": "대박 귀여워요",
          "original_form": "대박 귀여워요",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020617.1.22",
          "form": "아아ㅠㅠㅠㅠ 안봤는데 벌써 ㄱ여워요...",
          "original_form": "아아ㅠㅠㅠㅠ 안봤는데 벌써 ㄱ여워요...",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020617.1.23",
          "form": "말티즈 새하얀아이..",
          "original_form": "말티즈 새하얀아이..",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020617.1.24",
          "form": "어쩜 그리 귀엽게 태어날 수 있는지..",
          "original_form": "어쩜 그리 귀엽게 태어날 수 있는지..",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020617.1.25",
          "form": "성깔은 있는데 그래도 귀엽더라구요ㅋㅋㅋㅋ",
          "original_form": "성깔은 있는데 그래도 귀엽더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020617.1.26",
          "form": "막 화내요ㅋㅋㅋㅋ",
          "original_form": "막 화내요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020617.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ말티즈가 한 성깔있죸ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ말티즈가 한 성깔있죸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020617.1.28",
          "form": "하지만 화내는것도 귀여워요...",
          "original_form": "하지만 화내는것도 귀여워요...",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020617.1.29",
          "form": "맞아요 자기 귀여운거 알고 화내는것같아요ㅋㅋㅋ",
          "original_form": "맞아요 자기 귀여운거 알고 화내는것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:23"
        }
      ]
