      "utterance": [
        {
          "id": "MDRW2100019031.1.1",
          "form": "오 맛있었겠네",
          "original_form": "오 맛있었겠네",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019031.1.2",
          "form": "근데 전에 산거랑 맛이 완전 달랐어",
          "original_form": "근데 전에 산거랑 맛이 완전 달랐어",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019031.1.3",
          "form": "엥 왜?",
          "original_form": "엥 왜?",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019031.1.4",
          "form": "전에 산 건 많이 익어서 왔던데 얘는 날김치더라고",
          "original_form": "전에 산 건 많이 익어서 왔던데 얘는 날김치더라고",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019031.1.5",
          "form": "그게 살 때마다 익힘 정도가 다른가?",
          "original_form": "그게 살 때마다 익힘 정도가 다른가?",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019031.1.6",
          "form": "몰라 저번 건 푹 익어서 와서 원래 이런건가 했는데",
          "original_form": "몰라 저번 건 푹 익어서 와서 원래 이런건가 했는데",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019031.1.7",
          "form": "그래 맞아~ 새콤했잖아",
          "original_form": "그래 맞아~ 새콤했잖아",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019031.1.8",
          "form": "이번에 온건 하나도 안익어서 배추 짠맛이 아직도 나",
          "original_form": "이번에 온건 하나도 안익어서 배추 짠맛이 아직도 나",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019031.1.9",
          "form": "익혀서 먹어야지 어쩔 수 있나 ㅎ",
          "original_form": "익혀서 먹어야지 어쩔 수 있나 ㅎ",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019031.1.10",
          "form": "한참 익혀야 할 것 같아",
          "original_form": "한참 익혀야 할 것 같아",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019031.1.11",
          "form": "어떻게 그렇게 차이나게 배송이 올 수 있지?",
          "original_form": "어떻게 그렇게 차이나게 배송이 올 수 있지?",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019031.1.12",
          "form": "모르겠어.. 보내기전에 더워서 익었던건가?",
          "original_form": "모르겠어.. 보내기전에 더워서 익었던건가?",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019031.1.13",
          "form": "그럴지도 요즘은 시원하잖아",
          "original_form": "그럴지도 요즘은 시원하잖아",
          "speaker_id": "2",
          "time": "20210827 14:47"
        }
