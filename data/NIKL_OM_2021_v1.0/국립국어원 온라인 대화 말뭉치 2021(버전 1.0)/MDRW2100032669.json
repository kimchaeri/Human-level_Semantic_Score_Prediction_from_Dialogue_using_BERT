      "utterance": [
        {
          "id": "MDRW2100032669.1.1",
          "form": "학교 다니셨나요",
          "original_form": "학교 다니셨나요",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032669.1.2",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032669.1.3",
          "form": "대졸자입니다",
          "original_form": "대졸자입니다",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032669.1.4",
          "form": "오오 고학력이시네요",
          "original_form": "오오 고학력이시네요",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032669.1.5",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032669.1.6",
          "form": "대학 다녀 보셨나요?",
          "original_form": "대학 다녀 보셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032669.1.7",
          "form": "요새 대학 안 간 사람 찾기가 어렵죠",
          "original_form": "요새 대학 안 간 사람 찾기가 어렵죠",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032669.1.8",
          "form": "네 다녔습니다",
          "original_form": "네 다녔습니다",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032669.1.9",
          "form": "그럼 저한테 고학력자라고 한 건 빈말이셨나요?",
          "original_form": "그럼 저한테 고학력자라고 한 건 빈말이셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032669.1.10",
          "form": "졸업하셨나요?",
          "original_form": "졸업하셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032669.1.11",
          "form": "칼답 부탁요~",
          "original_form": "칼답 부탁요~",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032669.1.12",
          "form": "네 빈말이에요",
          "original_form": "네 빈말이에요",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032669.1.13",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032669.1.14",
          "form": "졸업했죠",
          "original_form": "졸업했죠",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032669.1.15",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032669.1.16",
          "form": "시험 언제 마지막으로 보셨나요",
          "original_form": "시험 언제 마지막으로 보셨나요",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.17",
          "form": "학업 스트레스 있으셨나요",
          "original_form": "학업 스트레스 있으셨나요",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.18",
          "form": "저는 한 10년 됐죠",
          "original_form": "저는 한 10년 됐죠",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.19",
          "form": "네 많았죠 시험 스트레스로",
          "original_form": "네 많았죠 시험 스트레스로",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.20",
          "form": "와 나이가 꽤 있으신가보네요",
          "original_form": "와 나이가 꽤 있으신가보네요",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.22",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.23",
          "form": "어르신이시네요",
          "original_form": "어르신이시네요",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.24",
          "form": "전 좀 어려요 아직",
          "original_form": "전 좀 어려요 아직",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032669.1.25",
