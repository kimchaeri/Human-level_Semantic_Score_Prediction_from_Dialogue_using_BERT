      "utterance": [
        {
          "id": "MDRW2100023940.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023940.1.2",
          "form": "최근에 잘샀다고 생각하는 물건 있어요?",
          "original_form": "최근에 잘샀다고 생각하는 물건 있어요?",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023940.1.3",
          "form": "음... 복숭아를 샀는데요",
          "original_form": "음... 복숭아를 샀는데요",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023940.1.4",
          "form": "향이 정말 기가막혔어요",
          "original_form": "향이 정말 기가막혔어요",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023940.1.5",
          "form": "맛은 기대보단 살짝 덜 달았지만요",
          "original_form": "맛은 기대보단 살짝 덜 달았지만요",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023940.1.6",
          "form": "어머 요새 복숭아가 제철이에요?",
          "original_form": "어머 요새 복숭아가 제철이에요?",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023940.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023940.1.8",
          "form": "저도 잘 몰랐는데 복숭아는 종류가 다양하고",
          "original_form": "저도 잘 몰랐는데 복숭아는 종류가 다양하고",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023940.1.9",
          "form": "그안에서 초여름부터 늦여름까지 수확시기가 나눠진데요",
          "original_form": "그안에서 초여름부터 늦여름까지 수확시기가 나눠진데요",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023940.1.10",
          "form": "오 ㅋㅋ 그렇구나 그럼 지금 사신거는",
          "original_form": "오 ㅋㅋ 그렇구나 그럼 지금 사신거는",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023940.1.11",
          "form": "말랑해요 아님 딱딱해요?",
          "original_form": "말랑해요 아님 딱딱해요?",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023940.1.12",
          "form": "그 중간?",
          "original_form": "그 중간?",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100023940.1.13",
          "form": "신품종이랬는데 제가 느끼기엔 약간 말랭이같았어요",
          "original_form": "신품종이랬는데 제가 느끼기엔 약간 말랭이같았어요",
          "speaker_id": "2",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023940.1.14",
          "form": "오 저 그런 식감 진짜 좋아해요",
          "original_form": "오 저 그런 식감 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023940.1.15",
          "form": "무르지도 않고 딱딱하지도 않고 적당하게 감싸는 느낌",
          "original_form": "무르지도 않고 딱딱하지도 않고 적당하게 감싸는 느낌",
          "speaker_id": "1",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023940.1.16",
          "form": "완전 잘 사셨네요.. 대박",
          "original_form": "완전 잘 사셨네요.. 대박",
          "speaker_id": "1",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023940.1.17",
          "form": "네, 그래서 좋았어요",
          "original_form": "네, 그래서 좋았어요",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023940.1.18",
          "form": "깎을때 힘들지도 않고 먹을때 딱딱하지도 않고",
          "original_form": "깎을때 힘들지도 않고 먹을때 딱딱하지도 않고",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023940.1.19",
          "form": "한 박스 사신거에요?",
          "original_form": "한 박스 사신거에요?",
          "speaker_id": "1",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023940.1.20",
          "form": "2.5키로 한박스요",
          "original_form": "2.5키로 한박스요",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023940.1.21",
          "form": "금방 먹었어요",
          "original_form": "금방 먹었어요",
          "speaker_id": "2",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100023940.1.22",
          "form": "허걱~ 한 박스면 몇개 정도 들어있는거에요?",
          "original_form": "허걱~ 한 박스면 몇개 정도 들어있는거에요?",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100023940.1.23",
          "form": "7개요",
          "original_form": "7개요",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100023940.1.24",
          "form": "꽤 큰 복숭아였어요",
          "original_form": "꽤 큰 복숭아였어요",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100023940.1.25",
          "form": "와.. 완전 잘익은 복숭아인가보다",
          "original_form": "와.. 완전 잘익은 복숭아인가보다",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023940.1.26",
          "form": "얼마정도 해요?",
          "original_form": "얼마정도 해요?",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023940.1.27",
          "form": "2만8천원정도",
          "original_form": "2만8천원정도",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023940.1.28",
          "form": "복숭아치곤 살짝 비싸지만",
          "original_form": "복숭아치곤 살짝 비싸지만",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023940.1.29",
          "form": "신품종이라니까요",
          "original_form": "신품종이라니까요",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023940.1.30",
          "form": "지금 쇼핑몰에 찾아보고 있는데 안보이네요 ㅜㅜ",
          "original_form": "지금 쇼핑몰에 찾아보고 있는데 안보이네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023940.1.31",
          "form": "설명 듣고 너무 먹고싶어졌어요",
          "original_form": "설명 듣고 너무 먹고싶어졌어요",
          "speaker_id": "1",
          "time": "20210830 20:50"
        }
      ]
