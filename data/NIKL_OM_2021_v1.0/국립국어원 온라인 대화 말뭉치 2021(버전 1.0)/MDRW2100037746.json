        {
          "id": "MDRW2100037746.1.1",
          "form": "네 가본적있어요ㅋ",
          "original_form": "네 가본적있어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100037746.1.2",
          "form": "코로나전에요",
