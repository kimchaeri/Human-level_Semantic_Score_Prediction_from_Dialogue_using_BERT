      "utterance": [
        {
          "id": "MDRW2100000604.1.1",
          "form": "요즘 로스트 아크가 그렇게 인기래",
          "original_form": "요즘 로스트 아크가 그렇게 인기래",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000604.1.2",
          "form": "포켓몬고 이벤트중이다",
          "original_form": "포켓몬고 이벤트중이다",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000604.1.3",
          "form": "처음 신규가입하면 1300레벨부터 점핑권도 주고",
          "original_form": "처음 신규가입하면 1300레벨부터 점핑권도 주고",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000604.1.4",
          "form": "로스트아크는 손대면 답없다",
          "original_form": "로스트아크는 손대면 답없다",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000604.1.5",
          "form": "로아도 리니지 같은거겠지",
          "original_form": "로아도 리니지 같은거겠지",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000604.1.6",
          "form": "예전 리니지 폐인들이 많았지 ㅋㅋ",
          "original_form": "예전 리니지 폐인들이 많았지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.7",
          "form": "요즘도 리니지 하는 사람들은 엄청 현질하더라",
          "original_form": "요즘도 리니지 하는 사람들은 엄청 현질하더라",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.8",
          "form": "소서리스 새로 나왔다던데~",
          "original_form": "소서리스 새로 나왔다던데~",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.9",
          "form": "로아도 그렇겠지만",
          "original_form": "로아도 그렇겠지만",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.10",
          "form": "주변에 로아 하는 사람 있나?",
          "original_form": "주변에 로아 하는 사람 있나?",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.11",
          "form": "주변에 좀 있어서 영업당하는 중이야",
          "original_form": "주변에 좀 있어서 영업당하는 중이야",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.12",
          "form": "키워주는 쩔러 있어야 함",
          "original_form": "키워주는 쩔러 있어야 함",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.13",
          "form": "포고나 해야지뭐",
          "original_form": "포고나 해야지뭐",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.14",
          "form": "쩔러가 괜찮으면 한 번 도전해봐도 됨",
          "original_form": "쩔러가 괜찮으면 한 번 도전해봐도 됨",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.15",
          "form": "내 캐릭터 1500레벨까지 키워준대",
          "original_form": "내 캐릭터 1500레벨까지 키워준대",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000604.1.16",
          "form": "포고는 일상이지 ㅋ",
          "original_form": "포고는 일상이지 ㅋ",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000604.1.17",
          "form": "근데 난 길을 잘 못찾겠어",
          "original_form": "근데 난 길을 잘 못찾겠어",
          "speaker_id": "1",
