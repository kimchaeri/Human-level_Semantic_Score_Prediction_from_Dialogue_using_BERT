      "utterance": [
        {
          "id": "MDRW2100025361.1.1",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "2",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100025361.1.2",
          "form": "하이루",
          "original_form": "하이루",
          "speaker_id": "1",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025361.1.3",
          "form": "커뮤니티 활동하세요?",
          "original_form": "커뮤니티 활동하세요?",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100025361.1.4",
          "form": "네이버랑 다음해요",
          "original_form": "네이버랑 다음해요",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100025361.1.5",
          "form": "어떤 종류하시나요",
          "original_form": "어떤 종류하시나요",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100025361.1.6",
          "form": "온라인 활동만요",
          "original_form": "온라인 활동만요",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100025361.1.7",
          "form": "쌍거풀코수술요ㅋ",
          "original_form": "쌍거풀코수술요ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100025361.1.8",
          "form": "성형수술 관련으로 보시는군요",
          "original_form": "성형수술 관련으로 보시는군요",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100025361.1.9",
          "form": "저는 중고물품이랑 애완동물 관련 위주로 봐요",
          "original_form": "저는 중고물품이랑 애완동물 관련 위주로 봐요",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100025361.1.10",
          "form": "뭐 이것저것 정보 커뮤니티에요",
          "original_form": "뭐 이것저것 정보 커뮤니티에요",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025361.1.11",
          "form": "시사관련도 보시나요?",
          "original_form": "시사관련도 보시나요?",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025361.1.12",
          "form": "name2은예?",
          "original_form": "&name2&은예?",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025361.1.13",
          "form": "주식카페도 봐요",
          "original_form": "주식카페도 봐요",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025361.1.14",
          "form": "거기 시사랑 정치. 경제 있어요",
          "original_form": "거기 시사랑 정치. 경제 있어요",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025361.1.15",
          "form": "저도 인기글 보고 시사 도 봐요",
          "original_form": "저도 인기글 보고 시사 도 봐요",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025361.1.16",
          "form": "그럼 정모도 하시나요?",
          "original_form": "그럼 정모도 하시나요?",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100025361.1.17",
          "form": "인기글은 지나칠수 없지요",
          "original_form": "인기글은 지나칠수 없지요",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100025361.1.18",
          "form": "맞아요 넘 잼나죠",
          "original_form": "맞아요 넘 잼나죠",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100025361.1.19",
          "form": "아뇨. 코로나때문에 정모안해요",
          "original_form": "아뇨. 코로나때문에 정모안해요",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100025361.1.20",
          "form": "꿀팁도 많이 얻구요",
          "original_form": "꿀팁도 많이 얻구요",
          "speaker_id": "2",
          "time": "20210902 00:20"
        }
      ]
