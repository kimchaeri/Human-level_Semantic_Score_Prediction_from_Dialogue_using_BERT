      "utterance": [
        {
          "id": "MDRW2100029217.1.1",
          "form": "빠져나옴",
          "original_form": "빠져나옴",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.2",
          "form": "신천지는 완전 체계적이던데요",
          "original_form": "신천지는 완전 체계적이던데요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.3",
          "form": "신천지 믿는다는애",
          "original_form": "신천지 믿는다는애",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.4",
          "form": "얘기를 좀더하자면",
          "original_form": "얘기를 좀더하자면",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.5",
          "form": "아유 다행이다",
          "original_form": "아유 다행이다",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.6",
          "form": "ㅇㅇ완전악질임",
          "original_form": "ㅇㅇ완전악질임",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.7",
          "form": "친구가",
          "original_form": "친구가",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.8",
          "form": "회사를 다니는 와중에",
          "original_form": "회사를 다니는 와중에",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.9",
          "form": "아직도 연락해요",
          "original_form": "아직도 연락해요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.10",
          "form": "본인의 취미 여가 활동으라",
          "original_form": "본인의 취미 여가 활동으라",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.11",
          "form": "ㅇㅇ한번씩 연락해",
          "original_form": "ㅇㅇ한번씩 연락해",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.12",
          "form": "성악을 하고 싶어했음",
          "original_form": "성악을 하고 싶어했음",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.13",
          "form": "아우 자주는 연락하지 마세요",
          "original_form": "아우 자주는 연락하지 마세요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029217.1.14",
          "form": "그래서 외국에도 한번씩 나가서 행사하는",
          "original_form": "그래서 외국에도 한번씩 나가서 행사하는",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.15",
          "form": "성악 멋있죠",
          "original_form": "성악 멋있죠",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.16",
          "form": "단체모임을 찾았음",
          "original_form": "단체모임을 찾았음",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.17",
          "form": "그런데 거기가",
          "original_form": "그런데 거기가",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.18",
          "form": "그게 신천진 가요",
          "original_form": "그게 신천진 가요",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.19",
          "form": "신천지소속이었단듯",
          "original_form": "신천지소속이었단듯",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.20",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.21",
          "form": "야 똑똑하다",
          "original_form": "야 똑똑하다",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.22",
          "form": "몇년을 거기서 활동을하는데",
          "original_form": "몇년을 거기서 활동을하는데",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.23",
          "form": "신천지  벌 받아야 되는데",
          "original_form": "신천지  벌 받아야 되는데",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029217.1.24",
          "form": "신천지인들로 다채워지진 않았나봐",
          "original_form": "신천지인들로 다채워지진 않았나봐",
          "speaker_id": "2",
          "time": "20210903 00:24"
        }
