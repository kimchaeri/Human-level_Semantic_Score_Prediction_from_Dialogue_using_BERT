      "utterance": [
        {
          "id": "MDRW2100011654.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011654.1.2",
          "form": "항상 가고싶긴 하져",
          "original_form": "항상 가고싶긴 하져",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011654.1.3",
          "form": "오 어디어디 가보셨나요",
          "original_form": "오 어디어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011654.1.4",
          "form": "전 통영이랑 여수요.",
          "original_form": "전 통영이랑 여수요.",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011654.1.5",
          "form": "오 저도 통영 완전 좋았어요",
          "original_form": "오 저도 통영 완전 좋았어요",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011654.1.6",
          "form": "통영가면 꿀빵은 꼭 사오게 되요",
          "original_form": "통영가면 꿀빵은 꼭 사오게 되요",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100011654.1.7",
          "form": "오오꿀빵 저는 스쿠터로 다녀서 정말좋았어요",
          "original_form": "오오꿀빵 저는 스쿠터로 다녀서 정말좋았어요",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011654.1.8",
          "form": "와 재밌을거 같아요. 바람도 시원하구요",
          "original_form": "와 재밌을거 같아요. 바람도 시원하구요",
          "speaker_id": "2",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011654.1.9",
          "form": "네네맞아요 통영 경치도 정말 좋았는데",
          "original_form": "네네맞아요 통영 경치도 정말 좋았는데",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011654.1.10",
          "form": "또 가보고 싶네요ㅠ",
          "original_form": "또 가보고 싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011654.1.11",
          "form": "코로나가 끝나야 맘놓고 갈텐데ㅠ",
          "original_form": "코로나가 끝나야 맘놓고 갈텐데ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011654.1.12",
          "form": "그러니깐요. 여행을 언제쯤 맘껏 가 볼수 있을지...",
          "original_form": "그러니깐요. 여행을 언제쯤 맘껏 가 볼수 있을지...",
