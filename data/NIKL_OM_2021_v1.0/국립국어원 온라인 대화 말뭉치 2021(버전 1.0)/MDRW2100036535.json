      "utterance": [
        {
          "id": "MDRW2100036535.1.1",
          "form": "아 떨려??",
          "original_form": "아 떨려??",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036535.1.2",
          "form": "기대돼?",
          "original_form": "기대돼?",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036535.1.3",
          "form": "학교생활 재밌을 거 같아",
          "original_form": "학교생활 재밌을 거 같아",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036535.1.4",
          "form": "재미있는 수업이 걸렸으면 좋겠내",
          "original_form": "재미있는 수업이 걸렸으면 좋겠내",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036535.1.5",
          "form": "교수님은 어때? 니가 좋아하는 교수님이야?",
          "original_form": "교수님은 어때? 니가 좋아하는 교수님이야?",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036535.1.6",
          "form": "나는 개강을 기대한 적은 없지만 친구를 만나는 게 좋았어. 그게 기대한 거라고 할 수 있나?",
          "original_form": "나는 개강을 기대한 적은 없지만 친구를 만나는 게 좋았어. 그게 기대한 거라고 할 수 있나?",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036535.1.7",
          "form": "음 그렇구나",
          "original_form": "음 그렇구나",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036535.1.8",
          "form": "나는 친구가 없어서 개강이 별로여",
          "original_form": "나는 친구가 없어서 개강이 별로여",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036535.1.9",
          "form": "근데 너 자니?",
          "original_form": "근데 너 자니?",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036535.1.10",
          "form": "교수님은 좋은 분들 같아",
          "original_form": "교수님은 좋은 분들 같아",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.11",
          "form": "흠 내가 너무 말을 많이 한 걸까?",
          "original_form": "흠 내가 너무 말을 많이 한 걸까?",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.12",
          "form": "하지만 과제를 내주시면 싫어져",
          "original_form": "하지만 과제를 내주시면 싫어져",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.13",
          "form": "다행이다! 도망간 줄 알았어",
          "original_form": "다행이다! 도망간 줄 알았어",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.14",
          "form": "맞아 과제때문에 드랒하고싶어져",
          "original_form": "맞아 과제때문에 드랒하고싶어져",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.15",
          "form": "너는 제일 좋은 과목이 뭐였어?",
          "original_form": "너는 제일 좋은 과목이 뭐였어?",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.16",
          "form": "교수님들이 좀 자제했으면 좋겠어",
          "original_form": "교수님들이 좀 자제했으면 좋겠어",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.17",
          "form": "잘 기억이 안나는데",
          "original_form": "잘 기억이 안나는데",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.18",
          "form": "교양같은거 추천하는거 있니?",
          "original_form": "교양같은거 추천하는거 있니?",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.19",
          "form": "심리평가 이런거였어",
          "original_form": "심리평가 이런거였어",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.20",
          "form": "흠\r\n재밌겟다",
          "original_form": "흠\r\n재밌겟다",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036535.1.21",
          "form": "교양은 잘 모르겠는걸",
          "original_form": "교양은 잘 모르겠는걸",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036535.1.22",
          "form": "그럼 짝지어서 서로 평가해주는거야?",
          "original_form": "그럼 짝지어서 서로 평가해주는거야?",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036535.1.23",
          "form": "대학생을 위한 실용금융 이런건 어때?",
          "original_form": "대학생을 위한 실용금융 이런건 어때?",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036535.1.24",
          "form": "너무 어려웠어 교양주제에",
          "original_form": "너무 어려웠어 교양주제에",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036535.1.25",
          "form": "응 심리분섣을 했어",
          "original_form": "응 심리분섣을 했어",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036535.1.26",
          "form": "와 그건 정말 도움될 것 같아",
          "original_form": "와 그건 정말 도움될 것 같아",
          "speaker_id": "1",
          "time": "20210831 00:17"
        }
      ]
    }
