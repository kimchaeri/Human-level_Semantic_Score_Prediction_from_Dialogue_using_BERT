      "utterance": [
        {
          "id": "MDRW2100025107.1.1",
          "form": "아직고 야식중이새요?",
          "original_form": "아직고 야식중이새요?",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025107.1.2",
          "form": "이제 먹고 치울준비요 설겆이 분리수거 등",
          "original_form": "이제 먹고 치울준비요 설겆이 분리수거 등",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025107.1.3",
          "form": "ㅎㅎ  엄지척이요",
          "original_form": "ㅎㅎ  엄지척이요",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025107.1.4",
          "form": "집에던킨 빵이 있어서",
          "original_form": "집에던킨 빵이 있어서",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025107.1.5",
          "form": "그거먹는중ㅋ",
          "original_form": "그거먹는중ㅋ",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025107.1.6",
          "form": "오융 근네 이시간에 먹늠 살안쪄요?",
          "original_form": "오융 근네 이시간에 먹늠 살안쪄요?",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025107.1.7",
          "form": "찌죠ㅋ",
          "original_form": "찌죠ㅋ",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025107.1.8",
          "form": "먹은거 정리좀",
          "original_form": "먹은거 정리좀",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100025107.1.9",
          "form": "네넹 어여 하세요",
          "original_form": "네넹 어여 하세요",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100025107.1.10",
          "form": "열심히 치우고 오는점심은 매밀",
          "original_form": "열심히 치우고 오는점심은 매밀",
          "speaker_id": "2",
          "time": "20210901 14:30"
        },
        {
          "id": "MDRW2100025107.1.11",
          "form": "저는 불고기!! 먹엇어여",
          "original_form": "저는 불고기!! 먹엇어여",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025107.1.12",
          "form": "오 불고기",
          "original_form": "오 불고기",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100025107.1.13",
          "form": "오늘 야식 가시나여?",
          "original_form": "오늘 야식 가시나여?",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100025107.1.14",
          "form": "생각중인데 일단 치워야해서ㅋ",
          "original_form": "생각중인데 일단 치워야해서ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100025107.1.15",
          "form": "치우고 고민 ㄱㄱ요",
          "original_form": "치우고 고민 ㄱㄱ요",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100025107.1.16",
          "form": "남은 빵과 맥주",
          "original_form": "남은 빵과 맥주",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100025107.1.17",
          "form": "ㅎㅎㅎ 냉파네요",
          "original_form": "ㅎㅎㅎ 냉파네요",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100025107.1.18",
          "form": "냉파???",
          "original_form": "냉파???",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100025107.1.19",
          "form": "냉징고 파먹기요 ㅎㅎ",
          "original_form": "냉징고 파먹기요 ㅎㅎ",
