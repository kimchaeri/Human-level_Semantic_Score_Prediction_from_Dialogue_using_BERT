      "utterance": [
        {
          "id": "MDRW2100037950.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100037950.1.2",
          "form": "거의 안봐요ㅠㅠ 링링은요?",
          "original_form": "거의 안봐요ㅠㅠ 링링은요?",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037950.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037950.1.4",
          "form": "저도요ㅋㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037950.1.5",
          "form": "전 그냥 티비 보면서 배달음식 시켜 먹는 거ㅋㅋㅋㅋ",
          "original_form": "전 그냥 티비 보면서 배달음식 시켜 먹는 거ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037950.1.6",
          "form": "그럼 전시회나 콘서트 가보신 적 있으신가요?",
          "original_form": "그럼 전시회나 콘서트 가보신 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037950.1.7",
          "form": "저  예전에 리쌍 콘서트 가봤어요",
          "original_form": "저  예전에 리쌍 콘서트 가봤어요",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037950.1.8",
          "form": "윤도현이랑",
          "original_form": "윤도현이랑",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037950.1.9",
          "form": "신은 나는데 어떻게 풀 줄을 모르겠더라구요ㅋㅋㅋ",
          "original_form": "신은 나는데 어떻게 풀 줄을 모르겠더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037950.1.10",
          "form": "우와 재밌었을 것 같아요\r\n티켓팅해서 가신건가요?",
          "original_form": "우와 재밌었을 것 같아요\r\n티켓팅해서 가신건가요?",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037950.1.11",
          "form": "그때 아는 분이 티켓을 줬어요",
          "original_form": "그때 아는 분이 티켓을 줬어요",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037950.1.12",
          "form": "그때 저한테는 너무 큰 돈이었거든요ㅋㅋ",
          "original_form": "그때 저한테는 너무 큰 돈이었거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037950.1.13",
          "form": "콘서트 관계자분이셨나요?",
          "original_form": "콘서트 관계자분이셨나요?",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037950.1.14",
          "form": "아뇨 그분도 뭐 어떻게 받았다고 들었어요",
          "original_form": "아뇨 그분도 뭐 어떻게 받았다고 들었어요",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037950.1.15",
          "form": "근데 그런 거에 관심 없어서  주셨어요",
          "original_form": "근데 그런 거에 관심 없어서  주셨어요",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037950.1.16",
          "form": "좋아하는 공연 있으세요?",
          "original_form": "좋아하는 공연 있으세요?",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037950.1.17",
          "form": "저는 아이유요",
          "original_form": "저는 아이유요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037950.1.18",
          "form": "아이유 너무 예쁘고 노래도 너무 잘하죠~",
          "original_form": "아이유 너무 예쁘고 노래도 너무 잘하죠~",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037950.1.19",
          "form": "저는 4번?정도 같은데 아는 노래가 많아서 좋았어요",
          "original_form": "저는 4번?정도 같은데 아는 노래가 많아서 좋았어요",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037950.1.20",
          "form": "저도 아이유는 한번 가보고 싶네요",
          "original_form": "저도 아이유는 한번 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100037950.1.21",
          "form": "리쌍 과 아이유",
          "original_form": "리쌍 과 아이유",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100037950.1.22",
