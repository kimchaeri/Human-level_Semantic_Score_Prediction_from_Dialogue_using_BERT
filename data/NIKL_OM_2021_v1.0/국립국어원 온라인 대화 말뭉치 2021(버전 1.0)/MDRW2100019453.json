      "utterance": [
        {
          "id": "MDRW2100019453.1.1",
          "form": "저녁 뭐 드셨나요?",
          "original_form": "저녁 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.2",
          "form": "왜 맛있는곳이 다 망한걸까요",
          "original_form": "왜 맛있는곳이 다 망한걸까요",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.3",
          "form": "코로나라그런가..",
          "original_form": "코로나라그런가..",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.4",
          "form": "저는 굴 돌솥비빔밥 먹었어요",
          "original_form": "저는 굴 돌솥비빔밥 먹었어요",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.5",
          "form": "진짜 폐업 많이 하더라고요",
          "original_form": "진짜 폐업 많이 하더라고요",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.6",
          "form": "저 새송이버섯이랑 동태랑 감자를 계란물에 부쳐서 먹었어요",
          "original_form": "저 새송이버섯이랑 동태랑 감자를 계란물에 부쳐서 먹었어요",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.7",
          "form": "메뉴가 매번 범상치않으시군요",
          "original_form": "메뉴가 매번 범상치않으시군요",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.8",
          "form": "하노이 지도 다시 보니까 임시휴업 엄청 많아요",
          "original_form": "하노이 지도 다시 보니까 임시휴업 엄청 많아요",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.9",
          "form": "헐 ㅜㅜㅜ",
          "original_form": "헐 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.10",
          "form": "유명한 커피집 있요 딘카페",
          "original_form": "유명한 커피집 있요 딘카페",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019453.1.11",
          "form": "관광객 많이 줄어서 더 그럴거같아요",
          "original_form": "관광객 많이 줄어서 더 그럴거같아요",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019453.1.12",
          "form": "거기도 휴업이고 포10인가 엄청 유명한 집도 휴업이더라고요",
          "original_form": "거기도 휴업이고 포10인가 엄청 유명한 집도 휴업이더라고요",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019453.1.13",
          "form": "버섯 동태 감자면 아주 삼위일체네요",
          "original_form": "버섯 동태 감자면 아주 삼위일체네요",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019453.1.14",
          "form": "영양소도 완벽하고",
          "original_form": "영양소도 완벽하고",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019453.1.15",
          "form": "일산에 굴토리라고 있는데 굴국밥이랑 굴 돌솥비빔밥이 아주 맛있어요. 예전만 못하지만 그래도 맛있어서 종종 포장해다 먹어요.",
          "original_form": "일산에 굴토리라고 있는데 굴국밥이랑 굴 돌솥비빔밥이 아주 맛있어요. 예전만 못하지만 그래도 맛있어서 종종 포장해다 먹어요.",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019453.1.16",
          "form": "삼위일쳌ㅋㅋㅋㅋㅋ",
          "original_form": "삼위일쳌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019453.1.17",
          "form": "아 일산 마스터시네요 또 지도에 찍겟습니다",
          "original_form": "아 일산 마스터시네요 또 지도에 찍겟습니다",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019453.1.18",
          "form": "감자 갈아서 전 해야 맛있는 줄 알앗는데",
          "original_form": "감자 갈아서 전 해야 맛있는 줄 알앗는데",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019453.1.19",
          "form": "그냥 썰어서 부쳐먹어도 맛있더라구요",
          "original_form": "그냥 썰어서 부쳐먹어도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019453.1.20",
          "form": "오 진짜요? 처음 들어보는데 저도 해봐야겠어요",
          "original_form": "오 진짜요? 처음 들어보는데 저도 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019453.1.21",
          "form": "감자 가는 격자무늬 칼판 있는데 치즈 강판으로 더 많이 쓰고 있거든요",
          "original_form": "감자 가는 격자무늬 칼판 있는데 치즈 강판으로 더 많이 쓰고 있거든요",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019453.1.22",
          "form": "근데 감자전 갈아서 하면 그것도 맛나죠",
          "original_form": "근데 감자전 갈아서 하면 그것도 맛나죠",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019453.1.23",
          "form": "감자 갈기 귀찮아서 걍 부쳐먹었는데 먹을만했다는 ㅎㅎ",
          "original_form": "감자 갈기 귀찮아서 걍 부쳐먹었는데 먹을만했다는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019453.1.24",
          "form": "그냥 달걀물 직행인가요 부침가루 후 첨벙인가요?",
          "original_form": "그냥 달걀물 직행인가요 부침가루 후 첨벙인가요?",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019453.1.25",
          "form": "치즈 강판 있으면 편할거같아요 예전에 치즈 칼로 다지다가 ㅋㅋㅋㅋ 엄청 현타왔어요",
          "original_form": "치즈 강판 있으면 편할거같아요 예전에 치즈 칼로 다지다가 ㅋㅋㅋㅋ 엄청 현타왔어요",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019453.1.26",
          "form": "저는 부침가루 묻혀서 첨벙했어요",
          "original_form": "저는 부침가루 묻혀서 첨벙했어요",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019453.1.27",
          "form": "알겠습니다 ㅋㅋ",
          "original_form": "알겠습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019453.1.28",
          "form": "치즈를 칼로 다지다니 ㅜ",
          "original_form": "치즈를 칼로 다지다니 ㅜ",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019453.1.29",
          "form": "ㅋㅋㅋㅋ 생쇼에요",
          "original_form": "ㅋㅋㅋㅋ 생쇼에요",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019453.1.30",
          "form": "저는 그 페코리노 로마노인데",
          "original_form": "저는 그 페코리노 로마노인데",
          "speaker_id": "2",
          "time": "20210827 19:34"
        }
      ]
