      "utterance": [
        {
          "id": "MDRW2100035022.1.1",
          "form": "그 사건 이후",
          "original_form": "그 사건 이후",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100035022.1.2",
          "form": "가정폭력이에요 그것도진짜",
          "original_form": "가정폭력이에요 그것도진짜",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100035022.1.3",
          "form": "폭력맞죠 ㅜㅜ",
          "original_form": "폭력맞죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100035022.1.4",
          "form": "그언니 그 후로 바로 이혼했어요",
          "original_form": "그언니 그 후로 바로 이혼했어요",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100035022.1.5",
          "form": "작년에 일어난 나름 따끈따근한 얘기였어요 ㅋㅋㅋ",
          "original_form": "작년에 일어난 나름 따끈따근한 얘기였어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100035022.1.6",
          "form": "벗어나셔서 정말 다행이에요ㅠㅠ",
          "original_form": "벗어나셔서 정말 다행이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100035022.1.7",
          "form": "안졸리시면 짧은 얘기하나 더 해드릴까요 ㅋㅋㅋ",
          "original_form": "안졸리시면 짧은 얘기하나 더 해드릴까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100035022.1.8",
          "form": "진짜 천벌 받을거에요 그 사람은",
          "original_form": "진짜 천벌 받을거에요 그 사람은",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100035022.1.9",
          "form": "네 좋아요ㅋㅋㅋ",
          "original_form": "네 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100035022.1.10",
          "form": "이건 제 직장동료 썰이에요 ㅋㅋㅋ",
          "original_form": "이건 제 직장동료 썰이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100035022.1.11",
          "form": "이야기 보따리시간 같아요ㅎㅎ",
          "original_form": "이야기 보따리시간 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100035022.1.12",
          "form": "이 동료도 저보다 한살언니라서 ㅋㅋ 언니라고 할게요 이 언니는 남친없는기간을 못견디는 언니에요 ㅋㅋㅋ",
          "original_form": "이 동료도 저보다 한살언니라서 ㅋㅋ 언니라고 할게요 이 언니는 남친없는기간을 못견디는 언니에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100035022.1.13",
          "form": "사랑꾼이시군요 ㅋㅋㅋㅋㅋ",
          "original_form": "사랑꾼이시군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100035022.1.14",
          "form": "맨날 만나고 헤어진다음에 저한테  아 이번 연애는 너무 힘들었어 몇달 쉬어야겠어 라고 말하고 일이주후 새남친이 생겼다고 말해요 ㅋㅋㅋ",
          "original_form": "맨날 만나고 헤어진다음에 저한테  아 이번 연애는 너무 힘들었어 몇달 쉬어야겠어 라고 말하고 일이주후 새남친이 생겼다고 말해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100035022.1.15",
          "form": "인기쟁이신가봐요 ㅋㅋㅋㅋ",
          "original_form": "인기쟁이신가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100035022.1.16",
          "form": "암튼 이 언니는 그 때 전남친과 헤어지고 동호회였나? 지인소개였나? 로 새남친을 만났어요",
          "original_form": "암튼 이 언니는 그 때 전남친과 헤어지고 동호회였나? 지인소개였나? 로 새남친을 만났어요",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100035022.1.17",
          "form": "인맥왕이시네요 ㅋㅋㅋ",
          "original_form": "인맥왕이시네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100035022.1.18",
          "form": "근데 얘가 만나면서 좀 너무 피곤하더래요 ㅋㅋㅋ 그렇게 술자리를 좋아하더래요 지인도 많고 너무 좀 활발히 놀아서 부담스럽더라는거에요",
          "original_form": "근데 얘가 만나면서 좀 너무 피곤하더래요 ㅋㅋㅋ 그렇게 술자리를 좋아하더래요 지인도 많고 너무 좀 활발히 놀아서 부담스럽더라는거에요",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100035022.1.19",
          "form": "극강의 외향적인 사람인가봐요 ㅋㅋㅋㅋ",
          "original_form": "극강의 외향적인 사람인가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:12"
        }
      ]
    }
