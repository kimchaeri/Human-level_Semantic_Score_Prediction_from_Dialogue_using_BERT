      "utterance": [
        {
          "id": "MDRW2100004171.1.1",
          "form": "저는 연애안한지 좀 됬는데",
          "original_form": "저는 연애안한지 좀 됬는데",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004171.1.2",
          "form": "지금 만나시는분 있으신가요?",
          "original_form": "지금 만나시는분 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004171.1.3",
          "form": "저도 연애를 안하고 있어요",
          "original_form": "저도 연애를 안하고 있어요",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004171.1.4",
          "form": "연애릉 안하는 이유가 있을까요?",
          "original_form": "연애릉 안하는 이유가 있을까요?",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004171.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004171.1.6",
          "form": "저는 그냥 딱히 만나고 싶다고 생각드는 사람을 못만나서..?",
          "original_form": "저는 그냥 딱히 만나고 싶다고 생각드는 사람을 못만나서..?",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004171.1.7",
          "form": "이사람 괜찮다",
          "original_form": "이사람 괜찮다",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004171.1.8",
          "form": "더 만나보고싶다",
          "original_form": "더 만나보고싶다",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004171.1.9",
          "form": "이렇게 생각드는 사람을 별로 못봣네요",
          "original_form": "이렇게 생각드는 사람을 별로 못봣네요",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004171.1.10",
          "form": "최근에는",
          "original_form": "최근에는",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004171.1.11",
          "form": "오 저두요",
          "original_form": "오 저두요",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004171.1.12",
          "form": "사람도 딱히....",
          "original_form": "사람도 딱히....",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004171.1.13",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004171.1.14",
          "form": "그냥 친구 만나서 노는게 더 편하고",
          "original_form": "그냥 친구 만나서 노는게 더 편하고",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004171.1.15",
          "form": "혼자노는게 더 편하규",
          "original_form": "혼자노는게 더 편하규",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004171.1.16",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004171.1.17",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004171.1.18",
          "form": "그리고 저는 귀찮음이커요",
          "original_form": "그리고 저는 귀찮음이커요",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004171.1.19",
          "form": "그 사람한테 시간 내기도 아깝고 하",
          "original_form": "그 사람한테 시간 내기도 아깝고 하",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004171.1.20",
          "form": "음 그렇죠",
          "original_form": "음 그렇죠",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004171.1.21",
          "form": "시간낭비 돈 낭비...?",
          "original_form": "시간낭비 돈 낭비...?",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004171.1.22",
          "form": "돈까지 가면 정 떨어진게 100프로긴한데",
          "original_form": "돈까지 가면 정 떨어진게 100프로긴한데",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004171.1.23",
          "form": "아무튼 새로운 사람 만나는것두 점점 귀찮은것같아요",
          "original_form": "아무튼 새로운 사람 만나는것두 점점 귀찮은것같아요",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004171.1.24",
          "form": "저랑 비슷항 생각을 가지셨네요",
          "original_form": "저랑 비슷항 생각을 가지셨네요",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004171.1.25",
          "form": "저도 그렇게 생각해요",
          "original_form": "저도 그렇게 생각해요",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004171.1.26",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004171.1.27",
          "form": "그냥 저에게 돈쓰고 싶구",
          "original_form": "그냥 저에게 돈쓰고 싶구",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004171.1.28",
          "form": "요새는 또 결혼 안한다고 닥달하지도 않구",
          "original_form": "요새는 또 결혼 안한다고 닥달하지도 않구",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004171.1.29",
          "form": "분위기가 연애안해도 뭐 어때",
          "original_form": "분위기가 연애안해도 뭐 어때",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004171.1.30",
          "form": "이런 분위기라서",
          "original_form": "이런 분위기라서",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004171.1.31",
          "form": "맞라요ㅋㅋ ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞라요ㅋㅋ ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004171.1.32",
          "form": "딱히 열심히 만나야겠다! 이런 생각도 잘 안들긴해요",
          "original_form": "딱히 열심히 만나야겠다! 이런 생각도 잘 안들긴해요",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004171.1.33",
          "form": "이런 분위기 아주 좋아요",
          "original_form": "이런 분위기 아주 좋아요",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004171.1.34",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004171.1.35",
          "form": "연애는 선택이지",
          "original_form": "연애는 선택이지",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004171.1.36",
          "form": "필수는 아니니까요",
          "original_form": "필수는 아니니까요",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004171.1.37",
          "form": "맞아요 !!! 요즘 마인드 즇아요",
          "original_form": "맞아요 !!! 요즘 마인드 즇아요",
          "speaker_id": "1",
          "time": "20210821 15:31"
        }
      ]
