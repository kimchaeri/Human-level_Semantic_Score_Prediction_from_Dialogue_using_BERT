      "utterance": [
        {
          "id": "MDRW2100034137.1.1",
          "form": "제일 최근에 구매한게 뭐예요?",
          "original_form": "제일 최근에 구매한게 뭐예요?",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100034137.1.2",
          "form": "저는 동물의숲 타이틀 샀어요",
          "original_form": "저는 동물의숲 타이틀 샀어요",
          "speaker_id": "1",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100034137.1.3",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100034137.1.4",
          "form": "전 음식밖에 안사요 ㅋㅋㅋ",
          "original_form": "전 음식밖에 안사요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100034137.1.5",
          "form": "저번주에 치즈맛 팝콘 사먹었어요",
          "original_form": "저번주에 치즈맛 팝콘 사먹었어요",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100034137.1.6",
          "form": "맛있었겠어요ㅋㅋㅋ",
          "original_form": "맛있었겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100034137.1.7",
          "form": "중고거래는 해본적있으세요?",
          "original_form": "중고거래는 해본적있으세요?",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100034137.1.8",
          "form": "네! 주로 책을 사고 팔아요",
          "original_form": "네! 주로 책을 사고 팔아요",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100034137.1.9",
          "form": "책 저는 제값받기가 그렇게 힘들던데",
          "original_form": "책 저는 제값받기가 그렇게 힘들던데",
          "speaker_id": "1",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100034137.1.10",
          "form": "거래 잘되시나요?",
          "original_form": "거래 잘되시나요?",
          "speaker_id": "1",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100034137.1.11",
          "form": "맞아요 그래서 살 때가 제일 좋죠 ㅋㅋ",
          "original_form": "맞아요 그래서 살 때가 제일 좋죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100034137.1.12",
          "form": "솔직히 잘 팔리진 않아요",
          "original_form": "솔직히 잘 팔리진 않아요",
          "speaker_id": "2",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100034137.1.13",
          "form": "그죠...저도 책이나 음반은 거의 공짜에 가깝게 팔았어요",
          "original_form": "그죠...저도 책이나 음반은 거의 공짜에 가깝게 팔았어요",
          "speaker_id": "1",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100034137.1.14",
          "form": "맞아요 저도 이번에 책 한권 팔았는데 3분의1 가격으로 팔았네요",
          "original_form": "맞아요 저도 이번에 책 한권 팔았는데 3분의1 가격으로 팔았네요",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100034137.1.15",
          "form": "그럼 책은 주로 사시는편이신가요?",
          "original_form": "그럼 책은 주로 사시는편이신가요?",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100034137.1.16",
          "form": "책을 잘 사는 편은 아닌데 산다면 중고로 사요",
          "original_form": "책을 잘 사는 편은 아닌데 산다면 중고로 사요",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100034137.1.17",
          "form": "절약을 위해...!",
          "original_form": "절약을 위해...!",
          "speaker_id": "2",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100034137.1.18",
          "form": "그럼 당근같은거 쓰세요? 아님 알라딘같은거 쓰시나요?",
          "original_form": "그럼 당근같은거 쓰세요? 아님 알라딘같은거 쓰시나요?",
          "speaker_id": "1",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100034137.1.19",
          "form": "당근, 중고나라, 알라딘 정도 써요",
          "original_form": "당근, 중고나라, 알라딘 정도 써요",
