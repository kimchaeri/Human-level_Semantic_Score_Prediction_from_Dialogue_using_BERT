      "utterance": [
        {
          "id": "MDRW2100043246.1.1",
          "form": "거기도 아직 비와요?",
          "original_form": "거기도 아직 비와요?",
          "speaker_id": "1",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100043246.1.2",
          "form": "왔다가 그쳤다가 하는 것 같아요!",
          "original_form": "왔다가 그쳤다가 하는 것 같아요!",
          "speaker_id": "2",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043246.1.3",
          "form": "여기는 계속 흩날려요 ㅠ",
          "original_form": "여기는 계속 흩날려요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043246.1.4",
          "form": "그럴땐 차라리 한번에 내리고 끝났으면 싶죠..",
          "original_form": "그럴땐 차라리 한번에 내리고 끝났으면 싶죠..",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043246.1.5",
          "form": "우산은 챙기셧나요?!",
          "original_form": "우산은 챙기셧나요?!",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043246.1.6",
          "form": "아뇨 차가 바로앞에 있어섴ㅋㅋㅋ그냥 맞고타려구요",
          "original_form": "아뇨 차가 바로앞에 있어섴ㅋㅋㅋ그냥 맞고타려구요",
          "speaker_id": "1",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043246.1.7",
          "form": "아 ㅋㅋㅋㅋ 퇴근할때는 잠깐 그치면 좋겠네여!",
          "original_form": "아 ㅋㅋㅋㅋ 퇴근할때는 잠깐 그치면 좋겠네여!",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043246.1.8",
          "form": "맞아용 ㅠㅠ 흐리니깐 부침개 먹고프네여",
          "original_form": "맞아용 ㅠㅠ 흐리니깐 부침개 먹고프네여",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043246.1.9",
          "form": "비올땐 전이랑 막걸리죠! ㅋㅋㅋ",
          "original_form": "비올땐 전이랑 막걸리죠! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043246.1.10",
          "form": "칼국수도 좋을 것 같아요",
          "original_form": "칼국수도 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043246.1.11",
          "form": "크 맞아용 해물칼국수 먹고싶네용",
          "original_form": "크 맞아용 해물칼국수 먹고싶네용",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043246.1.12",
          "form": "쌀쌀한 날씨에는 또 국물요리죠ㅜㅋㅋㅋ",
          "original_form": "쌀쌀한 날씨에는 또 국물요리죠ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100043246.1.13",
          "form": "그래도 요즘 미세먼지는 없죠? 비와성",
          "original_form": "그래도 요즘 미세먼지는 없죠? 비와성",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043246.1.14",
          "form": "그렇죵 미세먼지가 있을 틈 없이 비만 와서..",
          "original_form": "그렇죵 미세먼지가 있을 틈 없이 비만 와서..",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043246.1.15",
          "form": "그래도 내일이나 모레쯤엔 맑대요!",
          "original_form": "그래도 내일이나 모레쯤엔 맑대요!",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043246.1.16",
          "form": "주말에 맑았으면 좋겠어용 친구랑 약속있거든요",
          "original_form": "주말에 맑았으면 좋겠어용 친구랑 약속있거든요",
          "speaker_id": "1",
          "time": "20210907 16:34"
        }
      ]
