        },
        {
          "id": "MDRW2100036874.1.2",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100036874.1.3",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "1",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036874.1.4",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036874.1.5",
          "form": "만두국 먹었어요",
          "original_form": "만두국 먹었어요",
          "speaker_id": "1",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036874.1.6",
          "form": "헉 너무 맛있었을 거 같아요",
          "original_form": "헉 너무 맛있었을 거 같아요",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100036874.1.7",
          "form": "만들어 드셨나요?!",
          "original_form": "만들어 드셨나요?!",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100036874.1.8",
          "form": "아니요ㅎㅎ 식당에서 먹었어요",
          "original_form": "아니요ㅎㅎ 식당에서 먹었어요",
          "speaker_id": "1",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100036874.1.9",
          "form": "떡도 들어서 맛있었어요",
          "original_form": "떡도 들어서 맛있었어요",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100036874.1.10",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100036874.1.11",
          "form": "떡만둣국! 진짜 맛있죠",
          "original_form": "떡만둣국! 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100036874.1.12",
          "form": "저는 볶음밥이랑 미역국 먹었어요~",
          "original_form": "저는 볶음밥이랑 미역국 먹었어요~",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100036874.1.13",
          "form": "김치볶음밥인가요?",
          "original_form": "김치볶음밥인가요?",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100036874.1.14",
          "form": "아뇨 계란볶음밥 해먹었어요ㅎㅎ",
          "original_form": "아뇨 계란볶음밥 해먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100036874.1.15",
          "form": "맛있죠ㅋㅋ",
          "original_form": "맛있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036874.1.16",
          "form": "파기름 해서 볶으면 맛있게 되더라구요",
          "original_form": "파기름 해서 볶으면 맛있게 되더라구요",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036874.1.17",
          "form": "간단해도 너무 맛있었어요ㅋㅋㅋ",
          "original_form": "간단해도 너무 맛있었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036874.1.18",
          "form": "다음에는 파기름 내서 볶아볼게요!ㅎㅎ",
          "original_form": "다음에는 파기름 내서 볶아볼게요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036874.1.19",
          "form": "ㅋㅋ네 간단해서 해먹기 좋아요",
          "original_form": "ㅋㅋ네 간단해서 해먹기 좋아요",
