        },
        {
          "id": "MDRW2100041190.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100041190.1.3",
          "form": "가을이 왔어요.",
          "original_form": "가을이 왔어요.",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041190.1.4",
          "form": "가을 옷은 준비하셨어요?",
          "original_form": "가을 옷은 준비하셨어요?",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041190.1.5",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041190.1.6",
          "form": "아직 준비도 덜 됐는데",
          "original_form": "아직 준비도 덜 됐는데",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041190.1.7",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041190.1.8",
          "form": "가을이 성큼 와버렸네요 ㅜㅠ",
          "original_form": "가을이 성큼 와버렸네요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041190.1.9",
          "form": "저도 이번주말에 옷 정리 하려고 했는데",
          "original_form": "저도 이번주말에 옷 정리 하려고 했는데",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041190.1.10",
          "form": "준비되셨나요?",
          "original_form": "준비되셨나요?",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041190.1.11",
          "form": "결국은 ㅋ또 ㅋㅋㅋㅋ 못했네요",
          "original_form": "결국은 ㅋ또 ㅋㅋㅋㅋ 못했네요",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041190.1.12",
          "form": "저도 정리해야하는데 ㅜㅠ",
          "original_form": "저도 정리해야하는데 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041190.1.13",
          "form": "내일은 꼭 하려구요 ㅋㅋㅋ 한여름 옷 먼저 정리를 해야될것같아요. ㅋㅋ",
          "original_form": "내일은 꼭 하려구요 ㅋㅋㅋ 한여름 옷 먼저 정리를 해야될것같아요. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041190.1.14",
          "form": "자꾸 미루다가 겨울올지 ㅋ",
          "original_form": "자꾸 미루다가 겨울올지 ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041190.1.15",
          "form": "공간이 부족한 관계로 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "공간이 부족한 관계로 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041190.1.16",
          "form": "그러니깐요 ㅋㅋㅋ",
          "original_form": "그러니깐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041190.1.17",
          "form": "그냥 리빙박스에서 꺼내입다가 겨울맞이 할지도 ㅎ",
          "original_form": "그냥 리빙박스에서 꺼내입다가 겨울맞이 할지도 ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041190.1.18",
          "form": "네네 그럴것같아요 ㅋㅋ 저도 ㅋㅋ",
          "original_form": "네네 그럴것같아요 ㅋㅋ 저도 ㅋㅋ",
