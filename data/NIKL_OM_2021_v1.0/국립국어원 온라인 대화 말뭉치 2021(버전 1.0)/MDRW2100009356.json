      "utterance": [
        {
          "id": "MDRW2100009356.1.1",
          "form": "요즘 시간때울만한 재밌는게임 아시면 추천해주세요!",
          "original_form": "요즘 시간때울만한 재밌는게임 아시면 추천해주세요!",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009356.1.2",
          "form": "치킨각이랑 캐시퍼 추천해요 ㅋ",
          "original_form": "치킨각이랑 캐시퍼 추천해요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009356.1.3",
          "form": "폰게임인가요??",
          "original_form": "폰게임인가요??",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009356.1.4",
          "form": "폰게임인데 단순하고 쉬워요",
          "original_form": "폰게임인데 단순하고 쉬워요",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009356.1.5",
          "form": "심심할때 설치해봐야겠어요 ㅎㅎ",
          "original_form": "심심할때 설치해봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009356.1.6",
          "form": "그리고 코딱지만큼이지만 포인트주거든요",
          "original_form": "그리고 코딱지만큼이지만 포인트주거든요",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009356.1.7",
          "form": "음 치킨각으로 한달하고",
          "original_form": "음 치킨각으로 한달하고",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009356.1.8",
          "form": "싸이버거세트 하나 사먹을정도는 벌어요",
          "original_form": "싸이버거세트 하나 사먹을정도는 벌어요",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009356.1.9",
          "form": "아 다 포인트 쌓이는 게임인가봐요 ㅋㅋ",
          "original_form": "아 다 포인트 쌓이는 게임인가봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009356.1.10",
          "form": "네 ㅋ그래서 못끊어요",
          "original_form": "네 ㅋ그래서 못끊어요",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009356.1.11",
          "form": "중독성갑 ㅋㅋㅋ",
          "original_form": "중독성갑 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009356.1.12",
          "form": "ㅋㅋㅋㅋ되게 알뜰하시네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ되게 알뜰하시네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009356.1.13",
          "form": "모은포인트가아까워요 그게뭐라고 ㅋㅋㅋ",
          "original_form": "모은포인트가아까워요 그게뭐라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009356.1.14",
          "form": "두달모아야 버거하나먹을정도인데 ㅋㅋ",
          "original_form": "두달모아야 버거하나먹을정도인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 08:25"
        },
        {
          "id": "MDRW2100009356.1.15",
          "form": "아 어제 방탈출게임하다가 새벽 2시에 잤더니 너무 피곤하네요 ㅠ",
          "original_form": "아 어제 방탈출게임하다가 새벽 2시에 잤더니 너무 피곤하네요 ㅠ",
          "speaker_id": "1",
          "time": "20210823 08:25"
        },
        {
          "id": "MDRW2100009356.1.16",
          "form": "그거말고 그 광고하는 정원 꾸미거나 집꾸미는 게임도 잼나보이긴하더라고요",
          "original_form": "그거말고 그 광고하는 정원 꾸미거나 집꾸미는 게임도 잼나보이긴하더라고요",
          "speaker_id": "2",
          "time": "20210823 09:46"
        },
        {
          "id": "MDRW2100009356.1.17",
          "form": "ㅋㅋㅋㅋㅋ그거 집꾸미는건 부가적이고 캔디크러쉬사가같은 게임이더라구요 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ그거 집꾸미는건 부가적이고 캔디크러쉬사가같은 게임이더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210823 10:53"
        }
      ]
