      "utterance": [
        {
          "id": "MDRW2100006748.1.1",
          "form": "네 어플이 여러가지더라구요",
          "original_form": "네 어플이 여러가지더라구요",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100006748.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006748.1.3",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006748.1.4",
          "form": "거의 안 써봤다고는 했지만",
          "original_form": "거의 안 써봤다고는 했지만",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006748.1.5",
          "form": "사실?",
          "original_form": "사실?",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006748.1.6",
          "form": "한번도 안 써봐서",
          "original_form": "한번도 안 써봐서",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006748.1.7",
          "form": "잘 모르겠네요..",
          "original_form": "잘 모르겠네요..",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006748.1.8",
          "form": "아 저는 들어보기만 했어요 윌라랑 밀리의 서재?",
          "original_form": "아 저는 들어보기만 했어요 윌라랑 밀리의 서재?",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006748.1.9",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006748.1.10",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006748.1.11",
          "form": "전자책은",
          "original_form": "전자책은",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006748.1.12",
          "form": "리디북스 이용하는데",
          "original_form": "리디북스 이용하는데",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100006748.1.13",
          "form": "오디오북 기능이 있다는",
          "original_form": "오디오북 기능이 있다는",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100006748.1.14",
          "form": "소문...은 들어봤어요",
          "original_form": "소문...은 들어봤어요",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100006748.1.15",
          "form": "와이프가 걷기운동할때 사용하더라구요 ㅎㅎ",
          "original_form": "와이프가 걷기운동할때 사용하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006748.1.16",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006748.1.17",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006748.1.18",
          "form": "오디오북을 쓰더라도",
          "original_form": "오디오북을 쓰더라도",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006748.1.19",
          "form": "에잇 답답해 비켜 그냥 내가 읽는다",
          "original_form": "에잇 답답해 비켜 그냥 내가 읽는다",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006748.1.20",
          "form": "이렇게 될 거 같아서",
          "original_form": "이렇게 될 거 같아서",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006748.1.21",
          "form": "종이책을 선호하하나요",
          "original_form": "종이책을 선호하하나요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006748.1.22",
          "form": "하시나요",
          "original_form": "하시나요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006748.1.23",
          "form": "종이책보다는",
          "original_form": "종이책보다는",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006748.1.24",
          "form": "텍스트를 선호하는 쪽에 가깝습니다",
          "original_form": "텍스트를 선호하는 쪽에 가깝습니다",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006748.1.25",
          "form": "그러시군요 그럼 보관은 할만하세요? 자리차지를 하다보니",
          "original_form": "그러시군요 그럼 보관은 할만하세요? 자리차지를 하다보니",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006748.1.26",
          "form": "그래서 전자책은 이용합니다",
          "original_form": "그래서 전자책은 이용합니다",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006748.1.27",
          "form": "그렇죠 아이책이 많아져서 어른책운 처분하게 되더라구요",
          "original_form": "그렇죠 아이책이 많아져서 어른책운 처분하게 되더라구요",
