      "utterance": [
        {
          "id": "MDRW2100045836.1.1",
          "form": "맞아요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045836.1.2",
          "form": "어제 저녁에 강의가 있어서",
          "original_form": "어제 저녁에 강의가 있어서",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045836.1.3",
          "form": "야호 맞췄다",
          "original_form": "야호 맞췄다",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045836.1.4",
          "form": "강의들으면서 몰래 먹누라고 대충 밥에 김치찌개 말아먹었어요 ㅋㅋ",
          "original_form": "강의들으면서 몰래 먹누라고 대충 밥에 김치찌개 말아먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045836.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 맛있었겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045836.1.6",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045836.1.7",
          "form": "저도 name1님 저녁 맞춰볼래요!",
          "original_form": "저도 &name1&님 저녁 맞춰볼래요!",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045836.1.8",
          "form": "근데 밤에 드셨나요 왜 몰래 드셨어요 ㅡㅋㅋㅋㅋ",
          "original_form": "근데 밤에 드셨나요 왜 몰래 드셨어요 ㅡㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045836.1.9",
          "form": "헉 좋아요~",
          "original_form": "헉 좋아요~",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045836.1.10",
          "form": "강의중이라 ㅎㅎ 대놓고 펼쳐놓고 먹기가 뭐해서요 ㅎㅎ",
          "original_form": "강의중이라 ㅎㅎ 대놓고 펼쳐놓고 먹기가 뭐해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045836.1.11",
          "form": "밥종류이신가요?",
          "original_form": "밥종류이신가요?",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045836.1.12",
          "form": "아 화상강의 하셨나요ㅋㅋㅋㅋ",
          "original_form": "아 화상강의 하셨나요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045836.1.13",
          "form": "ㄴ네네 ㅎㅎ",
          "original_form": "ㄴ네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045836.1.14",
          "form": "밥이랑 같이 먹는 음식이예요!",
          "original_form": "밥이랑 같이 먹는 음식이예요!",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045836.1.15",
          "form": "국물종류일까요?",
          "original_form": "국물종류일까요?",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045836.1.16",
          "form": "아니요 국물이 없어용",
          "original_form": "아니요 국물이 없어용",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100045836.1.17",
          "form": "흐음",
          "original_form": "흐음",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100045836.1.18",
          "form": "달걀이 들어가나요..? ㅋㅋㅋㅋ",
          "original_form": "달걀이 들어가나요..? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100045836.1.19",
          "form": "헉 네 달걀이 들어가요!",
          "original_form": "헉 네 달걀이 들어가요!",
          "speaker_id": "1",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100045836.1.20",
          "form": "계란말이...?",
          "original_form": "계란말이...?",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100045836.1.21",
          "form": "아뇨! 아쉽게 계란말이는 아닙니다 ㅎㄹ",
          "original_form": "아뇨! 아쉽게 계란말이는 아닙니다 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045836.1.22",
          "form": "학 계란까진 잘 맞췄는데!!",
          "original_form": "학 계란까진 잘 맞췄는데!!",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045836.1.23",
          "form": "그니까요! 이름에도 계란이 들어가요",
          "original_form": "그니까요! 이름에도 계란이 들어가요",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045836.1.24",
          "form": "계란후라이인가요...? 너무 직관적으로 접근하나요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "계란후라이인가요...? 너무 직관적으로 접근하나요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045836.1.25",
          "form": "토달볶일수도있는뎈ㅋㅋ",
          "original_form": "토달볶일수도있는뎈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045836.1.26",
          "form": "ㅓ그헉",
          "original_form": "ㅓ그헉",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045836.1.27",
          "form": "토달볶 맞아요",
          "original_form": "토달볶 맞아요",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045836.1.28",
          "form": "정답!!!! 뿌이뿌이뿌아",
          "original_form": "정답!!!! 뿌이뿌이뿌아",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045836.1.29",
          "form": "헉 대박",
          "original_form": "헉 대박",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045836.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ어떻게 맞추신거죠 토마토는 말도 안 했는디",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어떻게 맞추신거죠 토마토는 말도 안 했는디",
          "speaker_id": "1",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045836.1.31",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱ",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045836.1.32",
          "form": "저 사실 친구들한테 돗자리 펴라는 소리 종종 들어요",
          "original_form": "저 사실 친구들한테 돗자리 펴라는 소리 종종 들어요",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045836.1.33",
          "form": "와중에 전 토달볶 한번도 안먹어봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "와중에 전 토달볶 한번도 안먹어봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045836.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진짜 피셔도 될 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진짜 피셔도 될 것 같아요",
          "speaker_id": "1",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045836.1.35",
          "form": "이럴수가 토마토 안 좋아하시나요",
          "original_form": "이럴수가 토마토 안 좋아하시나요",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045836.1.36",
          "form": "아뇨 토마토 좋아하는데",
          "original_form": "아뇨 토마토 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045836.1.37",
          "form": "아직 달걀이랑 같이 볶아보지는 못했어요 ㅋㅋㅋㅋ",
          "original_form": "아직 달걀이랑 같이 볶아보지는 못했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
