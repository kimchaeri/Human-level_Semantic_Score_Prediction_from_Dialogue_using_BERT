      "utterance": [
        {
          "id": "MDRW2100047620.1.1",
          "form": "별로 기대가 안되는데 이번 예능은",
          "original_form": "별로 기대가 안되는데 이번 예능은",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100047620.1.2",
          "form": "너무 똑같은게 많아 식상함",
          "original_form": "너무 똑같은게 많아 식상함",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100047620.1.3",
          "form": "엉 이제 다른 포맷으로 해야하겠다",
          "original_form": "엉 이제 다른 포맷으로 해야하겠다",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047620.1.4",
          "form": "신서유기에서 나오는 게임들 다른데서도 써먹고",
          "original_form": "신서유기에서 나오는 게임들 다른데서도 써먹고",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047620.1.5",
          "form": "솔직히 신서유기에서 나온 게임들도 옛날에 가족오락관에서 하던거다 ㅋㅋ",
          "original_form": "솔직히 신서유기에서 나온 게임들도 옛날에 가족오락관에서 하던거다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100047620.1.6",
          "form": "그래 고요속의 외침",
          "original_form": "그래 고요속의 외침",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100047620.1.7",
          "form": "응 게임이 다 비슷",
          "original_form": "응 게임이 다 비슷",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100047620.1.8",
          "form": "나영석 이번에 좀 바꺼야 해",
          "original_form": "나영석 이번에 좀 바꺼야 해",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047620.1.9",
          "form": "그러게 너무 비슷한걸 많이 했다",
          "original_form": "그러게 너무 비슷한걸 많이 했다",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047620.1.10",
          "form": "그래서 예전에 무도가 대단했어",
          "original_form": "그래서 예전에 무도가 대단했어",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100047620.1.11",
          "form": "무도가 매번 안에서 바뀌니 그래서 힘들었잖아",
          "original_form": "무도가 매번 안에서 바뀌니 그래서 힘들었잖아",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047620.1.12",
          "form": "김태호 피디가 대단해",
          "original_form": "김태호 피디가 대단해",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047620.1.13",
          "form": "지금 놀면뭐하니 하는거 보니 아이디어천재다",
          "original_form": "지금 놀면뭐하니 하는거 보니 아이디어천재다",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047620.1.14",
          "form": "그러니까 어떻게 그렇게 아이디어를 내는지",
          "original_form": "그러니까 어떻게 그렇게 아이디어를 내는지",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047620.1.15",
          "form": "그러게 약간 사차원인가",
          "original_form": "그러게 약간 사차원인가",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047620.1.16",
          "form": "ㅋㅋ 그래도  똑똑한거 아닌가",
          "original_form": "ㅋㅋ 그래도  똑똑한거 아닌가",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047620.1.17",
          "form": "똑똑한가 싶어서 검색해보ㅏㅆ다",
          "original_form": "똑똑한가 싶어서 검색해보ㅏㅆ다",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100047620.1.18",
          "form": "뭐가 나오는데?",
          "original_form": "뭐가 나오는데?",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047620.1.19",
          "form": "고려대 신방과 출신이네",
          "original_form": "고려대 신방과 출신이네",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047620.1.20",
          "form": "진짜 똑똑한거 맞네",
          "original_form": "진짜 똑똑한거 맞네",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047620.1.21",
          "form": "나영석피디가 연세대 행정학",
          "original_form": "나영석피디가 연세대 행정학",
