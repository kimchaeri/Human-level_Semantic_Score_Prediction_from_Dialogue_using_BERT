      "utterance": [
        {
          "id": "MDRW2100032271.1.1",
          "form": "대학교 다닐 때과 전공 관련 과목은 일 년에네 번 쳤던 기억이 나네요",
          "original_form": "대학교 다닐 때과 전공 관련 과목은 일 년에네 번 쳤던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100032271.1.2",
          "form": "저는 다른 학과 과목도 들었어요",
          "original_form": "저는 다른 학과 과목도 들었어요",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100032271.1.3",
          "form": "양이 너무 많아서 두 번 시험 쉬지 않고 4번 쳤던 기억이나요",
          "original_form": "양이 너무 많아서 두 번 시험 쉬지 않고 4번 쳤던 기억이나요",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100032271.1.4",
          "form": "스포츠 학과에서 스포츠 마사지 들었어요",
          "original_form": "스포츠 학과에서 스포츠 마사지 들었어요",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100032271.1.5",
          "form": "오 전공은 무엇이었고 어떤 과목을 추가로 들으신 거지요",
          "original_form": "오 전공은 무엇이었고 어떤 과목을 추가로 들으신 거지요",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.6",
          "form": "스포츠 와우",
          "original_form": "스포츠 와우",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.7",
          "form": "친구 보니까 유아교육과에서 피아노도 치더라고요",
          "original_form": "친구 보니까 유아교육과에서 피아노도 치더라고요",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.8",
          "form": "마사지 잘 하시나요",
          "original_form": "마사지 잘 하시나요",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.9",
          "form": "체육학과 가서 수영도 하고요",
          "original_form": "체육학과 가서 수영도 하고요",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.10",
          "form": "우와 그런 것도 있군요",
          "original_form": "우와 그런 것도 있군요",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.11",
          "form": "그때 다 같이 자격증을 땄는데",
          "original_form": "그때 다 같이 자격증을 땄는데",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.12",
          "form": "수영 와우",
          "original_form": "수영 와우",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.13",
          "form": "학교에서 다양하게 배우 셨네요",
          "original_form": "학교에서 다양하게 배우 셨네요",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.14",
          "form": "자격증 받으러 안 갔어요 귀찮아서",
          "original_form": "자격증 받으러 안 갔어요 귀찮아서",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.15",
          "form": "귀차니즘이 저랑 만만치 않으시네요",
          "original_form": "귀차니즘이 저랑 만만치 않으시네요",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.16",
          "form": "다른 학과 과목을 들으면 덜 아까워요",
          "original_form": "다른 학과 과목을 들으면 덜 아까워요",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.17",
          "form": "학비가 300만원이 넘었어요",
          "original_form": "학비가 300만원이 넘었어요",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.18",
          "form": "과학 과목을 들어 볼 생각은 없으셨나요",
          "original_form": "과학 과목을 들어 볼 생각은 없으셨나요",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032271.1.19",
          "form": "과학은 먹는 건가요",
          "original_form": "과학은 먹는 건가요",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032271.1.20",
          "form": "먹지 않습니다",
          "original_form": "먹지 않습니다",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032271.1.21",
          "form": "침대는 과학입니다",
          "original_form": "침대는 과학입니다",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032271.1.22",
          "form": "경제학를 복수 전공해서 따라가기 바빴어요",
          "original_form": "경제학를 복수 전공해서 따라가기 바빴어요",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032271.1.23",
          "form": "과학도 듣는 사람 굉장히 많았거든요",
          "original_form": "과학도 듣는 사람 굉장히 많았거든요",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032271.1.24",
          "form": "경제학 이란이 정말 심오한 학문입니다",
          "original_form": "경제학 이란이 정말 심오한 학문입니다",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032271.1.25",
          "form": "실험 같은 건 재밌긴 하겠네요",
          "original_form": "실험 같은 건 재밌긴 하겠네요",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032271.1.26",
          "form": "스트레스 받지 않으셨나요",
          "original_form": "스트레스 받지 않으셨나요",
          "speaker_id": "1",
          "time": "20210905 14:45"
        }
