        {
          "id": "MDRW2100036252.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100036252.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100036252.1.3",
          "form": "점심 맛있게 드셨나요?",
          "original_form": "점심 맛있게 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100036252.1.4",
          "form": "네 장어 먹었어요",
          "original_form": "네 장어 먹었어요",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100036252.1.5",
          "form": "맛있었겠네요",
          "original_form": "맛있었겠네요",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100036252.1.6",
          "form": "넹 저녁에 또 먹고싶네요",
          "original_form": "넹 저녁에 또 먹고싶네요",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100036252.1.7",
          "form": "저녁은 뭘 드실 예정이세요?",
          "original_form": "저녁은 뭘 드실 예정이세요?",
          "speaker_id": "2",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100036252.1.8",
          "form": "버섯볶고 있어요",
          "original_form": "버섯볶고 있어요",
          "speaker_id": "1",
          "time": "20210901 17:58"
        },
        {
          "id": "MDRW2100036252.1.9",
          "form": "들기름장 찍어먹으려구요",
          "original_form": "들기름장 찍어먹으려구요",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100036252.1.10",
          "form": "깔끔하고 맛있겠네요!",
          "original_form": "깔끔하고 맛있겠네요!",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100036252.1.11",
          "form": "갑자기 새송이버섯 구워 먹고싶어지네요 ㅎㅎ",
          "original_form": "갑자기 새송이버섯 구워 먹고싶어지네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100036252.1.12",
          "form": "맛있고 건강에도 좋아요 추천합니다",
          "original_form": "맛있고 건강에도 좋아요 추천합니다",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100036252.1.13",
          "form": "들기름은 뽑아 드시나요? 사서 드시나요??",
          "original_form": "들기름은 뽑아 드시나요? 사서 드시나요??",
          "speaker_id": "2",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100036252.1.14",
          "form": "사먹어요 시골에 아는 분이 없어서ㅜㅜㅜ",
          "original_form": "사먹어요 시골에 아는 분이 없어서ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100036252.1.15",
          "form": "방앗간에서 직접 짜면 좋을텐데",
          "original_form": "방앗간에서 직접 짜면 좋을텐데",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100036252.1.16",
          "form": "저희도 아는 분은 없지만 들깨 사서 뽑아와요 ㅎㅎ",
          "original_form": "저희도 아는 분은 없지만 들깨 사서 뽑아와요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100036252.1.17",
          "form": "확실히 파는 것보다 맛있고 좋더라고요!",
          "original_form": "확실히 파는 것보다 맛있고 좋더라고요!",
          "speaker_id": "2",
          "time": "20210901 20:01"
        }
      ]
