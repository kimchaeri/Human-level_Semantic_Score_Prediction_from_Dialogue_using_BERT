        {
          "id": "MDRW2100024192.1.1",
          "form": "네 전 강아지 키워요",
          "original_form": "네 전 강아지 키워요",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024192.1.2",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024192.1.3",
          "form": "안 키우이나요",
          "original_form": "안 키우이나요",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024192.1.4",
          "form": "아직 여건이 안돼서요",
          "original_form": "아직 여건이 안돼서요",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024192.1.5",
          "form": "금전적인 부분이 좀 커요",
          "original_form": "금전적인 부분이 좀 커요",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024192.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024192.1.7",
          "form": "그래도 키우면 행복해요",
          "original_form": "그래도 키우면 행복해요",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024192.1.8",
          "form": "저도 그럴 것 같아요",
          "original_form": "저도 그럴 것 같아요",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024192.1.9",
          "form": "고양이는 좋아하나요",
          "original_form": "고양이는 좋아하나요",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024192.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024192.1.11",
          "form": "전 키워본 적이 없어서요",
          "original_form": "전 키워본 적이 없어서요",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024192.1.12",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024192.1.13",
          "form": "이쁘긴한데 털이 너무 빠져요",
          "original_form": "이쁘긴한데 털이 너무 빠져요",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024192.1.14",
          "form": "그니까요",
