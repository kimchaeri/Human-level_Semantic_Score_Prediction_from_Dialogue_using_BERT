      "utterance": [
        {
          "id": "MDRW2100022143.1.1",
          "form": "어디로 여행가고싶은가요",
          "original_form": "어디로 여행가고싶은가요",
          "speaker_id": "1",
          "time": "20210829 09:17"
        },
        {
          "id": "MDRW2100022143.1.2",
          "form": "저는 바다요",
          "original_form": "저는 바다요",
          "speaker_id": "2",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100022143.1.3",
          "form": "바다좋죠",
          "original_form": "바다좋죠",
          "speaker_id": "1",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100022143.1.4",
          "form": "어느 지역으로 가고싶은가요",
          "original_form": "어느 지역으로 가고싶은가요",
          "speaker_id": "1",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100022143.1.5",
          "form": "서해쪽으로요",
          "original_form": "서해쪽으로요",
          "speaker_id": "2",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100022143.1.6",
          "form": "거리도 가깝고",
          "original_form": "거리도 가깝고",
          "speaker_id": "2",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100022143.1.7",
          "form": "명소가 많아요",
          "original_form": "명소가 많아요",
          "speaker_id": "2",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100022143.1.8",
          "form": "님은 어디로 여행가고 싶은가요",
          "original_form": "님은 어디로 여행가고 싶은가요",
          "speaker_id": "2",
          "time": "20210829 09:19"
        },
        {
          "id": "MDRW2100022143.1.9",
          "form": "저도 바다가 좋아요",
          "original_form": "저도 바다가 좋아요",
          "speaker_id": "1",
          "time": "20210829 09:19"
        },
        {
          "id": "MDRW2100022143.1.10",
          "form": "제주도 가고싶어요",
          "original_form": "제주도 가고싶어요",
          "speaker_id": "1",
          "time": "20210829 09:19"
        },
        {
          "id": "MDRW2100022143.1.11",
          "form": "제주바다 너무 멋있어요",
          "original_form": "제주바다 너무 멋있어요",
          "speaker_id": "2",
          "time": "20210829 09:19"
        },
        {
          "id": "MDRW2100022143.1.12",
          "form": "여행은 언제나 설레요",
          "original_form": "여행은 언제나 설레요",
          "speaker_id": "2",
          "time": "20210829 09:19"
        },
        {
          "id": "MDRW2100022143.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
