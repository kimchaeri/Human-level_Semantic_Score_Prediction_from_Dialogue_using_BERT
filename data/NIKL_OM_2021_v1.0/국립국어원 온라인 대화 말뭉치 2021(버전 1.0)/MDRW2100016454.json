        },
        {
          "id": "MDRW2100016454.1.2",
          "form": "지금 일을 하고있지는 않아요!",
          "original_form": "지금 일을 하고있지는 않아요!",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016454.1.3",
          "form": "name1은 일하고 계신가요?",
          "original_form": "&name1&은 일하고 계신가요?",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016454.1.4",
          "form": "네 일해요",
          "original_form": "네 일해요",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016454.1.5",
          "form": "무슨 일 하세요?",
          "original_form": "무슨 일 하세요?",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016454.1.6",
          "form": "사무직이요",
          "original_form": "사무직이요",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016454.1.7",
          "form": "아하 그러시구나!",
          "original_form": "아하 그러시구나!",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016454.1.8",
          "form": "일이 힘들진 않으세요?",
          "original_form": "일이 힘들진 않으세요?",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016454.1.9",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016454.1.10",
          "form": "어떤게 가장 힘드세요?",
          "original_form": "어떤게 가장 힘드세요?",
          "speaker_id": "2",
          "time": "20210826 13:05"
