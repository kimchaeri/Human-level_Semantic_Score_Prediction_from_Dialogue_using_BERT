      "utterance": [
        {
          "id": "MDRW2100045906.1.1",
          "form": "아침 드셧어요,?",
          "original_form": "아침 드셧어요,?",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045906.1.2",
          "form": "아침 먹었습니다! 토스트해서 쨈 발라 먹었어요!",
          "original_form": "아침 먹었습니다! 토스트해서 쨈 발라 먹었어요!",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045906.1.3",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045906.1.4",
          "form": "우아 저는 아직이요",
          "original_form": "우아 저는 아직이요",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045906.1.5",
          "form": "잼은 딸기잼 드셧나요",
          "original_form": "잼은 딸기잼 드셧나요",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045906.1.6",
          "form": "네네! 딸기잼 먹었습니다!",
          "original_form": "네네! 딸기잼 먹었습니다!",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045906.1.7",
          "form": "뭐 드실 예정이세요?",
          "original_form": "뭐 드실 예정이세요?",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045906.1.8",
          "form": "저는 간단하게 찌개에다가 먹을려구용",
          "original_form": "저는 간단하게 찌개에다가 먹을려구용",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045906.1.9",
          "form": "우왕 맛있겠네요 요즘같은 날씨에 국물 딱 좋은거 같아요",
          "original_form": "우왕 맛있겠네요 요즘같은 날씨에 국물 딱 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045906.1.10",
          "form": "ㅜㅜ ㄴ네 요리 잘하세요?",
          "original_form": "ㅜㅜ ㄴ네 요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045906.1.11",
          "form": "ㅠㅠ 아뇨... 요리 잘 못해요 이상하게 제가 하면 맛이 안나더하고요 ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅠㅠ 아뇨... 요리 잘 못해요 이상하게 제가 하면 맛이 안나더하고요 ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045906.1.12",
          "form": "우아 저도요 ㅠㅠ 저는 물도 못마추고 ㅋㅋㅋㅋ",
          "original_form": "우아 저도요 ㅠㅠ 저는 물도 못마추고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045906.1.13",
          "form": "레시피대로 하긴 하는데",
          "original_form": "레시피대로 하긴 하는데",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045906.1.14",
          "form": "ㅋㅋ큐ㅠㅠ 저도요",
          "original_form": "ㅋㅋ큐ㅠㅠ 저도요",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045906.1.15",
          "form": "이상하게 하라는 대로 하는데 우당탕탕 하면서 만들고 나면",
          "original_form": "이상하게 하라는 대로 하는데 우당탕탕 하면서 만들고 나면",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045906.1.16",
          "form": "맛은 그닥..다그닥...",
          "original_form": "맛은 그닥..다그닥...",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045906.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 뭔가 이프로 부족한 맛이에요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 뭔가 이프로 부족한 맛이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045906.1.18",
          "form": "그래도 뿌듯한건 잇어요",
          "original_form": "그래도 뿌듯한건 잇어요",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045906.1.19",
          "form": "ㅋㅋㅋㅋ 그건 그렇죠",
          "original_form": "ㅋㅋㅋㅋ 그건 그렇죠",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045906.1.20",
          "form": "내가 내 식생활을 해결했어! 하는 뿌듯함이요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내가 내 식생활을 해결했어! 하는 뿌듯함이요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045906.1.21",
          "form": "맞아요 ㅋㅋㅋㅋ 요리학원도 다녀보고싶네요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 요리학원도 다녀보고싶네요",
