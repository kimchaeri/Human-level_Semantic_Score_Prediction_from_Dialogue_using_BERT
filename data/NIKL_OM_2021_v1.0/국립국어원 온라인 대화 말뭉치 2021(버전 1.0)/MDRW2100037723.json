      "utterance": [
        {
          "id": "MDRW2100037723.1.1",
          "form": "삼겹살 드세여~~^^",
          "original_form": "삼겹살 드세여~~^^",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037723.1.2",
          "form": "앜ㅋㅋ삼겹살먹고싶다",
          "original_form": "앜ㅋㅋ삼겹살먹고싶다",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037723.1.3",
          "form": "침샘이 돋네요",
          "original_form": "침샘이 돋네요",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037723.1.4",
          "form": "언제먹어도 맛난ㅎ",
          "original_form": "언제먹어도 맛난ㅎ",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037723.1.5",
          "form": "빨리 먹고싶어요",
          "original_form": "빨리 먹고싶어요",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037723.1.6",
          "form": "배고프다ㅜㅜ배꼽시계가 요동을치네요",
          "original_form": "배고프다ㅜㅜ배꼽시계가 요동을치네요",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100037723.1.7",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037723.1.8",
          "form": "저녁은 고기류먹었어요",
          "original_form": "저녁은 고기류먹었어요",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100037723.1.9",
          "form": "대신아침은 못먹었네요",
          "original_form": "대신아침은 못먹었네요",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100037723.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100037723.1.11",
          "form": "전 식빵먹었어여ㅎ",
          "original_form": "전 식빵먹었어여ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100037723.1.12",
          "form": "이제 저 점심먹어요",
          "original_form": "이제 저 점심먹어요",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100037723.1.13",
          "form": "되게 배고프네요",
          "original_form": "되게 배고프네요",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100037723.1.14",
          "form": "우왕 맛나신가요?",
          "original_form": "우왕 맛나신가요?",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100037723.1.15",
          "form": "반찬은 그냥 무난했고 바나나가나와서 만족해요",
          "original_form": "반찬은 그냥 무난했고 바나나가나와서 만족해요",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100037723.1.16",
          "form": "바나나 좋아요",
          "original_form": "바나나 좋아요",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100037723.1.17",
          "form": "바나나맛있죵",
          "original_form": "바나나맛있죵",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100037723.1.18",
          "form": "배불러서",
          "original_form": "배불러서",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100037723.1.19",
          "form": "잠와요...큰일이네요",
          "original_form": "잠와요...큰일이네요",
