      "utterance": [
        {
          "id": "MDRW2100018987.1.1",
          "form": "나 키보드",
          "original_form": "나 키보드",
          "speaker_id": "2",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100018987.1.2",
          "form": "무조건 키보드 무접점 35g",
          "original_form": "무조건 키보드 무접점 35g",
          "speaker_id": "2",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100018987.1.3",
          "form": "키보드?",
          "original_form": "키보드?",
          "speaker_id": "1",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100018987.1.4",
          "form": "데스크탑용?",
          "original_form": "데스크탑용?",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.5",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.6",
          "form": "텐키리스 블루투스 버전으로 살거임 이때 아니면 못사 비싸서...",
          "original_form": "텐키리스 블루투스 버전으로 살거임 이때 아니면 못사 비싸서...",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.7",
          "form": "키보드고장남?",
          "original_form": "키보드고장남?",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.8",
          "form": "너는???",
          "original_form": "너는???",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.9",
          "form": "아 ㄴㄴㄴㄴ",
          "original_form": "아 ㄴㄴㄴㄴ",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.10",
          "form": "손가락이 고장남",
          "original_form": "손가락이 고장남",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.11",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.12",
          "form": "블루투스",
          "original_form": "블루투스",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018987.1.13",
          "form": "키보드괜찮대?",
          "original_form": "키보드괜찮대?",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.14",
          "form": "안끊긴대?",
          "original_form": "안끊긴대?",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.15",
          "form": "내가 지금 로지텍 380 쓰거 있는데",
          "original_form": "내가 지금 로지텍 380 쓰거 있는데",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.16",
          "form": "괜찮음",
          "original_form": "괜찮음",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.17",
          "form": "나는그냥옷이나기숙사갈때필요한거",
          "original_form": "나는그냥옷이나기숙사갈때필요한거",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.18",
          "form": "로지텍좋은거아님?",
          "original_form": "로지텍좋은거아님?",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.19",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.20",
          "form": "엥 젛긴 람",
          "original_form": "엥 젛긴 람",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.21",
          "form": "아 기숙사 가니까",
          "original_form": "아 기숙사 가니까",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018987.1.22",
          "form": "뭔가로지텍들어본적있어서",
          "original_form": "뭔가로지텍들어본적있어서",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018987.1.23",
          "form": "좋은건줄",
          "original_form": "좋은건줄",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018987.1.24",
          "form": "난키보드잘몰라서",
          "original_form": "난키보드잘몰라서",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018987.1.25",
          "form": "나도 손가락 아파서 찾기 전까진 몰랐음",
          "original_form": "나도 손가락 아파서 찾기 전까진 몰랐음",
          "speaker_id": "2",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018987.1.26",
          "form": "그냥 그럭저럳 쓸만함",
          "original_form": "그냥 그럭저럳 쓸만함",
          "speaker_id": "2",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018987.1.27",
          "form": "ㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018987.1.28",
          "form": "너는키버드많이쓰는구나",
          "original_form": "너는키버드많이쓰는구나",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018987.1.29",
          "form": "그걸생각못햇네",
          "original_form": "그걸생각못햇네",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018987.1.30",
          "form": "ㅇㅇㅇㅇㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅇㅇㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018987.1.31",
          "form": "나도 그걸 샹각못함",
          "original_form": "나도 그걸 샹각못함",
          "speaker_id": "2",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018987.1.32",
          "form": "야 근데 너 기숙사 들어가면 챙길게 많겠네",
          "original_form": "야 근데 너 기숙사 들어가면 챙길게 많겠네",
          "speaker_id": "2",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018987.1.33",
          "form": "그럴거같음",
          "original_form": "그럴거같음",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018987.1.34",
          "form": "침대시트이런거때문에",
          "original_form": "침대시트이런거때문에",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018987.1.35",
          "form": "아 침댜시트",
          "original_form": "아 침댜시트",
          "speaker_id": "2",
          "time": "20210827 14:25"
        },
        {
          "id": "MDRW2100018987.1.36",
          "form": "ㅇㅇㅇ대면여부안바뀌면10월쯤들어갈듯",
          "original_form": "ㅇㅇㅇ대면여부안바뀌면10월쯤들어갈듯",
          "speaker_id": "1",
          "time": "20210827 14:25"
        },
        {
          "id": "MDRW2100018987.1.37",
          "form": "키보드는어떤게좋은거임?",
          "original_form": "키보드는어떤게좋은거임?",
          "speaker_id": "1",
          "time": "20210827 14:26"
        },
        {
          "id": "MDRW2100018987.1.38",
          "form": "잘눌리는거?",
          "original_form": "잘눌리는거?",
          "speaker_id": "1",
          "time": "20210827 14:26"
        }
      ]
