        {
          "id": "MDRW2100013561.1.11",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013561.1.12",
          "form": "어릴때 받았어요!!",
          "original_form": "어릴때 받았어요!!",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013561.1.13",
          "form": "우와 여쭤봐도 될까요?",
          "original_form": "우와 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013561.1.14",
          "form": "저희 할머니는 불교이시고 사촌은 기독교예욤 tmi",
          "original_form": "저희 할머니는 불교이시고 사촌은 기독교예욤 tmi",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013561.1.15",
          "form": "ㅋㅋ전 name2에요",
          "original_form": "ㅋㅋ전 &name2&에요",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013561.1.16",
          "form": "저희할머니도 불교이심..",
          "original_form": "저희할머니도 불교이심..",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013561.1.17",
          "form": "우왕 멋져용",
          "original_form": "우왕 멋져용",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013561.1.18",
          "form": "템플스테이 가고싶어요",
          "original_form": "템플스테이 가고싶어요",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013561.1.19",
          "form": "템플스테이 왜 가고싶으신가요..",
          "original_form": "템플스테이 왜 가고싶으신가요..",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013561.1.20",
          "form": "아침일찍 기상해야한다고 들었어요 ㅋㅋㅋ",
          "original_form": "아침일찍 기상해야한다고 들었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013561.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ 헉 그런가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 헉 그런가요",
