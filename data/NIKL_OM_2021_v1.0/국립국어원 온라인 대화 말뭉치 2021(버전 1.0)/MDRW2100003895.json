      "utterance": [
        {
          "id": "MDRW2100003895.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100003895.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100003895.1.3",
          "form": "길거리 버스킹하면 가끔 듣다가요",
          "original_form": "길거리 버스킹하면 가끔 듣다가요",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100003895.1.4",
          "form": "오ㅜ버스킹 좋죠",
          "original_form": "오ㅜ버스킹 좋죠",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100003895.1.5",
          "form": "전 버스킹 본적이 없는데 부럽네요",
          "original_form": "전 버스킹 본적이 없는데 부럽네요",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100003895.1.6",
          "form": "길거리에서 자주 하더라구요ㅎㅎ",
          "original_form": "길거리에서 자주 하더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100003895.1.7",
          "form": "재프리님은 공연 좋아하시나요",
          "original_form": "재프리님은 공연 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100003895.1.8",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100003895.1.9",
          "form": "코로나 때문에 못 보는게 아쉽지만요",
          "original_form": "코로나 때문에 못 보는게 아쉽지만요",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100003895.1.10",
          "form": "어떤 종류의 공연을  좋아하시나요",
          "original_form": "어떤 종류의 공연을  좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100003895.1.11",
          "form": "전 콘서트 좋아하고",
          "original_form": "전 콘서트 좋아하고",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100003895.1.12",
          "form": "뮤지컬을 한번도 안가봐샤",
          "original_form": "뮤지컬을 한번도 안가봐샤",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100003895.1.13",
          "form": "너무 가보고싶어요",
          "original_form": "너무 가보고싶어요",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100003895.1.14",
          "form": "콘서트너무 좋죠",
          "original_form": "콘서트너무 좋죠",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100003895.1.15",
          "form": "어떤가수의 콘서트를 가보셨나요",
          "original_form": "어떤가수의 콘서트를 가보셨나요",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100003895.1.16",
          "form": "아이돌이요ㅋㅋ 가보셨어요?",
          "original_form": "아이돌이요ㅋㅋ 가보셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100003895.1.17",
          "form": "저는 콘서트 티케팅이 힘들어서ㅋ기",
          "original_form": "저는 콘서트 티케팅이 힘들어서ㅋ기",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100003895.1.18",
          "form": "매번실패했어요",
          "original_form": "매번실패했어요",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100003895.1.19",
          "form": "ㅋㅋ 아 힘들죠",
          "original_form": "ㅋㅋ 아 힘들죠",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100003895.1.20",
          "form": "누구 도전하셨나여",
          "original_form": "누구 도전하셨나여",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100003895.1.21",
          "form": "저 트와이스 공연보고싶었는데 어버버 하는순간 끝났어요",
          "original_form": "저 트와이스 공연보고싶었는데 어버버 하는순간 끝났어요",
          "speaker_id": "1",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100003895.1.22",
          "form": "핫..",
          "original_form": "핫..",
