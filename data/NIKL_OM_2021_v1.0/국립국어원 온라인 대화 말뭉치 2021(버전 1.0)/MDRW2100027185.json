      "utterance": [
        {
          "id": "MDRW2100027185.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027185.1.2",
          "form": "안녕요",
          "original_form": "안녕요",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100027185.1.3",
          "form": "집안일 하는 거 좋아하시나요~",
          "original_form": "집안일 하는 거 좋아하시나요~",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100027185.1.4",
          "form": "아뇨 ㅎ",
          "original_form": "아뇨 ㅎ",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100027185.1.5",
          "form": "가사일 하시나요?",
          "original_form": "가사일 하시나요?",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100027185.1.6",
          "form": "ㅋㅋㅋ안좋아하지만 해요...",
          "original_form": "ㅋㅋㅋ안좋아하지만 해요...",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100027185.1.7",
          "form": "가족들끼리 좀 나눠서 하고 있어요",
          "original_form": "가족들끼리 좀 나눠서 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100027185.1.8",
          "form": "맞아요 나눠야편하더라고요",
          "original_form": "맞아요 나눠야편하더라고요",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100027185.1.9",
          "form": "전 청소담당인데 제대로 못해서 맨날 혼나요",
          "original_form": "전 청소담당인데 제대로 못해서 맨날 혼나요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100027185.1.10",
          "form": "매일해야하나요? ㅎ",
          "original_form": "매일해야하나요? ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100027185.1.11",
          "form": "아뇨 청소기는 일주일에 한번해욬ㅋㅋㅋㅋ",
          "original_form": "아뇨 청소기는 일주일에 한번해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100027185.1.12",
          "form": "짜잘하게 정리하는건 수시로 하구요!",
          "original_form": "짜잘하게 정리하는건 수시로 하구요!",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100027185.1.13",
          "form": "저희도요 ㅎ 매일은 힘들더라고요",
          "original_form": "저희도요 ㅎ 매일은 힘들더라고요",
          "speaker_id": "2",
          "time": "20210903 09:21"
        },
        {
          "id": "MDRW2100027185.1.14",
          "form": "저는 이번에 좀 더 편하게 하려고 로봇청소기 살까 고민 중이에요",
          "original_form": "저는 이번에 좀 더 편하게 하려고 로봇청소기 살까 고민 중이에요",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100027185.1.15",
          "form": "바닥에 아무것도없어야한다는데 그게 더일이라서 못하겠더라고요 ㅎ",
          "original_form": "바닥에 아무것도없어야한다는데 그게 더일이라서 못하겠더라고요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100027185.1.16",
          "form": "뭔가 광고같은데;; 요번에 나온거는 센서 달려서 피해다닌다길래",
          "original_form": "뭔가 광고같은데;; 요번에 나온거는 센서 달려서 피해다닌다길래",
          "speaker_id": "1",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100027185.1.17",
          "form": "고민중이에요 ㅋㅋ",
          "original_form": "고민중이에요 ㅋㅋ",
