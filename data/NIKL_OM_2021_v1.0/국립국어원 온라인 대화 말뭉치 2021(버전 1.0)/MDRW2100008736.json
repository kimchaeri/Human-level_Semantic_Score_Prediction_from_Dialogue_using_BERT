      "utterance": [
        {
          "id": "MDRW2100008736.1.1",
          "form": "오늘 뭐 먹을 거야?",
          "original_form": "오늘 뭐 먹을 거야?",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008736.1.2",
          "form": "냉장고에 가지가 있으니까",
          "original_form": "냉장고에 가지가 있으니까",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008736.1.3",
          "form": "가지조림을 먹을까 생각중이야",
          "original_form": "가지조림을 먹을까 생각중이야",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008736.1.4",
          "form": "좋은 생각이야 나는 돼지김치찌개 먹으려구",
          "original_form": "좋은 생각이야 나는 돼지김치찌개 먹으려구",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008736.1.5",
          "form": "맛있겠다 돼지고기 김치찌개",
          "original_form": "맛있겠다 돼지고기 김치찌개",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008736.1.6",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008736.1.7",
          "form": "참치보단 고기지",
          "original_form": "참치보단 고기지",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008736.1.8",
          "form": "맞아 나 부대찌개도 좋아해",
          "original_form": "맞아 나 부대찌개도 좋아해",
          "speaker_id": "1",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008736.1.9",
          "form": "나도!",
          "original_form": "나도!",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008736.1.10",
          "form": "햄이랑 라면사리 가득 넣어서",
          "original_form": "햄이랑 라면사리 가득 넣어서",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008736.1.11",
          "form": "먹고싶다ㅋㅋ",
          "original_form": "먹고싶다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008736.1.12",
          "form": "나 부대찌개 맛집 알고 있거든",
          "original_form": "나 부대찌개 맛집 알고 있거든",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008736.1.13",
          "form": "두꺼비 부대찌개라고 체인점인지는 모르겠는데",
          "original_form": "두꺼비 부대찌개라고 체인점인지는 모르겠는데",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008736.1.14",
          "form": "진짜 맛있어",
          "original_form": "진짜 맛있어",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008736.1.15",
          "form": "너 주변에도 맛집 있어?",
          "original_form": "너 주변에도 맛집 있어?",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008736.1.16",
          "form": "주변은 아니고",
          "original_form": "주변은 아니고",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008736.1.17",
          "form": "수원에 이나경송탄부대찌개",
          "original_form": "수원에 이나경송탄부대찌개",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008736.1.18",
          "form": "여기도 정말 맛있어 꼭 가봐",
          "original_form": "여기도 정말 맛있어 꼭 가봐",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008736.1.19",
          "form": "좋아 수원 가면 꼭 가볼게",
          "original_form": "좋아 수원 가면 꼭 가볼게",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008736.1.20",
          "form": "내 주변에 맛있는 싱가포르 음식점이 있었거든? 만다린키친이라고 근데 셰프가 어디 간 건지.. 문을 닫았어",
          "original_form": "내 주변에 맛있는 싱가포르 음식점이 있었거든? 만다린키친이라고 근데 셰프가 어디 간 건지.. 문을 닫았어",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008736.1.21",
          "form": "여기 추천해주고 싶은데 나도 못 가는ㅠㅠ ㅋㅋㅋ",
          "original_form": "여기 추천해주고 싶은데 나도 못 가는ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008736.1.22",
          "form": "아쉽네ㅠㅠ",
          "original_form": "아쉽네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008736.1.23",
          "form": "요즘 코로나 때문에",
          "original_form": "요즘 코로나 때문에",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008736.1.24",
          "form": "내가 아는 맛집들도 문을 많이 닫았더라고",
          "original_form": "내가 아는 맛집들도 문을 많이 닫았더라고",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008736.1.25",
          "form": "맞아.. 좋아하는 라멘집도 있었는데 문 닫았어",
          "original_form": "맞아.. 좋아하는 라멘집도 있었는데 문 닫았어",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008736.1.26",
          "form": "배달 자주 시켜 먹는 편이야?",
          "original_form": "배달 자주 시켜 먹는 편이야?",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008736.1.27",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008736.1.28",
          "form": "떡볶이 치킨 진짜 자주 먹어ㅎㅎ",
          "original_form": "떡볶이 치킨 진짜 자주 먹어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008736.1.29",
          "form": "치킨.. 먹고 싶다 어제 뿌링클이 엄청 땡기더라고",
          "original_form": "치킨.. 먹고 싶다 어제 뿌링클이 엄청 땡기더라고",
          "speaker_id": "1",
          "time": "20210822 18:09"
        }
