      "utterance": [
        {
          "id": "MDRW2100014645.1.1",
          "form": "여행ㅠㅠㅠㅠ",
          "original_form": "여행ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014645.1.2",
          "form": "너무 가고 싶어요",
          "original_form": "너무 가고 싶어요",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014645.1.3",
          "form": "해외 많이.가보셧나여?",
          "original_form": "해외 많이.가보셧나여?",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014645.1.4",
          "form": "많이는 아니고 몇 번 가 봤어요!",
          "original_form": "많이는 아니고 몇 번 가 봤어요!",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014645.1.5",
          "form": "언제 또 갈 수 있을까요 ㅠㅠㅠㅠ",
          "original_form": "언제 또 갈 수 있을까요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014645.1.6",
          "form": "오.. 어디어디요? ㅋㅋㅋ 곧 코로나 끝나겠죠.",
          "original_form": "오.. 어디어디요? ㅋㅋㅋ 곧 코로나 끝나겠죠.",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014645.1.7",
          "form": "미국, 호주, 캐나다, 스페인, 프랑스, 필리핀, 일본, 중국에 가 봤네요!",
          "original_form": "미국, 호주, 캐나다, 스페인, 프랑스, 필리핀, 일본, 중국에 가 봤네요!",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014645.1.8",
          "form": "엄청 다니셧네요..",
          "original_form": "엄청 다니셧네요..",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014645.1.9",
          "form": "여행가이드세요? ㅋㅋㅋㅋㅋ",
          "original_form": "여행가이드세요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014645.1.10",
          "form": "아니요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014645.1.11",
          "form": "그냥 어쩌다보니 휴가때마다 나갔네요",
          "original_form": "그냥 어쩌다보니 휴가때마다 나갔네요",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014645.1.12",
          "form": "캐나다는 보통 잘  안가는데.. 오디가셨어요?",
          "original_form": "캐나다는 보통 잘  안가는데.. 오디가셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014645.1.13",
          "form": "TMI지만 캐나다는 남자친구 부모님 뵈러 갔었어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "TMI지만 캐나다는 남자친구 부모님 뵈러 갔었어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014645.1.14",
          "form": "밴쿠버요",
          "original_form": "밴쿠버요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014645.1.15",
          "form": "오... 너무 좋았겟다.. \r\n오래 머무셧어요?",
          "original_form": "오... 너무 좋았겟다.. \r\n오래 머무셧어요?",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014645.1.16",
          "form": "오래는 아니고 한 달 조금 넘게요!",
          "original_form": "오래는 아니고 한 달 조금 넘게요!",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014645.1.17",
          "form": "한달이면 엄청길죠.. 부럽다.. \r\n벤쿠버 날씨 짱 좋죠?",
          "original_form": "한달이면 엄청길죠.. 부럽다.. \r\n벤쿠버 날씨 짱 좋죠?",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014645.1.18",
          "form": "제가 갔을 때 진짜 좋긴했어요!",
          "original_form": "제가 갔을 때 진짜 좋긴했어요!",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014645.1.19",
          "form": "가을날씨!",
          "original_form": "가을날씨!",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014645.1.20",
          "form": "가을 벤쿠버면.. 천국이엇겟네요.. \r\n하늘 엄청 쨍하잖아요",
          "original_form": "가을 벤쿠버면.. 천국이엇겟네요.. \r\n하늘 엄청 쨍하잖아요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014645.1.21",
          "form": "네! 그림같이 예뻤어요",
          "original_form": "네! 그림같이 예뻤어요",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014645.1.22",
          "form": "다 좋고 다 예쁘고!",
          "original_form": "다 좋고 다 예쁘고!",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014645.1.23",
          "form": "또 가고 싶어요ㅋㅋㅋ",
          "original_form": "또 가고 싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014645.1.24",
          "form": "여행 어디가 제일 좋으셨어요?",
          "original_form": "여행 어디가 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:29"
        }
      ]
    }
