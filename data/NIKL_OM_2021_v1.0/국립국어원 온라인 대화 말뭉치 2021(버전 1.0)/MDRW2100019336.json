      "utterance": [
        {
          "id": "MDRW2100019336.1.1",
          "form": "여행 가는거 좋아하세요?",
          "original_form": "여행 가는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019336.1.2",
          "form": "저는 집순이라 별로 못 가봤어요 다른 분들은 많이 가보셨더라구여",
          "original_form": "저는 집순이라 별로 못 가봤어요 다른 분들은 많이 가보셨더라구여",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019336.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019336.1.4",
          "form": "오 저도 집순이에요 ㅋㅋ !",
          "original_form": "오 저도 집순이에요 ㅋㅋ !",
          "speaker_id": "1",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019336.1.5",
          "form": "근데 여행가는거 좋아해요 ㅎㅎ",
          "original_form": "근데 여행가는거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019336.1.6",
          "form": "동지네요 ㅋㅋㅋ",
          "original_form": "동지네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019336.1.7",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019336.1.8",
          "form": "전 이시국이지만 일본이 젤 좋았어요 ㅎㅎ",
          "original_form": "전 이시국이지만 일본이 젤 좋았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019336.1.9",
          "form": "다들 일본을 제일 좋아하더라구요 가깝기도 하고 ㅋㅋㅋ",
          "original_form": "다들 일본을 제일 좋아하더라구요 가깝기도 하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019336.1.10",
          "form": "다신 가지 않겠지만 ㅋㅋ 음식이 너무 맛있었어요",
          "original_form": "다신 가지 않겠지만 ㅋㅋ 음식이 너무 맛있었어요",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019336.1.11",
          "form": "재밌을거같아요",
          "original_form": "재밌을거같아요",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019336.1.12",
          "form": "ㅋㅋㅋㅋ 뭐가 제일 맛있나요?",
          "original_form": "ㅋㅋㅋㅋ 뭐가 제일 맛있나요?",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019336.1.13",
          "form": "돈까스가 진짜 맛있었어요 ㅋㅋ !",
          "original_form": "돈까스가 진짜 맛있었어요 ㅋㅋ !",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019336.1.14",
          "form": "초밥도 맛있었구요 ㅎㅎ",
          "original_form": "초밥도 맛있었구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019336.1.15",
          "form": "일본초밥은 어떻게 다른지 궁금해요",
          "original_form": "일본초밥은 어떻게 다른지 궁금해요",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019336.1.16",
          "form": "일식은 다 너무 맛나서 ㅠㅜ",
          "original_form": "일식은 다 너무 맛나서 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019336.1.17",
          "form": "약간 그 초밥 밥이 더 맛있어요 ! 식감이 다른 느낌 ㅠㅠ",
          "original_form": "약간 그 초밥 밥이 더 맛있어요 ! 식감이 다른 느낌 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019336.1.18",
          "form": "그래요? 갑자기 초밥 땡기네요 회덮밥도 ㅠㅠ",
          "original_form": "그래요? 갑자기 초밥 땡기네요 회덮밥도 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019336.1.19",
          "form": "저두요 ㅠㅠ 아 해산물이 엄청 땡기네요 ㅋㅋ",
          "original_form": "저두요 ㅠㅠ 아 해산물이 엄청 땡기네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019336.1.20",
          "form": "속초도 한번 가보고싶어요 물회먹으러 ㅎㅎ",
          "original_form": "속초도 한번 가보고싶어요 물회먹으러 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:50"
        }
      ]
