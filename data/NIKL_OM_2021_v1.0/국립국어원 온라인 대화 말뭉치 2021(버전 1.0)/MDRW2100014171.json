      "utterance": [
        {
          "id": "MDRW2100014171.1.1",
          "form": "컴퓨터가 왜 느린데",
          "original_form": "컴퓨터가 왜 느린데",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014171.1.2",
          "form": "산지 넘 오래되어서 사양이 안좋은듯",
          "original_form": "산지 넘 오래되어서 사양이 안좋은듯",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014171.1.3",
          "form": "얼마 전에 샀잖아",
          "original_form": "얼마 전에 샀잖아",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014171.1.4",
          "form": "아니다 10년은 되갈껄",
          "original_form": "아니다 10년은 되갈껄",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014171.1.5",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014171.1.6",
          "form": "대학생때 샀으니~ 그정도 되었당",
          "original_form": "대학생때 샀으니~ 그정도 되었당",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.7",
          "form": "새로 사려면 비싸잖아",
          "original_form": "새로 사려면 비싸잖아",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.8",
          "form": "그렇지 ㅠㅠ 돈 아깝다",
          "original_form": "그렇지 ㅠㅠ 돈 아깝다",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.9",
          "form": "그래도 필요하면 사야지",
          "original_form": "그래도 필요하면 사야지",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.10",
          "form": "옹 ㅠㅠ 좀 알아봐야겠다",
          "original_form": "옹 ㅠㅠ 좀 알아봐야겠다",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.11",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.12",
          "form": "옹..이대로 쓰다간 갑자기 고장날듯",
          "original_form": "옹..이대로 쓰다간 갑자기 고장날듯",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.13",
          "form": "그럼 안 되지",
          "original_form": "그럼 안 되지",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014171.1.14",
