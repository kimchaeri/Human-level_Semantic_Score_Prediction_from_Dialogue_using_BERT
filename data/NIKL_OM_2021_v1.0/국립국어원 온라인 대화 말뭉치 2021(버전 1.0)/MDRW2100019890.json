      "utterance": [
        {
          "id": "MDRW2100019890.1.1",
          "form": "name2님은 어떤 음식을 좋아하세요?",
          "original_form": "&name2&님은 어떤 음식을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019890.1.2",
          "form": "저는 웬만하면 정말 다 잘 먹습니다!",
          "original_form": "저는 웬만하면 정말 다 잘 먹습니다!",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019890.1.3",
          "form": "저는 가리는 거 빼고 다 잘먹어요ㅋㅋㅋㅋ",
          "original_form": "저는 가리는 거 빼고 다 잘먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019890.1.4",
          "form": "가리는건 비린걸 잘 못먹아요",
          "original_form": "가리는건 비린걸 잘 못먹아요",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019890.1.5",
          "form": "오오 생각해보니 저도",
          "original_form": "오오 생각해보니 저도",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019890.1.6",
          "form": "굴을 못먹네요",
          "original_form": "굴을 못먹네요",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019890.1.7",
          "form": "생선도... 요리해놓은지 시간이 지나면 손이 안가더라구요",
          "original_form": "생선도... 요리해놓은지 시간이 지나면 손이 안가더라구요",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019890.1.8",
          "form": "그럼 혹시 분식 좋아하시나요?",
          "original_form": "그럼 혹시 분식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019890.1.9",
          "form": "맞아요!!!!!그런거 비려요ㅠㅠ",
          "original_form": "맞아요!!!!!그런거 비려요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019890.1.10",
          "form": "분식 당연히 좋아합니다",
          "original_form": "분식 당연히 좋아합니다",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019890.1.11",
          "form": "저 어제 저녁에도 친구랑 만나서",
          "original_form": "저 어제 저녁에도 친구랑 만나서",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.12",
          "form": "떡볶이 + 튀김 + 순대 조합으로 먹고 왔는데",
          "original_form": "떡볶이 + 튀김 + 순대 조합으로 먹고 왔는데",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.13",
          "form": "가끔 점심을 떡볶이로 먹을정도로요ㅋㅋㅋㅋ",
          "original_form": "가끔 점심을 떡볶이로 먹을정도로요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.14",
          "form": "진짜진짜 맛있더라구요...",
          "original_form": "진짜진짜 맛있더라구요...",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.15",
          "form": "호오",
          "original_form": "호오",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.16",
          "form": "아 저도요~~~!",
          "original_form": "아 저도요~~~!",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.17",
          "form": "밀떡파이신가요... 쌀떡파이신가요?!",
          "original_form": "밀떡파이신가요... 쌀떡파이신가요?!",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.18",
          "form": "사실 저는 가리지 않습니다만 ㅎㅎㅎ",
          "original_form": "사실 저는 가리지 않습니다만 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.19",
          "form": "진리의 김떡순이예요",
          "original_form": "진리의 김떡순이예요",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019890.1.20",
          "form": "오오 김밥 떡볶이 순대 조합인가요?!",
          "original_form": "오오 김밥 떡볶이 순대 조합인가요?!",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019890.1.21",
          "form": "안가려요",
          "original_form": "안가려요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019890.1.22",
          "form": "아니요 튀김 떡볶이 순대인데 제가 학생때는 저렇게 불렀어요",
          "original_form": "아니요 튀김 떡볶이 순대인데 제가 학생때는 저렇게 불렀어요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019890.1.23",
          "form": "김밥은 꼬마김밥이라",
          "original_form": "김밥은 꼬마김밥이라",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019890.1.24",
          "form": "아 진짜요!!!!",
          "original_form": "아 진짜요!!!!",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019890.1.25",
          "form": "아 생각해보니 김이 가게 마다 달랐어요 !!!!!!! 김치전이나 계란김밥이 있는 가게도 있었네요",
          "original_form": "아 생각해보니 김이 가게 마다 달랐어요 !!!!!!! 김치전이나 계란김밥이 있는 가게도 있었네요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019890.1.26",
          "form": "우와 김치전이요?!?! 분식집에서 김치전이라니",
          "original_form": "우와 김치전이요?!?! 분식집에서 김치전이라니",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019890.1.27",
          "form": "정말 보기 드문 조합이에요!!!!",
          "original_form": "정말 보기 드문 조합이에요!!!!",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019890.1.28",
          "form": "혹시... 바나나튀김 들어보셨나요?",
          "original_form": "혹시... 바나나튀김 들어보셨나요?",
          "speaker_id": "1",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019890.1.29",
          "form": "아니요... 바나나를 튀긴건가요?",
          "original_form": "아니요... 바나나를 튀긴건가요?",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100019890.1.30",
          "form": "네... 바나나를 튀긴건데...",
          "original_form": "네... 바나나를 튀긴건데...",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019890.1.31",
          "form": "(숙연)",
          "original_form": "(숙연)",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019890.1.32",
          "form": "저희 학교 앞에 바나나튀김 파는 분식집이 있는데",
          "original_form": "저희 학교 앞에 바나나튀김 파는 분식집이 있는데",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019890.1.33",
          "form": "좋아하는 사람들은 진짜 좋아하더라구요..",
          "original_form": "좋아하는 사람들은 진짜 좋아하더라구요..",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019890.1.34",
          "form": "저는 있으면 먹어요!!!",
          "original_form": "저는 있으면 먹어요!!!",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019890.1.35",
          "form": "우오",
          "original_form": "우오",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019890.1.36",
          "form": "바나나 원래 불에 구워먹는게 맛있다고 하긴 하던데",
          "original_form": "바나나 원래 불에 구워먹는게 맛있다고 하긴 하던데",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019890.1.37",
          "form": "저도 있으면 먹어보고 싶어요!!!",
          "original_form": "저도 있으면 먹어보고 싶어요!!!",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019890.1.38",
          "form": "불에 구워먹는겈ㅋㅋㅋ 앜ㅋㅋㅋ",
          "original_form": "불에 구워먹는겈ㅋㅋㅋ 앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019890.1.39",
          "form": "제가 분식집 맛있는 곳 하나 소개해드릴게요",
          "original_form": "제가 분식집 맛있는 곳 하나 소개해드릴게요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019890.1.40",
          "form": "이대 근처에 <삭> 이라는 곳인데요",
          "original_form": "이대 근처에 <삭> 이라는 곳인데요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019890.1.41",
          "form": "진짜 맛집이에요 ",
          "original_form": "진짜 맛집이에요 ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019890.1.42",
          "form": "인천지역 분식집에서는 계란초라고 삶은계란에 오이채랑 초장 올려주는데 맛있어요",
          "original_form": "인천지역 분식집에서는 계란초라고 삶은계란에 오이채랑 초장 올려주는데 맛있어요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019890.1.43",
          "form": "오 감사합니다 다음에 한번가봐야겠네요~",
          "original_form": "오 감사합니다 다음에 한번가봐야겠네요~",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019890.1.44",
          "form": "우와 건강에 되게 좋을 것 같아요!!! 계란이랑 오이라니!!!",
          "original_form": "우와 건강에 되게 좋을 것 같아요!!! 계란이랑 오이라니!!!",
          "speaker_id": "1",
          "time": "20210827 23:09"
        }
      ]
