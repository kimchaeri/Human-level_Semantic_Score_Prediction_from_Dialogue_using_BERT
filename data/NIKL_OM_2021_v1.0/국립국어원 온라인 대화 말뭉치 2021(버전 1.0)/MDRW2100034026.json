      "utterance": [
        {
          "id": "MDRW2100034026.1.1",
          "form": "점심드셨나요?ㅋ",
          "original_form": "점심드셨나요?ㅋ",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100034026.1.2",
          "form": "네~ 오늘 육개장국밥먹었어요ㅎㅎ",
          "original_form": "네~ 오늘 육개장국밥먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100034026.1.3",
          "form": "맛있었겠어요ㅋㅋ 얼큰하게",
          "original_form": "맛있었겠어요ㅋㅋ 얼큰하게",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100034026.1.4",
          "form": "맞아요! 국밥좋아하세요?",
          "original_form": "맞아요! 국밥좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100034026.1.5",
          "form": "네 돼지도 좋고 순대도 좋고 ㅋㅋ",
          "original_form": "네 돼지도 좋고 순대도 좋고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100034026.1.6",
          "form": "저두 돼지국밥 좋아해요",
          "original_form": "저두 돼지국밥 좋아해요",
          "speaker_id": "1",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100034026.1.7",
          "form": "순대는 피순대말고ㅋㅋㄱ찹쌀로",
          "original_form": "순대는 피순대말고ㅋㅋㄱ찹쌀로",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100034026.1.8",
          "form": "오 먹잘알이시군요!",
          "original_form": "오 먹잘알이시군요!",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034026.1.9",
          "form": "피순대 드시나여?ㅋ",
          "original_form": "피순대 드시나여?ㅋ",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100034026.1.10",
          "form": "아요 ㅋㅋ 전사실 순대안좋아해요..ㅎㅎ",
          "original_form": "아요 ㅋㅋ 전사실 순대안좋아해요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034026.1.11",
          "form": "아 ㅎㅎ 그러시구나 ㅋㅋㅋ 순대 안드시는분 꽤있더라구요",
          "original_form": "아 ㅎㅎ 그러시구나 ㅋㅋㅋ 순대 안드시는분 꽤있더라구요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100034026.1.12",
          "form": "맞아요 그게 접니다 ㅜ ㅋㅋㅋ",
          "original_form": "맞아요 그게 접니다 ㅜ ㅋㅋㅋ",
