          "id": "MDRW2100021013.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100021013.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021013.1.3",
          "form": "자주 하시나봐요 저두요ㅎ",
          "original_form": "자주 하시나봐요 저두요ㅎ",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021013.1.4",
          "form": "옷 신발좋아해요",
          "original_form": "옷 신발좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021013.1.5",
          "form": "전 먹는거ㅋ",
          "original_form": "전 먹는거ㅋ",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021013.1.6",
          "form": "오 쿠팡쓰시나요",
          "original_form": "오 쿠팡쓰시나요",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021013.1.7",
          "form": "그냥 쓱배송이여",
          "original_form": "그냥 쓱배송이여",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021013.1.8",
          "form": "아그거 이마트배송맞나요",
          "original_form": "아그거 이마트배송맞나요",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021013.1.9",
          "form": "네 이마트여",
          "original_form": "네 이마트여",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021013.1.10",
          "form": "신세계포인트 쏠쏠하시겠네여",
          "original_form": "신세계포인트 쏠쏠하시겠네여",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021013.1.11",
          "form": "네 그래서 쓱배송만 시키게되네요",
          "original_form": "네 그래서 쓱배송만 시키게되네요",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021013.1.12",
