      "utterance": [
        {
          "id": "MDRW2100043166.1.1",
          "form": "요즘 사고싶은거 있으세요?",
          "original_form": "요즘 사고싶은거 있으세요?",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043166.1.2",
          "form": "요즘 온수매트요?",
          "original_form": "요즘 온수매트요?",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043166.1.3",
          "form": "날씨가 추워지니 살까말까 망설이는중이에요",
          "original_form": "날씨가 추워지니 살까말까 망설이는중이에요",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043166.1.4",
          "form": "저희집에서 저빼고 언니랑 엄빠가 온수매트 쓰는데",
          "original_form": "저희집에서 저빼고 언니랑 엄빠가 온수매트 쓰는데",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043166.1.5",
          "form": "와 ...",
          "original_form": "와 ...",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043166.1.6",
          "form": "좋아요..네..",
          "original_form": "좋아요..네..",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043166.1.7",
          "form": "사세요..",
          "original_form": "사세요..",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043166.1.8",
          "form": "ㅋㅋㅋ 어디꺼가 좋은가요?",
          "original_form": "ㅋㅋㅋ 어디꺼가 좋은가요?",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043166.1.9",
          "form": "ㅋㅋㅋㅋㅋ아...우리집 뭐드라..",
          "original_form": "ㅋㅋㅋㅋㅋ아...우리집 뭐드라..",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043166.1.10",
          "form": "약간 그 보일러회사에서",
          "original_form": "약간 그 보일러회사에서",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043166.1.11",
          "form": "파는거같던데",
          "original_form": "파는거같던데",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043166.1.12",
          "form": "나비엔?",
          "original_form": "나비엔?",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043166.1.13",
          "form": "약간 고런 너낌..",
          "original_form": "약간 고런 너낌..",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043166.1.14",
          "form": "거기서도 온수매트 광고낸거 본거같은디",
          "original_form": "거기서도 온수매트 광고낸거 본거같은디",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043166.1.15",
          "form": "경동나비엔 온수매트 ... 유지태가 선전하는걸",
          "original_form": "경동나비엔 온수매트 ... 유지태가 선전하는걸",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043166.1.16",
          "form": "본것같은 느낌적인느낌?",
          "original_form": "본것같은 느낌적인느낌?",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043166.1.17",
          "form": "ㅋㅋ 참고할게요",
          "original_form": "ㅋㅋ 참고할게요",
          "speaker_id": "1",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043166.1.18",
          "form": "혹시 지그재그 아이디 있으신가요 지금 선착순 이천포 주네요",
          "original_form": "혹시 지그재그 아이디 있으신가요 지금 선착순 이천포 주네요",
          "speaker_id": "1",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043166.1.19",
          "form": "아뇨 저는 안쓰네요",
          "original_form": "아뇨 저는 안쓰네요",
          "speaker_id": "2",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043166.1.20",
          "form": "ㅜㅜ아깝네요 양말사면 딱 인데",
          "original_form": "ㅜㅜ아깝네요 양말사면 딱 인데",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043166.1.21",
          "form": "가입하면 이천을 준대요?",
          "original_form": "가입하면 이천을 준대요?",
          "speaker_id": "2",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043166.1.22",
          "form": "ㄴㄴ 가입하지마세요",
          "original_form": "ㄴㄴ 가입하지마세요",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043166.1.23",
          "form": "원래 아이디 있음 이천포주는거예요",
          "original_form": "원래 아이디 있음 이천포주는거예요",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043166.1.24",
          "form": "자주 이벤트해요 ㅋㅋ",
          "original_form": "자주 이벤트해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043166.1.25",
          "form": "아 포인트 뿌리는 그런건가봐요",
          "original_form": "아 포인트 뿌리는 그런건가봐요",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043166.1.26",
          "form": "쩝쓰 먼가 아깝넹",
          "original_form": "쩝쓰 먼가 아깝넹",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043166.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043166.1.28",
          "form": "아 포인트 뿌리는건가봐요 가끔",
          "original_form": "아 포인트 뿌리는건가봐요 가끔",
          "speaker_id": "2",
          "time": "20210907 15:35"
        }
      ]
    }
