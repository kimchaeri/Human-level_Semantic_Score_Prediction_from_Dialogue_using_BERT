      "utterance": [
        {
          "id": "MDRW2100024113.1.1",
          "form": "무슨 국 좋아하세요?ㅋㅋ",
          "original_form": "무슨 국 좋아하세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024113.1.2",
          "form": "저는.. 찐한 찌개 좋아해요ㅎ",
          "original_form": "저는.. 찐한 찌개 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024113.1.3",
          "form": "특히 된찌.",
          "original_form": "특히 된찌.",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024113.1.4",
          "form": "님은 김치? 된장찌개?",
          "original_form": "님은 김치? 된장찌개?",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024113.1.5",
          "form": "된찌 맛있져 전 참치김치찌개 좋아해용 ㅋㅋㅋ",
          "original_form": "된찌 맛있져 전 참치김치찌개 좋아해용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024113.1.6",
          "form": "와 참치김치찌개는 놀러가 먹는 특식 느낌이에요",
          "original_form": "와 참치김치찌개는 놀러가 먹는 특식 느낌이에요",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024113.1.7",
          "form": "요리 잘하시나오?",
          "original_form": "요리 잘하시나오?",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024113.1.8",
          "form": "ㅋㅋㅋㅋ 요리보다 요즘 냉장고 파먹기 하고있어요 ㅠ 요리잘하시나요?",
          "original_form": "ㅋㅋㅋㅋ 요리보다 요즘 냉장고 파먹기 하고있어요 ㅠ 요리잘하시나요?",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024113.1.9",
          "form": "저에게 허락된 영역이 아니네요ㅜ",
          "original_form": "저에게 허락된 영역이 아니네요ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024113.1.10",
          "form": "아 진짜요?ㅋㅋㅋ 저도 어쩌다가만해요 요리라고 하기엔 뭐한수제비 이렁거 ㅋㅋ",
          "original_form": "아 진짜요?ㅋㅋㅋ 저도 어쩌다가만해요 요리라고 하기엔 뭐한수제비 이렁거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024113.1.11",
          "form": "아니 수제비를 하심서 ?",
          "original_form": "아니 수제비를 하심서 ?",
          "speaker_id": "1",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024113.1.12",
          "form": "이런 기만자!",
          "original_form": "이런 기만자!",
          "speaker_id": "1",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024113.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 김혜자선생님과 함께라면...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 김혜자선생님과 함께라면...",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024113.1.14",
          "form": "쉬워요 ㅋㅋㅋㅋㅋ",
          "original_form": "쉬워요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024113.1.15",
          "form": "아니ㅋㅋ 뭐죠ㅎ 이 능욕당하는 기분우",
          "original_form": "아니ㅋㅋ 뭐죠ㅎ 이 능욕당하는 기분우",
          "speaker_id": "1",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024113.1.16",
          "form": "가츠오부시 장국과 김혜자 선생님의 콜라보면 됩니다 아는맛이 무서워요 ㅋㅋㅋㅋ",
          "original_form": "가츠오부시 장국과 김혜자 선생님의 콜라보면 됩니다 아는맛이 무서워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024113.1.17",
          "form": "거기에 참기름과 깨소금 청양고추!!",
          "original_form": "거기에 참기름과 깨소금 청양고추!!",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024113.1.18",
          "form": "괜찮을까요!!",
          "original_form": "괜찮을까요!!",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024113.1.19",
          "form": "네 제입맛엔 !!!!",
          "original_form": "네 제입맛엔 !!!!",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024113.1.20",
          "form": "깊은맛은 기대하지마세요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "깊은맛은 기대하지마세요 ㅋㅋㅋㅋㅋㅋㅋ",
