      "utterance": [
        {
          "id": "MDRW2100009355.1.1",
          "form": "여행 많이다니세요?",
          "original_form": "여행 많이다니세요?",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009355.1.2",
          "form": "많이는 아닌데 예전에 혼자 여행가본적 있어요",
          "original_form": "많이는 아닌데 예전에 혼자 여행가본적 있어요",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009355.1.3",
          "form": "어 저도요 부산 혼자다녀왓어요",
          "original_form": "어 저도요 부산 혼자다녀왓어요",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009355.1.4",
          "form": "어디가셨어요?",
          "original_form": "어디가셨어요?",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009355.1.5",
          "form": "저는 포항 구룡포마을이요!",
          "original_form": "저는 포항 구룡포마을이요!",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009355.1.6",
          "form": "오 포항좋죠",
          "original_form": "오 포항좋죠",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009355.1.7",
          "form": "얘기하니깐 가고싶네요 ㅠㅠ",
          "original_form": "얘기하니깐 가고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009355.1.8",
          "form": "동백꽃필무렵 드라마 촬영진데 그거 촬영하기전에 다녀왔었어요",
          "original_form": "동백꽃필무렵 드라마 촬영진데 그거 촬영하기전에 다녀왔었어요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009355.1.9",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009355.1.10",
          "form": "저도 그전에 다녀왔는데",
          "original_form": "저도 그전에 다녀왔는데",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009355.1.11",
          "form": "그 드라마보니 또 가고싶더라구요",
          "original_form": "그 드라마보니 또 가고싶더라구요",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009355.1.12",
          "form": "조만간 계획 세워봐야겟어용",
          "original_form": "조만간 계획 세워봐야겟어용",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009355.1.13",
          "form": "어 진짜요? 또 가도 재밌을거 같아요",
          "original_form": "어 진짜요? 또 가도 재밌을거 같아요",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009355.1.14",
          "form": "혹시 해외는 어디가보셨어요?",
          "original_form": "혹시 해외는 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009355.1.15",
          "form": "저 학생때 일본가본거말고 없어요 ㅠㅠ 제작년에 블라디보스톡 예약다해놨는데",
          "original_form": "저 학생때 일본가본거말고 없어요 ㅠㅠ 제작년에 블라디보스톡 예약다해놨는데",
          "speaker_id": "2",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009355.1.16",
          "form": "코로나때문에 취소되구요 ㅠㅠ",
          "original_form": "코로나때문에 취소되구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009355.1.17",
          "form": "아아 아쉬우셨겠네요ㅠㅠ 저는 제주도 밖에 못가봐서 코로나 끝나면 유럽 가보고 싶어요",
          "original_form": "아아 아쉬우셨겠네요ㅠㅠ 저는 제주도 밖에 못가봐서 코로나 끝나면 유럽 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009355.1.18",
          "form": "저두용 .. 코로나 끝나자마자 해외갈거예여 ㅠ",
          "original_form": "저두용 .. 코로나 끝나자마자 해외갈거예여 ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009355.1.19",
          "form": "근데 끝나긴할까요 ㅠㅋㅋㅋㅋㅋ 슬프네여",
          "original_form": "근데 끝나긴할까요 ㅠㅋㅋㅋㅋㅋ 슬프네여",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009355.1.20",
          "form": "그러게요 돈만 모으고 있고ㅠ 끝나면 보복소비 해줄 수 있는데",
          "original_form": "그러게요 돈만 모으고 있고ㅠ 끝나면 보복소비 해줄 수 있는데",
          "speaker_id": "1",
          "time": "20210822 23:02"
        }
      ]
    }
