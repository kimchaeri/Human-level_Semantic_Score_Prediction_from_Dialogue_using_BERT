      "utterance": [
        {
          "id": "MDRW2100030993.1.1",
          "form": "점심 먹었어?",
          "original_form": "점심 먹었어?",
          "speaker_id": "1",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030993.1.2",
          "form": "아직",
          "original_form": "아직",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030993.1.3",
          "form": "지금 순대/떡볶이 사려고 기달중",
          "original_form": "지금 순대/떡볶이 사려고 기달중",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030993.1.4",
          "form": "타이밍 좋다해야할지\r\n마지막 떡볶이 만드시기 시작할 때 왔어",
          "original_form": "타이밍 좋다해야할지\r\n마지막 떡볶이 만드시기 시작할 때 왔어",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100030993.1.5",
          "form": "헐 맛있게따 난 떡볶이엔 무조건 튀김 찍어먹고싶엉",
          "original_form": "헐 맛있게따 난 떡볶이엔 무조건 튀김 찍어먹고싶엉",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100030993.1.6",
          "form": "이집 튀김 수제로 당일판매라 오전에 줄서 사가서 지금 품절 살수없었어ㅜㅜ",
          "original_form": "이집 튀김 수제로 당일판매라 오전에 줄서 사가서 지금 품절 살수없었어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030993.1.7",
          "form": "떡볶이 구매할수있어 다행ㅎ",
          "original_form": "떡볶이 구매할수있어 다행ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030993.1.8",
          "form": "순대찍어먹을려고 떡볶이 삼ㅋ",
          "original_form": "순대찍어먹을려고 떡볶이 삼ㅋ",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030993.1.9",
          "form": "쌈장이 없어ㅋㅋ",
          "original_form": "쌈장이 없어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030993.1.10",
          "form": "순대장이ㅋ",
          "original_form": "순대장이ㅋ",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030993.1.11",
          "form": "소금은 내 스탈 아니더라고ㅎ",
          "original_form": "소금은 내 스탈 아니더라고ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030993.1.12",
          "form": "완전 뜨겁ㅎ",
          "original_form": "완전 뜨겁ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030993.1.13",
          "form": "버스 기달중\r\n많이 식것다ㅎ",
          "original_form": "버스 기달중\r\n많이 식것다ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030993.1.14",
          "form": "튀김이 완판이었구만ㅠ",
          "original_form": "튀김이 완판이었구만ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100030993.1.15",
          "form": "나도 소금보단 막장!",
          "original_form": "나도 소금보단 막장!",
          "speaker_id": "1",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100030993.1.16",
          "form": "맞어ㅎ",
          "original_form": "맞어ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030993.1.17",
          "form": "기숙사에 만들어둬야하나 싶어",
          "original_form": "기숙사에 만들어둬야하나 싶어",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030993.1.18",
          "form": "여긴 막장을 안줘",
          "original_form": "여긴 막장을 안줘",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030993.1.19",
          "form": "팔지 않을까?",
          "original_form": "팔지 않을까?",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030993.1.20",
          "form": "인터넷에",
          "original_form": "인터넷에",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030993.1.21",
          "form": "함 봐야겠다\r\n이집 순대 한번먹고 반해서 계속 먹곱ㅎ",
          "original_form": "함 봐야겠다\r\n이집 순대 한번먹고 반해서 계속 먹곱ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100030993.1.22",
          "form": "순대 떡볶이에 찍어 맛있지",
          "original_form": "순대 떡볶이에 찍어 맛있지",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100030993.1.23",
          "form": "찍어먹음 맛있지",
          "original_form": "찍어먹음 맛있지",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100030993.1.24",
          "form": "난 떡볶이 떡보다 그 소스에 다른거 찤어먹는게 맛나더라",
          "original_form": "난 떡볶이 떡보다 그 소스에 다른거 찤어먹는게 맛나더라",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030993.1.25",
          "form": "맞어 나도 떡볶이 떡이 안뿔어서ㅎ\r\n난 떡이 몰캉 뿔은거솧아해서ㅎ",
          "original_form": "맞어 나도 떡볶이 떡이 안뿔어서ㅎ\r\n난 떡이 몰캉 뿔은거솧아해서ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030993.1.26",
          "form": "떡볶이 먹고있나 ㅋㅋ",
          "original_form": "떡볶이 먹고있나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100030993.1.27",
          "form": "아직 인자 기숙사가는 버스탔어\r\n앞에 버스 농쳐서ㅎ",
          "original_form": "아직 인자 기숙사가는 버스탔어\r\n앞에 버스 농쳐서ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100030993.1.28",
          "form": "ㅜㅜ 떡볶이 빨리 묵어야되는데",
          "original_form": "ㅜㅜ 떡볶이 빨리 묵어야되는데",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100030993.1.29",
          "form": "ㅎㅎ아마 식당 밥 먹어야해서\r\n조금 맛만 봐야할거같아\r\n생각보다 시간이 늦었네",
          "original_form": "ㅎㅎ아마 식당 밥 먹어야해서\r\n조금 맛만 봐야할거같아\r\n생각보다 시간이 늦었네",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100030993.1.30",
          "form": "넌 점심 챙겨먹었어?",
          "original_form": "넌 점심 챙겨먹었어?",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100030993.1.31",
          "form": "헐 이론 구래도 떡볶이는 전자렌지 데워먹어도 되서 다행이다",
          "original_form": "헐 이론 구래도 떡볶이는 전자렌지 데워먹어도 되서 다행이다",
          "speaker_id": "1",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100030993.1.32",
          "form": "난 엄마가 빵 사워서 먹었는데 괜히먹은듯",
          "original_form": "난 엄마가 빵 사워서 먹었는데 괜히먹은듯",
          "speaker_id": "1",
          "time": "20210904 16:13"
        }
      ]
    }
