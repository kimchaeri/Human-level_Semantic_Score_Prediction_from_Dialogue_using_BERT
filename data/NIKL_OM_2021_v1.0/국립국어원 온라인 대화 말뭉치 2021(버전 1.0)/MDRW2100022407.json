          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022407.1.5",
          "form": "땀삐질삐질",
          "original_form": "땀삐질삐질",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022407.1.6",
          "form": "그쵸 아침저녁으로 많이 시원해졌는데",
          "original_form": "그쵸 아침저녁으로 많이 시원해졌는데",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022407.1.7",
          "form": "점심엔 아직 덥네요",
          "original_form": "점심엔 아직 덥네요",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022407.1.8",
          "form": "씻고 에어컨트니까 천국이에요",
          "original_form": "씻고 에어컨트니까 천국이에요",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022407.1.9",
          "form": "여름의 끝인듯 싶네요 ㅎㅎ",
          "original_form": "여름의 끝인듯 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022407.1.10",
          "form": "저흰 점심쯤부터 에어컨 한번도 못껐어요~",
          "original_form": "저흰 점심쯤부터 에어컨 한번도 못껐어요~",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022407.1.11",
          "form": "주간날씨보니가",
          "original_form": "주간날씨보니가",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022407.1.12",
          "form": "쭈욱 비 비 비 네요ㅠ",
          "original_form": "쭈욱 비 비 비 네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022407.1.13",
          "form": "습하고ㅜㅜ 비가 계속 조금씩 내릴껀가바요",
          "original_form": "습하고ㅜㅜ 비가 계속 조금씩 내릴껀가바요",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022407.1.14",
          "form": "추석 전까지 그럴꺼같아욤",
          "original_form": "추석 전까지 그럴꺼같아욤",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022407.1.15",
          "form": "네 가을장마라더니 계속 올건가봐요",
          "original_form": "네 가을장마라더니 계속 올건가봐요",
          "speaker_id": "1",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022407.1.16",
          "form": "니무습해서 불쾌지수가 ...ㅜ",
          "original_form": "니무습해서 불쾌지수가 ...ㅜ",
          "speaker_id": "1",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022407.1.17",
          "form": "지금 이곳은 맑아요",
          "original_form": "지금 이곳은 맑아요",
          "speaker_id": "2",
          "time": "20210830 10:21"
        },
        {
          "id": "MDRW2100022407.1.18",
          "form": "출근길에 쫌 시원햇어요",
          "original_form": "출근길에 쫌 시원햇어요",
          "speaker_id": "2",
          "time": "20210830 10:21"
        },
        {
          "id": "MDRW2100022407.1.19",
          "form": "우와 저는 남쪽인데",
          "original_form": "우와 저는 남쪽인데",
          "speaker_id": "1",
          "time": "20210830 10:23"
        },
        {
          "id": "MDRW2100022407.1.20",
          "form": "아침부터 더웠어요ㅠㅠ",
          "original_form": "아침부터 더웠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:23"
        },
        {
          "id": "MDRW2100022407.1.21",
          "form": "오늘도 엄청 덥겠구나 했네요..",
          "original_form": "오늘도 엄청 덥겠구나 했네요..",
          "speaker_id": "1",
          "time": "20210830 10:23"
        },
        {
          "id": "MDRW2100022407.1.22",
          "form": "오 저도 남쪽인데!",
          "original_form": "오 저도 남쪽인데!",
          "speaker_id": "2",
          "time": "20210830 10:24"
        },
        {
          "id": "MDRW2100022407.1.23",
