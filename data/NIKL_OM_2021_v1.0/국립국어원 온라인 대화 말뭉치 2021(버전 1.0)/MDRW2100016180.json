        },
        {
          "id": "MDRW2100016180.1.3",
          "form": "군대얘기 좋아하시나요",
          "original_form": "군대얘기 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100016180.1.4",
          "form": "그냥저냥?",
          "original_form": "그냥저냥?",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016180.1.5",
          "form": "군대다녀오셨어요? 육군?",
          "original_form": "군대다녀오셨어요? 육군?",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016180.1.6",
          "form": "넵 육군이요",
          "original_form": "넵 육군이요",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016180.1.7",
          "form": "오 저도 육군이요 ㅎㅎ",
          "original_form": "오 저도 육군이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016180.1.8",
          "form": "몇사단나오셨어요? 저는 후방이라....ㅎ",
          "original_form": "몇사단나오셨어요? 저는 후방이라....ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016180.1.9",
          "form": "저 3사단이요!",
          "original_form": "저 3사단이요!",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016180.1.10",
          "form": "오  빡센데 나오셨네왜 ㅎㅎ",
