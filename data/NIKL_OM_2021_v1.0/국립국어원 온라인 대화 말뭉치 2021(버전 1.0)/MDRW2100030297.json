      "utterance": [
        {
          "id": "MDRW2100030297.1.1",
          "form": "요즘 유툽에서 보는거 있으신가오?",
          "original_form": "요즘 유툽에서 보는거 있으신가오?",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030297.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030297.1.3",
          "form": "뭐가 인기가 있나요?",
          "original_form": "뭐가 인기가 있나요?",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030297.1.4",
          "form": "저는 요즘 드라마몰아보기합니다",
          "original_form": "저는 요즘 드라마몰아보기합니다",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030297.1.5",
          "form": "오오 어떤 드라마 보나요",
          "original_form": "오오 어떤 드라마 보나요",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030297.1.6",
          "form": "요즘 나는 영화 리뷰 해주는 유툽 많이 보는듯ㅋㅋㅋ",
          "original_form": "요즘 나는 영화 리뷰 해주는 유툽 많이 보는듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030297.1.7",
          "form": "비밀의 숲 강추합니댜",
          "original_form": "비밀의 숲 강추합니댜",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030297.1.8",
          "form": "그거 다봤어요",
          "original_form": "그거 다봤어요",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030297.1.9",
          "form": "헐 그럼....얼마전에 끝난 괴물!",
          "original_form": "헐 그럼....얼마전에 끝난 괴물!",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030297.1.10",
          "form": "요즘 미치지 않고서야",
          "original_form": "요즘 미치지 않고서야",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.11",
          "form": "여진구랑 신하균 나오는것도 재밌음ㅋㅋ",
          "original_form": "여진구랑 신하균 나오는것도 재밌음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.12",
          "form": "인가 그거 봤어요",
          "original_form": "인가 그거 봤어요",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.13",
          "form": "오 그거 보는구낭 그거 재밌따고 하든데",
          "original_form": "오 그거 보는구낭 그거 재밌따고 하든데",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.14",
          "form": "저는 펜트하우스 봅니댜..",
          "original_form": "저는 펜트하우스 봅니댜..",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.15",
          "form": "막장드라마...",
          "original_form": "막장드라마...",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.16",
          "form": "넹 재미있게 봤어요",
          "original_form": "넹 재미있게 봤어요",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.17",
          "form": "막장ㅋㅋㅋ",
          "original_form": "막장ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.18",
          "form": "중간에보다가 끝날때까지 기다리고있어요",
          "original_form": "중간에보다가 끝날때까지 기다리고있어요",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030297.1.19",
          "form": "슬기로운 의사생활도 재밌어요",
          "original_form": "슬기로운 의사생활도 재밌어요",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.20",
          "form": "오오 끝나지 않았나오?",
          "original_form": "오오 끝나지 않았나오?",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.21",
          "form": "펜트하우스 아직안끝났어요",
          "original_form": "펜트하우스 아직안끝났어요",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.22",
          "form": "슬기로운 의사생활은 보고있어요",
          "original_form": "슬기로운 의사생활은 보고있어요",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.23",
          "form": "곧 끝나오",
          "original_form": "곧 끝나오",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.24",
          "form": "님 자오?",
          "original_form": "님 자오?",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.25",
          "form": "나 씻고 오겠음",
          "original_form": "나 씻고 오겠음",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.26",
          "form": "어제 결방했어요",
          "original_form": "어제 결방했어요",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.27",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030297.1.28",
          "form": "짜란",
          "original_form": "짜란",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030297.1.29",
          "form": "아직 막방 안했군여",
          "original_form": "아직 막방 안했군여",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030297.1.30",
          "form": "얼마 안남았대요",
          "original_form": "얼마 안남았대요",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030297.1.31",
          "form": "2편인가 남은걸로 알고있어요",
          "original_form": "2편인가 남은걸로 알고있어요",
          "speaker_id": "2",
