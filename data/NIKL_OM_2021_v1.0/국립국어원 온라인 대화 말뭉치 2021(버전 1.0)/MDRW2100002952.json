      "utterance": [
        {
          "id": "MDRW2100002952.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002952.1.2",
          "form": "안녕하세요~ 저녁 뭐드세요?",
          "original_form": "안녕하세요~ 저녁 뭐드세요?",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002952.1.3",
          "form": "오늘 저녁은 집밥 먹을거같아요~ 코로나때문에 그냥 집에서~",
          "original_form": "오늘 저녁은 집밥 먹을거같아요~ 코로나때문에 그냥 집에서~",
          "speaker_id": "1",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002952.1.4",
          "form": "응아님은요?",
          "original_form": "응아님은요?",
          "speaker_id": "1",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002952.1.5",
          "form": "저는 친구랑 약속있어서 양식먹을거같아요 ㅎㅎ",
          "original_form": "저는 친구랑 약속있어서 양식먹을거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002952.1.6",
          "form": "오호 저도 양식 좋아해용 스테이크같은거",
          "original_form": "오호 저도 양식 좋아해용 스테이크같은거",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002952.1.7",
          "form": "까르보나라랑 스테이크 먹고올거에요 ㅎㅎ설렌다",
          "original_form": "까르보나라랑 스테이크 먹고올거에요 ㅎㅎ설렌다",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002952.1.8",
          "form": "와 맛있겠네요",
          "original_form": "와 맛있겠네요",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002952.1.9",
          "form": "저는 집에서 김치랑 김 ㅋㅋㅋ..",
          "original_form": "저는 집에서 김치랑 김 ㅋㅋㅋ..",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002952.1.10",
          "form": "반찬이 ㅠㅠ넘나슬픈데요 계란후라이라도 해먹어요",
          "original_form": "반찬이 ㅠㅠ넘나슬픈데요 계란후라이라도 해먹어요",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002952.1.11",
          "form": "음료수는 어떤 음료수 좋아하세요?",
          "original_form": "음료수는 어떤 음료수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002952.1.12",
          "form": "알로에나 매실 좋아해요 ㅎ 알로에를 더좋아하는듯",
          "original_form": "알로에나 매실 좋아해요 ㅎ 알로에를 더좋아하는듯",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002952.1.13",
          "form": "저는 토마토 주스 좋아해요~ 근데 과일주스가 설탕이 엄청 들어갔대요",
          "original_form": "저는 토마토 주스 좋아해요~ 근데 과일주스가 설탕이 엄청 들어갔대요",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002952.1.14",
          "form": "마자요 ㅠ 시럽빼달라고하면 맛없어서 먹을수가없어요 ㅋㅋㅋ",
          "original_form": "마자요 ㅠ 시럽빼달라고하면 맛없어서 먹을수가없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002952.1.15",
          "form": "토마토 좋아하시는군요!",
          "original_form": "토마토 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002952.1.16",
          "form": "콜라랑 사이다 둘중에 어떤거 좋아하세요?",
          "original_form": "콜라랑 사이다 둘중에 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002952.1.17",
          "form": "저는 탄산별로안좋아해용 ㅠㅠㅋㅋㅋ 굳이 먹는다면 사이다!",
          "original_form": "저는 탄산별로안좋아해용 ㅠㅠㅋㅋㅋ 굳이 먹는다면 사이다!",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100002952.1.18",
          "form": "저도 사이다가 더 좋아요~",
          "original_form": "저도 사이다가 더 좋아요~",
          "speaker_id": "1",
          "time": "20210820 17:27"
        }
      ]
