      "utterance": [
        {
          "id": "MDRW2100015731.1.1",
          "form": "일은 하시고계신가용",
          "original_form": "일은 하시고계신가용",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015731.1.2",
          "form": "네ㅎㅎ 일하고 계신가요?",
          "original_form": "네ㅎㅎ 일하고 계신가요?",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015731.1.3",
          "form": "음 했었는데 지금 반백수에요 ㅋㅋㅋ ㅠ",
          "original_form": "음 했었는데 지금 반백수에요 ㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015731.1.4",
          "form": "다니기 좋아용?",
          "original_form": "다니기 좋아용?",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015731.1.5",
          "form": "아니요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015731.1.6",
          "form": "자기가 좋아하는 일 하면서 살아가는 분들이 부럽...",
          "original_form": "자기가 좋아하는 일 하면서 살아가는 분들이 부럽...",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015731.1.7",
          "form": "ㅋㅋㅋ칼퇴는 하시고요..?",
          "original_form": "ㅋㅋㅋ칼퇴는 하시고요..?",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015731.1.8",
          "form": "칼퇴는 못하지만 그래도 코로나때문에 회식을 안해서 그건 좋습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "칼퇴는 못하지만 그래도 코로나때문에 회식을 안해서 그건 좋습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015731.1.9",
          "form": "ㅋㅋㅋ 코로나의순기능",
          "original_form": "ㅋㅋㅋ 코로나의순기능",
          "speaker_id": "1",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015731.1.10",
          "form": "연차는 잘 쓰세용?",
          "original_form": "연차는 잘 쓰세용?",
          "speaker_id": "1",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015731.1.11",
          "form": "아니요...ㅋㅋㅋㅋ쉽지않아요 ㅋㅋㅋㅋㅠㅠㅠㅠ",
          "original_form": "아니요...ㅋㅋㅋㅋ쉽지않아요 ㅋㅋㅋㅋㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015731.1.12",
          "form": "ㅋㅋㅋㅋ 이직 생각은없으신가요",
          "original_form": "ㅋㅋㅋㅋ 이직 생각은없으신가요",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015731.1.13",
          "form": "마음은 굴뚝같은데 또 엄두가 안나서요 ㅠㅠ",
          "original_form": "마음은 굴뚝같은데 또 엄두가 안나서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015731.1.14",
          "form": "이직 준비중이세요?",
          "original_form": "이직 준비중이세요?",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015731.1.15",
          "form": "아뇨 아직 전 대학원다녀서.. 취직준비해야죠ㅠㅠㅋㅋ",
          "original_form": "아뇨 아직 전 대학원다녀서.. 취직준비해야죠ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015731.1.16",
          "form": "돈벌고싶은데 회사가긴싫어요",
          "original_form": "돈벌고싶은데 회사가긴싫어요",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015731.1.17",
          "form": "로또제발 ㅎㅎㅎ",
          "original_form": "로또제발 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015731.1.18",
          "form": "저도 로또 간절합니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 로또 간절합니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015731.1.19",
          "form": "대학원다니시는구나 멋지네요",
          "original_form": "대학원다니시는구나 멋지네요",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015731.1.20",
          "form": "사람들은 괜찮아요? ㅋㅋㅋ",
          "original_form": "사람들은 괜찮아요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015731.1.21",
          "form": "전공이 뭔지 여쭤봐도 될까요?",
          "original_form": "전공이 뭔지 여쭤봐도 될까요?",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015731.1.22",
          "form": "아 전 건설공학쪽..? ㅋㅋ",
          "original_form": "아 전 건설공학쪽..? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015731.1.23",
          "form": "사람이 참 어렵죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사람이 참 어렵죠 ㅋㅋㅋㅋㅋㅋ",
