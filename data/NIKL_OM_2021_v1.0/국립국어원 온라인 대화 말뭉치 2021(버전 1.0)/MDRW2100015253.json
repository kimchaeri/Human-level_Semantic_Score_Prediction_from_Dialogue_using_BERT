      "utterance": [
        {
          "id": "MDRW2100015253.1.1",
          "form": "name2님은 갖고 싶은 거 있으세요?",
          "original_form": "&name2&님은 갖고 싶은 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015253.1.2",
          "form": "저..",
          "original_form": "저..",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015253.1.3",
          "form": "차요",
          "original_form": "차요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015253.1.4",
          "form": "자동차",
          "original_form": "자동차",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015253.1.5",
          "form": "오 어떤 차요?",
          "original_form": "오 어떤 차요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015253.1.6",
          "form": "봐두신 거 있나요?",
          "original_form": "봐두신 거 있나요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015253.1.7",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015253.1.8",
          "form": "ㅋㅋ관심 있는 브랜드 있으세요?",
          "original_form": "ㅋㅋ관심 있는 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015253.1.9",
          "form": "그냥 아무거나 다 좋아요",
          "original_form": "그냥 아무거나 다 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015253.1.10",
          "form": "(사실 쿠퍼타고싶긴한데)",
          "original_form": "(사실 쿠퍼타고싶긴한데)",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015253.1.11",
          "form": "넘비싸서",
          "original_form": "넘비싸서",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015253.1.12",
          "form": "모닝도 괜찮아요",
          "original_form": "모닝도 괜찮아요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015253.1.13",
          "form": "ㅋㅋㅋ전 나중에 빨간 차 꼭 사고 싶어요",
          "original_form": "ㅋㅋㅋ전 나중에 빨간 차 꼭 사고 싶어요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015253.1.14",
          "form": "빨간차?",
          "original_form": "빨간차?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015253.1.15",
          "form": "티코요?",
          "original_form": "티코요?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015253.1.16",
          "form": "아뇨ㅋㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015253.1.17",
          "form": "중형차 타고 싶어요",
          "original_form": "중형차 타고 싶어요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015253.1.18",
          "form": "물론 돈을 많이 벌어야되겠지만요",
          "original_form": "물론 돈을 많이 벌어야되겠지만요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015253.1.19",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015253.1.20",
          "form": "염두해두신 차있으세요?",
          "original_form": "염두해두신 차있으세요?",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015253.1.21",
          "form": "딱히 없어요 굳이 말하자면  볼보?",
          "original_form": "딱히 없어요 굳이 말하자면  볼보?",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015253.1.22",
          "form": "전기차 끌어보고 싶은 마음도 있어요",
          "original_form": "전기차 끌어보고 싶은 마음도 있어요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015253.1.23",
          "form": "아아볼보",
          "original_form": "아아볼보",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015253.1.24",
          "form": "많이들어봤어용",
          "original_form": "많이들어봤어용",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015253.1.25",
          "form": "안전성 면에서는 유명하더라고요",
          "original_form": "안전성 면에서는 유명하더라고요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        }
      ]
