        {
          "id": "MDRW2100006339.1.1",
          "form": "결혼하셨나요",
          "original_form": "결혼하셨나요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100006339.1.2",
          "form": "아뇨ㅎㅎ아직이요!",
          "original_form": "아뇨ㅎㅎ아직이요!",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006339.1.3",
          "form": "결혼 일찍하셨나요?",
          "original_form": "결혼 일찍하셨나요?",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006339.1.4",
          "form": "29살이요",
          "original_form": "29살이요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006339.1.5",
          "form": "꽤 빨리 하셨네요!!",
          "original_form": "꽤 빨리 하셨네요!!",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006339.1.6",
          "form": "결혼하면 어떤가요?",
          "original_form": "결혼하면 어떤가요?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006339.1.7",
          "form": "ㅎㅎㅎ 장단점이 있죠..ㅎㅎ",
          "original_form": "ㅎㅎㅎ 장단점이 있죠..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006339.1.8",
          "form": "ㅋㅋㅋ맞죠...그렇겠죠! 안해봐서 궁굼하네요",
          "original_form": "ㅋㅋㅋ맞죠...그렇겠죠! 안해봐서 궁굼하네요",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006339.1.9",
          "form": "주변 친구들도 이직인5",
          "original_form": "주변 친구들도 이직인5",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006339.1.10",
          "form": "아직인가요?",
          "original_form": "아직인가요?",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006339.1.11",
          "form": "거의 반반이에요 안간친구반 간친구 반ㅎㅎ",
          "original_form": "거의 반반이에요 안간친구반 간친구 반ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006339.1.12",
          "form": "그렇군요 간 친구들은.어때보이나요 ㅎㅎ",
          "original_form": "그렇군요 간 친구들은.어때보이나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006339.1.13",
          "form": "겉으로 보기엔 다들 좋아보여요ㅎㅎㅎ",
          "original_form": "겉으로 보기엔 다들 좋아보여요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006339.1.14",
          "form": "그렇죠 ㅎㅎ 겉으론 속사정까진.모르니까요 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ 겉으론 속사정까진.모르니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006339.1.15",
          "form": "ㅎㅎ 아기낳은 친구들은 너무 바쁘고 힘들어보이긴 하더라구요",
          "original_form": "ㅎㅎ 아기낳은 친구들은 너무 바쁘고 힘들어보이긴 하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006339.1.16",
          "form": "맞아요 육아가 힘들어요 보람된만큼..",
