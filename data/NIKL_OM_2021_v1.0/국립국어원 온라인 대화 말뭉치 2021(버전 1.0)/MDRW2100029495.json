      "utterance": [
        {
          "id": "MDRW2100029495.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100029495.1.2",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029495.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029495.1.4",
          "form": "전 멀티요",
          "original_form": "전 멀티요",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029495.1.5",
          "form": "어떤 청소가 젤 하기 싫어요?",
          "original_form": "어떤 청소가 젤 하기 싫어요?",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100029495.1.6",
          "form": "화장실이대박이죠",
          "original_form": "화장실이대박이죠",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029495.1.7",
          "form": "맞아요 힘들죠ㅜㅜ",
          "original_form": "맞아요 힘들죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029495.1.8",
          "form": "매주 해요?",
          "original_form": "매주 해요?",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029495.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029495.1.10",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029495.1.11",
          "form": "몸에도 나쁘잖아요",
          "original_form": "몸에도 나쁘잖아요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029495.1.12",
          "form": "그러나 살균소독이 중요하니",
          "original_form": "그러나 살균소독이 중요하니",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029495.1.13",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029495.1.14",
          "form": "울며 겨자 먹기로",
          "original_form": "울며 겨자 먹기로",
          "speaker_id": "2",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100029495.1.15",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029495.1.16",
          "form": "진짜 조심해야 돼요 락스랑 퐁퐁 특히 절대 섞은 안 되고",
          "original_form": "진짜 조심해야 돼요 락스랑 퐁퐁 특히 절대 섞은 안 되고",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029495.1.17",
          "form": "유해가스 때문에",
          "original_form": "유해가스 때문에",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029495.1.18",
          "form": "마스크쓰고안되요",
          "original_form": "마스크쓰고안되요",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029495.1.19",
          "form": "마스크 쓰고 청소하면 너무 답답하지 않으세요?",
          "original_form": "마스크 쓰고 청소하면 너무 답답하지 않으세요?",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029495.1.20",
          "form": "안전때문에 답답해도 참아야죠",
          "original_form": "안전때문에 답답해도 참아야죠",
          "speaker_id": "1",
