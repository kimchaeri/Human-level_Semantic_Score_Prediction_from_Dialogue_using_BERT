      "utterance": [
        {
          "id": "MDRW2100046576.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211102 00:02"
        },
        {
          "id": "MDRW2100046576.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211102 00:02"
        },
        {
          "id": "MDRW2100046576.1.3",
          "form": "글램핑 좋아하시나용",
          "original_form": "글램핑 좋아하시나용",
          "speaker_id": "2",
          "time": "20211102 00:02"
        },
        {
          "id": "MDRW2100046576.1.4",
          "form": "아뇨, 야외로 놀러가는 건 좋아하는데",
          "original_form": "아뇨, 야외로 놀러가는 건 좋아하는데",
          "speaker_id": "1",
          "time": "20211102 00:03"
        },
        {
          "id": "MDRW2100046576.1.5",
          "form": "잠은 침대에서 자는 걸 선호해요",
          "original_form": "잠은 침대에서 자는 걸 선호해요",
          "speaker_id": "1",
          "time": "20211102 00:03"
        },
        {
          "id": "MDRW2100046576.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20211102 00:03"
        },
        {
          "id": "MDRW2100046576.1.7",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20211102 00:03"
        },
        {
          "id": "MDRW2100046576.1.8",
          "form": "글램핑도",
          "original_form": "글램핑도",
          "speaker_id": "2",
          "time": "20211102 00:03"
        },
        {
          "id": "MDRW2100046576.1.9",
          "form": "침대에서..",
          "original_form": "침대에서..",
          "speaker_id": "2",
          "time": "20211102 00:03"
        },
        {
          "id": "MDRW2100046576.1.10",
          "form": "자는거아닌가용??",
          "original_form": "자는거아닌가용??",
          "speaker_id": "2",
          "time": "20211102 00:03"
        },
        {
          "id": "MDRW2100046576.1.11",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "1",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.12",
          "form": "한번도 안가봐서요 ㅎㅎ",
          "original_form": "한번도 안가봐서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.13",
          "form": "아 그러셨구나 ㅎㅎ",
          "original_form": "아 그러셨구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.14",
          "form": "그냥 캠핑인데",
          "original_form": "그냥 캠핑인데",
          "speaker_id": "2",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.15",
          "form": "다 준비되어있는",
          "original_form": "다 준비되어있는",
          "speaker_id": "2",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.16",
          "form": "캠핑이라고 생각하시면 될 거 같아요 ㅎㅎ",
          "original_form": "캠핑이라고 생각하시면 될 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.17",
          "form": "안에 침대랑 필요한거 다 있고..",
          "original_form": "안에 침대랑 필요한거 다 있고..",
          "speaker_id": "2",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.18",
          "form": "화장실만 없는?",
          "original_form": "화장실만 없는?",
          "speaker_id": "2",
          "time": "20211102 00:04"
        },
        {
          "id": "MDRW2100046576.1.19",
          "form": "아 그렇군요, 캠핑보다는 확실히 편하겠어요",
          "original_form": "아 그렇군요, 캠핑보다는 확실히 편하겠어요",
          "speaker_id": "1",
          "time": "20211102 00:05"
        },
        {
          "id": "MDRW2100046576.1.20",
          "form": "님은 글램핑 가보셨어요?",
          "original_form": "님은 글램핑 가보셨어요?",
          "speaker_id": "1",
          "time": "20211102 00:05"
        },
        {
          "id": "MDRW2100046576.1.21",
          "form": "네네 저도 캠핑은",
          "original_form": "네네 저도 캠핑은",
          "speaker_id": "2",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046576.1.22",
          "form": "엄두가 안나는데",
          "original_form": "엄두가 안나는데",
          "speaker_id": "2",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046576.1.23",
          "form": "글램핑은 가서 편하고 재밌게 놀다왔어요 ㅎㅎ",
          "original_form": "글램핑은 가서 편하고 재밌게 놀다왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046576.1.24",
          "form": "씻기 좀 귀찮다는게 단점이지만 ㅋㅋㅋ",
          "original_form": "씻기 좀 귀찮다는게 단점이지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046576.1.25",
          "form": "가평으로 갔다왔어요!!",
          "original_form": "가평으로 갔다왔어요!!",
          "speaker_id": "2",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046576.1.26",
          "form": "오 가평이면 서울에서 그리 멀지는 않네요",
          "original_form": "오 가평이면 서울에서 그리 멀지는 않네요",
          "speaker_id": "1",
          "time": "20211102 00:07"
        },
        {
          "id": "MDRW2100046576.1.27",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.28",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.29",
          "form": "대부분 글램핑장은",
          "original_form": "대부분 글램핑장은",
          "speaker_id": "2",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.30",
          "form": "가평역 근처",
          "original_form": "가평역 근처",
          "speaker_id": "2",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.31",
          "form": "마트들이랑 제휴를 맺었는지",
          "original_form": "마트들이랑 제휴를 맺었는지",
          "speaker_id": "2",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.32",
          "form": "고기 사면 픽업해주고 그러더라구요",
          "original_form": "고기 사면 픽업해주고 그러더라구요",
          "speaker_id": "2",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.33",
          "form": "그래서 차 없는 분들도 편하게 놀다 올 수 있어요 ㅎㅎㅎ",
          "original_form": "그래서 차 없는 분들도 편하게 놀다 올 수 있어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.34",
          "form": "ㅋㅋ그런 서비스도 하는군요",
          "original_form": "ㅋㅋ그런 서비스도 하는군요",
          "speaker_id": "1",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.35",
          "form": "저도 한번 알아봐야겠네요",
          "original_form": "저도 한번 알아봐야겠네요",
          "speaker_id": "1",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046576.1.36",
          "form": "저는 주로 낚시하러 서해안을 주로 다녔었거든요",
          "original_form": "저는 주로 낚시하러 서해안을 주로 다녔었거든요",
          "speaker_id": "1",
          "time": "20211102 00:09"
        },
        {
          "id": "MDRW2100046576.1.37",
          "form": "네네 한 번 갔다와보세요 되게 좋은 경험이었어요 저는 ㅎㅎ",
          "original_form": "네네 한 번 갔다와보세요 되게 좋은 경험이었어요 저는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046576.1.38",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046576.1.39",
          "form": "낚시?!?!",
          "original_form": "낚시?!?!",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046576.1.40",
          "form": "저는 한 번도 못해봤는데....",
          "original_form": "저는 한 번도 못해봤는데....",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046576.1.41",
          "form": "낚시도 종류가 많아서...ㅎㅎ",
          "original_form": "낚시도 종류가 많아서...ㅎㅎ",
          "speaker_id": "1",
          "time": "20211102 00:11"
        },
        {
          "id": "MDRW2100046576.1.42",
          "form": "초보자가 해보기 좋은 바다낚시는 좌대낚시가 있어요",
          "original_form": "초보자가 해보기 좋은 바다낚시는 좌대낚시가 있어요",
          "speaker_id": "1",
          "time": "20211102 00:11"
        },
        {
          "id": "MDRW2100046576.1.43",
          "form": "배로 바다위에 떠있는 양식장에 가서 하는 낚시인데, 낚시대도 빌려주고 하니까 할만해요. 따로 장비 안챙겨가도 돼서요",
          "original_form": "배로 바다위에 떠있는 양식장에 가서 하는 낚시인데, 낚시대도 빌려주고 하니까 할만해요. 따로 장비 안챙겨가도 돼서요",
          "speaker_id": "1",
          "time": "20211102 00:12"
        },
        {
          "id": "MDRW2100046576.1.44",
          "form": "우와 이렇게 자세하게 설명해주실 줄이야 ㅠㅠㅠㅠ",
          "original_form": "우와 이렇게 자세하게 설명해주실 줄이야 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20211102 00:13"
        },
        {
          "id": "MDRW2100046576.1.45",
          "form": "우와우와 한 번 해보고싶었는데",
          "original_form": "우와우와 한 번 해보고싶었는데",
          "speaker_id": "2",
          "time": "20211102 00:13"
        },
        {
          "id": "MDRW2100046576.1.46",
          "form": "좌대낚시..메모했다가",
          "original_form": "좌대낚시..메모했다가",
          "speaker_id": "2",
          "time": "20211102 00:13"
        },
        {
          "id": "MDRW2100046576.1.47",
          "form": "꼭 해보겠습니다 ㅎㅎ",
          "original_form": "꼭 해보겠습니다 ㅎㅎ",
          "speaker_id": "2",
