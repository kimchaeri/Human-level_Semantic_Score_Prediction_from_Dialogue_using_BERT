      "utterance": [
        {
          "id": "MDRW2100008278.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008278.1.2",
          "form": "안녕하세요 학생이신가요?",
          "original_form": "안녕하세요 학생이신가요?",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008278.1.3",
          "form": "네 대학생이에요!",
          "original_form": "네 대학생이에요!",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008278.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008278.1.5",
          "form": "저는 졸업했어요",
          "original_form": "저는 졸업했어요",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008278.1.6",
          "form": "대학생이시구나 동아리활동 해요?",
          "original_form": "대학생이시구나 동아리활동 해요?",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008278.1.7",
          "form": "지금은 하는 게 없네요",
          "original_form": "지금은 하는 게 없네요",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008278.1.8",
          "form": "예전에는 교육봉사 동아리를 했었어요",
          "original_form": "예전에는 교육봉사 동아리를 했었어요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008278.1.9",
          "form": "name2님은 동아리 하셨던 것 잇나요?",
          "original_form": "&name2&님은 동아리 하셨던 것 잇나요?",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008278.1.10",
          "form": "교육봉사 친구가 하는거 몇번 따라가본적 있어요",
          "original_form": "교육봉사 친구가 하는거 몇번 따라가본적 있어요",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008278.1.11",
          "form": "저는 과동아리말고는 제대로 해본 동아리가 없네요",
          "original_form": "저는 과동아리말고는 제대로 해본 동아리가 없네요",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008278.1.12",
          "form": "학교는 어땠어요?",
          "original_form": "학교는 어땠어요?",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008278.1.13",
          "form": "축제같은거!",
          "original_form": "축제같은거!",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008278.1.14",
          "form": "저는 축제 제대로 즐긴적이 없거든요",
          "original_form": "저는 축제 제대로 즐긴적이 없거든요",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008278.1.15",
          "form": "코시국이라 ㅎ..",
          "original_form": "코시국이라 ㅎ..",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008278.1.16",
          "form": "아 코로나라 그렇겠다ㅠㅠ 아쉬우시겠어요",
          "original_form": "아 코로나라 그렇겠다ㅠㅠ 아쉬우시겠어요",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008278.1.17",
          "form": "저는 새내기때 지역 대학축제란축제 다 돌아다니면서",
          "original_form": "저는 새내기때 지역 대학축제란축제 다 돌아다니면서",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008278.1.18",
          "form": "무대보고 친구들하고 술마시고 하면서 놀았어요",
          "original_form": "무대보고 친구들하고 술마시고 하면서 놀았어요",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008278.1.19",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008278.1.20",
          "form": "진짜 재밌었겠어요",
          "original_form": "진짜 재밌었겠어요",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008278.1.21",
          "form": "저는 연예인 좀 보고싶었는데",
          "original_form": "저는 연예인 좀 보고싶었는데",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008278.1.22",
          "form": "한번도 못봤네요 ㅜㅜㅋㅋㅋㅋ",
          "original_form": "한번도 못봤네요 ㅜㅜㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008278.1.23",
          "form": "아쉽겠다ㅠㅠ 엠티는 가보셨어요?",
          "original_form": "아쉽겠다ㅠㅠ 엠티는 가보셨어요?",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008278.1.24",
          "form": "아 엠티는 가봤어요!",
          "original_form": "아 엠티는 가봤어요!",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008278.1.25",
          "form": "되게 재밌더라고요 ㅋㅋ",
          "original_form": "되게 재밌더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008278.1.26",
          "form": "name2님도 다녀오셨죠?",
          "original_form": "&name2&님도 다녀오셨죠?",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008278.1.27",
          "form": "네 술마실땐 다 친해지는점이 재밌었어요ㅋㅋㅋㅋ",
          "original_form": "네 술마실땐 다 친해지는점이 재밌었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008278.1.28",
          "form": "취하면 다 친구친구",
          "original_form": "취하면 다 친구친구",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008278.1.29",
          "form": "다음날 되면 다시",
          "original_form": "다음날 되면 다시",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008278.1.30",
          "form": "어색어색 해지기도 하지만요 ㅎㅎ",
          "original_form": "어색어색 해지기도 하지만요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008278.1.31",
          "form": "저는 다행히 오티는 해서 친구 사귀었지만",
          "original_form": "저는 다행히 오티는 해서 친구 사귀었지만",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008278.1.32",
          "form": "못한데는 진짜 친구 사귀는 것도 일인가봐요",
          "original_form": "못한데는 진짜 친구 사귀는 것도 일인가봐요",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008278.1.33",
          "form": "그럴거같아요ㅠㅠ 수업도 비대면위주 아니였나요",
          "original_form": "그럴거같아요ㅠㅠ 수업도 비대면위주 아니였나요",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100008278.1.34",
          "form": "만날일이 잘없으니...",
          "original_form": "만날일이 잘없으니...",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100008278.1.35",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008278.1.36",
          "form": "저는 과 특성상 자주 가긴했는데",
          "original_form": "저는 과 특성상 자주 가긴했는데",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008278.1.37",
          "form": "친구들 얘기 들어보면 동기들 얼굴도 모른다고 하더라고요 ㅠㅠ",
          "original_form": "친구들 얘기 들어보면 동기들 얼굴도 모른다고 하더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008278.1.38",
          "form": "만나져야 친해지는데 동기들하고 서먹한채로 졸업할수도 있겠네요ㅠㅠ",
          "original_form": "만나져야 친해지는데 동기들하고 서먹한채로 졸업할수도 있겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:24"
        }
      ]
