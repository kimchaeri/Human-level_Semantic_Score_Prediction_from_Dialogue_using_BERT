      "utterance": [
        {
          "id": "MDRW2100037309.1.1",
          "form": "카페가면 어떤거 자주 드세용?",
          "original_form": "카페가면 어떤거 자주 드세용?",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037309.1.2",
          "form": "아이스 아메리카노 자주먹어요 ㅎㄹㅎ",
          "original_form": "아이스 아메리카노 자주먹어요 ㅎㄹㅎ",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037309.1.3",
          "form": "오옹 커피 좋아하시는군욥",
          "original_form": "오옹 커피 좋아하시는군욥",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037309.1.4",
          "form": "좋아하진않는데 먹을게 그거뿐이어서요",
          "original_form": "좋아하진않는데 먹을게 그거뿐이어서요",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100037309.1.5",
          "form": "다른메뉴는 비싸고 달아서요 ㅜㅜ",
          "original_form": "다른메뉴는 비싸고 달아서요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037309.1.6",
          "form": "앗ㅠ 단거 안좋아하시는군요ㅠ",
          "original_form": "앗ㅠ 단거 안좋아하시는군요ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037309.1.7",
          "form": "네 살찔까봐서요 맛있기야하죠",
          "original_form": "네 살찔까봐서요 맛있기야하죠",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037309.1.8",
          "form": "아하ㅠ 관리하시는거군요",
          "original_form": "아하ㅠ 관리하시는거군요",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037309.1.9",
          "form": "대단하셔요",
          "original_form": "대단하셔요",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037309.1.10",
          "form": "저는 단거에 눈돌아가서..",
          "original_form": "저는 단거에 눈돌아가서..",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037309.1.11",
          "form": "그럼 스벅 프라푸치노 이런거 좋아하시나요?",
          "original_form": "그럼 스벅 프라푸치노 이런거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037309.1.12",
          "form": "화이트쵸코 좋아합니닷!",
          "original_form": "화이트쵸코 좋아합니닷!",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037309.1.13",
          "form": "커피는  별로 안마셔서요!",
          "original_form": "커피는  별로 안마셔서요!",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037309.1.14",
          "form": "아하 그러시군요 프라푸치노에도 커피 들어가긴 하죠",
          "original_form": "아하 그러시군요 프라푸치노에도 커피 들어가긴 하죠",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037309.1.15",
          "form": "네넴",
          "original_form": "네넴",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037309.1.16",
          "form": "그래도 커피맛 많이 안나서 가끔 피곤할때 마셔요!",
          "original_form": "그래도 커피맛 많이 안나서 가끔 피곤할때 마셔요!",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037309.1.17",
          "form": "그러시군요 저는 더울 때 먹어요",
          "original_form": "그러시군요 저는 더울 때 먹어요",
          "speaker_id": "1",
          "time": "20210831 22:50"
        }
