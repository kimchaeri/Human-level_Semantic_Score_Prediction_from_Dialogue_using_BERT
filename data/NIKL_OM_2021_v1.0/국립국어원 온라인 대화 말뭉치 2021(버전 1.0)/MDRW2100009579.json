      "utterance": [
        {
          "id": "MDRW2100009579.1.1",
          "form": "저는 딱히 잘하는건 없고",
          "original_form": "저는 딱히 잘하는건 없고",
          "speaker_id": "1",
          "time": "20210823 00:56"
        },
        {
          "id": "MDRW2100009579.1.2",
          "form": "테일즈런너라고 달리기 게임했었는데",
          "original_form": "테일즈런너라고 달리기 게임했었는데",
          "speaker_id": "1",
          "time": "20210823 00:56"
        },
        {
          "id": "MDRW2100009579.1.3",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009579.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.5",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.6",
          "form": "해보셨나요",
          "original_form": "해보셨나요",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.7",
          "form": "초등학교 때 중독이었어요",
          "original_form": "초등학교 때 중독이었어요",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.8",
          "form": "와 해보셨구나",
          "original_form": "와 해보셨구나",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.9",
          "form": "미친듯이 눌러라 다다다다다다다",
          "original_form": "미친듯이 눌러라 다다다다다다다",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.10",
          "form": "거기서 경쟁이란 무엇인가 깨우쳤습니다",
          "original_form": "거기서 경쟁이란 무엇인가 깨우쳤습니다",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.12",
          "form": "그 삐에로",
          "original_form": "그 삐에로",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.13",
          "form": "컨트롤키 겁나 눌러야해요",
          "original_form": "컨트롤키 겁나 눌러야해요",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.14",
          "form": "앙리3세",
          "original_form": "앙리3세",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.15",
          "form": "못생긴게 얼굴만떠다니고",
          "original_form": "못생긴게 얼굴만떠다니고",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009579.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009579.1.17",
          "form": "기억나요 앙리 3세",
          "original_form": "기억나요 앙리 3세",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009579.1.18",
          "form": "아직도 하세요?",
          "original_form": "아직도 하세요?",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009579.1.19",
          "form": "아니요 지금은 안하는데 가끔",
          "original_form": "아니요 지금은 안하는데 가끔",
          "speaker_id": "1",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009579.1.20",
          "form": "다다다다 떨어지다 대쉬 써지는 그느낌이 생각나서",
          "original_form": "다다다다 떨어지다 대쉬 써지는 그느낌이 생각나서",
          "speaker_id": "1",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009579.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009579.1.22",
          "form": "뭔지 알아요",
          "original_form": "뭔지 알아요",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
