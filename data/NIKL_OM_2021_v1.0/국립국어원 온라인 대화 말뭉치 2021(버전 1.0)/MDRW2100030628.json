          "speaker_id": "1",
          "time": "20210904 10:38"
        },
        {
          "id": "MDRW2100030628.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030628.1.5",
          "form": "알아요 ! ㅋㅋㅋㅋㅋㅋ",
          "original_form": "알아요 ! ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030628.1.6",
          "form": "부부만 같이사시나요",
          "original_form": "부부만 같이사시나요",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030628.1.7",
          "form": "아이도요",
          "original_form": "아이도요",
          "speaker_id": "1",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030628.1.8",
          "form": "그럼 엄마이신거 아닌가요",
