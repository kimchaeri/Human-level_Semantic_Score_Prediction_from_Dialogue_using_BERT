      "utterance": [
        {
          "id": "MDRW2100022945.1.1",
          "form": "뭐 다른 거 더 먹어",
          "original_form": "뭐 다른 거 더 먹어",
          "speaker_id": "2",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022945.1.2",
          "form": "흠.. 너구리 하나 끓여먹을까?",
          "original_form": "흠.. 너구리 하나 끓여먹을까?",
          "speaker_id": "1",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022945.1.3",
          "form": "ㅎㅎ 애들도 먹을건지 물어봐",
          "original_form": "ㅎㅎ 애들도 먹을건지 물어봐",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022945.1.4",
          "form": "라면 먹으면 당연히 먹겠지",
          "original_form": "라면 먹으면 당연히 먹겠지",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022945.1.5",
          "form": "그렇겠지? 엄청 좋아하니",
          "original_form": "그렇겠지? 엄청 좋아하니",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022945.1.6",
          "form": "색시도 먹을껴?",
          "original_form": "색시도 먹을껴?",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022945.1.7",
          "form": "아니 난 배불러서 안먹을래 ㅎㅎ",
          "original_form": "아니 난 배불러서 안먹을래 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022945.1.8",
          "form": "ㅇㅇ 그럼 세 개만 끓여야겠다",
          "original_form": "ㅇㅇ 그럼 세 개만 끓여야겠다",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022945.1.9",
          "form": "저녁 애매하게 먹어서 괜히 더 먹게 됐네 ㅎㅎ",
          "original_form": "저녁 애매하게 먹어서 괜히 더 먹게 됐네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022945.1.10",
          "form": "다이어트 점점 실패로ㅠ",
          "original_form": "다이어트 점점 실패로ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022945.1.11",
          "form": "어쩌겠어~ 그래도 배고프면 먹어야지 ㅎㅎ",
          "original_form": "어쩌겠어~ 그래도 배고프면 먹어야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022945.1.12",
          "form": "이제 우리집 남자들이 많이 먹어서",
          "original_form": "이제 우리집 남자들이 많이 먹어서",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022945.1.13",
          "form": "그러게 요리할 때 양을 늘려야겠어",
          "original_form": "그러게 요리할 때 양을 늘려야겠어",
          "speaker_id": "2",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022945.1.14",
          "form": "1박2일에 초계국수 나오는데 맛있어보인다",
