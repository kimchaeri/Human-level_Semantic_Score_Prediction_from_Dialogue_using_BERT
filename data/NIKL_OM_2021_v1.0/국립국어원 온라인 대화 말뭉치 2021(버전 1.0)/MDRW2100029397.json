        },
        {
          "id": "MDRW2100029397.1.17",
          "form": "외국에 비해 작고 아담하잖아요",
          "original_form": "외국에 비해 작고 아담하잖아요",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100029397.1.18",
