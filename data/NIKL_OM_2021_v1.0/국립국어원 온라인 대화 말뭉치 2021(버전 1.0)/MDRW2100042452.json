      "utterance": [
        {
          "id": "MDRW2100042452.1.1",
          "form": "언니 아들?",
          "original_form": "언니 아들?",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.2",
          "form": "그냥 막 말하고",
          "original_form": "그냥 막 말하고",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.3",
          "form": "아는 언니 아들",
          "original_form": "아는 언니 아들",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.4",
          "form": "일학년 선생님이요?",
          "original_form": "일학년 선생님이요?",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.5",
          "form": "일학년 때 선생님이 너무 무서워서",
          "original_form": "일학년 때 선생님이 너무 무서워서",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.6",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.7",
          "form": "숙제 안해 가면 손으로 머리 때리고 이래가지고",
          "original_form": "숙제 안해 가면 손으로 머리 때리고 이래가지고",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.8",
          "form": "근데 그거 진짜 커요",
          "original_form": "근데 그거 진짜 커요",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042452.1.9",
          "form": "애가 무조건 숙제해 가야 된다면서",
          "original_form": "애가 무조건 숙제해 가야 된다면서",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.10",
          "form": "나도 일학년 때 친구가 머리 맞는 거 봤음",
          "original_form": "나도 일학년 때 친구가 머리 맞는 거 봤음",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.11",
          "form": "아직도 그 선생님 생각하면 너무 무서워서",
          "original_form": "아직도 그 선생님 생각하면 너무 무서워서",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.12",
          "form": "나도 오줌 쌀 뻔 했잖아요",
          "original_form": "나도 오줌 쌀 뻔 했잖아요",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.13",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.15",
          "form": "나 별로 사람 잘 안 무서워하는데",
          "original_form": "나 별로 사람 잘 안 무서워하는데",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.16",
          "form": "그 선생님 얼굴 아직도 기억나요",
          "original_form": "그 선생님 얼굴 아직도 기억나요",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.17",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042452.1.18",
          "form": "사람이 너무 무서웠어요",
          "original_form": "사람이 너무 무서웠어요",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
