      "utterance": [
        {
          "id": "MDRW2100021242.1.1",
          "form": "언제쯤코로나가잠잠해질까요...ㅠㅠ",
          "original_form": "언제쯤코로나가잠잠해질까요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021242.1.2",
          "form": "그러게요ㅠㅠ 코로나이전에는",
          "original_form": "그러게요ㅠㅠ 코로나이전에는",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021242.1.3",
          "form": "이맘때쯤 항상 경주를 갔었는데요...",
          "original_form": "이맘때쯤 항상 경주를 갔었는데요...",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021242.1.4",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021242.1.5",
          "form": "오 경주 덥지 않아요??",
          "original_form": "오 경주 덥지 않아요??",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021242.1.6",
          "form": "덥긴한데 지금쯤가면 한낮빼고는 걷기좋아서 숙소에서 쉬다가 나오면 딱이예요",
          "original_form": "덥긴한데 지금쯤가면 한낮빼고는 걷기좋아서 숙소에서 쉬다가 나오면 딱이예요",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100021242.1.7",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021242.1.8",
          "form": "그거너무좋네요!!",
          "original_form": "그거너무좋네요!!",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021242.1.9",
          "form": "코로나잠잠해지면",
          "original_form": "코로나잠잠해지면",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021242.1.10",
          "form": "경주가봐야겟어요",
          "original_form": "경주가봐야겟어요",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021242.1.11",
          "form": "요즘은 사람이 많아졌긴한데 저도 또가고싶네요",
          "original_form": "요즘은 사람이 많아졌긴한데 저도 또가고싶네요",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021242.1.12",
          "form": "부산은 가본적 있으세요?",
          "original_form": "부산은 가본적 있으세요?",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021242.1.13",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021242.1.14",
          "form": "거의매년갑니당",
          "original_form": "거의매년갑니당",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021242.1.15",
          "form": "자주 가시는구나 어디 어디 가보셨어요?",
          "original_form": "자주 가시는구나 어디 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021242.1.16",
          "form": "할머니가부산사셔서",
          "original_form": "할머니가부산사셔서",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021242.1.17",
          "form": "막 어딜 돌아다녀 보진 않았어요ㅋㅋㅋ",
          "original_form": "막 어딜 돌아다녀 보진 않았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021242.1.18",
          "form": "해운대말곤",
          "original_form": "해운대말곤",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021242.1.19",
          "form": "거의 안 가본 것 같아요",
          "original_form": "거의 안 가본 것 같아요",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021242.1.20",
          "form": "그러시구나 저 부산사람이라",
          "original_form": "그러시구나 저 부산사람이라",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021242.1.21",
          "form": "좀 추천드릴까요?",
          "original_form": "좀 추천드릴까요?",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021242.1.22",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021242.1.23",
          "form": "네네!!너무좋죠!!",
          "original_form": "네네!!너무좋죠!!",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021242.1.24",
          "form": "요즘은 사람이 좀 많아지긴 했는데요",
          "original_form": "요즘은 사람이 좀 많아지긴 했는데요",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021242.1.25",
          "form": "흰여울길이라고 있는데 바다가 한눈에 보이는곳이라",
          "original_form": "흰여울길이라고 있는데 바다가 한눈에 보이는곳이라",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100021242.1.26",
          "form": "카페 경치가 너무 좋아요",
          "original_form": "카페 경치가 너무 좋아요",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100021242.1.27",
          "form": "어딜가도 경치가 좋은데 개인적으로는 에테르 추천드려요",
          "original_form": "어딜가도 경치가 좋은데 개인적으로는 에테르 추천드려요",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100021242.1.28",
          "form": "남포동에서 버스로 조금 더 가시면 돼요",
          "original_form": "남포동에서 버스로 조금 더 가시면 돼요",
          "speaker_id": "1",
          "time": "20210828 17:56"
        },
        {
          "id": "MDRW2100021242.1.29",
          "form": "오 방금 찾아봤는데",
          "original_form": "오 방금 찾아봤는데",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021242.1.30",
          "form": "진짜 뷰가",
          "original_form": "진짜 뷰가",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021242.1.31",
          "form": "끝내주네요!!",
          "original_form": "끝내주네요!!",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021242.1.32",
          "form": "좋은 정보 감사합니다!!",
          "original_form": "좋은 정보 감사합니다!!",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021242.1.33",
          "form": "해운대쪽은 해목이라는 장어덮밥집 있는데요 추천드려요",
          "original_form": "해운대쪽은 해목이라는 장어덮밥집 있는데요 추천드려요",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021242.1.34",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021242.1.35",
          "form": "장어 좋아하는데 감사해요",
          "original_form": "장어 좋아하는데 감사해요",
          "speaker_id": "2",
          "time": "20210828 17:58"
        }
      ]
