      "utterance": [
        {
          "id": "MDRW2100042534.1.1",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042534.1.2",
          "form": "거기에서 류수영 배우도 진짜 다정하고 좋더라고요",
          "original_form": "거기에서 류수영 배우도 진짜 다정하고 좋더라고요",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042534.1.3",
          "form": "진짜사나아죠 ㅎ 장혁은",
          "original_form": "진짜사나아죠 ㅎ 장혁은",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042534.1.4",
          "form": "ㅋㅋㅋ맞아요~~~!",
          "original_form": "ㅋㅋㅋ맞아요~~~!",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042534.1.5",
          "form": "장혁배우는 진짜사나이가 진짜 최고죠!",
          "original_form": "장혁배우는 진짜사나이가 진짜 최고죠!",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042534.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042534.1.7",
          "form": "박형식도 귀여워서 좋아랬는데",
          "original_form": "박형식도 귀여워서 좋아랬는데",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042534.1.8",
          "form": "지금은 뭘하는지 모르겠네요!!!",
          "original_form": "지금은 뭘하는지 모르겠네요!!!",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042534.1.9",
          "form": "그러게요 꾸준하 나오면 좋을텐데",
          "original_form": "그러게요 꾸준하 나오면 좋을텐데",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042534.1.10",
          "form": "맞아요! 혹시.. 군대갔나?",
          "original_form": "맞아요! 혹시.. 군대갔나?",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042534.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 군대갔을지도 모르겠네요ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 군대갔을지도 모르겠네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042534.1.12",
          "form": "그러게요 안조이네요",
          "original_form": "그러게요 안조이네요",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042534.1.13",
          "form": "그러니까요ㅜㅜ",
          "original_form": "그러니까요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042534.1.14",
          "form": "그럼 진짜사나이 다른 편도 보셨나요?",
          "original_form": "그럼 진짜사나이 다른 편도 보셨나요?",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042534.1.15",
          "form": "전 장혁배우 안나오자마자 안봤답니다..",
          "original_form": "전 장혁배우 안나오자마자 안봤답니다..",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042534.1.16",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042534.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 다 똑같나봐요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 다 똑같나봐요!",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042534.1.18",
          "form": "장혁만한 서나이를 못봤어요",
          "original_form": "장혁만한 서나이를 못봤어요",
