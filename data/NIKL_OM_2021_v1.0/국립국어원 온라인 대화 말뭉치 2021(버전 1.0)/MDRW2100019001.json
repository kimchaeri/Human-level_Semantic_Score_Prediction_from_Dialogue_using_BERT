        },
        {
          "id": "MDRW2100019001.1.2",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019001.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019001.1.4",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019001.1.5",
          "form": "오늘 밥 먹기 정말 힘들었어요ㅜㅜ",
          "original_form": "오늘 밥 먹기 정말 힘들었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019001.1.6",
          "form": "가는 식당 마다 재료 소진이나 영업을 안해서..",
          "original_form": "가는 식당 마다 재료 소진이나 영업을 안해서..",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019001.1.7",
          "form": "아이고.. 오늘 힘드셨네요ㅠㅠ",
          "original_form": "아이고.. 오늘 힘드셨네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019001.1.8",
          "form": "점심 맛있는거 드셨나요?",
          "original_form": "점심 맛있는거 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019001.1.9",
          "form": "원래 뭐 드시려고 하셨나요",
          "original_form": "원래 뭐 드시려고 하셨나요",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019001.1.10",
          "form": "전 아직 식전입니다",
          "original_form": "전 아직 식전입니다",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019001.1.11",
          "form": "배고프시겠어요",
          "original_form": "배고프시겠어요",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019001.1.12",
          "form": "원래는 햄버거집 가려했어요",
          "original_form": "원래는 햄버거집 가려했어요",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019001.1.13",
          "form": "약간 배고프긴 한데 괜찮아요",
          "original_form": "약간 배고프긴 한데 괜찮아요",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019001.1.14",
          "form": "그런데 뭐 드셨나요",
          "original_form": "그런데 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019001.1.15",
          "form": "햄버거집 덮밥집 백반집을 거쳐 결국 감자탕 먹었어요ㅋㅋ",
          "original_form": "햄버거집 덮밥집 백반집을 거쳐 결국 감자탕 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019001.1.16",
          "form": "긴 여정을 하셨네요ㅋㅋ",
          "original_form": "긴 여정을 하셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019001.1.17",
          "form": "감자탕은 어땠나요",
          "original_form": "감자탕은 어땠나요",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019001.1.18",
          "form": "그러게 말이에요. 이렇게 밥 먹기 힘든 적은 처음이네요ㅋㅋ",
          "original_form": "그러게 말이에요. 이렇게 밥 먹기 힘든 적은 처음이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019001.1.19",
          "form": "프랜차이즈 감자탕집이라 무난했어요",
          "original_form": "프랜차이즈 감자탕집이라 무난했어요",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019001.1.20",
          "form": "벌써 세신데 뭐 드셔야하는거 아니에요?",
          "original_form": "벌써 세신데 뭐 드셔야하는거 아니에요?",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019001.1.21",
          "form": "아 저 빵 먹으려고 픽업주문 넣어놨어요ㅋㅋ",
          "original_form": "아 저 빵 먹으려고 픽업주문 넣어놨어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019001.1.22",
          "form": "일있어서 점심이 늦어졌네요",
          "original_form": "일있어서 점심이 늦어졌네요",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019001.1.23",
          "form": "바쁘시네요ㅜㅜ 무슨 빵 드세요?",
          "original_form": "바쁘시네요ㅜㅜ 무슨 빵 드세요?",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019001.1.24",
          "form": "파리바게트 예약 걸어놨거든요",
          "original_form": "파리바게트 예약 걸어놨거든요",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019001.1.25",
          "form": "뽀드득 그릴소시지랑 식빵 먹으려구요",
          "original_form": "뽀드득 그릴소시지랑 식빵 먹으려구요",
          "speaker_id": "1",
          "time": "20210827 15:20"
        }
      ]
