      "utterance": [
        {
          "id": "MDRW2100023066.1.1",
          "form": "옹 그럼 경주 되게 잘맞으셧겟다",
          "original_form": "옹 그럼 경주 되게 잘맞으셧겟다",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023066.1.2",
          "form": "네네 어디 가보셨나요 국내#?",
          "original_form": "네네 어디 가보셨나요 국내#?",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023066.1.3",
          "form": "저는 군산이요 올해초에",
          "original_form": "저는 군산이요 올해초에",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023066.1.4",
          "form": "오오 유명한 빵집 잇잖아요",
          "original_form": "오오 유명한 빵집 잇잖아요",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023066.1.5",
          "form": "네ㅜ이성당인가 성심당인가",
          "original_form": "네ㅜ이성당인가 성심당인가",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023066.1.6",
          "form": "ㅋㅋㅋㅋㅋ가물가물하시나봐요",
          "original_form": "ㅋㅋㅋㅋㅋ가물가물하시나봐요",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023066.1.7",
          "form": "넼ㅋㅋㅋㅋ 쨋든 군산여행 재밋어요",
          "original_form": "넼ㅋㅋㅋㅋ 쨋든 군산여행 재밋어요",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023066.1.8",
          "form": "군산도 볼거리가 많나요?",
          "original_form": "군산도 볼거리가 많나요?",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023066.1.9",
          "form": "네 산이 많앗던 기억이 ㅋㅋㅋㅋㅋ",
          "original_form": "네 산이 많앗던 기억이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023066.1.10",
          "form": "네 군산도 약간 시골이 느낌이 났던거같아요 저도 옛날에 갓엇는데",
          "original_form": "네 군산도 약간 시골이 느낌이 났던거같아요 저도 옛날에 갓엇는데",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023066.1.11",
          "form": "ㅋㅋㅋㅋ네 사실 2일정도여행하기ㅜ딱좋앗어요",
          "original_form": "ㅋㅋㅋㅋ네 사실 2일정도여행하기ㅜ딱좋앗어요",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023066.1.12",
          "form": "저도 그정도 햇엇어요 ㅋㅋㅋ군산 재밋엇죠",
          "original_form": "저도 그정도 햇엇어요 ㅋㅋㅋ군산 재밋엇죠",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023066.1.13",
          "form": "네 빵도 냠냠하고 맛낫엉ㅌㅋㅌ",
          "original_form": "네 빵도 냠냠하고 맛낫엉ㅌㅋㅌ",
