      "utterance": [
        {
          "id": "MDRW2100031766.1.1",
          "form": "배달음식 많이 시켜 먹어?",
          "original_form": "배달음식 많이 시켜 먹어?",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100031766.1.2",
          "form": "아니 배민 깐적도없어",
          "original_form": "아니 배민 깐적도없어",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031766.1.3",
          "form": "우와ㅋㅋ 나도",
          "original_form": "우와ㅋㅋ 나도",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031766.1.4",
          "form": "요즘 배달음식 진짜 많이 시켜 먹잖아",
          "original_form": "요즘 배달음식 진짜 많이 시켜 먹잖아",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031766.1.5",
          "form": "굳이먹고싶으면 포장해 ㅇㅇ",
          "original_form": "굳이먹고싶으면 포장해 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031766.1.6",
          "form": "와 나랑 진짜 비슷하다.",
          "original_form": "와 나랑 진짜 비슷하다.",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031766.1.7",
          "form": "가족들도 배달음식 안좋아하기도하고",
          "original_form": "가족들도 배달음식 안좋아하기도하고",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031766.1.8",
          "form": "가족들이랑 그런 취향이 맞아서 부럽다~~",
          "original_form": "가족들이랑 그런 취향이 맞아서 부럽다~~",
          "speaker_id": "2",
          "time": "20210905 00:27"
        },
        {
          "id": "MDRW2100031766.1.9",
          "form": "같이 사는 사람이랑 음식 취향이 안 맞으면 많이 고통스럽더라고ㅠㅠ",
          "original_form": "같이 사는 사람이랑 음식 취향이 안 맞으면 많이 고통스럽더라고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 00:27"
        },
        {
          "id": "MDRW2100031766.1.10",
          "form": "넌 가족들이 배달음식좋아해?",
          "original_form": "넌 가족들이 배달음식좋아해?",
          "speaker_id": "1",
          "time": "20210905 00:29"
        },
        {
          "id": "MDRW2100031766.1.11",
          "form": "오빠가 배달음식을 진짜 자주 먹어.",
          "original_form": "오빠가 배달음식을 진짜 자주 먹어.",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031766.1.12",
          "form": "그래서 건강이 안 좋은데도 끼니때가 되니까 습관처럼 시켜 먹더라고.",
          "original_form": "그래서 건강이 안 좋은데도 끼니때가 되니까 습관처럼 시켜 먹더라고.",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031766.1.13",
          "form": "아...진짜 안좋은데 걱정되겠다",
          "original_form": "아...진짜 안좋은데 걱정되겠다",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031766.1.14",
          "form": "근데 요즘 20대들 거의 다 배달음식으로 끼니 때우는 것 같아.",
          "original_form": "근데 요즘 20대들 거의 다 배달음식으로 끼니 때우는 것 같아.",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031766.1.15",
          "form": "주변에 직접 해 먹는 경우가 더 드물긴 해~",
          "original_form": "주변에 직접 해 먹는 경우가 더 드물긴 해~",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031766.1.16",
          "form": "오 맞아 나중에 건강 훅갈텐데 20때부터 음식 건강하게 챙겨먹는거 좋은습관이다",
          "original_form": "오 맞아 나중에 건강 훅갈텐데 20때부터 음식 건강하게 챙겨먹는거 좋은습관이다",
          "speaker_id": "1",
          "time": "20210905 00:35"
        },
        {
          "id": "MDRW2100031766.1.17",
          "form": "맞아맞아 먹은 걸로 내 몸 만드는 거니까",
          "original_form": "맞아맞아 먹은 걸로 내 몸 만드는 거니까",
          "speaker_id": "2",
          "time": "20210905 00:36"
        }
      ]
