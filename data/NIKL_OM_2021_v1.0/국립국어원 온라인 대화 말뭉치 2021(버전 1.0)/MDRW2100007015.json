      "utterance": [
        {
          "id": "MDRW2100007015.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007015.1.2",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007015.1.3",
          "form": "네 좋아해요 ㅎㅎ",
          "original_form": "네 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007015.1.4",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007015.1.5",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007015.1.6",
          "form": "저는 주로 공포, 스릴러 좋아해요 ㅎㅎㅎ",
          "original_form": "저는 주로 공포, 스릴러 좋아해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007015.1.7",
          "form": "전 액션요",
          "original_form": "전 액션요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007015.1.8",
          "form": "공포도 잘봐요 스릴러도요",
          "original_form": "공포도 잘봐요 스릴러도요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007015.1.9",
          "form": "마블 영화도 좋아하고요",
          "original_form": "마블 영화도 좋아하고요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007015.1.10",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007015.1.11",
          "form": "무슨 영화 제일 재밌게 보셨나요?",
          "original_form": "무슨 영화 제일 재밌게 보셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007015.1.12",
          "form": "인생작?",
          "original_form": "인생작?",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007015.1.13",
          "form": "전 ㅋㅋ 마블영화 젤좋아해요",
          "original_form": "전 ㅋㅋ 마블영화 젤좋아해요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007015.1.14",
          "form": "인생작은 딱히 없어요",
          "original_form": "인생작은 딱히 없어요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007015.1.15",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007015.1.16",
          "form": "저는 인생작은 브레이브하트 라구",
          "original_form": "저는 인생작은 브레이브하트 라구",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007015.1.17",
          "form": "옛날 멜깁슨 영화 있어요..",
          "original_form": "옛날 멜깁슨 영화 있어요..",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007015.1.18",
          "form": "뭔가 가슴이 웅장해지는... ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭔가 가슴이 웅장해지는... ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007015.1.19",
          "form": "저는 마블은 어벤져스만 봤어요!",
          "original_form": "저는 마블은 어벤져스만 봤어요!",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007015.1.20",
          "form": "오 저도 생각나요",
          "original_form": "오 저도 생각나요",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007015.1.21",
          "form": "마지막 장면",
          "original_form": "마지막 장면",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007015.1.22",
          "form": "그 잔인함 속에서자유를 외치던",
          "original_form": "그 잔인함 속에서자유를 외치던",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007015.1.23",
          "form": "전 그정도 상황이면 상상도 못해요 배를",
          "original_form": "전 그정도 상황이면 상상도 못해요 배를",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007015.1.24",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007015.1.25",
          "form": "제 기억에도 또렷히 있네요",
          "original_form": "제 기억에도 또렷히 있네요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007015.1.26",
          "form": "또 무슨영화요?",
          "original_form": "또 무슨영화요?",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007015.1.27",
          "form": "저 말하면 다 알것 같아요 저도 감동했던 작품이예요",
          "original_form": "저 말하면 다 알것 같아요 저도 감동했던 작품이예요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007015.1.28",
          "form": "헉 대박 아시는군요",
          "original_form": "헉 대박 아시는군요",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007015.1.29",
          "form": "넘 반가워요 ㅠㅜ ㅎㅎ",
          "original_form": "넘 반가워요 ㅠㅜ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007015.1.30",
          "form": "네 마지막장면 전 정말 울며 봤어요",
          "original_form": "네 마지막장면 전 정말 울며 봤어요",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007015.1.31",
          "form": "지금도 소름이",
          "original_form": "지금도 소름이",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007015.1.32",
          "form": "네 그게 1992년? 그쯤 영화라",
          "original_form": "네 그게 1992년? 그쯤 영화라",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007015.1.33",
          "form": "그때 엄청 어릴때였는데도 잠못자고 본 것 같아요",
          "original_form": "그때 엄청 어릴때였는데도 잠못자고 본 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007015.1.34",
          "form": "약간 명예 스코틀랜드인처럼 같이 투지가 샘솟는 느낌..",
          "original_form": "약간 명예 스코틀랜드인처럼 같이 투지가 샘솟는 느낌..",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007015.1.35",
          "form": "아 그러셨군요 한참 뇌리에 남았어요",
          "original_form": "아 그러셨군요 한참 뇌리에 남았어요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007015.1.36",
          "form": "네 넷플에도 있던데",
          "original_form": "네 넷플에도 있던데",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007015.1.37",
          "form": "네 정말 대단한 사람이녔어요",
          "original_form": "네 정말 대단한 사람이녔어요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007015.1.38",
          "form": "막상 찜해놓고는 안보게 되더라구요..",
          "original_form": "막상 찜해놓고는 안보게 되더라구요..",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007015.1.39",
          "form": "전 기억이 그작품은 잘나네요",
          "original_form": "전 기억이 그작품은 잘나네요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007015.1.40",
          "form": "넷플이 좀 유독 집중이 안되는 것 같아요",
          "original_form": "넷플이 좀 유독 집중이 안되는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007015.1.41",
          "form": "원래 그래요 ㅋㅋ",
          "original_form": "원래 그래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007015.1.42",
          "form": "넷플이 요새 참그래요",
          "original_form": "넷플이 요새 참그래요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007015.1.43",
          "form": "볼것도 없고",
          "original_form": "볼것도 없고",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007015.1.44",
          "form": "넷플 화이트 타이거 이후에 본게 없네요",
          "original_form": "넷플 화이트 타이거 이후에 본게 없네요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007015.1.45",
          "form": "네 맞아요 ㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.46",
          "form": "잼났어요",
          "original_form": "잼났어요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.47",
          "form": "저도 맨날 결제만 하고..",
          "original_form": "저도 맨날 결제만 하고..",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.48",
          "form": "맨날 포스터만 10분 넘게 보다가",
          "original_form": "맨날 포스터만 10분 넘게 보다가",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.49",
          "form": "저도요 ㅡㅡ",
          "original_form": "저도요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.50",
          "form": "5분 보고 끄고..",
          "original_form": "5분 보고 끄고..",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.51",
          "form": "근데 뭔가 볼 거 같아서",
          "original_form": "근데 뭔가 볼 거 같아서",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.52",
          "form": "아 돈이 아까워요",
          "original_form": "아 돈이 아까워요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.53",
          "form": "끊기엔 아쉽구",
          "original_form": "끊기엔 아쉽구",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.54",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007015.1.55",
          "form": "미련이 ㅋㅋ",
          "original_form": "미련이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.56",
          "form": "맞아요 ㅋㅋㅋ 돈아까우니까 봐야지 하고",
          "original_form": "맞아요 ㅋㅋㅋ 돈아까우니까 봐야지 하고",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.57",
          "form": "또 반복 ㅋㅋㅋ",
          "original_form": "또 반복 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.58",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.59",
          "form": "화이트 타이거보셨어요?",
          "original_form": "화이트 타이거보셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.60",
          "form": "아니용 저한테는 추천을 안해줘서",
          "original_form": "아니용 저한테는 추천을 안해줘서",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.61",
          "form": "이번에 말씀하셔서 검색하고 알았어요!",
          "original_form": "이번에 말씀하셔서 검색하고 알았어요!",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.62",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.63",
          "form": "네 잼나요",
          "original_form": "네 잼나요",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007015.1.64",
          "form": "인도의 계급사회가 날것으로 보여져요",
          "original_form": "인도의 계급사회가 날것으로 보여져요",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007015.1.65",
          "form": "기억해둘게요",
          "original_form": "기억해둘게요",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007015.1.66",
