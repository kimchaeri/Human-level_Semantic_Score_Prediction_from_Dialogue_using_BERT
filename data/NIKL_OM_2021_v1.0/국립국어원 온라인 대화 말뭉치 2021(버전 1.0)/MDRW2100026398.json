      "utterance": [
        {
          "id": "MDRW2100026398.1.1",
          "form": "아 심즈ㅋㅋㅋ",
          "original_form": "아 심즈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026398.1.2",
          "form": "저도 심즈 저번에",
          "original_form": "저도 심즈 저번에",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026398.1.3",
          "form": "무료로 뿌렸을때 했었어요",
          "original_form": "무료로 뿌렸을때 했었어요",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026398.1.4",
          "form": "맨날 컨텐츠 추가돼서",
          "original_form": "맨날 컨텐츠 추가돼서",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026398.1.5",
          "form": "다살라면 부자되야겠더라구요 ㅋㅋㅋㅋ",
          "original_form": "다살라면 부자되야겠더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026398.1.6",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026398.1.7",
          "form": "컨텐츠가 맨날 추가되요",
          "original_form": "컨텐츠가 맨날 추가되요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026398.1.8",
          "form": "이것저것 다 사다보면 돈이 장난이 아닐거 같더라고요",
          "original_form": "이것저것 다 사다보면 돈이 장난이 아닐거 같더라고요",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026398.1.9",
          "form": "증말 똑똑해부려요 ㅋㅋ",
          "original_form": "증말 똑똑해부려요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026398.1.10",
          "form": "저 초딩때 심즈 1인가 나왔었는데",
          "original_form": "저 초딩때 심즈 1인가 나왔었는데",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026398.1.11",
          "form": "그때부터 게임에 빠져서",
          "original_form": "그때부터 게임에 빠져서",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026398.1.12",
          "form": "시력이 너무 나빠졌어요...",
          "original_form": "시력이 너무 나빠졌어요...",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026398.1.13",
          "form": "어이구ㅠㅠ",
          "original_form": "어이구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026398.1.14",
          "form": "게임의 또 다른 단점이에요 시력...",
          "original_form": "게임의 또 다른 단점이에요 시력...",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026398.1.15",
          "form": "라식수술 말릴정도로 나빠졌어요 ㅠㅠ",
          "original_form": "라식수술 말릴정도로 나빠졌어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026398.1.16",
          "form": "그런데도 끊을수 없는 게임 ㅋㅋㅋ",
          "original_form": "그런데도 끊을수 없는 게임 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026398.1.17",
          "form": "아이구야ㅠㅠ",
          "original_form": "아이구야ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026398.1.18",
          "form": "시력을 위해 안해야되는다는걸 알지만",
          "original_form": "시력을 위해 안해야되는다는걸 알지만",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026398.1.19",
          "form": "끊기 힘들죠",
          "original_form": "끊기 힘들죠",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026398.1.20",
          "form": "게임방송은 메이플만 보시는건가요?",
          "original_form": "게임방송은 메이플만 보시는건가요?",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026398.1.21",
          "form": "음...제가 보는 유튜버가 올리면 보는 편이라",
          "original_form": "음...제가 보는 유튜버가 올리면 보는 편이라",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026398.1.22",
          "form": "요즘 유행하는게임이면 거의 봤을거 같아요",
          "original_form": "요즘 유행하는게임이면 거의 봤을거 같아요",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026398.1.23",
          "form": "오 어느 유투버 좋아하시나요",
          "original_form": "오 어느 유투버 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026398.1.24",
          "form": "혹시 예랑가랑 아시나요...?",
          "original_form": "혹시 예랑가랑 아시나요...?",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026398.1.25",
          "form": "아 알죠알죠 부산사시던데 ㅎㅎ",
          "original_form": "아 알죠알죠 부산사시던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026398.1.26",
          "form": "냥이도 키우고",
          "original_form": "냥이도 키우고",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026398.1.27",
          "form": "그분들이 하신게임 영상들보는편인데",
          "original_form": "그분들이 하신게임 영상들보는편인데",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026398.1.28",
          "form": "리액션도 재미있어서 게임이 더 재미있어보여요",
          "original_form": "리액션도 재미있어서 게임이 더 재미있어보여요",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026398.1.29",
          "form": "아 약간 이상한게임 좋아하세요?",
          "original_form": "아 약간 이상한게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026398.1.30",
          "form": "오 저는 첨에 게임 좋아하신다고해서 남자분이신줄 알았어요 ㅎㅎ",
          "original_form": "오 저는 첨에 게임 좋아하신다고해서 남자분이신줄 알았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026398.1.31",
          "form": "예랑가랑 얘기들으니 왠지 여자분이실듯 ㅋㅋ",
          "original_form": "예랑가랑 얘기들으니 왠지 여자분이실듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026398.1.32",
          "form": "어떤 게임인가요?",
          "original_form": "어떤 게임인가요?",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026398.1.33",
          "form": "항아리게임이나 토끼둘이 합동해서 앞으로 나아가는 게임 이런게임 영상들 많이 봐서",
          "original_form": "항아리게임이나 토끼둘이 합동해서 앞으로 나아가는 게임 이런게임 영상들 많이 봐서",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026398.1.34",
          "form": "이런분야게임은 잘알아요ㅋㅋㅋ",
          "original_form": "이런분야게임은 잘알아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026398.1.35",
          "form": "항아리게임은 게임게 최고봉이죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "항아리게임은 게임게 최고봉이죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026398.1.36",
          "form": "똥겜 굿굿",
          "original_form": "똥겜 굿굿",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026398.1.37",
          "form": "저는 그게임으로",
          "original_form": "저는 그게임으로",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026398.1.38",
          "form": "풍월량 알고 계속 그 방송 봐요 트위치에서",
          "original_form": "풍월량 알고 계속 그 방송 봐요 트위치에서",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026398.1.39",
          "form": "항아리게임은 너무...웃겨요ㅋㅋㅋㅋ",
          "original_form": "항아리게임은 너무...웃겨요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026398.1.40",
          "form": "마리오고양이 게임도 아시나요?",
          "original_form": "마리오고양이 게임도 아시나요?",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026398.1.41",
          "form": "그 빡치는 게임맞나요 ㅋㅋㅋㅋㅋ",
          "original_form": "그 빡치는 게임맞나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:27"
        }
      ]
    }
  ]
