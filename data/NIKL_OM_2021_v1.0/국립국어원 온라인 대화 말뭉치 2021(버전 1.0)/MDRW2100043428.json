      "utterance": [
        {
          "id": "MDRW2100043428.1.1",
          "form": "백신맞고 아파도",
          "original_form": "백신맞고 아파도",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043428.1.2",
          "form": "맛있는거 많이드세요",
          "original_form": "맛있는거 많이드세요",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043428.1.3",
          "form": "더 많이드세요",
          "original_form": "더 많이드세요",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043428.1.4",
          "form": "저녁 뭐드셨나요?",
          "original_form": "저녁 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043428.1.5",
          "form": "밀면먹구 비요뜨먹구있습니다!   \r\n저녁뭐드시나용",
          "original_form": "밀면먹구 비요뜨먹구있습니다!   \r\n저녁뭐드시나용",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043428.1.6",
          "form": "전지금 불닭볶음면 제조중이에요",
          "original_form": "전지금 불닭볶음면 제조중이에요",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043428.1.7",
          "form": "치즈도 넣을거랍니다!",
          "original_form": "치즈도 넣을거랍니다!",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043428.1.8",
          "form": "오 배우신분ㅋㅋㄱ조합잘하셧군용",
          "original_form": "오 배우신분ㅋㅋㄱ조합잘하셧군용",
          "speaker_id": "2",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043428.1.9",
          "form": "불닭치즈를 먹었답니다",
          "original_form": "불닭치즈를 먹었답니다",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043428.1.10",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043428.1.11",
          "form": "불닭먹은지도오래됐네요",
          "original_form": "불닭먹은지도오래됐네요",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043428.1.12",
          "form": "한번씩꼭생각나는라면!!",
          "original_form": "한번씩꼭생각나는라면!!",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043428.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043428.1.14",
          "form": "전 매일 생각난답니다(?)",
          "original_form": "전 매일 생각난답니다(?)",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043428.1.15",
          "form": "중독되셨군요ㅋㅋㅋ",
          "original_form": "중독되셨군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043428.1.16",
          "form": "맞아요 매운맛이 중독성이 강해요",
          "original_form": "맞아요 매운맛이 중독성이 강해요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043428.1.17",
          "form": "다른변형된맛들도다맛있구불닭최고",
          "original_form": "다른변형된맛들도다맛있구불닭최고",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043428.1.18",
          "form": "불닭시리즈중에 가장 좋아하는 것 어떤건가요?",
          "original_form": "불닭시리즈중에 가장 좋아하는 것 어떤건가요?",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043428.1.19",
          "form": "저는까르보요ㅋㅋㅋㅋ",
          "original_form": "저는까르보요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043428.1.20",
          "form": "헉 저도까르보 좋아해요",
          "original_form": "헉 저도까르보 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043428.1.21",
          "form": "까르보에 감동란 같이먹으면 존맛이에요ㅎㅎ",
          "original_form": "까르보에 감동란 같이먹으면 존맛이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:52"
        }
      ]
