      "utterance": [
        {
          "id": "MDRW2100024869.1.1",
          "form": "이제부터",
          "original_form": "이제부터",
          "speaker_id": "2",
          "time": "20210831 20:01"
        },
        {
          "id": "MDRW2100024869.1.2",
          "form": "저녁을 굶어야겠어요!!",
          "original_form": "저녁을 굶어야겠어요!!",
          "speaker_id": "2",
          "time": "20210831 20:01"
        },
        {
          "id": "MDRW2100024869.1.3",
          "form": "살이 너무 많이 찐듰!!",
          "original_form": "살이 너무 많이 찐듰!!",
          "speaker_id": "2",
          "time": "20210831 20:01"
        },
        {
          "id": "MDRW2100024869.1.4",
          "form": "뭐가 쪄",
          "original_form": "뭐가 쪄",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100024869.1.5",
          "form": "요즘 몸무게가 너무 많이 나가,ㅠㅠㅠ",
          "original_form": "요즘 몸무게가 너무 많이 나가,ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024869.1.6",
          "form": "그냥 적게 먹으면 되지~",
          "original_form": "그냥 적게 먹으면 되지~",
          "speaker_id": "1",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024869.1.7",
          "form": "아냐~ 공복시간을 좀 늘려야겠어요!!",
          "original_form": "아냐~ 공복시간을 좀 늘려야겠어요!!",
          "speaker_id": "2",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024869.1.8",
          "form": "몇시간??",
          "original_form": "몇시간??",
          "speaker_id": "1",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024869.1.9",
          "form": "16시간 공복 유지해야 그나마 좋대요~",
          "original_form": "16시간 공복 유지해야 그나마 좋대요~",
          "speaker_id": "2",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024869.1.10",
          "form": "그럼 몇시부터 안 먹을건데??",
          "original_form": "그럼 몇시부터 안 먹을건데??",
          "speaker_id": "1",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024869.1.11",
          "form": "6시? 7시?",
          "original_form": "6시? 7시?",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024869.1.12",
          "form": "그럼 아침은?",
          "original_form": "그럼 아침은?",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024869.1.13",
          "form": "그후로 16시간 후에~",
          "original_form": "그후로 16시간 후에~",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024869.1.14",
          "form": "10시나 11시",
          "original_form": "10시나 11시",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024869.1.15",
          "form": "아침이 너무 늦는 거 아닌가???",
          "original_form": "아침이 너무 늦는 거 아닌가???",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024869.1.16",
          "form": "그럼 저녁을 엄청 일찍 먹어야 할듯한데,,,",
          "original_form": "그럼 저녁을 엄청 일찍 먹어야 할듯한데,,,",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024869.1.17",
          "form": "그냥 하루 두끼 먹는게 젤 간편할듯~",
          "original_form": "그냥 하루 두끼 먹는게 젤 간편할듯~",
          "speaker_id": "2",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100024869.1.18",
          "form": "일하는 사람이 잘 먹어야지!!",
          "original_form": "일하는 사람이 잘 먹어야지!!",
          "speaker_id": "1",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100024869.1.19",
          "form": "하루종일 앉아서 일만하는데 많이 먹으면 속도 더부룩하고,,",
          "original_form": "하루종일 앉아서 일만하는데 많이 먹으면 속도 더부룩하고,,",
          "speaker_id": "2",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100024869.1.20",
          "form": "한끼를 조금씩 먹어",
          "original_form": "한끼를 조금씩 먹어",
          "speaker_id": "1",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100024869.1.21",
          "form": "그게 제일 어렵다..ㅠㅠㅠ",
          "original_form": "그게 제일 어렵다..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:05"
        }
      ]
