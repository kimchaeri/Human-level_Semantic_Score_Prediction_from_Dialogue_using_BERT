      "utterance": [
        {
          "id": "MDRW2100029471.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100029471.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100029471.1.3",
          "form": "스포츠 보시는 거 있나요",
          "original_form": "스포츠 보시는 거 있나요",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100029471.1.4",
          "form": "배구랑 농구 봐요",
          "original_form": "배구랑 농구 봐요",
          "speaker_id": "1",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100029471.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029471.1.6",
          "form": "다섯개님은요?",
          "original_form": "다섯개님은요?",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100029471.1.7",
          "form": "안타깝게도",
          "original_form": "안타깝게도",
          "speaker_id": "2",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100029471.1.8",
          "form": "배구랑 농구는 아는게 없네요;;",
          "original_form": "배구랑 농구는 아는게 없네요;;",
          "speaker_id": "2",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100029471.1.9",
          "form": "ㅎㅎ 모르실 수도 있죠",
          "original_form": "ㅎㅎ 모르실 수도 있죠",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100029471.1.10",
          "form": "인기가 많은 스포츠는 아니니까요",
          "original_form": "인기가 많은 스포츠는 아니니까요",
          "speaker_id": "1",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100029471.1.11",
          "form": "그 김연경 선수는",
          "original_form": "그 김연경 선수는",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100029471.1.12",
          "form": "올해 모르는 사람이 없을 정도라는 거 정도랑",
          "original_form": "올해 모르는 사람이 없을 정도라는 거 정도랑",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100029471.1.13",
          "form": "올초 불미스러운 사안 정도만",
          "original_form": "올초 불미스러운 사안 정도만",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100029471.1.14",
          "form": "아는지라;;",
          "original_form": "아는지라;;",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100029471.1.15",
          "form": "그건 모두 아시는군요",
          "original_form": "그건 모두 아시는군요",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029471.1.16",
          "form": "저는 그 사안 덕분에 이번 국대가 잘했다고 생각해요",
          "original_form": "저는 그 사안 덕분에 이번 국대가 잘했다고 생각해요",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029471.1.17",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029471.1.18",
          "form": "없어도 된다",
          "original_form": "없어도 된다",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029471.1.19",
          "form": "라고 보여줄 수 있었다고들",
          "original_form": "라고 보여줄 수 있었다고들",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029471.1.20",
          "form": "말하더라고요",
          "original_form": "말하더라고요",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029471.1.21",
          "form": "맞아요 그리고 그 두명 때문에 불협화음이 있었다고 할까요",
          "original_form": "맞아요 그리고 그 두명 때문에 불협화음이 있었다고 할까요",
          "speaker_id": "1",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029471.1.22",
          "form": "흠흠",
          "original_form": "흠흠",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029471.1.23",
          "form": "여튼 팬 입장에서는",
          "original_form": "여튼 팬 입장에서는",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029471.1.24",
          "form": "마냥 달가운 사안은 아니었을텐데",
          "original_form": "마냥 달가운 사안은 아니었을텐데",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029471.1.25",
          "form": "팬들은 아직도 아니라고 잡아떼더라구요",
          "original_form": "팬들은 아직도 아니라고 잡아떼더라구요",
          "speaker_id": "1",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100029471.1.26",
          "form": "그리스로 도주한거 보면 모르나...ㅎㅎ",
          "original_form": "그리스로 도주한거 보면 모르나...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:06"
        }
      ]
