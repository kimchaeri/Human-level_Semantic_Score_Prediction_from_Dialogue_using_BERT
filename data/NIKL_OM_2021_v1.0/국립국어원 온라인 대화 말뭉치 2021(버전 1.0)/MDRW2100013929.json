      "utterance": [
        {
          "id": "MDRW2100013929.1.1",
          "form": "애완동물 키우세요?",
          "original_form": "애완동물 키우세요?",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013929.1.2",
          "form": "아뇨.. 안키워요.. \n뭐 키우시남여?",
          "original_form": "아뇨.. 안키워요.. \n뭐 키우시남여?",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013929.1.3",
          "form": "아...저는 거북이 한마리요",
          "original_form": "아...저는 거북이 한마리요",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013929.1.4",
          "form": "강아지좋아하세요?",
          "original_form": "강아지좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013929.1.5",
          "form": "어쩌다 거북이를?",
          "original_form": "어쩌다 거북이를?",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013929.1.6",
          "form": "전 털달린거 다 좋아해요..",
          "original_form": "전 털달린거 다 좋아해요..",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013929.1.7",
          "form": "멍멍이 야옹이 토끼 사자.. 공룡..ㅋㅋㅋ",
          "original_form": "멍멍이 야옹이 토끼 사자.. 공룡..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013929.1.8",
          "form": "아 네 전 강아지 무서워해요",
          "original_form": "아 네 전 강아지 무서워해요",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013929.1.9",
          "form": "물까봐요 ㅠ",
          "original_form": "물까봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013929.1.10",
          "form": "진짜요? 작은거도요?",
          "original_form": "진짜요? 작은거도요?",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013929.1.11",
          "form": "네 작은강아지도 무서워요 못만져요",
          "original_form": "네 작은강아지도 무서워요 못만져요",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013929.1.12",
          "form": "고양이는 눈이무섭고.. 너무재빨라서 식탁위에도 오르네 혼이빠지구요",
          "original_form": "고양이는 눈이무섭고.. 너무재빨라서 식탁위에도 오르네 혼이빠지구요",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013929.1.13",
          "form": "어릴때 트라우마 그런거 아니고 그냥 무서워요?",
          "original_form": "어릴때 트라우마 그런거 아니고 그냥 무서워요?",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100013929.1.14",
          "form": "아네 ㅜㅜ 저도 강아지 잘 안을수있으면 좋겠어요",
          "original_form": "아네 ㅜㅜ 저도 강아지 잘 안을수있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100013929.1.15",
          "form": "그럼 조류는요? 앵무새 같은거",
          "original_form": "그럼 조류는요? 앵무새 같은거",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100013929.1.16",
          "form": "무섭죠 ㅜㅜ 비둘기는 싫어해요 \n조류좋아하세요?",
          "original_form": "무섭죠 ㅜㅜ 비둘기는 싫어해요 \n조류좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100013929.1.17",
          "form": "앵무새나 펭귄은 귀엽던데.. \n갑각류 빼곤 다 좋아하는 편이에요",
          "original_form": "앵무새나 펭귄은 귀엽던데.. \n갑각류 빼곤 다 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100013929.1.18",
          "form": "아 네.. 좋은거죠..",
          "original_form": "아 네.. 좋은거죠..",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100013929.1.19",
          "form": "저는 소극적이라서요",
          "original_form": "저는 소극적이라서요",
          "speaker_id": "1",
