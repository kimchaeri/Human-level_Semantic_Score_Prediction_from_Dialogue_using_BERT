      "utterance": [
        {
          "id": "MDRW2100043158.1.1",
          "form": "아직 왜 3시냐",
          "original_form": "아직 왜 3시냐",
          "speaker_id": "1",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043158.1.2",
          "form": "그니까 시간진짜안가",
          "original_form": "그니까 시간진짜안가",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043158.1.3",
          "form": "나아까 민원들어와가지고 ㅠㅠ 스트레스야",
          "original_form": "나아까 민원들어와가지고 ㅠㅠ 스트레스야",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043158.1.4",
          "form": "무슨민원인데?",
          "original_form": "무슨민원인데?",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043158.1.5",
          "form": "요즘 엄청 잦아...그리고 다들 화가나있어...",
          "original_form": "요즘 엄청 잦아...그리고 다들 화가나있어...",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043158.1.6",
          "form": "냄새민원 근데 멀해도 안잡혀",
          "original_form": "냄새민원 근데 멀해도 안잡혀",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043158.1.7",
          "form": "이게 우리시설인지 타시설인지도 모르겟음",
          "original_form": "이게 우리시설인지 타시설인지도 모르겟음",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043158.1.8",
          "form": "근처에도 다른 시설이있어?",
          "original_form": "근처에도 다른 시설이있어?",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043158.1.9",
          "form": "전화와서는 어디다 민원넣으면 되녜... 너희시설 민원을 제가 어디다 넣으라고 말해야하나요...",
          "original_form": "전화와서는 어디다 민원넣으면 되녜... 너희시설 민원을 제가 어디다 넣으라고 말해야하나요...",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043158.1.10",
          "form": "웅웅 의심시설 몇군데있긴하지",
          "original_form": "웅웅 의심시설 몇군데있긴하지",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043158.1.11",
          "form": "그냥 지하화 햇어야돼...근데 그래도 냄새 못잡을듯",
          "original_form": "그냥 지하화 햇어야돼...근데 그래도 냄새 못잡을듯",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043158.1.12",
          "form": "집에가고싶다",
          "original_form": "집에가고싶다",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100043158.1.13",
          "form": "와 진짜 너네는 그런 민원땜에 스트레스받겠다;;;;",
          "original_form": "와 진짜 너네는 그런 민원땜에 스트레스받겠다;;;;",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043158.1.14",
          "form": "주변 민간인이그러는거야?",
          "original_form": "주변 민간인이그러는거야?",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043158.1.15",
          "form": "아니 업체들?...산업단지도 있고 아파트도 ㅠ",
          "original_form": "아니 업체들?...산업단지도 있고 아파트도 ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043158.1.16",
          "form": "근데 우리시설이 먼저였고...요근래 부쩍많아졋어",
          "original_form": "근데 우리시설이 먼저였고...요근래 부쩍많아졋어",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043158.1.17",
          "form": "원래 민원진짜 한두건? 올해 갑자기..ㅠ.ㅋ..",
          "original_form": "원래 민원진짜 한두건? 올해 갑자기..ㅠ.ㅋ..",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043158.1.18",
          "form": "애초에지을때피해서짓지그랬어요 ㅜㅜ",
          "original_form": "애초에지을때피해서짓지그랬어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043158.1.19",
          "form": "그니까요...",
          "original_form": "그니까요...",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043158.1.20",
          "form": "모르겠고..퇴근하고싶다...",
          "original_form": "모르겠고..퇴근하고싶다...",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043158.1.21",
          "form": "나도진짜퇴근하고싶다",
          "original_form": "나도진짜퇴근하고싶다",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043158.1.22",
          "form": "퇴근하고뭐할거야",
          "original_form": "퇴근하고뭐할거야",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043158.1.23",
          "form": "밥먹고 축구보고 잘거에용..",
          "original_form": "밥먹고 축구보고 잘거에용..",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043158.1.24",
          "form": "밥뭐먹을건데??",
          "original_form": "밥뭐먹을건데??",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043158.1.25",
          "form": "아직모르겠어...엄빠가 집에서 밥먹어야...",
          "original_form": "아직모르겠어...엄빠가 집에서 밥먹어야...",
          "speaker_id": "2",
          "time": "20210907 15:17"
        }
      ]
