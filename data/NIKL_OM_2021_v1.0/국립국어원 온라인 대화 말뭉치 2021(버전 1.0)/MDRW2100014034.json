      "utterance": [
        {
          "id": "MDRW2100014034.1.1",
          "form": "혹시 싸이월드 하셨었나요?",
          "original_form": "혹시 싸이월드 하셨었나요?",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014034.1.2",
          "form": "아 옛날에 했었죠ㅎㅎ",
          "original_form": "아 옛날에 했었죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014034.1.3",
          "form": "저도 열심히 했는데 ㅎㅎ",
          "original_form": "저도 열심히 했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014034.1.4",
          "form": "그 이번에 다시 생기는거 들어가보셨나요?",
          "original_form": "그 이번에 다시 생기는거 들어가보셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014034.1.5",
          "form": "어 들어는 봤어요 언젠진 잘 모르겠지만",
          "original_form": "어 들어는 봤어요 언젠진 잘 모르겠지만",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014034.1.6",
          "form": "다시 생기면 가입하려고요!",
          "original_form": "다시 생기면 가입하려고요!",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014034.1.7",
          "form": "저도 다시 하려구요! 이번에 들어가봤는데",
          "original_form": "저도 다시 하려구요! 이번에 들어가봤는데",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014034.1.8",
          "form": "15년도 이후에 로그인 안해서",
          "original_form": "15년도 이후에 로그인 안해서",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014034.1.9",
          "form": "안봐지더라구요",
          "original_form": "안봐지더라구요",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014034.1.10",
          "form": "뭔가 인스타가 요즘 너무 광고무더기가 돼서 새로운 sns 바라던 참이엇는데 반갑더라고요",
          "original_form": "뭔가 인스타가 요즘 너무 광고무더기가 돼서 새로운 sns 바라던 참이엇는데 반갑더라고요",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014034.1.11",
          "form": "헐 맞아요 !",
          "original_form": "헐 맞아요 !",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014034.1.12",
          "form": "그리고 뭔가 예전에 싸이월드 감성이",
          "original_form": "그리고 뭔가 예전에 싸이월드 감성이",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014034.1.13",
          "form": "그립달까 ㅎㅎ 그 bgm도 다시 사고싶네요",
          "original_form": "그립달까 ㅎㅎ 그 bgm도 다시 사고싶네요",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014034.1.14",
          "form": "맞아요 재밌었는데 홈피 꾸미고 파도타고",
          "original_form": "맞아요 재밌었는데 홈피 꾸미고 파도타고",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014034.1.15",
          "form": "저는 지금와서 말하기 부끄럽지만 좀 쿨한 컨셉 밀어가지고",
          "original_form": "저는 지금와서 말하기 부끄럽지만 좀 쿨한 컨셉 밀어가지고",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014034.1.16",
          "form": "다행히 딱히 흑역사는 없어요",
          "original_form": "다행히 딱히 흑역사는 없어요",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014034.1.17",
          "form": "아 쿨한 컨셉 ㅋㅋㅋ 뭔지 알거같아요",
          "original_form": "아 쿨한 컨셉 ㅋㅋㅋ 뭔지 알거같아요",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014034.1.18",
          "form": "저도 가끔 잠수중 적어놓고",
          "original_form": "저도 가끔 잠수중 적어놓고",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014034.1.19",
          "form": "다이어리 빼고 다 비공개로 하고",
          "original_form": "다이어리 빼고 다 비공개로 하고",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014034.1.20",
          "form": "그랬었어욬ㄱㅋ",
          "original_form": "그랬었어욬ㄱㅋ",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014034.1.21",
          "form": "아 저랑 컨셉 비슷하셨네ㅋㅋㅋㅋ",
          "original_form": "아 저랑 컨셉 비슷하셨네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014034.1.22",
          "form": "저도 막 비공개해놨었는데",
          "original_form": "저도 막 비공개해놨었는데",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014034.1.23",
          "form": "ㅋㅋㅋㅋ그땐 왜 그랬나 모르겠어욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그땐 왜 그랬나 모르겠어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014034.1.24",
          "form": "근데 또 다시 생기면 유행할거 같지 않나요?",
          "original_form": "근데 또 다시 생기면 유행할거 같지 않나요?",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014034.1.25",
          "form": "ㅋㅋㅋㅋㅋ그럴것 같아요 근데 곧 나올줄 알았는데 늦네요",
          "original_form": "ㅋㅋㅋㅋㅋ그럴것 같아요 근데 곧 나올줄 알았는데 늦네요",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014034.1.26",
          "form": "복구하는게 시간이 오래걸리나봐요",
          "original_form": "복구하는게 시간이 오래걸리나봐요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014034.1.27",
          "form": "예전부터 부활한다 한거같은데",
          "original_form": "예전부터 부활한다 한거같은데",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014034.1.28",
          "form": "막상 들어가보면 안봐지고 ㅋㅋㅋ",
          "original_form": "막상 들어가보면 안봐지고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014034.1.29",
          "form": "ㅋㅋㅋㅋ생각보다 원활하게 뭐가 잘 안되나보네요",
          "original_form": "ㅋㅋㅋㅋ생각보다 원활하게 뭐가 잘 안되나보네요",
          "speaker_id": "2",
          "time": "20210825 17:44"
        }
      ]
