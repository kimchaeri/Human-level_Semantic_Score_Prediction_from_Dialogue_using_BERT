      "utterance": [
        {
          "id": "MDRW2100004268.1.1",
          "form": "최근에 책 읽은 거 있어?",
          "original_form": "최근에 책 읽은 거 있어?",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004268.1.2",
          "form": "시집 읽어봤어",
          "original_form": "시집 읽어봤어",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004268.1.3",
          "form": "최영미 시인의 다시 오지 않는 것들",
          "original_form": "최영미 시인의 다시 오지 않는 것들",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004268.1.4",
          "form": "이벤트로 받아서 읽어봤는데",
          "original_form": "이벤트로 받아서 읽어봤는데",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004268.1.5",
          "form": "책 언제 읽었대 짱이다",
          "original_form": "책 언제 읽었대 짱이다",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004268.1.6",
          "form": "시집은 처음 읽어봤는데",
          "original_form": "시집은 처음 읽어봤는데",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004268.1.7",
          "form": "담담하고 간결하고 차분하고...",
          "original_form": "담담하고 간결하고 차분하고...",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004268.1.8",
          "form": "평소엔 커뮤니티에서 소통위주의 텍스트를 많이 읽는데",
          "original_form": "평소엔 커뮤니티에서 소통위주의 텍스트를 많이 읽는데",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004268.1.9",
          "form": "색다른 경험이었어",
          "original_form": "색다른 경험이었어",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004268.1.10",
          "form": "좋더라",
          "original_form": "좋더라",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004268.1.11",
          "form": "나도 최근에 책 읽긴 읽었어",
          "original_form": "나도 최근에 책 읽긴 읽었어",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004268.1.12",
          "form": "만들어진 신",
          "original_form": "만들어진 신",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004268.1.13",
          "form": "근데 완독은 못했어",
          "original_form": "근데 완독은 못했어",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004268.1.14",
          "form": "오 무슨 내용이야?",
          "original_form": "오 무슨 내용이야?",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004268.1.15",
          "form": "그 글쓴 사람이 불신자라 왜 신이 없는지 여러분야에서 오목조목 하나부터 열까지 다 따진 책인데 얼마 못읽었삼",
          "original_form": "그 글쓴 사람이 불신자라 왜 신이 없는지 여러분야에서 오목조목 하나부터 열까지 다 따진 책인데 얼마 못읽었삼",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004268.1.16",
          "form": "아직 책 읽는 습관이 안들어서 그런가봐 다시 붙잡기가 힘들다",
          "original_form": "아직 책 읽는 습관이 안들어서 그런가봐 다시 붙잡기가 힘들다",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004268.1.17",
          "form": "흥미롭다",
          "original_form": "흥미롭다",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004268.1.18",
          "form": "재밌을 것 같아",
          "original_form": "재밌을 것 같아",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004268.1.19",
          "form": "읽어보고 싶네",
          "original_form": "읽어보고 싶네",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004268.1.20",
          "form": "집 근처에 도서관 있으면 가서 회원증 만들고 빌려와",
          "original_form": "집 근처에 도서관 있으면 가서 회원증 만들고 빌려와",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004268.1.21",
          "form": "최근에 도서관 가보기 시작했는데 되게 좋은 것 같음",
          "original_form": "최근에 도서관 가보기 시작했는데 되게 좋은 것 같음",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004268.1.22",
          "form": "도서관은 있지",
          "original_form": "도서관은 있지",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004268.1.23",
          "form": "거기서 알바도 했었는데",
          "original_form": "거기서 알바도 했었는데",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004268.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004268.1.25",
          "form": "나도 담에 빌려봐야겠다",
          "original_form": "나도 담에 빌려봐야겠다",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004268.1.26",
          "form": "언제 도서관에서 알바했어???",
          "original_form": "언제 도서관에서 알바했어???",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004268.1.27",
          "form": "도서관 홈페이지 가서 책 있는지 검색해보삼",
          "original_form": "도서관 홈페이지 가서 책 있는지 검색해보삼",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.28",
          "form": "대학생때 방학에",
          "original_form": "대학생때 방학에",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.29",
          "form": "시청알바 했었잖아",
          "original_form": "시청알바 했었잖아",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.30",
          "form": "아니면 요즘 이북으로 빌릴 수 있던데",
          "original_form": "아니면 요즘 이북으로 빌릴 수 있던데",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.31",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.32",
          "form": "아 헐 시청알바 도서관이었어?",
          "original_form": "아 헐 시청알바 도서관이었어?",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.33",
          "form": "이북 좋은데",
          "original_form": "이북 좋은데",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.34",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.35",
          "form": "한번 확인해보삼",
          "original_form": "한번 확인해보삼",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.36",
          "form": "이북 뭐 도서관 어플 깔아야하나?",
          "original_form": "이북 뭐 도서관 어플 깔아야하나?",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.37",
          "form": "고고",
          "original_form": "고고",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.38",
          "form": "함 봐야겠다",
          "original_form": "함 봐야겠다",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004268.1.39",
          "form": "이북으로 대여해주는 거 되게 좋다",
          "original_form": "이북으로 대여해주는 거 되게 좋다",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004268.1.40",
          "form": "아니 일단 가서 확인해보삼 있으면 깔아",
          "original_form": "아니 일단 가서 확인해보삼 있으면 깔아",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004268.1.41",
          "form": "지역 도서관 회원이여?",
          "original_form": "지역 도서관 회원이여?",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004268.1.42",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004268.1.43",
          "form": "알바할때 만들었어 회원증",
          "original_form": "알바할때 만들었어 회원증",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004268.1.44",
          "form": "오 그럼 들어가봐 한번 아마 왠만하면 이북 빌릴 수 있을껄",
          "original_form": "오 그럼 들어가봐 한번 아마 왠만하면 이북 빌릴 수 있을껄",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004268.1.45",
          "form": "근데 책 진짜 두꺼워서 다 읽으려면 한참 걸릴듯...",
          "original_form": "근데 책 진짜 두꺼워서 다 읽으려면 한참 걸릴듯...",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004268.1.46",
          "form": "어렸을땐 책 진짜 많이 읽었었는데",
          "original_form": "어렸을땐 책 진짜 많이 읽었었는데",
          "speaker_id": "2",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004268.1.47",
          "form": "점점 크면서 책이랑 멀어져..",
          "original_form": "점점 크면서 책이랑 멀어져..",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004268.1.48",
          "form": "맞아.. 솔직히 1년에 한권 읽을까 말까인듯",
          "original_form": "맞아.. 솔직히 1년에 한권 읽을까 말까인듯",
          "speaker_id": "1",
