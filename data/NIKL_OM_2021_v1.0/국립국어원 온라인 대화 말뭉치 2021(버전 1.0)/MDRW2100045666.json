      "utterance": [
        {
          "id": "MDRW2100045666.1.1",
          "form": "저녁메뉴?",
          "original_form": "저녁메뉴?",
          "speaker_id": "1",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045666.1.2",
          "form": "제가 오늘 먹은 저녁메뉴 맞춰보실래요?",
          "original_form": "제가 오늘 먹은 저녁메뉴 맞춰보실래요?",
          "speaker_id": "2",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045666.1.3",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045666.1.4",
          "form": "추라이츄라이",
          "original_form": "추라이츄라이",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045666.1.5",
          "form": "편의점 샌드위치요?",
          "original_form": "편의점 샌드위치요?",
          "speaker_id": "1",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045666.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ첫번째부터 구체적으로 들어가시다니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ첫번째부터 구체적으로 들어가시다니",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045666.1.7",
          "form": "편드위치 아닙니다!",
          "original_form": "편드위치 아닙니다!",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045666.1.8",
          "form": "방금 다른분이 그거여서",
          "original_form": "방금 다른분이 그거여서",
          "speaker_id": "1",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045666.1.9",
          "form": "김밥?",
          "original_form": "김밥?",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045666.1.10",
          "form": "ㅋㅌㅌㅋㅋㅌㅋㅋ김밥도 아닙니다!",
          "original_form": "ㅋㅌㅌㅋㅋㅌㅋㅋ김밥도 아닙니다!",
          "speaker_id": "2",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045666.1.11",
          "form": "구체적으로 물어보세요 후후",
          "original_form": "구체적으로 물어보세요 후후",
          "speaker_id": "2",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045666.1.12",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "1",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045666.1.13",
          "form": "네! 한식입니다",
          "original_form": "네! 한식입니다",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045666.1.14",
          "form": "한식이면 음",
          "original_form": "한식이면 음",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045666.1.15",
          "form": "손에 들고먹을 수 있는 음식인가요?",
          "original_form": "손에 들고먹을 수 있는 음식인가요?",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045666.1.16",
          "form": "아니요! 손에 딱 들 수 없어요",
          "original_form": "아니요! 손에 딱 들 수 없어요",
          "speaker_id": "2",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045666.1.17",
          "form": "뜨겁게 먹나요?",
          "original_form": "뜨겁게 먹나요?",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045666.1.18",
          "form": "네! 뜨거운 음식이랍니다",
          "original_form": "네! 뜨거운 음식이랍니다",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045666.1.19",
          "form": "국물요리인가요?",
          "original_form": "국물요리인가요?",
          "speaker_id": "1",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045666.1.20",
          "form": "네! 국물요리입니다",
          "original_form": "네! 국물요리입니다",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045666.1.21",
          "form": "찌개인가요?",
          "original_form": "찌개인가요?",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045666.1.22",
          "form": "네! 찌개류입니다",
          "original_form": "네! 찌개류입니다",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045666.1.23",
          "form": "다 왔다 다 왔다",
          "original_form": "다 왔다 다 왔다",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045666.1.24",
          "form": "김치찌개?",
          "original_form": "김치찌개?",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045666.1.25",
          "form": "헉 엄청 빨리 맞추시는데요?",
          "original_form": "헉 엄청 빨리 맞추시는데요?",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045666.1.26",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045666.1.27",
          "form": "되게 쭉쭉 진행해서 맞추셨어요",
          "original_form": "되게 쭉쭉 진행해서 맞추셨어요",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045666.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:10"
        }
      ]
