      "utterance": [
        {
          "id": "MDRW2100039173.1.1",
          "form": "세컵 그렇게 생각하니 할수있을것같기도한데",
          "original_form": "세컵 그렇게 생각하니 할수있을것같기도한데",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100039173.1.2",
          "form": "1리터 통으로 생각하니 엄두가 안나요 ㅎ",
          "original_form": "1리터 통으로 생각하니 엄두가 안나요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100039173.1.3",
          "form": "겨울엔 억지로라도마셔요",
          "original_form": "겨울엔 억지로라도마셔요",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100039173.1.4",
          "form": "여름엔 에어컨추워서 오히려 안들어가요",
          "original_form": "여름엔 에어컨추워서 오히려 안들어가요",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100039173.1.5",
          "form": "아..계절별로 그럴수도 있겠네요",
          "original_form": "아..계절별로 그럴수도 있겠네요",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100039173.1.6",
          "form": "오늘도 아직 반컵도 못마셨어요ㅋ",
          "original_form": "오늘도 아직 반컵도 못마셨어요ㅋ",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100039173.1.7",
          "form": "저는 아예 물컵도 없네요 ㅎ",
          "original_form": "저는 아예 물컵도 없네요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039173.1.8",
          "form": "저도 3컵정도는 시도 해봐야겠어요 ㅎ",
          "original_form": "저도 3컵정도는 시도 해봐야겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039173.1.9",
          "form": "좋은방법 같아요",
          "original_form": "좋은방법 같아요",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100039173.1.10",
          "form": "이게 마신다고 늘지는 않나봐요",
          "original_form": "이게 마신다고 늘지는 않나봐요",
          "speaker_id": "1",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100039173.1.11",
          "form": "편하게 안들어가요ㅋㅋ",
          "original_form": "편하게 안들어가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100039173.1.12",
          "form": "습관같기도해도",
          "original_form": "습관같기도해도",
          "speaker_id": "2",
          "time": "20210903 12:45"
        },
        {
          "id": "MDRW2100039173.1.13",
          "form": "잘 마시는사람은 잘마시던데",
          "original_form": "잘 마시는사람은 잘마시던데",
          "speaker_id": "2",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100039173.1.14",
          "form": "그러게요 전힘들더라고요",
          "original_form": "그러게요 전힘들더라고요",
          "speaker_id": "1",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100039173.1.15",
          "form": "구내식당잇음 좋을거같아요",
          "original_form": "구내식당잇음 좋을거같아요",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039173.1.16",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039173.1.17",
          "form": "메뉴고민도 없고",
          "original_form": "메뉴고민도 없고",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039173.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039173.1.19",
          "form": "그게 최고예요",
          "original_form": "그게 최고예요",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039173.1.20",
          "form": "그고민만 한시간인데ㅋㅋ",
          "original_form": "그고민만 한시간인데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039173.1.21",
          "form": "예전에 있던곳에서 막내라서",
          "original_form": "예전에 있던곳에서 막내라서",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039173.1.22",
          "form": "제가 메뉴정하고 식당찾고",
          "original_form": "제가 메뉴정하고 식당찾고",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039173.1.23",
          "form": "비라도 오면 대리님 과장님들 나가기 귀찮다고",
          "original_form": "비라도 오면 대리님 과장님들 나가기 귀찮다고",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039173.1.24",
          "form": "중국집시키자고",
          "original_form": "중국집시키자고",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039173.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039173.1.26",
          "form": "장마철엔 진짜 토하는줄알았어요",
          "original_form": "장마철엔 진짜 토하는줄알았어요",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039173.1.27",
          "form": "진짜스트레스셨겠어요",
          "original_form": "진짜스트레스셨겠어요",
          "speaker_id": "1",
