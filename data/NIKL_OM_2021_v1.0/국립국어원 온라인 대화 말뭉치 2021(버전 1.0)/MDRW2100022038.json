      "utterance": [
        {
          "id": "MDRW2100022038.1.1",
          "form": "최근에 구매하신거 있으세용?",
          "original_form": "최근에 구매하신거 있으세용?",
          "speaker_id": "1",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100022038.1.2",
          "form": "최근에 스킨푸드 당근패드 샀어요!",
          "original_form": "최근에 스킨푸드 당근패드 샀어요!",
          "speaker_id": "2",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100022038.1.3",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "1",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100022038.1.4",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100022038.1.5",
          "form": "브랜디에서 구매하셨나용....?",
          "original_form": "브랜디에서 구매하셨나용....?",
          "speaker_id": "1",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100022038.1.6",
          "form": "넹넹ㅋㅋㅋㅋㅋ",
          "original_form": "넹넹ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100022038.1.7",
          "form": "왠지",
          "original_form": "왠지",
          "speaker_id": "1",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100022038.1.8",
          "form": "화장품을 굉장히 좋아하실것같아요!",
          "original_form": "화장품을 굉장히 좋아하실것같아요!",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 모으는것만 좋아해욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 모으는것만 좋아해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.11",
          "form": "특히 색조를....",
          "original_form": "특히 색조를....",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.12",
          "form": "아하 저도",
          "original_form": "아하 저도",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.13",
          "form": "화장품 구매 엄청해서",
          "original_form": "화장품 구매 엄청해서",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.14",
          "form": "앞으로 몇년은 안사도 될거에용",
          "original_form": "앞으로 몇년은 안사도 될거에용",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.15",
          "form": "그것때문에 집에서 팩폭맞아욬ㅋㅋㅋㅋ",
          "original_form": "그것때문에 집에서 팩폭맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.16",
          "form": "그래도 싸게 구매하니까",
          "original_form": "그래도 싸게 구매하니까",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100022038.1.17",
          "form": "가족들이 뭐 필요할때",
          "original_form": "가족들이 뭐 필요할때",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022038.1.18",
          "form": "찾아오지않아용?ㅋㅋㅋㅋㅋ",
          "original_form": "찾아오지않아용?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022038.1.19",
          "form": "맞아욬ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022038.1.20",
          "form": "맨날 집에 택배 박스 쌓여있고~ㅋㅋㅋㅋ",
          "original_form": "맨날 집에 택배 박스 쌓여있고~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022038.1.21",
          "form": "화장품은 아니더라도... 이거 좀 사봐라 저것 좀 사봐라....",
          "original_form": "화장품은 아니더라도... 이거 좀 사봐라 저것 좀 사봐라....",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022038.1.22",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022038.1.23",
          "form": "이번에 타미힐피거 모사셨어요?",
