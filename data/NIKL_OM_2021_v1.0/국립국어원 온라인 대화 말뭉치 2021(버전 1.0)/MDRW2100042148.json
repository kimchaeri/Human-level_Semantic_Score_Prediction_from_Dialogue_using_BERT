      "utterance": [
        {
          "id": "MDRW2100042148.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100042148.1.2",
          "form": "네 ! 집에 있는 반찬으로 먹었어요~",
          "original_form": "네 ! 집에 있는 반찬으로 먹었어요~",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042148.1.3",
          "form": "집밥 드셨네요!",
          "original_form": "집밥 드셨네요!",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042148.1.4",
          "form": "평소에도 집밥 많이 드세요?",
          "original_form": "평소에도 집밥 많이 드세요?",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042148.1.5",
          "form": "네 웬만하면 집밥 먹으려고 노력해요",
          "original_form": "네 웬만하면 집밥 먹으려고 노력해요",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042148.1.6",
          "form": "근데 요즘 배달음식을 많이 먹게 되더라구요..흑",
          "original_form": "근데 요즘 배달음식을 많이 먹게 되더라구요..흑",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042148.1.7",
          "form": "저는 거의 배달음식 먹어요.. ㅋㅋㅋㅋ",
          "original_form": "저는 거의 배달음식 먹어요.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042148.1.8",
          "form": "요리를 거의 안해요ㅜㅠ",
          "original_form": "요리를 거의 안해요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042148.1.9",
          "form": "맞아요 요리하기 너무 귀찮죠",
          "original_form": "맞아요 요리하기 너무 귀찮죠",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042148.1.10",
          "form": "주로 어떤음식 드세요 ?",
          "original_form": "주로 어떤음식 드세요 ?",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042148.1.11",
          "form": "저는 주로 한식 배달해서 먹어요!",
          "original_form": "저는 주로 한식 배달해서 먹어요!",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042148.1.12",
          "form": "고기나 찌개류요",
          "original_form": "고기나 찌개류요",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042148.1.13",
          "form": "고기 ! 맛잘알이시네요 역시 사람은 고기죠",
          "original_form": "고기 ! 맛잘알이시네요 역시 사람은 고기죠",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042148.1.14",
          "form": "돼지고기 vs 소고기 어떤걸 좋아하시나요 !",
          "original_form": "돼지고기 vs 소고기 어떤걸 좋아하시나요 !",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042148.1.15",
          "form": "그쵸그쵸!!",
          "original_form": "그쵸그쵸!!",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042148.1.16",
          "form": "아 너무 어려운데",
          "original_form": "아 너무 어려운데",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042148.1.17",
          "form": "원래는 돼지고기 좋아했는데",
          "original_form": "원래는 돼지고기 좋아했는데",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042148.1.18",
          "form": "지금은 각자의 매력을 알아서 못 놓아요ㅠㅠ",
          "original_form": "지금은 각자의 매력을 알아서 못 놓아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042148.1.19",
          "form": "훌륭하세요 고기라면 그게 뭐든 전부 입에 넣고 보는거죠",
          "original_form": "훌륭하세요 고기라면 그게 뭐든 전부 입에 넣고 보는거죠",
          "speaker_id": "2",
          "time": "20210906 20:40"
        },
        {
          "id": "MDRW2100042148.1.20",
          "form": "ㅋㅋㅋㄱㅋㅋ맞아요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋ맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100042148.1.21",
          "form": "고기는 정말 맛있으니까요...",
          "original_form": "고기는 정말 맛있으니까요...",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100042148.1.22",
          "form": "어떤 고기 더 좋아하세요?",
          "original_form": "어떤 고기 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100042148.1.23",
          "form": "돼지고기요 ! 구워서 냉면이랑 같이 먹고 싶네요 ㅠㅜㅜ",
          "original_form": "돼지고기요 ! 구워서 냉면이랑 같이 먹고 싶네요 ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100042148.1.24",
          "form": "크 너무 좋죠!!ㅠㅜㅜㅡ",
          "original_form": "크 너무 좋죠!!ㅠㅜㅜㅡ",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100042148.1.25",
          "form": "냉면은 물냉 비냉 중에 뭐 드세요?",
          "original_form": "냉면은 물냉 비냉 중에 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100042148.1.26",
          "form": "전 죽어도 물냉파입니다 물냉밖에 안먹어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 죽어도 물냉파입니다 물냉밖에 안먹어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100042148.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ확고하시네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ확고하시네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100042148.1.28",
          "form": "저도 비냉보다는 물냉 좋아해요",
          "original_form": "저도 비냉보다는 물냉 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100042148.1.29",
          "form": "오~ 통했네요 ! ",
          "original_form": "오~ 통했네요 ! ",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100042148.1.30",
          "form": "ㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋ그러게요",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100042148.1.31",
          "form": "냉면 너무 맛있어요",
          "original_form": "냉면 너무 맛있어요",
          "speaker_id": "1",
