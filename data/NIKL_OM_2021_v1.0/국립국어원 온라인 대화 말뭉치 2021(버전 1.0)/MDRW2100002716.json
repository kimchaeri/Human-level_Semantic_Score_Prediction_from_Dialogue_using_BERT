      "utterance": [
        {
          "id": "MDRW2100002716.1.1",
          "form": "요기요나 배민 할인있는거 찾아서요",
          "original_form": "요기요나 배민 할인있는거 찾아서요",
          "speaker_id": "2",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002716.1.2",
          "form": "아~저는 주로 요기요에서요 ㅡ",
          "original_form": "아~저는 주로 요기요에서요 ㅡ",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002716.1.3",
          "form": "요즘은 배달비가 너무비싸서",
          "original_form": "요즘은 배달비가 너무비싸서",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002716.1.4",
          "form": "비싼데는 팔천까지 받더라고요",
          "original_form": "비싼데는 팔천까지 받더라고요",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002716.1.5",
          "form": "맞아요 저희동네는 기본이 3천원이에요",
          "original_form": "맞아요 저희동네는 기본이 3천원이에요",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002716.1.6",
          "form": "저희도 주변에 뭐가없어서 다 멀리서 오거든요",
          "original_form": "저희도 주변에 뭐가없어서 다 멀리서 오거든요",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002716.1.7",
          "form": "너무비싸요",
          "original_form": "너무비싸요",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002716.1.8",
          "form": "아~저는 가까워도 비싸더라고요ㅠ",
          "original_form": "아~저는 가까워도 비싸더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002716.1.9",
          "form": "배달비가 아깝긴 한데 안시킬수는 없어서 참그래요",
          "original_form": "배달비가 아깝긴 한데 안시킬수는 없어서 참그래요",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002716.1.10",
          "form": "네~저는 요기요 슈퍼클럽을 가입해서 아주 자주 시키고 있어요",
          "original_form": "네~저는 요기요 슈퍼클럽을 가입해서 아주 자주 시키고 있어요",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002716.1.11",
          "form": "그거 사람들 가입많이 하긴 하더라고요",
          "original_form": "그거 사람들 가입많이 하긴 하더라고요",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002716.1.12",
          "form": "네~어차피 시켜먹으니까 할인 받을겸 ㅎ",
          "original_form": "네~어차피 시켜먹으니까 할인 받을겸 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002716.1.13",
          "form": "저도 가입 고민해봐야 겠네요",
          "original_form": "저도 가입 고민해봐야 겠네요",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002716.1.14",
          "form": "이번주에는 뚜레쥬르 할인이 대박이었어요",
          "original_form": "이번주에는 뚜레쥬르 할인이 대박이었어요",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002716.1.15",
          "form": "아마 가입하시면 더 자주 시키실수도 있어요",
          "original_form": "아마 가입하시면 더 자주 시키실수도 있어요",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002716.1.16",
          "form": "확실히 그럴거 같아요 \r\n할인 있을때마다 꼭 주문할듯요",
          "original_form": "확실히 그럴거 같아요 \r\n할인 있을때마다 꼭 주문할듯요",
          "speaker_id": "2",
          "time": "20210820 15:28"
        }
      ]
