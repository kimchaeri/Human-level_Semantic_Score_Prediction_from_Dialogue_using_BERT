      "utterance": [
        {
          "id": "MDRW2100029960.1.1",
          "form": "조랑말님 산책좋아하세요?",
          "original_form": "조랑말님 산책좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029960.1.2",
          "form": "당장 홍제천으로 따라나와",
          "original_form": "당장 홍제천으로 따라나와",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029960.1.3",
          "form": "어?",
          "original_form": "어?",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029960.1.4",
          "form": "저 작년에 한창 산책다녔었는데",
          "original_form": "저 작년에 한창 산책다녔었는데",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029960.1.5",
          "form": "살도 빠지고 확실히 기초체력도 올라가는게 느껴지더라고요",
          "original_form": "살도 빠지고 확실히 기초체력도 올라가는게 느껴지더라고요",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029960.1.6",
          "form": "햄스터님도 산책 좋아하세요?",
          "original_form": "햄스터님도 산책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029960.1.7",
          "form": "당장 홍제천으로 따라나와",
          "original_form": "당장 홍제천으로 따라나와",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029960.1.8",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029960.1.9",
          "form": "산책 좋긴한데..",
          "original_form": "산책 좋긴한데..",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029960.1.10",
          "form": "한데?",
          "original_form": "한데?",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029960.1.11",
          "form": "저 어제 필라테스햇는데",
          "original_form": "저 어제 필라테스햇는데",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029960.1.12",
          "form": "는데?",
          "original_form": "는데?",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029960.1.13",
          "form": "그거때문에 허벅지랑 등이",
          "original_form": "그거때문에 허벅지랑 등이",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029960.1.14",
          "form": "진짜 아파요",
          "original_form": "진짜 아파요",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029960.1.15",
          "form": "아파요?",
          "original_form": "아파요?",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100029960.1.16",
          "form": "왜...왜그러세요?",
          "original_form": "왜...왜그러세요?",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100029960.1.17",
          "form": "잘못했어요",
          "original_form": "잘못했어요",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100029960.1.18",
          "form": "잘못했어요?",
          "original_form": "잘못했어요?",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100029960.1.19",
          "form": "필라테스 하면",
          "original_form": "필라테스 하면",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100029960.1.20",
          "form": "근육이 아픈거예요 아니면",
          "original_form": "근육이 아픈거예요 아니면",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100029960.1.21",
          "form": "표면이 아픈거예요?",
          "original_form": "표면이 아픈거예요?",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100029960.1.22",
          "form": "필라테스를 제대로 했으면 근육이 아프구요",
          "original_form": "필라테스를 제대로 했으면 근육이 아프구요",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100029960.1.23",
          "form": "힘 잘못줫으면 표면이 아파요",
          "original_form": "힘 잘못줫으면 표면이 아파요",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100029960.1.24",
          "form": "전 근육이 아파요 어때요?제법 운동잘해보이나요?",
          "original_form": "전 근육이 아파요 어때요?제법 운동잘해보이나요?",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100029960.1.25",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100029960.1.26",
          "form": "그냥 평소에 근육을 안써서 그런거 아닌가요?",
          "original_form": "그냥 평소에 근육을 안써서 그런거 아닌가요?",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100029960.1.27",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100029960.1.28",
          "form": "님.",
          "original_form": "님.",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100029960.1.29",
          "form": "고소할거에요",
          "original_form": "고소할거에요",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100029960.1.30",
          "form": "님때문에 지금 뼈부러졌어요 병원비 청구할게요",
          "original_form": "님때문에 지금 뼈부러졌어요 병원비 청구할게요",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100029960.1.31",
          "form": "뼈 부러진거 그거 근육이 약해서그래요",
          "original_form": "뼈 부러진거 그거 근육이 약해서그래요",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100029960.1.32",
          "form": "산책하면 나아요",
          "original_form": "산책하면 나아요",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100029960.1.33",
          "form": "당장 홍제천으로 따라나와",
          "original_form": "당장 홍제천으로 따라나와",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100029960.1.34",
          "form": "지금 갓태어난 아기조랑말인데도요",
          "original_form": "지금 갓태어난 아기조랑말인데도요",
          "speaker_id": "1",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100029960.1.35",
          "form": "조랑말은 저인데두요",
          "original_form": "조랑말은 저인데두요",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100029960.1.36",
          "form": "우린 가좍이니까요",
          "original_form": "우린 가좍이니까요",
          "speaker_id": "1",
          "time": "20210903 17:23"
        }
      ]
    }
