      "utterance": [
        {
          "id": "MDRW2100032879.1.1",
          "form": "라면먹고싶다",
          "original_form": "라면먹고싶다",
          "speaker_id": "1",
          "time": "20210906 03:26"
        },
        {
          "id": "MDRW2100032879.1.2",
          "form": "치즈계란떡대파송송후춧가루랑 고춧가루 톡 뿌리고 고추장 넣으면 진짜 맛있어",
          "original_form": "치즈계란떡대파송송후춧가루랑 고춧가루 톡 뿌리고 고추장 넣으면 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210906 03:27"
        },
        {
          "id": "MDRW2100032879.1.3",
          "form": "다진 마늘도",
          "original_form": "다진 마늘도",
          "speaker_id": "1",
          "time": "20210906 03:27"
        },
        {
          "id": "MDRW2100032879.1.4",
          "form": "헐... 이시간에 라면 얘기는 반칙인데",
          "original_form": "헐... 이시간에 라면 얘기는 반칙인데",
          "speaker_id": "2",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032879.1.5",
          "form": "라면은 언제 어디서 먹어도 존맛...",
          "original_form": "라면은 언제 어디서 먹어도 존맛...",
          "speaker_id": "2",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032879.1.6",
          "form": "근데 라면에 고추장은 처음들어본다",
          "original_form": "근데 라면에 고추장은 처음들어본다",
          "speaker_id": "2",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032879.1.7",
          "form": "ㅋㅋㅋㅋㅋ그치 잘 모르는데 넣으면 맛 깊어져",
          "original_form": "ㅋㅋㅋㅋㅋ그치 잘 모르는데 넣으면 맛 깊어져",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032879.1.8",
          "form": "반스푼정도만.. 짬",
          "original_form": "반스푼정도만.. 짬",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032879.1.9",
          "form": "으아 진짜 침 나온다 끓일까",
          "original_form": "으아 진짜 침 나온다 끓일까",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032879.1.10",
          "form": "헐... 나도 먹고 싶다...",
          "original_form": "헐... 나도 먹고 싶다...",
          "speaker_id": "2",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.11",
          "form": "너는 라면 어떻게 먹어??",
          "original_form": "너는 라면 어떻게 먹어??",
          "speaker_id": "1",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.12",
          "form": "지금 그렇게 먹고 싶은데 집에 라면 다 떨어졌어ㅠㅠ",
          "original_form": "지금 그렇게 먹고 싶은데 집에 라면 다 떨어졌어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.13",
          "form": "나는 그냥 기본. 심플하겤ㅋㅋㅋ",
          "original_form": "나는 그냥 기본. 심플하겤ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.14",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.15",
          "form": "계란파?",
          "original_form": "계란파?",
          "speaker_id": "1",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.16",
          "form": "치즈는?!?",
          "original_form": "치즈는?!?",
          "speaker_id": "1",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.17",
          "form": "가끔 떡이나 계란 아니면 누릉지넣어 먹어",
          "original_form": "가끔 떡이나 계란 아니면 누릉지넣어 먹어",
          "speaker_id": "2",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032879.1.18",
          "form": "치즈는 노란 사각보다 모짜렐라 치즈 넣는거 좋아햌ㅋㅋ",
          "original_form": "치즈는 노란 사각보다 모짜렐라 치즈 넣는거 좋아햌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:33"
        },
        {
          "id": "MDRW2100032879.1.19",
          "form": "헉 누룽지는 처음 들어봐",
          "original_form": "헉 누룽지는 처음 들어봐",
          "speaker_id": "1",
          "time": "20210906 03:34"
        },
        {
          "id": "MDRW2100032879.1.20",
          "form": "밥 넣어먹는 맛이랑 비슷한가",
          "original_form": "밥 넣어먹는 맛이랑 비슷한가",
          "speaker_id": "1",
          "time": "20210906 03:34"
        },
        {
          "id": "MDRW2100032879.1.21",
          "form": "ㅋㅋㅋ헐 오짜렐라도 처음봐ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ헐 오짜렐라도 처음봐ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 03:35"
        },
        {
          "id": "MDRW2100032879.1.22",
          "form": "밥이랑은 조금 느낌이 달라",
          "original_form": "밥이랑은 조금 느낌이 달라",
          "speaker_id": "2",
          "time": "20210906 03:35"
        },
        {
          "id": "MDRW2100032879.1.23",
          "form": "뭐라고 설명하는게 좋은지는 모르겠는데 개인적으론 더 맛있어",
          "original_form": "뭐라고 설명하는게 좋은지는 모르겠는데 개인적으론 더 맛있어",
          "speaker_id": "2",
          "time": "20210906 03:35"
        },
        {
          "id": "MDRW2100032879.1.24",
          "form": "노란거 넣으면 나중에 다 녹아서 없어지는데 모짜렐라는 안 없어지고 늘어나서 좋앜ㅋㅋㅋㅋ",
          "original_form": "노란거 넣으면 나중에 다 녹아서 없어지는데 모짜렐라는 안 없어지고 늘어나서 좋앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:36"
        },
        {
          "id": "MDRW2100032879.1.25",
          "form": "뭔가 같이 먹는다는 느낌이 강해섴ㅋㅋ",
          "original_form": "뭔가 같이 먹는다는 느낌이 강해섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:36"
        },
        {
          "id": "MDRW2100032879.1.26",
          "form": "ㅋㅋㅋㅋㅋ다음에 누룽지도 해볼게",
          "original_form": "ㅋㅋㅋㅋㅋ다음에 누룽지도 해볼게",
          "speaker_id": "1",
          "time": "20210906 03:37"
        },
        {
          "id": "MDRW2100032879.1.27",
          "form": "아 으 노란 치즈가 아예 없어질만큼",
          "original_form": "아 으 노란 치즈가 아예 없어질만큼",
          "speaker_id": "1",
          "time": "20210906 03:37"
        },
        {
          "id": "MDRW2100032879.1.28",
          "form": "바글바글바글 끓이면",
          "original_form": "바글바글바글 끓이면",
          "speaker_id": "1",
          "time": "20210906 03:37"
        },
        {
          "id": "MDRW2100032879.1.29",
          "form": "어우~~! 국물 짱이야",
          "original_form": "어우~~! 국물 짱이야",
          "speaker_id": "1",
          "time": "20210906 03:37"
        },
        {
          "id": "MDRW2100032879.1.30",
          "form": "ㅇㅇ 아니면 참깨라면 좋아하면 GS에 참깨라면 누릉지 있으니까 한 번 먹어봐",
          "original_form": "ㅇㅇ 아니면 참깨라면 좋아하면 GS에 참깨라면 누릉지 있으니까 한 번 먹어봐",
          "speaker_id": "2",
          "time": "20210906 03:38"
        },
        {
          "id": "MDRW2100032879.1.31",
          "form": "그래??? 나는 여태 위에 얹어진것만 봐서 그렇데 끓이는 건 몰랐네",
          "original_form": "그래??? 나는 여태 위에 얹어진것만 봐서 그렇데 끓이는 건 몰랐네",
          "speaker_id": "2",
