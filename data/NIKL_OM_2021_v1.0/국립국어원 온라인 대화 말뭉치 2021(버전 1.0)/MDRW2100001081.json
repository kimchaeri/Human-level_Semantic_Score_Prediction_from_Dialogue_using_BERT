      "utterance": [
        {
          "id": "MDRW2100001081.1.1",
          "form": "요즘 날이 많이 덥죠",
          "original_form": "요즘 날이 많이 덥죠",
          "speaker_id": "2",
          "time": "20210723 11:00"
        },
        {
          "id": "MDRW2100001081.1.2",
          "form": "에어컨 없아는 못사는날씨..ㅠ",
          "original_form": "에어컨 없아는 못사는날씨..ㅠ",
          "speaker_id": "2",
          "time": "20210723 11:00"
        },
        {
          "id": "MDRW2100001081.1.3",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210723 11:01"
        },
        {
          "id": "MDRW2100001081.1.4",
          "form": "그런데 저는 에어컨보다 선풍기 자주 틀어요!",
          "original_form": "그런데 저는 에어컨보다 선풍기 자주 틀어요!",
          "speaker_id": "1",
          "time": "20210723 11:01"
        },
        {
          "id": "MDRW2100001081.1.5",
          "form": "오 저도요!",
          "original_form": "오 저도요!",
          "speaker_id": "2",
          "time": "20210723 11:02"
        },
        {
          "id": "MDRW2100001081.1.6",
          "form": "근데 선풍기 틀어도 요즘 너무 더워서 잘 못자겠더라고요",
          "original_form": "근데 선풍기 틀어도 요즘 너무 더워서 잘 못자겠더라고요",
          "speaker_id": "2",
          "time": "20210723 11:03"
        },
        {
          "id": "MDRW2100001081.1.7",
          "form": "ㅋㅋ맞아요",
          "original_form": "ㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210723 11:19"
        },
        {
          "id": "MDRW2100001081.1.8",
          "form": "그리고 자기전에 물을 많이 마시라고 하더라고요?",
          "original_form": "그리고 자기전에 물을 많이 마시라고 하더라고요?",
          "speaker_id": "1",
          "time": "20210723 11:20"
        },
        {
          "id": "MDRW2100001081.1.9",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210723 11:20"
        },
        {
          "id": "MDRW2100001081.1.10",
          "form": "전 예전에 다이소에서 냉온팩 사서 등에 대고 자곤 했어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 예전에 다이소에서 냉온팩 사서 등에 대고 자곤 했어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 11:21"
        },
        {
          "id": "MDRW2100001081.1.11",
          "form": "ㅋㅋ헐",
          "original_form": "ㅋㅋ헐",
          "speaker_id": "1",
          "time": "20210723 11:37"
        },
        {
          "id": "MDRW2100001081.1.12",
          "form": "그거 좋은데요ㅋㅋ 등이랑 이불 젖지않나요?",
          "original_form": "그거 좋은데요ㅋㅋ 등이랑 이불 젖지않나요?",
          "speaker_id": "1",
          "time": "20210723 11:37"
        },
        {
          "id": "MDRW2100001081.1.13",
          "form": "맞아요 그래서 수건으로 두르고 자는데 그래도 축축한게..",
          "original_form": "맞아요 그래서 수건으로 두르고 자는데 그래도 축축한게..",
          "speaker_id": "2",
          "time": "20210723 11:38"
        },
        {
          "id": "MDRW2100001081.1.14",
          "form": "나름 차선의 방법이였는데ㅋㅋㅋㅋ 항상 축축하게 일어나야했죠",
          "original_form": "나름 차선의 방법이였는데ㅋㅋㅋㅋ 항상 축축하게 일어나야했죠",
          "speaker_id": "2",
          "time": "20210723 11:38"
        },
        {
          "id": "MDRW2100001081.1.15",
          "form": "ㅋㅋㅋㅋ고생하셨네요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ고생하셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:40"
        },
        {
          "id": "MDRW2100001081.1.16",
          "form": "저는 맨날 이불들고 거실 나가서 잤어요ㅋㅋ",
          "original_form": "저는 맨날 이불들고 거실 나가서 잤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:40"
        },
        {
          "id": "MDRW2100001081.1.17",
          "form": "거실이 트여있으니 더 시원하잖아요ㅎㅎ",
          "original_form": "거실이 트여있으니 더 시원하잖아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210723 11:40"
        },
        {
          "id": "MDRW2100001081.1.18",
          "form": "아ㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210723 11:40"
        },
        {
          "id": "MDRW2100001081.1.19",
          "form": "저는 방문 안닫고 자면 잠이 안와서 거실에서 못자겠더라고요",
          "original_form": "저는 방문 안닫고 자면 잠이 안와서 거실에서 못자겠더라고요",
          "speaker_id": "2",
          "time": "20210723 11:41"
        },
        {
          "id": "MDRW2100001081.1.20",
          "form": "맞아요 그런 분들도 계시죠",
          "original_form": "맞아요 그런 분들도 계시죠",
          "speaker_id": "1",
          "time": "20210723 11:53"
        },
        {
          "id": "MDRW2100001081.1.21",
          "form": ".올해가 진짜 더운것같아요",
          "original_form": ".올해가 진짜 더운것같아요",
          "speaker_id": "1",
          "time": "20210723 12:50"
        },
        {
          "id": "MDRW2100001081.1.22",
          "form": "작년엔 이정도는 아니었는데",
          "original_form": "작년엔 이정도는 아니었는데",
          "speaker_id": "1",
          "time": "20210723 12:50"
        },
        {
          "id": "MDRW2100001081.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210723 13:59"
        },
        {
          "id": "MDRW2100001081.1.24",
          "form": "이번에 2018년 이후로 제일 덥다고 했던 것 같은데",
          "original_form": "이번에 2018년 이후로 제일 덥다고 했던 것 같은데",
          "speaker_id": "2",
          "time": "20210723 13:59"
        },
        {
          "id": "MDRW2100001081.1.25",
          "form": "진짜 이번여름 어떻게 버텨야하나 싶어요..ㅋㅋㅋ",
          "original_form": "진짜 이번여름 어떻게 버텨야하나 싶어요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 14:00"
        },
        {
          "id": "MDRW2100001081.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210723 15:49"
        },
        {
          "id": "MDRW2100001081.1.27",
          "form": "열대야 진짜 덥네요",
          "original_form": "열대야 진짜 덥네요",
