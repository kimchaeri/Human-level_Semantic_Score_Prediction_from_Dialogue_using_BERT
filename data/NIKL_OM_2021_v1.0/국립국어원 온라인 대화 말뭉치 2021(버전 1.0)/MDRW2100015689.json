      "utterance": [
        {
          "id": "MDRW2100015689.1.1",
          "form": "최근에 본 스포츠 경기 있으세요?",
          "original_form": "최근에 본 스포츠 경기 있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015689.1.2",
          "form": "직접 가서요?",
          "original_form": "직접 가서요?",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015689.1.3",
          "form": "직접 가거나 온라인 시청이요!",
          "original_form": "직접 가거나 온라인 시청이요!",
          "speaker_id": "1",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015689.1.4",
          "form": "온라인은 야구 가끔 보고",
          "original_form": "온라인은 야구 가끔 보고",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015689.1.5",
          "form": "직접도 야구가 마지막이네요",
          "original_form": "직접도 야구가 마지막이네요",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015689.1.6",
          "form": "야구를 제일 좋아하시나요?",
          "original_form": "야구를 제일 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015689.1.7",
          "form": "네 야구 유니폼도 있어요!!",
          "original_form": "네 야구 유니폼도 있어요!!",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015689.1.8",
          "form": "우왕! 어디 팬이신가요?",
          "original_form": "우왕! 어디 팬이신가요?",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015689.1.9",
          "form": "저 키움이요 ㅎㅎ name1님은요?",
          "original_form": "저 키움이요 ㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015689.1.10",
          "form": "저는 야구를 잘 모르지만",
          "original_form": "저는 야구를 잘 모르지만",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100015689.1.11",
          "form": "이기는 편이 우리 편이란ㅋㅋㅋㅋㅋㅋㅋ생각을 하고 가끔 봐요",
          "original_form": "이기는 편이 우리 편이란ㅋㅋㅋㅋㅋㅋㅋ생각을 하고 가끔 봐요",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100015689.1.12",
          "form": "보통 연고지 따라가잖아요 ㅎㅎ",
          "original_form": "보통 연고지 따라가잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100015689.1.13",
          "form": "아 그쵸그쵸",
          "original_form": "아 그쵸그쵸",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100015689.1.14",
          "form": "제가 대전 오래 살았어서",
          "original_form": "제가 대전 오래 살았어서",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100015689.1.15",
          "form": "휩쓸려서 한화 응원했던 것같아요",
          "original_form": "휩쓸려서 한화 응원했던 것같아요",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100015689.1.16",
          "form": "오 대전~~",
          "original_form": "오 대전~~",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015689.1.17",
          "form": "야구 말고는 뭐보세요?",
          "original_form": "야구 말고는 뭐보세요?",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015689.1.18",
          "form": "저는 주변에서 보자 해서 보는게 대부분이긴 한데요",
