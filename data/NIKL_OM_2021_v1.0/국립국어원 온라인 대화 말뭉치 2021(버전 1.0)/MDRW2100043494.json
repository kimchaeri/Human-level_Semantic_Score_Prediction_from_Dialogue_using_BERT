      "utterance": [
        {
          "id": "MDRW2100043494.1.1",
          "form": "아 숙소하니까 생각났는데",
          "original_form": "아 숙소하니까 생각났는데",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043494.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043494.1.3",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043494.1.4",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043494.1.5",
          "form": "어렸을때는 여행갈때 숙소 저렴한곳 가도 괜찮았는데",
          "original_form": "어렸을때는 여행갈때 숙소 저렴한곳 가도 괜찮았는데",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043494.1.6",
          "form": "나이를 들다보니까 점점 예민해지는것같아요",
          "original_form": "나이를 들다보니까 점점 예민해지는것같아요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043494.1.7",
          "form": "이제 게하는 잘 못가겠더라구요",
          "original_form": "이제 게하는 잘 못가겠더라구요",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043494.1.8",
          "form": "그쵸 ㅎ저도 예전에는 게하에서 자도 괜찮았는데 이제는 잠을 편히 자야겠더라고요",
          "original_form": "그쵸 ㅎ저도 예전에는 게하에서 자도 괜찮았는데 이제는 잠을 편히 자야겠더라고요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043494.1.9",
          "form": "나이가 들어서그런가",
          "original_form": "나이가 들어서그런가",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043494.1.10",
          "form": "그쵸그쵸 그리구 화장실이랑 샤워실 공유하는것도 이제 못하겠더라구요 ㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸 그리구 화장실이랑 샤워실 공유하는것도 이제 못하겠더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043494.1.11",
          "form": "맞아요ㅋㅋㅋㅋ진짜ㅈ",
          "original_form": "맞아요ㅋㅋㅋㅋ진짜ㅈ",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043494.1.12",
          "form": "그게 제일 중요해요ㅋㅋ",
          "original_form": "그게 제일 중요해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043494.1.13",
          "form": "화장실이 따로있고 욕조가있으면 더좋고",
          "original_form": "화장실이 따로있고 욕조가있으면 더좋고",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043494.1.14",
          "form": "ㅋㅋㅋㅋ맞아요 그럼 최고 ㅋㅋㅋㅋ 근데 그러면 숙소 가격이 훅 뛰죠 ㅠㅠㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아요 그럼 최고 ㅋㅋㅋㅋ 근데 그러면 숙소 가격이 훅 뛰죠 ㅠㅠㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043494.1.15",
          "form": "ㅠ눼....맞아요",
          "original_form": "ㅠ눼....맞아요",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043494.1.16",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043494.1.17",
          "form": "그래도 놓칠수없는 욕조...",
          "original_form": "그래도 놓칠수없는 욕조...",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043494.1.18",
          "form": "저녁에 몸담그면 최고잖아여",
          "original_form": "저녁에 몸담그면 최고잖아여",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043494.1.19",
          "form": "ㅋㅋㅋ맞아요ㅠㅠㅠㅠㅠㅠ흐엉",
          "original_form": "ㅋㅋㅋ맞아요ㅠㅠㅠㅠㅠㅠ흐엉",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043494.1.20",
          "form": "호캉스 가시는것도 좋아하시나요?",
          "original_form": "호캉스 가시는것도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043494.1.21",
          "form": "네 완전 좋아해요",
          "original_form": "네 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043494.1.22",
          "form": "오 어디어디 가보셨나요!!",
          "original_form": "오 어디어디 가보셨나요!!",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043494.1.23",
          "form": "저희지역은 호.테루가 별로없어서",
          "original_form": "저희지역은 호.테루가 별로없어서",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043494.1.24",
          "form": "서울로 한번씩갔는데 이젠 못가서ㅠ",
          "original_form": "서울로 한번씩갔는데 이젠 못가서ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043494.1.25",
          "form": "저.,,,그 비싼데는 못가봤어여",
          "original_form": "저.,,,그 비싼데는 못가봤어여",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043494.1.26",
          "form": "아항 ㅠㅠ 맞아요 서울 오는것 자체가 일이죠",
          "original_form": "아항 ㅠㅠ 맞아요 서울 오는것 자체가 일이죠",
