      "utterance": [
        {
          "id": "MDRW2100042962.1.1",
          "form": "안녕하세요~ 요즘 하시고 있는 일 있으신가요?",
          "original_form": "안녕하세요~ 요즘 하시고 있는 일 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042962.1.2",
          "form": "넹~저는직장인이에요",
          "original_form": "넹~저는직장인이에요",
          "speaker_id": "2",
          "time": "20210907 12:07"
        },
        {
          "id": "MDRW2100042962.1.3",
          "form": "헉 오늘도 가셨겠네요!",
          "original_form": "헉 오늘도 가셨겠네요!",
          "speaker_id": "1",
          "time": "20210907 12:07"
        },
        {
          "id": "MDRW2100042962.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042962.1.5",
          "form": "일하시는거있으신가요",
          "original_form": "일하시는거있으신가요",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042962.1.6",
          "form": "저는 알바해요!",
          "original_form": "저는 알바해요!",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042962.1.7",
          "form": "자주가시나요?",
          "original_form": "자주가시나요?",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042962.1.8",
          "form": "5일 간답니다..ㅠ",
          "original_form": "5일 간답니다..ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042962.1.9",
          "form": "ㅜㅜ오늘도가시나요",
          "original_form": "ㅜㅜ오늘도가시나요",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042962.1.10",
          "form": "오늘은 사정 때문에 안 가요!",
          "original_form": "오늘은 사정 때문에 안 가요!",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042962.1.11",
          "form": "아하 집근처인가요",
          "original_form": "아하 집근처인가요",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100042962.1.12",
          "form": "조금 거리가 있어요",
          "original_form": "조금 거리가 있어요",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042962.1.13",
          "form": "가기귀찮겠어용ㅜ",
          "original_form": "가기귀찮겠어용ㅜ",
          "speaker_id": "2",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100042962.1.14",
          "form": "맞아요ㅜㅜ 그래서 지금 안 가는게 너무 행복해요..",
          "original_form": "맞아요ㅜㅜ 그래서 지금 안 가는게 너무 행복해요..",
          "speaker_id": "1",
          "time": "20210907 12:31"
        },
        {
          "id": "MDRW2100042962.1.15",
          "form": "ㅎㅎㅎㅎ그럴거같아요",
          "original_form": "ㅎㅎㅎㅎ그럴거같아요",
          "speaker_id": "2",
