      "utterance": [
        {
          "id": "MDRW2100009856.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009856.1.2",
          "form": "안녕하세요~좋아하는 공연 있으셔요??",
          "original_form": "안녕하세요~좋아하는 공연 있으셔요??",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009856.1.3",
          "form": "코로나 끝나면 연극보러 가고 싶네요~",
          "original_form": "코로나 끝나면 연극보러 가고 싶네요~",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009856.1.4",
          "form": "영화 좋아했는데 ㅜㅜ요새 통 가서 못봤어요",
          "original_form": "영화 좋아했는데 ㅜㅜ요새 통 가서 못봤어요",
          "speaker_id": "1",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009856.1.5",
          "form": "오 연극좋죠",
          "original_form": "오 연극좋죠",
          "speaker_id": "1",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009856.1.6",
          "form": "전 아주 예전엔 봤는데 요샌 못봤어요",
          "original_form": "전 아주 예전엔 봤는데 요샌 못봤어요",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009856.1.7",
          "form": "지방이라 볼때도 없고요",
          "original_form": "지방이라 볼때도 없고요",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009856.1.8",
          "form": "에쿠스 햄릿이 마지막 연국이였어요",
          "original_form": "에쿠스 햄릿이 마지막 연국이였어요",
          "speaker_id": "1",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009856.1.9",
          "form": "어떤 연극 보고 싶으세요?",
          "original_form": "어떤 연극 보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009856.1.10",
          "form": "솔직히 뭐가 됐든 직접 가서 보고싶어요",
          "original_form": "솔직히 뭐가 됐든 직접 가서 보고싶어요",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009856.1.11",
          "form": "저도 지방이라 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 지방이라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009856.1.12",
          "form": "언제봤던지 기억도 안나요 ㅋㅋ",
          "original_form": "언제봤던지 기억도 안나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009856.1.13",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009856.1.14",
          "form": "원래도 문호ㅏ생활하기 어려웠는데..코로나때문에ㅠ",
          "original_form": "원래도 문호ㅏ생활하기 어려웠는데..코로나때문에ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009856.1.15",
          "form": "어딜가나 불안하죠ㅠ",
          "original_form": "어딜가나 불안하죠ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009856.1.16",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009856.1.17",
          "form": "저번달 극장 갔는데",
          "original_form": "저번달 극장 갔는데",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009856.1.18",
          "form": "아저씨들 20대 남성",
          "original_form": "아저씨들 20대 남성",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009856.1.19",
          "form": "노마스크 ㅡㅡ",
          "original_form": "노마스크 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009856.1.20",
          "form": "참나 직원은 확인도 안하고",
          "original_form": "참나 직원은 확인도 안하고",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009856.1.21",
          "form": "이제 안가려고요 ㅜㅜ정말 좋아했는데",
          "original_form": "이제 안가려고요 ㅜㅜ정말 좋아했는데",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009856.1.22",
          "form": "1년반만에 간게 그러네요",
          "original_form": "1년반만에 간게 그러네요",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009856.1.23",
          "form": "그쵸~영화보는내 벗고 마시고 먹고ㅠ",
          "original_form": "그쵸~영화보는내 벗고 마시고 먹고ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009856.1.24",
          "form": "네 참나",
          "original_form": "네 참나",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009856.1.25",
          "form": "서로 조심해줘야하는데ㅠ",
          "original_form": "서로 조심해줘야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009856.1.26",
          "form": "지키는 사람들만 지키죠ㅠ",
          "original_form": "지키는 사람들만 지키죠ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009856.1.27",
          "form": "네 세상은 그런거 같아요",
          "original_form": "네 세상은 그런거 같아요",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009856.1.28",
          "form": "바로 나오고 싶고 양화집중도 안됐어요",
          "original_form": "바로 나오고 싶고 양화집중도 안됐어요",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009856.1.29",
          "form": "영화",
          "original_form": "영화",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009856.1.30",
          "form": "서로 잘 지켜준다면 셧다운 없을텐데ㅠ",
          "original_form": "서로 잘 지켜준다면 셧다운 없을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009856.1.31",
          "form": "그렇죠 근데 힘들것 같아요",
          "original_form": "그렇죠 근데 힘들것 같아요",
          "speaker_id": "1",
          "time": "20210823 11:46"
        },
        {
          "id": "MDRW2100009856.1.32",
          "form": "요새 공연계 힘들다는데 그래도 매진되는 공연이 있더라구요",
          "original_form": "요새 공연계 힘들다는데 그래도 매진되는 공연이 있더라구요",
          "speaker_id": "1",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.33",
          "form": "공연,영화 얘기만 하면 슬퍼지네요 ㅋㅋㅋ",
          "original_form": "공연,영화 얘기만 하면 슬퍼지네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.34",
          "form": "갈 사람들은 다 가더라구요~",
          "original_form": "갈 사람들은 다 가더라구요~",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.35",
          "form": "무슨 음악공연인데 제친구 2번이나 티켓팅 실패",
          "original_form": "무슨 음악공연인데 제친구 2번이나 티켓팅 실패",
          "speaker_id": "1",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.36",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "1",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.37",
          "form": "35000원 대행도 했는데 실패했데요",
          "original_form": "35000원 대행도 했는데 실패했데요",
          "speaker_id": "1",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.38",
          "form": "신기했어요",
          "original_form": "신기했어요",
          "speaker_id": "1",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.39",
          "form": "그런거 보면 나만 너무 조심하나 싶고 ㅋㅋㅋ",
          "original_form": "그런거 보면 나만 너무 조심하나 싶고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009856.1.40",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009856.1.41",
          "form": "남편이 화내요 심하다고 극장가자고 ㅋㅋ",
          "original_form": "남편이 화내요 심하다고 극장가자고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009856.1.42",
          "form": "저 얼마전 영화티켓 그냥 버렸네요 ㅜㅜ망설이다 못봤어요",
          "original_form": "저 얼마전 영화티켓 그냥 버렸네요 ㅜㅜ망설이다 못봤어요",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009856.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋ그맘 넘 알아요~저도 애때문에 조심하는데  다들 유난이라는 눈빛 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그맘 넘 알아요~저도 애때문에 조심하는데  다들 유난이라는 눈빛 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009856.1.44",
          "form": "맞아요 ㅜㅜ외계인 됐네요 혼자 ㅋㅋ",
          "original_form": "맞아요 ㅜㅜ외계인 됐네요 혼자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009856.1.45",
          "form": "근데 아이들 걱정에 애아빠 직장도 걱정 못가겠어요",
          "original_form": "근데 아이들 걱정에 애아빠 직장도 걱정 못가겠어요",
          "speaker_id": "1",
          "time": "20210823 11:50"
        }
