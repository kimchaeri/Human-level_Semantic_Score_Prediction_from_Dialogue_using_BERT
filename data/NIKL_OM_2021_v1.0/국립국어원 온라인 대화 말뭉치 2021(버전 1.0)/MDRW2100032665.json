      "utterance": [
        {
          "id": "MDRW2100032665.1.1",
          "form": "여행은 좋아하시나요??",
          "original_form": "여행은 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032665.1.2",
          "form": "미쳐 살았죠!",
          "original_form": "미쳐 살았죠!",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032665.1.3",
          "form": "제인스타에여행사진뿐,,,",
          "original_form": "제인스타에여행사진뿐,,,",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032665.1.4",
          "form": "그 정도에요?",
          "original_form": "그 정도에요?",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032665.1.5",
          "form": "어디어디 다녀오셨어요?",
          "original_form": "어디어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032665.1.6",
          "form": "젤 마지막은 코로나가기전에",
          "original_form": "젤 마지막은 코로나가기전에",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032665.1.7",
          "form": "파리요!",
          "original_form": "파리요!",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032665.1.8",
          "form": "유럽!  너무 가고 싶었는데 부러워라 ㅠ",
          "original_form": "유럽!  너무 가고 싶었는데 부러워라 ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032665.1.9",
          "form": "사실 가려구 예약 다 해놨는데 딱 코로나 터져서 못 간거 있죠 ㅠ",
          "original_form": "사실 가려구 예약 다 해놨는데 딱 코로나 터져서 못 간거 있죠 ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032665.1.10",
          "form": "파리 어디어디 가보셨어요?",
          "original_form": "파리 어디어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032665.1.11",
          "form": "음...거의다가본거같아요",
          "original_form": "음...거의다가본거같아요",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032665.1.12",
          "form": "파리만8박9일있어서!",
          "original_form": "파리만8박9일있어서!",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032665.1.13",
          "form": "하루 디즈니간거빼고",
          "original_form": "하루 디즈니간거빼고",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032665.1.14",
          "form": "파리에만 8박 9일동안 계신거면 다른나라도 가셨었나요?",
          "original_form": "파리에만 8박 9일동안 계신거면 다른나라도 가셨었나요?",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032665.1.15",
          "form": "어디가 제일 기억이 남으세요?",
          "original_form": "어디가 제일 기억이 남으세요?",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032665.1.16",
          "form": "디즈니 ㅠ 좋았겠다 ㅠㅠ 완전 부러워요",
          "original_form": "디즈니 ㅠ 좋았겠다 ㅠㅠ 완전 부러워요",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032665.1.17",
          "form": "가서 보신것 자랑 해주세요!",
          "original_form": "가서 보신것 자랑 해주세요!",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032665.1.18",
          "form": "파리에만있었구요",
          "original_form": "파리에만있었구요",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032665.1.19",
          "form": "일단 교과서에서만보던",
          "original_form": "일단 교과서에서만보던",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032665.1.20",
          "form": "모나리자나 로댕의 작품들을 실제로본것도 그렇구",
          "original_form": "모나리자나 로댕의 작품들을 실제로본것도 그렇구",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032665.1.21",
          "form": "에펠탑은 그냥 하...",
          "original_form": "에펠탑은 그냥 하...{emoji:♥}",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032665.1.22",
          "form": "대박대박 모나리자 정말 작지 않아요? 그 그림에 사람들 되게 바글바글 모여서 보는거 봤었어요",
          "original_form": "대박대박 모나리자 정말 작지 않아요? 그 그림에 사람들 되게 바글바글 모여서 보는거 봤었어요",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032665.1.23",
          "form": "에펠탑 ㅠ 정말 꼭 가보고 싶어요",
          "original_form": "에펠탑 ㅠ 정말 꼭 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032665.1.24",
          "form": "거기다 다음 올림픽이 또 파리라 정말 가보고 싶은 장소에요 ㅠ",
          "original_form": "거기다 다음 올림픽이 또 파리라 정말 가보고 싶은 장소에요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032665.1.25",
          "form": "하 그니까요 저두 진짜 ㅠㅠㅠ",
          "original_form": "하 그니까요 저두 진짜 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032665.1.26",
          "form": "기념품으로 혹시 어떤거 사오셨나요?",
          "original_form": "기념품으로 혹시 어떤거 사오셨나요?",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032665.1.27",
          "form": "저는 어린왕자 책이랑",
          "original_form": "저는 어린왕자 책이랑",
          "speaker_id": "2",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100032665.1.28",
          "form": "파리에만 파는 스벅 텀블러",
          "original_form": "파리에만 파는 스벅 텀블러",
          "speaker_id": "2",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100032665.1.29",
          "form": "디즈니캐릭터 올라프 인형이요!",
          "original_form": "디즈니캐릭터 올라프 인형이요!",
          "speaker_id": "2",
          "time": "20210905 23:48"
        },
        {
          "id": "MDRW2100032665.1.30",
          "form": "스벅 컬렉션 모으시는군요!",
          "original_form": "스벅 컬렉션 모으시는군요!",
          "speaker_id": "1",
          "time": "20210905 23:48"
        },
        {
          "id": "MDRW2100032665.1.31",
          "form": "어린왕자 책도 상당히 의미 있는 기념품일 것 같아요",
          "original_form": "어린왕자 책도 상당히 의미 있는 기념품일 것 같아요",
          "speaker_id": "1",
          "time": "20210905 23:48"
        },
        {
          "id": "MDRW2100032665.1.32",
          "form": "올라프 너무 귀엽겠어요",
          "original_form": "올라프{emoji:♡} 너무 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032665.1.33",
          "form": "겨울왕국 첫 등장부터 굉장히 좋아하는 친구에요",
          "original_form": "겨울왕국 첫 등장부터 굉장히 좋아하는 친구에요",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032665.1.34",
          "form": "하맞아요!!!!",
          "original_form": "하맞아요!!!!",
          "speaker_id": "2",
          "time": "20210906 00:08"
        },
        {
          "id": "MDRW2100032665.1.35",
          "form": "올라프를 좋아하시다니 그렇다면",
          "original_form": "올라프를 좋아하시다니 그렇다면",
          "speaker_id": "1",
          "time": "20210906 00:10"
        }
      ]
    }
