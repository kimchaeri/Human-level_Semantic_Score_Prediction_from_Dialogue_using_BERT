      "utterance": [
        {
          "id": "MDRW2100032729.1.1",
          "form": "아 맞다 진짜 오늘 전복밥 먹었는데",
          "original_form": "아 맞다 진짜 오늘 전복밥 먹었는데",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032729.1.2",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032729.1.3",
          "form": "더더욱 제주도 살고 싶었어요ㅋㅋㅋ",
          "original_form": "더더욱 제주도 살고 싶었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032729.1.4",
          "form": "제주도에서 맨날 전복밥 먹고 싶다ㅜㅜ 이러면서 먹었어요",
          "original_form": "제주도에서 맨날 전복밥 먹고 싶다ㅜㅜ 이러면서 먹었어요",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032729.1.5",
          "form": "ㅋㅋㅋ 오징어 전복 류는 마트에 싸더라구요",
          "original_form": "ㅋㅋㅋ 오징어 전복 류는 마트에 싸더라구요",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032729.1.6",
          "form": "좋아하는 과자는요?",
          "original_form": "좋아하는 과자는요?",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032729.1.7",
          "form": "저 과자 다 좋아해요",
          "original_form": "저 과자 다 좋아해요",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032729.1.8",
          "form": "그중에서는 새우깡? 새우 맛나서 좋아해요",
          "original_form": "그중에서는 새우깡? 새우 맛나서 좋아해요",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032729.1.9",
          "form": "전 꼬북칩이랑 신당동 떡볶이과자요",
          "original_form": "전 꼬북칩이랑 신당동 떡볶이과자요",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032729.1.10",
          "form": "해물파시군요!",
          "original_form": "해물파시군요!",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032729.1.11",
          "form": "헉 대박 갑자기 꼬북칩 먹고 싶어요",
          "original_form": "헉 대박 갑자기 꼬북칩 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032729.1.12",
          "form": "제주로 오셔요",
          "original_form": "제주로 오셔요",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032729.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.14",
          "form": "네 ㅋㅋㅋ 해물파!!!",
          "original_form": "네 ㅋㅋㅋ 해물파!!!",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.15",
          "form": "진짜 넘 부러워요~",
          "original_form": "진짜 넘 부러워요~",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.16",
          "form": "전 몸에 해물이 잘 안 받아서",
          "original_form": "전 몸에 해물이 잘 안 받아서",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.17",
          "form": "아아앗 고생하시네요",
          "original_form": "아아앗 고생하시네요",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.18",
          "form": "좋아하는데 줄여서 먹어요",
          "original_form": "좋아하는데 줄여서 먹어요",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.19",
          "form": "그러시구나ㅠㅠ 저도 고기 소화 잘 안돼서 안먹어요",
          "original_form": "그러시구나ㅠㅠ 저도 고기 소화 잘 안돼서 안먹어요",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.20",
          "form": "해산물은 잘 받아요 ㅎㅎ",
          "original_form": "해산물은 잘 받아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032729.1.21",
          "form": "오 굿이네여",
          "original_form": "오 굿이네여",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032729.1.22",
          "form": "전 한의원에서 소고기 구워서 흰쌀밥 먹으랬는데",
          "original_form": "전 한의원에서 소고기 구워서 흰쌀밥 먹으랬는데",
