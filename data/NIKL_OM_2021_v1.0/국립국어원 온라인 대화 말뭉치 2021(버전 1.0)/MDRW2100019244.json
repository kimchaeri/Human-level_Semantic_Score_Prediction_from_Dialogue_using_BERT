      "utterance": [
        {
          "id": "MDRW2100019244.1.1",
          "form": "갈비 말고 다른 최애음식은 없으신가요?",
          "original_form": "갈비 말고 다른 최애음식은 없으신가요?",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019244.1.2",
          "form": "음 저는 살찌는건 다 좋아해요 ㅋㅋㅋㅋ 아이스크림 빵 ㅋㅋㅋ 빵순이에요",
          "original_form": "음 저는 살찌는건 다 좋아해요 ㅋㅋㅋㅋ 아이스크림 빵 ㅋㅋㅋ 빵순이에요",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019244.1.3",
          "form": "저도 빵 완전 좋아해요!",
          "original_form": "저도 빵 완전 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019244.1.4",
          "form": "무슨빵 좋아하세요??",
          "original_form": "무슨빵 좋아하세요??",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019244.1.5",
          "form": "전 거의 모든 빵 좋아해요!",
          "original_form": "전 거의 모든 빵 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019244.1.6",
          "form": "저두요 ㅋㅋㅋ 단거 좋아하시나요",
          "original_form": "저두요 ㅋㅋㅋ 단거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019244.1.7",
          "form": "완전 좋아해요",
          "original_form": "완전 좋아해요",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019244.1.8",
          "form": "너무 좋아해요!",
          "original_form": "너무 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019244.1.9",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019244.1.10",
          "form": "디저트 환장해요",
          "original_form": "디저트 환장해요",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019244.1.11",
          "form": "단건 다 맛있어요!",
          "original_form": "단건 다 맛있어요!",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019244.1.12",
          "form": "맞아요 엔돌핀돌아요",
          "original_form": "맞아요 엔돌핀돌아요",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019244.1.13",
          "form": "아이스크림 좋아하세요?",
          "original_form": "아이스크림 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019244.1.14",
          "form": "너무 좋아해서 문제에요",
          "original_form": "너무 좋아해서 문제에요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019244.1.15",
          "form": "어디거 드세요??",
          "original_form": "어디거 드세요??",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019244.1.16",
          "form": "저 베라 좋아해요",
          "original_form": "저 베라 좋아해요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019244.1.17",
          "form": "저두요 ㅋㅋㅋㅋ 무슨 맛 드세요??",
          "original_form": "저두요 ㅋㅋㅋㅋ 무슨 맛 드세요??",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019244.1.18",
          "form": "저 이상한나라의 솜사탕이요!",
          "original_form": "저 이상한나라의 솜사탕이요!",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019244.1.19",
          "form": "오전 그거 안먹어본거같아요",
          "original_form": "오전 그거 안먹어본거같아요",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019244.1.20",
          "form": "진짜 솜사탕맛인가요?? 상상이안가요 ㅋㅋㅋ",
          "original_form": "진짜 솜사탕맛인가요?? 상상이안가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019244.1.21",
          "form": "약간 달고나맛..? 그런 느낌이에요",
          "original_form": "약간 달고나맛..? 그런 느낌이에요",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019244.1.22",
          "form": "오호 저도 나중에 먹어보겠어요",
          "original_form": "오호 저도 나중에 먹어보겠어요",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019244.1.23",
          "form": "저는 레인보우샤베트 좋아하는데 이제 없다더라구요 ㅠㅠ",
