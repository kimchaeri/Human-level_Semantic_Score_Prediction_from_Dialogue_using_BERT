      "utterance": [
        {
          "id": "MDRW2100029891.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029891.1.2",
          "form": "요즘 뭐 근래에 사신것 있으신가요?",
          "original_form": "요즘 뭐 근래에 사신것 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029891.1.3",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029891.1.4",
          "form": "저 폰케이스 새로 샀어요",
          "original_form": "저 폰케이스 새로 샀어요",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029891.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029891.1.6",
          "form": "어떤재질로 사셨나요?",
          "original_form": "어떤재질로 사셨나요?",
          "speaker_id": "1",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029891.1.7",
          "form": "재질까진 정확하게 모르겠네요ㅋㅋ",
          "original_form": "재질까진 정확하게 모르겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029891.1.8",
          "form": "플라스틱? 카드도 넣을수있는걸루 샀어요",
          "original_form": "플라스틱? 카드도 넣을수있는걸루 샀어요",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029891.1.9",
          "form": "편의성 좋은게 저도 좋은거 같아요!",
          "original_form": "편의성 좋은게 저도 좋은거 같아요!",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029891.1.10",
          "form": "저두 이번에ㅜ갤럭시워치샀거든요 !",
          "original_form": "저두 이번에ㅜ갤럭시워치샀거든요 !",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029891.1.11",
          "form": "우와 요새 워치 사는 사람들이 많네요",
          "original_form": "우와 요새 워치 사는 사람들이 많네요",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029891.1.12",
          "form": "곧생일인데 사게됐어요",
          "original_form": "곧생일인데 사게됐어요",
          "speaker_id": "1",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029891.1.13",
          "form": "생일선물이군요ㅋㅋㅋㅋ",
          "original_form": "생일선물이군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029891.1.14",
          "form": "ㅋㅋㅋㅋ저한테 주는 소소한 선뮬이랄까...?",
          "original_form": "ㅋㅋㅋㅋ저한테 주는 소소한 선뮬이랄까...?",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029891.1.15",
          "form": "좋네요ㅋㅋ 전 요새 노트북 사고싶더라구요",
          "original_form": "좋네요ㅋㅋ 전 요새 노트북 사고싶더라구요",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029891.1.16",
          "form": "아~ 어떤용도로 사시려구요?",
          "original_form": "아~ 어떤용도로 사시려구요?",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029891.1.17",
          "form": "사무작업은 거의 안해서ㅋㅋ 게임도 돌아가면 좋을거 같긴 해요",
          "original_form": "사무작업은 거의 안해서ㅋㅋ 게임도 돌아가면 좋을거 같긴 해요",
          "speaker_id": "2",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029891.1.18",
          "form": "저두 요즘 사촌누나 노트북 선물을 해줬어서",
          "original_form": "저두 요즘 사촌누나 노트북 선물을 해줬어서",
          "speaker_id": "1",
          "time": "20210903 16:20"
        }
      ]
