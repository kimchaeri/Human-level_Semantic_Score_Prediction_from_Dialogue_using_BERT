      "utterance": [
        {
          "id": "MDRW2100039281.1.1",
          "form": "무슨 캐릭터 제일 좋아하세요?",
          "original_form": "무슨 캐릭터 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039281.1.2",
          "form": "저는 뱀파이어맛 쿠키 제일좋아했었어욬ㅋㅋㅋㅋ",
          "original_form": "저는 뱀파이어맛 쿠키 제일좋아했었어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039281.1.3",
          "form": "ㅋㅋ 뱀파이어 쿠키 좋죠",
          "original_form": "ㅋㅋ 뱀파이어 쿠키 좋죠",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039281.1.4",
          "form": "달릴때 속도감이 진짜",
          "original_form": "달릴때 속도감이 진짜",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039281.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ그리고 생김새도 좋아했어요 그리고 무슨 치즈? 황금색으로 된 돈많이 나오는 캐릭터도 좋아했어욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그리고 생김새도 좋아했어요 그리고 무슨 치즈? 황금색으로 된 돈많이 나오는 캐릭터도 좋아했어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039281.1.6",
          "form": "치즈케이크맛이요?",
          "original_form": "치즈케이크맛이요?",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039281.1.7",
          "form": "아맞아요! 그이름이었던것같아욬ㅋㅋㅋ",
          "original_form": "아맞아요! 그이름이었던것같아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039281.1.8",
          "form": "ㅋㅋ 그 친구 덕분에 부자 됐었다구요",
          "original_form": "ㅋㅋ 그 친구 덕분에 부자 됐었다구요",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039281.1.9",
          "form": "ㅋㅋㅋㅋ돈많이 벌어서 좋아했었어욬ㅋㅋㅋㅋㅋ좀비맛쿠키?도 귀여웠어욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ돈많이 벌어서 좋아했었어욬ㅋㅋㅋㅋㅋ좀비맛쿠키?도 귀여웠어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039281.1.10",
          "form": "으어으어 하면서 뛰는거ㅋㅋ",
          "original_form": "으어으어 하면서 뛰는거ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039281.1.11",
          "form": "부활해도 몇초 못가고 쓰러지는게 킬링 포인트죠",
          "original_form": "부활해도 몇초 못가고 쓰러지는게 킬링 포인트죠",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039281.1.12",
          "form": "ㅋㅋㅋㅋㅋ멍/청해보여서 귀여웠어욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ멍/청해보여서 귀여웠어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039281.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039281.1.14",
          "form": "혹시 쿠키런 딱지 모아보셨나요",
          "original_form": "혹시 쿠키런 딱지 모아보셨나요",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039281.1.15",
          "form": "고무딱지",
          "original_form": "고무딱지",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039281.1.16",
          "form": "아니용ㅋㅋㅋㅋㅋ쿠키런으로 고무딱지도 나왔었나용??신기해욬ㅋㅋㅋㅋ",
          "original_form": "아니용ㅋㅋㅋㅋㅋ쿠키런으로 고무딱지도 나왔었나용??신기해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039281.1.17",
          "form": "예전에 카카오 시절에 나왔는데",
          "original_form": "예전에 카카오 시절에 나왔는데",
          "speaker_id": "1",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100039281.1.18",
          "form": "모르는 분들이 많으시네요",
          "original_form": "모르는 분들이 많으시네요",
          "speaker_id": "1",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039281.1.19",
          "form": "와신기해욬ㅋㅋㅋㅋㅋ쿠키런으로 이제 빵도 나오겠어욬ㅋㅋ",
          "original_form": "와신기해욬ㅋㅋㅋㅋㅋ쿠키런으로 이제 빵도 나오겠어욬ㅋㅋ",
