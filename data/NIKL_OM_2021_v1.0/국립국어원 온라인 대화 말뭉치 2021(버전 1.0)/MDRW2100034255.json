      "utterance": [
        {
          "id": "MDRW2100034255.1.1",
          "form": "헐 그게 겜 이름이에요?",
          "original_form": "헐 그게 겜 이름이에요?",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034255.1.2",
          "form": "ㅋㅋㅋㅋ 찾아봐야겠네요",
          "original_form": "ㅋㅋㅋㅋ 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034255.1.3",
          "form": "네네! RPG 게임인 것 같더라구여",
          "original_form": "네네! RPG 게임인 것 같더라구여",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034255.1.4",
          "form": "오호 캐릭터 봤는데",
          "original_form": "오호 캐릭터 봤는데",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034255.1.5",
          "form": "약간 지브리 스타일이에요ㅋㅋㅋ",
          "original_form": "약간 지브리 스타일이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034255.1.6",
          "form": "네네ㅋㅋㅋㅋ 약간 모마와 완전 다르더라구여",
          "original_form": "네네ㅋㅋㅋㅋ 약간 모마와 완전 다르더라구여",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034255.1.7",
          "form": "ㅋㅋ 저런게임 좋아하면",
          "original_form": "ㅋㅋ 저런게임 좋아하면",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100034255.1.8",
          "form": "재밌을 고 같아요",
          "original_form": "재밌을 고 같아요",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100034255.1.9",
          "form": "저는 아직은 흥미가 안 생겨 미루고있어요ㅋㅋ",
          "original_form": "저는 아직은 흥미가 안 생겨 미루고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034255.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 전 좀 고민해보고..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 전 좀 고민해보고..",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100034255.1.11",
          "form": "근데 저것보단 약간 단순한 게임을",
          "original_form": "근데 저것보단 약간 단순한 게임을",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034255.1.12",
          "form": "하고싶긴해요",
          "original_form": "하고싶긴해요",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034255.1.13",
          "form": "저도 스토리 있는 게임보다 단순한 게임을 찾아보려구여ㅠ",
          "original_form": "저도 스토리 있는 게임보다 단순한 게임을 찾아보려구여ㅠ",
          "speaker_id": "1",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100034255.1.14",
          "form": "저랑 비슷하시네요 ㅎㅎ",
          "original_form": "저랑 비슷하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034255.1.15",
          "form": "약간 생각 없이 하고싶은데 잘 없네요ㅠ",
          "original_form": "약간 생각 없이 하고싶은데 잘 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100034255.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034255.1.17",
          "form": "머리 안굴리고..",
          "original_form": "머리 안굴리고..",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034255.1.18",
          "form": "그저 할 수 있는 게임...",
          "original_form": "그저 할 수 있는 게임...",
          "speaker_id": "1",
          "time": "20210828 13:04"
        },
