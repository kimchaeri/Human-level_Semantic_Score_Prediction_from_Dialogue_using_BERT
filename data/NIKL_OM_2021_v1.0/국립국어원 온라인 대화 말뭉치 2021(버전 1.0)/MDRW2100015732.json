      "utterance": [
        {
          "id": "MDRW2100015732.1.1",
          "form": "요즘 처서라 그런가 살짝 쌀쌀해요",
          "original_form": "요즘 처서라 그런가 살짝 쌀쌀해요",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015732.1.2",
          "form": "그러네요.",
          "original_form": "그러네요.",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015732.1.3",
          "form": "비도 가끔씩 오고.",
          "original_form": "비도 가끔씩 오고.",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015732.1.4",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015732.1.5",
          "form": "저는 경기도에 살지만 남부지역 사는 친구들은 비 많이 왔다고 하더라구요.",
          "original_form": "저는 경기도에 살지만 남부지역 사는 친구들은 비 많이 왔다고 하더라구요.",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015732.1.6",
          "form": "뉴스 봤어요.",
          "original_form": "뉴스 봤어요.",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015732.1.7",
          "form": "비 엄청 오던데...",
          "original_form": "비 엄청 오던데...",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015732.1.8",
          "form": "장마 다 끝났는데 이게 웬 날벼락인지",
          "original_form": "장마 다 끝났는데 이게 웬 날벼락인지",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015732.1.9",
          "form": "모르겠어요.",
          "original_form": "모르겠어요.",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015732.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015732.1.11",
          "form": "그래도 덥지 않아서 다행이에요",
          "original_form": "그래도 덥지 않아서 다행이에요",
          "speaker_id": "2",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015732.1.12",
          "form": "맞아요. 이번 여름 엄청 더웠어요.",
          "original_form": "맞아요. 이번 여름 엄청 더웠어요.",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015732.1.13",
          "form": "저는 선풍기 계속 틀었어요.",
          "original_form": "저는 선풍기 계속 틀었어요.",
          "speaker_id": "2",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015732.1.14",
          "form": "저두요. 진짜 더운 날에는 카페로 피서 갔어요.",
          "original_form": "저두요. 진짜 더운 날에는 카페로 피서 갔어요.",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015732.1.15",
          "form": "아이스 아메리카노 마시고 그랬어요",
          "original_form": "아이스 아메리카노 마시고 그랬어요",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015732.1.16",
          "form": "더울 때 집에 있으면 힘들죠...",
          "original_form": "더울 때 집에 있으면 힘들죠...",
          "speaker_id": "2",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015732.1.17",
          "form": "그래도 곧 가을이잖아요.",
          "original_form": "그래도 곧 가을이잖아요.",
          "speaker_id": "2",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015732.1.18",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015732.1.19",
          "form": "혹시 어떤 계절 좋아하세요?",
          "original_form": "혹시 어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015732.1.20",
          "form": "저는 봄 좋아해요.",
          "original_form": "저는 봄 좋아해요.",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015732.1.21",
          "form": "꽃을 좋아해서",
          "original_form": "꽃을 좋아해서",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015732.1.22",
          "form": "저도 봄 좋아요!!",
          "original_form": "저도 봄 좋아요!!",
          "speaker_id": "1",
          "time": "20210826 00:44"
        }
