      "utterance": [
        {
          "id": "MDRW2100023544.1.1",
          "form": "대탈출 봤어?",
          "original_form": "대탈출 봤어?",
          "speaker_id": "2",
          "time": "20210830 11:18"
        },
        {
          "id": "MDRW2100023544.1.2",
          "form": "아니 아직ㅠ",
          "original_form": "아니 아직ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100023544.1.3",
          "form": "어땠어?",
          "original_form": "어땠어?",
          "speaker_id": "1",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100023544.1.4",
          "form": "어젠 좀 무섭더라",
          "original_form": "어젠 좀 무섭더라",
          "speaker_id": "2",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100023544.1.5",
          "form": "아진짜? 많이무서워?",
          "original_form": "아진짜? 많이무서워?",
          "speaker_id": "1",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100023544.1.6",
          "form": "악마나오긴했는데 낮에 볼만해",
          "original_form": "악마나오긴했는데 낮에 볼만해",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100023544.1.7",
          "form": "악마 실체가 나왔어?",
          "original_form": "악마 실체가 나왔어?",
          "speaker_id": "1",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100023544.1.8",
          "form": "실체? 악마가 보였어!",
          "original_form": "실체? 악마가 보였어!",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100023544.1.9",
          "form": "말해도돼?",
          "original_form": "말해도돼?",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100023544.1.10",
          "form": "악마가 실제로 나온거지?\n음..",
          "original_form": "악마가 실제로 나온거지?\n음..",
          "speaker_id": "1",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100023544.1.11",
          "form": "그래해봐",
          "original_form": "그래해봐",
          "speaker_id": "1",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100023544.1.12",
          "form": "아니 스포는 아닌데",
          "original_form": "아니 스포는 아닌데",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100023544.1.13",
          "form": "맞아 실제로 나왔어",
          "original_form": "맞아 실제로 나왔어",
          "speaker_id": "2",
          "time": "20210830 11:21"
        },
        {
          "id": "MDRW2100023544.1.14",
          "form": "헐..무섭게 생겼어?",
          "original_form": "헐..무섭게 생겼어?",
          "speaker_id": "1",
          "time": "20210830 11:22"
        },
        {
          "id": "MDRW2100023544.1.15",
          "form": "딱 첨에 볼때는 좀? 쭉 보면 괜찮아",
          "original_form": "딱 첨에 볼때는 좀? 쭉 보면 괜찮아",
          "speaker_id": "2",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100023544.1.16",
          "form": "연출이 조금 무섭달까?",
          "original_form": "연출이 조금 무섭달까?",
          "speaker_id": "2",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100023544.1.17",
          "form": "얼굴도 나와?",
          "original_form": "얼굴도 나와?",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100023544.1.18",
          "form": "응 나와ㅋㅋ",
          "original_form": "응 나와ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:23"
        }
      ]
