      "utterance": [
        {
          "id": "MDRW2100003495.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003495.1.2",
          "form": "네 코로나라 못가지만",
          "original_form": "네 코로나라 못가지만",
          "speaker_id": "2",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003495.1.3",
          "form": "여행 좋아해요",
          "original_form": "여행 좋아해요",
          "speaker_id": "2",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003495.1.4",
          "form": "제일 좋았던 여행지가 어디예요?",
          "original_form": "제일 좋았던 여행지가 어디예요?",
          "speaker_id": "1",
          "time": "20210821 09:21"
        },
        {
          "id": "MDRW2100003495.1.5",
          "form": "저는 스위스요 ㅎㅎ",
          "original_form": "저는 스위스요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:27"
        },
        {
          "id": "MDRW2100003495.1.6",
          "form": "진째 평온 그 자체였어요",
          "original_form": "진째 평온 그 자체였어요",
          "speaker_id": "2",
          "time": "20210821 09:27"
        },
        {
          "id": "MDRW2100003495.1.7",
          "form": "헙 저도 스위스 좋아요ㅠㅠ",
          "original_form": "헙 저도 스위스 좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003495.1.8",
          "form": "평온하고 공기맑고...",
          "original_form": "평온하고 공기맑고...",
          "speaker_id": "1",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003495.1.9",
          "form": "진짜 물가만 아니면 눌러살고싶죠 ㅋㅋ",
          "original_form": "진짜 물가만 아니면 눌러살고싶죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003495.1.10",
          "form": "물가는 허거덕 숨막혀요",
          "original_form": "물가는 허거덕 숨막혀요",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003495.1.11",
          "form": "맞아요..ㅋㅋㅋㅋ 스위스에선 딱히 뭘 사먹은 기억도 없어요",
          "original_form": "맞아요..ㅋㅋㅋㅋ 스위스에선 딱히 뭘 사먹은 기억도 없어요",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100003495.1.12",
          "form": "학생때 여행간거라ㅠ",
          "original_form": "학생때 여행간거라ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100003495.1.13",
          "form": "저도 ㅋㅋ",
          "original_form": "저도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100003495.1.14",
          "form": "마트에서 작은 초콜렛 정도만 사먹었어요",
          "original_form": "마트에서 작은 초콜렛 정도만 사먹었어요",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100003495.1.15",
          "form": "넘 그립네요",
          "original_form": "넘 그립네요",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100003495.1.16",
          "form": "편의점에서 샌드위치 사먹었는데 넘 비싸고 양 작아서 놀란 기억이 있네요.. 관광지여서 그렇긴했지만 ㅋㅋㅋ",
          "original_form": "편의점에서 샌드위치 사먹었는데 넘 비싸고 양 작아서 놀란 기억이 있네요.. 관광지여서 그렇긴했지만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100003495.1.17",
          "form": "유럽도 좋지만 동남아쪽은 물가가 싸서 심적으로 편해서 좋은거같아요",
          "original_form": "유럽도 좋지만 동남아쪽은 물가가 싸서 심적으로 편해서 좋은거같아요",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100003495.1.18",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100003495.1.19",
          "form": "거긴 특히 과일이 싸고 맛있어서 좋아요",
          "original_form": "거긴 특히 과일이 싸고 맛있어서 좋아요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100003495.1.20",
          "form": "동남아는 어디 여행 해보셨나요?",
          "original_form": "동남아는 어디 여행 해보셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100003495.1.21",
          "form": "세부가봤어요",
          "original_form": "세부가봤어요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100003495.1.22",
          "form": "세부는 물이 넘 맑고 좋아보이더라구요.. 여행가서 호핑투어? 하셨나요?",
          "original_form": "세부는 물이 넘 맑고 좋아보이더라구요.. 여행가서 호핑투어? 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100003495.1.23",
          "form": "호핑투어하려던 찰나에 같이 간 조카가 울어서 못했어요",
          "original_form": "호핑투어하려던 찰나에 같이 간 조카가 울어서 못했어요",
          "speaker_id": "2",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100003495.1.24",
          "form": "아이고.. 아쉬우시겠어요. 호핑이 세부 여행의 하이라이트라던데..!",
          "original_form": "아이고.. 아쉬우시겠어요. 호핑이 세부 여행의 하이라이트라던데..!",
          "speaker_id": "1",
