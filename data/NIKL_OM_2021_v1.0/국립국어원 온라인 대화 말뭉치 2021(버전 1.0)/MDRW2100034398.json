      "utterance": [
        {
          "id": "MDRW2100034398.1.1",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210828 04:20"
        },
        {
          "id": "MDRW2100034398.1.2",
          "form": "저는 대나무칫솔이요",
          "original_form": "저는 대나무칫솔이요",
          "speaker_id": "2",
          "time": "20210828 04:21"
        },
        {
          "id": "MDRW2100034398.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210828 04:21"
        },
        {
          "id": "MDRW2100034398.1.4",
          "form": "저는 작은 스탠드 조명 샀어용ㅎㅎ",
          "original_form": "저는 작은 스탠드 조명 샀어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 04:24"
        },
        {
          "id": "MDRW2100034398.1.5",
          "form": "어디에쓰시나요",
          "original_form": "어디에쓰시나요",
          "speaker_id": "2",
          "time": "20210828 04:25"
        },
        {
          "id": "MDRW2100034398.1.6",
          "form": "자기전에 핸드폰 잠깐할때 써요ㅎㅎ",
          "original_form": "자기전에 핸드폰 잠깐할때 써요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 04:26"
        },
        {
          "id": "MDRW2100034398.1.7",
          "form": "너무 밝은곳에 있다가 갑자기 불 끄면 잠이 안와서ㅠ",
          "original_form": "너무 밝은곳에 있다가 갑자기 불 끄면 잠이 안와서ㅠ",
          "speaker_id": "1",
          "time": "20210828 04:26"
        },
        {
          "id": "MDRW2100034398.1.8",
          "form": "아 저는지금 불켜고있어요",
          "original_form": "아 저는지금 불켜고있어요",
          "speaker_id": "2",
          "time": "20210828 04:26"
        },
        {
          "id": "MDRW2100034398.1.9",
          "form": "불끄고 폰보면 눈나빠져서요",
          "original_form": "불끄고 폰보면 눈나빠져서요",
          "speaker_id": "2",
          "time": "20210828 04:27"
        },
        {
          "id": "MDRW2100034398.1.10",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 04:27"
        },
        {
          "id": "MDRW2100034398.1.11",
          "form": "그래서 와트 좀 높은걸로 led 전구 쓰는데",
          "original_form": "그래서 와트 좀 높은걸로 led 전구 쓰는데",
          "speaker_id": "1",
          "time": "20210828 04:27"
        },
        {
          "id": "MDRW2100034398.1.12",
          "form": "적당한것같아요",
          "original_form": "적당한것같아요",
          "speaker_id": "1",
          "time": "20210828 04:27"
        },
        {
          "id": "MDRW2100034398.1.13",
          "form": "오 많이 알아보셨군요",
          "original_form": "오 많이 알아보셨군요",
          "speaker_id": "2",
          "time": "20210828 04:28"
        },
        {
          "id": "MDRW2100034398.1.14",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 04:29"
        },
        {
          "id": "MDRW2100034398.1.15",
          "form": "사고싶은 물건 있으세요?",
          "original_form": "사고싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210828 04:29"
        },
        {
          "id": "MDRW2100034398.1.16",
          "form": "어디제품이 좋은가요",
          "original_form": "어디제품이 좋은가요",
          "speaker_id": "2",
          "time": "20210828 04:29"
        },
        {
          "id": "MDRW2100034398.1.17",
          "form": "음 그냥 저는 눈에 예쁜거 사요ㅋㅋㅋ",
          "original_form": "음 그냥 저는 눈에 예쁜거 사요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:29"
        },
        {
          "id": "MDRW2100034398.1.18",
          "form": "가격대는 중요하지않아용",
          "original_form": "가격대는 중요하지않아용",
          "speaker_id": "1",
          "time": "20210828 04:29"
        },
        {
          "id": "MDRW2100034398.1.19",
          "form": "아니 위에는 성능살피시더니ㅋ",
          "original_form": "아니 위에는 성능살피시더니ㅋ",
          "speaker_id": "2",
          "time": "20210828 04:29"
        },
        {
          "id": "MDRW2100034398.1.20",
          "form": "ㅋㅋㅋ예쁜거도 보시는군요",
          "original_form": "ㅋㅋㅋ예쁜거도 보시는군요",
          "speaker_id": "2",
          "time": "20210828 04:30"
        },
        {
          "id": "MDRW2100034398.1.21",
          "form": "네 디자인도 포기못해서요ㅋㅋ",
          "original_form": "네 디자인도 포기못해서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:30"
        }
