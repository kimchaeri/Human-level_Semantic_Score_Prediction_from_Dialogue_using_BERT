      "utterance": [
        {
          "id": "MDRW2100018189.1.1",
          "form": "부인이요???",
          "original_form": "부인이요???",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018189.1.2",
          "form": "네 기억났어요",
          "original_form": "네 기억났어요",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018189.1.3",
          "form": "부인이 자작극 한거요",
          "original_form": "부인이 자작극 한거요",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018189.1.4",
          "form": "오 보험때문인가요?",
          "original_form": "오 보험때문인가요?",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018189.1.5",
          "form": "아니요 남편이 넘 못살게 굴어서",
          "original_form": "아니요 남편이 넘 못살게 굴어서",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018189.1.6",
          "form": "남편한테 복수하려고",
          "original_form": "남편한테 복수하려고",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018189.1.7",
          "form": "남편이 살인한 것처럼",
          "original_form": "남편이 살인한 것처럼",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018189.1.8",
          "form": "꾸민거에요",
          "original_form": "꾸민거에요",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018189.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018189.1.10",
          "form": "소름돋아요",
          "original_form": "소름돋아요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018189.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018189.1.12",
          "form": "근데 역시 귀신 얘기보다",
          "original_form": "근데 역시 귀신 얘기보다",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018189.1.13",
          "form": "너무 불쾌하네요 ㅠㅠ",
          "original_form": "너무 불쾌하네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018189.1.14",
          "form": "네 재미를 넘어선...",
          "original_form": "네 재미를 넘어선...",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018189.1.15",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018189.1.16",
          "form": "네 이런건 좀 별로네요",
          "original_form": "네 이런건 좀 별로네요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018189.1.17",
          "form": "심괴 이런 방향으로 가면",
          "original_form": "심괴 이런 방향으로 가면",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018189.1.18",
          "form": "안 볼 것 같아요",
          "original_form": "안 볼 것 같아요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018189.1.19",
          "form": "네.. 끝맛이 안 좋네요",
          "original_form": "네.. 끝맛이 안 좋네요",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018189.1.20",
          "form": "네 그냥 당혹사나 봐야겠어요",
          "original_form": "네 그냥 당혹사나 봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
