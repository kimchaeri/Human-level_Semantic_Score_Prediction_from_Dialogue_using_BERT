      "utterance": [
        {
          "id": "MDRW2100030454.1.1",
          "form": "둥근당면이 조아용",
          "original_form": "둥근당면이 조아용",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030454.1.2",
          "form": "담에 찜닭도 같이 먹어용",
          "original_form": "담에 찜닭도 같이 먹어용",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030454.1.3",
          "form": "같이 할게만군여 먹을것두 많구",
          "original_form": "같이 할게만군여 먹을것두 많구",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030454.1.4",
          "form": "로제찜닭 한번 먹어보고 싶슴다!ㅠㅠ",
          "original_form": "로제찜닭 한번 먹어보고 싶슴다!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100030454.1.5",
          "form": "분모자 넣은 것도 먹어보고 싶어유",
          "original_form": "분모자 넣은 것도 먹어보고 싶어유",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100030454.1.6",
          "form": "분모자 마저 한번은 먹어보고싶당ㅋㅋㅋㅋ",
          "original_form": "분모자 마저 한번은 먹어보고싶당ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100030454.1.7",
          "form": "name1님은 그때 두찜 같이 못갓던가",
          "original_form": "&name1&님은 그때 두찜 같이 못갓던가",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100030454.1.8",
          "form": "두찜에 로제찜닭팔던데",
          "original_form": "두찜에 로제찜닭팔던데",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100030454.1.9",
          "form": "얍얍 함께하지 못했슴다ㅠㅠ",
          "original_form": "얍얍 함께하지 못했슴다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100030454.1.10",
          "form": "두찜 로제 맛있나유?",
          "original_form": "두찜 로제 맛있나유?",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100030454.1.11",
          "form": "흠 나쁘지않았습니다",
          "original_form": "흠 나쁘지않았습니다",
          "speaker_id": "2",
          "time": "20210906 02:23"
        },
        {
          "id": "MDRW2100030454.1.12",
          "form": "아님 담에 같이",
          "original_form": "아님 담에 같이",
          "speaker_id": "2",
          "time": "20210906 02:23"
        },
        {
          "id": "MDRW2100030454.1.13",
          "form": "동성로에 먹으러갈래?",
          "original_form": "동성로에 먹으러갈래?",
          "speaker_id": "2",
          "time": "20210906 02:23"
        },
        {
          "id": "MDRW2100030454.1.14",
          "form": "파스타면 넣어주는 찜닭 파는 곳 있던데",
          "original_form": "파스타면 넣어주는 찜닭 파는 곳 있던데",
          "speaker_id": "2",
          "time": "20210906 02:23"
        },
        {
          "id": "MDRW2100030454.1.15",
          "form": "홀 파스타면? 좋아유 좋아유 파스타면은 또 색다르구만",
          "original_form": "홀 파스타면? 좋아유 좋아유 파스타면은 또 색다르구만",
          "speaker_id": "1",
          "time": "20210906 13:47"
        },
        {
          "id": "MDRW2100030454.1.16",
          "form": "식당이름이 뭐쥬",
          "original_form": "식당이름이 뭐쥬",
          "speaker_id": "1",
          "time": "20210906 13:47"
        },
        {
          "id": "MDRW2100030454.1.17",
          "form": "동성로 오키도키 찜닭이라구",
          "original_form": "동성로 오키도키 찜닭이라구",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100030454.1.18",
          "form": "합니다",
          "original_form": "합니다",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100030454.1.19",
          "form": "오 처음 들어봐유 나중에 동성로 가서 먹읍시다ㅎㅅㅎ",
          "original_form": "오 처음 들어봐유 나중에 동성로 가서 먹읍시다ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210906 18:46"
        },
        {
          "id": "MDRW2100030454.1.20",
          "form": "조아용 납작만두는근데 추가해야하나벼",
          "original_form": "조아용 납작만두는근데 추가해야하나벼",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100030454.1.21",
          "form": "아하 그냥 주는게 아니라 추가하는거였군ㅠㅠ",
          "original_form": "아하 그냥 주는게 아니라 추가하는거였군ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100030454.1.22",
          "form": "웅ㅠㅠ 3000원같더라",
          "original_form": "웅ㅠㅠ 3000원같더라",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100030454.1.23",
          "form": "그날가서땡기면먹ㄱᆢ",
          "original_form": "그날가서땡기면먹ㄱᆢ",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100030454.1.24",
          "form": "먹고",
          "original_form": "먹고",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100030454.1.25",
          "form": "아님마는걸로..",
          "original_form": "아님마는걸로..",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100030454.1.26",
          "form": "오케 애들 다 먹고싶다고 하면 먹자ㅋㅋㅋ",
          "original_form": "오케 애들 다 먹고싶다고 하면 먹자ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100030454.1.27",
          "form": "조아용 벌써 기대가되는군여",
          "original_form": "조아용 벌써 기대가되는군여",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100030454.1.28",
          "form": "같이먹을거많다~",
          "original_form": "같이먹을거많다~",
          "speaker_id": "2",
          "time": "20210907 18:25"
        }
      ]
