      "utterance": [
        {
          "id": "MDRW2100010379.1.1",
          "form": "안녕하세요 최군에 스포츠 보신거 있으세요?",
          "original_form": "안녕하세요 최군에 스포츠 보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010379.1.2",
          "form": "최근에 메달 경기 봤죵",
          "original_form": "최근에 메달 경기 봤죵",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100010379.1.3",
          "form": "네 저도 올림픽 재밌게 봤어요ㅎㅎ",
          "original_form": "네 저도 올림픽 재밌게 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100010379.1.4",
          "form": "그춍ㅎㅎ저두 잼있게 봤어요",
          "original_form": "그춍ㅎㅎ저두 잼있게 봤어요",
          "speaker_id": "2",
          "time": "20210825 10:22"
        },
        {
          "id": "MDRW2100010379.1.5",
          "form": "네 스포츠 잘안보는데 감동적이더라구요",
          "original_form": "네 스포츠 잘안보는데 감동적이더라구요",
          "speaker_id": "1",
          "time": "20210825 10:23"
        },
        {
          "id": "MDRW2100010379.1.6",
          "form": "메달경기는 재미있죵ㅎㅎ",
          "original_form": "메달경기는 재미있죵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 10:23"
        },
        {
          "id": "MDRW2100010379.1.7",
          "form": "네 선수분들이 정말 열심히 하더라구요ㅋㅋ",
          "original_form": "네 선수분들이 정말 열심히 하더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:24"
        },
        {
          "id": "MDRW2100010379.1.8",
          "form": "맞아요ㅋㅋㅋ그만큼 열심히 했으니깐요",
          "original_form": "맞아요ㅋㅋㅋ그만큼 열심히 했으니깐요",
          "speaker_id": "2",
          "time": "20210825 10:24"
        },
        {
          "id": "MDRW2100010379.1.9",
          "form": "저도 막 열심히 살아야겠단 의지가ㅋㅋ",
          "original_form": "저도 막 열심히 살아야겠단 의지가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:25"
        },
        {
          "id": "MDRW2100010379.1.10",
          "form": "스포츠경기 또 보고 싶네요",
          "original_form": "스포츠경기 또 보고 싶네요",
          "speaker_id": "2",
          "time": "20210825 10:26"
        },
        {
          "id": "MDRW2100010379.1.11",
          "form": "축구도 되게 좋아하는데",
          "original_form": "축구도 되게 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 10:26"
        },
        {
          "id": "MDRW2100010379.1.12",
          "form": "코로나 때문에",
          "original_form": "코로나 때문에",
          "speaker_id": "2",
          "time": "20210825 10:27"
        },
        {
          "id": "MDRW2100010379.1.13",
          "form": "월드컵할려나요ㅜㅜ",
          "original_form": "월드컵할려나요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 10:27"
        },
        {
          "id": "MDRW2100010379.1.14",
          "form": "오 어디응원하세요?",
          "original_form": "오 어디응원하세요?",
          "speaker_id": "1",
          "time": "20210825 10:27"
        },
        {
          "id": "MDRW2100010379.1.15",
          "form": "아 카타르라 열릴거같긴해요ㅋㅋ",
          "original_form": "아 카타르라 열릴거같긴해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:27"
        },
        {
          "id": "MDRW2100010379.1.16",
          "form": "스포츠경기 하고나서 코로나가 더 심해졌다는.....",
          "original_form": "스포츠경기 하고나서 코로나가 더 심해졌다는.....",
          "speaker_id": "2",
          "time": "20210825 10:27"
