        },
        {
          "id": "MDRW2100017267.1.3",
          "form": "할퀴고",
          "original_form": "할퀴고",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.4",
          "form": "저희누나 고양이 키우는데 성격진짜 드럽던데",
          "original_form": "저희누나 고양이 키우는데 성격진짜 드럽던데",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.5",
          "form": "하지말라는 거 다 하고",
          "original_form": "하지말라는 거 다 하고",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.6",
          "form": "아.. 고양이는 다 비슷한가봐요",
          "original_form": "아.. 고양이는 다 비슷한가봐요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.7",
          "form": "훈련이 안되니까",
          "original_form": "훈련이 안되니까",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.8",
          "form": "아 난 진짜 고양이는 안되는구나",
          "original_form": "아 난 진짜 고양이는 안되는구나",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.9",
          "form": "고양이는 어려워요",
          "original_form": "고양이는 어려워요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.10",
          "form": "너무 어려워요",
          "original_form": "너무 어려워요",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.11",
          "form": "name2님잘못이아니에요",
          "original_form": "&name2&님잘못이아니에요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.12",
          "form": "사람으로 따지면",
          "original_form": "사람으로 따지면",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017267.1.13",
          "form": "너무 예쁜데 성격이 안좋은 여자같아요",
          "original_form": "너무 예쁜데 성격이 안좋은 여자같아요",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017267.1.14",
          "form": "ㅋㅋㅋㅋ 어떤느낌인지 알거같아요",
          "original_form": "ㅋㅋㅋㅋ 어떤느낌인지 알거같아요",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017267.1.15",
          "form": "하지만 너무 예뻐서",
          "original_form": "하지만 너무 예뻐서",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017267.1.16",
          "form": "계속 선물을 바치게되는...",
          "original_form": "계속 선물을 바치게되는...",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017267.1.17",
          "form": "그런 느낌이었어요",
          "original_form": "그런 느낌이었어요",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017267.1.18",
          "form": "고양이는 제가 가서 재롱을 떨어야 하더라구요",
