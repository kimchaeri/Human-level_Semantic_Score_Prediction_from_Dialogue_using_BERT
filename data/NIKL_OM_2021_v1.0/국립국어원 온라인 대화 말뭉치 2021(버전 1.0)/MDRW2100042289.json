      "utterance": [
        {
          "id": "MDRW2100042289.1.1",
          "form": "저는 메이커로 편하게 내려마시죠 ㅎㅎ",
          "original_form": "저는 메이커로 편하게 내려마시죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042289.1.2",
          "form": "커피메이커에서 내릴때 커피 냄새도 좋죠?",
          "original_form": "커피메이커에서 내릴때 커피 냄새도 좋죠?",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042289.1.3",
          "form": "집안에 커피향이 나잔아요",
          "original_form": "집안에 커피향이 나잔아요",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042289.1.4",
          "form": "그 향때문에 커피를 포기 못해요",
          "original_form": "그 향때문에 커피를 포기 못해요",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042289.1.5",
          "form": "제가 커피 마시면 잠을 못자서 진짜 그림의 떡인데",
          "original_form": "제가 커피 마시면 잠을 못자서 진짜 그림의 떡인데",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042289.1.6",
          "form": "향이 너무 좋아요",
          "original_form": "향이 너무 좋아요",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042289.1.7",
          "form": "마자요 커피는 포기못하죠",
          "original_form": "마자요 커피는 포기못하죠",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042289.1.8",
          "form": "저는 아메리카노도 좀 진한걸 좋아해요",
          "original_form": "저는 아메리카노도 좀 진한걸 좋아해요",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042289.1.9",
          "form": "얼마나 맛있을까요",
          "original_form": "얼마나 맛있을까요",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042289.1.10",
          "form": "이제는 따뜻한 아메가 더 땡기는 계절이 되었네요",
          "original_form": "이제는 따뜻한 아메가 더 땡기는 계절이 되었네요",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042289.1.11",
          "form": "저는 여름에도 뜨아 마셔요",
          "original_form": "저는 여름에도 뜨아 마셔요",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042289.1.12",
          "form": "그래서 얼죽아인 분들이 신기해요",
          "original_form": "그래서 얼죽아인 분들이 신기해요",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042289.1.13",
          "form": "저는 계절에 맞게 갈아타며 마시죠^^",
          "original_form": "저는 계절에 맞게 갈아타며 마시죠^^",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042289.1.14",
          "form": "전통차도 좋아하세요?",
          "original_form": "전통차도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042289.1.15",
          "form": "네~ 왠만한 먹거리는 다 잘먹어요",
          "original_form": "네~ 왠만한 먹거리는 다 잘먹어요",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042289.1.16",
          "form": "진한 대추차도 점점 좋아지더라구요",
          "original_form": "진한 대추차도 점점 좋아지더라구요",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042289.1.17",
          "form": "요새 마시고 있어요",
          "original_form": "요새 마시고 있어요",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042289.1.18",
          "form": "대추가 숙면에 좋다고 해서 ㅎㅎ",
          "original_form": "대추가 숙면에 좋다고 해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042289.1.19",
          "form": "와~직접해서 드시는군요",
          "original_form": "와~직접해서 드시는군요",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042289.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 06:28"
        },
        {
          "id": "MDRW2100042289.1.21",
          "form": "저는 배,대추를 끓여서 먹어보긴했어요",
          "original_form": "저는 배,대추를 끓여서 먹어보긴했어요",
          "speaker_id": "2",
          "time": "20210907 06:49"
        },
        {
          "id": "MDRW2100042289.1.22",
          "form": "걸죽한 대추차 해보고싶은데 레시피가 어렵더라구요",
          "original_form": "걸죽한 대추차 해보고싶은데 레시피가 어렵더라구요",
          "speaker_id": "2",
          "time": "20210907 06:50"
        },
        {
          "id": "MDRW2100042289.1.23",
          "form": "대추와 통밀 감초 넣고 끓이면",
          "original_form": "대추와 통밀 감초 넣고 끓이면",
          "speaker_id": "1",
          "time": "20210907 08:28"
        },
        {
          "id": "MDRW2100042289.1.24",
          "form": "숙면에 정말 도움이 돼요",
          "original_form": "숙면에 정말 도움이 돼요",
          "speaker_id": "1",
          "time": "20210907 08:28"
        },
        {
          "id": "MDRW2100042289.1.25",
          "form": "꼭 해봐야겠어요",
          "original_form": "꼭 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042289.1.26",
          "form": "이건 약사가 추천해준 방식이라 괜찮더라구요",
          "original_form": "이건 약사가 추천해준 방식이라 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042289.1.27",
          "form": "저는 숙면을 못하는 편이라 꼭 먹어보고 싶어요",
          "original_form": "저는 숙면을 못하는 편이라 꼭 먹어보고 싶어요",
          "speaker_id": "2",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100042289.1.28",
          "form": "오 그렇다면 진짜 해보세요",
          "original_form": "오 그렇다면 진짜 해보세요",
          "speaker_id": "1",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100042289.1.29",
          "form": "전 효과를 봤어요",
          "original_form": "전 효과를 봤어요",
          "speaker_id": "1",
          "time": "20210907 17:04"
        }
      ]
