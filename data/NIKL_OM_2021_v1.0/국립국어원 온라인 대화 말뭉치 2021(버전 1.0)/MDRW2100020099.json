      "utterance": [
        {
          "id": "MDRW2100020099.1.1",
          "form": "최근 쇼핑한 상품 뭐있으세요?",
          "original_form": "최근 쇼핑한 상품 뭐있으세요?",
          "speaker_id": "1",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100020099.1.2",
          "form": "바닐라코 클렌징밤 샀어요!",
          "original_form": "바닐라코 클렌징밤 샀어요!",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100020099.1.3",
          "form": "그게뭐에요?",
          "original_form": "그게뭐에요?",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100020099.1.4",
          "form": "화장 지우는 오일이 밤 형식으로 되어있다고 보시면 돼요!",
          "original_form": "화장 지우는 오일이 밤 형식으로 되어있다고 보시면 돼요!",
          "speaker_id": "2",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100020099.1.5",
          "form": "화장한지 오래라 잘 몰랐어요",
          "original_form": "화장한지 오래라 잘 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100020099.1.6",
          "form": "사실 저도 밤은 처음 사보는 거라서 신기해요ㅎㅎ",
          "original_form": "사실 저도 밤은 처음 사보는 거라서 신기해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100020099.1.7",
          "form": "최근에 쇼핑한 상품 있으신가요?",
          "original_form": "최근에 쇼핑한 상품 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100020099.1.8",
          "form": "액정타블렛 샀어요",
          "original_form": "액정타블렛 샀어요",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100020099.1.9",
          "form": "오! 혹시 그림 그리시나요?",
          "original_form": "오! 혹시 그림 그리시나요?",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100020099.1.10",
          "form": "이제부터 해보려고 샀어요",
          "original_form": "이제부터 해보려고 샀어요",
          "speaker_id": "1",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100020099.1.11",
          "form": "우와! 전 그림 그리시는 분들 보면 멋있더라구요",
          "original_form": "우와! 전 그림 그리시는 분들 보면 멋있더라구요",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100020099.1.12",
          "form": "차근차근 배우면",
          "original_form": "차근차근 배우면",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100020099.1.13",
          "form": "누구나 할 수 있을거 같더라구요",
          "original_form": "누구나 할 수 있을거 같더라구요",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100020099.1.14",
          "form": "일종의 공부같더군요",
          "original_form": "일종의 공부같더군요",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100020099.1.15",
          "form": "그런 마인드 너무 멋있어요...",
          "original_form": "그런 마인드 너무 멋있어요...",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100020099.1.16",
          "form": "전 항상 해보기도 전에 포기해버려서..",
          "original_form": "전 항상 해보기도 전에 포기해버려서..",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100020099.1.17",
          "form": "저도 기회가 되면 한번 도전해봐야겠어요!",
          "original_form": "저도 기회가 되면 한번 도전해봐야겠어요!",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100020099.1.18",
          "form": "저도 그림은 재능있는 사람만 하는줄 알았거든요",
          "original_form": "저도 그림은 재능있는 사람만 하는줄 알았거든요",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100020099.1.19",
          "form": "근데 그리는 방식(?) 같은게 있었어요",
          "original_form": "근데 그리는 방식(?) 같은게 있었어요",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100020099.1.20",
          "form": "거기서 시키는대로 그리니 나름대로 그려지더라구요 ㅋㅋ 나중에 서점가서 책한번 살펴보세요 인터넷에도 자료많아요",
          "original_form": "거기서 시키는대로 그리니 나름대로 그려지더라구요 ㅋㅋ 나중에 서점가서 책한번 살펴보세요 인터넷에도 자료많아요",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100020099.1.21",
          "form": "아 맞아요! 지나가다 강의 보면 방식을 다 알려주더라고요",
          "original_form": "아 맞아요! 지나가다 강의 보면 방식을 다 알려주더라고요",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100020099.1.22",
          "form": "무료 강의 찾아보고 한번 흥미를 가져봐야겠어요",
          "original_form": "무료 강의 찾아보고 한번 흥미를 가져봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        }
      ]
    }
  ]
