      "utterance": [
        {
          "id": "MDRW2100006867.1.1",
          "form": "가을날씨에요",
          "original_form": "가을날씨에요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006867.1.2",
          "form": "이불을 여름이불을 빨아서 정리해야겠어요",
          "original_form": "이불을 여름이불을 빨아서 정리해야겠어요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006867.1.3",
          "form": "저두 밤엔 좀 쌀쌀해요",
          "original_form": "저두 밤엔 좀 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006867.1.4",
          "form": "너무 시원하고 좋지않나요",
          "original_form": "너무 시원하고 좋지않나요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006867.1.5",
          "form": "에어건 아닌 바람좋죠",
          "original_form": "에어건 아닌 바람좋죠",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006867.1.6",
          "form": "에어컨 바람과 자연바람은 180도 달라요",
          "original_form": "에어컨 바람과 자연바람은 180도 달라요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006867.1.7",
          "form": "그러게요 그래서 밤마다 산책가요",
          "original_form": "그러게요 그래서 밤마다 산책가요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006867.1.8",
          "form": "산책은 아침보다 밤이 좋더라구요 하루를 마감하는 기분",
          "original_form": "산책은 아침보다 밤이 좋더라구요 하루를 마감하는 기분",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006867.1.9",
          "form": "점점걷기 좋아져요",
          "original_form": "점점걷기 좋아져요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006867.1.10",
          "form": "면역이 좋아지려면 겨울에도 걸어야해요",
          "original_form": "면역이 좋아지려면 겨울에도 걸어야해요",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006867.1.11",
          "form": "겨울이 오면 건조해지죠",
          "original_form": "겨울이 오면 건조해지죠",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006867.1.12",
          "form": "여름은 너무 습하고요",
          "original_form": "여름은 너무 습하고요",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006867.1.13",
          "form": "비가좀 오면 금새추워져요",
          "original_form": "비가좀 오면 금새추워져요",
