      "utterance": [
        {
          "id": "MDRW2100000425.1.1",
          "form": "오늘 점심 드셨나요??",
          "original_form": "오늘 점심 드셨나요??",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000425.1.2",
          "form": "아니요 지금까지 공복입니다ㅠ",
          "original_form": "아니요 지금까지 공복입니다ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000425.1.3",
          "form": "어쩌다 아직 공복이세요 ㅠ ㅠ 뭐 드실 생각이세요?",
          "original_form": "어쩌다 아직 공복이세요 ㅠ ㅠ 뭐 드실 생각이세요?",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000425.1.4",
          "form": "이것저것 하다보니 놓쳤어요ㅎ",
          "original_form": "이것저것 하다보니 놓쳤어요ㅎ",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000425.1.5",
          "form": "저녁에 배달시키려구요",
          "original_form": "저녁에 배달시키려구요",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000425.1.6",
          "form": "저녁까지 참아야겠네요 배달맛집있나요?",
          "original_form": "저녁까지 참아야겠네요 배달맛집있나요?",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000425.1.7",
          "form": "맛집은 아니지만.. 1인 1닭 준비중 입니다",
          "original_form": "맛집은 아니지만.. 1인 1닭 준비중 입니다",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000425.1.8",
          "form": "설마 치킨이신가요? 아님 기력회복 삼계탕?",
          "original_form": "설마 치킨이신가요? 아님 기력회복 삼계탕?",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000425.1.9",
          "form": "치킨 입니다 치느님은 언제나 최고죠",
          "original_form": "치킨 입니다 치느님은 언제나 최고죠",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000425.1.10",
          "form": "저도 거의 일주일에 두번씩 치킨먹다가 내장지방 만드는 음식 1위가 치킨이라는 말 듣고 충격먹었어요 ㅠ",
          "original_form": "저도 거의 일주일에 두번씩 치킨먹다가 내장지방 만드는 음식 1위가 치킨이라는 말 듣고 충격먹었어요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000425.1.11",
          "form": "일주일에 두번씩.. 존경합니다",
          "original_form": "일주일에 두번씩.. 존경합니다",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000425.1.12",
          "form": "요리는 따로 안하시나요",
          "original_form": "요리는 따로 안하시나요",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000425.1.13",
          "form": "하려고 하는데 맨날 남으니까 다 썩어버리더라구요 ㅠ 아까워요",
          "original_form": "하려고 하는데 맨날 남으니까 다 썩어버리더라구요 ㅠ 아까워요",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000425.1.14",
          "form": "이제 슬슬 치킨 주문해야겠어요",
          "original_form": "이제 슬슬 치킨 주문해야겠어요",
          "speaker_id": "1",
          "time": "20210819 16:27"
        }
      ]
