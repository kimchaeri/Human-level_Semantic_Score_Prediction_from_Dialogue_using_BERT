      "utterance": [
        {
          "id": "MDRW2100019728.1.1",
          "form": "청소 매일 하세요?",
          "original_form": "청소 매일 하세요?",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019728.1.2",
          "form": "청소기는 매일 돌려요",
          "original_form": "청소기는 매일 돌려요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019728.1.3",
          "form": "저는 식재료관리가 제일 어려운데",
          "original_form": "저는 식재료관리가 제일 어려운데",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019728.1.4",
          "form": "어떻게 하고계신가요?",
          "original_form": "어떻게 하고계신가요?",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019728.1.5",
          "form": "팁있나요?",
          "original_form": "팁있나요?",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019728.1.6",
          "form": "식재료는 각 식재료의 특징을 알고 있어야해요 ㅎㅎ",
          "original_form": "식재료는 각 식재료의 특징을 알고 있어야해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019728.1.7",
          "form": "어떤건 냉장고에 넣으면 안되는 것도 있더라구요",
          "original_form": "어떤건 냉장고에 넣으면 안되는 것도 있더라구요",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019728.1.8",
          "form": "양파나 마요네즈 같은거 말하시는거죠",
          "original_form": "양파나 마요네즈 같은거 말하시는거죠",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019728.1.9",
          "form": "또 어떤게 있는지 알려주세요",
          "original_form": "또 어떤게 있는지 알려주세요",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019728.1.10",
          "form": "네 맞아요 ㅎㅎ 근데 마요네즈는 냉장보관이에요 ㅜㅜ",
          "original_form": "네 맞아요 ㅎㅎ 근데 마요네즈는 냉장보관이에요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019728.1.11",
          "form": "고구마, 감자는 실온보관 하는 게 좋고 바나나랑 아보카도도 마찬가지에요!",
          "original_form": "고구마, 감자는 실온보관 하는 게 좋고 바나나랑 아보카도도 마찬가지에요!",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019728.1.12",
          "form": "헉 마요네즈 냉장보관하면 상하기 쉽다던데요? 잘못알고있는건가요ㅠ",
          "original_form": "헉 마요네즈 냉장보관하면 상하기 쉽다던데요? 잘못알고있는건가요ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019728.1.13",
          "form": "아보카도도 실온이군요 팁감사해요",
          "original_form": "아보카도도 실온이군요 팁감사해요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019728.1.14",
          "form": "아니에요! 마요네즈 실온에 두면 분리돼요 ㅋㅋㅋ",
          "original_form": "아니에요! 마요네즈 실온에 두면 분리돼요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019728.1.15",
          "form": "그리고 바나나는 냉장고에 넣으면 까매져요... 원래 얘가 열대에서 온 과일이라서요",
          "original_form": "그리고 바나나는 냉장고에 넣으면 까매져요... 원래 얘가 열대에서 온 과일이라서요",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019728.1.16",
          "form": "그렇군요 제일싫어하는 집안일이 뭐예요?",
          "original_form": "그렇군요 제일싫어하는 집안일이 뭐예요?",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019728.1.17",
          "form": "설거지요 ㅋㅋㅋㅋ",
          "original_form": "설거지요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019728.1.18",
          "form": "식기세척기 사고 싶을 정도에요 정말",
          "original_form": "식기세척기 사고 싶을 정도에요 정말",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019728.1.19",
          "form": "본가에서 쓰는데 식기세척기 요즘 잘나와서 살만한거 같아요",
          "original_form": "본가에서 쓰는데 식기세척기 요즘 잘나와서 살만한거 같아요",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019728.1.20",
          "form": "엘지에서 나온거 사고싶더라구요...",
          "original_form": "엘지에서 나온거 사고싶더라구요...",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019728.1.21",
          "form": "그게 설거지 전문가가 하는 것보다 훨씬 깨끗하게 된대요",
          "original_form": "그게 설거지 전문가가 하는 것보다 훨씬 깨끗하게 된대요",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019728.1.22",
          "form": "이젠 식세기랑 건조기는...살수있으면 얼른 사는게 맞는거 같아요",
          "original_form": "이젠 식세기랑 건조기는...살수있으면 얼른 사는게 맞는거 같아요",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019728.1.23",
          "form": "ㅋㅋㅋ자취집에선 들일 공간도 없어요 ㅜㅜ",
          "original_form": "ㅋㅋㅋ자취집에선 들일 공간도 없어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 22:17"
        }
      ]
    }
  ]
