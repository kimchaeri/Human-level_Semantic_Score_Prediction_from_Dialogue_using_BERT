      "utterance": [
        {
          "id": "MDRW2100013026.1.1",
          "form": "음식 잘하세요?",
          "original_form": "음식 잘하세요?",
          "speaker_id": "1",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100013026.1.2",
          "form": "아니요 먹는건 잘 하는것 같네요 ㅋㅋ",
          "original_form": "아니요 먹는건 잘 하는것 같네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:32"
        },
        {
          "id": "MDRW2100013026.1.3",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100013026.1.4",
          "form": "완전 빵순이에요 밀가루 끊어야하는데ㅜ",
          "original_form": "완전 빵순이에요 밀가루 끊어야하는데ㅜ",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100013026.1.5",
          "form": "저도요!! 빵집 어디 좋아하세요?",
          "original_form": "저도요!! 빵집 어디 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013026.1.6",
          "form": "크라상점 가보고싶어요 ㅜㅜ",
          "original_form": "크라상점 가보고싶어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013026.1.7",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013026.1.8",
          "form": "오 들어본적 있는거같아요!",
          "original_form": "오 들어본적 있는거같아요!",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013026.1.9",
          "form": "어디지역에있어요??",
          "original_form": "어디지역에있어요??",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013026.1.10",
          "form": "그냥 프랜차이즈같아요 ㅎㅎ",
          "original_form": "그냥 프랜차이즈같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013026.1.11",
          "form": "무슨 빵 즐겨드세여?",
          "original_form": "무슨 빵 즐겨드세여?",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013026.1.12",
          "form": "저는 초코 크림 같은게 들은 달달한 빵이좋더라고요",
          "original_form": "저는 초코 크림 같은게 들은 달달한 빵이좋더라고요",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100013026.1.13",
          "form": "우아 저도요!! 과자도 초코파이 같은거 좋아해용",
          "original_form": "우아 저도요!! 과자도 초코파이 같은거 좋아해용",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100013026.1.14",
          "form": "뭔가 좀건강식으로 먹어야하는데 당분이 땡겨서 큰일이네요 ㅎㅎ",
          "original_form": "뭔가 좀건강식으로 먹어야하는데 당분이 땡겨서 큰일이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100013026.1.15",
          "form": "그니까요 ㅜㅜ",
          "original_form": "그니까요 ㅜㅜ",
          "speaker_id": "1",
