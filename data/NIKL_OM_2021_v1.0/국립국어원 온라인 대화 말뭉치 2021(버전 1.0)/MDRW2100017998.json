      "utterance": [
        {
          "id": "MDRW2100017998.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017998.1.2",
          "form": "동호회나 봉사활동 하는거 있어?",
          "original_form": "동호회나 봉사활동 하는거 있어?",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017998.1.3",
          "form": "전에는 태권도 동아리했었고 지금은안해",
          "original_form": "전에는 태권도 동아리했었고 지금은안해",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017998.1.4",
          "form": "가끔 네이버 기부 그거로 기부해",
          "original_form": "가끔 네이버 기부 그거로 기부해",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017998.1.5",
          "form": "나도 네이버 기부 빈 가끔해",
          "original_form": "나도 네이버 기부 빈 가끔해",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017998.1.6",
          "form": "태권도 동아리 지금은 왜 안해?",
          "original_form": "태권도 동아리 지금은 왜 안해?",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017998.1.7",
          "form": "바빠서 할시간이 없어졌어",
          "original_form": "바빠서 할시간이 없어졌어",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017998.1.8",
          "form": "기회되면 다시하고싶어 이번엔 유도동아리로",
          "original_form": "기회되면 다시하고싶어 이번엔 유도동아리로",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017998.1.9",
          "form": "하고싶네",
          "original_form": "하고싶네",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017998.1.10",
          "form": "선생님.. 부끄러우니 존댓말하시죠..",
          "original_form": "선생님.. 부끄러우니 존댓말하시죠..",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017998.1.11",
          "form": "넵ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017998.1.12",
          "form": "태권도도 그렇고 유도도 그렇고 운동 좋아하시나봐요",
          "original_form": "태권도도 그렇고 유도도 그렇고 운동 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017998.1.13",
          "form": "네 돈많으면 운동 하루에 두개씩하고싶어욬ㅋㅋㅋ",
          "original_form": "네 돈많으면 운동 하루에 두개씩하고싶어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017998.1.14",
          "form": "멋진사람.....",
          "original_form": "멋진사람.....{emoji:♡}",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100017998.1.15",
          "form": "운동 꾸준히하기가 진짜 어려운거같아요",
          "original_form": "운동 꾸준히하기가 진짜 어려운거같아요",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100017998.1.16",
          "form": "학원 안다니고 동아리로 하는 이유가 있어요?",
          "original_form": "학원 안다니고 동아리로 하는 이유가 있어요?",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100017998.1.17",
          "form": "대학교에서 동아리모집하더라고요 타학교 졸업생도요",
          "original_form": "대학교에서 동아리모집하더라고요 타학교 졸업생도요",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100017998.1.18",
          "form": "친구가 먼저 다니다가 저 데려갔어요~",
          "original_form": "친구가 먼저 다니다가 저 데려갔어요~",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017998.1.19",
          "form": "가격도 월에 3만원인가 4만원인가",
          "original_form": "가격도 월에 3만원인가 4만원인가",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017998.1.20",
          "form": "그랬슴다",
          "original_form": "그랬슴다",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017998.1.21",
          "form": "우와 좋은거같아요 가격도 저렴하고",
          "original_form": "우와 좋은거같아요 가격도 저렴하고",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100017998.1.22",
          "form": "바빠지셔서 못한다니 아쉽네요ㅠㅠ",
          "original_form": "바빠지셔서 못한다니 아쉽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100017998.1.23",
          "form": "저도 동아리나 동호회같은거 나가보고싶은데 요즘 코로나가 심해서 시작할 엄두가 안나요",
          "original_form": "저도 동아리나 동호회같은거 나가보고싶은데 요즘 코로나가 심해서 시작할 엄두가 안나요",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017998.1.24",
          "form": "오오맞아요",
          "original_form": "오오맞아요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017998.1.25",
          "form": "너무 짜증나요",
          "original_form": "너무 짜증나요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017998.1.26",
          "form": "어떤거 하고싶으세요?",
          "original_form": "어떤거 하고싶으세요?",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100017998.1.27",
          "form": "야구 동호회 들고싶어요",
          "original_form": "야구 동호회 들고싶어요",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100017998.1.28",
          "form": "야구 되게 좋아해서 코로나 전에는 혼자도 종종 갔었는데",
          "original_form": "야구 되게 좋아해서 코로나 전에는 혼자도 종종 갔었는데",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.29",
          "form": "헉 찢었다",
          "original_form": "헉 찢었다",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.30",
          "form": "간지 찢었다",
          "original_form": "간지 찢었다",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.31",
          "form": "동호회 들어서 사람들이랑 같이 응원하고싶어요ㅎㅎㅎㅎ",
          "original_form": "동호회 들어서 사람들이랑 같이 응원하고싶어요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.32",
          "form": "넘 멋있는데요?",
          "original_form": "넘 멋있는데요?",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.33",
          "form": "아하~~",
          "original_form": "아하~~",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 제가사는 지역에 야구장이 있어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 제가사는 지역에 야구장이 있어서",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.35",
          "form": "직접하는건 아닌가요?",
          "original_form": "직접하는건 아닌가요?",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100017998.1.36",
          "form": "앗 넵 하는건 아니고 보러다니는거 좋아해요",
          "original_form": "앗 넵 하는건 아니고 보러다니는거 좋아해요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100017998.1.37",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100017998.1.38",
          "form": "그런동호회도있군요 신기해요",
          "original_form": "그런동호회도있군요 신기해요",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100017998.1.39",
          "form": "진짜잼쓸것같아요 ㅋㅋㅋㅋ",
          "original_form": "진짜잼쓸것같아요 ㅋㅋㅋㅋ",
