      "utterance": [
        {
          "id": "MDRW2100035864.1.1",
          "form": "헐 그런분들은 진짜 대단하신분들이에요",
          "original_form": "헐 그런분들은 진짜 대단하신분들이에요",
          "speaker_id": "2",
          "time": "20210830 09:42"
        },
        {
          "id": "MDRW2100035864.1.2",
          "form": "3시간... 덜덜",
          "original_form": "3시간... 덜덜",
          "speaker_id": "1",
          "time": "20210830 09:42"
        },
        {
          "id": "MDRW2100035864.1.3",
          "form": "전...매번지각했어요",
          "original_form": "전...매번지각했어요",
          "speaker_id": "2",
          "time": "20210830 09:42"
        },
        {
          "id": "MDRW2100035864.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:42"
        },
        {
          "id": "MDRW2100035864.1.5",
          "form": "통학시간동안 뭐하셨어용?",
          "original_form": "통학시간동안 뭐하셨어용?",
          "speaker_id": "1",
          "time": "20210830 09:42"
        },
        {
          "id": "MDRW2100035864.1.6",
          "form": "저도 지각파에욬ㅋㅋㅋㅋ",
          "original_form": "저도 지각파에욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:42"
        },
        {
          "id": "MDRW2100035864.1.7",
          "form": "저는 그때 예능보고 드라마보고 그랬어용!",
          "original_form": "저는 그때 예능보고 드라마보고 그랬어용!",
          "speaker_id": "1",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.8",
          "form": "저 통학시간 내내 잤어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 통학시간 내내 잤어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.9",
          "form": "헐 신기하다! 전 진짜 계속 잤어요",
          "original_form": "헐 신기하다! 전 진짜 계속 잤어요",
          "speaker_id": "2",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.10",
          "form": "아아ㅠㅠㅠ 9시면... 그럴만도....",
          "original_form": "아아ㅠㅠㅠ 9시면... 그럴만도....",
          "speaker_id": "1",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.11",
          "form": "제가 13학번이잖아용?",
          "original_form": "제가 13학번이잖아용?",
          "speaker_id": "1",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.12",
          "form": "그 통학시간 알차게 쓰시는 분들 너무 대단해요..",
          "original_form": "그 통학시간 알차게 쓰시는 분들 너무 대단해요..",
          "speaker_id": "2",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.13",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.14",
          "form": "딱 12~13년도 이때쯤부터",
          "original_form": "딱 12~13년도 이때쯤부터",
          "speaker_id": "1",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.15",
          "form": "스마트폰이 너도나도 있기시작하면서",
          "original_form": "스마트폰이 너도나도 있기시작하면서",
          "speaker_id": "1",
          "time": "20210830 09:43"
        },
        {
          "id": "MDRW2100035864.1.16",
          "form": "스마트폰에 담아서 드라마도 보고",
          "original_form": "스마트폰에 담아서 드라마도 보고",
          "speaker_id": "1",
          "time": "20210830 09:44"
        },
        {
          "id": "MDRW2100035864.1.17",
          "form": "이게 너무 좋았어요",
          "original_form": "이게 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210830 09:44"
        },
        {
          "id": "MDRW2100035864.1.18",
          "form": "행운이라고 생각해요!ㅋㅋㅋㅋ",
          "original_form": "행운이라고 생각해요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:44"
        },
        {
          "id": "MDRW2100035864.1.19",
          "form": "아! 헐 그렇네요! 너무 좋았겠다 그쵸!!!!!!",
          "original_form": "아! 헐 그렇네요! 너무 좋았겠다 그쵸!!!!!!",
          "speaker_id": "2",
          "time": "20210830 09:44"
        },
        {
          "id": "MDRW2100035864.1.20",
          "form": "학교에서 스마트폰 전용 어플도 만들었는데 가장 처음 써본 학번이기도하고ㅎㅎ",
          "original_form": "학교에서 스마트폰 전용 어플도 만들었는데 가장 처음 써본 학번이기도하고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 09:44"
        },
        {
          "id": "MDRW2100035864.1.21",
          "form": "헐 신기해요 학교전용 어플이...약간 에타 같은거겠죠?!",
          "original_form": "헐 신기해요 학교전용 어플이...약간 에타 같은거겠죠?!",
          "speaker_id": "2",
          "time": "20210830 09:45"
        },
        {
          "id": "MDRW2100035864.1.22",
          "form": "네네! 대숲 에타 이런거?ㅎㅎ",
          "original_form": "네네! 대숲 에타 이런거?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 09:45"
        },
        {
          "id": "MDRW2100035864.1.23",
          "form": "우와... 그때당시에 어플 막 만들어졌으면 그런 오류 있지 않나요?!",
          "original_form": "우와... 그때당시에 어플 막 만들어졌으면 그런 오류 있지 않나요?!",
          "speaker_id": "2",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100035864.1.24",
          "form": "많았어요ㅠㅠㅋㅋㅋ",
          "original_form": "많았어요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100035864.1.25",
          "form": "그래서 매번 업데이트하고 그랬죵...ㅎㅎ",
          "original_form": "그래서 매번 업데이트하고 그랬죵...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 09:46"
        }
      ]
