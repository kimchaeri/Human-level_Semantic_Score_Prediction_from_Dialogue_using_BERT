      "utterance": [
        {
          "id": "MDRW2100027138.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027138.1.2",
          "form": "전 음식 거의 다 좋아해요 ㅎㅎ",
          "original_form": "전 음식 거의 다 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027138.1.3",
          "form": "오늘 저녁은 전어회먹어요!",
          "original_form": "오늘 저녁은 전어회먹어요!",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027138.1.4",
          "form": "저녁 뭐드시나요~?",
          "original_form": "저녁 뭐드시나요~?",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027138.1.5",
          "form": "돼지고기 볶아 먹을려구요",
          "original_form": "돼지고기 볶아 먹을려구요",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100027138.1.6",
          "form": "간장양념인가요~?",
          "original_form": "간장양념인가요~?",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100027138.1.7",
          "form": "남편이 김치찌개해서 먹었어요.ㅋㅋ",
          "original_form": "남편이 김치찌개해서 먹었어요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100027138.1.8",
          "form": "오 부러워요! 전 동생이랑 사는데 요리를 거의 안해요ㅠ",
          "original_form": "오 부러워요! 전 동생이랑 사는데 요리를 거의 안해요ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100027138.1.9",
          "form": "저만 하고",
          "original_form": "저만 하고",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100027138.1.10",
          "form": "사실 저도 요샌 별로 안하지만 ㅋㅋ",
          "original_form": "사실 저도 요샌 별로 안하지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100027138.1.11",
          "form": "백신 맞고 팔 아프다고 했더니 해 준 거에요. 평소에는 안 해요.ㅋㅋ",
          "original_form": "백신 맞고 팔 아프다고 했더니 해 준 거에요. 평소에는 안 해요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100027138.1.12",
          "form": "아 ㅋㅋㅋㅋㅋ 맛은 있으셨나요??ㅎㅎ",
          "original_form": "아 ㅋㅋㅋㅋㅋ 맛은 있으셨나요??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:31"
        },
        {
          "id": "MDRW2100027138.1.13",
          "form": "네. 맛있더라구요. 저는 물에 빠진 고기 잘 안 먹어서 김치만 먹었지만요.ㅋㅋㅋ",
          "original_form": "네. 맛있더라구요. 저는 물에 빠진 고기 잘 안 먹어서 김치만 먹었지만요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:33"
        },
        {
          "id": "MDRW2100027138.1.14",
          "form": "전 고기라면 다 좋아해요",
          "original_form": "전 고기라면 다 좋아해요",
          "speaker_id": "2",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100027138.1.15",
          "form": "근데 고기가 환경오염의 주범이기도하대서.. 요즘엔 덩어리 고기를 자주 안먹고잇어요. 비건은 아니고 비덩이라고 하더라구요 ㅎㅎㅎ",
          "original_form": "근데 고기가 환경오염의 주범이기도하대서.. 요즘엔 덩어리 고기를 자주 안먹고잇어요. 비건은 아니고 비덩이라고 하더라구요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100027138.1.16",
          "form": "아예 안먹긴 힘들고 최대한 줄여보려는 노력입니다..!",
          "original_form": "아예 안먹긴 힘들고 최대한 줄여보려는 노력입니다..!",
          "speaker_id": "2",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100027138.1.17",
          "form": "삼겹살 먹고싶을때 2번 중 1번은 생선구이 먹는다던지..ㅎㅎ 그러고있어요",
          "original_form": "삼겹살 먹고싶을때 2번 중 1번은 생선구이 먹는다던지..ㅎㅎ 그러고있어요",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100027138.1.18",
          "form": "주말엔 삼겹살먹고싶네요..ㅠ",
          "original_form": "주말엔 삼겹살먹고싶네요..ㅠ",
          "speaker_id": "2",
          "time": "20210903 08:39"
        },
        {
          "id": "MDRW2100027138.1.19",
          "form": "오...환경보호 중이시네요. ^^ 삼겹살 이야기하니 삼겹살 먹고 싶어요.ㅋㅋㅋ",
          "original_form": "오...환경보호 중이시네요. ^^ 삼겹살 이야기하니 삼겹살 먹고 싶어요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100027138.1.20",
          "form": "삼겹살이랑 같이 먹는 반찬 뭐ㅓ 좋아하시나요~?",
          "original_form": "삼겹살이랑 같이 먹는 반찬 뭐ㅓ 좋아하시나요~?",
          "speaker_id": "2",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100027138.1.21",
          "form": "상추에 고 추 마늘 양파 김치 간장에 절인 깻잎 넣어서 싸 먹어요",
          "original_form": "상추에 고 추 마늘 양파 김치 간장에 절인 깻잎 넣어서 싸 먹어요",
          "speaker_id": "1",
          "time": "20210903 19:31"
        },
        {
          "id": "MDRW2100027138.1.22",
          "form": "야채 좋아하시나봐요 ㅎㅎ",
          "original_form": "야채 좋아하시나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100027138.1.23",
          "form": "전 구운김치가 젤 좋더라구요",
          "original_form": "전 구운김치가 젤 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 20:17"
        }
      ]
