      "utterance": [
        {
          "id": "MDRW2100027198.1.1",
          "form": "유투브같은건 뭐 보시는거있으세요?",
          "original_form": "유투브같은건 뭐 보시는거있으세요?",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027198.1.2",
          "form": "저는 음악채널많이틀어놓고 들어요!",
          "original_form": "저는 음악채널많이틀어놓고 들어요!",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027198.1.3",
          "form": "저도 음악채널 자주 틀어놔요",
          "original_form": "저도 음악채널 자주 틀어놔요",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027198.1.4",
          "form": "에센셜로 잘 듣고 있습니다",
          "original_form": "에센셜로 잘 듣고 있습니다",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027198.1.5",
          "form": "맞아요 뭐할때 틀어놓고 하면 좋더라구요",
          "original_form": "맞아요 뭐할때 틀어놓고 하면 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027198.1.6",
          "form": "땡스포커밍이라고 아시나요",
          "original_form": "땡스포커밍이라고 아시나요",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027198.1.7",
          "form": "아니요 처음 들어봐요",
          "original_form": "아니요 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027198.1.8",
          "form": "재즈위주 선곡하는 유투번데 플레이리스트 틀어놓기 좋아요!",
          "original_form": "재즈위주 선곡하는 유투번데 플레이리스트 틀어놓기 좋아요!",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027198.1.9",
          "form": "오 재즈 위주라니 좋네요",
          "original_form": "오 재즈 위주라니 좋네요",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027198.1.10",
          "form": "한번 들어봐야겠어요",
          "original_form": "한번 들어봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027198.1.11",
          "form": "저도 지금 틀어놓고",
          "original_form": "저도 지금 틀어놓고",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027198.1.12",
          "form": "설거지중이에요",
          "original_form": "설거지중이에요",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027198.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027198.1.14",
          "form": "앜ㅋㅋ 집안일중이셨군요",
          "original_form": "앜ㅋㅋ 집안일중이셨군요",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027198.1.15",
          "form": "네 거실티비로 유툽틀어놓고 식빵에 쨈발라먹었서요",
          "original_form": "네 거실티비로 유툽틀어놓고 식빵에 쨈발라먹었서요",
          "speaker_id": "1",
          "time": "20210902 13:03"
        },
        {
          "id": "MDRW2100027198.1.16",
          "form": "앜 맛있었겠어요",
          "original_form": "앜 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027198.1.17",
          "form": "영화보는 거 좋아하세요?",
          "original_form": "영화보는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027198.1.18",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027198.1.19",
          "form": "이번에 샹치? 개봉한거 보러가려구요",
          "original_form": "이번에 샹치? 개봉한거 보러가려구요",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027198.1.20",
          "form": "마블영화 좋아하시는구나",
          "original_form": "마블영화 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027198.1.21",
          "form": "저도 예전엔 좋아했는데",
          "original_form": "저도 예전엔 좋아했는데",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027198.1.22",
          "form": "인피니티워 끝나니깐 뭔가 팍 식더라구요ㅠ",
          "original_form": "인피니티워 끝나니깐 뭔가 팍 식더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027198.1.23",
          "form": "아이언맨 캡아 돌려내!",
          "original_form": "아이언맨 캡아 돌려내!",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027198.1.24",
          "form": "아쉽긴하죠ㅠ 하지만 세대교체는 해야하니",
          "original_form": "아쉽긴하죠ㅠ 하지만 세대교체는 해야하니",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027198.1.25",
          "form": "어쩔 수 없는거 같아요",
          "original_form": "어쩔 수 없는거 같아요",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027198.1.26",
          "form": "그렇죠 저는 그래서 요새 액션쪽보다 장르물 많이보게 되는 것 같아여",
          "original_form": "그렇죠 저는 그래서 요새 액션쪽보다 장르물 많이보게 되는 것 같아여",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027198.1.27",
          "form": "어떤거 보셨나요",
          "original_form": "어떤거 보셨나요",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027198.1.28",
          "form": "음 최근에는 스릴러같은거 많이 나오더라구요",
          "original_form": "음 최근에는 스릴러같은거 많이 나오더라구요",
          "speaker_id": "1",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027198.1.29",
          "form": "영화는 아니긴한데 스위트홈이랑 킹덤도 재밌어요",
          "original_form": "영화는 아니긴한데 스위트홈이랑 킹덤도 재밌어요",
          "speaker_id": "1",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027198.1.30",
          "form": "엌ㅋㅋ 저도 그 두개는 다 봤어요",
          "original_form": "엌ㅋㅋ 저도 그 두개는 다 봤어요",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027198.1.31",
          "form": "재밌죠?!",
          "original_form": "재밌죠?!",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100027198.1.32",
          "form": "넷플하시나보네요",
          "original_form": "넷플하시나보네요",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100027198.1.33",
          "form": "저도 친구들끼리 4명해서 넷플이랑 왓챠도 같이 공유해서써요",
          "original_form": "저도 친구들끼리 4명해서 넷플이랑 왓챠도 같이 공유해서써요",
          "speaker_id": "1",
          "time": "20210902 18:53"
        }
      ]
