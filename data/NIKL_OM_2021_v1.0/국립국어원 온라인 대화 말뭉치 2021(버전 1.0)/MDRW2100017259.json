      "utterance": [
        {
          "id": "MDRW2100017259.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017259.1.2",
          "form": "안녕하세요 요즘 좋아하는 음식 있으신가요?",
          "original_form": "안녕하세요 요즘 좋아하는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017259.1.3",
          "form": "전 초밥이요ㅋㅋㅋ",
          "original_form": "전 초밥이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017259.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017259.1.5",
          "form": "전 떡볶이랑 샐러드요!",
          "original_form": "전 떡볶이랑 샐러드요!",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017259.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017259.1.7",
          "form": "어떤브랜즈좋아하세요?",
          "original_form": "어떤브랜즈좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017259.1.8",
          "form": "저 로제떡볶이 좋아하는데 왠만한 브랜드는 다 맛있더라고요",
          "original_form": "저 로제떡볶이 좋아하는데 왠만한 브랜드는 다 맛있더라고요",
          "speaker_id": "2",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017259.1.9",
          "form": "특별히 선호하는 곳이 있으세요?",
          "original_form": "특별히 선호하는 곳이 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017259.1.10",
          "form": "ㅋㅋㅋ전 떡군이네요!",
          "original_form": "ㅋㅋㅋ전 떡군이네요!",
          "speaker_id": "1",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017259.1.11",
          "form": "아 주말에 먹어봐야겠어요!",
          "original_form": "아 주말에 먹어봐야겠어요!",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017259.1.12",
          "form": "초밥 추천해주실 수 있으세요?",
          "original_form": "초밥 추천해주실 수 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017259.1.13",
          "form": "초밥은근데",
          "original_form": "초밥은근데",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017259.1.14",
          "form": "체인점이 아니라 동네초밥집들이라서요ㅠ",
          "original_form": "체인점이 아니라 동네초밥집들이라서요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017259.1.15",
          "form": "그나마 체인점은 은행골?",
          "original_form": "그나마 체인점은 은행골?",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017259.1.16",
          "form": "은행골 맛있나요? 전 오마카세 두번 가봤는데 혹시 서울사시면 마포쪽 추천해드릴게요",
          "original_form": "은행골 맛있나요? 전 오마카세 두번 가봤는데 혹시 서울사시면 마포쪽 추천해드릴게요",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017259.1.17",
          "form": "어딘가요?? 이요이요? 스시소라?",
          "original_form": "어딘가요?? 이요이요? 스시소라?",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017259.1.18",
          "form": "스시소라요?",
          "original_form": "스시소라요?",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017259.1.19",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017259.1.20",
          "form": "아시는군요!",
          "original_form": "아시는군요!",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017259.1.21",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017259.1.22",
          "form": "전 광화문점가봤어요",
          "original_form": "전 광화문점가봤어요",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017259.1.23",
          "form": "진짜 배부른데 맛은 있고 계속 먹다 지쳤어요",
          "original_form": "진짜 배부른데 맛은 있고 계속 먹다 지쳤어요",
          "speaker_id": "2",
          "time": "20210826 19:25"
        }
      ]
