      "utterance": [
        {
          "id": "MDRW2100012847.1.1",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100012847.1.2",
          "form": "생수 업체 중의 절반 이상이",
          "original_form": "생수 업체 중의 절반 이상이",
          "speaker_id": "1",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100012847.1.3",
          "form": "수질 부적합으로 걸린 적이 있대요",
          "original_form": "수질 부적합으로 걸린 적이 있대요",
          "speaker_id": "1",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100012847.1.4",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100012847.1.5",
          "form": "찾아봐야겠어요",
          "original_form": "찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100012847.1.6",
          "form": "네 저는 동원샘물 먹었는대",
          "original_form": "네 저는 동원샘물 먹었는대",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.7",
          "form": "그것도 역시나 걸린 적 있더라구요",
          "original_form": "그것도 역시나 걸린 적 있더라구요",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.8",
          "form": "저는 백산수 먹고있는데",
          "original_form": "저는 백산수 먹고있는데",
          "speaker_id": "2",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.9",
          "form": "걱정되네요",
          "original_form": "걱정되네요",
          "speaker_id": "2",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.10",
          "form": "백산수는 일단은 제 기억엔 없는데",
          "original_form": "백산수는 일단은 제 기억엔 없는데",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.11",
          "form": "그게 관리가 전반적으로 잘 안된다더라구요",
          "original_form": "그게 관리가 전반적으로 잘 안된다더라구요",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.12",
          "form": "의외네요",
          "original_form": "의외네요",
          "speaker_id": "2",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.13",
          "form": "네 워낙 싸게 팔아서",
          "original_form": "네 워낙 싸게 팔아서",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.14",
          "form": "관리에 별로 돈을 안 쓴대요",
          "original_form": "관리에 별로 돈을 안 쓴대요",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012847.1.15",
          "form": "저도 이따 좀 찾아봐야겠어요",
          "original_form": "저도 이따 좀 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012847.1.16",
          "form": "네 삼다수만 안 걸렸대요",
          "original_form": "네 삼다수만 안 걸렸대요",
          "speaker_id": "1",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012847.1.17",
          "form": "삼다수 비싼 값을 하나보네요",
          "original_form": "삼다수 비싼 값을 하나보네요",
          "speaker_id": "2",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012847.1.18",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012847.1.19",
          "form": "거긴 자체 관리를 철저히 하나봐요",
          "original_form": "거긴 자체 관리를 철저히 하나봐요",
          "speaker_id": "1",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012847.1.20",
          "form": "삼다수를 먹어야하나 ㅎㅎ",
          "original_form": "삼다수를 먹어야하나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012847.1.21",
          "form": "근데 식수로 쟁여놓고 먹긴 은근 비싸서 ㅜㅜ",
          "original_form": "근데 식수로 쟁여놓고 먹긴 은근 비싸서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100012847.1.22",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100012847.1.23",
          "form": "정수기를 써야되려나요..",
          "original_form": "정수기를 써야되려나요..",
          "speaker_id": "2",
          "time": "20210825 02:45"
        }
      ]
