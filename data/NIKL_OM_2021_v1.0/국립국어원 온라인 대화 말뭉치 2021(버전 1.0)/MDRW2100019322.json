      "utterance": [
        {
          "id": "MDRW2100019322.1.1",
          "form": "너무 피곤해 재택",
          "original_form": "너무 피곤해 재택",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100019322.1.2",
          "form": "재택근무 하면 좋지 않아?",
          "original_form": "재택근무 하면 좋지 않아?",
          "speaker_id": "2",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100019322.1.3",
          "form": "너무나 바뻐",
          "original_form": "너무나 바뻐",
          "speaker_id": "1",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100019322.1.4",
          "form": "집에서 일하면 할게 더 많은거야?",
          "original_form": "집에서 일하면 할게 더 많은거야?",
          "speaker_id": "2",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019322.1.5",
          "form": "출퇴근 시간만큼 더 일해야지",
          "original_form": "출퇴근 시간만큼 더 일해야지",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019322.1.6",
          "form": "난 출퇴근 시간이 없어지니까 더 좋던데",
          "original_form": "난 출퇴근 시간이 없어지니까 더 좋던데",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019322.1.7",
          "form": "그 시간에 잠도 더 잘 수 있고",
          "original_form": "그 시간에 잠도 더 잘 수 있고",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019322.1.8",
          "form": "회의가 너무 많아져",
          "original_form": "회의가 너무 많아져",
          "speaker_id": "1",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019322.1.9",
          "form": "그러면 컨디션도 좋아서 일할 때 더 집중할 수 있더라고",
          "original_form": "그러면 컨디션도 좋아서 일할 때 더 집중할 수 있더라고",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100019322.1.10",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019322.1.11",
          "form": "하긴 그것도 그렇네",
          "original_form": "하긴 그것도 그렇네",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019322.1.12",
          "form": "ㅋㅋ 어찌되었든 근무라는데 힘들지",
          "original_form": "ㅋㅋ 어찌되었든 근무라는데 힘들지",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019322.1.13",
          "form": "그래도 난 집에 있으니까 마음도 더 편하고 좋더라",
          "original_form": "그래도 난 집에 있으니까 마음도 더 편하고 좋더라",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019322.1.14",
          "form": "계속 재택근무 했으면 좋겠어 ㅋㅋ",
          "original_form": "계속 재택근무 했으면 좋겠어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019322.1.15",
          "form": "그렇지 우리가 같이 있을수도있고",
          "original_form": "그렇지 우리가 같이 있을수도있고",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019322.1.16",
          "form": "재택이 좋아",
          "original_form": "재택이 좋아",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019322.1.17",
          "form": "요즘은 화상회의도 잘 되어있어서 일할 때 불편함은 별로 못느끼겠어",
          "original_form": "요즘은 화상회의도 잘 되어있어서 일할 때 불편함은 별로 못느끼겠어",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100019322.1.18",
          "form": "어르신들은 화상을 잘 못쓰기도 해",
          "original_form": "어르신들은 화상을 잘 못쓰기도 해",
          "speaker_id": "1",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100019322.1.19",
          "form": "배워서 해야지 뭐",
          "original_form": "배워서 해야지 뭐",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100019322.1.20",
          "form": "세상이 이렇게 변해가는데",
          "original_form": "세상이 이렇게 변해가는데",
          "speaker_id": "2",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100019322.1.21",
          "form": "맞아 변한 세상에 적웅해야지",
          "original_form": "맞아 변한 세상에 적웅해야지",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100019322.1.22",
          "form": "우리 회사가 재택근무 할거라고 누가 생각이나 했겠어 ㅋㅋ",
          "original_form": "우리 회사가 재택근무 할거라고 누가 생각이나 했겠어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:35"
        }
      ]
    }
