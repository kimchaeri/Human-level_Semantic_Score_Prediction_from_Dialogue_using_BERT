      "utterance": [
        {
          "id": "MDRW2100003313.1.1",
          "form": "name2님 게임알아요?",
          "original_form": "&name2&님 게임알아요?",
          "speaker_id": "1",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100003313.1.2",
          "form": "어떤게임이요?",
          "original_form": "어떤게임이요?",
          "speaker_id": "2",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100003313.1.3",
          "form": "온라인게임이나 오프라인게임이요",
          "original_form": "온라인게임이나 오프라인게임이요",
          "speaker_id": "1",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003313.1.4",
          "form": "전 그냥 퍼즐겜 이런거 간단한거해요",
          "original_form": "전 그냥 퍼즐겜 이런거 간단한거해요",
          "speaker_id": "2",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003313.1.5",
          "form": "좋아하시는 겜 있나요?",
          "original_form": "좋아하시는 겜 있나요?",
          "speaker_id": "2",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003313.1.6",
          "form": "저는 그냥 예전게임이좋아요",
          "original_form": "저는 그냥 예전게임이좋아요",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003313.1.7",
          "form": "크레이지 아케이드나 포트리스요",
          "original_form": "크레이지 아케이드나 포트리스요",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003313.1.8",
          "form": "아 옛날에 포트리스 잠깐해봤는데 그게 아직도있나요?",
          "original_form": "아 옛날에 포트리스 잠깐해봤는데 그게 아직도있나요?",
          "speaker_id": "2",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003313.1.9",
          "form": "아니요 요즘에는 거의 게임을 안하고 보기만해요",
          "original_form": "아니요 요즘에는 거의 게임을 안하고 보기만해요",
          "speaker_id": "1",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003313.1.10",
          "form": "스타정도만해요",
          "original_form": "스타정도만해요",
          "speaker_id": "1",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003313.1.11",
          "form": "요즘 게임 종류도 너무 많아서 ㅎ",
          "original_form": "요즘 게임 종류도 너무 많아서 ㅎ",
          "speaker_id": "2",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003313.1.12",
          "form": "아이템도 돈주고사고 그러나요?",
          "original_form": "아이템도 돈주고사고 그러나요?",
          "speaker_id": "2",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003313.1.13",
          "form": "아니요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003313.1.14",
          "form": "저는 한번밖에 사본적이없어요",
          "original_form": "저는 한번밖에 사본적이없어요",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003313.1.15",
          "form": "그렇군요ㅎ 게임은 어릴때 오락실가서 ㅋ",
          "original_form": "그렇군요ㅎ 게임은 어릴때 오락실가서 ㅋ",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003313.1.16",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003313.1.17",
          "form": "어릴적이재밌었는데",
          "original_form": "어릴적이재밌었는데",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003313.1.18",
          "form": "연령대가 저랑 비슷하신가봐요 공감하시는거보니ㅋ",
          "original_form": "연령대가 저랑 비슷하신가봐요 공감하시는거보니ㅋ",
          "speaker_id": "2",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003313.1.19",
          "form": "ㅋㅋㄱㅋ저는 삼십대초반이에요",
          "original_form": "ㅋㅋㄱㅋ저는 삼십대초반이에요",
          "speaker_id": "1",
          "time": "20210820 21:55"
        },
        {
          "id": "MDRW2100003313.1.20",
          "form": "오프라인과 온라인을다겪었죠",
          "original_form": "오프라인과 온라인을다겪었죠",
          "speaker_id": "1",
          "time": "20210820 21:56"
        }
      ]
