      "utterance": [
        {
          "id": "MDRW2100047475.1.1",
          "form": "저는..",
          "original_form": "저는..",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100047475.1.2",
          "form": "내장을못먹어요",
          "original_form": "내장을못먹어요",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100047475.1.3",
          "form": "아하! 그럼 곱창도 못드시겠군요..!!!",
          "original_form": "아하! 그럼 곱창도 못드시겠군요..!!!",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100047475.1.4",
          "form": "아 그런건먹어요ㅋㅋㅋ",
          "original_form": "아 그런건먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100047475.1.5",
          "form": "알탕은 못먹어요ㅋㅋㅋ",
          "original_form": "알탕은 못먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100047475.1.6",
          "form": "비린 음식이라 더 그런가봐요!ㅋㅋ 오늘 저녁은 뭘 드실건가요?",
          "original_form": "비린 음식이라 더 그런가봐요!ㅋㅋ 오늘 저녁은 뭘 드실건가요?",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100047475.1.7",
          "form": "아직 고민중이랍니다ㅠ",
          "original_form": "아직 고민중이랍니다ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100047475.1.8",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100047475.1.9",
          "form": "전 오늘 치킨이요!!!",
          "original_form": "전 오늘 치킨이요!!!",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100047475.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100047475.1.11",
          "form": "어디껀가요?!!",
          "original_form": "어디껀가요?!!",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100047475.1.12",
          "form": "치킨신드롬이요! 교촌맛나는 치킨입니다!",
          "original_form": "치킨신드롬이요! 교촌맛나는 치킨입니다!",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100047475.1.13",
          "form": "오 할인하나요.?!!",
          "original_form": "오 할인하나요.?!!",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100047475.1.14",
          "form": "음 특별히 할인하는 건 없고 포장할인 받으려구요!!ㅎㅎ",
          "original_form": "음 특별히 할인하는 건 없고 포장할인 받으려구요!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100047475.1.15",
          "form": "2천원 할인해준답니다 ㅋㅋ",
          "original_form": "2천원 할인해준답니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100047475.1.16",
          "form": "오 포장좋네요ㅋㅋㅋ",
          "original_form": "오 포장좋네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100047475.1.17",
          "form": "제꺼도어떻게 같이되나요?(넝담)",
          "original_form": "제꺼도어떻게 같이되나요?(넝담)",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100047475.1.18",
          "form": "ㅋㅋㅋ문열어둘게요 어서오세요!!(누구세요)",
          "original_form": "ㅋㅋㅋ문열어둘게요 어서오세요!!(누구세요)",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100047475.1.19",
          "form": "저왔어요!!!!!!",
          "original_form": "저왔어요!!!!!!",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047475.1.20",
