      "utterance": [
        {
          "id": "MDRW2100011252.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100011252.1.2",
          "form": "비오고 나니까 날씨가 많이",
          "original_form": "비오고 나니까 날씨가 많이",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100011252.1.3",
          "form": "선선해진 거 같아요",
          "original_form": "선선해진 거 같아요",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100011252.1.4",
          "form": "여긴 아직 비가와요 거긴 그쳤나요?",
          "original_form": "여긴 아직 비가와요 거긴 그쳤나요?",
          "speaker_id": "2",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011252.1.5",
          "form": "좀전까지 오다가",
          "original_form": "좀전까지 오다가",
          "speaker_id": "1",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011252.1.6",
          "form": "방금 그쳤어요",
          "original_form": "방금 그쳤어요",
          "speaker_id": "1",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011252.1.7",
          "form": "확실히 비오고나니까 시원해요",
          "original_form": "확실히 비오고나니까 시원해요",
          "speaker_id": "2",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011252.1.8",
          "form": "실내는 습기때문에 좀 더운것같은데 실외는 시원하네요",
          "original_form": "실내는 습기때문에 좀 더운것같은데 실외는 시원하네요",
          "speaker_id": "2",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011252.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100011252.1.10",
          "form": "실외는 시원한데",
          "original_form": "실외는 시원한데",
          "speaker_id": "1",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100011252.1.11",
          "form": "실내는 살짝 ... ㅎㅎ",
          "original_form": "실내는 살짝 ... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100011252.1.12",
          "form": "그쵸 요즘 일교차가 커서 겉옷 챙겨야 겠어요",
          "original_form": "그쵸 요즘 일교차가 커서 겉옷 챙겨야 겠어요",
          "speaker_id": "2",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100011252.1.13",
          "form": "맞아요 감기 조심해야해요 하지만 전 지금 너무 좋아요",
          "original_form": "맞아요 감기 조심해야해요 하지만 전 지금 너무 좋아요",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011252.1.14",
          "form": "더운게 너무 싫어서 ㅠㅠ",
          "original_form": "더운게 너무 싫어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011252.1.15",
          "form": "헉 저두요ㅠㅠ 이번여름 엄청 힘들었죠ㅠㅠ",
          "original_form": "헉 저두요ㅠㅠ 이번여름 엄청 힘들었죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 10:14"
        },
        {
          "id": "MDRW2100011252.1.16",
          "form": "인간적으로 너무 더웠어요",
          "original_form": "인간적으로 너무 더웠어요",
          "speaker_id": "2",
          "time": "20210824 10:14"
        },
        {
          "id": "MDRW2100011252.1.17",
          "form": "네 엄청요 ㅠㅠ",
          "original_form": "네 엄청요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011252.1.18",
          "form": "근데 지금 또 해가 엄청",
          "original_form": "근데 지금 또 해가 엄청",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011252.1.19",
          "form": "쨍쨍해요..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "쨍쨍해요..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011252.1.20",
          "form": "헉....나가지말아야겠다",
          "original_form": "헉....나가지말아야겠다",
          "speaker_id": "2",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100011252.1.21",
          "form": "습도도 높은데 에어컨 틀어야겠네요ㅠ",
          "original_form": "습도도 높은데 에어컨 틀어야겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100011252.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011252.1.23",
          "form": "이런 날에 추운데도",
          "original_form": "이런 날에 추운데도",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011252.1.24",
          "form": "에어컨 필수에요ㅋㅋㅋ",
          "original_form": "에어컨 필수에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011252.1.25",
          "form": "온도는 안높은데 땀나요ㅋㅋㅋ",
          "original_form": "온도는 안높은데 땀나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011252.1.26",
          "form": "제습기 사고싶어지는 날이네요",
          "original_form": "제습기 사고싶어지는 날이네요",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011252.1.27",
          "form": "이런날 제습기 필수입니다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이런날 제습기 필수입니다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100011252.1.28",
          "form": "제습기 사용하고계신가요?",
          "original_form": "제습기 사용하고계신가요?",
          "speaker_id": "2",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100011252.1.29",
          "form": "네ㅋㅋ 근데 제습기틀면",
          "original_form": "네ㅋㅋ 근데 제습기틀면",
          "speaker_id": "1",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100011252.1.30",
          "form": "뜨거운 바람 때뭉에 항상 고민이에요",
          "original_form": "뜨거운 바람 때뭉에 항상 고민이에요",
          "speaker_id": "1",
          "time": "20210824 12:07"
        }
      ]
