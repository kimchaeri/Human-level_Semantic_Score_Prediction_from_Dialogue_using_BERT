      "utterance": [
        {
          "id": "MDRW2100017698.1.1",
          "form": "아침식사는 했어요?",
          "original_form": "아침식사는 했어요?",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100017698.1.2",
          "form": "네 식사 잘 하셨어요?",
          "original_form": "네 식사 잘 하셨어요?",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100017698.1.3",
          "form": "갈비탕 끓여 먹었어요",
          "original_form": "갈비탕 끓여 먹었어요",
          "speaker_id": "1",
          "time": "20210829 15:18"
        },
        {
          "id": "MDRW2100017698.1.4",
          "form": "저도 갈비탕 좋아해요",
          "original_form": "저도 갈비탕 좋아해요",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100017698.1.5",
          "form": "추어탕도",
          "original_form": "추어탕도",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100017698.1.6",
          "form": "맞아요 맛있고 영양도 있고",
          "original_form": "맞아요 맛있고 영양도 있고",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100017698.1.7",
          "form": "속이 든든하죠",
          "original_form": "속이 든든하죠",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100017698.1.8",
          "form": "요즘은 1인분 포장된채로 나오는 제품도 많아 좋아요",
          "original_form": "요즘은 1인분 포장된채로 나오는 제품도 많아 좋아요",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100017698.1.9",
          "form": "그렇군요. 집에서 제대로 맛내려면 손이 워낙 많이 가니",
          "original_form": "그렇군요. 집에서 제대로 맛내려면 손이 워낙 많이 가니",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100017698.1.10",
          "form": "그렇게 사먹는게 훨 좋을수도 있겠네요",
          "original_form": "그렇게 사먹는게 훨 좋을수도 있겠네요",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100017698.1.11",
          "form": "맞아요 원산지랑 위생만 믿을 수 있으면",
          "original_form": "맞아요 원산지랑 위생만 믿을 수 있으면",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100017698.1.12",
          "form": "아님 맛집에서 포장해오거나",
          "original_form": "아님 맛집에서 포장해오거나",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100017698.1.13",
          "form": "그 편이 더 좋더라구요",
          "original_form": "그 편이 더 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100017698.1.14",
          "form": "좋아하는 브랜드(?)있어요?",
          "original_form": "좋아하는 브랜드(?)있어요?",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100017698.1.15",
          "form": "주로 어디서 구매하는 편이에요?",
          "original_form": "주로 어디서 구매하는 편이에요?",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100017698.1.16",
          "form": "저는 마켓컬리 헬로네이처 많이 쓰고",
          "original_form": "저는 마켓컬리 헬로네이처 많이 쓰고",
          "speaker_id": "2",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100017698.1.17",
          "form": "컬리!",
          "original_form": "컬리!",
          "speaker_id": "1",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100017698.1.18",
          "form": "배달어플도 자주 사용해요",
          "original_form": "배달어플도 자주 사용해요",
          "speaker_id": "2",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100017698.1.19",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 18:28"
        }
      ]
