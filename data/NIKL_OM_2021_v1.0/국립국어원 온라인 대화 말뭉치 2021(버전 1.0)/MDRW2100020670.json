      "utterance": [
        {
          "id": "MDRW2100020670.1.1",
          "form": "나쵸 좋아하시면",
          "original_form": "나쵸 좋아하시면",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020670.1.2",
          "form": "이마트에 노브랜드 또띠아칩이라고있는데",
          "original_form": "이마트에 노브랜드 또띠아칩이라고있는데",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020670.1.3",
          "form": "그것도 먹어보세요",
          "original_form": "그것도 먹어보세요",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020670.1.4",
          "form": "아니 노브랜드에 그런게 있군요",
          "original_form": "아니 노브랜드에 그런게 있군요",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.5",
          "form": "들러야겠네요",
          "original_form": "들러야겠네요",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.6",
          "form": "노브랜드 장보러 자주가는데ㅋㅋㅋ",
          "original_form": "노브랜드 장보러 자주가는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.7",
          "form": "겁나맛있어요",
          "original_form": "겁나맛있어요",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.8",
          "form": "ㅋㅋㅋㅋ노브랜드가면 뭐사요",
          "original_form": "ㅋㅋㅋㅋ노브랜드가면 뭐사요",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.9",
          "form": "치파이랑 호두랑 아몬드랑...",
          "original_form": "치파이랑 호두랑 아몬드랑...",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.10",
          "form": "초콜릿이요..",
          "original_form": "초콜릿이요..",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.11",
          "form": "가장 최근에 샀답니다",
          "original_form": "가장 최근에 샀답니다",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020670.1.12",
          "form": "초콜릿 진짲맛있어",
          "original_form": "초콜릿 진짲맛있어",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.13",
          "form": "노브랜드 꼬지도 좋아해요",
          "original_form": "노브랜드 꼬지도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.14",
          "form": "초콜릿 맛있죠 거기",
          "original_form": "초콜릿 맛있죠 거기",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.15",
          "form": "노브랜드 진짜 먹을거천국",
          "original_form": "노브랜드 진짜 먹을거천국",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.16",
          "form": "저 여러개 사서 얼려놔요",
          "original_form": "저 여러개 사서 얼려놔요",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.17",
          "form": "지금은 슬프게도 밀카 초콜릿 넣어뒀는데",
          "original_form": "지금은 슬프게도 밀카 초콜릿 넣어뒀는데",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.18",
          "form": "다크좋아해요 밀크좋아해요",
          "original_form": "다크좋아해요 밀크좋아해요",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.19",
          "form": "노브랜드가 더 맛있는거 같아요",
          "original_form": "노브랜드가 더 맛있는거 같아요",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.20",
          "form": "하 지금은 다크",
          "original_form": "하 지금은 다크",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.21",
          "form": "노브랜드  진짜 초콜릿이",
          "original_form": "노브랜드  진짜 초콜릿이",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.22",
          "form": "기분따라 달라지는 선택이에요",
          "original_form": "기분따라 달라지는 선택이에요",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.23",
          "form": "함량이 높아서그런가",
          "original_form": "함량이 높아서그런가",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020670.1.24",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020670.1.25",
          "form": "겁나 꾸덕한맛",
          "original_form": "겁나 꾸덕한맛",
