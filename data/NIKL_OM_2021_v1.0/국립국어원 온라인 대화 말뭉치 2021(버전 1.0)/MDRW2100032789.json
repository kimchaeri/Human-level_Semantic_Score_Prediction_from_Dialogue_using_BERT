      "utterance": [
        {
          "id": "MDRW2100032789.1.1",
          "form": "금방 끝나지 않을 갓 같아요 ㅠㅠ",
          "original_form": "금방 끝나지 않을 갓 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100032789.1.2",
          "form": "부다페스트를 갔다가",
          "original_form": "부다페스트를 갔다가",
          "speaker_id": "2",
          "time": "20210905 23:38"
        },
        {
          "id": "MDRW2100032789.1.3",
          "form": "체코랑 ㅠ",
          "original_form": "체코랑 ㅠ",
          "speaker_id": "2",
          "time": "20210905 23:38"
        },
        {
          "id": "MDRW2100032789.1.4",
          "form": "오스트리아",
          "original_form": "오스트리아",
          "speaker_id": "2",
          "time": "20210905 23:38"
        },
        {
          "id": "MDRW2100032789.1.5",
          "form": "아하 ㅠㅠ 다 너무 좋았을 것 같아요...",
          "original_form": "아하 ㅠㅠ 다 너무 좋았을 것 같아요...",
          "speaker_id": "1",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032789.1.6",
          "form": "스위스 독일까지",
          "original_form": "스위스 독일까지",
          "speaker_id": "2",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032789.1.7",
          "form": "쫙 찍고 올 생각 만땅이어ㅛ는데 ㅠㅠㅜ",
          "original_form": "쫙 찍고 올 생각 만땅이어ㅛ는데 ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032789.1.8",
          "form": "유럽 가보셨나요??",
          "original_form": "유럽 가보셨나요??",
          "speaker_id": "2",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032789.1.9",
          "form": "와 스위스 넘 가보고 싶은 곳이예요!...",
          "original_form": "와 스위스 넘 가보고 싶은 곳이예요!...",
          "speaker_id": "1",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032789.1.10",
          "form": "네 저는 독일에서 교환학생 했었어요!",
          "original_form": "네 저는 독일에서 교환학생 했었어요!",
          "speaker_id": "1",
          "time": "20210905 23:40"
        },
        {
          "id": "MDRW2100032789.1.11",
          "form": "세상에... 독일어도 당연히 하실줄 아시겠죠?",
          "original_form": "세상에... 독일어도 당연히 하실줄 아시겠죠?",
          "speaker_id": "2",
          "time": "20210905 23:40"
        },
        {
          "id": "MDRW2100032789.1.12",
          "form": "너무 부러워요",
          "original_form": "너무 부러워요",
          "speaker_id": "2",
          "time": "20210905 23:40"
        },
        {
          "id": "MDRW2100032789.1.13",
          "form": "헉 아니예요 ㅋㅋㅋ",
          "original_form": "헉 아니예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:40"
        },
        {
          "id": "MDRW2100032789.1.14",
          "form": "구텐탁이 뭔지도 모르고 갔어요...",
          "original_form": "구텐탁이 뭔지도 모르고 갔어요...",
          "speaker_id": "1",
          "time": "20210905 23:40"
        },
        {
          "id": "MDRW2100032789.1.15",
          "form": "6개월동안 조금 배웟는데 지금은 다 까먹었어요 ㅋ큐ㅠ",
          "original_form": "6개월동안 조금 배웟는데 지금은 다 까먹었어요 ㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:41"
        },
        {
          "id": "MDRW2100032789.1.16",
          "form": "어떻게 독일로 가시게 됐어요??",
          "original_form": "어떻게 독일로 가시게 됐어요??",
          "speaker_id": "2",
          "time": "20210905 23:41"
        },
        {
          "id": "MDRW2100032789.1.17",
          "form": "사실 저는 폴란드랑 리투아니아? 이런 대학 지원 썼었는데",
          "original_form": "사실 저는 폴란드랑 리투아니아? 이런 대학 지원 썼었는데",
          "speaker_id": "1",
          "time": "20210905 23:41"
        },
        {
          "id": "MDRW2100032789.1.18",
          "form": "갑자기 독일로 뺑뺑이? 당했어요 ㅋㅋ",
          "original_form": "갑자기 독일로 뺑뺑이? 당했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:41"
        },
        {
          "id": "MDRW2100032789.1.19",
          "form": "일단.. 선생님 언어 능력자시군요",
          "original_form": "일단.. 선생님 언어 능력자시군요",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032789.1.20",
          "form": "독일은 어땠나요?",
          "original_form": "독일은 어땠나요?",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032789.1.21",
          "form": "자랑해주세요 ㅎㅎ",
          "original_form": "자랑해주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032789.1.22",
          "form": "음 아니예요 ㅋㅋ어차피 가면 다 영어예요!",
          "original_form": "음 아니예요 ㅋㅋ어차피 가면 다 영어예요!",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032789.1.23",
          "form": "너무 오래된 것 같긴한데요 ㅋㅋㅋ 독일 음...",
          "original_form": "너무 오래된 것 같긴한데요 ㅋㅋㅋ 독일 음...",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032789.1.24",
          "form": "맥주가 싸고 맛잇다? ㅎㅎ",
          "original_form": "맥주가 싸고 맛잇다? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032789.1.25",
          "form": "아핳핳 ㅋㅋㅋㅋ",
          "original_form": "아핳핳 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032789.1.26",
          "form": "맥주 유명하죠",
          "original_form": "맥주 유명하죠",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032789.1.27",
          "form": "역시 가장 임팩트는 맥주가 있었군요",
          "original_form": "역시 가장 임팩트는 맥주가 있었군요",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032789.1.28",
          "form": "관광도 하셨다면 기억에 남는 장소가 있으세요?",
          "original_form": "관광도 하셨다면 기억에 남는 장소가 있으세요?",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032789.1.29",
          "form": "음...저는 겨울학기여서 크리스마스마켓을 여러곳 갔었는데",
          "original_form": "음...저는 겨울학기여서 크리스마스마켓을 여러곳 갔었는데",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032789.1.30",
          "form": "뉘른베르크 크리스마스마켓도 예뻤고",
          "original_form": "뉘른베르크 크리스마스마켓도 예뻤고",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032789.1.31",
          "form": "비엔나 크리스마스마켓도 너무 예뻤어요!",
          "original_form": "비엔나 크리스마스마켓도 너무 예뻤어요!",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032789.1.32",
          "form": "너무 추웠을 뿐 ㅠ..",
          "original_form": "너무 추웠을 뿐 ㅠ..",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032789.1.33",
          "form": "크리스마스 마켓은 어떤 분위기에요??",
          "original_form": "크리스마스 마켓은 어떤 분위기에요??",
          "speaker_id": "2",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032789.1.34",
          "form": "독일의 겨울도 한국 만큼 춥나요??",
          "original_form": "독일의 겨울도 한국 만큼 춥나요??",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032789.1.35",
          "form": "크리스마스마켓은 생소한 거라서 그런지 구경구리도 많고",
          "original_form": "크리스마스마켓은 생소한 거라서 그런지 구경구리도 많고",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032789.1.36",
          "form": "나름 먹을것도 많구 그랬어요 ㅋㅋ",
          "original_form": "나름 먹을것도 많구 그랬어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032789.1.37",
          "form": "독일은 그렇게 안추웠는데...오스트리아는 정말 추웠어요!",
          "original_form": "독일은 그렇게 안추웠는데...오스트리아는 정말 추웠어요!",
          "speaker_id": "1",
          "time": "20210906 00:00"
        }
      ]
    }
