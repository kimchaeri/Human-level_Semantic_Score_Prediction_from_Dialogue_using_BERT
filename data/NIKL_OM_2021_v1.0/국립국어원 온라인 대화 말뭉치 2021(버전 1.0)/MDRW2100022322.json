        },
        {
          "id": "MDRW2100022322.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ굽네 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ굽네 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.3",
          "form": "계란값 파동 나고부터",
          "original_form": "계란값 파동 나고부터",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.4",
          "form": "계란 왜 안줘요 갑자기?",
          "original_form": "계란 왜 안줘요 갑자기?",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.5",
          "form": "안줬던가같던데",
          "original_form": "안줬던가같던데",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.6",
          "form": "파동 끝나고나서도 안줬잖아요....",
          "original_form": "파동 끝나고나서도 안줬잖아요....",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.7",
          "form": "그 이후로 안주기로 했나봐오",
          "original_form": "그 이후로 안주기로 했나봐오",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.8",
          "form": "ㅠㅠ진ㅉ ㅏ너무해요 굽ㄴ에",
          "original_form": "ㅠㅠ진ㅉ ㅏ너무해요 굽ㄴ에",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.9",
          "form": "서비스가 더 좋아지지도 않고 퇴행하다니",
          "original_form": "서비스가 더 좋아지지도 않고 퇴행하다니",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022322.1.10",
          "form": "약간 굽네는 계란이랑같이 오는게 매력이긴했는데",
          "original_form": "약간 굽네는 계란이랑같이 오는게 매력이긴했는데",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.11",
          "form": "그니까요 근데 계란갖고 또 싸웠어요",
          "original_form": "그니까요 근데 계란갖고 또 싸웠어요",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.12",
          "form": "너가 먹느니 내가 먹느니",
          "original_form": "너가 먹느니 내가 먹느니",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.13",
          "form": "그거 생각하니까 없어진게 잘한 거 같기도 하다",
          "original_form": "그거 생각하니까 없어진게 잘한 거 같기도 하다",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.14",
          "form": "ㅋㅋㅋㅋㄱ 저희집은 계란오면 그건 강아지꺼",
          "original_form": "ㅋㅋㅋㅋㄱ 저희집은 계란오면 그건 강아지꺼",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.15",
          "form": "ㅋㅋㅋㅋ아 진짜 귀여우시네요",
          "original_form": "ㅋㅋㅋㅋ아 진짜 귀여우시네요",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.16",
          "form": "우리 누나는 퍽퍽살 고르면 그거 강아지 줘요^^",
          "original_form": "우리 누나는 퍽퍽살 고르면 그거 강아지 줘요^^",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.17",
          "form": "그래서 뼈닭 시킬 때마다 핵화가나요",
          "original_form": "그래서 뼈닭 시킬 때마다 핵화가나요",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 웃기다",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022322.1.19",
          "form": "음식 가지고 자주 싸우시나봐요",
