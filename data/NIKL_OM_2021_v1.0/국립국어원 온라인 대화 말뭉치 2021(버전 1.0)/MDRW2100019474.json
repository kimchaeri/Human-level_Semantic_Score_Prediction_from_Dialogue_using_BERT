          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019474.1.5",
          "form": "나도 밥해주는사람",
          "original_form": "나도 밥해주는사람",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019474.1.6",
          "form": "잇으면 좋겠다",
          "original_form": "잇으면 좋겠다",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019474.1.7",
          "form": "솔직히 저렇게 밥 다차려주면",
          "original_form": "솔직히 저렇게 밥 다차려주면",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019474.1.8",
          "form": "남주아니여도 같이 살 가치가 잇다",
          "original_form": "남주아니여도 같이 살 가치가 잇다",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019474.1.9",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019474.1.10",
          "form": "젤 부러움..",
          "original_form": "젤 부러움..",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019474.1.11",
          "form": "설마 뽄새안나게",
          "original_form": "설마 뽄새안나게",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019474.1.12",
          "form": "나도 집 싹 치워주고 밥해주는 사람 잇엇음 좋겠니",
          "original_form": "나도 집 싹 치워주고 밥해주는 사람 잇엇음 좋겠니",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019474.1.13",
          "form": "자기가 생선이며 꼬막이며 담아놓고",
          "original_form": "자기가 생선이며 꼬막이며 담아놓고",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019474.1.14",
          "form": "여주카드로 계산하진",
          "original_form": "여주카드로 계산하진",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019474.1.15",
          "form": "않을거아냐",
          "original_form": "않을거아냐",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019474.1.16",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
