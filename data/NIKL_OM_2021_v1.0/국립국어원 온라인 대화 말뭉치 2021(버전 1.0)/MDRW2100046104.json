      "utterance": [
        {
          "id": "MDRW2100046104.1.1",
          "form": "아메리카노는 못마셔요",
          "original_form": "아메리카노는 못마셔요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046104.1.2",
          "form": "전 요즘 크림라떼에 빠져서",
          "original_form": "전 요즘 크림라떼에 빠져서",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.3",
          "form": "요즘 아아를 마니 먹드라구요",
          "original_form": "요즘 아아를 마니 먹드라구요",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.4",
          "form": "오 크림 라떼는 어떤건가요",
          "original_form": "오 크림 라떼는 어떤건가요",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.5",
          "form": "매일 크림라떼 마시고 있어요",
          "original_form": "매일 크림라떼 마시고 있어요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.7",
          "form": "위에 생크림 올려진거요 ㅎㅎ",
          "original_form": "위에 생크림 올려진거요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.8",
          "form": "우아 생크림 !!",
          "original_form": "우아 생크림 !!",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.9",
          "form": "저도 달달한 것만 마셔서",
          "original_form": "저도 달달한 것만 마셔서",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.10",
          "form": "느끼하지 않나용?",
          "original_form": "느끼하지 않나용?",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.11",
          "form": "바닐라 라떼 만 마셨는데",
          "original_form": "바닐라 라떼 만 마셨는데",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.12",
          "form": "그림 라떼 너무 맛있더라구요",
          "original_form": "그림 라떼 너무 맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046104.1.13",
          "form": "전혀 느끼하지 않아요",
          "original_form": "전혀 느끼하지 않아요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046104.1.14",
          "form": "오오 !! 다른 음료는 어떤거 드세요",
          "original_form": "오오 !! 다른 음료는 어떤거 드세요",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046104.1.15",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046104.1.16",
          "form": "커피 말고 다른 음료는 잘 안 마셔요",
          "original_form": "커피 말고 다른 음료는 잘 안 마셔요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046104.1.17",
          "form": "저는 녹차라떼 !",
          "original_form": "저는 녹차라떼 !",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046104.1.18",
          "form": "아하 녹차라떼",
          "original_form": "아하 녹차라떼",
          "speaker_id": "2",
          "time": "20210908 10:24"
        }
      ]
