      "utterance": [
        {
          "id": "MDRW2100006227.1.1",
          "form": "메이저에서도 투타 다 이렇게 잘 할 줄이야ㅋㅋ",
          "original_form": "메이저에서도 투타 다 이렇게 잘 할 줄이야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006227.1.2",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100006227.1.3",
          "form": "메이저리그 전체 홈런 1위",
          "original_form": "메이저리그 전체 홈런 1위",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100006227.1.4",
          "form": "완전 만화야구ㅋㅋ",
          "original_form": "완전 만화야구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100006227.1.5",
          "form": "만화같죠",
          "original_form": "만화같죠",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100006227.1.6",
          "form": "이렇게 잘할줄이야",
          "original_form": "이렇게 잘할줄이야",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006227.1.7",
          "form": "메이저리그도 오타니나 페타주 같은 스타 나오니까 현지에서도 반응 올라오나보더라구요",
          "original_form": "메이저리그도 오타니나 페타주 같은 스타 나오니까 현지에서도 반응 올라오나보더라구요",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006227.1.8",
          "form": "젊은 애들은 잘 안 본다고 그랬었는데",
          "original_form": "젊은 애들은 잘 안 본다고 그랬었는데",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100006227.1.9",
          "form": "페타주는 어깨가 자꾸 빠져가지고",
          "original_form": "페타주는 어깨가 자꾸 빠져가지고",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100006227.1.10",
          "form": "개인적으로 최애팀이 다저스라",
          "original_form": "개인적으로 최애팀이 다저스라",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100006227.1.11",
          "form": "요즘 샌프랑 1위싸움 보는맛으로",
          "original_form": "요즘 샌프랑 1위싸움 보는맛으로",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100006227.1.12",
          "form": "자주봅니다 ㅋㅋ",
          "original_form": "자주봅니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100006227.1.13",
          "form": "다저스가 작년 월시 우승했죠?",
          "original_form": "다저스가 작년 월시 우승했죠?",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006227.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006227.1.15",
          "form": "월시 우승요",
          "original_form": "월시 우승요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006227.1.16",
          "form": "올해는 샌프가 엄청 분발하네요",
          "original_form": "올해는 샌프가 엄청 분발하네요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006227.1.17",
          "form": "샌디는 돈 엄청쓰고도 탈락위긴데",
          "original_form": "샌디는 돈 엄청쓰고도 탈락위긴데",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006227.1.18",
          "form": "ㅋㅋㅋ 초반에 난리였는데...",
          "original_form": "ㅋㅋㅋ 초반에 난리였는데...",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006227.1.19",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006227.1.20",
          "form": "다저스 잡겠다고",
          "original_form": "다저스 잡겠다고",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006227.1.21",
          "form": "돈 엄청썼는데",
          "original_form": "돈 엄청썼는데",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006227.1.22",
          "form": "샌프가 이리 잘할줄이야",
          "original_form": "샌프가 이리 잘할줄이야",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006227.1.23",
          "form": "샌프는 그래도 계속 잘 하는 편이었죠?",
          "original_form": "샌프는 그래도 계속 잘 하는 편이었죠?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006227.1.24",
          "form": "잘하다가",
          "original_form": "잘하다가",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006227.1.25",
          "form": "다저스에 밀려서",
          "original_form": "다저스에 밀려서",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006227.1.26",
          "form": "지구우승을 못했죠",
          "original_form": "지구우승을 못했죠",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006227.1.27",
          "form": "메이저리그는 관중 북적북적하니 딴 세상 같더라구요",
          "original_form": "메이저리그는 관중 북적북적하니 딴 세상 같더라구요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006227.1.28",
          "form": "근데 확실히 관중이 있어야 중계로도 재밌어요ㅋㅋ",
          "original_form": "근데 확실히 관중이 있어야 중계로도 재밌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006227.1.29",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006227.1.30",
          "form": "거긴 그냥 다들 마스크없이 먹고 마시고",
          "original_form": "거긴 그냥 다들 마스크없이 먹고 마시고",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006227.1.31",
          "form": "북적북적한데 한번 갔다오면",
          "original_form": "북적북적한데 한번 갔다오면",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006227.1.32",
          "form": "바로 감염될거 같은",
          "original_form": "바로 감염될거 같은",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006227.1.33",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006227.1.34",
          "form": "ㅋㅋㅋㅋ 아직은 좀 무섭죠",
          "original_form": "ㅋㅋㅋㅋ 아직은 좀 무섭죠",
          "speaker_id": "2",
          "time": "20210907 22:09"
        }
