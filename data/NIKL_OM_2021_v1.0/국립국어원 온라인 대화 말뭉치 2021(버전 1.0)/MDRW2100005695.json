      "utterance": [
        {
          "id": "MDRW2100005695.1.1",
          "form": "몇 명의 가족과 살고 있나요?",
          "original_form": "몇 명의 가족과 살고 있나요?",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005695.1.2",
          "form": "혼자 살아요!",
          "original_form": "혼자 살아요!",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005695.1.3",
          "form": "보요는요?",
          "original_form": "보요는요?",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005695.1.4",
          "form": "저는 가족들하고 같이 살아요",
          "original_form": "저는 가족들하고 같이 살아요",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005695.1.5",
          "form": "저 포함 4명",
          "original_form": "저 포함 4명",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005695.1.6",
          "form": "아 북적북적 좋겠어요!",
          "original_form": "아 북적북적 좋겠어요!",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005695.1.7",
          "form": "근데 단점도 많은거 같아요",
          "original_form": "근데 단점도 많은거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005695.1.8",
          "form": "혼자 있고 싶어요ㅎㅎ",
          "original_form": "혼자 있고 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005695.1.9",
          "form": "맞아요 그럴 때도 있지요",
          "original_form": "맞아요 그럴 때도 있지요",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005695.1.10",
          "form": "저도 독립한지 얼마 안 됐어요!",
          "original_form": "저도 독립한지 얼마 안 됐어요!",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005695.1.11",
          "form": "아직은 좋은 점이 더 많아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아직은 좋은 점이 더 많아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005695.1.12",
          "form": "부러워요 ㅎㅎ",
          "original_form": "부러워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005695.1.13",
          "form": "가족들이랑 있는거 재미있긴하지만 혼자이고 싶을때가 요즘은 더 많은거 같아요",
          "original_form": "가족들이랑 있는거 재미있긴하지만 혼자이고 싶을때가 요즘은 더 많은거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005695.1.14",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005695.1.15",
          "form": "떨어지니까 오히려 더 애틋한 것도 있어요",
          "original_form": "떨어지니까 오히려 더 애틋한 것도 있어요",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005695.1.16",
          "form": "다들 그러더라고요 독립하고 나면 사이가 더 좋아진다고",
          "original_form": "다들 그러더라고요 독립하고 나면 사이가 더 좋아진다고",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100005695.1.17",
          "form": "좀 더 일찍 했어야 했는데",
          "original_form": "좀 더 일찍 했어야 했는데",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100005695.1.18",
          "form": "사실 좀 늦었어요!",
          "original_form": "사실 좀 늦었어요!",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100005695.1.19",
          "form": "저도 올해 안에는 독립하고 싶은데 부모님이 아직도 말리셔서 고민이에요",
          "original_form": "저도 올해 안에는 독립하고 싶은데 부모님이 아직도 말리셔서 고민이에요",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005695.1.20",
          "form": "혼자 사는 거 걱정하시는구나!",
          "original_form": "혼자 사는 거 걱정하시는구나!",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100005695.1.21",
          "form": "저도 나가기 직전까지 그러셨어요",
          "original_form": "저도 나가기 직전까지 그러셨어요",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100005695.1.22",
          "form": "맞아요 아직도 애 같이 느껴지나봐요",
          "original_form": "맞아요 아직도 애 같이 느껴지나봐요",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100005695.1.23",
          "form": "지도 덩달아 걱정했는데",
          "original_form": "지도 덩달아 걱정했는데",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100005695.1.24",
          "form": "또 살아지더라고요",
          "original_form": "또 살아지더라고요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100005695.1.25",
          "form": "혼자서 뚝딱뚝딱",
          "original_form": "혼자서 뚝딱뚝딱",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100005695.1.26",
          "form": "저도 가끔 머릿속으로 상상 해봐요 ㅎㅎ",
          "original_form": "저도 가끔 머릿속으로 상상 해봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100005695.1.27",
          "form": "이렇게 저렇게 살아봐야지",
          "original_form": "이렇게 저렇게 살아봐야지",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100005695.1.28",
          "form": "스스로 준비되면 과감하게 지르세요!",
          "original_form": "스스로 준비되면 과감하게 지르세요!",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100005695.1.29",
          "form": "뭐 별로면 다시 들어가면 되니까!",
          "original_form": "뭐 별로면 다시 들어가면 되니까!",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100005695.1.30",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100005695.1.31",
          "form": "올해는 꼭!!!",
          "original_form": "올해는 꼭!!!",
          "speaker_id": "1",
          "time": "20210822 00:24"
        }
      ]
    }
