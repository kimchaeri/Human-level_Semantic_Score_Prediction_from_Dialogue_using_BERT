      "utterance": [
        {
          "id": "MDRW2100024990.1.1",
          "form": "방심하면 안되겠어요",
          "original_form": "방심하면 안되겠어요",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024990.1.2",
          "form": "음식 잘 만들어드세요?",
          "original_form": "음식 잘 만들어드세요?",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024990.1.3",
          "form": "전 노동이 싫어서 살기 위해 만드는 끼니 요리는 만들긴하지만 즐겁진 않아요",
          "original_form": "전 노동이 싫어서 살기 위해 만드는 끼니 요리는 만들긴하지만 즐겁진 않아요",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024990.1.4",
          "form": "가끔할때는 즐겁지만요",
          "original_form": "가끔할때는 즐겁지만요",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024990.1.5",
          "form": "아 맞아요 공감삽니다",
          "original_form": "아 맞아요 공감삽니다",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024990.1.6",
          "form": "가끔 파스타 만들어먹는데 귀찮다가도 맛있어요",
          "original_form": "가끔 파스타 만들어먹는데 귀찮다가도 맛있어요",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024990.1.7",
          "form": "사먹는 것보다 맛있을땐 좀 뿌듯해요 ㅋㅋ",
          "original_form": "사먹는 것보다 맛있을땐 좀 뿌듯해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024990.1.8",
          "form": "내게 더 맛있는데 ? 으쓱으쓱",
          "original_form": "내게 더 맛있는데 ? 으쓱으쓱",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024990.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100024990.1.10",
          "form": "그리고 재료를 아끼지않는다는 점",
          "original_form": "그리고 재료를 아끼지않는다는 점",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100024990.1.11",
          "form": "면보다 재료가 더 많은ㅋㅋ",
          "original_form": "면보다 재료가 더 많은ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100024990.1.12",
          "form": "집에서 만들면 새우반 면반 파스타도 가능하죸ㅋㅋ",
          "original_form": "집에서 만들면 새우반 면반 파스타도 가능하죸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100024990.1.13",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100024990.1.14",
          "form": "오늘 점심에 맛있는거 나오면 좋겠네요",
          "original_form": "오늘 점심에 맛있는거 나오면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100024990.1.15",
          "form": "기대중",
          "original_form": "기대중",
          "speaker_id": "2",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100024990.1.16",
          "form": "저는 구내식당이없어서 ㅎㄹㄹ",
          "original_form": "저는 구내식당이없어서 ㅎㄹㄹ",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100024990.1.17",
          "form": "구내식당에서 드시나봐요 부럽다",
          "original_form": "구내식당에서 드시나봐요 부럽다",
          "speaker_id": "1",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100024990.1.18",
          "form": "막 대기업 이런거 아니고 진짜 작은 식당이에요 ㅎ",
          "original_form": "막 대기업 이런거 아니고 진짜 작은 식당이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100024990.1.19",
          "form": "작을수록 아예 운영안해주는데도 많은데",
          "original_form": "작을수록 아예 운영안해주는데도 많은데",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100024990.1.20",
          "form": "운영해줌 좋죠  누군가 돈만 삥땅을 안치면 재료와 품질이 보장되는 것 같아요",
          "original_form": "운영해줌 좋죠  누군가 돈만 삥땅을 안치면 재료와 품질이 보장되는 것 같아요",
          "speaker_id": "1",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100024990.1.21",
          "form": "여기는 삥땅은 못쳐서 좋아요 ㅎ",
          "original_form": "여기는 삥땅은 못쳐서 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100024990.1.22",
          "form": "엄청 좋지도 나쁘지도 않은",
          "original_form": "엄청 좋지도 나쁘지도 않은",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100024990.1.23",
          "form": "그래도 괜찮아요 먹을만하고",
          "original_form": "그래도 괜찮아요 먹을만하고",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100024990.1.24",
          "form": "오늘은 샤브국? 그런거 나왔어요",
          "original_form": "오늘은 샤브국? 그런거 나왔어요",
          "speaker_id": "2",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100024990.1.25",
          "form": "적당히 나오고 저렴하고 남이 꾸준히 해주는 밥은 모든 가사전담자의 꿈의 식단아닌가요",
          "original_form": "적당히 나오고 저렴하고 남이 꾸준히 해주는 밥은 모든 가사전담자의 꿈의 식단아닌가요",
          "speaker_id": "1",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100024990.1.26",
          "form": "그래서 제가 어릴때 저희 어머니가 학교 급식으 그렇게 좋아했어요 ㅋㅋ",
          "original_form": "그래서 제가 어릴때 저희 어머니가 학교 급식으 그렇게 좋아했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100024990.1.27",
          "form": "맞아요맞아요. 그렇죠",
          "original_form": "맞아요맞아요. 그렇죠",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100024990.1.28",
          "form": "배부른 소리였어요ㅋㅋㄱㅋ",
          "original_form": "배부른 소리였어요ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100024990.1.29",
          "form": "아ㅋㅋ학교급식 ㅋㅋ 도시락 안싸도 되서요?",
          "original_form": "아ㅋㅋ학교급식 ㅋㅋ 도시락 안싸도 되서요?",
          "speaker_id": "2",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100024990.1.30",
          "form": "네 너무 좋아하셨어요 ㅋ",
          "original_form": "네 너무 좋아하셨어요 ㅋ",
