      "utterance": [
        {
          "id": "MDRW2100034450.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100034450.1.2",
          "form": "안녕하세요 아침 드셨나요",
          "original_form": "안녕하세요 아침 드셨나요",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100034450.1.3",
          "form": "네~ 아침으로베이글먹얶어요~",
          "original_form": "네~ 아침으로베이글먹얶어요~",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100034450.1.4",
          "form": "우와저도 샌드위치 찾아보가가 접었어요",
          "original_form": "우와저도 샌드위치 찾아보가가 접었어요",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100034450.1.5",
          "form": "앜ㅋㅋㅋㅋㅋ 샌드위치 너무좋아해요..",
          "original_form": "앜ㅋㅋㅋㅋㅋ 샌드위치 너무좋아해요..",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034450.1.6",
          "form": "가볍게먹기엔 서브웨이가좋죠",
          "original_form": "가볍게먹기엔 서브웨이가좋죠",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034450.1.7",
          "form": "그러다보쌈먹었어요 뜬금",
          "original_form": "그러다보쌈먹었어요 뜬금",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034450.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 보쌈 저도 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 보쌈 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034450.1.9",
          "form": "보쌈에 무말랭이 같이먹는거 좋아해요전",
          "original_form": "보쌈에 무말랭이 같이먹는거 좋아해요전",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034450.1.10",
          "form": "오늘그렇게먹고 이젠 피자시켜요",
          "original_form": "오늘그렇게먹고 이젠 피자시켜요",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034450.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ 피자 뭐드셧어요?",
          "original_form": "앜ㅋㅋㅋㅋㅋ 피자 뭐드셧어요?",
          "speaker_id": "1",
          "time": "20210829 06:43"
        },
        {
          "id": "MDRW2100034450.1.12",
          "form": "청년피자먹었어욥",
          "original_form": "청년피자먹었어욥",
          "speaker_id": "2",
          "time": "20210829 11:03"
        },
        {
          "id": "MDRW2100034450.1.13",
          "form": "아하! 청년피자 뭐가맛나나욤??",
          "original_form": "아하! 청년피자 뭐가맛나나욤??",
          "speaker_id": "1",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100034450.1.14",
          "form": "에그콘이랑 새우용",
          "original_form": "에그콘이랑 새우용",
          "speaker_id": "2",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100034450.1.15",
          "form": "아하! 안먹어봣는데 메모해둬야겠어요~",
          "original_form": "아하! 안먹어봣는데 메모해둬야겠어요~",
