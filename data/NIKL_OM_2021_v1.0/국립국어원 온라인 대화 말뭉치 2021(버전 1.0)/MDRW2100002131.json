      "utterance": [
        {
          "id": "MDRW2100002131.1.1",
          "form": "요새 요요와서 넘 쪘어요",
          "original_form": "요새 요요와서 넘 쪘어요",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002131.1.2",
          "form": "코로나가 비만을 부르네요 ㅡㅡ사실 핑계죠",
          "original_form": "코로나가 비만을 부르네요 ㅡㅡ사실 핑계죠",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002131.1.3",
          "form": "20킬로뺐다 다시 쪘네요",
          "original_form": "20킬로뺐다 다시 쪘네요",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002131.1.4",
          "form": "헉 저도 살빼고 요요 왓어요 ㅎㅎㅎ",
          "original_form": "헉 저도 살빼고 요요 왓어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002131.1.5",
          "form": "그게 수순 같아요",
          "original_form": "그게 수순 같아요",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002131.1.6",
          "form": "ㅋㅋㅋname1님이랑 비슷한게 많은거같아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ&name1&님이랑 비슷한게 많은거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002131.1.7",
          "form": "네 그렇네요 ㅋㅋ",
          "original_form": "네 그렇네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002131.1.8",
          "form": "저 맨날 낼부터 다욧해요",
          "original_form": "저 맨날 낼부터 다욧해요",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002131.1.9",
          "form": "오늘없이 삽니다",
          "original_form": "오늘없이 삽니다",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002131.1.10",
          "form": "ㅠㅠ 엄마가 살빼라고 하더라구요",
          "original_form": "ㅠㅠ 엄마가 살빼라고 하더라구요",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002131.1.11",
          "form": "저도요 요새 피하고 있어요",
          "original_form": "저도요 요새 피하고 있어요",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002131.1.12",
          "form": "빼도 폭식으로 다시쪄요",
          "original_form": "빼도 폭식으로 다시쪄요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002131.1.13",
          "form": "진정한 다이어트 약 나오면 진짜 세상 젤부자 될텐데",
          "original_form": "진정한 다이어트 약 나오면 진짜 세상 젤부자 될텐데",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002131.1.14",
          "form": "돈 다 털어 살거예요 ㅡㅡ",
          "original_form": "돈 다 털어 살거예요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002131.1.15",
          "form": "다욧하면 변비도 문제예요 많이 먹던 사람이 확줄이니 여러모로 힘들어요",
          "original_form": "다욧하면 변비도 문제예요 많이 먹던 사람이 확줄이니 여러모로 힘들어요",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002131.1.16",
          "form": "ㅋ.ㅋ 양양 한번 먹어봤는데\r\n살은 빠지는데  와 무기력증 대박이더라구요",
          "original_form": "ㅋ.ㅋ 양양 한번 먹어봤는데\r\n살은 빠지는데  와 무기력증 대박이더라구요",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002131.1.17",
          "form": "양약 오타요 ㅋ",
          "original_form": "양약 오타요 ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002131.1.18",
          "form": "그게 우울증 동반되서 위험하대요",
          "original_form": "그게 우울증 동반되서 위험하대요",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002131.1.19",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002131.1.20",
          "form": "전 그래서 패쓰했어요 ㅜㅜ",
          "original_form": "전 그래서 패쓰했어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002131.1.21",
          "form": "먹지마셔요",
          "original_form": "먹지마셔요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002131.1.22",
          "form": "저도 한번먹고 안먹어요 ㅎㅎ",
          "original_form": "저도 한번먹고 안먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002131.1.23",
          "form": "그럴려고요 사실 굶고 운동해야죠",
          "original_form": "그럴려고요 사실 굶고 운동해야죠",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002131.1.24",
          "form": "전 시도도 못했어요 약은 겁나서",
          "original_form": "전 시도도 못했어요 약은 겁나서",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002131.1.25",
          "form": "ㅎㅎㅎ 안하시는게.좋은거엥ㅅ",
          "original_form": "ㅎㅎㅎ 안하시는게.좋은거엥ㅅ",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002131.1.26",
          "form": "평생 다욧 안하는 사람이 젤 부러워요",
          "original_form": "평생 다욧 안하는 사람이 젤 부러워요",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002131.1.27",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002131.1.28",
          "form": "네 ㅜㅜ 저희오빠요",
          "original_form": "네 ㅜㅜ 저희오빠요",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002131.1.29",
          "form": "뱃살을 지방흡입은 해보고 싶네요 ㅋㅋ",
          "original_form": "뱃살을 지방흡입은 해보고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002131.1.30",
          "form": "중3부터 허리가 29입니다",
          "original_form": "중3부터 허리가 29입니다",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002131.1.31",
          "form": "네 저희 아들요",
          "original_form": "네 저희 아들요",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002131.1.32",
          "form": "저희 아들 40킬로대 입니다 고3",
          "original_form": "저희 아들 40킬로대 입니다 고3",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002131.1.33",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002131.1.34",
          "form": "군대면제 몸무게에 가까워요 ㅜㅜ",
          "original_form": "군대면제 몸무게에 가까워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002131.1.35",
          "form": "참세상은 중간이 없어요",
          "original_form": "참세상은 중간이 없어요",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002131.1.36",
          "form": "40대면 걱정되겠어요",
          "original_form": "40대면 걱정되겠어요",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002131.1.37",
          "form": "네 절대 안쪄요",
          "original_form": "네 절대 안쪄요",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002131.1.38",
          "form": "남편친구가  검사 받앗는데 많이 먹어도 흡수 못하는 체질이 있다고 해요",
          "original_form": "남편친구가  검사 받앗는데 많이 먹어도 흡수 못하는 체질이 있다고 해요",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002131.1.39",
          "form": "엄청 드시는데 배만 살짝나오고 소화되면 다시 제자리요",
          "original_form": "엄청 드시는데 배만 살짝나오고 소화되면 다시 제자리요",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002131.1.40",
          "form": "그런가봐요 밥은 많이 먹는데 안쪄요",
          "original_form": "그런가봐요 밥은 많이 먹는데 안쪄요",
          "speaker_id": "1",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002131.1.41",
          "form": "아들이 배도 안나와요",
          "original_form": "아들이 배도 안나와요",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002131.1.42",
          "form": "아뱃살 부러워요 없어서",
          "original_form": "아뱃살 부러워요 없어서",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002131.1.43",
          "form": "저희가 그런체질이면 축복 받은건데",
          "original_form": "저희가 그런체질이면 축복 받은건데",
