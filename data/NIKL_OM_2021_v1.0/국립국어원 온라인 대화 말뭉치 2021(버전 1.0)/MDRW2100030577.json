      "utterance": [
        {
          "id": "MDRW2100030577.1.1",
          "form": "해외 아무데나요ㅋㅋㅋ",
          "original_form": "해외 아무데나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:30"
        },
        {
          "id": "MDRW2100030577.1.2",
          "form": "전 국내도 맘 놓고 다니고 싶어요",
          "original_form": "전 국내도 맘 놓고 다니고 싶어요",
          "speaker_id": "1",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030577.1.3",
          "form": "조금 잇음 추워지니 따뜻한 나라로 여행 좋을것 같아요",
          "original_form": "조금 잇음 추워지니 따뜻한 나라로 여행 좋을것 같아요",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030577.1.4",
          "form": "제주도 가고는 싶은데",
          "original_form": "제주도 가고는 싶은데",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030577.1.5",
          "form": "한국인 다 거기 모일까봐 ㅋㅋ",
          "original_form": "한국인 다 거기 모일까봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030577.1.6",
          "form": "코로나가 좀 잠잠 하면 모두다 제주도로 가죠",
          "original_form": "코로나가 좀 잠잠 하면 모두다 제주도로 가죠",
          "speaker_id": "1",
          "time": "20210904 14:42"
        },
        {
          "id": "MDRW2100030577.1.7",
          "form": "그니까요 사람 미어터질듯",
          "original_form": "그니까요 사람 미어터질듯",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100030577.1.8",
          "form": "여행지는 어딜가도 사람들이 많죠",
          "original_form": "여행지는 어딜가도 사람들이 많죠",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100030577.1.9",
          "form": "언제나 맘편히 여행 다닐수 있을까요 ?",
          "original_form": "언제나 맘편히 여행 다닐수 있을까요 ?",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100030577.1.10",
          "form": "올해도 망한거 같고 내년은 가능할까요",
          "original_form": "올해도 망한거 같고 내년은 가능할까요",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100030577.1.11",
          "form": "내년에 과연 코로나 종식이 될까 ?",
          "original_form": "내년에 과연 코로나 종식이 될까 ?",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100030577.1.12",
          "form": "글쎄요 그것도 모르겠어요 ㅎㅎ",
          "original_form": "글쎄요 그것도 모르겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100030577.1.13",
          "form": "진짜 진짜 이젠 점점 지쳐가네요",
          "original_form": "진짜 진짜 이젠 점점 지쳐가네요",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100030577.1.14",
          "form": "그러게요 너무 길어요 ㅋㅋㅋㅋ 휴",
          "original_form": "그러게요 너무 길어요 ㅋㅋㅋㅋ 휴",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100030577.1.15",
          "form": "가을 날씨도 좋아서 단풍구경 가고 싶어요",
          "original_form": "가을 날씨도 좋아서 단풍구경 가고 싶어요",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100030577.1.16",
          "form": "단풍구경도 재밌겠네요 ㅎㅎ",
          "original_form": "단풍구경도 재밌겠네요 ㅎㅎ",
          "speaker_id": "2",
