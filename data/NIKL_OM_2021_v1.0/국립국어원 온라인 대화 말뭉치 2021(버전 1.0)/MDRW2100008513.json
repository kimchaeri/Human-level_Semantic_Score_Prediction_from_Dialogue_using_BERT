      "utterance": [
        {
          "id": "MDRW2100008513.1.1",
          "form": "쨘 또바꿈",
          "original_form": "쨘 또바꿈",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008513.1.2",
          "form": "내 룸메는 여행을 못다닌게 한이 돼서",
          "original_form": "내 룸메는 여행을 못다닌게 한이 돼서",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008513.1.3",
          "form": "제주도로 이사갔다 ㅋ",
          "original_form": "제주도로 이사갔다 ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008513.1.4",
          "form": "넘나 극단적",
          "original_form": "넘나 극단적",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008513.1.5",
          "form": "아니 진짜 극단적 ㅋㅋㅅ",
          "original_form": "아니 진짜 극단적 ㅋㅋㅅ",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008513.1.6",
          "form": "현생은어쩌고?",
          "original_form": "현생은어쩌고?",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008513.1.7",
          "form": "생업은?",
          "original_form": "생업은?",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008513.1.8",
          "form": "아 지금 일 그만둿거든 ㅋ",
          "original_form": "아 지금 일 그만둿거든 ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008513.1.9",
          "form": "자유인임",
          "original_form": "자유인임",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008513.1.10",
          "form": "제주도 여행이 안되니 아예 살러감ㅋ",
          "original_form": "제주도 여행이 안되니 아예 살러감ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008513.1.11",
          "form": "와 자유롭다",
          "original_form": "와 자유롭다",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008513.1.12",
          "form": "멋지고 대단타",
          "original_form": "멋지고 대단타",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008513.1.13",
          "form": "용감타 ㅋㅋ",
          "original_form": "용감타 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008513.1.14",
          "form": "음 존경스러워!",
          "original_form": "음 존경스러워!",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008513.1.15",
          "form": "친구로 간접여행이나마 함ㅋㅋㅋ",
          "original_form": "친구로 간접여행이나마 함ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008513.1.16",
          "form": "name2은 영어잘하니 해외여행도",
          "original_form": "&name2&은 영어잘하니 해외여행도",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008513.1.17",
          "form": "잘다닐수있을거같은데 코로나가 문제야",
          "original_form": "잘다닐수있을거같은데 코로나가 문제야",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008513.1.18",
          "form": "마음이 불편할듯",
          "original_form": "마음이 불편할듯",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008513.1.19",
          "form": "여행경비도 그렇고",
          "original_form": "여행경비도 그렇고",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008513.1.20",
          "form": "여행경비도 문제야ㅋㅋ",
          "original_form": "여행경비도 문제야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008513.1.21",
          "form": "ㅇㅇ낭만적이지 못하지만",
          "original_form": "ㅇㅇ낭만적이지 못하지만",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008513.1.22",
          "form": "현실적으로 중요하져",
          "original_form": "현실적으로 중요하져",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008513.1.23",
          "form": "친구는 아일랜드 살다와서 언어가 편하더라고",
          "original_form": "친구는 아일랜드 살다와서 언어가 편하더라고",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008513.1.24",
          "form": "부럽더라",
          "original_form": "부럽더라",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008513.1.25",
          "form": "근데 언어는 또 안쓰면 까먹고",
          "original_form": "근데 언어는 또 안쓰면 까먹고",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008513.1.26",
          "form": "어디가고싶음?",
          "original_form": "어디가고싶음?",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008513.1.27",
          "form": "아니 나쁜말의 기준이뮈야",
          "original_form": "아니 나쁜말의 기준이뮈야",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008513.1.28",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008513.1.29",
          "form": "근데 언어는 또 안쓰면 까먹고",
          "original_form": "근데 언어는 또 안쓰면 까먹고",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008513.1.30",
          "form": "이게 어디가 나쁜말이얏",
          "original_form": "이게 어디가 나쁜말이얏",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008513.1.31",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008513.1.32",
          "form": "나도 그것이 궁금하다!",
          "original_form": "나도 그것이 궁금하다!",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008513.1.33",
          "form": "나는 아는데가 없어서 어디든 좋은데",
          "original_form": "나는 아는데가 없어서 어디든 좋은데",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008513.1.34",
          "form": "코타키나발루가서 스쿠버다이빙 해보고파",
          "original_form": "코타키나발루가서 스쿠버다이빙 해보고파",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008513.1.35",
          "form": "스쿠버다이빙이 핵심임",
          "original_form": "스쿠버다이빙이 핵심임",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008513.1.36",
          "form": "코타키나발루는 어디있음?",
          "original_form": "코타키나발루는 어디있음?",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008513.1.37",
          "form": "지방이름임? 나라이름임?",
          "original_form": "지방이름임? 나라이름임?",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008513.1.38",
          "form": "섬이던데 작은섬",
          "original_form": "섬이던데 작은섬",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008513.1.39",
          "form": "스쿠버다이빙 ㅋㅋ 해본적있음?",
          "original_form": "스쿠버다이빙 ㅋㅋ 해본적있음?",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008513.1.40",
          "form": "아니ㅋㅋ안해봄",
          "original_form": "아니ㅋㅋ안해봄",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008513.1.41",
          "form": "name2은여",
          "original_form": "&name2&은여",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008513.1.42",
          "form": "안해봤는데",
          "original_form": "안해봤는데",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008513.1.43",
          "form": "굳이 시도도 안하고픈?",
          "original_form": "굳이 시도도 안하고픈?",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008513.1.44",
          "form": "모두 취향이 있지ㅋㅋ",
          "original_form": "모두 취향이 있지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:45"
        }
      ]
