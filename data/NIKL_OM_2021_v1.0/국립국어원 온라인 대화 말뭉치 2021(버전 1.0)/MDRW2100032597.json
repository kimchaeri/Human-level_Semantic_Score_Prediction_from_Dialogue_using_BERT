      "utterance": [
        {
          "id": "MDRW2100032597.1.1",
          "form": "좋아는했던것같아요 ㅎㅎ",
          "original_form": "좋아는했던것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032597.1.2",
          "form": "주로 어떤 수업이 좋으셨어요??",
          "original_form": "주로 어떤 수업이 좋으셨어요??",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032597.1.3",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032597.1.4",
          "form": "수채화좋아했어요",
          "original_form": "수채화좋아했어요",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032597.1.5",
          "form": "name1님은요?ㅎㅎ",
          "original_form": "&name1&님은요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100032597.1.6",
          "form": "수채화랑 포스터 ㅠ 너무 어려웠어요",
          "original_form": "수채화랑 포스터 ㅠ 너무 어려웠어요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100032597.1.7",
          "form": "종이가 자꾸 슬퍼하더라구요",
          "original_form": "종이가 자꾸 슬퍼하더라구요",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032597.1.8",
          "form": "저는 만들기쪽을 좋아했어요",
          "original_form": "저는 만들기쪽을 좋아했어요",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032597.1.9",
          "form": "ㅋㅋㅋㅋㄱㅋㄱㅋㅋㅋ아 표현넘 센스",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱㅋㅋㅋ아 표현넘 센스",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032597.1.10",
          "form": "만들기는 진짜 금손이어야 가능한데!!!",
          "original_form": "만들기는 진짜 금손이어야 가능한데!!!",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032597.1.11",
          "form": "만들기 대단해요",
          "original_form": "만들기 대단해요",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032597.1.12",
          "form": "공간지각능력!!!",
          "original_form": "공간지각능력!!!",
          "speaker_id": "2",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032597.1.13",
          "form": "공간 지각능력보다는",
          "original_form": "공간 지각능력보다는",
          "speaker_id": "1",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032597.1.14",
          "form": "주로 공간을 자주 파괴하는 쪽이었어요 ㅋㅋㅋ",
          "original_form": "주로 공간을 자주 파괴하는 쪽이었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032597.1.15",
          "form": "그래서 자유 주제때 상당히 재미있었던 것 같아요",
          "original_form": "그래서 자유 주제때 상당히 재미있었던 것 같아요",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032597.1.16",
          "form": "ㅋㅋㅋㅋㅋ작품 구경해보고싶어요",
          "original_form": "ㅋㅋㅋㅋㅋ작품 구경해보고싶어요",
          "speaker_id": "2",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032597.1.17",
          "form": "멋질거같아요",
          "original_form": "멋질거같아요",
          "speaker_id": "2",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032597.1.18",
          "form": "아.. 사진을 차마 보내지 못하는게 아쉽네요",
          "original_form": "아.. 사진을 차마 보내지 못하는게 아쉽네요",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100032597.1.19",
          "form": "아주 현대 미술의 거장들이 울고갈 작품인데",
          "original_form": "아주 현대 미술의 거장들이 울고갈 작품인데",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032597.1.20",
          "form": "ㅋㅋㅋ 농담이에요",
          "original_form": "ㅋㅋㅋ 농담이에요",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032597.1.21",
