        {
          "id": "MDRW2100003094.1.1",
          "form": "학교 생활은 어때?",
          "original_form": "학교 생활은 어때?",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003094.1.2",
          "form": "졸업한 지가 오래돼서 ㅋㅋ",
          "original_form": "졸업한 지가 오래돼서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003094.1.3",
          "form": "ㅋㅋ 사실 나도 그래",
          "original_form": "ㅋㅋ 사실 나도 그래",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003094.1.4",
          "form": "학교 생활은 어땠어?",
          "original_form": "학교 생활은 어땠어?",
          "speaker_id": "2",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003094.1.5",
          "form": "언제?",
          "original_form": "언제?",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003094.1.6",
          "form": "초중고대 중에서 ㅋㅋ",
          "original_form": "초중고대 중에서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003094.1.7",
          "form": "대학교?",
          "original_form": "대학교?",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003094.1.8",
          "form": "어땠어?",
          "original_form": "어땠어?",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003094.1.9",
          "form": "나름 열심히 다녔던 거 같아",
          "original_form": "나름 열심히 다녔던 거 같아",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003094.1.10",
          "form": "재미있었어?",
          "original_form": "재미있었어?",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003094.1.11",
          "form": "아니 ㅋㅋㅋ 그냥 그랬어",
          "original_form": "아니 ㅋㅋㅋ 그냥 그랬어",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003094.1.12",
          "form": "재밌는 일도 있었지",
          "original_form": "재밌는 일도 있었지",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003094.1.13",
          "form": "나는 좀 아싸였어",
          "original_form": "나는 좀 아싸였어",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003094.1.14",
          "form": "오 그랬구나",
          "original_form": "오 그랬구나",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003094.1.15",
          "form": "친한 친구들끼리만 몰려다녔거든 ㅋㅋ",
          "original_form": "친한 친구들끼리만 몰려다녔거든 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003094.1.16",
          "form": "그래도 재밌었겠다",
