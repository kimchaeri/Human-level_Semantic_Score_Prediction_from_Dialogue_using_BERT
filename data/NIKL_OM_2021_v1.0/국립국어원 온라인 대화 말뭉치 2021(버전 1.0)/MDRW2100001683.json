        },
        {
          "id": "MDRW2100001683.1.2",
          "form": "요즘 보는 반려동물 유투버있나",
          "original_form": "요즘 보는 반려동물 유투버있나",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001683.1.3",
          "form": "나 사실 고양이 키워",
          "original_form": "나 사실 고양이 키워",
          "speaker_id": "2",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001683.1.4",
          "form": "반짝반짝 빛나는 고양이?",
          "original_form": "반짝반짝 빛나는 고양이?",
          "speaker_id": "1",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001683.1.5",
          "form": "아니 ㅋㅋㅋㅋㅋ진짜 고양이",
          "original_form": "아니 ㅋㅋㅋㅋㅋ진짜 고양이",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100001683.1.6",
          "form": "얼마전에 입양했음",
          "original_form": "얼마전에 입양했음",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100001683.1.7",
          "form": "무슨소리야",
          "original_form": "무슨소리야",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100001683.1.8",
          "form": "고양이 이름이 뭐야",
          "original_form": "고양이 이름이 뭐야",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100001683.1.9",
          "form": "name2?",
          "original_form": "&name2&?",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100001683.1.10",
          "form": "어어 내닉네임을 고양이 이름으로 지었음",
          "original_form": "어어 내닉네임을 고양이 이름으로 지었음",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100001683.1.11",
          "form": "오오 나중에 함 보러갈게",
          "original_form": "오오 나중에 함 보러갈게",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.12",
          "form": "근데 사료랑 간식 사려니까 돈 엄청 많이들더라",
          "original_form": "근데 사료랑 간식 사려니까 돈 엄청 많이들더라",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.13",
          "form": "역시 반려동물은 돈 많아야 키울 수 있는듯 ㅠ",
          "original_form": "역시 반려동물은 돈 많아야 키울 수 있는듯 ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.14",
          "form": "고양이 예방접종 이런거는 했나",
          "original_form": "고양이 예방접종 이런거는 했나",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.15",
          "form": "냥이들 간식 비싸더라",
          "original_form": "냥이들 간식 비싸더라",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.16",
          "form": "했지했지",
          "original_form": "했지했지",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.17",
          "form": "병원가니까 싸악 맞춰주더라",
          "original_form": "병원가니까 싸악 맞춰주더라",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.18",
          "form": "맞아.. 그래도 냥이 산책은 안시켜도 되니까 편하겠다",
          "original_form": "맞아.. 그래도 냥이 산책은 안시켜도 되니까 편하겠다",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.19",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.20",
          "form": "싸악. 편하겠네",
          "original_form": "싸악. 편하겠네",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.21",
          "form": "강아지는 산책 시켜야하는데",
          "original_form": "강아지는 산책 시켜야하는데",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.22",
          "form": "나도 산책안하는데",
          "original_form": "나도 산책안하는데",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100001683.1.23",
          "form": "나 어제 산책하다가 고양이전용 미용실?봤는데",
          "original_form": "나 어제 산책하다가 고양이전용 미용실?봤는데",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100001683.1.24",
          "form": "강아지가 상전이야,,",
          "original_form": "강아지가 상전이야,,",
