      "utterance": [
        {
          "id": "MDRW2100002329.1.1",
          "form": "요즘 육아 좀 어때 ㅋㅋㅋ",
          "original_form": "요즘 육아 좀 어때 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002329.1.2",
          "form": "오늘도 새벽에 잠 ??",
          "original_form": "오늘도 새벽에 잠 ??",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002329.1.3",
          "form": "새벽3시에 일어나서 분유먹이고",
          "original_form": "새벽3시에 일어나서 분유먹이고",
          "speaker_id": "2",
          "time": "20210820 09:46"
        },
        {
          "id": "MDRW2100002329.1.4",
          "form": "트림시키고 재우고 4시에 다시잤다 ㅋㅋ",
          "original_form": "트림시키고 재우고 4시에 다시잤다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:46"
        },
        {
          "id": "MDRW2100002329.1.5",
          "form": "어제는 그래도 잘잔편이야 ㅋㅋ",
          "original_form": "어제는 그래도 잘잔편이야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:47"
        },
        {
          "id": "MDRW2100002329.1.6",
          "form": "와...... 대단하다",
          "original_form": "와...... 대단하다",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002329.1.7",
          "form": "아직 도우미분 오시는거야???",
          "original_form": "아직 도우미분 오시는거야???",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002329.1.8",
          "form": "도우미는 저번주에 끝났다ㅠㅠ",
          "original_form": "도우미는 저번주에 끝났다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002329.1.9",
          "form": "도우미분 또 못 불러?",
          "original_form": "도우미분 또 못 불러?",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002329.1.10",
          "form": "힘들잖아....",
          "original_form": "힘들잖아....",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002329.1.11",
          "form": "도우미 비싸다..",
          "original_form": "도우미 비싸다..",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002329.1.12",
          "form": "2주에 100만원 넘어 ㅋㅋ",
          "original_form": "2주에 100만원 넘어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002329.1.13",
          "form": "처음할땐 정부에서 지원 잘해줘서 했지 ㅋㅋ",
          "original_form": "처음할땐 정부에서 지원 잘해줘서 했지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002329.1.14",
          "form": "헐 엄청 비싸다",
          "original_form": "헐 엄청 비싸다",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002329.1.15",
          "form": "지원을 한번만 받을수있는거야?",
          "original_form": "지원을 한번만 받을수있는거야?",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002329.1.16",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002329.1.17",
          "form": "계속 해주면 좋을텐데ㅋㅋ",
          "original_form": "계속 해주면 좋을텐데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002329.1.18",
          "form": "잘알아둬라",
          "original_form": "잘알아둬라",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002329.1.19",
          "form": "너 집안일 좀 잘 함?",
          "original_form": "너 집안일 좀 잘 함?",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002329.1.20",
          "form": "어릴때부터 했었지",
          "original_form": "어릴때부터 했었지",
          "speaker_id": "1",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100002329.1.21",
          "form": "요리는 못하지만 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "요리는 못하지만 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100002329.1.22",
          "form": "ㅋㅋㅋㅋㅋ지금도 해야될거아냐",
          "original_form": "ㅋㅋㅋㅋㅋ지금도 해야될거아냐",
          "speaker_id": "2",
          "time": "20210820 19:30"
        },
        {
          "id": "MDRW2100002329.1.23",
          "form": "빨래하고 설거지하고",
          "original_form": "빨래하고 설거지하고",
          "speaker_id": "2",
          "time": "20210820 19:30"
        },
        {
          "id": "MDRW2100002329.1.24",
          "form": "요리는 아예 안하냐 ㅋㅋ",
          "original_form": "요리는 아예 안하냐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:30"
        },
        {
          "id": "MDRW2100002329.1.25",
          "form": "지금도 해 ㅡㅡ",
          "original_form": "지금도 해 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100002329.1.26",
          "form": "안한다고 안했는데",
          "original_form": "안한다고 안했는데",
          "speaker_id": "1",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100002329.1.27",
          "form": "아하 잘하고있구만",
          "original_form": "아하 잘하고있구만",
          "speaker_id": "2",
          "time": "20210820 20:17"
        },
        {
          "id": "MDRW2100002329.1.28",
          "form": "집안일은 끝이없지",
          "original_form": "집안일은 끝이없지",
          "speaker_id": "2",
          "time": "20210820 20:17"
        },
        {
          "id": "MDRW2100002329.1.29",
          "form": "박지성이 한 말 생각난다",
          "original_form": "박지성이 한 말 생각난다",
          "speaker_id": "1",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002329.1.30",
          "form": "축구랑 육아 중에 뭐가 더 힘드냐니까 육아래",
          "original_form": "축구랑 육아 중에 뭐가 더 힘드냐니까 육아래",
          "speaker_id": "1",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002329.1.31",
          "form": "그래서 왜냐니까 육아는 종료 휘슬이 없다고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래서 왜냐니까 육아는 종료 휘슬이 없다고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:40"
        }
      ]
    }
