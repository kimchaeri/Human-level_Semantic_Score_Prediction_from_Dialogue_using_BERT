      "utterance": [
        {
          "id": "MDRW2100018632.1.1",
          "form": "종교활동 하시나요?",
          "original_form": "종교활동 하시나요?",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018632.1.2",
          "form": "아뇨 제가 종교가 없어서.. ㅎㅎ name1은요?",
          "original_form": "아뇨 제가 종교가 없어서.. ㅎㅎ &name1&은요?",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018632.1.3",
          "form": "저두요 무교예요",
          "original_form": "저두요 무교예요",
          "speaker_id": "1",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018632.1.4",
          "form": "교회 가보신적은 있나요?",
          "original_form": "교회 가보신적은 있나요?",
          "speaker_id": "1",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018632.1.5",
          "form": "어릴때 몇번 가본적있어요 ㅎㅎ",
          "original_form": "어릴때 몇번 가본적있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018632.1.6",
          "form": "저도 어릴때 크리스마스 시즌에 과자받으러 갔었어요",
          "original_form": "저도 어릴때 크리스마스 시즌에 과자받으러 갔었어요",
          "speaker_id": "1",
          "time": "20210827 09:30"
        },
        {
          "id": "MDRW2100018632.1.7",
          "form": "ㅋㅋㅋㅋ 맞아요 저도 초코파이 먹으러 갔어요",
          "original_form": "ㅋㅋㅋㅋ 맞아요 저도 초코파이 먹으러 갔어요",
          "speaker_id": "2",
          "time": "20210827 09:30"
        },
        {
          "id": "MDRW2100018632.1.8",
          "form": "앜ㅋㅋㅋㄱ역시 다들 비슷하네요",
          "original_form": "앜ㅋㅋㅋㄱ역시 다들 비슷하네요",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018632.1.9",
          "form": "절은 가보신적 있으세요?",
          "original_form": "절은 가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018632.1.10",
          "form": "ㅋㅋ절은 아직한번도 없는데 가보고싶긴해요! name1은여?",
          "original_form": "ㅋㅋ절은 아직한번도 없는데 가보고싶긴해요! &name1&은여?",
          "speaker_id": "2",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100018632.1.11",
          "form": "저는 몇번 가봤어요",
          "original_form": "저는 몇번 가봤어요",
          "speaker_id": "1",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100018632.1.12",
          "form": "절이 보통 산속에 있다보니까 풍경이좋아서",
          "original_form": "절이 보통 산속에 있다보니까 풍경이좋아서",
          "speaker_id": "1",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100018632.1.13",
          "form": "관광느낌으로 가봤어요",
          "original_form": "관광느낌으로 가봤어요",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018632.1.14",
          "form": "저도 한번 가보고싶어요 ㅎㅎ",
          "original_form": "저도 한번 가보고싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100018632.1.15",
          "form": "저번에 가려다 못가서..",
          "original_form": "저번에 가려다 못가서..",
          "speaker_id": "2",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100018632.1.16",
          "form": "가을에 가는거 추천드려요.! 확실히 가을이 이뻐서요",
          "original_form": "가을에 가는거 추천드려요.! 확실히 가을이 이뻐서요",
          "speaker_id": "1",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018632.1.17",
          "form": "낙엽 물들고 좋을 거 같아요 ㅎㅎ 추천감사합니다",
          "original_form": "낙엽 물들고 좋을 거 같아요 ㅎㅎ 추천감사합니다",
          "speaker_id": "2",
          "time": "20210827 09:49"
        },
        {
          "id": "MDRW2100018632.1.18",
          "form": "절은 종교활동으로 오시는분반 관광 반인거 같아요",
          "original_form": "절은 종교활동으로 오시는분반 관광 반인거 같아요",
