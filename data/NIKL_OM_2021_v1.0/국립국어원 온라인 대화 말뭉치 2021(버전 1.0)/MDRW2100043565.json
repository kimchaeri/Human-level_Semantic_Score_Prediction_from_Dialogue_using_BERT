      "utterance": [
        {
          "id": "MDRW2100043565.1.1",
          "form": "놀토재밋어서 챙겨봐요",
          "original_form": "놀토재밋어서 챙겨봐요",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043565.1.2",
          "form": "아하 저도 놀토 보긴보는데 게스트 재밌을거 같은사람 나오는편만 봐여 ㅋㅋㅋ",
          "original_form": "아하 저도 놀토 보긴보는데 게스트 재밌을거 같은사람 나오는편만 봐여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043565.1.3",
          "form": "최근에는 안봤는데 재밌는편 있었나요?",
          "original_form": "최근에는 안봤는데 재밌는편 있었나요?",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043565.1.4",
          "form": "전 다재미잇어서 안빼놓고 봐요ㅋㅋ",
          "original_form": "전 다재미잇어서 안빼놓고 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043565.1.5",
          "form": "맛녀석도 좋아해요 전",
          "original_form": "맛녀석도 좋아해요 전",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100043565.1.6",
          "form": "아하 ㅎㅎㅎ 전 먹방을 그닥 선호하지 않는 편이에요ㅎㅎ",
          "original_form": "아하 ㅎㅎㅎ 전 먹방을 그닥 선호하지 않는 편이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100043565.1.7",
          "form": "요즘 넷플에서 볼만한거 있나요?",
          "original_form": "요즘 넷플에서 볼만한거 있나요?",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100043565.1.8",
          "form": "전 유튜브를 주로 봐서요ㅋㅋ",
          "original_form": "전 유튜브를 주로 봐서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100043565.1.9",
          "form": "의학드라마 리뷰해주는거 자주봐요ㅋㅋ",
          "original_form": "의학드라마 리뷰해주는거 자주봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100043565.1.10",
          "form": "의학드라마면 슬의생이요?",
          "original_form": "의학드라마면 슬의생이요?",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100043565.1.11",
          "form": "네 의사들이 리뷰해주는거 재밋어요",
          "original_form": "네 의사들이 리뷰해주는거 재밋어요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100043565.1.12",
          "form": "저도 한번보긴했어요 ㅋㅋㅋ 유쾌하신분들이 리뷰해주시더라구요",
          "original_form": "저도 한번보긴했어요 ㅋㅋㅋ 유쾌하신분들이 리뷰해주시더라구요",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100043565.1.13",
          "form": "슬의생도 보시나요?",
          "original_form": "슬의생도 보시나요?",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100043565.1.14",
          "form": "네 챙겨봐요",
          "original_form": "네 챙겨봐요",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100043565.1.15",
          "form": "넘재밋죠",
          "original_form": "넘재밋죠",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100043565.1.16",
          "form": "맞아요 중간에 좀 지루하다 싶었는데 저번주에 다시 재밌어 지더라구요",
          "original_form": "맞아요 중간에 좀 지루하다 싶었는데 저번주에 다시 재밌어 지더라구요",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100043565.1.17",
          "form": "네 설마 시즌3잇진 않겟죠",
          "original_form": "네 설마 시즌3잇진 않겟죠",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100043565.1.18",
          "form": "있었으면 좋겠어요 중간중간 조정석이 웃겨줘서 좋아요 ㅋㅋㅋ",
          "original_form": "있었으면 좋겠어요 중간중간 조정석이 웃겨줘서 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100043565.1.19",
          "form": "캐릭터 너무 귀여운듯해요",
          "original_form": "캐릭터 너무 귀여운듯해요",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100043565.1.20",
          "form": "실제로도 그럴거같은 느낌!",
          "original_form": "실제로도 그럴거같은 느낌!",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100043565.1.21",
          "form": "네 조정석 진짜 연기잘해요ㅋ",
          "original_form": "네 조정석 진짜 연기잘해요ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100043565.1.22",
          "form": "하는건 좋은데 또 일년기다려야하나요ㅠ",
          "original_form": "하는건 좋은데 또 일년기다려야하나요ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100043565.1.23",
          "form": "ㅠㅠ 기다리기 힘들어요",
          "original_form": "ㅠㅠ 기다리기 힘들어요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100043565.1.24",
          "form": "전 응답하라 또 했으면 좋겠어요",
          "original_form": "전 응답하라 또 했으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100043565.1.25",
          "form": "응답하라도 재밌었어요 ㅋㅋ",
          "original_form": "응답하라도 재밌었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100043565.1.26",
          "form": "네 재미잇엇죠 혜리가 연기를 잘하더라고요",
          "original_form": "네 재미잇엇죠 혜리가 연기를 잘하더라고요",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100043565.1.27",
          "form": "맞아요 ㅋㅋㅋ 1988이 전 제일 재밌었어요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 1988이 전 제일 재밌었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100043565.1.28",
          "form": "근데 보라는 진짜ㅜㅜ 제 딸이였으면 뚜두려 팼을듯해요..",
          "original_form": "근데 보라는 진짜ㅜㅜ 제 딸이였으면 뚜두려 팼을듯해요..",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100043565.1.29",
          "form": "폭력은 나쁘지만 못 참았을듯해요 ㅋㅋㅋㅋ",
          "original_form": "폭력은 나쁘지만 못 참았을듯해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100043565.1.30",
          "form": "ㅋㅋ진짜요 \r\n전 혜리 좋아져서 간동거도 봣어요",
          "original_form": "ㅋㅋ진짜요 \r\n전 혜리 좋아져서 간동거도 봣어요",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100043565.1.31",
          "form": "간동거도 보셧나요",
          "original_form": "간동거도 보셧나요",
          "speaker_id": "1",
          "time": "20210908 10:33"
        }
      ]
    }
