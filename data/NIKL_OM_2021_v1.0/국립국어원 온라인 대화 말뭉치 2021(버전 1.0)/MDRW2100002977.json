      "utterance": [
        {
          "id": "MDRW2100002977.1.1",
          "form": "넌 수강신청 했어?",
          "original_form": "넌 수강신청 했어?",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002977.1.2",
          "form": "응 진작에 했지",
          "original_form": "응 진작에 했지",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002977.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002977.1.4",
          "form": "나도 했어",
          "original_form": "나도 했어",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002977.1.5",
          "form": "원래 하고싶었던 교수님 못했는데",
          "original_form": "원래 하고싶었던 교수님 못했는데",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002977.1.6",
          "form": "그래도 원래 하고싶었던 시간은 성공했어",
          "original_form": "그래도 원래 하고싶었던 시간은 성공했어",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002977.1.7",
          "form": "너는 시간이나 과목 마음에 들어?",
          "original_form": "너는 시간이나 과목 마음에 들어?",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002977.1.8",
          "form": "아니 폐강된 강의가 있어서",
          "original_form": "아니 폐강된 강의가 있어서",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002977.1.9",
          "form": "수강정정기간에 다시 신청 해야해ㅠㅠ",
          "original_form": "수강정정기간에 다시 신청 해야해ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002977.1.10",
          "form": "으악! 이럴수가",
          "original_form": "으악! 이럴수가",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002977.1.11",
          "form": "너무 번거롭겠다",
          "original_form": "너무 번거롭겠다",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002977.1.12",
          "form": "폐강된 적은 처음이라 당황스러운데",
          "original_form": "폐강된 적은 처음이라 당황스러운데",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002977.1.13",
          "form": "잘되겠지?",
          "original_form": "잘되겠지?",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002977.1.14",
          "form": "응응 잘 정정해보자",
          "original_form": "응응 잘 정정해보자",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002977.1.15",
          "form": "당황스러웠겠다",
          "original_form": "당황스러웠겠다",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002977.1.16",
          "form": "그리고 강의 하나가 대기에서 멈춘 상태라",
          "original_form": "그리고 강의 하나가 대기에서 멈춘 상태라",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002977.1.17",
          "form": "이 강의도 어떻게 될지 모르겠어",
          "original_form": "이 강의도 어떻게 될지 모르겠어",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002977.1.18",
          "form": "헉 그거 무섭네",
          "original_form": "헉 그거 무섭네",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002977.1.19",
          "form": "그냥 다 됐으면 좋겠다",
          "original_form": "그냥 다 됐으면 좋겠다",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002977.1.20",
          "form": "상황보고 바꾸든지 그냥 두든지 해야지ㅠㅠ",
          "original_form": "상황보고 바꾸든지 그냥 두든지 해야지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002977.1.21",
          "form": "응 수강신청 잘 해결되길 바라",
          "original_form": "응 수강신청 잘 해결되길 바라",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002977.1.22",
          "form": "아 제발 그랬으면 좋겠어ㅠㅠ",
          "original_form": "아 제발 그랬으면 좋겠어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002977.1.23",
          "form": "이번에 몇학점 들어?",
          "original_form": "이번에 몇학점 들어?",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002977.1.24",
          "form": "저거 두개가 된다는 전제하에 19학점...",
          "original_form": "저거 두개가 된다는 전제하에 19학점...",
          "speaker_id": "2",
          "time": "20210820 17:25"
        }
      ]
