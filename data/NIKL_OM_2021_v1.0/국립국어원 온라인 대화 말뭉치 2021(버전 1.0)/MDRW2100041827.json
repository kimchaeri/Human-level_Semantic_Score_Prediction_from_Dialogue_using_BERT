      "utterance": [
        {
          "id": "MDRW2100041827.1.1",
          "form": "식사 메뉴 매일 고민이에요ㅠㅠ",
          "original_form": "식사 메뉴 매일 고민이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041827.1.2",
          "form": "아 그쵸 ㅎㅎ",
          "original_form": "아 그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100041827.1.3",
          "form": "엄마가 항상 점심드시고 돌아서시면 저녁은 뭐먹지 하시는데",
          "original_form": "엄마가 항상 점심드시고 돌아서시면 저녁은 뭐먹지 하시는데",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100041827.1.4",
          "form": "요리하는 입장에서는 맨날 고민이실것같습니당",
          "original_form": "요리하는 입장에서는 맨날 고민이실것같습니당",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100041827.1.5",
          "form": "저녁 맛있게 드셨나요",
          "original_form": "저녁 맛있게 드셨나요",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100041827.1.6",
          "form": "애들 먹이고 간단히 먹었어요",
          "original_form": "애들 먹이고 간단히 먹었어요",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100041827.1.7",
          "form": "돌아서면 배고프다고 해서ㅠ",
          "original_form": "돌아서면 배고프다고 해서ㅠ",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100041827.1.8",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100041827.1.9",
          "form": "오늘 하루도 정신없이 지나가셨겠네요",
          "original_form": "오늘 하루도 정신없이 지나가셨겠네요",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100041827.1.10",
          "form": "회사점심때는 매일 배달 음식 먹어요",
          "original_form": "회사점심때는 매일 배달 음식 먹어요",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041827.1.11",
          "form": "지겹네요ㅠㅠ",
          "original_form": "지겹네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041827.1.12",
          "form": "아아 진짜요?ㅜㅜ",
          "original_form": "아아 진짜요?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041827.1.13",
          "form": "요새 밖에서는 못드시나요?",
          "original_form": "요새 밖에서는 못드시나요?",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041827.1.14",
          "form": "저희 회사는 밖에서는 최대한 먹지 말라그래서",
          "original_form": "저희 회사는 밖에서는 최대한 먹지 말라그래서",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041827.1.15",
          "form": "걸어가서 먹을곳이 근처에 없어서 차타야해서",
          "original_form": "걸어가서 먹을곳이 근처에 없어서 차타야해서",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041827.1.16",
          "form": "다들 귀찮은가봐요",
          "original_form": "다들 귀찮은가봐요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041827.1.17",
          "form": "요새 도시락이나 배달음식 시켜요",
          "original_form": "요새 도시락이나 배달음식 시켜요",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100041827.1.18",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100041827.1.19",
          "form": "배달음식은 주로 뭐 시켜드세요?",
          "original_form": "배달음식은 주로 뭐 시켜드세요?",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100041827.1.20",
          "form": "네 1년정도 시켜먹으니 질리네요",
          "original_form": "네 1년정도 시켜먹으니 질리네요",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100041827.1.21",
          "form": "찌개 국밥 중국집 덮밥 등이요",
          "original_form": "찌개 국밥 중국집 덮밥 등이요",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100041827.1.22",
          "form": "아 네..하기야 1년이상 배달음식 드셨으면 질릴만도 하시네요 ㅠㅠ",
          "original_form": "아 네..하기야 1년이상 배달음식 드셨으면 질릴만도 하시네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100041827.1.23",
          "form": "그래도 코로나 전만 해도 근처에 새로운 식당도 가보고 했던것 같은데",
          "original_form": "그래도 코로나 전만 해도 근처에 새로운 식당도 가보고 했던것 같은데",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100041827.1.24",
          "form": "제약이 너무 많아진것같아여",
          "original_form": "제약이 너무 많아진것같아여",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100041827.1.25",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100041827.1.26",
          "form": "식당에서 갓 나왔을때가 제일 맛있는데",
          "original_form": "식당에서 갓 나왔을때가 제일 맛있는데",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100041827.1.27",
          "form": "그쵸..음식은 따땃할때 먹어야하는데 ㅠㅠㅠㅠ",
          "original_form": "그쵸..음식은 따땃할때 먹어야하는데 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041827.1.28",
          "form": "카페도 가서 마스크없이 앉아있고싶네요..ㅜㅜ",
          "original_form": "카페도 가서 마스크없이 앉아있고싶네요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041827.1.29",
          "form": "맛있는 메뉴 시켜도 배달하니 맛없네요ㅜ",
          "original_form": "맛있는 메뉴 시켜도 배달하니 맛없네요ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041827.1.30",
          "form": "혹시 커피 좋아하시나요?",
          "original_form": "혹시 커피 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041827.1.31",
          "form": "예전이 그리워요ㅠㅠ",
          "original_form": "예전이 그리워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041827.1.32",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:43"
        }
      ]
