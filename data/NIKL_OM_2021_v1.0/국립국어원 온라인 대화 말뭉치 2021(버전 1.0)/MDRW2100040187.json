      "utterance": [
        {
          "id": "MDRW2100040187.1.1",
          "form": "제페토 라는거 아세요?",
          "original_form": "제페토 라는거 아세요?",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100040187.1.2",
          "form": "오 알아요!!!",
          "original_form": "오 알아요!!!",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100040187.1.3",
          "form": "제 친구의 지인의 지인이,,,",
          "original_form": "제 친구의 지인의 지인이,,,",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100040187.1.4",
          "form": "제페토 개발자라고",
          "original_form": "제페토 개발자라고",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100040187.1.5",
          "form": "들어가지곸ㅋㅋㅋㅋㅋ 돈 많이 버셨대요....",
          "original_form": "들어가지곸ㅋㅋㅋㅋㅋ 돈 많이 버셨대요....",
          "speaker_id": "1",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.6",
          "form": "우와 부러워요",
          "original_form": "우와 부러워요",
          "speaker_id": "2",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.7",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.8",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "2",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.9",
          "form": "이제는 개발자의 시대예요",
          "original_form": "이제는 개발자의 시대예요",
          "speaker_id": "1",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.10",
          "form": "그 자기 아바타 만드는거 맞죠!!!",
          "original_form": "그 자기 아바타 만드는거 맞죠!!!",
          "speaker_id": "1",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.11",
          "form": "사진 찍어서!",
          "original_form": "사진 찍어서!",
          "speaker_id": "1",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.12",
          "form": "저 한 번 해본거같아요 폰이 갤럭시라?!",
          "original_form": "저 한 번 해본거같아요 폰이 갤럭시라?!",
          "speaker_id": "1",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040187.1.13",
          "form": "아 그렇군요 제페토ㅋㅋ  주식있나요?주식을 사야되나",
          "original_form": "아 그렇군요 제페토ㅋㅋ  주식있나요?주식을 사야되나",
          "speaker_id": "2",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040187.1.14",
          "form": "앜ㅋㅋㅋㅋㅋ 주식",
          "original_form": "앜ㅋㅋㅋㅋㅋ 주식",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040187.1.15",
          "form": "전어제 첨들었는데 흥미롭더라구요",
          "original_form": "전어제 첨들었는데 흥미롭더라구요",
          "speaker_id": "2",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040187.1.16",
          "form": "좋은 관점인데요?!",
          "original_form": "좋은 관점인데요?!",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040187.1.17",
          "form": "저는 항상 대기업을 주로 사서",
          "original_form": "저는 항상 대기업을 주로 사서",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040187.1.18",
          "form": "다른 시장에 대해선 너무 문외한이에요ㅠ",
          "original_form": "다른 시장에 대해선 너무 문외한이에요ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040187.1.19",
          "form": "전 사실 한번도 안해봤어요ㅋㅋ",
          "original_form": "전 사실 한번도 안해봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100040187.1.20",
          "form": "제페토 재밌나요?",
          "original_form": "제페토 재밌나요?",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100040187.1.21",
          "form": "오 제페토 검색해보니까",
          "original_form": "오 제페토 검색해보니까",
          "speaker_id": "1",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100040187.1.22",
          "form": "증강현실",
          "original_form": "증강현실",
