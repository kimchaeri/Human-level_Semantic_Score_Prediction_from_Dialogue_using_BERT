      "utterance": [
        {
          "id": "MDRW2100025628.1.1",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025628.1.2",
          "form": "영화관은 자주 안가요",
          "original_form": "영화관은 자주 안가요",
          "speaker_id": "1",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025628.1.3",
          "form": "저도 거리두기때문에",
          "original_form": "저도 거리두기때문에",
          "speaker_id": "2",
          "time": "20210901 16:31"
        },
        {
          "id": "MDRW2100025628.1.4",
          "form": "네 요즘에는 가기가 좀그렇죠",
          "original_form": "네 요즘에는 가기가 좀그렇죠",
          "speaker_id": "1",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025628.1.5",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025628.1.6",
          "form": "어디가지도 못하고 전 집에서 티비 많이 봐요",
          "original_form": "어디가지도 못하고 전 집에서 티비 많이 봐요",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025628.1.7",
          "form": "저도 그게 제일",
          "original_form": "저도 그게 제일",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025628.1.8",
          "form": "어떤거 챙겨보세요",
          "original_form": "어떤거 챙겨보세요",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025628.1.9",
          "form": "영화티비에서 하는걸요",
          "original_form": "영화티비에서 하는걸요",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025628.1.10",
          "form": "전 티비로는 영화가 집중이 안되요\r\n자꾸 딴일하고",
          "original_form": "전 티비로는 영화가 집중이 안되요\r\n자꾸 딴일하고",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025628.1.11",
          "form": "밤에 보면 좀 나아요",
          "original_form": "밤에 보면 좀 나아요",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025628.1.12",
          "form": "아 그렇겠네요\r\n담에  밤에 봐봐야겠어요",
          "original_form": "아 그렇겠네요\r\n담에  밤에 봐봐야겠어요",
