      "utterance": [
        {
          "id": "MDRW2100047726.1.1",
          "form": "2주나 어떻게 기다릴까여ㅠ",
          "original_form": "2주나 어떻게 기다릴까여ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100047726.1.2",
          "form": "2주나 기다여야 하네여ㅠ",
          "original_form": "2주나 기다여야 하네여ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100047726.1.3",
          "form": "다음주 안하나요?",
          "original_form": "다음주 안하나요?",
          "speaker_id": "2",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100047726.1.4",
          "form": "다음주 카타르 월드컵 예선? 때문에 결방인걸로 알아요ㅠ",
          "original_form": "다음주 카타르 월드컵 예선? 때문에 결방인걸로 알아요ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100047726.1.5",
          "form": "학 진짜요? 채널도 많은데 왜하필 거기서 해주나요ㅠ",
          "original_form": "학 진짜요? 채널도 많은데 왜하필 거기서 해주나요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100047726.1.6",
          "form": "그니까요ㅠㅜㅠ 월드컵 예선이 뭐라구....ㅠㅠ",
          "original_form": "그니까요ㅠㅜㅠ 월드컵 예선이 뭐라구....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100047726.1.7",
          "form": "일주일 한번하는것도 아쉬운데 이주나 잇어야 하다니\n너무하네요ㅠ",
          "original_form": "일주일 한번하는것도 아쉬운데 이주나 잇어야 하다니\n너무하네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100047726.1.8",
          "form": "그동안 뭘봐야하죠",
          "original_form": "그동안 뭘봐야하죠",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100047726.1.9",
          "form": "국대분들 나온 예능이나 보려구요...ㅎ",
          "original_form": "국대분들 나온 예능이나 보려구요...ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100047726.1.10",
          "form": "전볼거없으면 맛있는녀석들만 돌려봐여",
          "original_form": "전볼거없으면 맛있는녀석들만 돌려봐여",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100047726.1.11",
          "form": "재밌고 다 좋은데 늦게 보면 배고파져요ㅠㅠ",
          "original_form": "재밌고 다 좋은데 늦게 보면 배고파져요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100047726.1.12",
          "form": "맞아여ㅋㅋ\n김준현 빠져서 너무 아쉬워요ㅠ",
          "original_form": "맞아여ㅋㅋ\n김준현 빠져서 너무 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100047726.1.13",
          "form": "그니까여ㅠㅠ 예전 방송들을 더 보게 돼요ㅋㅋ",
          "original_form": "그니까여ㅠㅠ 예전 방송들을 더 보게 돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100047726.1.14",
          "form": "맛표현은 김준현 한입만은 문세윤인데 진짜ㅠ",
          "original_form": "맛표현은 김준현 한입만은 문세윤인데 진짜ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100047726.1.15",
          "form": "김준현 맛표현 정말 인정해요...진짜 어나더인ㅜㅜ 운동은 김민경ㅋㅋㅋ",
          "original_form": "김준현 맛표현 정말 인정해요...진짜 어나더인ㅜㅜ 운동은 김민경ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:54"
        }
