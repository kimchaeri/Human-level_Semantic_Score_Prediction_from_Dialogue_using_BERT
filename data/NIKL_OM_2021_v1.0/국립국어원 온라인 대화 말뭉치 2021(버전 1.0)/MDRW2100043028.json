      "utterance": [
        {
          "id": "MDRW2100043028.1.1",
          "form": "네 수요일엔 특식이 나와요",
          "original_form": "네 수요일엔 특식이 나와요",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.2",
          "form": "지난주엔 갈비탕",
          "original_form": "지난주엔 갈비탕",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.3",
          "form": "이번주 치킨",
          "original_form": "이번주 치킨",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.4",
          "form": "담주는 주꾸미비빔밥",
          "original_form": "담주는 주꾸미비빔밥",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.5",
          "form": "ㅋㅋ저는 수욜엔 열두시반 퇴근이라서",
          "original_form": "ㅋㅋ저는 수욜엔 열두시반 퇴근이라서",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.6",
          "form": "이라고ㅋㅋ",
          "original_form": "이라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.7",
          "form": "빨리 내일됏음",
          "original_form": "빨리 내일됏음",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.9",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.10",
          "form": "식당밥 미쳣네요",
          "original_form": "식당밥 미쳣네요",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.11",
          "form": "아아!! 특식보단 빠른퇴근이 더 좋은데요",
          "original_form": "아아!! 특식보단 빠른퇴근이 더 좋은데요",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043028.1.12",
          "form": "일할맛나겟어유",
          "original_form": "일할맛나겟어유",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043028.1.13",
          "form": "ㅋㅋ그쳐",
          "original_form": "ㅋㅋ그쳐",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043028.1.14",
          "form": "지금도 다섯시만 기다림다",
          "original_form": "지금도 다섯시만 기다림다",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043028.1.15",
          "form": "집에가고싶어요ㅋㅋㅋㅋ",
          "original_form": "집에가고싶어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043028.1.16",
          "form": "네네 아무리 맛있는 밥을 줘도",
          "original_form": "네네 아무리 맛있는 밥을 줘도",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043028.1.17",
          "form": "퇴근후 집밥 먹는 게 제일이죠",
          "original_form": "퇴근후 집밥 먹는 게 제일이죠",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043028.1.18",
          "form": "ㅋㅋ네!!",
          "original_form": "ㅋㅋ네!!",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043028.1.19",
          "form": "다섯시!!!!빨리와라!!!!!",
          "original_form": "다섯시!!!!빨리와라!!!!!",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043028.1.20",
          "form": "그러게요!! 3시간 반 얼른 지나라!!",
          "original_form": "그러게요!! 3시간 반 얼른 지나라!!",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043028.1.21",
          "form": "ㅋㅋ몇시퇴근이신가용",
          "original_form": "ㅋㅋ몇시퇴근이신가용",
          "speaker_id": "1",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043028.1.22",
          "form": "저도 5시 전후예요",
          "original_form": "저도 5시 전후예요",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043028.1.23",
          "form": "ㅋㅋ저흰 4시50분에 튀어나갈준비",
          "original_form": "ㅋㅋ저흰 4시50분에 튀어나갈준비",
