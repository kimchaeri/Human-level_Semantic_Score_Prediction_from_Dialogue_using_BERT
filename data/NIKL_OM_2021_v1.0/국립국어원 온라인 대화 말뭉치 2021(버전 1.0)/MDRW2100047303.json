      "utterance": [
        {
          "id": "MDRW2100047303.1.1",
          "form": "결론은",
          "original_form": "결론은",
          "speaker_id": "2",
          "time": "20210826 10:08"
        },
        {
          "id": "MDRW2100047303.1.2",
          "form": "이미 중나에도 시장가가 7천원이고",
          "original_form": "이미 중나에도 시장가가 7천원이고",
          "speaker_id": "2",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100047303.1.3",
          "form": "방금 동생들에게 파인트 베라쿠폰주니까",
          "original_form": "방금 동생들에게 파인트 베라쿠폰주니까",
          "speaker_id": "1",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100047303.1.4",
          "form": "내동생들 넷이 웅성웅성 함ㅋㅋ",
          "original_form": "내동생들 넷이 웅성웅성 함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100047303.1.5",
          "form": "둘 아니었음? ㅋㅋ",
          "original_form": "둘 아니었음? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100047303.1.6",
          "form": "아 중나 시세 싸네 담에 거기서 구해야겠당ㅋ",
          "original_form": "아 중나 시세 싸네 담에 거기서 구해야겠당ㅋ",
          "speaker_id": "1",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100047303.1.7",
          "form": "단체 챗방 있는데 이종사촌동생까지 있음 우리는 함께 놀았거든",
          "original_form": "단체 챗방 있는데 이종사촌동생까지 있음 우리는 함께 놀았거든",
          "speaker_id": "1",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100047303.1.8",
          "form": "그것도 올라오면 바로 순삭되는 가격은 아니었음 ㅋㅋ",
          "original_form": "그것도 올라오면 바로 순삭되는 가격은 아니었음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100047303.1.9",
          "form": "어케 저런게 자꾸생기지? 웅성웅성함 난 설명해주기 기차늠ㅋ",
          "original_form": "어케 저런게 자꾸생기지? 웅성웅성함 난 설명해주기 기차늠ㅋ",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100047303.1.10",
          "form": "더우면 순삭할껄",
          "original_form": "더우면 순삭할껄",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100047303.1.11",
          "form": "음 그래도 썩 올라가지않음",
          "original_form": "음 그래도 썩 올라가지않음",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100047303.1.12",
          "form": "보통 그정도 가격 형성되면 거기맞춰올림",
          "original_form": "보통 그정도 가격 형성되면 거기맞춰올림",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100047303.1.13",
          "form": "아..중고나라 말고 당근은?",
          "original_form": "아..중고나라 말고 당근은?",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100047303.1.14",
          "form": "그래서 당근에만 올림",
          "original_form": "그래서 당근에만 올림",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100047303.1.15",
          "form": "난 중고나라 싫엉ㅜ 신뢰가 안감ㅜㅜ",
          "original_form": "난 중고나라 싫엉ㅜ 신뢰가 안감ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100047303.1.16",
          "form": "3500 이랑 7000에",
          "original_form": "3500 이랑 7000에",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100047303.1.17",
          "form": "암튼 돈벌었구만 ㅊㅋㅈㅋ",
          "original_form": "암튼 돈벌었구만 ㅊㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100047303.1.18",
          "form": "근데 거기도 3400에 안팔리고있는거가 있었음",
          "original_form": "근데 거기도 3400에 안팔리고있는거가 있었음",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100047303.1.19",
          "form": "그말인 즉 내가 3300 으로 올려야한다는 말임",
          "original_form": "그말인 즉 내가 3300 으로 올려야한다는 말임",
          "speaker_id": "2",
          "time": "20210826 10:15"
        },
        {
          "id": "MDRW2100047303.1.20",
          "form": "근데 연락온 사람은 3200원함",
          "original_form": "근데 연락온 사람은 3200원함",
          "speaker_id": "2",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100047303.1.21",
          "form": "그래서 그사람한테 둘다 데려가면 10000해준다함",
          "original_form": "그래서 그사람한테 둘다 데려가면 10000해준다함",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100047303.1.22",
          "form": "이해함? ㅋㅋ",
          "original_form": "이해함? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100047303.1.23",
          "form": "오 확내림",
          "original_form": "오 확내림",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100047303.1.24",
          "form": "골치아픈걸 치워주만 파격할인 가능한분이시군요",
          "original_form": "골치아픈걸 치워주만 파격할인 가능한분이시군요",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100047303.1.25",
          "form": "파격할인해주는 당신은 천사!",
          "original_form": "파격할인해주는 당신은 천사!",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100047303.1.26",
          "form": "머리아픈거 한번에 해결했네",
          "original_form": "머리아픈거 한번에 해결했네",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100047303.1.27",
          "form": "사람 심리가 올려놓음 얼른 팔고 싶어함",
          "original_form": "사람 심리가 올려놓음 얼른 팔고 싶어함",
          "speaker_id": "2",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100047303.1.28",
          "form": "근데 팔고나면 후회함",
          "original_form": "근데 팔고나면 후회함",
          "speaker_id": "2",
