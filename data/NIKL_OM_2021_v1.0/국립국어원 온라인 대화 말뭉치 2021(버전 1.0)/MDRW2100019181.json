      "utterance": [
        {
          "id": "MDRW2100019181.1.1",
          "form": "서일페는 좋았어?",
          "original_form": "서일페는 좋았어?",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019181.1.2",
          "form": "응 천국이었지",
          "original_form": "응 천국이었지",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019181.1.3",
          "form": "나는 지갑전사가 되어서 돌아왔다",
          "original_form": "나는 지갑전사가 되어서 돌아왔다",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019181.1.4",
          "form": "지갑전사ㅋㅋㅋ",
          "original_form": "지갑전사ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019181.1.5",
          "form": "전시회가서 많이 샀너",
          "original_form": "전시회가서 많이 샀너",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019181.1.6",
          "form": "같이간 친구가 그만사라고 말리더라..",
          "original_form": "같이간 친구가 그만사라고 말리더라..",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019181.1.7",
          "form": "그정도로ㅋㅋㅋ",
          "original_form": "그정도로ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019181.1.8",
          "form": "전시회 사전예약해서 갓음?",
          "original_form": "전시회 사전예약해서 갓음?",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019181.1.9",
          "form": "아니 참가한 지인있어서",
          "original_form": "아니 참가한 지인있어서",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019181.1.10",
          "form": "티켓을 고맙게도 얻어서 갔지",
          "original_form": "티켓을 고맙게도 얻어서 갔지",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019181.1.11",
          "form": "아 초대권 오",
          "original_form": "아 초대권 오",
          "speaker_id": "1",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019181.1.12",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019181.1.13",
          "form": "고마워서 쿠키도 챙겨가고 생품도 사고왔지",
          "original_form": "고마워서 쿠키도 챙겨가고 생품도 사고왔지",
          "speaker_id": "2",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019181.1.14",
          "form": "상품!",
          "original_form": "상품!",
          "speaker_id": "2",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019181.1.15",
          "form": "생품ㅋㅋ",
          "original_form": "생품ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019181.1.16",
          "form": "어디서햇어?",
          "original_form": "어디서햇어?",
          "speaker_id": "1",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019181.1.17",
          "form": "코엑스에서 했지",
          "original_form": "코엑스에서 했지",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019181.1.18",
          "form": "넓고 좋더라",
          "original_form": "넓고 좋더라",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019181.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100019181.1.20",
          "form": "코엑스에서 그런거 하는거 좋은듯ㅋㅋ",
          "original_form": "코엑스에서 그런거 하는거 좋은듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100019181.1.21",
          "form": "스타필드도 있어서 가보고싶었는데 체력이안댐..ㅜ",
          "original_form": "스타필드도 있어서 가보고싶었는데 체력이안댐..ㅜ",
          "speaker_id": "2",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100019181.1.22",
          "form": "늙어서 체력이 ㅋㅋㅋㅋㅋ",
          "original_form": "늙어서 체력이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
