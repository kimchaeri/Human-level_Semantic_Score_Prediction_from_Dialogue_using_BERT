      "utterance": [
        {
          "id": "MDRW2100004596.1.1",
          "form": "공연 보러 가신 경험있나요?",
          "original_form": "공연 보러 가신 경험있나요?",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004596.1.2",
          "form": "아주 예전에 있어요",
          "original_form": "아주 예전에 있어요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004596.1.3",
          "form": "요즘 안가네요",
          "original_form": "요즘 안가네요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004596.1.4",
          "form": "저는 지방에 살아서 뮤지컬이랑 콘서트 딱 한 번 가봤네요 ㅎ",
          "original_form": "저는 지방에 살아서 뮤지컬이랑 콘서트 딱 한 번 가봤네요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004596.1.5",
          "form": "뮤지컬이 신세계였어요\r\n수도권 사람들 부럽더라고요",
          "original_form": "뮤지컬이 신세계였어요\r\n수도권 사람들 부럽더라고요",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004596.1.6",
          "form": "뮤지컬 돈의 압박때문에 좀체 손이 안가더라구요 전",
          "original_form": "뮤지컬 돈의 압박때문에 좀체 손이 안가더라구요 전",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004596.1.7",
          "form": "근데 이젠 돈 있어도 가기 껄끄러워져서 좀 후회되네요",
          "original_form": "근데 이젠 돈 있어도 가기 껄끄러워져서 좀 후회되네요",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004596.1.8",
          "form": "맞아요 뮤지컬 참 비싸죠 ㅎ..",
          "original_form": "맞아요 뮤지컬 참 비싸죠 ㅎ..",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004596.1.9",
          "form": "과연 마스크 없이 문화생활 할 수 있는 날이 언제 오려나 싶네유",
          "original_form": "과연 마스크 없이 문화생활 할 수 있는 날이 언제 오려나 싶네유",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004596.1.10",
          "form": "그리고 막 같은 뮤지컬 꽂혀서 여러번 본다고도 해서.. 그렇게 될까봐 무서워서 못보겠어요ㅋㅋ",
          "original_form": "그리고 막 같은 뮤지컬 꽂혀서 여러번 본다고도 해서.. 그렇게 될까봐 무서워서 못보겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004596.1.11",
          "form": "저도 그럴까봐 무서워요 ㅋㅋㅋㅋㅋ\r\n한 번 꽂히면 끝까지 가는 지독한 성격이라...",
          "original_form": "저도 그럴까봐 무서워요 ㅋㅋㅋㅋㅋ\r\n한 번 꽂히면 끝까지 가는 지독한 성격이라...",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004596.1.12",
          "form": "같은 영화 10번까지 본 적 있어요",
          "original_form": "같은 영화 10번까지 본 적 있어요",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004596.1.13",
          "form": "와 대단해요! 무슨 영화였길래? 궁금하네요",
          "original_form": "와 대단해요! 무슨 영화였길래? 궁금하네요",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004596.1.14",
          "form": "대단한 영화는 아닌데\r\n라라랜드요 ㅋㅋㅋㅋ\r\n주인공 상황이랑 제가 힘들었을 때랑 뭔가 동질감이 느껴져서 볼때마다 울어요 ㅠ",
          "original_form": "대단한 영화는 아닌데\r\n라라랜드요 ㅋㅋㅋㅋ\r\n주인공 상황이랑 제가 힘들었을 때랑 뭔가 동질감이 느껴져서 볼때마다 울어요 ㅠ",
          "speaker_id": "1",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004596.1.15",
          "form": "라라랜드는 볼만한것 같아요! 저도 전체를 본건 아닌데 노래는 많이 들었어요",
          "original_form": "라라랜드는 볼만한것 같아요! 저도 전체를 본건 아닌데 노래는 많이 들었어요",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004596.1.16",
          "form": "전 노래도 주기적으로 들어요ㅋㅋㅋ",
          "original_form": "전 노래도 주기적으로 들어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004596.1.17",
          "form": "코로나 끝나면 전시회도 돌고 싶네요,,",
          "original_form": "코로나 끝나면 전시회도 돌고 싶네요,,",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004596.1.18",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004596.1.19",
          "form": "공연 업종에서 일하시는 분들도 힘들겠어요,,",
          "original_form": "공연 업종에서 일하시는 분들도 힘들겠어요,,",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004596.1.20",
          "form": "그렇다고 하더라고요 안타까워요",
          "original_form": "그렇다고 하더라고요 안타까워요",
          "speaker_id": "2",
          "time": "20210821 19:56"
        }
      ]
