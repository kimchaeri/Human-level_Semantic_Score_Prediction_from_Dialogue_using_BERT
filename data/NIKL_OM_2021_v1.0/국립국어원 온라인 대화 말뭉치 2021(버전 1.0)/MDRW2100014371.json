      "utterance": [
        {
          "id": "MDRW2100014371.1.1",
          "form": "오늘 점심 뭐먹었어?",
          "original_form": "오늘 점심 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014371.1.2",
          "form": "나는 김 김치 계란후라이!!    name1는?",
          "original_form": "나는 김 김치 계란후라이!!    &name1&는?",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014371.1.3",
          "form": "나는 짬뽕 먹었어",
          "original_form": "나는 짬뽕 먹었어",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014371.1.4",
          "form": "우리 맛집 가야하는데",
          "original_form": "우리 맛집 가야하는데",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014371.1.5",
          "form": "그러게언제가지... 맛집많이아는데 뚭이가",
          "original_form": "그러게언제가지... 맛집많이아는데 뚭이가",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014371.1.6",
          "form": "짬뽕맛있었겄다ㅜㅜ",
          "original_form": "짬뽕맛있었겄다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014371.1.7",
          "form": "저녁은먹었어?",
          "original_form": "저녁은먹었어?",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014371.1.8",
          "form": "곧 전복죽먹을겨 해원이는",
          "original_form": "곧 전복죽먹을겨 해원이는",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014371.1.9",
          "form": "헐 전복죽",
          "original_form": "헐 전복죽",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014371.1.10",
          "form": "나는 데리버거세트",
          "original_form": "나는 데리버거세트",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014371.1.11",
          "form": "+데리버거2개",
          "original_form": "+데리버거2개",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014371.1.12",
          "form": "ㄱㅋㅈㄱㅈㄱㅈㅋㅈㄱ",
          "original_form": "ㄱㅋㅈㄱㅈㄱㅈㅋㅈㄱ",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014371.1.13",
          "form": "우와 4개??",
          "original_form": "우와 4개??",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014371.1.14",
          "form": "햄버거 먹고 잠들었니 어디갔니",
          "original_form": "햄버거 먹고 잠들었니 어디갔니",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014371.1.15",
          "form": "아니지3개지..!",
          "original_form": "아니지3개지..!",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014371.1.16",
          "form": "나잠들었었다잉..",
          "original_form": "나잠들었었다잉..",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014371.1.17",
          "form": "아 세트구나 난독증",
          "original_form": "아 세트구나 난독증",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014371.1.18",
          "form": "나 전복죽 잘 머금 나도 먹ㄱ고 잠들엇다가 이제일어났어",
          "original_form": "나 전복죽 잘 머금 나도 먹ㄱ고 잠들엇다가 이제일어났어",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014371.1.19",
          "form": "ㅋㄲㅋㅋㅋㅋ둘다잘먹고잘잤네ㅋㅋㅋㅋ",
          "original_form": "ㅋㄲㅋㅋㅋㅋ둘다잘먹고잘잤네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014371.1.20",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014371.1.21",
          "form": "잘했군 잘했어",
          "original_form": "잘했군 잘했어",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014371.1.22",
          "form": "아주그냥잘찌것어ㅋㅋㅋ",
          "original_form": "아주그냥잘찌것어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014371.1.23",
          "form": "내일아침은뭐먹고나갈고냥",
          "original_form": "내일아침은뭐먹고나갈고냥",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014371.1.24",
          "form": "요새도아침에커피사가지구다녀?",
          "original_form": "요새도아침에커피사가지구다녀?",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014371.1.25",
          "form": "회사카페에서 받아먹어ㅠㅠ",
          "original_form": "회사카페에서 받아먹어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014371.1.26",
          "form": "카페인이 노화의 지름길이래 어떡해",
          "original_form": "카페인이 노화의 지름길이래 어떡해",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014371.1.27",
          "form": "해원이 들고가?",
          "original_form": "해원이 들고가?",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014371.1.28",
          "form": "아니 물도안들고다니는걸...ㅋㅋㅋ",
          "original_form": "아니 물도안들고다니는걸...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014371.1.29",
          "form": "노화의지름길이가 헙",
          "original_form": "노화의지름길이가 헙",
