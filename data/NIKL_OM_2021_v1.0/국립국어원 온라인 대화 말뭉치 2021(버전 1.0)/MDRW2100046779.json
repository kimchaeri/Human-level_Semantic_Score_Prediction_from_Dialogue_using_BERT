        },
        {
          "id": "MDRW2100046779.1.2",
          "form": "안녕하세요ㅎ",
          "original_form": "안녕하세요ㅎ",
          "speaker_id": "2",
          "time": "20211104 07:01"
        },
        {
          "id": "MDRW2100046779.1.3",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20211104 07:03"
        },
        {
          "id": "MDRW2100046779.1.4",
          "form": "오늘은 뭐 하시나요!",
          "original_form": "오늘은 뭐 하시나요!",
          "speaker_id": "2",
          "time": "20211104 07:50"
        },
        {
          "id": "MDRW2100046779.1.5",
          "form": "오늘은 쉬어요",
          "original_form": "오늘은 쉬어요",
          "speaker_id": "1",
          "time": "20211104 08:30"
        },
        {
          "id": "MDRW2100046779.1.6",
          "form": "쉬는날이에요",
          "original_form": "쉬는날이에요",
          "speaker_id": "1",
          "time": "20211104 08:30"
        },
        {
          "id": "MDRW2100046779.1.7",
          "form": "와! 부러워요ㅠ",
          "original_form": "와! 부러워요ㅠ",
          "speaker_id": "2",
          "time": "20211104 08:52"
        },
        {
          "id": "MDRW2100046779.1.8",
          "form": "그럼 오늘 특별한 계획있으신가요?",
          "original_form": "그럼 오늘 특별한 계획있으신가요?",
          "speaker_id": "2",
          "time": "20211104 08:52"
        },
        {
          "id": "MDRW2100046779.1.9",
          "form": "저는 쉬는날이면 친구집에 놀러가거든요",
          "original_form": "저는 쉬는날이면 친구집에 놀러가거든요",
          "speaker_id": "2",
          "time": "20211104 08:53"
        },
        {
          "id": "MDRW2100046779.1.10",
          "form": "전 집에서 게임하고 애니보면서 놀아요",
          "original_form": "전 집에서 게임하고 애니보면서 놀아요",
          "speaker_id": "1",
          "time": "20211105 11:22"
        },
        {
          "id": "MDRW2100046779.1.11",
          "form": "사실 친구가 없어요",
          "original_form": "사실 친구가 없어요",
          "speaker_id": "1",
          "time": "20211105 11:22"
        },
        {
          "id": "MDRW2100046779.1.12",
          "form": "헉 혹시 어떤 애니 좋아하세요 저도 애니 자주봐요!",
          "original_form": "헉 혹시 어떤 애니 좋아하세요 저도 애니 자주봐요!",
          "speaker_id": "2",
          "time": "20211105 11:22"
        },
        {
          "id": "MDRW2100046779.1.13",
          "form": "소드아트온라인",
          "original_form": "소드아트온라인",
          "speaker_id": "1",
          "time": "20211105 11:25"
        },
        {
          "id": "MDRW2100046779.1.14",
          "form": "이요",
          "original_form": "이요",
          "speaker_id": "1",
          "time": "20211105 11:25"
        },
        {
          "id": "MDRW2100046779.1.15",
          "form": "",
          "original_form": "{share:info}",
          "speaker_id": "1",
          "time": "20211105 11:25"
        },
        {
          "id": "MDRW2100046779.1.16",
          "form": "나온지 꽤되서 지금보고있는데",
          "original_form": "나온지 꽤되서 지금보고있는데",
          "speaker_id": "1",
          "time": "20211105 11:25"
        },
        {
          "id": "MDRW2100046779.1.17",
          "form": "시간가는줄모르고 봤어요",
          "original_form": "시간가는줄모르고 봤어요",
          "speaker_id": "1",
          "time": "20211105 11:26"
        },
        {
          "id": "MDRW2100046779.1.18",
          "form": "앗 이거 보려고 관심목록에 넣어놨어요",
          "original_form": "앗 이거 보려고 관심목록에 넣어놨어요",
          "speaker_id": "2",
          "time": "20211105 11:26"
        },
        {
          "id": "MDRW2100046779.1.19",
          "form": "작년엔가 한거 아닌가요",
          "original_form": "작년엔가 한거 아닌가요",
          "speaker_id": "2",
          "time": "20211105 11:26"
        },
        {
          "id": "MDRW2100046779.1.20",
          "form": "후기좀만 알려주세요!",
          "original_form": "후기좀만 알려주세요!",
          "speaker_id": "2",
          "time": "20211105 11:26"
        },
        {
          "id": "MDRW2100046779.1.21",
          "form": "시리즈가 많아서",
          "original_form": "시리즈가 많아서",
          "speaker_id": "1",
          "time": "20211105 11:26"
        },
        {
          "id": "MDRW2100046779.1.22",
          "form": "처음시작한건 3~4년 되었어요  후기는 엄청 재미있게 봤어요",
          "original_form": "처음시작한건 3~4년 되었어요  후기는 엄청 재미있게 봤어요",
          "speaker_id": "1",
          "time": "20211105 11:27"
        },
        {
          "id": "MDRW2100046779.1.23",
          "form": "오.. 저도 이번 방학에 못봤던것좀 보려고 했거든요",
          "original_form": "오.. 저도 이번 방학에 못봤던것좀 보려고 했거든요",
          "speaker_id": "2",
          "time": "20211105 11:28"
        },
        {
          "id": "MDRW2100046779.1.24",
          "form": "밀린게 많아서 ㅋㅋㅋ",
          "original_form": "밀린게 많아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 11:28"
        },
        {
          "id": "MDRW2100046779.1.25",
          "form": "방학 부러워요",
