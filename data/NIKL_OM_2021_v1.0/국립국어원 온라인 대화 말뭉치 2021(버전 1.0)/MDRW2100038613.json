      "utterance": [
        {
          "id": "MDRW2100038613.1.1",
          "form": "최근 영화보신거 있으세요??",
          "original_form": "최근 영화보신거 있으세요??",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100038613.1.2",
          "form": "아뇨ㅜㅜ",
          "original_form": "아뇨ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100038613.1.3",
          "form": "극장 못가고 있어요...ㅜㅜ",
          "original_form": "극장 못가고 있어요...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100038613.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100038613.1.5",
          "form": "코로나 터지고는 한번도 못갔네요",
          "original_form": "코로나 터지고는 한번도 못갔네요",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100038613.1.6",
          "form": "넷플로 영화 본거 있으세요??",
          "original_form": "넷플로 영화 본거 있으세요??",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100038613.1.7",
          "form": "넷플은 안써요ㅜㅜ",
          "original_form": "넷플은 안써요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100038613.1.8",
          "form": "최근 티비로 킬러들의 보디가든 구매해서 봤어요ㅋ",
          "original_form": "최근 티비로 킬러들의 보디가든 구매해서 봤어요ㅋ",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100038613.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100038613.1.10",
          "form": "전 최근에 본게",
          "original_form": "전 최근에 본게",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100038613.1.11",
          "form": "리틀포레스트 봤어요",
          "original_form": "리틀포레스트 봤어요",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100038613.1.12",
          "form": "류준열 팬이라 봤는데",
          "original_form": "류준열 팬이라 봤는데",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100038613.1.13",
          "form": "보고나서는 김태리가 매력적이더라구요",
          "original_form": "보고나서는 김태리가 매력적이더라구요",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100038613.1.14",
          "form": "김태리 매력적이죠",
          "original_form": "김태리 매력적이죠",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100038613.1.15",
          "form": "눈도 편해지는 이목구비고ㅎㅎ",
          "original_form": "눈도 편해지는 이목구비고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100038613.1.16",
          "form": "네 ㅎㅎ 목소리도 편해지고",
          "original_form": "네 ㅎㅎ 목소리도 편해지고",
          "speaker_id": "2",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100038613.1.17",
          "form": "승리호도 봤어요",
          "original_form": "승리호도 봤어요",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100038613.1.18",
          "form": "승리호 재밌나요?",
          "original_form": "승리호 재밌나요?",
          "speaker_id": "1",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100038613.1.19",
          "form": "네 괜찮았어요^^",
          "original_form": "네 괜찮았어요^^",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100038613.1.20",
          "form": "뭐 결말은 뻔했지만",
          "original_form": "뭐 결말은 뻔했지만",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100038613.1.21",
          "form": "그래도 볼만 하더라구요^^",
          "original_form": "그래도 볼만 하더라구요^^",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100038613.1.22",
          "form": "보다보면",
          "original_form": "보다보면",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100038613.1.23",
          "form": "결말이 보이죠ㅋㅋ",
          "original_form": "결말이 보이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100038613.1.24",
