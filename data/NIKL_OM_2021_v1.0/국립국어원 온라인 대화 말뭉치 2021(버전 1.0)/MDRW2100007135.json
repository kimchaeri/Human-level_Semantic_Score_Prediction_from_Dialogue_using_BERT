      "utterance": [
        {
          "id": "MDRW2100007135.1.1",
          "form": "운동좋아하세요?",
          "original_form": "운동좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007135.1.2",
          "form": "하아아... 저 진~~~짜 운동싫어걸이었거든요",
          "original_form": "하아아... 저 진~~~짜 운동싫어걸이었거든요",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007135.1.3",
          "form": "근데 그러니까 요즘 몸이 엄청 고장나는게 느껴지더라고요ㅋㅋㅋㅋ",
          "original_form": "근데 그러니까 요즘 몸이 엄청 고장나는게 느껴지더라고요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007135.1.4",
          "form": "그래서... 이번에 필라테스 신청했어요ㅜㅜ",
          "original_form": "그래서... 이번에 필라테스 신청했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007135.1.5",
          "form": "name1님은요? 운동 좋아하시나요?!",
          "original_form": "&name1&님은요? 운동 좋아하시나요?!",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007135.1.6",
          "form": "오 필라테스 좋죠",
          "original_form": "오 필라테스 좋죠",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007135.1.7",
          "form": "전 줌바랑 스피닝 배워봤어요",
          "original_form": "전 줌바랑 스피닝 배워봤어요",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007135.1.8",
          "form": "오 이것저것 하셨었네요!",
          "original_form": "오 이것저것 하셨었네요!",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007135.1.9",
          "form": "스피닝이 뭔가요?! 와 운알못 진짜 처음들어봐요ㅠㅠ!!",
          "original_form": "스피닝이 뭔가요?! 와 운알못 진짜 처음들어봐요ㅠㅠ!!",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007135.1.10",
          "form": "아 고정된 자전거타고 엄청 달리면서 운동하는거예요",
          "original_form": "아 고정된 자전거타고 엄청 달리면서 운동하는거예요",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007135.1.11",
          "form": "신나는음악에 맞춰서",
          "original_form": "신나는음악에 맞춰서",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007135.1.12",
          "form": "히에엑 진짜 힘들 것 같은데",
          "original_form": "히에엑 진짜 힘들 것 같은데",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007135.1.13",
          "form": "체력... 되게 좋으실 것 같아요ㅠㅠㅠ!!",
          "original_form": "체력... 되게 좋으실 것 같아요ㅠㅠㅠ!!",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007135.1.14",
          "form": "엄청 힘든데 신나고 재밌어요",
          "original_form": "엄청 힘든데 신나고 재밌어요",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007135.1.15",
          "form": "줌바도 잼나고",
          "original_form": "줌바도 잼나고",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007135.1.16",
          "form": "아 그러게요 약간 다 뭔가 신나는 느낌의 스포츠들이네요!",
          "original_form": "아 그러게요 약간 다 뭔가 신나는 느낌의 스포츠들이네요!",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007135.1.17",
          "form": "그럼 진짜로 취미활동 느낌으로다가 운동하시는건가요?!",
          "original_form": "그럼 진짜로 취미활동 느낌으로다가 운동하시는건가요?!",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007135.1.18",
          "form": "요즘은 안해요 ㅎ 운동에 취미가없어서 그나마 즐거운걸 찾다보니",
          "original_form": "요즘은 안해요 ㅎ 운동에 취미가없어서 그나마 즐거운걸 찾다보니",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007135.1.19",
          "form": "헉 그러셨구나",
          "original_form": "헉 그러셨구나",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007135.1.20",
          "form": "그래도 진짜 약간... 재밌는데 운동도 잘 되는거 완전 잘 찾으신 것 같아요...!",
          "original_form": "그래도 진짜 약간... 재밌는데 운동도 잘 되는거 완전 잘 찾으신 것 같아요...!",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007135.1.21",
          "form": "저는 그냥 남들 다 한다니까... 얼레벌레 필라테스 신청한거였거든요ㅎㅎㅎ...",
          "original_form": "저는 그냥 남들 다 한다니까... 얼레벌레 필라테스 신청한거였거든요ㅎㅎㅎ...",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007135.1.22",
          "form": "얼마정도 하나요",
          "original_form": "얼마정도 하나요",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007135.1.23",
          "form": "비싸던데",
          "original_form": "비싸던데",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007135.1.24",
          "form": "맞아요 나름 싸게 해보려구 그룹레슨으로 들은건데두",
          "original_form": "맞아요 나름 싸게 해보려구 그룹레슨으로 들은건데두",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007135.1.25",
          "form": "싸봤자 거진 회당 2만원 가량이에요ㅠㅠ",
          "original_form": "싸봤자 거진 회당 2만원 가량이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007135.1.26",
          "form": "그룹이면 6대1-",
          "original_form": "그룹이면 6대1-",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007135.1.27",
          "form": "맞아요 그거!!",
          "original_form": "맞아요 그거!!",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007135.1.28",
          "form": "필라테스가 체형교정도 되고 좋은거같아요",
          "original_form": "필라테스가 체형교정도 되고 좋은거같아요",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007135.1.29",
          "form": "맞아요 약간 이래저래 기대하는 부분이 있긴 해요ㅋㅋㅋㅋ",
          "original_form": "맞아요 약간 이래저래 기대하는 부분이 있긴 해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007135.1.30",
          "form": "때마침 제가 척추층만증도 있다고 해서...",
          "original_form": "때마침 제가 척추층만증도 있다고 해서...",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007135.1.31",
          "form": "근데 정작 하는 사람이 자세불량이라 잘 되고있는건지는 모르겠네요ㅋㅋㅋㅋ",
