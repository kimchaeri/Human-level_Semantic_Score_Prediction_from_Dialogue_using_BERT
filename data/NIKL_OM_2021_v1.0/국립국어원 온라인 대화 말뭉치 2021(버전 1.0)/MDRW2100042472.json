      "utterance": [
        {
          "id": "MDRW2100042472.1.1",
          "form": "name2님은 백신 맞으셨나요???",
          "original_form": "&name2&님은 백신 맞으셨나요???",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042472.1.2",
          "form": "저는아직입니다!  이번주에맞는데 긴장돼요ㅜㅜ",
          "original_form": "저는아직입니다!  이번주에맞는데 긴장돼요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042472.1.3",
          "form": "맞으셧나요!",
          "original_form": "맞으셧나요!",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042472.1.4",
          "form": "네 오늘 맞았어요!!!",
          "original_form": "네 오늘 맞았어요!!!",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042472.1.5",
          "form": "생활에 이상은 없는데 맞은 부위가",
          "original_form": "생활에 이상은 없는데 맞은 부위가",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042472.1.6",
          "form": "아프네요ㅠㅠ 멍든것처럼",
          "original_form": "아프네요ㅠㅠ 멍든것처럼",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042472.1.7",
          "form": "헐..다들되게아프다그래가지구...",
          "original_form": "헐..다들되게아프다그래가지구...",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042472.1.8",
          "form": "두렵습니다 오늘맞으셧다니ㅜㅜ",
          "original_form": "두렵습니다 오늘맞으셧다니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042472.1.9",
          "form": "약준비해두셧나요!",
          "original_form": "약준비해두셧나요!",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042472.1.10",
          "form": "네네 타이레놀요!",
          "original_form": "네네 타이레놀요!",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042472.1.11",
          "form": "다음날 아플수도있다는데ㅜㅠ",
          "original_form": "다음날 아플수도있다는데ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042472.1.12",
          "form": "오늘은 괜찮습니다!!",
          "original_form": "오늘은 괜찮습니다!!",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042472.1.13",
          "form": "일단 그정도라다행입니다ㅜㅜ",
          "original_form": "일단 그정도라다행입니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042472.1.14",
          "form": "제동생은갑자기입맛이떨어졋다며",
          "original_form": "제동생은갑자기입맛이떨어졋다며",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042472.1.15",
          "form": "죽만먹구그랬습니다..",
          "original_form": "죽만먹구그랬습니다..",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042472.1.16",
          "form": "헐 안돼",
          "original_form": "헐 안돼",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100042472.1.17",
          "form": "입맛떨어지면 정말아픈건데...",
          "original_form": "입맛떨어지면 정말아픈건데...",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100042472.1.18",
          "form": "전 오늘도 입맛이 잘돌아서 디저트까지 챙겨먹었답니다(?)",
          "original_form": "전 오늘도 입맛이 잘돌아서 디저트까지 챙겨먹었답니다(?)",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100042472.1.19",
          "form": "ㅋㅋㅋ굉장히좋은현상입니다",
          "original_form": "ㅋㅋㅋ굉장히좋은현상입니다",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042472.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042472.1.21",
          "form": "피곤한와중에 먹는건 잘먹더라고요...저 자신..",
          "original_form": "피곤한와중에 먹는건 잘먹더라고요...저 자신..",
          "speaker_id": "1",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100042472.1.22",
          "form": "ㅋㅋㅋ저도그랫음좋겟어여...",
          "original_form": "ㅋㅋㅋ저도그랫음좋겟어여...",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100042472.1.23",
          "form": "그럴거에요!! 생각보다 괜찮은 사람 많더라고요!!",
          "original_form": "그럴거에요!! 생각보다 괜찮은 사람 많더라고요!!",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100042472.1.24",
          "form": "근데 저도 맞은쪽 팔 근육통이있어서",
          "original_form": "근데 저도 맞은쪽 팔 근육통이있어서",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100042472.1.25",
          "form": "멍든 느낌이었어요",
          "original_form": "멍든 느낌이었어요",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100042472.1.26",
          "form": "아..멍든느낌이라니ㅜㅡㅜ",
          "original_form": "아..멍든느낌이라니ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100042472.1.27",
          "form": "일상생활 지장있을정돈 아니에요!!!",
          "original_form": "일상생활 지장있을정돈 아니에요!!!",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100042472.1.28",
          "form": "정말 거슬리는군 정도",
          "original_form": "정말 거슬리는군 정도",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100042472.1.29",
          "form": "아하 그정도면 모!!!! 참을만하겠슴돠ㅋㅋㅋㄱ",
          "original_form": "아하 그정도면 모!!!! 참을만하겠슴돠ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 18:57"
        }
      ]
    }
