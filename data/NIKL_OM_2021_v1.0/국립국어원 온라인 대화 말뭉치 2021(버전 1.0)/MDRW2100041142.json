      "utterance": [
        {
          "id": "MDRW2100041142.1.1",
          "form": "콩국수 만들어드세요?",
          "original_form": "콩국수 만들어드세요?",
          "speaker_id": "1",
          "time": "20210905 17:55"
        },
        {
          "id": "MDRW2100041142.1.2",
          "form": "엄마가 만들어 주세요",
          "original_form": "엄마가 만들어 주세요",
          "speaker_id": "2",
          "time": "20210905 17:55"
        },
        {
          "id": "MDRW2100041142.1.3",
          "form": "어머님이 요리왕이시네요",
          "original_form": "어머님이 요리왕이시네요",
          "speaker_id": "1",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100041142.1.4",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100041142.1.5",
          "form": "ㅎㅎ 감사합니다",
          "original_form": "ㅎㅎ 감사합니다",
          "speaker_id": "2",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100041142.1.6",
          "form": "요리 유튜브 보시는 거 있어요?",
          "original_form": "요리 유튜브 보시는 거 있어요?",
          "speaker_id": "2",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100041142.1.7",
          "form": "저 백종원,,?ㅋㅋㄱㅋ가끔봐요",
          "original_form": "저 백종원,,?ㅋㅋㄱㅋ가끔봐요",
          "speaker_id": "1",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100041142.1.8",
          "form": "아 백종원 유튜브 구독자 엄청 많더라고요",
          "original_form": "아 백종원 유튜브 구독자 엄청 많더라고요",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100041142.1.9",
          "form": "보시는거 있으세요?",
          "original_form": "보시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100041142.1.10",
          "form": "저는 이름은 기억 안 나는데",
          "original_form": "저는 이름은 기억 안 나는데",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100041142.1.11",
          "form": "요리하는 장면 클로즈업해서 보여주는 채널 자주 봐요",
          "original_form": "요리하는 장면 클로즈업해서 보여주는 채널 자주 봐요",
          "speaker_id": "2",
          "time": "20210905 18:28"
        },
        {
          "id": "MDRW2100041142.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100041142.1.13",
          "form": "국내유튜번가요?",
          "original_form": "국내유튜번가요?",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100041142.1.14",
          "form": "유튜버 라기보다는 채널인 거 같아요 회사채널요",
          "original_form": "유튜버 라기보다는 채널인 거 같아요 회사채널요",
          "speaker_id": "2",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100041142.1.15",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100041142.1.16",
          "form": "따라서만들기도하세요?",
          "original_form": "따라서만들기도하세요?",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100041142.1.17",
          "form": "아뇨 ㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100041142.1.18",
          "form": "아 생각하나났어요",
          "original_form": "아 생각하나났어요",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100041142.1.19",
          "form": "Muggo",
          "original_form": "Muggo",
          "speaker_id": "2",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100041142.1.20",
          "form": "여기재밌어요",
          "original_form": "여기재밌어요",
          "speaker_id": "2",
          "time": "20210905 18:33"
        },
        {
