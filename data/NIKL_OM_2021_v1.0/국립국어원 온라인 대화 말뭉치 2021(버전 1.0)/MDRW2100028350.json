      "utterance": [
        {
          "id": "MDRW2100028350.1.1",
          "form": "학교다닐때 무슨 과목 좋아하셨어요?",
          "original_form": "학교다닐때 무슨 과목 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100028350.1.2",
          "form": "국어 시간을 좋아했어요",
          "original_form": "국어 시간을 좋아했어요",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100028350.1.3",
          "form": "문학소녀셨군요",
          "original_form": "문학소녀셨군요",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100028350.1.4",
          "form": "글을 읽고 쓰는 걸 좋아했지요",
          "original_form": "글을 읽고 쓰는 걸 좋아했지요",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100028350.1.5",
          "form": "어쩐지 말씀을 잘 하시더라고요",
          "original_form": "어쩐지 말씀을 잘 하시더라고요",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100028350.1.6",
          "form": "그리고 국어 선생님이 참 좋은 분이셨어요",
          "original_form": "그리고 국어 선생님이 참 좋은 분이셨어요",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100028350.1.7",
          "form": "학교 선생님들이 중요하죠",
          "original_form": "학교 선생님들이 중요하죠",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100028350.1.8",
          "form": "지금도 한 번씩 생각이 나네요",
          "original_form": "지금도 한 번씩 생각이 나네요",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100028350.1.9",
          "form": "왜 선생님 좋아하면 그 과목 성적이 급상승하고 그러잖아요",
          "original_form": "왜 선생님 좋아하면 그 과목 성적이 급상승하고 그러잖아요",
          "speaker_id": "1",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100028350.1.10",
          "form": "맞아요 우리 때도 그랬어요",
          "original_form": "맞아요 우리 때도 그랬어요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100028350.1.11",
          "form": "정말 좋은 분이셨나 봐요 아직까지 생각이 나는 걸 보면",
          "original_form": "정말 좋은 분이셨나 봐요 아직까지 생각이 나는 걸 보면",
          "speaker_id": "1",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100028350.1.12",
          "form": "선생님 따라 문학가가 되고 싶어 했어요",
          "original_form": "선생님 따라 문학가가 되고 싶어 했어요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100028350.1.13",
          "form": "선생님이 등단한 시인이었거든요",
          "original_form": "선생님이 등단한 시인이었거든요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100028350.1.14",
          "form": "어머 낭만적이에요",
          "original_form": "어머 낭만적이에요",
          "speaker_id": "1",
