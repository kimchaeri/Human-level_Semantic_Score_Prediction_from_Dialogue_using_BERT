      "utterance": [
        {
          "id": "MDRW2100027196.1.1",
          "form": "점심메뉴는",
          "original_form": "점심메뉴는",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027196.1.2",
          "form": "카레에볶음밥",
          "original_form": "카레에볶음밥",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027196.1.3",
          "form": "전복죽입니다",
          "original_form": "전복죽입니다",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027196.1.4",
          "form": "또 감자탕먹나요",
          "original_form": "또 감자탕먹나요",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027196.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.6",
          "form": "엄마가 요리를 잘하세요",
          "original_form": "엄마가 요리를 잘하세요",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.7",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.8",
          "form": "남이해주는 요리가 존맛인데요",
          "original_form": "남이해주는 요리가 존맛인데요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.9",
          "form": "어제 생선, 전복, 장어를 이웃이 주셔서",
          "original_form": "어제 생선, 전복, 장어를 이웃이 주셔서",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.10",
          "form": "좋은 이웃분이네요",
          "original_form": "좋은 이웃분이네요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.11",
          "form": "함께 소진하기로 마음먹엇어요",
          "original_form": "함께 소진하기로 마음먹엇어요",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.12",
          "form": "넘치는정입니다",
          "original_form": "넘치는정입니다",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.13",
          "form": "배낚시 하시는 분이에요",
          "original_form": "배낚시 하시는 분이에요",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.14",
          "form": "생선류 맛있겠네요",
          "original_form": "생선류 맛있겠네요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.15",
          "form": "회먹고싶어요",
          "original_form": "회먹고싶어요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.16",
          "form": "요즘 전어가 철인데",
          "original_form": "요즘 전어가 철인데",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.17",
          "form": "네 소라도 많아요",
          "original_form": "네 소라도 많아요",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.18",
          "form": "오늘 사먹을까봐요",
          "original_form": "오늘 사먹을까봐요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027196.1.19",
          "form": "회는 부산이 좋죠",
          "original_form": "회는 부산이 좋죠",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027196.1.20",
          "form": "돈을 벌었거든요",
          "original_form": "돈을 벌었거든요",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027196.1.21",
          "form": "전어는 세꼬시입니다",
          "original_form": "전어는 세꼬시입니다",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027196.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027196.1.23",
          "form": "세꼬시가 꼬들하고",
          "original_form": "세꼬시가 꼬들하고",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027196.1.24",
          "form": "고소하고 진리죠",
          "original_form": "고소하고 진리죠",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027196.1.25",
          "form": "번거 바로 먹는데 쓰시나요",
          "original_form": "번거 바로 먹는데 쓰시나요",
          "speaker_id": "2",
          "time": "20210902 10:28"
        }
      ]
