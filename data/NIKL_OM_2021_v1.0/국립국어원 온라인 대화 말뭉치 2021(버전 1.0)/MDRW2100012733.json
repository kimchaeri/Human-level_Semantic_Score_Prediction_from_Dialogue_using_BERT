      "utterance": [
        {
          "id": "MDRW2100012733.1.1",
          "form": "경기도 오산에 보면은",
          "original_form": "경기도 오산에 보면은",
          "speaker_id": "1",
          "time": "20210825 00:24"
        },
        {
          "id": "MDRW2100012733.1.2",
          "form": "경기도 오산엔 왜?",
          "original_form": "경기도 오산엔 왜?",
          "speaker_id": "2",
          "time": "20210825 00:24"
        },
        {
          "id": "MDRW2100012733.1.3",
          "form": "거기에 꿈두레 도서관이라고 있어",
          "original_form": "거기에 꿈두레 도서관이라고 있어",
          "speaker_id": "1",
          "time": "20210825 00:24"
        },
        {
          "id": "MDRW2100012733.1.4",
          "form": "도서관은 여기도 많은데",
          "original_form": "도서관은 여기도 많은데",
          "speaker_id": "2",
          "time": "20210825 00:24"
        },
        {
          "id": "MDRW2100012733.1.5",
          "form": "여긴 좀 이색 도서관",
          "original_form": "여긴 좀 이색 도서관",
          "speaker_id": "1",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100012733.1.6",
          "form": "뭐가 틀린데",
          "original_form": "뭐가 틀린데",
          "speaker_id": "2",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100012733.1.7",
          "form": "응 캠팽과 독서를 결합한 독서캠핑",
          "original_form": "응 캠팽과 독서를 결합한 독서캠핑",
          "speaker_id": "1",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100012733.1.8",
          "form": "ㅋㅋ그게뭐야 웃긴다",
          "original_form": "ㅋㅋ그게뭐야 웃긴다",
          "speaker_id": "2",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100012733.1.9",
          "form": "신기하지 근데 무료이긴 한데 퇴소시 소감문 제출",
          "original_form": "신기하지 근데 무료이긴 한데 퇴소시 소감문 제출",
          "speaker_id": "1",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012733.1.10",
          "form": "과제가 있네 ㅜ.ㅜ",
          "original_form": "과제가 있네 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012733.1.11",
          "form": "소감문이 있어서 좀 그렇긴 한데 가볼만은 한거 같에",
          "original_form": "소감문이 있어서 좀 그렇긴 한데 가볼만은 한거 같에",
          "speaker_id": "1",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012733.1.12",
          "form": "흠 이건 생각해봐야겠다",
          "original_form": "흠 이건 생각해봐야겠다",
          "speaker_id": "2",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012733.1.13",
          "form": "왜 애들보고 해라고 하면 되지 교육도 되고",
          "original_form": "왜 애들보고 해라고 하면 되지 교육도 되고",
          "speaker_id": "1",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012733.1.14",
          "form": "맞네 근데 애들이 안하면 내가 해야해",
          "original_form": "맞네 근데 애들이 안하면 내가 해야해",
          "speaker_id": "2",
          "time": "20210825 00:30"
        }
      ]
