      "utterance": [
        {
          "id": "MDRW2100009596.1.1",
          "form": "다른 분들은 진짜 좋으셨거든요",
          "original_form": "다른 분들은 진짜 좋으셨거든요",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009596.1.2",
          "form": "이상한 사람이 드물긴하죠",
          "original_form": "이상한 사람이 드물긴하죠",
          "speaker_id": "1",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009596.1.3",
          "form": "어떻게든 공부 싫어하는 애들도",
          "original_form": "어떻게든 공부 싫어하는 애들도",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009596.1.4",
          "form": "공부 시키려고",
          "original_form": "공부 시키려고",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009596.1.5",
          "form": "대부분 그러지않으니까",
          "original_form": "대부분 그러지않으니까",
          "speaker_id": "1",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009596.1.6",
          "form": "수학 관련 게임 매번 만들어오시고",
          "original_form": "수학 관련 게임 매번 만들어오시고",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009596.1.7",
          "form": "근데 그 반만 그랬어요",
          "original_form": "근데 그 반만 그랬어요",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009596.1.8",
          "form": "다른 반이나 학년 수업 들으면",
          "original_form": "다른 반이나 학년 수업 들으면",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.9",
          "form": "진짜 실습나간 사람도 그 반 아이들도 힘들었겠어요ㅠ",
          "original_form": "진짜 실습나간 사람도 그 반 아이들도 힘들었겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.10",
          "form": "쉬는 시간에 화장실 가서 울었어요",
          "original_form": "쉬는 시간에 화장실 가서 울었어요",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.11",
          "form": "우리반 애들이랑 너무 비교되잖아요",
          "original_form": "우리반 애들이랑 너무 비교되잖아요",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.12",
          "form": "우리반이 불쌍해서",
          "original_form": "우리반이 불쌍해서",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.13",
          "form": "알거같아요ㅠㅠ",
          "original_form": "알거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.14",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.15",
          "form": "애들이 무슨 죄가있겠어요",
          "original_form": "애들이 무슨 죄가있겠어요",
          "speaker_id": "1",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.16",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.17",
          "form": "애들이 고작 10살인데",
          "original_form": "애들이 고작 10살인데",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009596.1.18",
          "form": "그래서 실습 마지막날",
          "original_form": "그래서 실습 마지막날",
          "speaker_id": "2",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.19",
          "form": "발이 안떨어졌겠다.입금",
          "original_form": "발이 안떨어졌겠다.입금",
          "speaker_id": "1",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.20",
          "form": "뭐야 잘못쳤어요",
          "original_form": "뭐야 잘못쳤어요",
          "speaker_id": "1",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.21",
          "form": "실습 담당 선생님께 말씀 드렸어요",
          "original_form": "실습 담당 선생님께 말씀 드렸어요",
          "speaker_id": "2",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.23",
          "form": "뭐라구요",
          "original_form": "뭐라구요",
          "speaker_id": "1",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.24",
          "form": "저희가 실습생으로써",
          "original_form": "저희가 실습생으로써",
          "speaker_id": "2",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.25",
          "form": "서",
          "original_form": "서",
          "speaker_id": "2",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.26",
          "form": "교육을 받지 못했다는 거랑",
          "original_form": "교육을 받지 못했다는 거랑",
          "speaker_id": "2",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009596.1.27",
          "form": "반에 학교 폭력이 심각한데",
          "original_form": "반에 학교 폭력이 심각한데",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.28",
          "form": "해결 의지가 없고",
          "original_form": "해결 의지가 없고",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.29",
          "form": "등등",
          "original_form": "등등",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.30",
          "form": "음.. 잘 말하셨어요",
          "original_form": "음.. 잘 말하셨어요",
          "speaker_id": "1",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.31",
          "form": "근데 실습 담당 선생님이 훨씬 후배시거든요",
          "original_form": "근데 실습 담당 선생님이 훨씬 후배시거든요",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.32",
          "form": "그랬더니 뭐라던가요",
          "original_form": "그랬더니 뭐라던가요",
          "speaker_id": "1",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.33",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.34",
          "form": "그 선생님보다",
          "original_form": "그 선생님보다",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.35",
          "form": "그분이 제 말 자르시더니",
          "original_form": "그분이 제 말 자르시더니",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.36",
          "form": "다시 돌려보내시는 거예요",
          "original_form": "다시 돌려보내시는 거예요",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.37",
          "form": "그래서 아 안 되나",
          "original_form": "그래서 아 안 되나",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.38",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.39",
          "form": "안 바뀌나",
          "original_form": "안 바뀌나",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.40",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "2",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009596.1.41",
          "form": "1시간 뒤에",
          "original_form": "1시간 뒤에",
          "speaker_id": "2",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009596.1.42",
          "form": "조취가 취해지던가요",
          "original_form": "조취가 취해지던가요",
          "speaker_id": "1",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009596.1.43",
          "form": "교장선생님께서 부르셨어요",
          "original_form": "교장선생님께서 부르셨어요",
          "speaker_id": "2",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009596.1.44",
          "form": "실습 담당선생님께서 얼추 들으셨을 때도",
          "original_form": "실습 담당선생님께서 얼추 들으셨을 때도",
          "speaker_id": "2",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009596.1.45",
          "form": "사안이 너무 심각해서",
          "original_form": "사안이 너무 심각해서",
          "speaker_id": "2",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009596.1.46",
          "form": "부장한테 말하고",
          "original_form": "부장한테 말하고",
          "speaker_id": "2",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009596.1.47",
          "form": "부장이 교장한테 말했나봐요",
          "original_form": "부장이 교장한테 말했나봐요",
          "speaker_id": "2",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.48",
          "form": "가서 다 말했어요",
          "original_form": "가서 다 말했어요",
          "speaker_id": "2",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.49",
          "form": "한 3시간?",
          "original_form": "한 3시간?",
          "speaker_id": "2",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.50",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.51",
          "form": "근데 부장선생님이랑",
          "original_form": "근데 부장선생님이랑",
          "speaker_id": "2",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.52",
          "form": "교장선생님껫ㆍㄱ",
          "original_form": "교장선생님껫ㆍㄱ",
          "speaker_id": "2",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.53",
          "form": "께서",
          "original_form": "께서",
          "speaker_id": "2",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.54",
          "form": "우시더라고요",
          "original_form": "우시더라고요",
          "speaker_id": "2",
          "time": "20210823 01:29"
        },
        {
          "id": "MDRW2100009596.1.55",
          "form": "아예 모르고계셨구나",
          "original_form": "아예 모르고계셨구나",
          "speaker_id": "1",
          "time": "20210823 01:29"
        }
      ]
