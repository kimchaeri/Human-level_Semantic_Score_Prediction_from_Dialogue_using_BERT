      "utterance": [
        {
          "id": "MDRW2100014721.1.1",
          "form": "여름에는 음쓰 정말 신경쓰였는데",
          "original_form": "여름에는 음쓰 정말 신경쓰였는데",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014721.1.2",
          "form": "요샌 날이 점점 선선해지네요",
          "original_form": "요샌 날이 점점 선선해지네요",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014721.1.3",
          "form": "그러게요. 일교차가 상당해요",
          "original_form": "그러게요. 일교차가 상당해요",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014721.1.4",
          "form": "비많이오나요",
          "original_form": "비많이오나요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014721.1.5",
          "form": "지금은 비 안 오고 있어요",
          "original_form": "지금은 비 안 오고 있어요",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014721.1.6",
          "form": "저희쪽도요 부산은 홍수났대요",
          "original_form": "저희쪽도요 부산은 홍수났대요",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014721.1.7",
          "form": "엄청 왔나 보더라구요",
          "original_form": "엄청 왔나 보더라구요",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014721.1.8",
          "form": "아무래도 태풍이 왔다보니 규모가 있나봐요",
          "original_form": "아무래도 태풍이 왔다보니 규모가 있나봐요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014721.1.9",
          "form": "그래도 내일은 비 안 올 것 같아요. 드디어!",
          "original_form": "그래도 내일은 비 안 올 것 같아요. 드디어!",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014721.1.10",
          "form": "오 다행이네요 이제 추워지려나ㅜ",
          "original_form": "오 다행이네요 이제 추워지려나ㅜ",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014721.1.11",
          "form": "아직은 두고 봐야 할 것 같아요ㅋㅋ",
          "original_form": "아직은 두고 봐야 할 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014721.1.12",
          "form": "금요일에는 또 비 예보 있더라구요",
          "original_form": "금요일에는 또 비 예보 있더라구요",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014721.1.13",
          "form": "ㅋㅋ 안돼 ㅋㅋ신발젖는게 싫어요ㅜ",
          "original_form": "ㅋㅋ 안돼 ㅋㅋ신발젖는게 싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014721.1.14",
          "form": "맞아요. 그렇다고 샌들 신고 발 젖는 것도 싫구요ㅋㅋ",
          "original_form": "맞아요. 그렇다고 샌들 신고 발 젖는 것도 싫구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014721.1.15",
          "form": "네 점점 추워지면 양말신는데 ㅋㅋ 양말젖음 더 찝찝해요",
          "original_form": "네 점점 추워지면 양말신는데 ㅋㅋ 양말젖음 더 찝찝해요",
          "speaker_id": "1",
          "time": "20210825 20:41"
        }
      ]
