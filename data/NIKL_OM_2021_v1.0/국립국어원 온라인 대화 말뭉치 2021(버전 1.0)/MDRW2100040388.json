        {
          "id": "MDRW2100040388.1.1",
          "form": "아우 자꾸 나쁜말썼다고 채팅이 안 가네요",
          "original_form": "아우 자꾸 나쁜말썼다고 채팅이 안 가네요",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040388.1.2",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040388.1.3",
          "form": "에셈에서 안만들어주니까",
          "original_form": "에셈에서 안만들어주니까",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040388.1.4",
          "form": "엠버 이제 미국돌아갔나...ㅠㅠ",
          "original_form": "엠버 이제 미국돌아갔나...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040388.1.5",
          "form": "엠버가 뮤비 없으면 안될것같다고",
          "original_form": "엠버가 뮤비 없으면 안될것같다고",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040388.1.6",
          "form": "멤버들한테 찍게했댜요",
          "original_form": "멤버들한테 찍게했댜요",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040388.1.7",
          "form": "그래서 막 콘서트하던중이었는데",
          "original_form": "그래서 막 콘서트하던중이었는데",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040388.1.8",
          "form": "카메라 들고 찍었대요 ㅋㅋㅋ ㅋㅋ",
          "original_form": "카메라 들고 찍었대요 ㅋㅋㅋ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040388.1.9",
          "form": "맴찢 ㅜ",
          "original_form": "맴찢 ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040388.1.10",
          "form": "하아 미쳐.........진짜 맴찢이네요 ㅠㅠ",
          "original_form": "하아 미쳐.........진짜 맴찢이네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040388.1.11",
          "form": "아 근데 솔직히 빅토리아가 중국가면서 더 그렇게된것같아서",
          "original_form": "아 근데 솔직히 빅토리아가 중국가면서 더 그렇게된것같아서",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040388.1.12",
          "form": "사실 전 빅토리아가 좀 미워요 ㅠ",
          "original_form": "사실 전 빅토리아가 좀 미워요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100040388.1.13",
          "form": "에프엑스 진짜 좋았는데 왜구랬냐 에스엠 ㅜㅜ",
          "original_form": "에프엑스 진짜 좋았는데 왜구랬냐 에스엠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100040388.1.14",
          "form": "헉 그렇군요 그건 몰랐어요",
          "original_form": "헉 그렇군요 그건 몰랐어요",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100040388.1.15",
          "form": "솔직히 외국인 멤버들",
          "original_form": "솔직히 외국인 멤버들",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040388.1.16",
          "form": "말도많고 탈도 많은데 왜 자꾸 타국에서 데려오는지..",
          "original_form": "말도많고 탈도 많은데 왜 자꾸 타국에서 데려오는지..",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040388.1.17",
          "form": "맨날 루나랑 엠버가 우리 곧 올거라고 희망고문하는데 빅송은 중국가서 돌아오지도않구....",
          "original_form": "맨날 루나랑 엠버가 우리 곧 올거라고 희망고문하는데 빅송은 중국가서 돌아오지도않구....",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040388.1.18",
          "form": "그니까요 !!!!",
          "original_form": "그니까요 !!!!",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040388.1.19",
          "form": "특히 중국멤버들 ㅜㅠ",
          "original_form": "특히 중국멤버들 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040388.1.20",
          "form": "중국은 이제 무슨 덕질금지법도 만들어서",
          "original_form": "중국은 이제 무슨 덕질금지법도 만들어서",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040388.1.21",
          "form": "앨범도 인당 1개씩밖에 못쓰게 한다고 했던 것 같은데",
          "original_form": "앨범도 인당 1개씩밖에 못쓰게 한다고 했던 것 같은데",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100040388.1.22",
          "form": "이참에 한국멤버들로만 냈우면 좋겠네요",
          "original_form": "이참에 한국멤버들로만 냈우면 좋겠네요",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100040388.1.23",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100040388.1.24",
          "form": "인당 한개라니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "인당 한개라니 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100040388.1.25",
          "form": "아니 우리나라 아이돌들 음판 줄어들겠는데요?",
          "original_form": "아니 우리나라 아이돌들 음판 줄어들겠는데요?",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100040388.1.26",
          "form": "요즘 초동 인플레 쩔었는데",
          "original_form": "요즘 초동 인플레 쩔었는데",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100040388.1.27",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100040388.1.28",
          "form": "맨날 중국에서 공구해서",
          "original_form": "맨날 중국에서 공구해서",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100040388.1.29",
          "form": "앨범판매량 엄청 많던데",
          "original_form": "앨범판매량 엄청 많던데",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100040388.1.30",
          "form": "근데진짜 환경오염이긴해요... 앨범 재활용도 안되고 ... 솔직히 포카때매 사는건데",
          "original_form": "근데진짜 환경오염이긴해요... 앨범 재활용도 안되고 ... 솔직히 포카때매 사는건데",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100040388.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100040388.1.32",
          "form": "솔직히 앨범판매량 진짜 의미없다고 생각해요",
