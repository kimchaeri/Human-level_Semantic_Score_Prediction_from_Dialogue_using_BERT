      "utterance": [
        {
          "id": "MDRW2100030670.1.1",
          "form": "소개팅만요",
          "original_form": "소개팅만요",
          "speaker_id": "2",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030670.1.2",
          "form": "저는 미팅 한두번 소개팅 한번",
          "original_form": "저는 미팅 한두번 소개팅 한번",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030670.1.3",
          "form": "미팅은 여럿이 하죠",
          "original_form": "미팅은 여럿이 하죠",
          "speaker_id": "2",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030670.1.4",
          "form": "소개팅에 나온분이 별로라 안 조앗어요",
          "original_form": "소개팅에 나온분이 별로라 안 조앗어요",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030670.1.5",
          "form": "아 넹",
          "original_form": "아 넹",
          "speaker_id": "2",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030670.1.6",
          "form": "네 사대사미팅으로 해봣어요",
          "original_form": "네 사대사미팅으로 해봣어요",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030670.1.7",
          "form": "짝 되셨나요?",
          "original_form": "짝 되셨나요?",
          "speaker_id": "2",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030670.1.8",
          "form": "미팅이 차라리 재밌었는듯",
          "original_form": "미팅이 차라리 재밌었는듯",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030670.1.9",
          "form": "저는 소개팅이 나은거 같아요",
          "original_form": "저는 소개팅이 나은거 같아요",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030670.1.10",
          "form": "음 저는 폭탄처리반으로 갔었어요",
          "original_form": "음 저는 폭탄처리반으로 갔었어요",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030670.1.11",
          "form": "아님 그냥 같이 여럿이 노는게",
          "original_form": "아님 그냥 같이 여럿이 노는게",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030670.1.12",
          "form": "하지만 주체할 수 없는 인기로 쉽지 않았죠",
          "original_form": "하지만 주체할 수 없는 인기로 쉽지 않았죠",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030670.1.13",
          "form": "편한것 같아요",
          "original_form": "편한것 같아요",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030670.1.14",
          "form": "글쿤요 소개팅은 상대가 복불복",
          "original_form": "글쿤요 소개팅은 상대가 복불복",
          "speaker_id": "1",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100030670.1.15",
          "form": "인기가 좀 있으신가봐요",
          "original_form": "인기가 좀 있으신가봐요",
          "speaker_id": "2",
          "time": "20210904 11:15"
        }
