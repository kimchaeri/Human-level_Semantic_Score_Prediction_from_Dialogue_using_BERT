      "utterance": [
        {
          "id": "MDRW2100032626.1.1",
          "form": "여행가고싶은곳 있으세요?",
          "original_form": "여행가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100032626.1.2",
          "form": "해외는 영국이요!",
          "original_form": "해외는 영국이요!",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032626.1.3",
          "form": "국내는 강원도! name1님은?",
          "original_form": "국내는 강원도! &name1&님은?",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032626.1.4",
          "form": "해외는 아무데나 다 가고싶어요",
          "original_form": "해외는 아무데나 다 가고싶어요",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032626.1.5",
          "form": "국내는 제주도!",
          "original_form": "국내는 제주도!",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032626.1.6",
          "form": "영국 가고싶은 이유 있어요?",
          "original_form": "영국 가고싶은 이유 있어요?",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032626.1.7",
          "form": "오오 그러시구나 ㅋㅋㅋ",
          "original_form": "오오 그러시구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032626.1.8",
          "form": "아 저는 축구좋아해서요 ㅎㅎ",
          "original_form": "아 저는 축구좋아해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032626.1.9",
          "form": "경기장 직관 가보고싶어요!",
          "original_form": "경기장 직관 가보고싶어요!",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032626.1.10",
          "form": "아 왠지 축구가 떠오르긴했어요ㅋㅋ",
          "original_form": "아 왠지 축구가 떠오르긴했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032626.1.11",
          "form": "아직 안가보신거에요?",
          "original_form": "아직 안가보신거에요?",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032626.1.12",
          "form": "네네 ㅠㅠ 가려고했는데 코로나가...ㅠㅠ",
          "original_form": "네네 ㅠㅠ 가려고했는데 코로나가...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032626.1.13",
          "form": "해리포터도 좋아하고 친구가 닥터후 좋아해서 같이가기로했는데 그만..ㅠㅋㅋ",
          "original_form": "해리포터도 좋아하고 친구가 닥터후 좋아해서 같이가기로했는데 그만..ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032626.1.14",
          "form": "앗..저도 해리포터 생각하니 가고싶네요ㅋㅋ",
          "original_form": "앗..저도 해리포터 생각하니 가고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100032626.1.15",
          "form": "꼭 코로나끝나고 버터맥주마시러ㅠ가봅시당",
          "original_form": "꼭 코로나끝나고 버터맥주마시러ㅠ가봅시당",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100032626.1.16",
          "form": "좋아요 ㅋㅋㅋㅠㅠ 버터맥주 먹고싶네요 ..",
          "original_form": "좋아요 ㅋㅋㅋㅠㅠ 버터맥주 먹고싶네요 ..",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032626.1.17",
          "form": "제주도 가면 뭐하고싶으세요??",
          "original_form": "제주도 가면 뭐하고싶으세요??",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032626.1.18",
          "form": "바다보면서 쉬고싶어요ㅎㅎ",
          "original_form": "바다보면서 쉬고싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032626.1.19",
          "form": "맛있는것도 많이먹고",
          "original_form": "맛있는것도 많이먹고",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032626.1.20",
          "form": "너무좋죠 ㅠㅠ 바다보이는 카페에서 커피도 먹구..",
          "original_form": "너무좋죠 ㅠㅠ 바다보이는 카페에서 커피도 먹구..",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100032626.1.21",
          "form": "그쵸..관광지가면 뭘 안해도 힐링되는 기분이에요",
          "original_form": "그쵸..관광지가면 뭘 안해도 힐링되는 기분이에요",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032626.1.22",
          "form": "맞아요 ㅋㅋㅋ 그 분위기란게 있잖아여ㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋ 그 분위기란게 있잖아여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032626.1.23",
          "form": "맞아요 그런설렘 느껴본지가언젠지",
          "original_form": "맞아요 그런설렘 느껴본지가언젠지",
          "speaker_id": "1",
          "time": "20210905 23:02"
        }
      ]
    }
