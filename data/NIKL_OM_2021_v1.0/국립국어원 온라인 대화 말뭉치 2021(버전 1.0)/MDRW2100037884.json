      "utterance": [
        {
          "id": "MDRW2100037884.1.1",
          "form": "좋아하는운동 있어요?",
          "original_form": "좋아하는운동 있어요?",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100037884.1.2",
          "form": "네 줌바댄스좋아하고요ㅋㅋ",
          "original_form": "네 줌바댄스좋아하고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037884.1.3",
          "form": "트램플린잼있는것같아요",
          "original_form": "트램플린잼있는것같아요",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037884.1.4",
          "form": "오 트램플린은 어디서해요?",
          "original_form": "오 트램플린은 어디서해요?",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037884.1.5",
          "form": "저도집에 트램폴린 있어요",
          "original_form": "저도집에 트램폴린 있어요",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037884.1.6",
          "form": "센터같은데서요.ㅋ운동하는곳",
          "original_form": "센터같은데서요.ㅋ운동하는곳",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037884.1.7",
          "form": "오오 요즘에는 트램폴린으로 유튜브틀어놓고도 하더라구요",
          "original_form": "오오 요즘에는 트램폴린으로 유튜브틀어놓고도 하더라구요",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037884.1.8",
          "form": "제친구는 그렇게하고있대요",
          "original_form": "제친구는 그렇게하고있대요",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037884.1.9",
          "form": "같이하면 재밌을것같아요",
          "original_form": "같이하면 재밌을것같아요",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037884.1.10",
          "form": "줌바댄스는 어때요?",
          "original_form": "줌바댄스는 어때요?",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037884.1.11",
          "form": "줌바댄스재미있어요ㅋㅋ",
          "original_form": "줌바댄스재미있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037884.1.12",
          "form": "스트레스도 풀리고요ㅋㅋ",
          "original_form": "스트레스도 풀리고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037884.1.13",
          "form": "재밌을것같아요",
          "original_form": "재밌을것같아요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037884.1.14",
          "form": "저는수영좋아하는데 못한지 오래되었어요",
          "original_form": "저는수영좋아하는데 못한지 오래되었어요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037884.1.15",
          "form": "네네 수영좋죠ㅜㅋㅋㅋ",
          "original_form": "네네 수영좋죠ㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037884.1.16",
          "form": "저도 수영배우고싶었는데",
          "original_form": "저도 수영배우고싶었는데",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037884.1.17",
          "form": "못배웠네요, ㅜ",
          "original_form": "못배웠네요, ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037884.1.18",
          "form": "ㅎㅎ 수영좋아요!",
          "original_form": "ㅎㅎ 수영좋아요!",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037884.1.19",
          "form": "저도 완전 못했다가",
          "original_form": "저도 완전 못했다가",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037884.1.20",
          "form": "처음배웠는데",
          "original_form": "처음배웠는데",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037884.1.21",
          "form": "수영할수있으니 삶을 즐기는범위가 늘어나는것 같더라구요",
          "original_form": "수영할수있으니 삶을 즐기는범위가 늘어나는것 같더라구요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100037884.1.22",
          "form": "그렇죠ㅋㅋ",
          "original_form": "그렇죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037884.1.23",
          "form": "수영배우고싶어요,  진짜ㅋㅋ",
          "original_form": "수영배우고싶어요,  진짜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037884.1.24",
          "form": "이시국 끝나면 배워보세요.",
          "original_form": "이시국 끝나면 배워보세요.",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037884.1.25",
          "form": "추천해요! ㅎㅎ",
          "original_form": "추천해요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037884.1.26",
          "form": "네네 코로나끝나면",
          "original_form": "네네 코로나끝나면",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037884.1.27",
          "form": "배워봐야겠어요",
          "original_form": "배워봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037884.1.28",
          "form": "줌바댄스는 몸치도 수강할수있나요??",
          "original_form": "줌바댄스는 몸치도 수강할수있나요??",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100037884.1.29",
          "form": "네 몸치 크게 상관없는 것 같아요. 저도 몸치거든요 ㅋ",
          "original_form": "네 몸치 크게 상관없는 것 같아요. 저도 몸치거든요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 21:59"
        }
      ]
    }
