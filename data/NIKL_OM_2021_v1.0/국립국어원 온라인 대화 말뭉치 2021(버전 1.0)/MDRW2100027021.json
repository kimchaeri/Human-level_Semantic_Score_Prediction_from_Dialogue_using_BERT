        {
          "id": "MDRW2100027021.1.3",
          "form": "네 그쵸",
          "original_form": "네 그쵸",
          "speaker_id": "2",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027021.1.4",
          "form": "집안일 안하고 살순없잖아요",
          "original_form": "집안일 안하고 살순없잖아요",
          "speaker_id": "2",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027021.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027021.1.6",
          "form": "혼자 사시나요? 아님 가족이랑 같이?",
          "original_form": "혼자 사시나요? 아님 가족이랑 같이?",
          "speaker_id": "2",
          "time": "20210902 07:58"
        },
        {
          "id": "MDRW2100027021.1.7",
          "form": "가족이랑요",
          "original_form": "가족이랑요",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027021.1.8",
          "form": "집안일은 하시는 편이신가여?",
          "original_form": "집안일은 하시는 편이신가여?",
          "speaker_id": "2",
          "time": "20210902 08:09"
        },
        {
          "id": "MDRW2100027021.1.9",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210902 08:11"
        },
        {
          "id": "MDRW2100027021.1.10",
          "form": "그쵸 저도 가족 4명이랑 같이 사는데 집안일이 너무 많아요",
          "original_form": "그쵸 저도 가족 4명이랑 같이 사는데 집안일이 너무 많아요",
          "speaker_id": "2",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100027021.1.11",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100027021.1.12",
          "form": "그쵸 몸에 안좋잖아요",
          "original_form": "그쵸 몸에 안좋잖아요",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027021.1.13",
          "form": "심지어 옷에묻으면 색도변하고",
          "original_form": "심지어 옷에묻으면 색도변하고",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027021.1.14",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027021.1.15",
          "form": "저는 분리수거하는 게 너무 힘들더라구요",
          "original_form": "저는 분리수거하는 게 너무 힘들더라구요",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100027021.1.16",
          "form": "가족이 5명이니가 하루안버려도 가득차고",
          "original_form": "가족이 5명이니가 하루안버려도 가득차고",
          "speaker_id": "2",
