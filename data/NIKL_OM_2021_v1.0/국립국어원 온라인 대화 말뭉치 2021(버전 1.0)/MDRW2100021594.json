        {
          "id": "MDRW2100021594.1.2",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021594.1.3",
          "form": "로오히 엘리트 스토리 열렸는데 밀었나요?",
          "original_form": "로오히 엘리트 스토리 열렸는데 밀었나요?",
          "speaker_id": "2",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021594.1.4",
          "form": "안 밀었어요",
          "original_form": "안 밀었어요",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021594.1.5",
          "form": "할 건가요?",
          "original_form": "할 건가요?",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021594.1.6",
          "form": "다시 깔아야해서 아직",
          "original_form": "다시 깔아야해서 아직",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021594.1.7",
          "form": "접으려고했는데 더 할까 고민중이에요",
          "original_form": "접으려고했는데 더 할까 고민중이에요",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021594.1.8",
          "form": "핸드폰에 깔려있으면 그냥 해",
          "original_form": "핸드폰에 깔려있으면 그냥 해",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021594.1.9",
          "form": "지는 안하면서 막 얘기하는군요?",
          "original_form": "지는 안하면서 막 얘기하는군요?",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021594.1.10",
          "form": "저는 지웠자나요",
          "original_form": "저는 지웠자나요",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021594.1.11",
          "form": "다시 할건가요?",
          "original_form": "다시 할건가요?",
          "speaker_id": "1",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021594.1.12",
          "form": "고민이야 쿠킹덤 하는 것도 바빠",
          "original_form": "고민이야 쿠킹덤 하는 것도 바빠",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021594.1.13",
          "form": "그렇군.. 재밌나바여",
          "original_form": "그렇군.. 재밌나바여",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021594.1.14",
          "form": "재미는 모르겠고 작업지시하느라...",
          "original_form": "재미는 모르겠고 작업지시하느라...",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021594.1.15",
          "form": "게임이 아니라 로동이잖아여?",
          "original_form": "게임이 아니라 로동이잖아여?",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021594.1.16",
          "form": "그런것이죠 그래서 노움들이 횃불을 든게 아니겠어",
          "original_form": "그런것이죠 그래서 노움들이 횃불을 든게 아니겠어",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100021594.1.17",
          "form": "다 노조 결성을 위해...",
          "original_form": "다 노조 결성을 위해...",
          "speaker_id": "2",
          "time": "20210828 21:08"
        }
      ]
