      "utterance": [
        {
          "id": "MDRW2100005778.1.1",
          "form": "여행 좋아하세요??",
          "original_form": "여행 좋아하세요??",
          "speaker_id": "2",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005778.1.2",
          "form": "전 지리교육과인데 별로 안 좋아해요",
          "original_form": "전 지리교육과인데 별로 안 좋아해요",
          "speaker_id": "2",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005778.1.3",
          "form": "귀찮아요",
          "original_form": "귀찮아요",
          "speaker_id": "2",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005778.1.4",
          "form": "네 여행 조아하죠..ㅠ",
          "original_form": "네 여행 조아하죠..ㅠ",
          "speaker_id": "1",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005778.1.5",
          "form": "구라치지마세요 놀러가는건 좋을걸",
          "original_form": "구라치지마세요 놀러가는건 좋을걸",
          "speaker_id": "1",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005778.1.6",
          "form": "누가 돈 다 대주고 계획 다 짜주고 몸만간다생각해봐",
          "original_form": "누가 돈 다 대주고 계획 다 짜주고 몸만간다생각해봐",
          "speaker_id": "1",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005778.1.7",
          "form": "그럼 감사합니다...",
          "original_form": "그럼 감사합니다...",
          "speaker_id": "2",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100005778.1.8",
          "form": "계획 짜는 거랑 여행지로 이동하는게 너무 귀찮습니다ㅠㅠ",
          "original_form": "계획 짜는 거랑 여행지로 이동하는게 너무 귀찮습니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100005778.1.9",
          "form": "그리고 올해 출장 다니면서 각 시도를 다 다녀와서 괜찮아요...",
          "original_form": "그리고 올해 출장 다니면서 각 시도를 다 다녀와서 괜찮아요...",
          "speaker_id": "2",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100005778.1.10",
          "form": "해외놀러오세요 ...",
          "original_form": "해외놀러오세요 ...",
          "speaker_id": "1",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005778.1.11",
          "form": "나 대만 또가고싶다",
          "original_form": "나 대만 또가고싶다",
          "speaker_id": "1",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005778.1.12",
          "form": "대만에 가서 뭐가 제일 좋았나요??",
          "original_form": "대만에 가서 뭐가 제일 좋았나요??",
          "speaker_id": "2",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100005778.1.13",
          "form": "좀 습하지 않았음????",
          "original_form": "좀 습하지 않았음????",
          "speaker_id": "2",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100005778.1.14",
          "form": "ㄴㄴ 12월이라 딱 좋앗음",
          "original_form": "ㄴㄴ 12월이라 딱 좋앗음",
          "speaker_id": "1",
          "time": "20210907 13:14"
        },
        {
          "id": "MDRW2100005778.1.15",
          "form": "맨투맨에 경량조끼 쌉가능",
          "original_form": "맨투맨에 경량조끼 쌉가능",
          "speaker_id": "1",
          "time": "20210907 13:14"
        },
        {
          "id": "MDRW2100005778.1.16",
          "form": "오 여행하기 딱 좋은 기후였네",
          "original_form": "오 여행하기 딱 좋은 기후였네",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100005778.1.17",
          "form": "관광 명소 중에는 어디가 가장 좋았음?????",
          "original_form": "관광 명소 중에는 어디가 가장 좋았음?????",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100005778.1.18",
          "form": "어..빅톤 팬미팅장.",
          "original_form": "어..빅톤 팬미팅장.",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005778.1.19",
          "form": "앗 좋은 관광명소였네요^^",
          "original_form": "앗 좋은 관광명소였네요^^",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005778.1.20",
          "form": "아주 멋진 여행경험이네",
          "original_form": "아주 멋진 여행경험이네",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005778.1.21",
          "form": "기념품은 설마 굿즈를 샀나요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "기념품은 설마 굿즈를 샀나요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005778.1.22",
          "form": "기념품은 치약샀는데",
          "original_form": "기념품은 치약샀는데",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005778.1.23",
          "form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005778.1.24",
          "form": "캐리어안들고가서 가방에 넣었단말야",
          "original_form": "캐리어안들고가서 가방에 넣었단말야",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005778.1.25",
          "form": "100미리넘어서 버려졌어",
          "original_form": "100미리넘어서 버려졌어",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005778.1.26",
          "form": "눈물나지",
          "original_form": "눈물나지",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005778.1.27",
          "form": "아......",
          "original_form": "아......",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005778.1.28",
          "form": "앞으로는 수하물 기준을 잘 찾아보도록 해요....",
          "original_form": "앞으로는 수하물 기준을 잘 찾아보도록 해요....",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100005778.1.29",
          "form": "다음에 대만 가면 덕질이 아니라 관광하러 갈것임???",
          "original_form": "다음에 대만 가면 덕질이 아니라 관광하러 갈것임???",
          "speaker_id": "2",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100005778.1.30",
          "form": "알면서도 그랬네...",
          "original_form": "알면서도 그랬네...",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100005778.1.31",
          "form": "물론.. 나 대만너무좋아서 엄마데리고 가려고 했는데  코어쩌구 터진것임",
          "original_form": "물론.. 나 대만너무좋아서 엄마데리고 가려고 했는데  코어쩌구 터진것임",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100005778.1.32",
          "form": "이런... 내년에 꼭 갈 수 있기를...",
          "original_form": "이런... 내년에 꼭 갈 수 있기를...",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100005778.1.33",
