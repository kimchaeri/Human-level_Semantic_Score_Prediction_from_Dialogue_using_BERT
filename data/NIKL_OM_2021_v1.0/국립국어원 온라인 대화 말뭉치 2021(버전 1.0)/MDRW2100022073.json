      "utterance": [
        {
          "id": "MDRW2100022073.1.1",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "2",
          "time": "20210831 01:41"
        },
        {
          "id": "MDRW2100022073.1.2",
          "form": "네..안냥하세요...",
          "original_form": "네..안냥하세요...",
          "speaker_id": "1",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100022073.1.3",
          "form": "평소에 하시는 게임이 잇으세여?",
          "original_form": "평소에 하시는 게임이 잇으세여?",
          "speaker_id": "2",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100022073.1.4",
          "form": "저 쿠킹덤이랑 동물의숲해요 ㅋㅋㅋㅋ",
          "original_form": "저 쿠킹덤이랑 동물의숲해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:44"
        },
        {
          "id": "MDRW2100022073.1.5",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 01:44"
        },
        {
          "id": "MDRW2100022073.1.6",
          "form": "요줌에 하는건없지만 뭔가 하는건 좋아해요 ㅋㅋ\n제일 핫한것만 하시는군요 ㅋㅋㅋ",
          "original_form": "요줌에 하는건없지만 뭔가 하는건 좋아해요 ㅋㅋ\n제일 핫한것만 하시는군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:45"
        },
        {
          "id": "MDRW2100022073.1.7",
          "form": "앗 저 둘도 이제 한물가지 않았나요 ㅋㅋ",
          "original_form": "앗 저 둘도 이제 한물가지 않았나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:46"
        },
        {
          "id": "MDRW2100022073.1.8",
          "form": "무슨게임하세요?",
          "original_form": "무슨게임하세요?",
          "speaker_id": "1",
          "time": "20210831 01:46"
        },
        {
          "id": "MDRW2100022073.1.9",
          "form": "ㅋㅋㅋㅋ 아 그렁가요?? 저는 평소에 농사하고 뭐 짓고 그러는거 좋아해여 ㅋㅋㅋㅋㅋㅋ\n그런류를 골고루하는편이에요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 아 그렁가요?? 저는 평소에 농사하고 뭐 짓고 그러는거 좋아해여 ㅋㅋㅋㅋㅋㅋ\n그런류를 골고루하는편이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:47"
        },
        {
          "id": "MDRW2100022073.1.10",
          "form": "동물의 숲은 모바일 맞죠?? 아니면 스위치로하시는거세여?",
          "original_form": "동물의 숲은 모바일 맞죠?? 아니면 스위치로하시는거세여?",
          "speaker_id": "2",
          "time": "20210831 01:47"
        },
        {
          "id": "MDRW2100022073.1.11",
          "form": "스위치로 해요 ㅋㅋㅋㅋ",
          "original_form": "스위치로 해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100022073.1.12",
          "form": "농사면 예전에",
          "original_form": "농사면 예전에",
          "speaker_id": "1",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100022073.1.13",
          "form": "오..네네",
          "original_form": "오..네네",
          "speaker_id": "2",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100022073.1.14",
          "form": "놀러와마이홈 이런 건가요..?",
          "original_form": "놀러와마이홈 이런 건가요..?",
          "speaker_id": "1",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100022073.1.15",
          "form": "아마 그런 류엿던것같아요 ㅋㅋㅋ",
          "original_form": "아마 그런 류엿던것같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100022073.1.16",
          "form": "모바일 스타듀밸리 이런것도 살짝해보고요 ㅎㅎㅎ\n원래 굴고 짧게 다양하게해요 ㅋㅋㅋ",
          "original_form": "모바일 스타듀밸리 이런것도 살짝해보고요 ㅎㅎㅎ\n원래 굴고 짧게 다양하게해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100022073.1.17",
          "form": "동물의숲은 닌텐도로해야 재밋잖아요 ㅠㅜ",
          "original_form": "동물의숲은 닌텐도로해야 재밋잖아요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 01:49"
        },
        {
          "id": "MDRW2100022073.1.18",
          "form": "ㅋㅋㅋㅋㅋ 그쵸",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸",
          "speaker_id": "1",
          "time": "20210831 01:50"
        },
        {
          "id": "MDRW2100022073.1.19",
          "form": "전 폰으로는 배그도 자주 해요",
          "original_form": "전 폰으로는 배그도 자주 해요",
          "speaker_id": "1",
          "time": "20210831 01:50"
        },
        {
          "id": "MDRW2100022073.1.20",
          "form": "오... 배그... 어려운것도 잘하시네요 ㅋㅋㅋ\n스위치로 보통 다른게임은 안하세여?",
          "original_form": "오... 배그... 어려운것도 잘하시네요 ㅋㅋㅋ\n스위치로 보통 다른게임은 안하세여?",
          "speaker_id": "2",
