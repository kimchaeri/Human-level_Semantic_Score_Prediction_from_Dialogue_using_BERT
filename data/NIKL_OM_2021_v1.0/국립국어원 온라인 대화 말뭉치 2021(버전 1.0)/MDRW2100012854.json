      "utterance": [
        {
          "id": "MDRW2100012854.1.1",
          "form": "내일 일은 안가세요?",
          "original_form": "내일 일은 안가세요?",
          "speaker_id": "2",
          "time": "20210825 02:53"
        },
        {
          "id": "MDRW2100012854.1.2",
          "form": "네 반차내려구요",
          "original_form": "네 반차내려구요",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012854.1.3",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012854.1.4",
          "form": "네 재택근무라",
          "original_form": "네 재택근무라",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012854.1.5",
          "form": "반차내고 가기 편해서 좋아요",
          "original_form": "반차내고 가기 편해서 좋아요",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012854.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012854.1.7",
          "form": "저는 요즘 일을 쉬고 있너서..",
          "original_form": "저는 요즘 일을 쉬고 있너서..",
          "speaker_id": "2",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012854.1.8",
          "form": "제일 좋네요 ㅎㅎㅎ",
          "original_form": "제일 좋네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012854.1.9",
          "form": "몸은 편한데",
          "original_form": "몸은 편한데",
          "speaker_id": "2",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012854.1.10",
          "form": "그래더 돈이 들어와야.",
          "original_form": "그래더 돈이 들어와야.",
          "speaker_id": "2",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012854.1.11",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012854.1.12",
          "form": "일장일단이 있죠 ㅠㅠ",
          "original_form": "일장일단이 있죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012854.1.13",
          "form": "네 조만간 다시 일해야죠",
          "original_form": "네 조만간 다시 일해야죠",
          "speaker_id": "2",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012854.1.14",
          "form": "네 완전 파이팅입니다!!!",
          "original_form": "네 완전 파이팅입니다!!!",
          "speaker_id": "1",
          "time": "20210825 02:55"
        },
        {
          "id": "MDRW2100012854.1.15",
          "form": "왜 반차만 쓰세요?",
          "original_form": "왜 반차만 쓰세요?",
          "speaker_id": "2",
          "time": "20210825 02:56"
        },
        {
          "id": "MDRW2100012854.1.16",
          "form": "연차는 좀 더 몰았다가 장기로 쓰고싶어서요",
          "original_form": "연차는 좀 더 몰았다가 장기로 쓰고싶어서요",
          "speaker_id": "1",
          "time": "20210825 02:56"
        },
        {
          "id": "MDRW2100012854.1.17",
          "form": "연차 모으시는 중이군요 ㅎㅎ",
          "original_form": "연차 모으시는 중이군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:56"
        },
        {
          "id": "MDRW2100012854.1.18",
          "form": "네 벌써 7일 모았어요 ㅋㅋ",
          "original_form": "네 벌써 7일 모았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:56"
        },
        {
          "id": "MDRW2100012854.1.19",
          "form": "좋네요 ㅎㅎ",
          "original_form": "좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:57"
        },
        {
          "id": "MDRW2100012854.1.20",
          "form": "네 아니면 나중에 연차 수당으로 돌려받을 수도 있어요",
          "original_form": "네 아니면 나중에 연차 수당으로 돌려받을 수도 있어요",
          "speaker_id": "1",
          "time": "20210825 02:57"
        },
        {
          "id": "MDRW2100012854.1.21",
          "form": "와 좋은 회사 다니시네요",
          "original_form": "와 좋은 회사 다니시네요",
          "speaker_id": "2",
          "time": "20210825 02:57"
        }
