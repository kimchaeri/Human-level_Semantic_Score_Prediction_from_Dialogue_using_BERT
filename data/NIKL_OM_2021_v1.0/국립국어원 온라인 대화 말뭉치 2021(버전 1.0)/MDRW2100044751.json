      "utterance": [
        {
          "id": "MDRW2100044751.1.1",
          "form": "챙겨보시는거 뭐라고 하셨죠?? 어떤내용이에요??",
          "original_form": "챙겨보시는거 뭐라고 하셨죠?? 어떤내용이에요??",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044751.1.2",
          "form": "환승연애요!!",
          "original_form": "환승연애요!!",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044751.1.3",
          "form": "티빙에서만 하는건데",
          "original_form": "티빙에서만 하는건데",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044751.1.4",
          "form": "일반인들 썸타는거에여ㅋㅋ",
          "original_form": "일반인들 썸타는거에여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044751.1.5",
          "form": "왜 환승이에요? 갈아타나요",
          "original_form": "왜 환승이에요? 갈아타나요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044751.1.6",
          "form": "헤어진 커플들이",
          "original_form": "헤어진 커플들이",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044751.1.7",
          "form": "같이사는거라서?",
          "original_form": "같이사는거라서?",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044751.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044751.1.9",
          "form": "네? 구남친이랑요??",
          "original_form": "네? 구남친이랑요??",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044751.1.10",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044751.1.11",
          "form": "5커플",
          "original_form": "5커플",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044751.1.12",
          "form": "10명이서",
          "original_form": "10명이서",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044751.1.13",
          "form": "ㅋㅋㅋㅋㅋ썸타유",
          "original_form": "ㅋㅋㅋㅋㅋ썸타유",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044751.1.14",
          "form": "헤어졋지만 다시만나고싶은 커플도잇고",
          "original_form": "헤어졋지만 다시만나고싶은 커플도잇고",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044751.1.15",
          "form": "아 ㅋㅋㅋ 갈아타고?",
          "original_form": "아 ㅋㅋㅋ 갈아타고?",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044751.1.16",
          "form": "완전 다른사람이랑 잘되가는 케이스도잇고",
          "original_form": "완전 다른사람이랑 잘되가는 케이스도잇고",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044751.1.17",
          "form": "ㅋㅋㅋㄱ난리에유",
          "original_form": "ㅋㅋㅋㄱ난리에유",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044751.1.18",
          "form": "와 ㅋㅋㅋㅋㅋ 신박하네요",
          "original_form": "와 ㅋㅋㅋㅋㅋ 신박하네요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044751.1.19",
          "form": "전 옛날사람이라 그런가.",
          "original_form": "전 옛날사람이라 그런가.",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044751.1.20",
          "form": "저두여",
          "original_form": "저두여",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044751.1.21",
          "form": "그래서 보게됏어요",
          "original_form": "그래서 보게됏어요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044751.1.22",
          "form": "요새애들 쩌네 헐 대박",
          "original_form": "요새애들 쩌네 헐 대박",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044751.1.23",
          "form": "이럼서ㅋㅋㅋㅋㅋ",
          "original_form": "이럼서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044751.1.24",
          "form": "그러게요 ㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044751.1.25",
          "form": "유튜브는 보는거 있으세요??",
          "original_form": "유튜브는 보는거 있으세요??",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044751.1.26",
          "form": "유툽 뷰티 종종봐요",
          "original_form": "유툽 뷰티 종종봐요",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044751.1.27",
          "form": "유트루님",
          "original_form": "유트루님",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044751.1.28",
          "form": "회사원 채널 이런거요!",
          "original_form": "회사원 채널 이런거요!",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044751.1.29",
          "form": "유트루??",
          "original_form": "유트루??",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044751.1.30",
          "form": "들어본것같은데. 제가 생각하는게 맞나 모르겠네요",
