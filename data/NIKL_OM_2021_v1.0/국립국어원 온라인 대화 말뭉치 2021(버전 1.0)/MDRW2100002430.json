      "utterance": [
        {
          "id": "MDRW2100002430.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 12:07"
        },
        {
          "id": "MDRW2100002430.1.2",
          "form": "안녕하세요 점심 뭐 드세요?",
          "original_form": "안녕하세요 점심 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002430.1.3",
          "form": "저는 돈까스에 쫄면먹었습니다ㅎㅎ",
          "original_form": "저는 돈까스에 쫄면먹었습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002430.1.4",
          "form": "점심메뉴가 무엇인가요ㅣ",
          "original_form": "점심메뉴가 무엇인가요ㅣ",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002430.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002430.1.6",
          "form": "우와 완전 맛있겠어요",
          "original_form": "우와 완전 맛있겠어요",
          "speaker_id": "2",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002430.1.7",
          "form": "저는 콩나물국 먹었어요",
          "original_form": "저는 콩나물국 먹었어요",
          "speaker_id": "2",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002430.1.8",
          "form": "맛있겠네요ㅎㅎㅎ\r\n혹시 매운거잘드시나요ㆍ",
          "original_form": "맛있겠네요ㅎㅎㅎ\r\n혹시 매운거잘드시나요ㆍ",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100002430.1.9",
          "form": "좋아하시나요??",
          "original_form": "좋아하시나요??",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002430.1.10",
          "form": "저는 잘못먹어요ㅎㅎ 엽기떡볶이",
          "original_form": "저는 잘못먹어요ㅎㅎ 엽기떡볶이",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002430.1.11",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002430.1.12",
          "form": "네 저 엽기떡볶이 정말 좋아해요",
          "original_form": "네 저 엽기떡볶이 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002430.1.13",
          "form": "근데 매워서 맨날 착한맛만 먹네요",
          "original_form": "근데 매워서 맨날 착한맛만 먹네요",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002430.1.14",
          "form": "무슨 맛 드세요???",
          "original_form": "무슨 맛 드세요???",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002430.1.15",
          "form": "저도 착한맛으로 먹습니다ㅎㅎ 너무맵더라구요",
          "original_form": "저도 착한맛으로 먹습니다ㅎㅎ 너무맵더라구요",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002430.1.16",
          "form": "좋아하는음식있으신가요?",
          "original_form": "좋아하는음식있으신가요?",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002430.1.17",
          "form": "전 마라탕 아주 좋아해요",
          "original_form": "전 마라탕 아주 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002430.1.18",
          "form": "매운건 못 먹지만 꼭 먹어요",
          "original_form": "매운건 못 먹지만 꼭 먹어요",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002430.1.19",
          "form": "마라탕 어떻게 어떠세요?",
          "original_form": "마라탕 어떻게 어떠세요?",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002430.1.20",
          "form": "아 저 마라탕진짜좋아해요ㅋㅋㅋ",
          "original_form": "아 저 마라탕진짜좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002430.1.21",
          "form": "사리추가뭐하시나요?",
          "original_form": "사리추가뭐하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002430.1.22",
          "form": "저 야채는 필수고 푸주랑 건두부 푸주 다 필수에요 ㅎ",
          "original_form": "저 야채는 필수고 푸주랑 건두부 푸주 다 필수에요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002430.1.23",
          "form": "뭐넣으세요??",
          "original_form": "뭐넣으세요??",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002430.1.24",
          "form": "아 저는 청경채를 필수로꼭넣어요ㅋㅋㅋ",
          "original_form": "아 저는 청경채를 필수로꼭넣어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002430.1.25",
          "form": "면사리는 뭐좋아하시나요?",
          "original_form": "면사리는 뭐좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002430.1.26",
          "form": "옥수수면 아니면 중국당면이요 ㅎ",
          "original_form": "옥수수면 아니면 중국당면이요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002430.1.27",
          "form": "있어야 맛있더라구요",
          "original_form": "있어야 맛있더라구요",
          "speaker_id": "2",
          "time": "20210820 12:34"
        }
      ]
