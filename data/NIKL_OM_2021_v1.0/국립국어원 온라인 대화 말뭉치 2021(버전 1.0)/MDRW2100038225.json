      "utterance": [
        {
          "id": "MDRW2100038225.1.1",
          "form": "글쵸  잘만들었어요",
          "original_form": "글쵸  잘만들었어요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038225.1.2",
          "form": "음료는 커피 많이 먹는거같아요",
          "original_form": "음료는 커피 많이 먹는거같아요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038225.1.3",
          "form": "음료도 좋아하시나요??",
          "original_form": "음료도 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038225.1.4",
          "form": "ㅋㅋ 그러시구나",
          "original_form": "ㅋㅋ 그러시구나",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038225.1.5",
          "form": "저는 라떼 좋아해요",
          "original_form": "저는 라떼 좋아해요",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038225.1.6",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038225.1.7",
          "form": "아메는 맛이 없어요 ㅋ",
          "original_form": "아메는 맛이 없어요 ㅋ",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038225.1.8",
          "form": "스벅에 돌체라떼 좋아해요",
          "original_form": "스벅에 돌체라떼 좋아해요",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038225.1.9",
          "form": "저는 스벅가면 프라푸치노 자주 마셔요",
          "original_form": "저는 스벅가면 프라푸치노 자주 마셔요",
          "speaker_id": "1",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038225.1.10",
          "form": "그럼 에소프레소프라푸치노 드셔보셨어요?",
          "original_form": "그럼 에소프레소프라푸치노 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038225.1.11",
          "form": "네 맛있더라고요",
          "original_form": "네 맛있더라고요",
          "speaker_id": "1",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038225.1.12",
          "form": "그거 맛있어서 좋아해요",
          "original_form": "그거 맛있어서 좋아해요",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038225.1.13",
          "form": "먹어요 ㅎㅎ",
          "original_form": "먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038225.1.14",
          "form": "저도 여름엔 시원하게 에소프라푸지노",
          "original_form": "저도 여름엔 시원하게 에소프라푸지노",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038225.1.15",
          "form": "그 외엔 녹차프라푸치노 먹고 봄에는 슈크림라떼",
          "original_form": "그 외엔 녹차프라푸치노 먹고 봄에는 슈크림라떼",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038225.1.16",
          "form": "휘핑이나 이런거 안들어가서 맛이 깔끔해서 좋더라구요",
          "original_form": "휘핑이나 이런거 안들어가서 맛이 깔끔해서 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038225.1.17",
          "form": "맞아요 깔끔한게 좋을때도 있어서 좋아요",
          "original_form": "맞아요 깔끔한게 좋을때도 있어서 좋아요",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038225.1.18",
          "form": "휘핑도 지점마다 맛 많이 다르더라고요",
          "original_form": "휘핑도 지점마다 맛 많이 다르더라고요",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038225.1.19",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038225.1.20",
          "form": "많이 흔들면 진짜 더 맛있나봐요ㅋㅋ",
          "original_form": "많이 흔들면 진짜 더 맛있나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038225.1.21",
          "form": "같은재료아닌가요?",
          "original_form": "같은재료아닌가요?",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038225.1.22",
          "form": "아 그렇게 하는것으로도 맛이 달라지는군요",
          "original_form": "아 그렇게 하는것으로도 맛이 달라지는군요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038225.1.23",
          "form": "그게 흔들어야 돼서 조금 맛이 달라지더라고요",
          "original_form": "그게 흔들어야 돼서 조금 맛이 달라지더라고요",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038225.1.24",
          "form": "단단하면 더 맛있어요",
          "original_form": "단단하면 더 맛있어요",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038225.1.25",
          "form": "생각도 못했어요",
          "original_form": "생각도 못했어요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100038225.1.26",
          "form": "덕분에 지식추가됐어요 ㅎㅎ",
          "original_form": "덕분에 지식추가됐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:13"
        }
      ]
    }
