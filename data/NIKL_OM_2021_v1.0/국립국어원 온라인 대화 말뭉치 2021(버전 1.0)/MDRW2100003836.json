      "utterance": [
        {
          "id": "MDRW2100003836.1.1",
          "form": "암튼 그래서 남친도 결혼생각 없고",
          "original_form": "암튼 그래서 남친도 결혼생각 없고",
          "speaker_id": "2",
          "time": "20210821 12:44"
        },
        {
          "id": "MDRW2100003836.1.2",
          "form": "저도 없어요",
          "original_form": "저도 없어요",
          "speaker_id": "2",
          "time": "20210821 12:44"
        },
        {
          "id": "MDRW2100003836.1.3",
          "form": "결혼생활이라는게 참 어려운것 같아요",
          "original_form": "결혼생활이라는게 참 어려운것 같아요",
          "speaker_id": "1",
          "time": "20210821 12:44"
        },
        {
          "id": "MDRW2100003836.1.4",
          "form": "한번 가본사람이라 더 없는듯하고",
          "original_form": "한번 가본사람이라 더 없는듯하고",
          "speaker_id": "2",
          "time": "20210821 12:44"
        },
        {
          "id": "MDRW2100003836.1.5",
          "form": "저는 남친얘기 들으니 죽어도 못하겠고",
          "original_form": "저는 남친얘기 들으니 죽어도 못하겠고",
          "speaker_id": "2",
          "time": "20210821 12:44"
        },
        {
          "id": "MDRW2100003836.1.6",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "1",
          "time": "20210821 12:44"
        },
        {
          "id": "MDRW2100003836.1.7",
          "form": "남친은 다정해요?",
          "original_form": "남친은 다정해요?",
          "speaker_id": "1",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.8",
          "form": "아뇨 무뚝뚝해요ㅡㅡ",
          "original_form": "아뇨 무뚝뚝해요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.9",
          "form": "애정표현 하나도 없어요",
          "original_form": "애정표현 하나도 없어요",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.10",
          "form": "앗 ㅠㅠ 맙소사",
          "original_form": "앗 ㅠㅠ 맙소사",
          "speaker_id": "1",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.11",
          "form": "취해야 좀 하는편?",
          "original_form": "취해야 좀 하는편?",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.12",
          "form": "제가 애교가 많아서",
          "original_form": "제가 애교가 많아서",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.13",
          "form": "어떤 면이 좋으세요?",
          "original_form": "어떤 면이 좋으세요?",
          "speaker_id": "1",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.14",
          "form": "그냥 제가 표현하고 앵기고 해요",
          "original_form": "그냥 제가 표현하고 앵기고 해요",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.15",
          "form": "진짜 그게 의문인데",
          "original_form": "진짜 그게 의문인데",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.16",
          "form": "처음엔 얼굴보고 만났는데",
          "original_form": "처음엔 얼굴보고 만났는데",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003836.1.17",
          "form": "아~ 남친이 좋아하시겠다",
          "original_form": "아~ 남친이 좋아하시겠다",
          "speaker_id": "1",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.18",
          "form": "지금은 못생겨보이거든요?",
          "original_form": "지금은 못생겨보이거든요?",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.21",
          "form": "남친이 애정표현하는거 싫어해요ㅡㅡ",
          "original_form": "남친이 애정표현하는거 싫어해요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.22",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "1",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.23",
          "form": "자기가 안하니 제가 하는게 부담스럽나봐요",
          "original_form": "자기가 안하니 제가 하는게 부담스럽나봐요",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.24",
          "form": "모르겠어요ㅋㅋ",
          "original_form": "모르겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.25",
          "form": "보통 남자들은 좋아하잖아요",
          "original_form": "보통 남자들은 좋아하잖아요",
          "speaker_id": "1",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.26",
          "form": "지금은 그냥 인간성? 보고 만나는것같아요",
          "original_form": "지금은 그냥 인간성? 보고 만나는것같아요",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.27",
          "form": "멘탈 하나는 끝내주거든요",
          "original_form": "멘탈 하나는 끝내주거든요",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003836.1.28",
          "form": "장난 많이치는것 빼면 매너도 좋고",
          "original_form": "장난 많이치는것 빼면 매너도 좋고",
          "speaker_id": "2",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003836.1.29",
          "form": "아ㅋㅋ 매너 좋으시구나",
          "original_form": "아ㅋㅋ 매너 좋으시구나",
          "speaker_id": "1",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003836.1.30",
          "form": "그니까요 보통은 애교 좋아하는데 아주 배가불러가지고",
          "original_form": "그니까요 보통은 애교 좋아하는데 아주 배가불러가지고",
          "speaker_id": "2",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003836.1.31",
          "form": "ㅋㅋ 진짜 배가 불렀네요",
          "original_form": "ㅋㅋ 진짜 배가 불렀네요",
          "speaker_id": "1",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003836.1.32",
          "form": "근데 매너가 좋긴한데",
          "original_form": "근데 매너가 좋긴한데",
          "speaker_id": "2",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003836.1.33",
          "form": "장난을 너무쳐서 힘들어요",
          "original_form": "장난을 너무쳐서 힘들어요",
          "speaker_id": "2",
          "time": "20210821 12:47"
        }
      ]
    }
  ]
