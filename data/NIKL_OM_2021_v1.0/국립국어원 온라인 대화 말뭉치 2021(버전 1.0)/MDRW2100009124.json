      "utterance": [
        {
          "id": "MDRW2100009124.1.1",
          "form": "참 비효율적인 취미라는 생각은 들어요",
          "original_form": "참 비효율적인 취미라는 생각은 들어요",
          "speaker_id": "1",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009124.1.2",
          "form": "운동하려고 이거저거 샀는데 그때뿐이라서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "운동하려고 이거저거 샀는데 그때뿐이라서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009124.1.3",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009124.1.4",
          "form": "마음과 몸이 따로놀죠",
          "original_form": "마음과 몸이 따로놀죠",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009124.1.5",
          "form": "링피트, 저스트댄스, 복싱 다 사 봤는데 뭐하나 오래하는게 없더라구요ㅋㅋ",
          "original_form": "링피트, 저스트댄스, 복싱 다 사 봤는데 뭐하나 오래하는게 없더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009124.1.6",
          "form": "저스트댄스 초반만 재밌겠어요",
          "original_form": "저스트댄스 초반만 재밌겠어요",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009124.1.7",
          "form": "흥떨어지면 ㅠ",
          "original_form": "흥떨어지면 ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009124.1.8",
          "form": "맞아요 오래하기가 힘들어요",
          "original_form": "맞아요 오래하기가 힘들어요",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009124.1.9",
          "form": "저스트댄스 해본 적 있으세요?",
          "original_form": "저스트댄스 해본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009124.1.10",
          "form": "보기만 했어요 ㅎ 많이 나오더라고요",
          "original_form": "보기만 했어요 ㅎ 많이 나오더라고요",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009124.1.11",
          "form": "그거 핸드폰으로도 가능하거든요! 기회되면 한번 해보세요~",
          "original_form": "그거 핸드폰으로도 가능하거든요! 기회되면 한번 해보세요~",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009124.1.12",
          "form": "하루에 두곡인가 무료로 할 수 있어요!",
          "original_form": "하루에 두곡인가 무료로 할 수 있어요!",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009124.1.13",
          "form": "오 꿀팁이네요 ㅎ",
          "original_form": "오 꿀팁이네요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009124.1.14",
          "form": "혼자있을때 해봐야겠어요",
          "original_form": "혼자있을때 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009124.1.15",
          "form": "재밌어요! 노래도 주기적으로 업데이트 되거든요",
          "original_form": "재밌어요! 노래도 주기적으로 업데이트 되거든요",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100009124.1.16",
          "form": "폰하고 게임 띄울 노트북이나 컴퓨터만 있으면 돼요",
          "original_form": "폰하고 게임 띄울 노트북이나 컴퓨터만 있으면 돼요",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100009124.1.17",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100009124.1.18",
          "form": "유산소 운동겸 하면 좋더라구요",
          "original_form": "유산소 운동겸 하면 좋더라구요",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100009124.1.19",
          "form": "그러게요 따로바려면 재미없으니까요",
          "original_form": "그러게요 따로바려면 재미없으니까요",
          "speaker_id": "2",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100009124.1.20",
          "form": "심심하실 때 추천드립니다!",
          "original_form": "심심하실 때 추천드립니다!",
          "speaker_id": "1",
          "time": "20210823 17:10"
        }
      ]
