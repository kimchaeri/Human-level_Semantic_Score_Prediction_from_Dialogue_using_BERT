      "utterance": [
        {
          "id": "MDRW2100028458.1.1",
          "form": "최근에 구매하신거 있으세요?!",
          "original_form": "최근에 구매하신거 있으세요?!",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028458.1.2",
          "form": "저... 요즘 쇼핑을 잘 안해서ㅋㅋㅋ큐ㅠㅠㅜ",
          "original_form": "저... 요즘 쇼핑을 잘 안해서ㅋㅋㅋ큐ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028458.1.3",
          "form": "책만 몇 권 산 것 같아요!",
          "original_form": "책만 몇 권 산 것 같아요!",
          "speaker_id": "2",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028458.1.4",
          "form": "오 어떤책사셨어요?!",
          "original_form": "오 어떤책사셨어요?!",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028458.1.5",
          "form": "에세이사셨나요?!",
          "original_form": "에세이사셨나요?!",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028458.1.6",
          "form": "문학 좋아해서 몇 권 샀어요!",
          "original_form": "문학 좋아해서 몇 권 샀어요!",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028458.1.7",
          "form": "name1님은 새로 사신 거 있으세요???",
          "original_form": "&name1&님은 새로 사신 거 있으세요???",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028458.1.8",
          "form": "요즘 갤럭시워치 새로나와서ㅋㅋ큐ㅠㅜ 그거 좀 가지고싶더라구요 전",
          "original_form": "요즘 갤럭시워치 새로나와서ㅋㅋ큐ㅠㅜ 그거 좀 가지고싶더라구요 전",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028458.1.9",
          "form": "저는 발목다쳐서 발목아대샀어요!",
          "original_form": "저는 발목다쳐서 발목아대샀어요!",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028458.1.10",
          "form": "우와 워치 이번거좋아보이더라고요",
          "original_form": "우와 워치 이번거좋아보이더라고요",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028458.1.11",
          "form": "맞아요 당뇨체크 기능 있던데 엄청 신기해요!",
          "original_form": "맞아요 당뇨체크 기능 있던데 엄청 신기해요!",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028458.1.12",
          "form": "친구가샀으면좋겠어요,,",
          "original_form": "친구가샀으면좋겠어요,,",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028458.1.13",
          "form": "인바디한번하게,,,",
          "original_form": "인바디한번하게,,,",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028458.1.14",
          "form": "그게 피를 안 뽑고 체크하는 기능이 구현하기 엄청 어렵다던데 그걸 했다는 얘기를 들었던 것 같아요",
          "original_form": "그게 피를 안 뽑고 체크하는 기능이 구현하기 엄청 어렵다던데 그걸 했다는 얘기를 들었던 것 같아요",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028458.1.15",
          "form": "ㅋㅋㅋ저두... 빌려서 인바디 해보고싶은마음",
          "original_form": "ㅋㅋㅋ저두... 빌려서 인바디 해보고싶은마음",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028458.1.16",
          "form": "와 삼성대단하네요",
          "original_form": "와 삼성대단하네요",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028458.1.17",
          "form": "삼성,,, 주가도 그렇게 관리잘했으면,,,",
          "original_form": "삼성,,, 주가도 그렇게 관리잘했으면,,,",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028458.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ제 생각엔... 오너가 감옥에서 나와버려서....ㅎ.ㅎ.ㅎ....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ제 생각엔... 오너가 감옥에서 나와버려서....ㅎ.ㅎ.ㅎ....",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028458.1.19",
          "form": "ㅋㅋㄱㅋㅋㅋㄱㄱㄱ종종들어갔으면 좋겠어요",
          "original_form": "ㅋㅋㄱㅋㅋㅋㄱㄱㄱ종종들어갔으면 좋겠어요",
          "speaker_id": "1",
