      "utterance": [
        {
          "id": "MDRW2100000535.1.1",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000535.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000535.1.3",
          "form": "카누",
          "original_form": "카누",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000535.1.4",
          "form": "오 저도 좋아해요",
          "original_form": "오 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000535.1.5",
          "form": "달달한거도좋아해여",
          "original_form": "달달한거도좋아해여",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000535.1.6",
          "form": "집에서 간편하게 먹기 넘 좋아요 ㅎㅎ",
          "original_form": "집에서 간편하게 먹기 넘 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000535.1.7",
          "form": "저두 달달한거 좋아요 근데 맥심은 좀 안맞아요 전 ㅋㅋ",
          "original_form": "저두 달달한거 좋아요 근데 맥심은 좀 안맞아요 전 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000535.1.8",
          "form": "저도맥심먹으면배아파서",
          "original_form": "저도맥심먹으면배아파서",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000535.1.9",
          "form": "안맞아용",
          "original_form": "안맞아용",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000535.1.10",
          "form": "오 ㅠㅠ 진짜 안맞나보네용",
          "original_form": "오 ㅠㅠ 진짜 안맞나보네용",
          "speaker_id": "1",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000535.1.11",
          "form": "재택근무 하는데 넘 졸려서 커피를 계속 먹게되네요~",
          "original_form": "재택근무 하는데 넘 졸려서 커피를 계속 먹게되네요~",
          "speaker_id": "1",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000535.1.12",
          "form": "재택부럽네용",
          "original_form": "재택부럽네용",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000535.1.13",
          "form": "ㅎㅎㅎ 좋긴한데 일에 집중이 안되서 큰일이네요,, 카페인 섭취가 필수",
          "original_form": "ㅎㅎㅎ 좋긴한데 일에 집중이 안되서 큰일이네요,, 카페인 섭취가 필수",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000535.1.14",
          "form": "저도커피로연명해요",
          "original_form": "저도커피로연명해요",
          "speaker_id": "2",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000535.1.15",
          "form": "회사에서",
          "original_form": "회사에서",
          "speaker_id": "2",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000535.1.16",
          "form": "에고고 우리 힘내요 ㅠㅠ",
          "original_form": "에고고 우리 힘내요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000535.1.17",
          "form": "직장인의 빛과소금 커피 ,,",
          "original_form": "직장인의 빛과소금 커피 ,,",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000535.1.18",
          "form": "저녁도멀었는데",
          "original_form": "저녁도멀었는데",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000535.1.19",
          "form": "배고프네여",
          "original_form": "배고프네여",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000535.1.20",
          "form": "저두요 .. 이따 맛있는거 드세요!",
          "original_form": "저두요 .. 이따 맛있는거 드세요!",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000535.1.21",
          "form": "고기먹을려구요",
          "original_form": "고기먹을려구요",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000535.1.22",
          "form": "고기 좋아하시나 보네요!",
          "original_form": "고기 좋아하시나 보네요!",
          "speaker_id": "1",
          "time": "20210819 16:47"
        }
      ]
