      "utterance": [
        {
          "id": "MDRW2100007461.1.1",
          "form": "프로그램 하나 잘되면",
          "original_form": "프로그램 하나 잘되면",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007461.1.2",
          "form": "비슷한 프로그램도 너무 많이 나오는 것 같아요",
          "original_form": "비슷한 프로그램도 너무 많이 나오는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007461.1.3",
          "form": "맞아요 따라쟁이 ㅎㅎ\r\n유사상품에 주의하십시오",
          "original_form": "맞아요 따라쟁이 ㅎㅎ\r\n유사상품에 주의하십시오",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007461.1.4",
          "form": "근데 그럼 저희는 볼게 많아지긴 하지만...ㅎㅎ",
          "original_form": "근데 그럼 저희는 볼게 많아지긴 하지만...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007461.1.5",
          "form": "아..그렇긴하네요 ㅎㅎ",
          "original_form": "아..그렇긴하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007461.1.6",
          "form": "그래도 너무 따라하지 말고",
          "original_form": "그래도 너무 따라하지 말고",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007461.1.7",
          "form": "유투브 자주보세요?",
          "original_form": "유투브 자주보세요?",
          "speaker_id": "2",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007461.1.8",
          "form": "더 신박한 포맷 많이 만들어 오면 좋겠어요!",
          "original_form": "더 신박한 포맷 많이 만들어 오면 좋겠어요!",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007461.1.9",
          "form": "네! 저 완전 자주봐요",
          "original_form": "네! 저 완전 자주봐요",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007461.1.10",
          "form": "프리미엄이신가요?",
          "original_form": "프리미엄이신가요?",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007461.1.11",
          "form": "아뇨 저는 그냥 봐요ㅋㅋㅋ",
          "original_form": "아뇨 저는 그냥 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007461.1.12",
          "form": "저도 그냥 봐도 되는데 광고 다볼 여유있다고 ㅎㅎ",
          "original_form": "저도 그냥 봐도 되는데 광고 다볼 여유있다고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007461.1.13",
          "form": "근데 신랑이 인도계정인가? 그걸로하면 싸다고",
          "original_form": "근데 신랑이 인도계정인가? 그걸로하면 싸다고",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007461.1.14",
          "form": "방법을 알아와서 가입했어요",
          "original_form": "방법을 알아와서 가입했어요",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007461.1.15",
          "form": "헉 맞아요 그렇게들 많이 하신다더라구요",
          "original_form": "헉 맞아요 그렇게들 많이 하신다더라구요",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007461.1.16",
          "form": "그러면 얼마정도 나오나요?",
          "original_form": "그러면 얼마정도 나오나요?",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007461.1.17",
          "form": "달러로 청구되던데 2000원정도?",
          "original_form": "달러로 청구되던데 2000원정도?",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007461.1.18",
          "form": "되는거같아요",
          "original_form": "되는거같아요",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007461.1.19",
          "form": "오 완전 저렴하네요!",
          "original_form": "오 완전 저렴하네요!",
          "speaker_id": "1",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007461.1.20",
          "form": "가격 괜찮은 거 같아요",
          "original_form": "가격 괜찮은 거 같아요",
          "speaker_id": "1",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007461.1.21",
          "form": "네 저도 첨엔 듣고 깜놀했어요",
          "original_form": "네 저도 첨엔 듣고 깜놀했어요",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007461.1.22",
          "form": "싸서 허락했어요 ㅎㅎ",
          "original_form": "싸서 허락했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:46"
        }
      ]
