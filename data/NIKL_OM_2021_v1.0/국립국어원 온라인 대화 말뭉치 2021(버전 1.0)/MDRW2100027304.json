      "utterance": [
        {
          "id": "MDRW2100027304.1.1",
          "form": "뭐사고싶어요",
          "original_form": "뭐사고싶어요",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027304.1.2",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027304.1.3",
          "form": "아죠흥청망청 쿠팡에서요즘맨날 둘러보는데",
          "original_form": "아죠흥청망청 쿠팡에서요즘맨날 둘러보는데",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027304.1.4",
          "form": "살것이 뭐가없네요ㅋㅋㅋ",
          "original_form": "살것이 뭐가없네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027304.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ살게 왜 없어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ살게 왜 없어요",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027304.1.6",
          "form": "널리고 널렸다구요",
          "original_form": "널리고 널렸다구요",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027304.1.7",
          "form": "저 탕수육샀잖아요",
          "original_form": "저 탕수육샀잖아요",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027304.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 탕수육을샀다구요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 탕수육을샀다구요?",
          "speaker_id": "2",
          "time": "20210902 12:40"
        },
        {
          "id": "MDRW2100027304.1.9",
          "form": "나지금케이크먹고싶어",
          "original_form": "나지금케이크먹고싶어",
          "speaker_id": "2",
          "time": "20210902 12:40"
        },
        {
          "id": "MDRW2100027304.1.10",
          "form": "ㅋㅋ탕수육 샀어",
          "original_form": "ㅋㅋ탕수육 샀어",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027304.1.11",
          "form": "케이크?  무슨케이크요",
          "original_form": "케이크?  무슨케이크요",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027304.1.12",
          "form": "생크림케이쿠",
          "original_form": "생크림케이쿠",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027304.1.13",
          "form": "딸기왕왕올려진걸로",
          "original_form": "딸기왕왕올려진걸로",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027304.1.14",
          "form": "요즘 딸기가 맛이없어요",
          "original_form": "요즘 딸기가 맛이없어요",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027304.1.15",
          "form": "저는 소고기에 라면 먹고싶어요",
          "original_form": "저는 소고기에 라면 먹고싶어요",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027304.1.16",
          "form": "가는길에 집에마트들려서 사가야겠구만여",
          "original_form": "가는길에 집에마트들려서 사가야겠구만여",
          "speaker_id": "2",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027304.1.17",
          "form": "다른거뭐사고싶어요?",
          "original_form": "다른거뭐사고싶어요?",
          "speaker_id": "2",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027304.1.18",
          "form": "온라인으론 못사지만 집 사고싶어요",
          "original_form": "온라인으론 못사지만 집 사고싶어요",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027304.1.19",
          "form": "요즘 가장가지고싶은게 집이군요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "요즘 가장가지고싶은게 집이군요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027304.1.20",
          "form": "집은 물건이아니에요ㅋㅋㅋ",
          "original_form": "집은 물건이아니에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027304.1.21",
          "form": "뭐사고싶은거 있으세요?",
          "original_form": "뭐사고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027304.1.22",
          "form": "요즘은 다이슨에어랩이가지고싶어여",
          "original_form": "요즘은 다이슨에어랩이가지고싶어여",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027304.1.23",
          "form": "최근에 비싼거를 질러버려서 아무것도못하고있어여ㅜㅜ",
          "original_form": "최근에 비싼거를 질러버려서 아무것도못하고있어여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027304.1.24",
          "form": "그게 뭐에요?????",
          "original_form": "그게 뭐에요?????",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027304.1.25",
          "form": "다이슨에서 나오는건데 드라이기도되고 고데기도되는거에여",
          "original_form": "다이슨에서 나오는건데 드라이기도되고 고데기도되는거에여",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027304.1.26",
          "form": "가격이 후덜덜해요",
          "original_form": "가격이 후덜덜해요",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027304.1.27",
