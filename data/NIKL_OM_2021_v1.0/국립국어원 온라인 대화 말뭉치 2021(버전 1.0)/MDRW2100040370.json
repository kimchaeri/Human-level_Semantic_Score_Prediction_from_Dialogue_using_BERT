      "utterance": [
        {
          "id": "MDRW2100040370.1.1",
          "form": "오..중고생이라 좋아요",
          "original_form": "오..중고생이라 좋아요",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100040370.1.2",
          "form": "루지도 좋아요..한번밖에 안타봐서..",
          "original_form": "루지도 좋아요..한번밖에 안타봐서..",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040370.1.3",
          "form": "가서타야겠어요",
          "original_form": "가서타야겠어요",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040370.1.4",
          "form": "네네 기장에 새로생겨서 가보셔요",
          "original_form": "네네 기장에 새로생겨서 가보셔요",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040370.1.5",
          "form": "스케줄 다 짤수있겠네요",
          "original_form": "스케줄 다 짤수있겠네요",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040370.1.6",
          "form": "대전에 오시면 성심당이라는 빵집이 엄청유명해요",
          "original_form": "대전에 오시면 성심당이라는 빵집이 엄청유명해요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040370.1.7",
          "form": "시내에있는..",
          "original_form": "시내에있는..",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040370.1.8",
          "form": "네네 재밌는 여행 되시길",
          "original_form": "네네 재밌는 여행 되시길",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040370.1.9",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040370.1.10",
          "form": "아아 팥소보로 아닌가요",
          "original_form": "아아 팥소보로 아닌가요",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040370.1.11",
          "form": "또 추천할곳이나 맛집생각나시면 알려주세요",
          "original_form": "또 추천할곳이나 맛집생각나시면 알려주세요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040370.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040370.1.13",
          "form": "전 튀긴거라 갠적으로 별로인데 다들맛있다고",
          "original_form": "전 튀긴거라 갠적으로 별로인데 다들맛있다고",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040370.1.14",
          "form": "계피들어간 쫄낀한빵 맛나요",
          "original_form": "계피들어간 쫄낀한빵 맛나요",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040370.1.15",
          "form": "오오 계피빵 맛있겠어요",
          "original_form": "오오 계피빵 맛있겠어요",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040370.1.16",
          "form": "저는 서울친구들 만나려고 중간지점인 대전에서 본적 있어요 ㅋㅋ",
          "original_form": "저는 서울친구들 만나려고 중간지점인 대전에서 본적 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100040370.1.17",
          "form": "아..그래요?",
          "original_form": "아..그래요?",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100040370.1.18",
          "form": "대전시내는 구심이라 좀 지져분하고 둔산쪽이나 신세계쪽이 깨끗해요",
          "original_form": "대전시내는 구심이라 좀 지져분하고 둔산쪽이나 신세계쪽이 깨끗해요",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040370.1.19",
          "form": "타임월드쪽",
          "original_form": "타임월드쪽",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040370.1.20",
          "form": "아하 그렇구나 요즘은 많이 다르겠네요",
          "original_form": "아하 그렇구나 요즘은 많이 다르겠네요",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100040370.1.21",
          "form": "대전은 딱히 구경거리가없어요",
          "original_form": "대전은 딱히 구경거리가없어요",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100040370.1.22",
          "form": "부산은 바다가있어서 넘좋을것같아요",
          "original_form": "부산은 바다가있어서 넘좋을것같아요",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100040370.1.23",
          "form": "네네 바다 자주 가죠",
          "original_form": "네네 바다 자주 가죠",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040370.1.24",
          "form": "요즘은 광안리에 마스크 안한 사람이 너무 많아서 힘들어요",
          "original_form": "요즘은 광안리에 마스크 안한 사람이 너무 많아서 힘들어요",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040370.1.25",
          "form": "마스크안쓰고다니는사람이있나봐요",
          "original_form": "마스크안쓰고다니는사람이있나봐요",
          "speaker_id": "2",
          "time": "20210904 19:20"
        }
      ]
