      "utterance": [
        {
          "id": "MDRW2100030462.1.1",
          "form": "아니 왓챠팝콘 껍데기는 왜놓고감",
          "original_form": "아니 왓챠팝콘 껍데기는 왜놓고감",
          "speaker_id": "2",
          "time": "20210904 01:31"
        },
        {
          "id": "MDRW2100030462.1.2",
          "form": "아쏴리",
          "original_form": "아쏴리",
          "speaker_id": "1",
          "time": "20210904 01:31"
        },
        {
          "id": "MDRW2100030462.1.3",
          "form": "가져갈게",
          "original_form": "가져갈게",
          "speaker_id": "1",
          "time": "20210904 01:32"
        },
        {
          "id": "MDRW2100030462.1.4",
          "form": "내갤럭시와치오늘와",
          "original_form": "내갤럭시와치오늘와",
          "speaker_id": "1",
          "time": "20210904 01:32"
        },
        {
          "id": "MDRW2100030462.1.5",
          "form": "빨리 와서 구매확정하고 네이버포인트받았으면",
          "original_form": "빨리 와서 구매확정하고 네이버포인트받았으면",
          "speaker_id": "1",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100030462.1.6",
          "form": "시계는 와치 시계는 와치",
          "original_form": "시계는 와치 시계는 와치",
          "speaker_id": "2",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100030462.1.7",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030462.1.8",
          "form": "갤럭시 와치갖고싶다",
          "original_form": "갤럭시 와치갖고싶다",
          "speaker_id": "2",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030462.1.9",
          "form": "머래는거야",
          "original_form": "머래는거야",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030462.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030462.1.11",
          "form": "갤와치로 심전도체크해야지",
          "original_form": "갤와치로 심전도체크해야지",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030462.1.12",
          "form": "화이자 맞아서 심근염오면어떠케",
          "original_form": "화이자 맞아서 심근염오면어떠케",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030462.1.13",
          "form": "근데오늘올줄모르고있다가",
          "original_form": "근데오늘올줄모르고있다가",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030462.1.14",
          "form": "케이스를안시킴",
          "original_form": "케이스를안시킴",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030462.1.15",
          "form": "그거 멀라 버스에서 어떤 남자가 재채기하니까 앞에 취한 아저씨가 시계는 와치 시계는 와치 했다는거",
          "original_form": "그거 멀라 버스에서 어떤 남자가 재채기하니까 앞에 취한 아저씨가 시계는 와치 시계는 와치 했다는거",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030462.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
