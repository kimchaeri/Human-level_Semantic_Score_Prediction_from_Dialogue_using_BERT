      "utterance": [
        {
          "id": "MDRW2100015714.1.1",
          "form": "콜라보레이션 마케팅이라고 아시나요?",
          "original_form": "콜라보레이션 마케팅이라고 아시나요?",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015714.1.2",
          "form": "헐 아니요 궁금합니다",
          "original_form": "헐 아니요 궁금합니다",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015714.1.3",
          "form": "곰표 맥주는 들어보셨어요?",
          "original_form": "곰표 맥주는 들어보셨어요?",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015714.1.4",
          "form": "네 좋아해요 ㅋㅋ",
          "original_form": "네 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015714.1.5",
          "form": "네 그런 것처럼 타깃이 겹치는 두 브랜드가 함께 상품을 만들어서 마케팅하는 거에요",
          "original_form": "네 그런 것처럼 타깃이 겹치는 두 브랜드가 함께 상품을 만들어서 마케팅하는 거에요",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015714.1.6",
          "form": "곰표가 원래 밀가루로 유명했잖아요 ㅎㅎ",
          "original_form": "곰표가 원래 밀가루로 유명했잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015714.1.7",
          "form": "아하 요즘 핫하잖아요",
          "original_form": "아하 요즘 핫하잖아요",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015714.1.8",
          "form": "모나미도 뭐 있던데",
          "original_form": "모나미도 뭐 있던데",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.9",
          "form": "그쵸 전 안먹어봤는데 맛있다고 하더라구요",
          "original_form": "그쵸 전 안먹어봤는데 맛있다고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.10",
          "form": "모나미 논란 있었는데 ㅠㅠㅠ",
          "original_form": "모나미 논란 있었는데 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.11",
          "form": "맞아요 ㅠㅠ 시멘트 브랜드도",
          "original_form": "맞아요 ㅠㅠ 시멘트 브랜드도",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.12",
          "form": "그 식용이랑 헷갈린다고?",
          "original_form": "그 식용이랑 헷갈린다고?",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.13",
          "form": "그래도 모나미랑 삼성에스펜이랑 콜라보한거 예뻤어요",
          "original_form": "그래도 모나미랑 삼성에스펜이랑 콜라보한거 예뻤어요",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.14",
          "form": "맞아요 ㅋㅋㅋ 그 말표 그것도 ㅋㅋ구두약...",
          "original_form": "맞아요 ㅋㅋㅋ 그 말표 그것도 ㅋㅋ구두약...",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.15",
          "form": "아 말표 말표",
          "original_form": "아 말표 말표",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015714.1.16",
          "form": "그런 건 좀 마케팅 역효과인 것 같아요",
          "original_form": "그런 건 좀 마케팅 역효과인 것 같아요",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.17",
          "form": "어린애들은 그럴거같기도해요",
          "original_form": "어린애들은 그럴거같기도해요",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.18",
          "form": "진짜 헷갈리게 만들어놨던데요 ...",
          "original_form": "진짜 헷갈리게 만들어놨던데요 ...",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.19",
          "form": "유성매직인가 그것도 있었던 것 같아요",
          "original_form": "유성매직인가 그것도 있었던 것 같아요",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.20",
          "form": "곰표가 딱 좋았는데 밀가루라서 요 ㅎ",
          "original_form": "곰표가 딱 좋았는데 밀가루라서 요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.21",
          "form": "맞아요 매직",
          "original_form": "맞아요 매직",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.22",
          "form": "곰표맥주 처음엔 거부감 있었는데..밀가루와 맥주 생각나서 ㅋㅋㅋ",
          "original_form": "곰표맥주 처음엔 거부감 있었는데..밀가루와 맥주 생각나서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.23",
          "form": "편의점에서 한번먹었는덕 ㅋㅋ",
          "original_form": "편의점에서 한번먹었는덕 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.24",
          "form": "괜찮더라구요",
          "original_form": "괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.25",
          "form": "근데 그거 망고링고 맛 난다면서요?!",
          "original_form": "근데 그거 망고링고 맛 난다면서요?!",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015714.1.26",
          "form": "망고링고?!를안먹어봐서",
          "original_form": "망고링고?!를안먹어봐서",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015714.1.27",
          "form": "앗 ㅋㅋ 약간 달달한 향과 맛이 난다고 들었어요",
          "original_form": "앗 ㅋㅋ 약간 달달한 향과 맛이 난다고 들었어요",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015714.1.28",
          "form": "맛있어요 ㅋㅋㅋㅋㅋㅋ근데 잘안보이더라구요",
          "original_form": "맛있어요 ㅋㅋㅋㅋㅋㅋ근데 잘안보이더라구요",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015714.1.29",
          "form": "콜라보레이션 마케팅이 왜효과가 큰걸까요?",
          "original_form": "콜라보레이션 마케팅이 왜효과가 큰걸까요?",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015714.1.30",
          "form": "그거야 타켓수요층이 겹쳐서이지 않을까요?",
          "original_form": "그거야 타켓수요층이 겹쳐서이지 않을까요?",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015714.1.31",
          "form": "삼성 이번에 콜라보 했던데요",
          "original_form": "삼성 이번에 콜라보 했던데요",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015714.1.32",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015714.1.33",
          "form": "톰브라운이랑 ㅠㅠ 너무예뻐요 플립폰....",
          "original_form": "톰브라운이랑 ㅠㅠ 너무예뻐요 플립폰....",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015714.1.34",
          "form": "삼성 플립 콜라보도 콜라보레이션 마케팅인가요??",
          "original_form": "삼성 플립 콜라보도 콜라보레이션 마케팅인가요??",
