      "utterance": [
        {
          "id": "MDRW2100023741.1.1",
          "form": "몇명 가족이신가요?",
          "original_form": "몇명 가족이신가요?",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023741.1.2",
          "form": "4인가족 이에요",
          "original_form": "4인가족 이에요",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100023741.1.3",
          "form": "아.. 가족이랑 같이 사시나봐요..",
          "original_form": "아.. 가족이랑 같이 사시나봐요..",
          "speaker_id": "1",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100023741.1.4",
          "form": "같이 살고 있죠",
          "original_form": "같이 살고 있죠",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100023741.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100023741.1.6",
          "form": "전 1인가구에요..",
          "original_form": "전 1인가구에요..",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100023741.1.7",
          "form": "가사는 많이 도와드리나요?",
          "original_form": "가사는 많이 도와드리나요?",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100023741.1.8",
          "form": "그래야하는데.. 많이 참여하진 않고 있어요",
          "original_form": "그래야하는데.. 많이 참여하진 않고 있어요",
          "speaker_id": "2",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100023741.1.9",
          "form": "혼자 사는건 어떤가요",
          "original_form": "혼자 사는건 어떤가요",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023741.1.10",
          "form": "좋죠... 전 외로움도 안타서..",
          "original_form": "좋죠... 전 외로움도 안타서..",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100023741.1.11",
          "form": "잔소리도 엄고.. ㅋㅋㅋㅋㅋ",
          "original_form": "잔소리도 엄고.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100023741.1.12",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023741.1.13",
          "form": "혼자 산지 얼마나 되셨어요?",
          "original_form": "혼자 산지 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023741.1.14",
          "form": "15년? 인가.. 그정도 된듯해요..",
          "original_form": "15년? 인가.. 그정도 된듯해요..",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100023741.1.15",
          "form": "대학가자마자 독립해서",
          "original_form": "대학가자마자 독립해서",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100023741.1.16",
          "form": "와 진짜 빨리 독립하셨네요",
          "original_form": "와 진짜 빨리 독립하셨네요",
          "speaker_id": "2",
          "time": "20210830 16:48"
        },
        {
          "id": "MDRW2100023741.1.17",
          "form": "룸메이트 만들고 싶은 생각은 든 적 없으세요?",
          "original_form": "룸메이트 만들고 싶은 생각은 든 적 없으세요?",
          "speaker_id": "2",
          "time": "20210830 16:49"
        },
        {
          "id": "MDRW2100023741.1.18",
          "form": "아니면 누군가랑 함께 살고 싶다거나",
          "original_form": "아니면 누군가랑 함께 살고 싶다거나",
          "speaker_id": "2",
          "time": "20210830 16:49"
        },
        {
          "id": "MDRW2100023741.1.19",
          "form": "절대요... ㅋㅋㅋㅋ",
          "original_form": "절대요... ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023741.1.20",
          "form": "지금 누구랑 살면 넘 불편할거 같아요",
          "original_form": "지금 누구랑 살면 넘 불편할거 같아요",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023741.1.21",
          "form": "너무 혼자에 익숙해져린 몸.. ㅋㅋ",
          "original_form": "너무 혼자에 익숙해져린 몸.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023741.1.22",
          "form": "오.. 혹시 그럼 결혼 생각도 없으신 거예요?",
          "original_form": "오.. 혹시 그럼 결혼 생각도 없으신 거예요?",
          "speaker_id": "2",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023741.1.23",
          "form": "네.. 전 사람을 못 좋아하는 그렇게 된거 같아여.. ㅋㅋㅋㅋ",
