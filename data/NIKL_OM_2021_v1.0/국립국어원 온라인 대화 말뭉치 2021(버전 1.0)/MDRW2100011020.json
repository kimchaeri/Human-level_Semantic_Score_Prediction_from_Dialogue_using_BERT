      "utterance": [
        {
          "id": "MDRW2100011020.1.1",
          "form": "요즘 뭐 핫한 상품 없니?",
          "original_form": "요즘 뭐 핫한 상품 없니?",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011020.1.2",
          "form": "오늘 지플립마지막 사전예약이었어",
          "original_form": "오늘 지플립마지막 사전예약이었어",
          "speaker_id": "1",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011020.1.3",
          "form": "3분만에 동났지 뭐니",
          "original_form": "3분만에 동났지 뭐니",
          "speaker_id": "1",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011020.1.4",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011020.1.5",
          "form": "아. 지플립은 접히는 휴대폰이야",
          "original_form": "아. 지플립은 접히는 휴대폰이야",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.6",
          "form": "점점 입소문을 타나봐",
          "original_form": "점점 입소문을 타나봐",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.7",
          "form": "지금 사전예액하먄 언제 받을수있대?",
          "original_form": "지금 사전예액하먄 언제 받을수있대?",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.8",
          "form": "플립은 세로로 접히고 폴드는 가로로 접혀",
          "original_form": "플립은 세로로 접히고 폴드는 가로로 접혀",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.9",
          "form": "글쎄... 물량이 적은가봐",
          "original_form": "글쎄... 물량이 적은가봐",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.10",
          "form": "좋은 구별법이야 고마워",
          "original_form": "좋은 구별법이야 고마워",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.11",
          "form": "그냥 많이 만들어 팔았으면해",
          "original_form": "그냥 많이 만들어 팔았으면해",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.12",
          "form": "역시 수요는 많고 공급은 적으면 이런 사달이 나는구나",
          "original_form": "역시 수요는 많고 공급은 적으면 이런 사달이 나는구나",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.13",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.14",
          "form": "그리고 가격이 좀 더 저렴해졌으면 해.",
          "original_form": "그리고 가격이 좀 더 저렴해졌으면 해.",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.15",
          "form": "20만원대면 좋겠어.",
          "original_form": "20만원대면 좋겠어.",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.16",
          "form": "그래도 예전 시리즈보다는 싸졌어",
          "original_form": "그래도 예전 시리즈보다는 싸졌어",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.17",
          "form": "음 그건 너무 도둑놈같아",
          "original_form": "음 그건 너무 도둑놈같아",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.18",
          "form": "맞아. 만족스러워",
          "original_form": "맞아. 만족스러워",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011020.1.19",
          "form": "흠. 그런 거 같아.",
          "original_form": "흠. 그런 거 같아.",
          "speaker_id": "1",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011020.1.20",
          "form": "자중할게!",
          "original_form": "자중할게!",
          "speaker_id": "1",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011020.1.21",
          "form": "빠른 반성을 하는구나",
          "original_form": "빠른 반성을 하는구나",
          "speaker_id": "2",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011020.1.22",
          "form": "역시 지성인의 쇼핑이야",
          "original_form": "역시 지성인의 쇼핑이야",
          "speaker_id": "2",
          "time": "20210823 23:50"
        }
      ]
