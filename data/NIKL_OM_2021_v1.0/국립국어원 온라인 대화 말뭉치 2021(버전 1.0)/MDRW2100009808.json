      "utterance": [
        {
          "id": "MDRW2100009808.1.1",
          "form": "라면 좋아하시나요?",
          "original_form": "라면 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009808.1.2",
          "form": "네 라면 오늘같이 흐린날 더 땡기네요",
          "original_form": "네 라면 오늘같이 흐린날 더 땡기네요",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009808.1.3",
          "form": "라명좋아하세요?",
          "original_form": "라명좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009808.1.4",
          "form": "저도 좋아해요!",
          "original_form": "저도 좋아해요!",
          "speaker_id": "1",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009808.1.5",
          "form": "저는 열라면을 좋아하는데 name2님은 어떤 라면 좋아하시나요",
          "original_form": "저는 열라면을 좋아하는데 &name2&님은 어떤 라면 좋아하시나요",
          "speaker_id": "1",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009808.1.6",
          "form": "저는 짬뽕라면도 좋아하구 불닭볶음면 좋아해요ㅎㅎ",
          "original_form": "저는 짬뽕라면도 좋아하구 불닭볶음면 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009808.1.7",
          "form": "불닭볶음면 완전 맛있죠!",
          "original_form": "불닭볶음면 완전 맛있죠!",
          "speaker_id": "1",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009808.1.8",
          "form": "불닭볶음면에 추가해서 먹는게 있나요?",
          "original_form": "불닭볶음면에 추가해서 먹는게 있나요?",
          "speaker_id": "1",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009808.1.9",
          "form": "네 오늘 먹어야겠네요ㅎㅎ 열라면도 좋아해요 맛있게 매운맛ㅋㅋ",
          "original_form": "네 오늘 먹어야겠네요ㅎㅎ 열라면도 좋아해요 맛있게 매운맛ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009808.1.10",
          "form": "저는 딱 치즈만ㅎㅎ",
          "original_form": "저는 딱 치즈만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009808.1.11",
          "form": "치즈는 모짜렐라? 체다?",
          "original_form": "치즈는 모짜렐라? 체다?",
          "speaker_id": "1",
          "time": "20210823 10:13"
        },
        {
          "id": "MDRW2100009808.1.12",
          "form": "저는 모짜렐라가 좋더라구요ㅎㅎ",
          "original_form": "저는 모짜렐라가 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:13"
        },
        {
          "id": "MDRW2100009808.1.13",
          "form": "역시 불닭은 모짜렐라죠ㅎ",
          "original_form": "역시 불닭은 모짜렐라죠ㅎ",
          "speaker_id": "1",
          "time": "20210823 10:13"
        },
        {
          "id": "MDRW2100009808.1.14",
          "form": "뭔가 매운맛도 잡아주고 더 맛있어지는 거 같아요",
          "original_form": "뭔가 매운맛도 잡아주고 더 맛있어지는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 10:13"
        },
        {
          "id": "MDRW2100009808.1.15",
          "form": "맞아요 고소하구ㅎㅎ",
          "original_form": "맞아요 고소하구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:13"
        },
        {
          "id": "MDRW2100009808.1.16",
          "form": "국물라면에는 계란을 넣어드시나요?",
          "original_form": "국물라면에는 계란을 넣어드시나요?",
          "speaker_id": "1",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009808.1.17",
          "form": "저는 계란넣어먹는걸 좋아해요",
          "original_form": "저는 계란넣어먹는걸 좋아해요",
          "speaker_id": "1",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009808.1.18",
          "form": "아 저는 넣는데 풀지는 않구ㅎㅎ",
          "original_form": "아 저는 넣는데 풀지는 않구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009808.1.19",
          "form": "탁한걸 안좋아하시는구나",
          "original_form": "탁한걸 안좋아하시는구나",
          "speaker_id": "1",
          "time": "20210823 10:14"
        }
