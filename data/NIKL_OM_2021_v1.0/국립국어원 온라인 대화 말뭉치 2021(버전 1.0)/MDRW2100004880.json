      "utterance": [
        {
          "id": "MDRW2100004880.1.1",
          "form": "고양이 저 너무너무 좋아해요 ㅠ",
          "original_form": "고양이 저 너무너무 좋아해요 ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004880.1.2",
          "form": "고양이 너무 좋아 ㅠㅠㅠㅠㅠ",
          "original_form": "고양이 너무 좋아 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004880.1.3",
          "form": "나만 고양이 없어!",
          "original_form": "나만 고양이 없어!",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004880.1.4",
          "form": "너무 좋은 주제에욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 좋은 주제에욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004880.1.5",
          "form": "나는 고양이 있지롱!",
          "original_form": "나는 고양이 있지롱!",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004880.1.6",
          "form": "으로 받아치겠습니다ㅎㅎ",
          "original_form": "으로 받아치겠습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004880.1.7",
          "form": "그럼 반려동물을 기르신 적은 있으신가요",
          "original_form": "그럼 반려동물을 기르신 적은 있으신가요",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004880.1.8",
          "form": "있으시다면 자랑 함 해주시죠",
          "original_form": "있으시다면 자랑 함 해주시죠",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004880.1.9",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004880.1.10",
          "form": "집사에요?",
          "original_form": "집사에요?",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004880.1.11",
          "form": "와 부러워요 ㅠㅠㅠㅠ",
          "original_form": "와 부러워요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004880.1.12",
          "form": "저는 부모님이 말씀하시길 동물을 데려온다면",
          "original_form": "저는 부모님이 말씀하시길 동물을 데려온다면",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004880.1.13",
          "form": "너를 내다 버리겠다고 협박하셔서",
          "original_form": "너를 내다 버리겠다고 협박하셔서",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004880.1.14",
          "form": "키울 수 없었습니다...",
          "original_form": "키울 수 없었습니다...",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004880.1.15",
          "form": "어릴 때 키운 강아지 정도?",
          "original_form": "어릴 때 키운 강아지 정도?",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004880.1.16",
          "form": "이제 3년차 접어드는 집사예요ㅎ",
          "original_form": "이제 3년차 접어드는 집사예요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004880.1.17",
          "form": "아이고 제가 잘못했습니다",
          "original_form": "아이고 제가 잘못했습니다",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004880.1.18",
          "form": "강아지 되게 귀여웠겠어요ㅠㅠ",
          "original_form": "강아지 되게 귀여웠겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004880.1.19",
          "form": "너무 부러워요 ㅠ 고양이님 자랑 좀 해주세요",
          "original_form": "너무 부러워요 ㅠ 고양이님 자랑 좀 해주세요",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004880.1.20",
          "form": "2살 된 코숏이구 수컷이에요!",
          "original_form": "2살 된 코숏이구 수컷이에요!",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004880.1.21",
          "form": "강아지 귀여웠는데 너무 잠깐 키워서 기억이 많이 없어요ㅠㅠ",
          "original_form": "강아지 귀여웠는데 너무 잠깐 키워서 기억이 많이 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004880.1.22",
          "form": "이름은 name3구요!!",
          "original_form": "이름은 &name3&구요!!",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004880.1.23",
