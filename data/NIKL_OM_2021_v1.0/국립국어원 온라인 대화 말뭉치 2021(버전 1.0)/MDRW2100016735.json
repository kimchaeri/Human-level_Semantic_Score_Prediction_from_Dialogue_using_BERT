      "utterance": [
        {
          "id": "MDRW2100016735.1.1",
          "form": "맞아요 맛있는거 엄청 많았어요",
          "original_form": "맞아요 맛있는거 엄청 많았어요",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016735.1.2",
          "form": "풍년제과 초코파이도!",
          "original_form": "풍년제과 초코파이도!",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016735.1.3",
          "form": "은근 맛있었어요!",
          "original_form": "은근 맛있었어요!",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016735.1.4",
          "form": "피순대국밥은 드셨나요?",
          "original_form": "피순대국밥은 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016735.1.5",
          "form": "전주가 피순대국밥이 유명하대요",
          "original_form": "전주가 피순대국밥이 유명하대요",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016735.1.6",
          "form": "그건 못 먹어봤어요 맛있었나요?",
          "original_form": "그건 못 먹어봤어요 맛있었나요?",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016735.1.7",
          "form": "저는 국밥 특히 좀 얼큰한 국밥 좋아해서",
          "original_form": "저는 국밥 특히 좀 얼큰한 국밥 좋아해서",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016735.1.8",
          "form": "맛있게 먹었어요",
          "original_form": "맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016735.1.9",
          "form": "담에 가면 꼭 먹어봐야겠어요",
          "original_form": "담에 가면 꼭 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016735.1.10",
          "form": "그냥 순대국밥이랑 다른거죠?",
          "original_form": "그냥 순대국밥이랑 다른거죠?",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016735.1.11",
          "form": "네 좀더 얼큰하고 진한 맛이예요",
          "original_form": "네 좀더 얼큰하고 진한 맛이예요",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016735.1.12",
          "form": "국내여행지로 또 어디 좋아하세요?",
          "original_form": "국내여행지로 또 어디 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016735.1.13",
          "form": "경주도 자주 가요 제가 지방에 살아서ㅎ",
          "original_form": "경주도 자주 가요 제가 지방에 살아서ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016735.1.14",
          "form": "저도 부산출신이라 경주도 자주 갔었어요!",
          "original_form": "저도 부산출신이라 경주도 자주 갔었어요!",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016735.1.15",
          "form": "경주 언제가도 좋죠...",
          "original_form": "경주 언제가도 좋죠...",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016735.1.16",
          "form": "맞아요. 요즘은 너무 인기가 많아서 잘 안가지만ㅎㅎ",
          "original_form": "맞아요. 요즘은 너무 인기가 많아서 잘 안가지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016735.1.17",
          "form": "옛날의 한적한 경주가 그리워요",
          "original_form": "옛날의 한적한 경주가 그리워요",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016735.1.18",
          "form": "저도요ㅠㅠ 마지막으로 갔을때도 사람많아져서 별로였어요",
          "original_form": "저도요ㅠㅠ 마지막으로 갔을때도 사람많아져서 별로였어요",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016735.1.19",
          "form": "그쵸 이번에 모임이 있어서 주말에 경주가야하는데 추천해주실곳있으세요?",
          "original_form": "그쵸 이번에 모임이 있어서 주말에 경주가야하는데 추천해주실곳있으세요?",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016735.1.20",
          "form": "저는 안압지를 갈때마다 가요",
          "original_form": "저는 안압지를 갈때마다 가요",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016735.1.21",
          "form": "이미유명해서 가보셨겠지만 저는 매번좋아서요",
          "original_form": "이미유명해서 가보셨겠지만 저는 매번좋아서요",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016735.1.22",
          "form": "아 십년전에 가본듯\r\n최근엔 안가봤네요 시간되면 꼭 들러야겠어요",
          "original_form": "아 십년전에 가본듯\r\n최근엔 안가봤네요 시간되면 꼭 들러야겠어요",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016735.1.23",
          "form": "저는 경주 사람없을때 자전거타고 다니는것도 좋아했어요",
          "original_form": "저는 경주 사람없을때 자전거타고 다니는것도 좋아했어요",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016735.1.24",
          "form": "오 저도요 특히 벚꽃시즌에 자전거타면 정말 좋아요",
          "original_form": "오 저도요 특히 벚꽃시즌에 자전거타면 정말 좋아요",
          "speaker_id": "2",
          "time": "20210826 16:22"
        }
      ]
    }
  ]
