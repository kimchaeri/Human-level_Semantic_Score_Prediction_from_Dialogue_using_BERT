      "utterance": [
        {
          "id": "MDRW2100044642.1.1",
          "form": "야식 땡기지않나요?",
          "original_form": "야식 땡기지않나요?",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044642.1.2",
          "form": "ㅋㅋ야식으로 아이스크림먹었습니다ㅜㅜ",
          "original_form": "ㅋㅋ야식으로 아이스크림먹었습니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044642.1.3",
          "form": "누누씌님은 야식 시켜드시려구용?",
          "original_form": "누누씌님은 야식 시켜드시려구용?",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044642.1.4",
          "form": "전 크레이프케익 한겹반 먹었답니다",
          "original_form": "전 크레이프케익 한겹반 먹었답니다",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044642.1.5",
          "form": "(3호인게 함정)",
          "original_form": "(3호인게 함정)",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044642.1.6",
          "form": "워후",
          "original_form": "워후",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044642.1.7",
          "form": "혹시 빌리엔젤인가요...?",
          "original_form": "혹시 빌리엔젤인가요...?",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044642.1.8",
          "form": "스페로스페라",
          "original_form": "스페로스페라",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044642.1.9",
          "form": "라고 돼있네요!",
          "original_form": "라고 돼있네요!",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044642.1.10",
          "form": "배송으로 받은거에요!",
          "original_form": "배송으로 받은거에요!",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044642.1.11",
          "form": "아!!! 카카오선물거기에 있는거죠?",
          "original_form": "아!!! 카카오선물거기에 있는거죠?",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044642.1.12",
          "form": "거기꺼 진짜맛있던데",
          "original_form": "거기꺼 진짜맛있던데",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044642.1.13",
          "form": "네 그런것같아요!!!",
          "original_form": "네 그런것같아요!!!",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044642.1.14",
          "form": "배부른 상태에서먹어도 맛있는걸보니",
          "original_form": "배부른 상태에서먹어도 맛있는걸보니",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044642.1.15",
          "form": "진짜 맛있는거같아요",
          "original_form": "진짜 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044642.1.16",
          "form": "저도 셀프선물로 사먹을까봐요ㅋㅋㅋㅋ",
          "original_form": "저도 셀프선물로 사먹을까봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044642.1.17",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044642.1.18",
          "form": "수고한 나자신 좀 먹어줘야해요!",
          "original_form": "수고한 나자신 좀 먹어줘야해요!",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044642.1.19",
          "form": "ㅋㅋㅋㅋ크레이프케익 제일 좋아하시나요?",
          "original_form": "ㅋㅋㅋㅋ크레이프케익 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044642.1.20",
          "form": "그건아닌데 선물로 들어와서",
          "original_form": "그건아닌데 선물로 들어와서",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044642.1.21",
          "form": "감사한 마음으로 먹고있어요 ㅋㅋㅋ",
          "original_form": "감사한 마음으로 먹고있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044642.1.22",
          "form": "케익을 전반적으로 좋아해요",
          "original_form": "케익을 전반적으로 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044642.1.23",
          "form": "저는 빌리엔젤에서 먹어보고 크레이프케익을 좋아하게되었어요ㅎ",
          "original_form": "저는 빌리엔젤에서 먹어보고 크레이프케익을 좋아하게되었어요ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044642.1.24",
          "form": "빌리엔젤도 유명한곳인가요???",
          "original_form": "빌리엔젤도 유명한곳인가요???",
          "speaker_id": "1",
          "time": "20210907 23:56"
        }
      ]
