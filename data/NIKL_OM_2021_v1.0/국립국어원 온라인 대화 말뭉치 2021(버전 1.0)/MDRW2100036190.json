      "utterance": [
        {
          "id": "MDRW2100036190.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100036190.1.2",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100036190.1.3",
          "form": "아뇨 아직요ㅎㅎ 님은 식사 하셨나요?",
          "original_form": "아뇨 아직요ㅎㅎ 님은 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100036190.1.4",
          "form": "저는 단백질바 랑 두유로 끝냈어요 ㅎ",
          "original_form": "저는 단백질바 랑 두유로 끝냈어요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 18:36"
        },
        {
          "id": "MDRW2100036190.1.5",
          "form": "나름 건강생각해서 단백질바 먹었는데\r\n9시되면 배고파질듯요 ㅎㅎ",
          "original_form": "나름 건강생각해서 단백질바 먹었는데\r\n9시되면 배고파질듯요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100036190.1.6",
          "form": "다이어트 하시는건가요?",
          "original_form": "다이어트 하시는건가요?",
          "speaker_id": "2",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100036190.1.7",
          "form": "어제 너무 많이 먹어서요",
          "original_form": "어제 너무 많이 먹어서요",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100036190.1.8",
          "form": "오늘은 좀 적게 먹자싶어서요",
          "original_form": "오늘은 좀 적게 먹자싶어서요",
          "speaker_id": "1",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036190.1.9",
          "form": "어제의 나를 오늘의 내가 뒷수습하는건가요ㅎㅎㅎ",
          "original_form": "어제의 나를 오늘의 내가 뒷수습하는건가요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036190.1.10",
          "form": "글쵸 죄책감줄이기?",
          "original_form": "글쵸 죄책감줄이기?",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100036190.1.11",
          "form": "배달음식 자주 이용하세요?",
          "original_form": "배달음식 자주 이용하세요?",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036190.1.12",
          "form": "아뇨 배달음식 좋아하긴 하는데 먹고나면 몸이 많이 부어서 자주는 못먹어요ㅋㅋ",
          "original_form": "아뇨 배달음식 좋아하긴 하는데 먹고나면 몸이 많이 부어서 자주는 못먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036190.1.13",
          "form": "저두 편하긴한데 양도 많고",
          "original_form": "저두 편하긴한데 양도 많고",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036190.1.14",
          "form": "쓰레기가 많이 나와서 잘 안시켜먹게되더라구요",
          "original_form": "쓰레기가 많이 나와서 잘 안시켜먹게되더라구요",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036190.1.15",
          "form": "그쵸ㅎㅎ 쓰레기도 문제에요ㅠ",
          "original_form": "그쵸ㅎㅎ 쓰레기도 문제에요ㅠ",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036190.1.16",
          "form": "며칠전 밀키트 백순대볶음 시켰는데",
          "original_form": "며칠전 밀키트 백순대볶음 시켰는데",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036190.1.17",
          "form": "맛나고 재료도 필요한거만큼 있으니 좋더라구요",
          "original_form": "맛나고 재료도 필요한거만큼 있으니 좋더라구요",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036190.1.18",
          "form": "남는 재료있음 신경쓰이는데 그런것두없구요",
          "original_form": "남는 재료있음 신경쓰이는데 그런것두없구요",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036190.1.19",
          "form": "오 백순대볶음! 저도 한번 검색해봐야겠어요 맛있을것같아요 집에서 조리하니까 간도 조절할 수 있고ㅎㅎ",
          "original_form": "오 백순대볶음! 저도 한번 검색해봐야겠어요 맛있을것같아요 집에서 조리하니까 간도 조절할 수 있고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100036190.1.20",
          "form": "재료는 별거없더라구요",
          "original_form": "재료는 별거없더라구요",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100036190.1.21",
          "form": "들깨가루 양배추 순대 쫄면 정도였어요",
          "original_form": "들깨가루 양배추 순대 쫄면 정도였어요",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036190.1.22",
          "form": "근데 거기서 깻잎이랑 찍어먹는장을 주는데 그게 궁합이 잘 맞더라구요",
          "original_form": "근데 거기서 깻잎이랑 찍어먹는장을 주는데 그게 궁합이 잘 맞더라구요",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036190.1.23",
          "form": "윙잇? 인가 그 사이트였어요",
          "original_form": "윙잇? 인가 그 사이트였어요",
          "speaker_id": "1",
          "time": "20210830 18:46"
        }
      ]
    }
  ]
