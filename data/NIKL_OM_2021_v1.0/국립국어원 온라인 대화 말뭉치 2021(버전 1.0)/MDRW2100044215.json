      "utterance": [
        {
          "id": "MDRW2100044215.1.1",
          "form": "저 수호요",
          "original_form": "저 수호요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044215.1.2",
          "form": "아하 얼굴 보시는구나?",
          "original_form": "아하 얼굴 보시는구나?",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044215.1.3",
          "form": "네 얼굴이랑 성격봐요",
          "original_form": "네 얼굴이랑 성격봐요",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044215.1.4",
          "form": "님은 이상형뭐에요?",
          "original_form": "님은 이상형뭐에요?",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044215.1.5",
          "form": "저요 얼굴 몸 성격이요",
          "original_form": "저요 얼굴 몸 성격이요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044215.1.6",
          "form": "몸은",
          "original_form": "몸은",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044215.1.7",
          "form": "어떤몸이요?",
          "original_form": "어떤몸이요?",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044215.1.8",
          "form": "마른몸?",
          "original_form": "마른몸?",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044215.1.9",
          "form": "그거도 좋구요",
          "original_form": "그거도 좋구요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044215.1.10",
          "form": "적당한 근육도 좋아요",
          "original_form": "적당한 근육도 좋아요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044215.1.11",
          "form": "마른데 근육 조아요",
          "original_form": "마른데 근육 조아요",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.12",
          "form": "김종국은요?",
          "original_form": "김종국은요?",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.13",
          "form": "아니면 진짜 타고난 몸 근육",
          "original_form": "아니면 진짜 타고난 몸 근육",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.14",
          "form": "어 김종국 실제본적 업서서",
          "original_form": "어 김종국 실제본적 업서서",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.15",
          "form": "갠차늘듯...그냥 타고난 체격 조은사람",
          "original_form": "갠차늘듯...그냥 타고난 체격 조은사람",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.16",
          "form": "조아함",
          "original_form": "조아함",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.17",
          "form": "보여드리고싶네",
          "original_form": "보여드리고싶네",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.18",
          "form": "ㅋㅋㅋ마자요 마인드 건강해서 좋아보여요",
          "original_form": "ㅋㅋㅋ마자요 마인드 건강해서 좋아보여요",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.19",
          "form": "어떡해요 저",
          "original_form": "어떡해요 저",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044215.1.20",
          "form": "반햇다구요",
          "original_form": "반햇다구요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044215.1.21",
          "form": "누구에게요?",
          "original_form": "누구에게요?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044215.1.22",
          "form": "운동갓는데 잘생남 목격",
          "original_form": "운동갓는데 잘생남 목격",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044215.1.23",
          "form": "헐 자세히 설명해주세요",
          "original_form": "헐 자세히 설명해주세요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044215.1.24",
          "form": "자세히 설명 부탁해요",
          "original_form": "자세히 설명 부탁해요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        }
      ]
