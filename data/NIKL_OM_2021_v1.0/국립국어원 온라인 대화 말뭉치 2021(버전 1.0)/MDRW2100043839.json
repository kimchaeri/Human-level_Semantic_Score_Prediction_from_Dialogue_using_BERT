      "utterance": [
        {
          "id": "MDRW2100043839.1.1",
          "form": "과식했는데 산책하러갈지 고민이에요",
          "original_form": "과식했는데 산책하러갈지 고민이에요",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043839.1.2",
          "form": "비오니까 오늘은 쉬시죠...ㅍㅍ",
          "original_form": "비오니까 오늘은 쉬시죠...ㅍㅍ",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043839.1.3",
          "form": "ㅠㅠ 비그만왔음좋겠네요",
          "original_form": "ㅠㅠ 비그만왔음좋겠네요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043839.1.4",
          "form": "내일 친구랑 밥먹는데",
          "original_form": "내일 친구랑 밥먹는데",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043839.1.5",
          "form": "뭐먹을지고민이에요",
          "original_form": "뭐먹을지고민이에요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043839.1.6",
          "form": "음 .. 어떤거 좋아하세요?",
          "original_form": "음 .. 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043839.1.7",
          "form": "파스타 마라탕 떡볶이 ..요론거 저는 자주 먹어요",
          "original_form": "파스타 마라탕 떡볶이 ..요론거 저는 자주 먹어요",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043839.1.8",
          "form": "헉 마라탕",
          "original_form": "헉 마라탕",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043839.1.9",
          "form": "안먹은지 오래됐어요!!",
          "original_form": "안먹은지 오래됐어요!!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043839.1.10",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043839.1.11",
          "form": "네!너무좋죠",
          "original_form": "네!너무좋죠",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043839.1.12",
          "form": "어떤 재료 꼭 넣으시나요!",
          "original_form": "어떤 재료 꼭 넣으시나요!",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043839.1.13",
          "form": "숙주 팽이버섯 중국당면 소고기 감자나고구마",
          "original_form": "숙주 팽이버섯 중국당면 소고기 감자나고구마",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043839.1.14",
          "form": "꼭 넣는것같아요!",
          "original_form": "꼭 넣는것같아요!",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043839.1.15",
          "form": "오오 맞아요 다 필수재료들이네요",
          "original_form": "오오 맞아요 다 필수재료들이네요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043839.1.16",
          "form": "저는 옥수수당면이랑 청경채 좋아해서 꼭 넣어요",
          "original_form": "저는 옥수수당면이랑 청경채 좋아해서 꼭 넣어요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043839.1.17",
          "form": "청경채!!!",
          "original_form": "청경채!!!",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043839.1.18",
          "form": "바로 다음번 마라탕엔 청경채 넣어야겠어요",
          "original_form": "바로 다음번 마라탕엔 청경채 넣어야겠어요",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043839.1.19",
          "form": "청경채 아주 굿입니다..",
          "original_form": "청경채 아주 굿입니다..",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043839.1.20",
          "form": "맵기는 어떻게 드세요?",
          "original_form": "맵기는 어떻게 드세요?",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043839.1.21",
          "form": "중간이나 매운맛이요!!",
          "original_form": "중간이나 매운맛이요!!",
