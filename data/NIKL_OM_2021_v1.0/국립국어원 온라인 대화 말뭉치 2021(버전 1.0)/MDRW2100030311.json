      "utterance": [
        {
          "id": "MDRW2100030311.1.1",
          "form": "날치 안농",
          "original_form": "날치 안농",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030311.1.2",
          "form": "얼릉 가족 얘기 좀 해봐바",
          "original_form": "얼릉 가족 얘기 좀 해봐바",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030311.1.3",
          "form": "지금 엄마 주무심",
          "original_form": "지금 엄마 주무심",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030311.1.4",
          "form": "너도 얘기해",
          "original_form": "너도 얘기해",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030311.1.5",
          "form": "ㅋㅋㅋㅋㅋ 울 할머니도 지금 주무셔",
          "original_form": "ㅋㅋㅋㅋㅋ 울 할머니도 지금 주무셔",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030311.1.6",
          "form": "늦게까지 안 자다가 걸리면 할머니한테 완전 혼남 ㅠ",
          "original_form": "늦게까지 안 자다가 걸리면 할머니한테 완전 혼남 ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030311.1.7",
          "form": "헐 완전 조심해야 겠구나",
          "original_form": "헐 완전 조심해야 겠구나",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030311.1.8",
          "form": "웅 근데 아직까진 시간 괜찮어",
          "original_form": "웅 근데 아직까진 시간 괜찮어",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030311.1.9",
          "form": "ㅎㅎ 오늘 한 집안일 말하기 시작~",
          "original_form": "ㅎㅎ 오늘 한 집안일 말하기 시작~",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030311.1.10",
          "form": "잠깐만",
          "original_form": "잠깐만",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030311.1.11",
          "form": "웅 생각해보구 와 나도 생각해본돳",
          "original_form": "웅 생각해보구 와 나도 생각해본돳",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030311.1.12",
          "form": "일단 우리 어머니 퇴근하시기 전에 온 집안 청소기 돌리고 설거지하고 우리 스릉스러운 티모,사랑이, 밥주고 화장실 청소했어",
          "original_form": "일단 우리 어머니 퇴근하시기 전에 온 집안 청소기 돌리고 설거지하고 우리 스릉스러운 티모,사랑이, 밥주고 화장실 청소했어",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030311.1.13",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030311.1.14",
          "form": "이열 착한딸",
          "original_form": "이열 착한딸",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030311.1.15",
          "form": "나는 쓰레기통 비우고 설거지 했어",
          "original_form": "나는 쓰레기통 비우고 설거지 했어",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030311.1.16",
          "form": "ㅎㅎ 좀 더 이것저것 많이 해볼걸",
          "original_form": "ㅎㅎ 좀 더 이것저것 많이 해볼걸",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030311.1.17",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030311.1.18",
          "form": "거의 집안일 반은 했어",
          "original_form": "거의 집안일 반은 했어",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030311.1.19",
          "form": "바쁘게 사는구낭... 나는 내 방 청소도 잘 안 해서 개판인데 ㅠ",
          "original_form": "바쁘게 사는구낭... 나는 내 방 청소도 잘 안 해서 개판인데 ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030311.1.20",
          "form": "난 방이 거실이라 청소돌릴때 같이 하는겸이 됨",
          "original_form": "난 방이 거실이라 청소돌릴때 같이 하는겸이 됨",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030311.1.21",
          "form": "울 할무니 이제 나 잠 포기하셨나봐 아침에 늦게 일어난다고 안 혼내",
          "original_form": "울 할무니 이제 나 잠 포기하셨나봐 아침에 늦게 일어난다고 안 혼내",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030311.1.22",
          "form": "구래???",
          "original_form": "구래???",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030311.1.23",
          "form": "가장 넓은 방 쓰네",
          "original_form": "가장 넓은 방 쓰네",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030311.1.24",
          "form": "젠장 그래도 프라이버시가 없엉...",
          "original_form": "젠장 그래도 프라이버시가 없엉...",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030311.1.25",
          "form": "할머님도 포기하신",
          "original_form": "할머님도 포기하신",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030311.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030311.1.27",
          "form": "할무니 죄송함돠..",
          "original_form": "할무니 죄송함돠..",
