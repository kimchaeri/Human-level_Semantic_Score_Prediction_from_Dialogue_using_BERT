      "utterance": [
        {
          "id": "MDRW2100028619.1.1",
          "form": "홍삼 사야 하는데",
          "original_form": "홍삼 사야 하는데",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028619.1.2",
          "form": "맞아 정관장 홍삼",
          "original_form": "맞아 정관장 홍삼",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028619.1.3",
          "form": "얼마 주면 돼?",
          "original_form": "얼마 주면 돼?",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028619.1.4",
          "form": "아직 주문 안 했어 이제 할게",
          "original_form": "아직 주문 안 했어 이제 할게",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028619.1.5",
          "form": "너는 드러이기 언제 살 거야?",
          "original_form": "너는 드러이기 언제 살 거야?",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028619.1.6",
          "form": "다이슨",
          "original_form": "다이슨",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028619.1.7",
          "form": "천천히 해도 돼 난 추석 끝나고 지원금 받으면 살래",
          "original_form": "천천히 해도 돼 난 추석 끝나고 지원금 받으면 살래",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028619.1.8",
          "form": "다이슨 체험해보고 싶오",
          "original_form": "다이슨 체험해보고 싶오",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028619.1.9",
          "form": "홍대에서 체험할 수 있대 체험하고 구매해",
          "original_form": "홍대에서 체험할 수 있대 체험하고 구매해",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028619.1.10",
          "form": "그래야겠다 너무 먼데 다른지점도 있는지 찾아봐야지",
          "original_form": "그래야겠다 너무 먼데 다른지점도 있는지 찾아봐야지",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028619.1.11",
          "form": "너도 살의향 있어?",
          "original_form": "너도 살의향 있어?",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028619.1.12",
          "form": "너 사면 써 보고 살래 드라이기",
          "original_form": "너 사면 써 보고 살래 드라이기",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028619.1.13",
          "form": "그래 좋아",
          "original_form": "그래 좋아",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028619.1.14",
          "form": "그게 롱버전이 있고 보통이있다는데 난 보통으로 사도 되겠지",
          "original_form": "그게 롱버전이 있고 보통이있다는데 난 보통으로 사도 되겠지",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028619.1.15",
          "form": "머리 기를 생각이면 롱버전 드라이기가 좋지 않을까?",
          "original_form": "머리 기를 생각이면 롱버전 드라이기가 좋지 않을까?",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028619.1.16",
          "form": "아 그런가 고민이네",
          "original_form": "아 그런가 고민이네",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028619.1.17",
          "form": "가격차이가 있는 것 같더라고",
          "original_form": "가격차이가 있는 것 같더라고",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028619.1.18",
          "form": "써보고 결정하겠어",
          "original_form": "써보고 결정하겠어",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028619.1.19",
          "form": "중고나라에서 싸게 파는데 중고는 별로니?",
          "original_form": "중고나라에서 싸게 파는데 중고는 별로니?",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028619.1.20",
          "form": "중고도 좋아 알아보는게 귀찮을뿐",
          "original_form": "중고도 좋아 알아보는게 귀찮을뿐",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028619.1.21",
          "form": "맞아 새거로 구매하자",
          "original_form": "맞아 새거로 구매하자",
          "speaker_id": "2",
          "time": "20210902 21:13"
        }
