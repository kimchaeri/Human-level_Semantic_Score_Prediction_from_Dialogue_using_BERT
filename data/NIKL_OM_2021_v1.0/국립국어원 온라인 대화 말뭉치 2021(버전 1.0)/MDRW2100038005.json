      "utterance": [
        {
          "id": "MDRW2100038005.1.1",
          "form": "안으로 기어서 올라갔어..........",
          "original_form": "안으로 기어서 올라갔어..........",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100038005.1.2",
          "form": "......벤티없는 유저는 이 일을 기억할것입니다..........",
          "original_form": "......벤티없는 유저는 이 일을 기억할것입니다..........",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100038005.1.3",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100038005.1.4",
          "form": "근데 황성이 뭐야?",
          "original_form": "근데 황성이 뭐야?",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100038005.1.5",
          "form": "아 그",
          "original_form": "아 그",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100038005.1.6",
          "form": "주인공 바위 스킬",
          "original_form": "주인공 바위 스킬",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100038005.1.7",
          "form": "그거 이름이 황성이잔아",
          "original_form": "그거 이름이 황성이잔아",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100038005.1.8",
          "form": "근데 게임 제작자가",
          "original_form": "근데 게임 제작자가",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038005.1.9",
          "form": "그거 쓸걸 생각했는지",
          "original_form": "그거 쓸걸 생각했는지",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038005.1.10",
          "form": "황성보다 틈이 더 넓게 해서",
          "original_form": "황성보다 틈이 더 넓게 해서",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038005.1.11",
          "form": "올라가지질 않는거야",
          "original_form": "올라가지질 않는거야",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038005.1.12",
          "form": "ㄹㅇ겁나 박침",
          "original_form": "ㄹㅇ겁나 박침",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038005.1.13",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100038005.1.14",
          "form": "그거 이름이 황성이구나 ㅋㅋㅋㅋ",
          "original_form": "그거 이름이 황성이구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038005.1.15",
          "form": "ㅇㅇ그치만 좀",
          "original_form": "ㅇㅇ그치만 좀",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038005.1.16",
          "form": "촌스러워.....",
          "original_form": "촌스러워.....",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038005.1.17",
          "form": "내 머릿속 황성은 시골이라구....",
          "original_form": "내 머릿속 황성은 시골이라구....",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038005.1.18",
          "form": "난 왕궁 생각나서",
          "original_form": "난 왕궁 생각나서",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038005.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 그 황성",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 그 황성",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038005.1.20",
          "form": "ㅇㅇ ㅋㅋㅋ 뜬금없어",
          "original_form": "ㅇㅇ ㅋㅋㅋ 뜬금없어",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038005.1.21",
          "form": "거기 가봤어?",
          "original_form": "거기 가봤어?",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038005.1.22",
          "form": "그 번개섬",
          "original_form": "그 번개섬",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038005.1.23",
          "form": "아니 아직",
          "original_form": "아니 아직",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038005.1.24",
          "form": "거긴 ㄹㅇ 폐허야.......",
          "original_form": "거긴 ㄹㅇ 폐허야.......",
