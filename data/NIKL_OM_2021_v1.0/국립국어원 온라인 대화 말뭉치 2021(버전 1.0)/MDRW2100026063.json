      "utterance": [
        {
          "id": "MDRW2100026063.1.1",
          "form": "음....중고폰 할부...?",
          "original_form": "음....중고폰 할부...?",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100026063.1.2",
          "form": "헐 폰바꿈?",
          "original_form": "헐 폰바꿈?",
          "speaker_id": "1",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100026063.1.3",
          "form": "너 만났을때 이미 바꾼거였으 ㅋㅋㅋ",
          "original_form": "너 만났을때 이미 바꾼거였으 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026063.1.4",
          "form": "난 왜몰랐냐",
          "original_form": "난 왜몰랐냐",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026063.1.5",
          "form": "같은 갤럭시 라인이라 별 차이가 안나서? ㅋㅋㅋ",
          "original_form": "같은 갤럭시 라인이라 별 차이가 안나서? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100026063.1.6",
          "form": "이전 폰 터치 튀기 시작해서 바꿈",
          "original_form": "이전 폰 터치 튀기 시작해서 바꿈",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100026063.1.7",
          "form": "하긴 오래쓰긴했지않니",
          "original_form": "하긴 오래쓰긴했지않니",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100026063.1.8",
          "form": "엉 한 3년?",
          "original_form": "엉 한 3년?",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100026063.1.9",
          "form": "ㅋㅋㅋㅋ옛날폴더폰때처럼",
          "original_form": "ㅋㅋㅋㅋ옛날폴더폰때처럼",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100026063.1.10",
          "form": "튼튼하게만들어줬으면좋겠다",
          "original_form": "튼튼하게만들어줬으면좋겠다",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100026063.1.11",
          "form": "오래오래 안바꾸고싶음",
          "original_form": "오래오래 안바꾸고싶음",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100026063.1.12",
          "form": "근데 요즘 폰들은 딱 2년 3년 지나고나면 맛가지 않냐",
          "original_form": "근데 요즘 폰들은 딱 2년 3년 지나고나면 맛가지 않냐",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100026063.1.13",
          "form": "ㄹㅇ 뭔가짠듯이",
          "original_form": "ㄹㅇ 뭔가짠듯이",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100026063.1.14",
          "form": "그때되면 고장나",
          "original_form": "그때되면 고장나",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100026063.1.15",
          "form": "ㅁㅈ 어디 이상이 생겨도 생기드라",
          "original_form": "ㅁㅈ 어디 이상이 생겨도 생기드라",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100026063.1.16",
          "form": "너는 최대 소비 역시 약인가? ㅋㅋㅋㅋ",
          "original_form": "너는 최대 소비 역시 약인가? ㅋㅋㅋㅋ",
