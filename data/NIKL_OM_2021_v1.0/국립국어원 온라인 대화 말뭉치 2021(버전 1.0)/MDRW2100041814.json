      "utterance": [
        {
          "id": "MDRW2100041814.1.1",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100041814.1.2",
          "form": "전 무조겅",
          "original_form": "전 무조겅",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100041814.1.3",
          "form": "떡파요",
          "original_form": "떡파요",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100041814.1.4",
          "form": "어묵은 거의 안드세여!?",
          "original_form": "어묵은 거의 안드세여!?",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100041814.1.5",
          "form": "넵 어묵은 딱히 안좋아해요",
          "original_form": "넵 어묵은 딱히 안좋아해요",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041814.1.6",
          "form": "뭔가.. 별로더라구요..",
          "original_form": "뭔가.. 별로더라구요..",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041814.1.7",
          "form": "오 그럴수있죠 입맛은 다양하니까용ㅎㅎ",
          "original_form": "오 그럴수있죠 입맛은 다양하니까용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:28"
        },
        {
          "id": "MDRW2100041814.1.8",
          "form": "중국당면 이런것도 조아하세요?",
          "original_form": "중국당면 이런것도 조아하세요?",
          "speaker_id": "1",
          "time": "20210906 16:28"
        },
        {
          "id": "MDRW2100041814.1.9",
          "form": "ㅋㅋㅋ 그래도 있으면 먹긴해요",
          "original_form": "ㅋㅋㅋ 그래도 있으면 먹긴해요",
          "speaker_id": "2",
          "time": "20210906 16:28"
        },
        {
          "id": "MDRW2100041814.1.10",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210906 16:28"
        },
        {
          "id": "MDRW2100041814.1.11",
          "form": "저두요ㅎㅎ추가해서 먹으면 맛있더라구요",
          "original_form": "저두요ㅎㅎ추가해서 먹으면 맛있더라구요",
          "speaker_id": "1",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041814.1.12",
          "form": "쫀뜩쫀뜩해서 맛있어요",
          "original_form": "쫀뜩쫀뜩해서 맛있어요",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041814.1.13",
          "form": "분모자도 좋아하시나요?",
          "original_form": "분모자도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041814.1.14",
          "form": "네! 분모자도 좋아요 탱글탱글",
          "original_form": "네! 분모자도 좋아요 탱글탱글",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041814.1.15",
          "form": "분모자는 마라탕에 있는것만 먹어봤어요",
          "original_form": "분모자는 마라탕에 있는것만 먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041814.1.16",
          "form": "저두요 ㅎㅎㅎ ㅎㅎ",
          "original_form": "저두요 ㅎㅎㅎ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041814.1.17",
          "form": "맛있죠 ㅠㅠ",
          "original_form": "맛있죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041814.1.18",
          "form": "넹ㅜㅜ식감도 좋고 맛나용",
          "original_form": "넹ㅜㅜ식감도 좋고 맛나용",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041814.1.19",
          "form": "칼로리는 생각하고 싶지 않..",
          "original_form": "칼로리는 생각하고 싶지 않..",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041814.1.20",
          "form": "곤약 느낌이니까.  .. 칼로리는 뭐",
          "original_form": "곤약 느낌이니까.  .. 칼로리는 뭐",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041814.1.21",
          "form": "생각하지맙시다",
          "original_form": "생각하지맙시다",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041814.1.22",
          "form": "ㅋㅋㅋ떡이아니고 곤약이라고 생각하면 되는군요",
          "original_form": "ㅋㅋㅋ떡이아니고 곤약이라고 생각하면 되는군요",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041814.1.23",
