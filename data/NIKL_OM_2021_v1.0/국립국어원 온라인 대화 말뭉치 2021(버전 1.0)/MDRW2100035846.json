        {
          "id": "MDRW2100035846.1.2",
          "form": "결혼하셨다 했나요?ㅋ",
          "original_form": "결혼하셨다 했나요?ㅋ",
          "speaker_id": "2",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100035846.1.3",
          "form": "아니요 아직",
          "original_form": "아니요 아직",
          "speaker_id": "1",
          "time": "20210830 10:50"
        },
        {
          "id": "MDRW2100035846.1.4",
          "form": "아~  연애는요?",
          "original_form": "아~  연애는요?",
          "speaker_id": "2",
          "time": "20210830 10:58"
        },
        {
          "id": "MDRW2100035846.1.5",
          "form": "연애는 하고 있어요~",
          "original_form": "연애는 하고 있어요~",
          "speaker_id": "1",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100035846.1.6",
          "form": "아 오래 하셨나요??",
          "original_form": "아 오래 하셨나요??",
          "speaker_id": "2",
          "time": "20210830 14:38"
        },
        {
          "id": "MDRW2100035846.1.7",
          "form": "지금 만나고 있는 사람과는 6개월 정도요",
          "original_form": "지금 만나고 있는 사람과는 6개월 정도요",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100035846.1.8",
          "form": "name2님은 결혼하신 분과 오래 연애하셨나요?",
          "original_form": "&name2&님은 결혼하신 분과 오래 연애하셨나요?",
          "speaker_id": "1",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100035846.1.9",
          "form": "네 저는 5년정도요 ㅎㅎ",
          "original_form": "네 저는 5년정도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100035846.1.10",
          "form": "오 오래 만나셨네요",
          "original_form": "오 오래 만나셨네요",
          "speaker_id": "1",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035846.1.11",
          "form": "이상형과 가까운 분이셨나요?ㅎㅎ",
          "original_form": "이상형과 가까운 분이셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100035846.1.12",
          "form": "아니요ㅠ살아보고 겪으니ㅋㅋ안맞는다는게",
          "original_form": "아니요ㅠ살아보고 겪으니ㅋㅋ안맞는다는게",
          "speaker_id": "2",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100035846.1.13",
          "form": "ㅎㅎ오래 연애하고 결혼해도 안 맞는 부분이 있군요ㅜ",
          "original_form": "ㅎㅎ오래 연애하고 결혼해도 안 맞는 부분이 있군요ㅜ",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100035846.1.14",
          "form": "네 그리고 또 오래살다보면 더 그러는것같아여ㅠ",
          "original_form": "네 그리고 또 오래살다보면 더 그러는것같아여ㅠ",
          "speaker_id": "2",
