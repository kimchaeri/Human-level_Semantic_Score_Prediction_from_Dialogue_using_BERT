      "utterance": [
        {
          "id": "MDRW2100042510.1.1",
          "form": "해피엔딩 드라마도 조연",
          "original_form": "해피엔딩 드라마도 조연",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042510.1.2",
          "form": "주연급 얼굴은 아님",
          "original_form": "주연급 얼굴은 아님",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042510.1.3",
          "form": "마의 사극에서도 조연",
          "original_form": "마의 사극에서도 조연",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042510.1.4",
          "form": "조연도 개성 있는 조연도 아님",
          "original_form": "조연도 개성 있는 조연도 아님",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042510.1.5",
          "form": "안타깝다 계속 조 연 이 네",
          "original_form": "안타깝다 계속 조 연 이 네",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042510.1.6",
          "form": "동상이몽에서 뜬듯ㅋ",
          "original_form": "동상이몽에서 뜬듯ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042510.1.7",
          "form": "ㅋㅋ예능으로",
          "original_form": "ㅋㅋ예능으로",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042510.1.8",
          "form": "예능으로 떠도 상관없죠. 뜨면 되니ㅋ",
          "original_form": "예능으로 떠도 상관없죠. 뜨면 되니ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042510.1.9",
          "form": "그 다음해 사극 구암허준에서 마지막 조연인가",
          "original_form": "그 다음해 사극 구암허준에서 마지막 조연인가",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042510.1.10",
          "form": "그 뒤로 안 나왔나요?",
          "original_form": "그 뒤로 안 나왔나요?",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042510.1.11",
          "form": "드디어 주연을 한다",
          "original_form": "드디어 주연을 한다",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042510.1.12",
          "form": "어디서요?",
          "original_form": "어디서요?",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042510.1.13",
