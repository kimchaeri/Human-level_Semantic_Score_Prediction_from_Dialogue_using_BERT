      "utterance": [
        {
          "id": "MDRW2100009683.1.1",
          "form": "어떠신가요",
          "original_form": "어떠신가요",
          "speaker_id": "1",
          "time": "20210823 03:26"
        },
        {
          "id": "MDRW2100009683.1.2",
          "form": "사실 제 강아지는없지만",
          "original_form": "사실 제 강아지는없지만",
          "speaker_id": "1",
          "time": "20210823 03:26"
        },
        {
          "id": "MDRW2100009683.1.3",
          "form": "친구 강아지가 있어서",
          "original_form": "친구 강아지가 있어서",
          "speaker_id": "1",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.4",
          "form": "오 조어요",
          "original_form": "오 조어요",
          "speaker_id": "2",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.5",
          "form": "칭긔 강아지 이름 뭔가요",
          "original_form": "칭긔 강아지 이름 뭔가요",
          "speaker_id": "2",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.6",
          "form": "name3이입니다",
          "original_form": "&name3&이입니다",
          "speaker_id": "1",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.7",
          "form": "말티쥬에요",
          "original_form": "말티쥬에요",
          "speaker_id": "1",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.8",
          "form": "흰색 말티쥬",
          "original_form": "흰색 말티쥬",
          "speaker_id": "1",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.9",
          "form": "넘 이뻐요 진짜",
          "original_form": "넘 이뻐요 진짜",
          "speaker_id": "1",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.10",
          "form": "name3이",
          "original_form": "&name3&이",
          "speaker_id": "2",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.11",
          "form": "헐 우리 고모 갱얼쥐 이름도 name3이인데",
          "original_form": "헐 우리 고모 갱얼쥐 이름도 &name3&이인데",
          "speaker_id": "2",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.12",
          "form": "스테디 이름인가봐요",
          "original_form": "스테디 이름인가봐요",
          "speaker_id": "2",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009683.1.13",
          "form": "오 말티즈인가요",
          "original_form": "오 말티즈인가요",
          "speaker_id": "1",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.15",
          "form": "말티쥬",
          "original_form": "말티쥬",
          "speaker_id": "2",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.16",
          "form": "오 역시",
          "original_form": "오 역시",
          "speaker_id": "1",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.17",
          "form": "말랑콩떡",
          "original_form": "말랑콩떡",
          "speaker_id": "2",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.19",
          "form": "말랑콩떡 기야미",
          "original_form": "말랑콩떡 기야미",
          "speaker_id": "1",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.20",
          "form": "그러면말이죠",
          "original_form": "그러면말이죠",
          "speaker_id": "2",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.21",
          "form": "친구집에 친구는 없는데 말티쥬만 있음 vs 친구집에 말티쥬 없고 친구만 있음",
          "original_form": "친구집에 친구는 없는데 말티쥬만 있음 vs 친구집에 말티쥬 없고 친구만 있음",
          "speaker_id": "2",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.22",
          "form": "아~~~~",
          "original_form": "아~~~~",
          "speaker_id": "1",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.23",
          "form": "가고싶은 곳은?",
          "original_form": "가고싶은 곳은?",
          "speaker_id": "2",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.24",
          "form": "2번 하겠습니다",
          "original_form": "2번 하겠습니다",
          "speaker_id": "1",
          "time": "20210823 03:28"
        },
        {
          "id": "MDRW2100009683.1.25",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.26",
          "form": "와 우정.",
          "original_form": "와 우정.",
          "speaker_id": "2",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.27",
          "form": "1번은 자주",
          "original_form": "1번은 자주",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.28",
          "form": "있엇습니다",
          "original_form": "있엇습니다",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.30",
          "form": "이미",
          "original_form": "이미",
          "speaker_id": "2",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.31",
          "form": "말티쥬 독차지한 경험이 있군요",
          "original_form": "말티쥬 독차지한 경험이 있군요",
          "speaker_id": "2",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.32",
          "form": "한 번은 친구집 내놔서",
          "original_form": "한 번은 친구집 내놔서",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.33",
          "form": "집보러오는데",
          "original_form": "집보러오는데",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.34",
          "form": "제가 강아지 끌어안고",
          "original_form": "제가 강아지 끌어안고",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.35",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.36",
          "form": "손님 맞았어요",
          "original_form": "손님 맞았어요",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.37",
          "form": "걔가 사람보면 하도 짖어서",
          "original_form": "걔가 사람보면 하도 짖어서",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.38",
          "form": "손님 집 안보고 말티쥬 보지않나요",
          "original_form": "손님 집 안보고 말티쥬 보지않나요",
          "speaker_id": "2",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.39",
          "form": "안고 있어야했거든요",
          "original_form": "안고 있어야했거든요",
          "speaker_id": "1",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.40",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210823 03:29"
        },
        {
          "id": "MDRW2100009683.1.41",
          "form": "역시 말티즈는 참지않는구나",
          "original_form": "역시 말티즈는 참지않는구나",
          "speaker_id": "2",
          "time": "20210823 03:30"
        },
        {
          "id": "MDRW2100009683.1.42",
          "form": "어린애들은 말티쥬만 보고",
          "original_form": "어린애들은 말티쥬만 보고",
          "speaker_id": "1",
          "time": "20210823 03:30"
        },
        {
          "id": "MDRW2100009683.1.43",
          "form": "말티즈는 절대!....",
          "original_form": "말티즈는 절대!....",
          "speaker_id": "1",
          "time": "20210823 03:30"
        },
        {
          "id": "MDRW2100009683.1.44",
          "form": "말티즈랑 치와와 중에 성격좋은 놈을 못봤어요",
          "original_form": "말티즈랑 치와와 중에 성격좋은 놈을 못봤어요",
          "speaker_id": "2",
          "time": "20210823 03:30"
        },
        {
          "id": "MDRW2100009683.1.45",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "1",
          "time": "20210823 03:30"
        },
        {
          "id": "MDRW2100009683.1.46",
          "form": "시바견은",
          "original_form": "시바견은",
          "speaker_id": "2",
          "time": "20210823 03:30"
        },
        {
          "id": "MDRW2100009683.1.47",
          "form": "그래도 한두마리 있는데",
          "original_form": "그래도 한두마리 있는데",
          "speaker_id": "2",
          "time": "20210823 03:30"
        },
        {
          "id": "MDRW2100009683.1.48",
          "form": "말티즈는 열이면 열",
          "original_form": "말티즈는 열이면 열",
          "speaker_id": "2",
          "time": "20210823 03:30"
        }
      ]
    }
