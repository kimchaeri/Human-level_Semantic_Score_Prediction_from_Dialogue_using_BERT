      "utterance": [
        {
          "id": "MDRW2100002010.1.1",
          "form": "최애 수업은 뭐였어요 한학기동안",
          "original_form": "최애 수업은 뭐였어요 한학기동안",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002010.1.2",
          "form": "수업이라.. 다 재미없었는데",
          "original_form": "수업이라.. 다 재미없었는데",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002010.1.3",
          "form": "굳이 고르자면",
          "original_form": "굳이 고르자면",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002010.1.4",
          "form": "K-VALUEUP 미래설계라구 있는데",
          "original_form": "K-VALUEUP 미래설계라구 있는데",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002010.1.5",
          "form": "교양과목이라",
          "original_form": "교양과목이라",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002010.1.6",
          "form": "대충 넘어가서 재밌었죠 ㅎㅎ",
          "original_form": "대충 넘어가서 재밌었죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002010.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ다 재미없었다니ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ다 재미없었다니ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002010.1.8",
          "form": "미래설계 재밌어 보이는데",
          "original_form": "미래설계 재밌어 보이는데",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002010.1.9",
          "form": "첫학기 학점은 어때요",
          "original_form": "첫학기 학점은 어때요",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002010.1.10",
          "form": "아니이.. 수업이 재밌을 수가 있나요오..",
          "original_form": "아니이.. 수업이 재밌을 수가 있나요오..",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002010.1.11",
          "form": "학점..",
          "original_form": "학점..",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002010.1.12",
          "form": "망했지요..",
          "original_form": "망했지요..",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002010.1.13",
          "form": "괜찮아요 저는 1학년때 학고도 맞아봤어요",
          "original_form": "괜찮아요 저는 1학년때 학고도 맞아봤어요",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002010.1.14",
          "form": "어떻게든 살아지더라고요",
          "original_form": "어떻게든 살아지더라고요",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002010.1.15",
          "form": "3.57...",
          "original_form": "3.57...",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002010.1.16",
          "form": "망했지요~ ㅎㅎ",
          "original_form": "망했지요~ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002010.1.17",
          "form": "뭐 2학기가 있으니!!",
          "original_form": "뭐 2학기가 있으니!!",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002010.1.18",
          "form": "아뭐야",
          "original_form": "아뭐야",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002010.1.19",
          "form": "이게 약간 이런 건가요",
          "original_form": "이게 약간 이런 건가요",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002010.1.20",
          "form": "뭐가요?",
          "original_form": "뭐가요?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002010.1.21",
          "form": "전교1등이 하나 틀렸다고 망했다고 하는 이런 느낌",
          "original_form": "전교1등이 하나 틀렸다고 망했다고 하는 이런 느낌",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002010.1.22",
          "form": "아니이",
          "original_form": "아니이",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002010.1.23",
          "form": "에이",
          "original_form": "에이",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002010.1.24",
          "form": "제 친구",
          "original_form": "제 친구",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002010.1.25",
          "form": "4.37이였나",
          "original_form": "4.37이였나",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002010.1.26",
          "form": "다들 저보다 높더라구요..",
          "original_form": "다들 저보다 높더라구요..",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002010.1.27",
          "form": "전 딱 반에서 평균인데..",
          "original_form": "전 딱 반에서 평균인데..",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002010.1.28",
          "form": "다들 성실하시네요",
          "original_form": "다들 성실하시네요",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002010.1.29",
          "form": "원래 1학년은 밤새 술먹고 수업 째는게 국룰 아닌가요",
          "original_form": "원래 1학년은 밤새 술먹고 수업 째는게 국룰 아닌가요",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002010.1.30",
          "form": "많이 바뀌었네요..",
          "original_form": "많이 바뀌었네요..",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002010.1.31",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002010.1.32",
          "form": "수업도 째나요",
          "original_form": "수업도 째나요",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002010.1.33",
          "form": "조금 좋아지긴 했네요 ㅋㅋㅋㅋㅋ",
          "original_form": "조금 좋아지긴 했네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002010.1.34",
          "form": "저는 한 2학년때까진 9시 수업을 가본 기억이 없어요",
          "original_form": "저는 한 2학년때까진 9시 수업을 가본 기억이 없어요",
          "speaker_id": "1",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002010.1.35",
          "form": "저희는 그래도 수업한번 놓치면 어쩔줄 몰라하던데",
          "original_form": "저희는 그래도 수업한번 놓치면 어쩔줄 몰라하던데",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002010.1.36",
          "form": "약간 이렇게 살면 안되는 사람의 좋은 예",
          "original_form": "약간 이렇게 살면 안되는 사람의 좋은 예",
          "speaker_id": "1",
          "time": "20210819 22:55"
        }
      ]
