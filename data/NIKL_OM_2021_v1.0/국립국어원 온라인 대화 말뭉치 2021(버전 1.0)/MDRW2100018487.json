      "utterance": [
        {
          "id": "MDRW2100018487.1.1",
          "form": "우리의 결혼 어캐할건데 자갸?",
          "original_form": "우리의 결혼 어캐할건데 자갸?",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100018487.1.2",
          "form": "난 기왕이면 신라호텔에서 하면 좋겟어",
          "original_form": "난 기왕이면 신라호텔에서 하면 좋겟어",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100018487.1.3",
          "form": "기왕 한번 하는 결혼인데",
          "original_form": "기왕 한번 하는 결혼인데",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100018487.1.4",
          "form": "동네 웨딩 센터에서 하면 없어보여ㅠ",
          "original_form": "동네 웨딩 센터에서 하면 없어보여ㅠ",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100018487.1.5",
          "form": "식대 3만언 이런데 사람 부르면 하ㅠ 쪽팔려 자갸",
          "original_form": "식대 3만언 이런데 사람 부르면 하ㅠ 쪽팔려 자갸",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100018487.1.6",
          "form": "ㅠㅠ 3만원짜리 식사는",
          "original_form": "ㅠㅠ 3만원짜리 식사는",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100018487.1.7",
          "form": "평소에 먹으면 비싼 식사인데...",
          "original_form": "평소에 먹으면 비싼 식사인데...",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100018487.1.8",
          "form": "간소하게 하자... 스몰웨딩으로",
          "original_form": "간소하게 하자... 스몰웨딩으로",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100018487.1.9",
          "form": "이효리였나? 스몰웨딩한 연예인처럼...",
          "original_form": "이효리였나? 스몰웨딩한 연예인처럼...",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100018487.1.10",
          "form": "하지만... 결혼식장에선 쪽팔린걸!",
          "original_form": "하지만... 결혼식장에선 쪽팔린걸!",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100018487.1.11",
          "form": "스몰웨딩 너무 가성비 웨딩 느낌인걸",
          "original_form": "스몰웨딩 너무 가성비 웨딩 느낌인걸",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100018487.1.12",
          "form": "희망하지않아.",
          "original_form": "희망하지않아.",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100018487.1.13",
          "form": "신라호텔 정도는 되어야...",
          "original_form": "신라호텔 정도는 되어야...",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100018487.1.14",
          "form": "결혼에 식장이 중요할까?",
          "original_form": "결혼에 식장이 중요할까?",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100018487.1.15",
          "form": "난 중요한 건 마음이라고 생각해...",
          "original_form": "난 중요한 건 마음이라고 생각해...",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100018487.1.16",
          "form": "너는 나에대한 마음이 부족한가봐",
          "original_form": "너는 나에대한 마음이 부족한가봐",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100018487.1.17",
          "form": "우리 12년 됐는데...",
          "original_form": "우리 12년 됐는데...",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100018487.1.18",
          "form": "너야말로 나에 대한 마음이 부족해",
          "original_form": "너야말로 나에 대한 마음이 부족해",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100018487.1.19",
          "form": "12년이면 장기를 팔아서라도",
          "original_form": "12년이면 장기를 팔아서라도",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100018487.1.20",
          "form": "신라호텔 예식장을 구해와야지!!!",
          "original_form": "신라호텔 예식장을 구해와야지!!!",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100018487.1.21",
          "form": "넌 지금 날 가스라이팅 하고있어!!",
          "original_form": "넌 지금 날 가스라이팅 하고있어!!",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100018487.1.22",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100018487.1.23",
          "form": "너가 식장이랑 집 해오는거아니었어...?",
          "original_form": "너가 식장이랑 집 해오는거아니었어...?",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100018487.1.24",
          "form": "하...ㅠ",
          "original_form": "하...ㅠ",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100018487.1.25",
          "form": "속상해",
          "original_form": "속상해",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100018487.1.26",
          "form": "아니 왜 내가 해와야해? 어이가 없네",
          "original_form": "아니 왜 내가 해와야해? 어이가 없네",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100018487.1.27",
          "form": "내개 누누히 말했지",
          "original_form": "내개 누누히 말했지",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100018487.1.28",
          "form": "난 취집할거라고!!!!!",
          "original_form": "난 취집할거라고!!!!!",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100018487.1.29",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100018487.1.30",
          "form": "우리 실버타운갈거잖아",
          "original_form": "우리 실버타운갈거잖아",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100018487.1.31",
          "form": "실버타운에 뭔 취집이야...?",
          "original_form": "실버타운에 뭔 취집이야...?",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100018487.1.32",
          "form": "뭐??",
          "original_form": "뭐??",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100018487.1.33",
          "form": "너 나랑 결혼해준가며",
          "original_form": "너 나랑 결혼해준가며",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100018487.1.34",
          "form": "결혼해준다며!!!!!",
          "original_form": "결혼해준다며!!!!!",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100018487.1.35",
          "form": "결혼은해주는데",
          "original_form": "결혼은해주는데",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100018487.1.36",
          "form": "너가집해오는조건으로",
          "original_form": "너가집해오는조건으로",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100018487.1.37",
          "form": "결혼하는거야ㅡㅡ",
          "original_form": "결혼하는거야ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100018487.1.38",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100018487.1.39",
          "form": "한입으로 두말하지마",
          "original_form": "한입으로 두말하지마",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100018487.1.40",
          "form": "너가 내 취집 시켜준댓어",
          "original_form": "너가 내 취집 시켜준댓어",
          "speaker_id": "2",
