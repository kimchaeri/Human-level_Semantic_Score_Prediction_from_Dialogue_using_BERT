        },
        {
          "id": "MDRW2100030898.1.2",
          "form": "안뇽",
          "original_form": "안뇽",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030898.1.3",
          "form": "학교 다니는 중이야?",
          "original_form": "학교 다니는 중이야?",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030898.1.4",
          "form": "학교다니는 중이야?",
          "original_form": "학교다니는 중이야?",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.5",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.8",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.9",
          "form": "난 졸업했어",
          "original_form": "난 졸업했어",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.10",
          "form": "너 뭐야",
          "original_form": "너 뭐야",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.11",
          "form": "왜 자꾸 내가할 말 해 ㅠㅠ",
          "original_form": "왜 자꾸 내가할 말 해 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.12",
          "form": "나도 졸업했어",
          "original_form": "나도 졸업했어",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030898.1.13",
          "form": "ㅋㅋㅋ아웃겨",
          "original_form": "ㅋㅋㅋ아웃겨",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030898.1.14",
          "form": "직장인이야?",
          "original_form": "직장인이야?",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030898.1.15",
          "form": "과거를 추억해볼까?",
          "original_form": "과거를 추억해볼까?",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030898.1.16",
          "form": "동아리는 했어?",
          "original_form": "동아리는 했어?",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030898.1.17",
          "form": "사실 나 동아리를 안들었어ㅜㅜ",
          "original_form": "사실 나 동아리를 안들었어ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030898.1.18",
          "form": "과생활만 함!",
          "original_form": "과생활만 함!",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030898.1.19",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030898.1.20",
          "form": "나는 1학년 1학기에 발만 담갔다가 탈출했어",
          "original_form": "나는 1학년 1학기에 발만 담갔다가 탈출했어",
          "speaker_id": "2",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030898.1.21",
          "form": "오 별로여서?",
          "original_form": "오 별로여서?",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030898.1.22",
          "form": "응 가입하자 마자 첫 행사가 너무 별로였어 ㅋㅋ 봉사 동아리 들었는데 왜 춤추라고 하지...?",
          "original_form": "응 가입하자 마자 첫 행사가 너무 별로였어 ㅋㅋ 봉사 동아리 들었는데 왜 춤추라고 하지...?",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030898.1.23",
          "form": "동아리는 탈퇴하고 나중에 학회들갔는데 그건 좋았어!",
          "original_form": "동아리는 탈퇴하고 나중에 학회들갔는데 그건 좋았어!",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030898.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030898.1.25",
          "form": "장기자랑..?",
          "original_form": "장기자랑..?",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030898.1.26",
          "form": "진짜싫다",
          "original_form": "진짜싫다",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030898.1.27",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030898.1.28",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030898.1.29",
          "form": "재미도없고의미도없는거같아",
          "original_form": "재미도없고의미도없는거같아",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030898.1.30",
          "form": "학회간게 다행이다!",
          "original_form": "학회간게 다행이다!",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030898.1.31",
          "form": "학회는 학회 활동도 그냥저냥 괜찮긴했는데 학회에 모인 동가들이랑  너무 잘 맞아서 좋았어",
          "original_form": "학회는 학회 활동도 그냥저냥 괜찮긴했는데 학회에 모인 동가들이랑  너무 잘 맞아서 좋았어",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030898.1.32",
          "form": "*동기",
          "original_form": "*동기",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030898.1.33",
          "form": "너는 무슨 학과였어? 너무 자세하게는 말고 자연대나 사과대 처럼 큼직한 분류로!",
          "original_form": "너는 무슨 학과였어? 너무 자세하게는 말고 자연대나 사과대 처럼 큼직한 분류로!",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030898.1.34",
          "form": "팀플 많았니...?",
          "original_form": "팀플 많았니...?",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030898.1.35",
          "form": "오 나는 예체능이었엉",
          "original_form": "오 나는 예체능이었엉",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100030898.1.36",
          "form": "ㅋㅋㅋ 거의 솔플가득한 학과생활이었어",
          "original_form": "ㅋㅋㅋ 거의 솔플가득한 학과생활이었어",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100030898.1.37",
          "form": "오오 예체능이었구나! 일학년 때 체대생 친구랑 얘기 자주 했었는데!",
          "original_form": "오오 예체능이었구나! 일학년 때 체대생 친구랑 얘기 자주 했었는데!",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100030898.1.38",
          "form": "나는 사과대였는데 팀플 과제가 너무 많아서 힘들었어...",
          "original_form": "나는 사과대였는데 팀플 과제가 너무 많아서 힘들었어...",
          "speaker_id": "2",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100030898.1.39",
          "form": "헐 그렇겠다",
          "original_form": "헐 그렇겠다",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100030898.1.40",
          "form": "팀플.. 나도 많이한건아니지만 정말..ㅋ",
          "original_form": "팀플.. 나도 많이한건아니지만 정말..ㅋ",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100030898.1.41",
          "form": "발암이지",
          "original_form": "발암이지",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100030898.1.42",
          "form": "진.짜.싫.어.",
          "original_form": "진.짜.싫.어.",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100030898.1.43",
          "form": "ㅋㅋㅋ짧은 말에 모든게 다느껴져",
          "original_form": "ㅋㅋㅋ짧은 말에 모든게 다느껴져",
          "speaker_id": "1",
          "time": "20210904 21:15"
        }
      ]
    }
