      "utterance": [
        {
          "id": "MDRW2100037457.1.1",
          "form": "name2님은\r\n아이들이 몇 살이에요?",
          "original_form": "&name2&님은\r\n아이들이 몇 살이에요?",
          "speaker_id": "1",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100037457.1.2",
          "form": "6살 유치원생이에요^^",
          "original_form": "6살 유치원생이에요^^",
          "speaker_id": "2",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100037457.1.3",
          "form": "아이 한 명인가요? ^^",
          "original_form": "아이 한 명인가요? ^^",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037457.1.4",
          "form": "쌍둥이에요 ㅎㅎ",
          "original_form": "쌍둥이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037457.1.5",
          "form": "우왕:) 쌍둥이였군요^^",
          "original_form": "우왕:) 쌍둥이였군요^^",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037457.1.6",
          "form": "남자아이들인가요?",
          "original_form": "남자아이들인가요?",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037457.1.7",
          "form": "딸들이에요^^ 일란성",
          "original_form": "딸들이에요^^ 일란성",
          "speaker_id": "2",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100037457.1.8",
          "form": "부럽네요:)",
          "original_form": "부럽네요:)",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037457.1.9",
          "form": "아들키우시나봐용^^",
          "original_form": "아들키우시나봐용^^",
          "speaker_id": "2",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037457.1.10",
          "form": "저는 투아들맘이에요ㅋㅋ",
          "original_form": "저는 투아들맘이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037457.1.11",
          "form": "와우!ㅎㅎㅎ",
          "original_form": "와우!ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037457.1.12",
          "form": "전 모르는사람들이",
          "original_form": "전 모르는사람들이",
          "speaker_id": "2",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037457.1.13",
          "form": "그렇~~게  이제 아들하나만 더 낳으면 되겠네",
          "original_form": "그렇~~게  이제 아들하나만 더 낳으면 되겠네",
          "speaker_id": "2",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037457.1.14",
          "form": "소리 참 많이들었어요ㅜ",
          "original_form": "소리 참 많이들었어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037457.1.15",
          "form": "그래도 동성이 아이들한테는 정말 좋은것같아요^^",
          "original_form": "그래도 동성이 아이들한테는 정말 좋은것같아요^^",
          "speaker_id": "2",
          "time": "20210901 00:32"
        },
        {
          "id": "MDRW2100037457.1.16",
          "form": "맞아요! \r\n아이들만 보면 그게 좋은거 같은데",
          "original_form": "맞아요! \r\n아이들만 보면 그게 좋은거 같은데",
          "speaker_id": "1",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100037457.1.17",
          "form": "또 딸은 있었음 좋겠는\r\n엄마 마음 ㅜㅜㅋ",
          "original_form": "또 딸은 있었음 좋겠는\r\n엄마 마음 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100037457.1.18",
          "form": "ㅎㅎ그쵸ㅠㅠ 저도 하나였으면",
          "original_form": "ㅎㅎ그쵸ㅠㅠ 저도 하나였으면",
          "speaker_id": "2",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100037457.1.19",
          "form": "하나더 낳아볼까 생각했을텐데..",
          "original_form": "하나더 낳아볼까 생각했을텐데..",
          "speaker_id": "2",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100037457.1.20",
          "form": "허허... 한국에선  애둘도 벅차네요ㅜ",
          "original_form": "허허... 한국에선  애둘도 벅차네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100037457.1.21",
          "form": "동시에 돈이드는것두요 ㅠ",
          "original_form": "동시에 돈이드는것두요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100037457.1.22",
          "form": "그러게요ㅠ 아이들 키우는데\r\n들어가는 돈도 어마무시하죠ㅠ",
          "original_form": "그러게요ㅠ 아이들 키우는데\r\n들어가는 돈도 어마무시하죠ㅠ",
