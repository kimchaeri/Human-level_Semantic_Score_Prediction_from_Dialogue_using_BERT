      "utterance": [
        {
          "id": "MDRW2100017359.1.1",
          "form": "오티티 구독해요",
          "original_form": "오티티 구독해요",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017359.1.2",
          "form": "아니 구독했어요?",
          "original_form": "아니 구독했어요?",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017359.1.3",
          "form": "아니요! 어떤 유튜브인가요?",
          "original_form": "아니요! 어떤 유튜브인가요?",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017359.1.4",
          "form": "아니 넷플릭스나 왓챠 이런거요~",
          "original_form": "아니 넷플릭스나 왓챠 이런거요~",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017359.1.5",
          "form": "아하 그런거군요! 유튜브채널인줄알았어요!ㅋㅋㅋ",
          "original_form": "아하 그런거군요! 유튜브채널인줄알았어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017359.1.6",
          "form": "저는 왓챠는 있어요!",
          "original_form": "저는 왓챠는 있어요!",
          "speaker_id": "2",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017359.1.7",
          "form": "아 제가 실수로 엔터쳐서 ㅎㅎ",
          "original_form": "아 제가 실수로 엔터쳐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017359.1.8",
          "form": "왓챠에 볼거리 많나요?",
          "original_form": "왓챠에 볼거리 많나요?",
          "speaker_id": "1",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017359.1.9",
          "form": "왓챠랑 냇플릭스랑 서로 없는거랑 있는게 반대인거같더라구요!ㅋㅋㅋ",
          "original_form": "왓챠랑 냇플릭스랑 서로 없는거랑 있는게 반대인거같더라구요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017359.1.10",
          "form": "저는 디즈니플러스들어오면 디즈니플러스 구독하고싶어요!",
          "original_form": "저는 디즈니플러스들어오면 디즈니플러스 구독하고싶어요!",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017359.1.11",
          "form": "앗 맞아요 ㅋㅋ 잘아시네요.",
          "original_form": "앗 맞아요 ㅋㅋ 잘아시네요.",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017359.1.12",
          "form": "저도 디즈니플러스 구독하고 싶어요 ㅜㅜ 언제 들어와요 그거?",
          "original_form": "저도 디즈니플러스 구독하고 싶어요 ㅜㅜ 언제 들어와요 그거?",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017359.1.13",
          "form": "디즈니플러스 11월쯤에 우리나라 들어오는거같더라구요!",
          "original_form": "디즈니플러스 11월쯤에 우리나라 들어오는거같더라구요!",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017359.1.14",
          "form": "마블 드라마들 보고싶어요ㅠㅠ",
          "original_form": "마블 드라마들 보고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017359.1.15",
          "form": "오~ 마블 드마라도 있어요?",
          "original_form": "오~ 마블 드마라도 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017359.1.16",
          "form": "디즈니플러스 들어오는거 확정인가요?",
          "original_form": "디즈니플러스 들어오는거 확정인가요?",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017359.1.17",
          "form": "네 확정인더같아요!",
          "original_form": "네 확정인더같아요!",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017359.1.18",
          "form": "마블 드라마도 재미있어요!!ㅎㅎ",
          "original_form": "마블 드라마도 재미있어요!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017359.1.19",
          "form": "저는 마블 영화 밖에 안봤어요",
          "original_form": "저는 마블 영화 밖에 안봤어요",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017359.1.20",
          "form": "네이버 영화에도 마블 영화 있어서 어찌나 반가우던지!",
          "original_form": "네이버 영화에도 마블 영화 있어서 어찌나 반가우던지!",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017359.1.21",
          "form": "제가 마블영화를 엄청 좋아해서 드라마도 얼른 보고싶어요!",
          "original_form": "제가 마블영화를 엄청 좋아해서 드라마도 얼른 보고싶어요!",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017359.1.22",
          "form": "드라마로 나오면 매주 한회씩 나오는거에요?",
          "original_form": "드라마로 나오면 매주 한회씩 나오는거에요?",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017359.1.23",
          "form": "네! 지금 여러개 나왔는데 로키는 아직 못봤어요!",
          "original_form": "네! 지금 여러개 나왔는데 로키는 아직 못봤어요!",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017359.1.24",
          "form": "아 로키 은근 매력이던데 아닌가요?ㅋㅋㅋ",
          "original_form": "아 로키 은근 매력이던데 아닌가요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017359.1.25",
          "form": "로키가 그 토르 동생 맞죠?",
          "original_form": "로키가 그 토르 동생 맞죠?",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017359.1.26",
          "form": "네 맞아요!! 빌럼중에 인기 가장 많은더같아요ㅋㅋㅋㅋ",
          "original_form": "네 맞아요!! 빌럼중에 인기 가장 많은더같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:34"
        }
      ]
