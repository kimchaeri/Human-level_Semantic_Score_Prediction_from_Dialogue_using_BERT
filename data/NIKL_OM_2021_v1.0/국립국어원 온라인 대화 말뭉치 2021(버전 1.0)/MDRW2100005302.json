      "utterance": [
        {
          "id": "MDRW2100005302.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005302.1.2",
          "form": "낮에 비 많이 왔는데 지금은 그쳤어요",
          "original_form": "낮에 비 많이 왔는데 지금은 그쳤어요",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005302.1.3",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005302.1.4",
          "form": "그쪽도 지금은 그쳤어요?",
          "original_form": "그쪽도 지금은 그쳤어요?",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005302.1.5",
          "form": "네 이제 쌀쌀해뇨",
          "original_form": "네 이제 쌀쌀해뇨",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005302.1.6",
          "form": "좋네요. 시원한게 최고죠",
          "original_form": "좋네요. 시원한게 최고죠",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005302.1.7",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005302.1.8",
          "form": "얼마전까지만해도 밤에도 얼마나 덥던지요...",
          "original_form": "얼마전까지만해도 밤에도 얼마나 덥던지요...",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005302.1.9",
          "form": "아침에도 더웠고... 요즘 정말 시원하고 쾌적해요",
          "original_form": "아침에도 더웠고... 요즘 정말 시원하고 쾌적해요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005302.1.10",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005302.1.11",
          "form": "이번 여름에 에어컨 많이 트셨어요?",
          "original_form": "이번 여름에 에어컨 많이 트셨어요?",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005302.1.12",
          "form": "네 지옥이더라구요",
          "original_form": "네 지옥이더라구요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005302.1.13",
          "form": "ㅋㅋㅋㅋ 맞아요 이번 여름 진짜 지옥불이었어요 최악",
          "original_form": "ㅋㅋㅋㅋ 맞아요 이번 여름 진짜 지옥불이었어요 최악",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005302.1.14",
          "form": "우리집은 에어컨 잘 못틀었는데 부럽네요 ㅠㅠ",
          "original_form": "우리집은 에어컨 잘 못틀었는데 부럽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005302.1.15",
          "form": "더운게 아니고 뜨겁더라구요",
          "original_form": "더운게 아니고 뜨겁더라구요",
          "speaker_id": "1",
