          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043087.1.4",
          "form": "님 영화 뭐볼거",
          "original_form": "님 영화 뭐볼거",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043087.1.5",
          "form": "님차례에",
          "original_form": "님차례에",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043087.1.6",
          "form": "아지구내차례아니잔ㄹ아",
          "original_form": "아지구내차례아니잔ㄹ아",
          "speaker_id": "1",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043087.1.7",
          "form": "샹치는 안볼거?",
          "original_form": "샹치는 안볼거?",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043087.1.8",
          "form": "볼건데 시간이없네",
          "original_form": "볼건데 시간이없네",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.9",
          "form": "프리가이는 봤었니",
          "original_form": "프리가이는 봤었니",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.10",
          "form": "오늘은 안돼",
          "original_form": "오늘은 안돼",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.11",
          "form": "봄봄",
          "original_form": "봄봄",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.12",
          "form": "프리가이 잼지",
          "original_form": "프리가이 잼지",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.13",
          "form": "존잼",
          "original_form": "존잼",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.14",
          "form": "라이언조아",
          "original_form": "라이언조아",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.15",
          "form": "그런 영화 또 없나 보고싶다",
          "original_form": "그런 영화 또 없나 보고싶다",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.16",
          "form": "라이언 최고야",
          "original_form": "라이언 최고야",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.17",
          "form": "게임기반??",
          "original_form": "게임기반??",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.18",
          "form": "데드풀이랑",
          "original_form": "데드풀이랑",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.19",
          "form": "킬러의보디가드때알아봤어",
          "original_form": "킬러의보디가드때알아봤어",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.20",
          "form": "있던데 무슨..게임캐릭타나오는거",
          "original_form": "있던데 무슨..게임캐릭타나오는거",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.21",
          "form": "아 뭔가 신박한 영화보고싶다",
          "original_form": "아 뭔가 신박한 영화보고싶다",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043087.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ보디가드 그거 3안나올까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ보디가드 그거 3안나올까",
