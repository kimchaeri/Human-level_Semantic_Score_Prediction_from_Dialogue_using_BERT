        },
        {
          "id": "MDRW2100037760.1.3",
          "form": "뭐 파는 데야?",
          "original_form": "뭐 파는 데야?",
          "speaker_id": "1",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100037760.1.4",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100037760.1.5",
          "form": "옷 파는곳",
          "original_form": "옷 파는곳",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100037760.1.6",
          "form": "거의 옷 많이 팔아",
