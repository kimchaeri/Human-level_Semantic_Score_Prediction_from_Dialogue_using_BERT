      "utterance": [
        {
          "id": "MDRW2100024134.1.1",
          "form": "ㅋㅋㅋㅋㅋ더 어린 학생 자주보시자나요",
          "original_form": "ㅋㅋㅋㅋㅋ더 어린 학생 자주보시자나요",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024134.1.2",
          "form": "우리반 애들 09년생이에요",
          "original_form": "우리반 애들 09년생이에요",
          "speaker_id": "1",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.3",
          "form": "저 김남길 좋아해요",
          "original_form": "저 김남길 좋아해요",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.4",
          "form": "우와 09년생ㄷㄷ",
          "original_form": "우와 09년생ㄷㄷ",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.5",
          "form": "김남길이요?",
          "original_form": "김남길이요?",
          "speaker_id": "1",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.6",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.7",
          "form": "누군지 잘 모르는데",
          "original_form": "누군지 잘 모르는데",
          "speaker_id": "1",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.8",
          "form": "송중기랑 김선호도",
          "original_form": "송중기랑 김선호도",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.9",
          "form": "소개해줄 수 있어요?",
          "original_form": "소개해줄 수 있어요?",
          "speaker_id": "1",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.10",
          "form": "꺅 송중기랑 김선호 ㅠ 넘 좋아요",
          "original_form": "꺅 송중기랑 김선호 ㅠ 넘 좋아요",
          "speaker_id": "1",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024134.1.12",
          "form": "뭔가 말랑말랑한 상 좋아요",
          "original_form": "뭔가 말랑말랑한 상 좋아요",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.13",
          "form": "검색해봐도 누군지 모르겠네",
          "original_form": "검색해봐도 누군지 모르겠네",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.14",
          "form": "김남길은 열혈사제 나온 사람이에용",
          "original_form": "김남길은 열혈사제 나온 사람이에용",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.15",
          "form": "아 열혈사제 안봐서 잘 모르겠어요 ㅜㅜ",
          "original_form": "아 열혈사제 안봐서 잘 모르겠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.16",
          "form": "이분 몇살이에요",
          "original_form": "이분 몇살이에요",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.17",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.18",
          "form": "82년생쯤 됏단가",
          "original_form": "82년생쯤 됏단가",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.19",
          "form": "무슨 포털 검색에 나이도 안나와요",
          "original_form": "무슨 포털 검색에 나이도 안나와요",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.21",
          "form": "40대이시구나",
          "original_form": "40대이시구나",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024134.1.22",
          "form": "마흔즈음 되면 다들 지우더라구요",
          "original_form": "마흔즈음 되면 다들 지우더라구요",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024134.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ아핰ㅋ 신비로 돌아서는군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아핰ㅋ 신비로 돌아서는군요",
