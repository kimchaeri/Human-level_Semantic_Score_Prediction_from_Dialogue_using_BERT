      "utterance": [
        {
          "id": "MDRW2100005586.1.1",
          "form": "나 강원도로 여행가보고 싶음",
          "original_form": "나 강원도로 여행가보고 싶음",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005586.1.2",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005586.1.3",
          "form": "ㅁㄹ 걍 낯선 곳이라는 이미지라서 그런지",
          "original_form": "ㅁㄹ 걍 낯선 곳이라는 이미지라서 그런지",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005586.1.4",
          "form": "강원도로 여행가면 색다른게 있을 거 같음",
          "original_form": "강원도로 여행가면 색다른게 있을 거 같음",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005586.1.5",
          "form": "강원도 겨울에 가면",
          "original_form": "강원도 겨울에 가면",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005586.1.6",
          "form": "눈 많이 와서 조을ㄹ듯",
          "original_form": "눈 많이 와서 조을ㄹ듯",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005586.1.7",
          "form": "오 그니까",
          "original_form": "오 그니까",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005586.1.8",
          "form": "거긴 눈이 허리까지 온다매",
          "original_form": "거긴 눈이 허리까지 온다매",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005586.1.9",
          "form": "여행 갔다가 고립되는 거 아녀",
          "original_form": "여행 갔다가 고립되는 거 아녀",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005586.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005586.1.11",
          "form": "나는 전주쪽도 가보고싶어",
          "original_form": "나는 전주쪽도 가보고싶어",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005586.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005586.1.13",
          "form": "전주는 왜?",
          "original_form": "전주는 왜?",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005586.1.14",
          "form": "맛집이 많대서",
          "original_form": "맛집이 많대서",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005586.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005586.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005586.1.17",
          "form": "나 수학여행",
          "original_form": "나 수학여행",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005586.1.18",
          "form": "전주로 갔다 왔는데 확실히 먹을 거 많더라",
          "original_form": "전주로 갔다 왔는데 확실히 먹을 거 많더라",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005586.1.19",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005586.1.20",
          "form": "어디 갔었는데",
          "original_form": "어디 갔었는데",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005586.1.21",
          "form": "가봤자",
          "original_form": "가봤자",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005586.1.22",
          "form": "한옥마을이지 뭐...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "한옥마을이지 뭐...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005586.1.23",
          "form": "ㅇㅎㅇㅎ",
          "original_form": "ㅇㅎㅇㅎ",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005586.1.24",
          "form": "근제 한옥마을 안에서도 먹을게 많음",
          "original_form": "근제 한옥마을 안에서도 먹을게 많음",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005586.1.25",
          "form": "문어 꼬지 맛있었어",
          "original_form": "문어 꼬지 맛있었어",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005586.1.26",
          "form": "뭐가 맛있었어?",
          "original_form": "뭐가 맛있었어?",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005586.1.27",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005586.1.28",
          "form": "그거 말곤 딱히 기억나는 음식이 없다...",
          "original_form": "그거 말곤 딱히 기억나는 음식이 없다...",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005586.1.29",
          "form": "왜냐면 거기서 설빙 갔음...",
          "original_form": "왜냐면 거기서 설빙 갔음...",
          "speaker_id": "2",
          "time": "20210821 23:24"
        }
      ]
    }
  ]
