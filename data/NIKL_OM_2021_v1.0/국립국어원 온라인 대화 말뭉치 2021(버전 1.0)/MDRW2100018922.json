      "utterance": [
        {
          "id": "MDRW2100018922.1.1",
          "form": "일하고 계신가요",
          "original_form": "일하고 계신가요",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018922.1.2",
          "form": "네 그렇습니다",
          "original_form": "네 그렇습니다",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018922.1.3",
          "form": "저도요. 업무 스트레스는 없으세요?",
          "original_form": "저도요. 업무 스트레스는 없으세요?",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018922.1.4",
          "form": "많죠 ㅎㅎ업무도사람도쉬운게 없네요",
          "original_form": "많죠 ㅎㅎ업무도사람도쉬운게 없네요",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018922.1.5",
          "form": "상사땜에 힘드세요?",
          "original_form": "상사땜에 힘드세요?",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018922.1.6",
          "form": "상사도 아랫사람도 사람이 얽히면 다 ㅋㅋㅋㅋ",
          "original_form": "상사도 아랫사람도 사람이 얽히면 다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018922.1.7",
          "form": "그렇겠네요. 전 혼자 일해서 사람 스트레스는 없어요 ㅎ",
          "original_form": "그렇겠네요. 전 혼자 일해서 사람 스트레스는 없어요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018922.1.8",
          "form": "아 완전 혼자 하세요?",
          "original_form": "아 완전 혼자 하세요?",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018922.1.9",
          "form": "네 사장님 한 분 있고 직원은 저만",
          "original_form": "네 사장님 한 분 있고 직원은 저만",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018922.1.10",
          "form": "아 작네요 규모가",
          "original_form": "아 작네요 규모가",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018922.1.11",
          "form": "글쵸^^ 그래서 편하게 일하고 있어요\r\n직원 몇 명 근무 하나요?",
          "original_form": "글쵸^^ 그래서 편하게 일하고 있어요\r\n직원 몇 명 근무 하나요?",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018922.1.12",
          "form": "저흰 60-70 정도요",
          "original_form": "저흰 60-70 정도요",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100018922.1.13",
          "form": "우와 사람 많네요!! 북적이는 일 터에서 일 해보고 싶기도 하네요",
          "original_form": "우와 사람 많네요!! 북적이는 일 터에서 일 해보고 싶기도 하네요",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100018922.1.14",
          "form": "실이 많아 북적이진 않아요",
          "original_form": "실이 많아 북적이진 않아요",
          "speaker_id": "2",
