      "utterance": [
        {
          "id": "MDRW2100016481.1.1",
          "form": "학교 생활에 대해 얘기해봐요",
          "original_form": "학교 생활에 대해 얘기해봐요",
          "speaker_id": "1",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016481.1.2",
          "form": "수업은 잘 들으셨나요",
          "original_form": "수업은 잘 들으셨나요",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016481.1.3",
          "form": "수업은나름잘듣고",
          "original_form": "수업은나름잘듣고",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016481.1.4",
          "form": "쉬는시간에잠만잤던거같아요",
          "original_form": "쉬는시간에잠만잤던거같아요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016481.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016481.1.6",
          "form": "ㅋㅋㅋㅋ저는 수업시간에도 잘 잤는데",
          "original_form": "ㅋㅋㅋㅋ저는 수업시간에도 잘 잤는데",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016481.1.7",
          "form": "맨날 자는 사람한테 사탕주던 선생님이",
          "original_form": "맨날 자는 사람한테 사탕주던 선생님이",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016481.1.8",
          "form": "맨날 사탕 준 사람 = 저",
          "original_form": "맨날 사탕 준 사람 = 저",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016481.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016481.1.10",
          "form": "사탕득템",
          "original_form": "사탕득템",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016481.1.11",
          "form": "현정쌤생각나네요",
          "original_form": "현정쌤생각나네요",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016481.1.12",
          "form": "아 뭐하시려나",
          "original_form": "아 뭐하시려나",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016481.1.13",
          "form": "이제 50대가 되셨을텐데",
          "original_form": "이제 50대가 되셨을텐데",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016481.1.14",
          "form": "애두 대학 갔겠네여",
          "original_form": "애두 대학 갔겠네여",
          "speaker_id": "1",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016481.1.15",
          "form": "와..애가 대학을 갔을거라니  제나이가 다시한번 실감나네여..",
          "original_form": "와..애가 대학을 갔을거라니  제나이가 다시한번 실감나네여..",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016481.1.16",
          "form": "이미 10년 전도 대학 졸업반의 나이더라구요",
          "original_form": "이미 10년 전도 대학 졸업반의 나이더라구요",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016481.1.17",
          "form": "놀랍네요..학창시절그리워요",
          "original_form": "놀랍네요..학창시절그리워요",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016481.1.18",
          "form": "하지만 대학교때",
          "original_form": "하지만 대학교때",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016481.1.19",
          "form": "발표수업하는건싫었어요 돌아가고싶지않아요",
          "original_form": "발표수업하는건싫었어요 돌아가고싶지않아요",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016481.1.20",
          "form": "나도요 발표수업 싫어요",
          "original_form": "나도요 발표수업 싫어요",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100016481.1.21",
          "form": "근데 지금 돌아가면 잘할 수 있을 거같아요",
          "original_form": "근데 지금 돌아가면 잘할 수 있을 거같아요",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100016481.1.22",
          "form": "ㅋㅋㅋㅋ뭐든잘하실거같아요 name1님은",
          "original_form": "ㅋㅋㅋㅋ뭐든잘하실거같아요 &name1&님은",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016481.1.23",
          "form": "전교1등의위엄",
          "original_form": "전교1등의위엄",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016481.1.24",
          "form": "대진학원의자랑",
          "original_form": "대진학원의자랑",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016481.1.25",
          "form": "name2님도 자랑이었죠",
          "original_form": "&name2&님도 자랑이었죠",
          "speaker_id": "1",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100016481.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ리즈시절ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ리즈시절ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100016481.1.27",
          "form": "중학생 재밌었어요",
          "original_form": "중학생 재밌었어요",
          "speaker_id": "1",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016481.1.28",
          "form": "지금 중딩들보며 약간 부러워하는중",
          "original_form": "지금 중딩들보며 약간 부러워하는중",
