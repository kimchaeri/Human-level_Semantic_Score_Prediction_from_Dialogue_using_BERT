      "utterance": [
        {
          "id": "MDRW2100020534.1.1",
          "form": "배달음식드시나요?",
          "original_form": "배달음식드시나요?",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020534.1.2",
          "form": "네 저는 자주 먹어요~님은요?",
          "original_form": "네 저는 자주 먹어요~님은요?",
          "speaker_id": "2",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100020534.1.3",
          "form": "저도요 중독됐어요",
          "original_form": "저도요 중독됐어요",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100020534.1.4",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100020534.1.5",
          "form": "그쵸?요즘 쿠폰도 자주주니",
          "original_form": "그쵸?요즘 쿠폰도 자주주니",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100020534.1.6",
          "form": "어떤 음식 자주 시키세요?",
          "original_form": "어떤 음식 자주 시키세요?",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100020534.1.7",
          "form": "기름기와 밀가루ㅠ",
          "original_form": "기름기와 밀가루ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100020534.1.8",
          "form": "피자 치킨 스파게티",
          "original_form": "피자 치킨 스파게티",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100020534.1.9",
          "form": "저희는 햄버거 피자네요",
          "original_form": "저희는 햄버거 피자네요",
          "speaker_id": "2",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100020534.1.10",
          "form": "얘들이 좋아해서..",
          "original_form": "얘들이 좋아해서..",
          "speaker_id": "2",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100020534.1.11",
          "form": "자주시키는 배달앱 있으세요?",
          "original_form": "자주시키는 배달앱 있으세요?",
          "speaker_id": "2",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100020534.1.12",
          "form": "전 배민주로 써요",
          "original_form": "전 배민주로 써요",
          "speaker_id": "1",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100020534.1.13",
          "form": "행버거도 배달 괜찮나요?",
          "original_form": "행버거도 배달 괜찮나요?",
          "speaker_id": "1",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100020534.1.14",
          "form": "저는 배민 요기요 쿠팡잇츠 시켜요 롯데리아는 빨리 오더라구요",
          "original_form": "저는 배민 요기요 쿠팡잇츠 시켜요 롯데리아는 빨리 오더라구요",
          "speaker_id": "2",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100020534.1.15",
          "form": "아 햄버거도 먹고싶네요",
          "original_form": "아 햄버거도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100020534.1.16",
          "form": "저녁은 모드세요?",
          "original_form": "저녁은 모드세요?",
          "speaker_id": "1",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100020534.1.17",
          "form": "저녁은 한식위주로 먹으려고 해요",
          "original_form": "저녁은 한식위주로 먹으려고 해요",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100020534.1.18",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100020534.1.19",
          "form": "전 비가오니 칼국수가 땡겨요",
          "original_form": "전 비가오니 칼국수가 땡겨요",
          "speaker_id": "1",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100020534.1.20",
          "form": "아 칼국수 좋네요~해드시나요?",
          "original_form": "아 칼국수 좋네요~해드시나요?",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100020534.1.21",
          "form": "사먹어용ㅋㅋ",
          "original_form": "사먹어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100020534.1.22",
          "form": "요알못ㅠ",
          "original_form": "요알못ㅠ",
          "speaker_id": "1",
