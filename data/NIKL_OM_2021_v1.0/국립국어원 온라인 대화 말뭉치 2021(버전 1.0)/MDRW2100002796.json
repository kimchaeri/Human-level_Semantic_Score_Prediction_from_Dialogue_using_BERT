      "utterance": [
        {
          "id": "MDRW2100002796.1.1",
          "form": "너도 커뮤니티 하지?",
          "original_form": "너도 커뮤니티 하지?",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.2",
          "form": "그러게 커뮤니티에 보면 다양한 커플이 존재해",
          "original_form": "그러게 커뮤니티에 보면 다양한 커플이 존재해",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.3",
          "form": "다들 힘들어하면서도 만나더라",
          "original_form": "다들 힘들어하면서도 만나더라",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.4",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.5",
          "form": "사랑꾼들이 많아",
          "original_form": "사랑꾼들이 많아",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.6",
          "form": "정말 드라마같은 일들도 많아",
          "original_form": "정말 드라마같은 일들도 많아",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.7",
          "form": "커뮤니티는 익명으로",
          "original_form": "커뮤니티는 익명으로",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.8",
          "form": "커뮤니티에는 역시 다양한 사람이 모이나봐",
          "original_form": "커뮤니티에는 역시 다양한 사람이 모이나봐",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002796.1.9",
          "form": "자신의 고민을 이야기하기에 좋은 것 같아",
          "original_form": "자신의 고민을 이야기하기에 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.10",
          "form": "주변인에게 털어놓지 못하는 고민을",
          "original_form": "주변인에게 털어놓지 못하는 고민을",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.11",
          "form": "다 털어놓는구나",
          "original_form": "다 털어놓는구나",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.12",
          "form": "맞아맞다",
          "original_form": "맞아맞다",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.13",
          "form": "커뮤니티의 순기능이자 악기능이구나",
          "original_form": "커뮤니티의 순기능이자 악기능이구나",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.14",
          "form": "근데 익명아라서 말을 함부러하는 사람들도ㅜ많아",
          "original_form": "근데 익명아라서 말을 함부러하는 사람들도ㅜ많아",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.15",
          "form": "나쁜말을 하는 사람들도",
          "original_form": "나쁜말을 하는 사람들도",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.16",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.17",
          "form": "오늘 이심전심이구나.",
          "original_form": "오늘 이심전심이구나.",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.18",
          "form": "그로게",
          "original_form": "그로게",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.19",
          "form": "한명만 말해도되겠어.",
          "original_form": "한명만 말해도되겠어.",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.20",
          "form": "그리고 또 가짜로 일을 지어내는 사람들도 많아",
          "original_form": "그리고 또 가짜로 일을 지어내는 사람들도 많아",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.21",
          "form": "커뮤니티 정모가 있다면 나갈거니?",
          "original_form": "커뮤니티 정모가 있다면 나갈거니?",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.22",
          "form": "맞아.. 관심을 끌려고 그래.",
          "original_form": "맞아.. 관심을 끌려고 그래.",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002796.1.23",
          "form": "음 고민해볼꺼같아",
          "original_form": "음 고민해볼꺼같아",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002796.1.24",
          "form": "안타깝고도 나쁜 사람이야",
          "original_form": "안타깝고도 나쁜 사람이야",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002796.1.25",
          "form": "같은 지역 친구는 만나보고싶어!",
          "original_form": "같은 지역 친구는 만나보고싶어!",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002796.1.26",
          "form": "커뮤니티에 지역별 모임이 있다는 건 좋은거야 정말",
          "original_form": "커뮤니티에 지역별 모임이 있다는 건 좋은거야 정말",
          "speaker_id": "2",
          "time": "20210820 15:40"
        }
      ]
    }
  ]
