        },
        {
          "id": "MDRW2100023118.1.2",
          "form": "티켓팅하고싶어요ㅠ",
          "original_form": "티켓팅하고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 09:25"
        },
        {
          "id": "MDRW2100023118.1.3",
          "form": "name2님의 티켓팅 팁이 있나요?",
          "original_form": "&name2&님의 티켓팅 팁이 있나요?",
          "speaker_id": "1",
          "time": "20210830 09:25"
        },
        {
          "id": "MDRW2100023118.1.4",
          "form": "이제다까뭇어요ㅠㅠ",
          "original_form": "이제다까뭇어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023118.1.5",
          "form": "용병활용!ㅋㅋㅋㅋ",
          "original_form": "용병활용!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100023118.1.6",
          "form": "ㅋㅋㅋ티켓팅잘하는 친구들이있나요?",
          "original_form": "ㅋㅋㅋ티켓팅잘하는 친구들이있나요?",
          "speaker_id": "1",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100023118.1.7",
          "form": "ㅋㅋㅋㅋㅋ 네 그냥 ...많지는 않구요!!!",
          "original_form": "ㅋㅋㅋㅋㅋ 네 그냥 ...많지는 않구요!!!",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023118.1.8",
          "form": "상부상조 합니다 ㅋㅋㅋㅋ",
          "original_form": "상부상조 합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023118.1.9",
          "form": "ㅋㅋ좋네요 티켓예약 사이트는 어디서 가장 강하세요?",
          "original_form": "ㅋㅋ좋네요 티켓예약 사이트는 어디서 가장 강하세요?",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023118.1.10",
          "form": "멜론?",
          "original_form": "멜론?",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023118.1.11",
          "form": "앗 어떻게아샷됴",
          "original_form": "앗 어떻게아샷됴",
          "speaker_id": "2",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100023118.1.12",
          "form": "근데.진짜 다 까먹어서",
          "original_form": "근데.진짜 다 까먹어서",
          "speaker_id": "2",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100023118.1.13",
          "form": "처참하게 망할듯..",
          "original_form": "처참하게 망할듯..",
          "speaker_id": "2",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100023118.1.14",
          "form": "ㅋㅋㅋ저두요 ㅋㅋㅋ근데약간 요즘에 뮤지컬 하긴하는데",
          "original_form": "ㅋㅋㅋ저두요 ㅋㅋㅋ근데약간 요즘에 뮤지컬 하긴하는데",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023118.1.15",
          "form": "옛날만큼 가고싶은 맘조차 없어서",
          "original_form": "옛날만큼 가고싶은 맘조차 없어서",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023118.1.16",
          "form": "티켓팅도 안하고있어여 ㅋㅋㅋ",
          "original_form": "티켓팅도 안하고있어여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023118.1.17",
          "form": "ㅋㅋㅋ 저도 뮤지컬 ...뭐 한번도 안들여다봣네요",
          "original_form": "ㅋㅋㅋ 저도 뮤지컬 ...뭐 한번도 안들여다봣네요",
          "speaker_id": "2",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100023118.1.18",
          "form": "뮤지컬이.. 티켓팅 잘되서 앞열만 가면 ㅋㅋ 진짜 좋긴하더라구요 ㅋㅋㅋ 그치만 뭐....흑흑",
          "original_form": "뮤지컬이.. 티켓팅 잘되서 앞열만 가면 ㅋㅋ 진짜 좋긴하더라구요 ㅋㅋㅋ 그치만 뭐....흑흑",
