      "utterance": [
        {
          "id": "MDRW2100010600.1.1",
          "form": "업무스트레스?",
          "original_form": "업무스트레스?",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010600.1.2",
          "form": "많이 심하세요?",
          "original_form": "많이 심하세요?",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010600.1.3",
          "form": "업무스트레스 엄청 심해요 ㅠ",
          "original_form": "업무스트레스 엄청 심해요 ㅠ",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010600.1.4",
          "form": "이직하고 싶네요",
          "original_form": "이직하고 싶네요",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010600.1.5",
          "form": "사람이 제일 힘든가요?",
          "original_form": "사람이 제일 힘든가요?",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010600.1.6",
          "form": "사람이랑 의사소통이 잘 안되는 거 같아요",
          "original_form": "사람이랑 의사소통이 잘 안되는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010600.1.7",
          "form": "사람이면 단한명?",
          "original_form": "사람이면 단한명?",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010600.1.8",
          "form": "사람들이라고 말하지 않으셔서",
          "original_form": "사람들이라고 말하지 않으셔서",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010600.1.9",
          "form": "특정 한명인가요?",
          "original_form": "특정 한명인가요?",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010600.1.10",
          "form": "아뇨아뇨 사람들이죠....  인수인계 파일이 따로 만들어져 있으면 좋을텐데",
          "original_form": "아뇨아뇨 사람들이죠....  인수인계 파일이 따로 만들어져 있으면 좋을텐데",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010600.1.11",
          "form": "병원에서 일하시나보네요 !",
          "original_form": "병원에서 일하시나보네요 !",
          "speaker_id": "2",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010600.1.12",
          "form": "제동생도 병원에서 일하는데",
          "original_form": "제동생도 병원에서 일하는데",
          "speaker_id": "2",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010600.1.13",
          "form": "그부분이 많이 스트레스 받는다고 해요",
          "original_form": "그부분이 많이 스트레스 받는다고 해요",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010600.1.14",
          "form": "엥 병원에서 말한다고 말 안했는데 거기서 일하긴해요",
          "original_form": "엥 병원에서 말한다고 말 안했는데 거기서 일하긴해요",
          "speaker_id": "1",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010600.1.15",
          "form": "얼마나 일했어요 ?",
          "original_form": "얼마나 일했어요 ?",
          "speaker_id": "2",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010600.1.16",
          "form": "1년 반정도 일하다가 잠시 쉬었어요",
          "original_form": "1년 반정도 일하다가 잠시 쉬었어요",
          "speaker_id": "1",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010600.1.17",
          "form": "name2은 일 하고 계신가요?",
          "original_form": "&name2&은 일 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010600.1.18",
          "form": "저는 구직활동중이에요",
          "original_form": "저는 구직활동중이에요",
          "speaker_id": "2",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010600.1.19",
          "form": "오 힘드시겠어요",
          "original_form": "오 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010600.1.20",
          "form": "백수생활 힘들죠",
          "original_form": "백수생활 힘들죠",
          "speaker_id": "1",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010600.1.21",
          "form": "아니요 너무 좋아요",
          "original_form": "아니요 너무 좋아요",
          "speaker_id": "2",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010600.1.22",
          "form": "오오 ,, 전 백수생활이 너무 힘들었어요",
          "original_form": "오오 ,, 전 백수생활이 너무 힘들었어요",
          "speaker_id": "1",
          "time": "20210823 20:05"
        },
        {
          "id": "MDRW2100010600.1.23",
          "form": "어떤점이요?",
          "original_form": "어떤점이요?",
          "speaker_id": "2",
          "time": "20210823 20:05"
        },
        {
          "id": "MDRW2100010600.1.24",
          "form": "취업에 대한 걱정이 너무 앞서서 전전긍긍했거든요",
          "original_form": "취업에 대한 걱정이 너무 앞서서 전전긍긍했거든요",
          "speaker_id": "1",
          "time": "20210823 20:05"
        }
      ]
