      "utterance": [
        {
          "id": "MDRW2100043029.1.1",
          "form": "안녕하세용 점심 뭐드셨나용",
          "original_form": "안녕하세용 점심 뭐드셨나용",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043029.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043029.1.3",
          "form": "함박스테이크나왔어여",
          "original_form": "함박스테이크나왔어여",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043029.1.4",
          "form": "낼 점심은 새우튀김이 나와욤",
          "original_form": "낼 점심은 새우튀김이 나와욤",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043029.1.5",
          "form": "우왕 진짜 잘나오네요 ㅠㅠ",
          "original_form": "우왕 진짜 잘나오네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043029.1.6",
          "form": "새우튀김 맛있게썽요",
          "original_form": "새우튀김 맛있게썽요",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043029.1.7",
          "form": "저 새우좋아해용",
          "original_form": "저 새우좋아해용",
          "speaker_id": "2",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100043029.1.8",
          "form": "저두용 감바스먹고싶내용",
          "original_form": "저두용 감바스먹고싶내용",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100043029.1.9",
          "form": "새우 치킨만큼 좋아해요",
          "original_form": "새우 치킨만큼 좋아해요",
          "speaker_id": "2",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043029.1.10",
          "form": "ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043029.1.11",
          "form": "저녁머먹을껀가요?",
          "original_form": "저녁머먹을껀가요?",
          "speaker_id": "2",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043029.1.12",
          "form": "로제 파스타 만들어먹을까 생각중입미다",
          "original_form": "로제 파스타 만들어먹을까 생각중입미다",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043029.1.13",
          "form": "배떡을 시켜먹고싶기도 하고 ㅎㅎ",
          "original_form": "배떡을 시켜먹고싶기도 하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043029.1.14",
          "form": "우와...만들어먹는것도 대단하죠",
          "original_form": "우와...만들어먹는것도 대단하죠",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043029.1.15",
          "form": "맛있겠답",
          "original_form": "맛있겠답",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043029.1.16",
          "form": "로제가 요새 맛잇더라구용 ㅎㅎ",
          "original_form": "로제가 요새 맛잇더라구용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043029.1.17",
          "form": "떡볶이좋아하시나용",
          "original_form": "떡볶이좋아하시나용",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043029.1.18",
          "form": "미친듯이요",
          "original_form": "미친듯이요",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043029.1.19",
          "form": "ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043029.1.20",
          "form": "악 추천해주싱 브랜드 있나요 ㅋㅋ",
          "original_form": "악 추천해주싱 브랜드 있나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043029.1.21",
          "form": "청년다방 맛있어용",
          "original_form": "청년다방 맛있어용",
          "speaker_id": "2",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100043029.1.22",
          "form": "욜 거기도 배달 ㄱㄱ해야겠옹ㅅ",
          "original_form": "욜 거기도 배달 ㄱㄱ해야겠옹ㅅ",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043029.1.23",
          "form": "역전할매도 맛있었는데 ㅠㅠ",
          "original_form": "역전할매도 맛있었는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:10"
        }
      ]
