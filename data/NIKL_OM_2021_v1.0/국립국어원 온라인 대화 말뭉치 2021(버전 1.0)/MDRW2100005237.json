      "utterance": [
        {
          "id": "MDRW2100005237.1.1",
          "form": "동물키우시나요(?)",
          "original_form": "동물키우시나요(?)",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005237.1.2",
          "form": "동물은 키웠었어요ㅎ",
          "original_form": "동물은 키웠었어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005237.1.3",
          "form": "어떤동물요(?)",
          "original_form": "어떤동물요(?)",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005237.1.4",
          "form": "강아지2마리요ㅎㅎ 비글2마리였는데",
          "original_form": "강아지2마리요ㅎㅎ 비글2마리였는데",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005237.1.5",
          "form": "엄청활발했어요ㅎ",
          "original_form": "엄청활발했어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005237.1.6",
          "form": "아 비글 사고 뭉치라던데",
          "original_form": "아 비글 사고 뭉치라던데",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005237.1.7",
          "form": "엄청났죠ㅎ 에너지가 넘쳐서 맨날 등산갔엇어요ㅎ",
          "original_form": "엄청났죠ㅎ 에너지가 넘쳐서 맨날 등산갔엇어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005237.1.8",
          "form": "두마리 데리고 등산갔다오면 땀ㅇㅠ",
          "original_form": "두마리 데리고 등산갔다오면 땀ㅇㅠ",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005237.1.9",
          "form": "고생많으셨네요",
          "original_form": "고생많으셨네요",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005237.1.10",
          "form": "저도키우고싶은데 아직이네요",
          "original_form": "저도키우고싶은데 아직이네요",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005237.1.11",
          "form": "애기가 있으시니깐ㅎ",
          "original_form": "애기가 있으시니깐ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005237.1.12",
          "form": "키우실필요는없죠ㅎ",
          "original_form": "키우실필요는없죠ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005237.1.13",
          "form": "강아지도 아이랑 똑같애요 ㅎ",
          "original_form": "강아지도 아이랑 똑같애요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005237.1.14",
          "form": "애들크면 키워봐야죠 \r\n교육이어려울듯 똥오줌요",
          "original_form": "애들크면 키워봐야죠 \r\n교육이어려울듯 똥오줌요",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005237.1.15",
          "form": "맞아요ㅎ 강아지는 그게 가장힘들어요",
          "original_form": "맞아요ㅎ 강아지는 그게 가장힘들어요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005237.1.16",
          "form": "근데 또 하면 잘하는데 하기까지의 과정이ㅠㅠ",
          "original_form": "근데 또 하면 잘하는데 하기까지의 과정이ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005237.1.17",
          "form": "그리고 장난많이쳐서 휴지 엄청 풀어놓고ㅠ",
          "original_form": "그리고 장난많이쳐서 휴지 엄청 풀어놓고ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005237.1.18",
          "form": "아구 장난기 마당있는집가고싶네요",
          "original_form": "아구 장난기 마당있는집가고싶네요",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100005237.1.19",
          "form": "마당있는집 저두요ㅎ 로또 되야지 가겟네요ㅎㅎ",
          "original_form": "마당있는집 저두요ㅎ 로또 되야지 가겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100005237.1.20",
          "form": "근데 name1님은 반려동물 키워본적있으세요?",
          "original_form": "근데 &name1&님은 반려동물 키워본적있으세요?",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100005237.1.21",
          "form": "동물키운적이 없어요\r\n지금은 열대어정도",
          "original_form": "동물키운적이 없어요\r\n지금은 열대어정도",
          "speaker_id": "1",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100005237.1.22",
          "form": "열대어ㅎㅎ 좋죠ㅎㅎ",
          "original_form": "열대어ㅎㅎ 좋죠ㅎㅎ",
          "speaker_id": "2",
