      "utterance": [
        {
          "id": "MDRW2100035965.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100035965.1.2",
          "form": "먹으로 가는 길이에요!",
          "original_form": "먹으로 가는 길이에요!",
          "speaker_id": "2",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100035965.1.3",
          "form": "뭐 드실 예정인가요?!",
          "original_form": "뭐 드실 예정인가요?!",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100035965.1.4",
          "form": "그냥 구내 식당 가요 ㅎㅎ",
          "original_form": "그냥 구내 식당 가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035965.1.5",
          "form": "점심은 보통 구내식당에서 드시나요?",
          "original_form": "점심은 보통 구내식당에서 드시나요?",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035965.1.6",
          "form": "네 맞아요. 점심 드셨어요?",
          "original_form": "네 맞아요. 점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035965.1.7",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100035965.1.8",
          "form": "오늘 메뉴는 뭔가요?",
          "original_form": "오늘 메뉴는 뭔가요?",
          "speaker_id": "1",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100035965.1.9",
          "form": "찜닭 나왔어요",
          "original_form": "찜닭 나왔어요",
          "speaker_id": "2",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100035965.1.10",
          "form": "맛있겠어요~ 구내 식당 음식 먹을만 하신가요?",
          "original_form": "맛있겠어요~ 구내 식당 음식 먹을만 하신가요?",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035965.1.11",
          "form": "그냥저냥 먹을만해요",
          "original_form": "그냥저냥 먹을만해요",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035965.1.12",
          "form": "점심 뭐 드실지 생각해놓운 거 있나요?",
          "original_form": "점심 뭐 드실지 생각해놓운 거 있나요?",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035965.1.13",
          "form": "주말에 해둔 나시고랭 데워먹으려고요!",
          "original_form": "주말에 해둔 나시고랭 데워먹으려고요!",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100035965.1.14",
          "form": "직접 만드셨어요?",
          "original_form": "직접 만드셨어요?",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100035965.1.15",
          "form": "네 양념 사다가 한번 만들어봤어요ㅎㅎ",
          "original_form": "네 양념 사다가 한번 만들어봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035965.1.16",
          "form": "우와 요리 되게 잘 하시나봐요",
          "original_form": "우와 요리 되게 잘 하시나봐요",
          "speaker_id": "2",
          "time": "20210830 13:42"
        }
