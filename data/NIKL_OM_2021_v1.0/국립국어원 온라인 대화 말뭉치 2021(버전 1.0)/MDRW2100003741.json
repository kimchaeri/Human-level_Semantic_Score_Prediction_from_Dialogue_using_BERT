      "utterance": [
        {
          "id": "MDRW2100003741.1.1",
          "form": "소개팅 해보신적 있으세요?",
          "original_form": "소개팅 해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003741.1.2",
          "form": "아뇨 아직 없어요",
          "original_form": "아뇨 아직 없어요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003741.1.3",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003741.1.4",
          "form": "저는 키 176이상에 재밌는 사람이요 ㅋㅋㄱ그쪽은요?",
          "original_form": "저는 키 176이상에 재밌는 사람이요 ㅋㅋㄱ그쪽은요?",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003741.1.5",
          "form": "저는 배려심이 깊은 사람이요",
          "original_form": "저는 배려심이 깊은 사람이요",
          "speaker_id": "2",
          "time": "20210821 12:43"
        },
        {
          "id": "MDRW2100003741.1.6",
          "form": "사귀는 사람있으세요?",
          "original_form": "사귀는 사람있으세요?",
          "speaker_id": "2",
          "time": "20210821 12:43"
        },
        {
          "id": "MDRW2100003741.1.7",
          "form": "네! 그쪽은요?",
          "original_form": "네! 그쪽은요?",
          "speaker_id": "1",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100003741.1.8",
          "form": "저는 아직 없네요...ㅜ",
          "original_form": "저는 아직 없네요...ㅜ",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100003741.1.9",
          "form": "연애하면 안좋은점은 없어요?",
          "original_form": "연애하면 안좋은점은 없어요?",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100003741.1.10",
          "form": "ㅋㅋㅋ싸우면 감정소모가 심하죠 ㅠ 왜 이렇게 상처받아야되나싶고 ㅠ",
          "original_form": "ㅋㅋㅋ싸우면 감정소모가 심하죠 ㅠ 왜 이렇게 상처받아야되나싶고 ㅠ",
          "speaker_id": "1",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100003741.1.11",
          "form": "결혼하신거에요?",
          "original_form": "결혼하신거에요?",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100003741.1.12",
          "form": "아뇨 ㅠ 연애중 ㅠ 님은 10대시죠?",
          "original_form": "아뇨 ㅠ 연애중 ㅠ 님은 10대시죠?",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100003741.1.13",
          "form": "네 내년이면 20대네요",
          "original_form": "네 내년이면 20대네요",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100003741.1.14",
          "form": "아 고3이면 공부해야될 나이라 연애하긴 힘들겠네요",
          "original_form": "아 고3이면 공부해야될 나이라 연애하긴 힘들겠네요",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100003741.1.15",
          "form": "연애하시면 좋은점은 뭐에요?",
          "original_form": "연애하시면 좋은점은 뭐에요?",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100003741.1.16",
          "form": "ㅋㅋㅋ그나마 덜 심심하다는거?",
          "original_form": "ㅋㅋㅋ그나마 덜 심심하다는거?",
