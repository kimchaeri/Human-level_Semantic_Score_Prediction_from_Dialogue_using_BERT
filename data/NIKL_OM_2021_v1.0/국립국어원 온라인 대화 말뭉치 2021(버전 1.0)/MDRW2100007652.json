        {
          "id": "MDRW2100007652.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210822 09:05"
        },
        {
          "id": "MDRW2100007652.1.2",
          "form": "모바일게임만 해요",
          "original_form": "모바일게임만 해요",
          "speaker_id": "2",
          "time": "20210822 09:17"
        },
        {
          "id": "MDRW2100007652.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007652.1.4",
          "form": "무슨 게임해요?",
          "original_form": "무슨 게임해요?",
          "speaker_id": "2",
          "time": "20210822 09:21"
        },
        {
          "id": "MDRW2100007652.1.5",
          "form": "브롤이요",
          "original_form": "브롤이요",
          "speaker_id": "1",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100007652.1.6",
          "form": "뭔지 모르겠네요",
          "original_form": "뭔지 모르겠네요",
          "speaker_id": "2",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100007652.1.7",
          "form": "브롤스타즈라고 초딩겜이여",
          "original_form": "브롤스타즈라고 초딩겜이여",
          "speaker_id": "1",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100007652.1.8",
          "form": "초딩겜인데 잘 맞으시나봐여",
          "original_form": "초딩겜인데 잘 맞으시나봐여",
          "speaker_id": "2",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100007652.1.9",
          "form": "아들이 레벨업 때문에",
          "original_form": "아들이 레벨업 때문에",
          "speaker_id": "1",
          "time": "20210822 09:25"
        },
        {
          "id": "MDRW2100007652.1.10",
          "form": "아빠찬스네요",
          "original_form": "아빠찬스네요",
          "speaker_id": "2",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007652.1.11",
          "form": "네 매일 레벨 물어보죠",
          "original_form": "네 매일 레벨 물어보죠",
          "speaker_id": "1",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007652.1.12",
          "form": "숙제 같겠어요\r\n좋아하는 게임은 따로 있을텐데",
          "original_form": "숙제 같겠어요\r\n좋아하는 게임은 따로 있을텐데",
          "speaker_id": "2",
          "time": "20210822 09:39"
        },
        {
          "id": "MDRW2100007652.1.13",
          "form": "결혼하고 겜 다 포기요",
