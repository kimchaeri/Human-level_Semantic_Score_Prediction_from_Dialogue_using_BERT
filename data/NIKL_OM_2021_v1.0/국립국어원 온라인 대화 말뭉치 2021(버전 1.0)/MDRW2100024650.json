      "utterance": [
        {
          "id": "MDRW2100024650.1.1",
          "form": "둘째날은 뭐했어?",
          "original_form": "둘째날은 뭐했어?",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024650.1.2",
          "form": "둘째 날에 뭐 했지",
          "original_form": "둘째 날에 뭐 했지",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024650.1.3",
          "form": "가물가물하구나",
          "original_form": "가물가물하구나",
          "speaker_id": "2",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024650.1.4",
          "form": "아 둘째날에 온천 갔어",
          "original_form": "아 둘째날에 온천 갔어",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024650.1.5",
          "form": "넌 여행가면   조식챙기니",
          "original_form": "넌 여행가면   조식챙기니",
          "speaker_id": "2",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024650.1.6",
          "form": "챙기려고 했는데 늦게 일어나서 점심부터 먹었던 것 같아ㅋㅋㅋㅋ",
          "original_form": "챙기려고 했는데 늦게 일어나서 점심부터 먹었던 것 같아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024650.1.7",
          "form": "난  옛날이면  그냥 잘텐대",
          "original_form": "난  옛날이면  그냥 잘텐대",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024650.1.8",
          "form": "지금의  나라면 챙겨먹을듯",
          "original_form": "지금의  나라면 챙겨먹을듯",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024650.1.9",
          "form": "조식뷔페 너무 궁금해",
          "original_form": "조식뷔페 너무 궁금해",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024650.1.10",
          "form": "그러게 나도 그럴 것 같아",
          "original_form": "그러게 나도 그럴 것 같아",
          "speaker_id": "1",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024650.1.11",
          "form": "넌 여행 가봤니?",
          "original_form": "넌 여행 가봤니?",
          "speaker_id": "1",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024650.1.12",
          "form": "난 국내만 가봤어",
          "original_form": "난 국내만 가봤어",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024650.1.13",
          "form": "이번에 친구따라  캐나다가게되거나",
          "original_form": "이번에 친구따라  캐나다가게되거나",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024650.1.14",
          "form": "아니면    일본가보려고",
          "original_form": "아니면    일본가보려고",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024650.1.15",
          "form": "캐나가 가면 뭐 해보고싶어?",
          "original_form": "캐나가 가면 뭐 해보고싶어?",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024650.1.16",
          "form": "그러게 캐나다 뭐가 유명한지도 모르고",
          "original_form": "그러게 캐나다 뭐가 유명한지도 모르고",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024650.1.17",
          "form": "친구따라 강남가네",
          "original_form": "친구따라 강남가네",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024650.1.18",
          "form": "아는게없어",
          "original_form": "아는게없어",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024650.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 웃긴다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 웃긴다",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024650.1.20",
          "form": "캐나다하면 메이플시럽?",
          "original_form": "캐나다하면 메이플시럽?",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024650.1.21",
          "form": ".아이스하키..?",
          "original_form": ".아이스하키..?",
          "speaker_id": "2",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024650.1.22",
          "form": "오 나도 그 생각 했는데",
          "original_form": "오 나도 그 생각 했는데",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024650.1.23",
          "form": "기념품으로 메이플시럽 많이들 사오더라",
          "original_form": "기념품으로 메이플시럽 많이들 사오더라",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024650.1.24",
          "form": "진짜   알아봐야겠다\r\n이러다  시럽만 사오겠어",
          "original_form": "진짜   알아봐야겠다\r\n이러다  시럽만 사오겠어",
          "speaker_id": "2",
          "time": "20210831 16:43"
        }
      ]
    }
