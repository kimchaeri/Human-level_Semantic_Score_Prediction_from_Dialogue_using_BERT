      "utterance": [
        {
          "id": "MDRW2100005368.1.1",
          "form": "저랑 반대로 드시는군요 ㅋㅋㅋ",
          "original_form": "저랑 반대로 드시는군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005368.1.2",
          "form": "네 구운거 좋아해요",
          "original_form": "네 구운거 좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005368.1.3",
          "form": "매운걸 좋아하는데 고추는 잘 안 먹게되더라구요",
          "original_form": "매운걸 좋아하는데 고추는 잘 안 먹게되더라구요",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005368.1.4",
          "form": "저도 고추는 삼겹살 먹을 때 썰어놓은 것만 먹어요",
          "original_form": "저도 고추는 삼겹살 먹을 때 썰어놓은 것만 먹어요",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005368.1.5",
          "form": "엽떡이나 불닭매운건 좋아해도",
          "original_form": "엽떡이나 불닭매운건 좋아해도",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.6",
          "form": "뭔가 고추자체는 부담스러워서 혀에 스칠 정도의 식감만 즐겨요ㅎㅎ",
          "original_form": "뭔가 고추자체는 부담스러워서 혀에 스칠 정도의 식감만 즐겨요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.7",
          "form": "생고추의 풋내? 를 잘 못먹어요",
          "original_form": "생고추의 풋내? 를 잘 못먹어요",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.8",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.9",
          "form": "아 ㅋㅋ 그건 뭔지알겠어요",
          "original_form": "아 ㅋㅋ 그건 뭔지알겠어요",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.10",
          "form": "식감 중요하죠",
          "original_form": "식감 중요하죠",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.11",
          "form": "그래서 쌈싸먹을 때",
          "original_form": "그래서 쌈싸먹을 때",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.12",
          "form": "들어갔나? 싶을정도로 작게 썰어요",
          "original_form": "들어갔나? 싶을정도로 작게 썰어요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005368.1.13",
          "form": "향만 느끼시능군요",
          "original_form": "향만 느끼시능군요",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005368.1.14",
          "form": "맞아욬ㅋㅋ 이럴거면 왜 먹니? 소리 들을 정도로",
          "original_form": "맞아욬ㅋㅋ 이럴거면 왜 먹니? 소리 들을 정도로",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005368.1.15",
          "form": "큰 차이가있죠",
          "original_form": "큰 차이가있죠",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005368.1.16",
          "form": "요리 다하고 파슬리를 뿌리는것처럼",
          "original_form": "요리 다하고 파슬리를 뿌리는것처럼",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005368.1.17",
          "form": "오 뭘 좀 아시는군요!",
          "original_form": "오 뭘 좀 아시는군요!",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005368.1.18",
          "form": "ㅋㅋㅋ 삼겹살에 진심인거죠",
          "original_form": "ㅋㅋㅋ 삼겹살에 진심인거죠",
          "speaker_id": "2",
          "time": "20210821 22:04"
