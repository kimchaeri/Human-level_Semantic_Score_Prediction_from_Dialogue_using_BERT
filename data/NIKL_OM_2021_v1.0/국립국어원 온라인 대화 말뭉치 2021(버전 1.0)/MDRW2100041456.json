      "utterance": [
        {
          "id": "MDRW2100041456.1.1",
          "form": "지금 드시고싶은 음식 있으세요?",
          "original_form": "지금 드시고싶은 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041456.1.2",
          "form": "지금요? 음..지금은 냉면같이 시원한게  땡기네요",
          "original_form": "지금요? 음..지금은 냉면같이 시원한게  땡기네요",
          "speaker_id": "2",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041456.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041456.1.4",
          "form": "오오 저는 라면이요..!!",
          "original_form": "오오 저는 라면이요..!!",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041456.1.5",
          "form": "라면 잘 끓이세요?",
          "original_form": "라면 잘 끓이세요?",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041456.1.6",
          "form": "오호!라면 좋죠",
          "original_form": "오호!라면 좋죠",
          "speaker_id": "2",
          "time": "20210906 00:26"
        },
        {
          "id": "MDRW2100041456.1.7",
          "form": "뭐 막 못끓이지는 않는거같아요ㅋㅋ",
          "original_form": "뭐 막 못끓이지는 않는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:26"
        },
        {
          "id": "MDRW2100041456.1.8",
          "form": "라면 꼬들한거 좋아하시나요?",
          "original_form": "라면 꼬들한거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 00:26"
        },
        {
          "id": "MDRW2100041456.1.9",
          "form": "네네 무조건 꼬들이요 ㅠㅠㅠ",
          "original_form": "네네 무조건 꼬들이요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 00:27"
        },
        {
          "id": "MDRW2100041456.1.10",
          "form": "햇님님은요?",
          "original_form": "햇님님은요?",
          "speaker_id": "1",
          "time": "20210906 00:27"
        },
        {
          "id": "MDRW2100041456.1.11",
          "form": "저는 컵라면은 꼬들인데\r\n끓여먹는거는 잘익은게 좋더라구요 요새는",
          "original_form": "저는 컵라면은 꼬들인데\r\n끓여먹는거는 잘익은게 좋더라구요 요새는",
          "speaker_id": "2",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100041456.1.12",
          "form": "와 저는 둘다 꼬들파에요 ㅎㅎㅎ",
          "original_form": "와 저는 둘다 꼬들파에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 00:33"
        },
        {
          "id": "MDRW2100041456.1.13",
          "form": "약간 불어도 맛있긴해요 ㅋㅋㅋ",
          "original_form": "약간 불어도 맛있긴해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:33"
        },
        {
          "id": "MDRW2100041456.1.14",
          "form": "마자요! 가끔 불은것도 맛있죠ㅋㅋ",
          "original_form": "마자요! 가끔 불은것도 맛있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100041456.1.15",
          "form": "무슨라면 좋아하세요?",
          "original_form": "무슨라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100041456.1.16",
          "form": "저는 너구리요!!",
          "original_form": "저는 너구리요!!",
          "speaker_id": "1",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100041456.1.17",
          "form": "짜파게티도 좋아해요 ㅎㄹ",
          "original_form": "짜파게티도 좋아해요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041456.1.18",
          "form": "너구리도 맛있죠! 짜파게티도 맛있구",
          "original_form": "너구리도 맛있죠! 짜파게티도 맛있구",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041456.1.19",
          "form": "넵넵!!!!  짜파게티 잘 끓이세용?",
          "original_form": "넵넵!!!!  짜파게티 잘 끓이세용?",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041456.1.20",
          "form": "음..못끓이지는 않는거같아요ㅋㅋㄱ",
          "original_form": "음..못끓이지는 않는거같아요ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100041456.1.21",
          "form": "근데 짜파게티도 끓이는 방법따라서 호불호갈리더라구요",
          "original_form": "근데 짜파게티도 끓이는 방법따라서 호불호갈리더라구요",
