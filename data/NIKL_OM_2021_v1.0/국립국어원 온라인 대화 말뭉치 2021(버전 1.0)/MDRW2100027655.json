      "utterance": [
        {
          "id": "MDRW2100027655.1.1",
          "form": "프라하좋았어",
          "original_form": "프라하좋았어",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027655.1.2",
          "form": "눈내리는게 로맨틱했어",
          "original_form": "눈내리는게 로맨틱했어",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027655.1.3",
          "form": "투어버스가 2층이라 좋았어",
          "original_form": "투어버스가 2층이라 좋았어",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027655.1.4",
          "form": "2층이 잘보이지",
          "original_form": "2층이 잘보이지",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027655.1.5",
          "form": "설경이 예술이야",
          "original_form": "설경이 예술이야",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027655.1.6",
          "form": "프라하성당이 좋더라 웅장했어",
          "original_form": "프라하성당이 좋더라 웅장했어",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027655.1.7",
          "form": "다시갈수있을까",
          "original_form": "다시갈수있을까",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027655.1.8",
          "form": "맥주가 그립다",
          "original_form": "맥주가 그립다",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027655.1.9",
          "form": "생맥주가 어찌그리맛날까",
          "original_form": "생맥주가 어찌그리맛날까",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027655.1.10",
          "form": "이해안되는 신선함",
          "original_form": "이해안되는 신선함",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027655.1.11",
          "form": "프라하는 낭만적이야",
          "original_form": "프라하는 낭만적이야",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027655.1.12",
          "form": "신혼여행지로 추천해",
          "original_form": "신혼여행지로 추천해",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027655.1.13",
          "form": "버스도 거리도 깨끗해",
          "original_form": "버스도 거리도 깨끗해",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027655.1.14",
          "form": "팁이 불편해",
          "original_form": "팁이 불편해",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027655.1.15",
          "form": "익숙하지않아서 그래",
          "original_form": "익숙하지않아서 그래",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027655.1.16",
          "form": "독일은 어땠어",
          "original_form": "독일은 어땠어",
          "speaker_id": "1",
          "time": "20210902 14:05"
        }
