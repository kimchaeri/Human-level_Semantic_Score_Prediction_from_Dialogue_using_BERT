      "utterance": [
        {
          "id": "MDRW2100038448.1.1",
          "form": "날씨가 많이 시원해졌죠",
          "original_form": "날씨가 많이 시원해졌죠",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100038448.1.2",
          "form": "제법 가을 같구요",
          "original_form": "제법 가을 같구요",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038448.1.3",
          "form": "네 정말 가을이네요 ㅋㅋ",
          "original_form": "네 정말 가을이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038448.1.4",
          "form": "가을 옷 꺼내셨어요?",
          "original_form": "가을 옷 꺼내셨어요?",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038448.1.5",
          "form": "며칠 전에 꺼냈어요",
          "original_form": "며칠 전에 꺼냈어요",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038448.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038448.1.7",
          "form": "저도 며칠전에 꺼냈어요 ㅋㅋㅋ 그냥 일단 보이는대로 입고 있어요 ㅋㅋ",
          "original_form": "저도 며칠전에 꺼냈어요 ㅋㅋㅋ 그냥 일단 보이는대로 입고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038448.1.8",
          "form": "여름옷 정리를 해야되는데 ㅋㅋㅋ 옷이 너무 많아서 ㅋㅋㅋㅋㅋㅋ 쉽지 않네요 ㅋ",
          "original_form": "여름옷 정리를 해야되는데 ㅋㅋㅋ 옷이 너무 많아서 ㅋㅋㅋㅋㅋㅋ 쉽지 않네요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038448.1.9",
          "form": "옷정리가 힘들죠 참...",
          "original_form": "옷정리가 힘들죠 참...",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038448.1.10",
          "form": "누가 대신 정리해줬으면 좋겠어요",
          "original_form": "누가 대신 정리해줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038448.1.11",
          "form": "그러니깐요 ㅠㅠㅠ 아이들 옷이 너무 많아서 ㅠㅠ",
          "original_form": "그러니깐요 ㅠㅠㅠ 아이들 옷이 너무 많아서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038448.1.12",
          "form": "ㅋㅋㅋㅋㅋ뭐가 작아진건지 좀 대보고싶은데 ㅋㅋ 입기 귀찮아 하니 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ뭐가 작아진건지 좀 대보고싶은데 ㅋㅋ 입기 귀찮아 하니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038448.1.13",
          "form": "애들이 있었군요ㅋㅋ 죄송하지만 저도 어렸을때 그거 너무 귀찮아했었어요",
          "original_form": "애들이 있었군요ㅋㅋ 죄송하지만 저도 어렸을때 그거 너무 귀찮아했었어요",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100038448.1.14",
          "form": "네 그러니깐요 ㅋㅋㅋㅋㅋㅋㅋ정말 귀찮아 해서 대보는것도 싫어해요 ㅋㅋㅋ 제가 대충 정리해야되요 ㅋㅋ",
          "original_form": "네 그러니깐요 ㅋㅋㅋㅋㅋㅋㅋ정말 귀찮아 해서 대보는것도 싫어해요 ㅋㅋㅋ 제가 대충 정리해야되요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038448.1.15",
          "form": "옷 한개만 기준으로 대보고 정리하는건 어떨까요",
          "original_form": "옷 한개만 기준으로 대보고 정리하는건 어떨까요",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038448.1.16",
          "form": "그러고 있어요 ㅁㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러고 있어요 ㅁㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038448.1.17",
          "form": "오늘은 시원하고 좋네요 ㅋㅋㅋ 비도 안오구요 ㅋㅋ",
          "original_form": "오늘은 시원하고 좋네요 ㅋㅋㅋ 비도 안오구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038448.1.18",
          "form": "흐린 것만 풀리면 딱일거 같아요",
          "original_form": "흐린 것만 풀리면 딱일거 같아요",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038448.1.19",
          "form": "네네 ㅋㅋ 너무 좋을것같네요 ㅋㅋ",
          "original_form": "네네 ㅋㅋ 너무 좋을것같네요 ㅋㅋ",
