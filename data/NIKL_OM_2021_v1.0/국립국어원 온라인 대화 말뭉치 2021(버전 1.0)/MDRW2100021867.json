      "utterance": [
        {
          "id": "MDRW2100021867.1.1",
          "form": "콜라보제품 어떤거 이용하세요?",
          "original_form": "콜라보제품 어떤거 이용하세요?",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021867.1.2",
          "form": "아 특별한건아니구",
          "original_form": "아 특별한건아니구",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021867.1.3",
          "form": "곰표밀맥주 먹어봤는데",
          "original_form": "곰표밀맥주 먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021867.1.4",
          "form": "맛이 없더라구요 ㅠ",
          "original_form": "맛이 없더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021867.1.5",
          "form": "앗글큰요",
          "original_form": "앗글큰요",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021867.1.6",
          "form": "전 곰표 밀가루 쿠션 사용중인데",
          "original_form": "전 곰표 밀가루 쿠션 사용중인데",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021867.1.7",
          "form": "스와니 코코코 얼래 쓰던 브랜드라 잘맞더라구요 ㅎㅎ",
          "original_form": "스와니 코코코 얼래 쓰던 브랜드라 잘맞더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021867.1.8",
          "form": "아 그런거도 나왔어요?",
          "original_form": "아 그런거도 나왔어요?",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021867.1.9",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021867.1.10",
          "form": "파리바게트에서 얼마전에",
          "original_form": "파리바게트에서 얼마전에",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021867.1.11",
          "form": "팔도 비빔면 양념장 넣은",
          "original_form": "팔도 비빔면 양념장 넣은",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021867.1.12",
          "form": "오래 됐어요 ㅋ",
          "original_form": "오래 됐어요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021867.1.13",
          "form": "아 ㅋㅋ 뭔지 알아요 ㅋㅋ",
          "original_form": "아 ㅋㅋ 뭔지 알아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021867.1.14",
          "form": "고로케가 먹어봤는데 나름 괜찮더라는?",
          "original_form": "고로케가 먹어봤는데 나름 괜찮더라는?",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021867.1.15",
          "form": "기대가 없어서 그랬나봐여",
          "original_form": "기대가 없어서 그랬나봐여",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021867.1.16",
          "form": "전 아직 못먹어봤는데",
          "original_form": "전 아직 못먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021867.1.17",
          "form": "근데 가성비 떨어지는 가격이었눈데",
          "original_form": "근데 가성비 떨어지는 가격이었눈데",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021867.1.18",
          "form": "2600원인가",
          "original_form": "2600원인가",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021867.1.19",
          "form": "요즘 빵값이 그렇게 다 비싼가여",
          "original_form": "요즘 빵값이 그렇게 다 비싼가여",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021867.1.20",
          "form": "넘 비싸쥬",
          "original_form": "넘 비싸쥬",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021867.1.21",
          "form": "차라리  맛집 빵짐이 더 나은거 같아요",
          "original_form": "차라리  맛집 빵짐이 더 나은거 같아요",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021867.1.22",
          "form": "가격은 비슷란데 맛이 훨씬 맛잇어요",
          "original_form": "가격은 비슷란데 맛이 훨씬 맛잇어요",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021867.1.23",
          "form": "네 비건베이커리도 맛난거 많은데",
          "original_form": "네 비건베이커리도 맛난거 많은데",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021867.1.24",
          "form": "근데 핫플에 많으니까여",
          "original_form": "근데 핫플에 많으니까여",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021867.1.25",
          "form": "물가가 비싸긴 하죠 ㅎㅎ",
          "original_form": "물가가 비싸긴 하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021867.1.26",
          "form": "콜라보 마켓팅 하면 사실 눈에 확들어오긴해여",
          "original_form": "콜라보 마켓팅 하면 사실 눈에 확들어오긴해여",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021867.1.27",
          "form": "그래서 맛없어도 신기해서 산달까",
          "original_form": "그래서 맛없어도 신기해서 산달까",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021867.1.28",
          "form": "편의점에 안비싸면 가끔씩 심심하고 패키지 이뻐서 손이가는 듯요",
          "original_form": "편의점에 안비싸면 가끔씩 심심하고 패키지 이뻐서 손이가는 듯요",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021867.1.29",
          "form": "ㅎㅎ 눈에 뜨긴 해요 ㅎㅎ",
          "original_form": "ㅎㅎ 눈에 뜨긴 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:29"
        }
      ]
