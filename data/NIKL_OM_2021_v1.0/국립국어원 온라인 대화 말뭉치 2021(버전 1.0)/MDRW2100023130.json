      "utterance": [
        {
          "id": "MDRW2100023130.1.1",
          "form": "그냥 마사지로요 ㅋㅋㅋㅋ",
          "original_form": "그냥 마사지로요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023130.1.2",
          "form": "주무르면 되나요?",
          "original_form": "주무르면 되나요?",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023130.1.3",
          "form": "주물주물",
          "original_form": "주물주물",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023130.1.4",
          "form": "네네 주물러주면 좀 낫더라고요",
          "original_form": "네네 주물러주면 좀 낫더라고요",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023130.1.5",
          "form": "에잇 김빠졌어요 ㅜ",
          "original_form": "에잇 김빠졌어요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023130.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023130.1.7",
          "form": "뭔가 특별한 요령 있을 줄 알았어요 ㅋㅋㅋㅋ",
          "original_form": "뭔가 특별한 요령 있을 줄 알았어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023130.1.8",
          "form": "뭐 운동에 대단한 비법있고 그런건 아니자나요",
          "original_form": "뭐 운동에 대단한 비법있고 그런건 아니자나요",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.10",
          "form": "돈으로 사는 운동법..ㅠㅠ",
          "original_form": "돈으로 사는 운동법..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.11",
          "form": "그럼 혹시 그거 써보셨어요?",
          "original_form": "그럼 혹시 그거 써보셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.12",
          "form": "그냥 내가 꾸준히 못하니까 다니는거지ㅠㅠ",
          "original_form": "그냥 내가 꾸준히 못하니까 다니는거지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.13",
          "form": "요새 마사지기가 유행이더라구요",
          "original_form": "요새 마사지기가 유행이더라구요",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.14",
          "form": "아 마사지건 같은거요?",
          "original_form": "아 마사지건 같은거요?",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.15",
          "form": "네 그런거요!",
          "original_form": "네 그런거요!",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.16",
          "form": "써보셨어요?",
          "original_form": "써보셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.17",
          "form": "마사지건같은건 무거워서 제가 못들겟던데요",
          "original_form": "마사지건같은건 무거워서 제가 못들겟던데요",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.19",
          "form": "아 ㅋㅋㅋ 진짜요 그렇게 무거워요?",
          "original_form": "아 ㅋㅋㅋ 진짜요 그렇게 무거워요?",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.20",
          "form": "거의 아령급인가요...",
          "original_form": "거의 아령급인가요...",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023130.1.21",
          "form": "아 그정도는 아닌거 같은데",
          "original_form": "아 그정도는 아닌거 같은데",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023130.1.22",
          "form": "그럼요? 한 생수병 무게??",
          "original_form": "그럼요? 한 생수병 무게??",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023130.1.23",
          "form": "전 좀 무거운 마사지기가 집에 잇어서",
          "original_form": "전 좀 무거운 마사지기가 집에 잇어서",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023130.1.24",
          "form": "마사지건은 어떤 무게인지 모르겟어요",
          "original_form": "마사지건은 어떤 무게인지 모르겟어요",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023130.1.25",
          "form": "아.. 뭔지 알 것 같아요 ㅋㅋ 저희 엄마도 무거운거 써요",
          "original_form": "아.. 뭔지 알 것 같아요 ㅋㅋ 저희 엄마도 무거운거 써요",
