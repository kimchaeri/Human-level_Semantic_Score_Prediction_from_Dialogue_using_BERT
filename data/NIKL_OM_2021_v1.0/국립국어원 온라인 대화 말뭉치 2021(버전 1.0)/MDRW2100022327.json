      "utterance": [
        {
          "id": "MDRW2100022327.1.1",
          "form": "그러게 말야~ 너무 찌뿌둥해",
          "original_form": "그러게 말야~ 너무 찌뿌둥해",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022327.1.2",
          "form": "비가 한 번 쏴~~~ 시원하게 내리고 해뜨면 좋겠다",
          "original_form": "비가 한 번 쏴~~~ 시원하게 내리고 해뜨면 좋겠다",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022327.1.3",
          "form": "맞아 먹구름 좀 걷히게",
          "original_form": "맞아 먹구름 좀 걷히게",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022327.1.4",
          "form": "너무 답답한 날씨야",
          "original_form": "너무 답답한 날씨야",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022327.1.5",
          "form": "빨래도 할까말까 고민중이야~ 날씨가 너무 흐려서",
          "original_form": "빨래도 할까말까 고민중이야~ 날씨가 너무 흐려서",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022327.1.6",
          "form": "ㅎㅎ 완전 해뜰 때 하는 게 좋을듯",
          "original_form": "ㅎㅎ 완전 해뜰 때 하는 게 좋을듯",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022327.1.7",
          "form": "그렇겠지?",
          "original_form": "그렇겠지?",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022327.1.8",
          "form": "괜히 빨래 했다가 비오면 망이야",
          "original_form": "괜히 빨래 했다가 비오면 망이야",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022327.1.9",
          "form": "ㅎㅎ 그건 그래~ 습도 더하기 습도가 될테니",
          "original_form": "ㅎㅎ 그건 그래~ 습도 더하기 습도가 될테니",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022327.1.10",
          "form": "해뜬 날을 이렇게 기다리게 될 줄이야",
          "original_form": "해뜬 날을 이렇게 기다리게 될 줄이야",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022327.1.11",
          "form": "나도 그래 ㅠ 해뜬 하늘 보고 싶다",
          "original_form": "나도 그래 ㅠ 해뜬 하늘 보고 싶다",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022327.1.12",
          "form": "가을 장마 한 주만 더 지나면 좋아지겠지",
          "original_form": "가을 장마 한 주만 더 지나면 좋아지겠지",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022327.1.13",
          "form": "그러길 바래 ㅠ",
          "original_form": "그러길 바래 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022327.1.14",
          "form": "저녁 되니까 너무 시원하다",
