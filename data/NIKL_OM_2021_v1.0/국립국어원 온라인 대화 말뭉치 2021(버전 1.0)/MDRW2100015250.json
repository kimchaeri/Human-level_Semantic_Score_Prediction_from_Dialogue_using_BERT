      "utterance": [
        {
          "id": "MDRW2100015250.1.1",
          "form": "우와~ 어떤 봉사해?",
          "original_form": "우와~ 어떤 봉사해?",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015250.1.2",
          "form": "다문화 센터에서 봉사해!!",
          "original_form": "다문화 센터에서 봉사해!!",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015250.1.3",
          "form": "넌 봉사 안 해도 돼?",
          "original_form": "넌 봉사 안 해도 돼?",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015250.1.4",
          "form": "나도 학교 봉사시간 슬슬 채워야 될 것 같긴 해",
          "original_form": "나도 학교 봉사시간 슬슬 채워야 될 것 같긴 해",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015250.1.5",
          "form": "어떤 봉사가 하고싶어?",
          "original_form": "어떤 봉사가 하고싶어?",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015250.1.6",
          "form": "응~ 쉬운 거",
          "original_form": "응~ 쉬운 거",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015250.1.7",
          "form": "ㅋㅋㅋㅋㅋ 봉사가 힘들긴 하지...",
          "original_form": "ㅋㅋㅋㅋㅋ 봉사가 힘들긴 하지...",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015250.1.8",
          "form": "도서관 봉사 같은 거 어떨까?",
          "original_form": "도서관 봉사 같은 거 어떨까?",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015250.1.9",
          "form": "나 해봤어!",
          "original_form": "나 해봤어!",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015250.1.10",
          "form": "어땠어?",
          "original_form": "어땠어?",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015250.1.11",
          "form": "책장만 닦고 별로 할 일은 없어서 쉬웠어",
          "original_form": "책장만 닦고 별로 할 일은 없어서 쉬웠어",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015250.1.12",
          "form": "오~ 그런거면 나도 해볼까?",
          "original_form": "오~ 그런거면 나도 해볼까?",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015250.1.13",
          "form": "그래그래 다음에 같이 가자",
          "original_form": "그래그래 다음에 같이 가자",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
