      "utterance": [
        {
          "id": "MDRW2100002758.1.1",
          "form": "요즘 사고싶은거 있어?",
          "original_form": "요즘 사고싶은거 있어?",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002758.1.2",
          "form": "나는 제일 최근에 갤럭시워치 샀어",
          "original_form": "나는 제일 최근에 갤럭시워치 샀어",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002758.1.3",
          "form": "난 최근에 타블렛 샀는데 사놓고 게임 용도로 쓰고 있어",
          "original_form": "난 최근에 타블렛 샀는데 사놓고 게임 용도로 쓰고 있어",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002758.1.4",
          "form": "태블릿 좋지",
          "original_form": "태블릿 좋지",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002758.1.5",
          "form": "나도 태블릿 최근에 샀어",
          "original_form": "나도 태블릿 최근에 샀어",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002758.1.6",
          "form": "태블릿 말구 타블렛!",
          "original_form": "태블릿 말구 타블렛!",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002758.1.7",
          "form": "타블렛은 그림그릴때 쓰는 용도야",
          "original_form": "타블렛은 그림그릴때 쓰는 용도야",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002758.1.8",
          "form": "아 타블렛을 샀구나",
          "original_form": "아 타블렛을 샀구나",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002758.1.9",
          "form": "그런데 그걸로 어떻게 게임을 해?",
          "original_form": "그런데 그걸로 어떻게 게임을 해?",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002758.1.10",
          "form": "osu라고 리듬 게임이 있는데 그거 마우스나 타블렛으로 할수 있거든",
          "original_form": "osu라고 리듬 게임이 있는데 그거 마우스나 타블렛으로 할수 있거든",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002758.1.11",
          "form": "마우스 대용이라고 보면돼",
          "original_form": "마우스 대용이라고 보면돼",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002758.1.12",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002758.1.13",
          "form": "나도 최근에 동생이 그림그린다고 해서",
          "original_form": "나도 최근에 동생이 그림그린다고 해서",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002758.1.14",
          "form": "타블렛 선물해줬어",
          "original_form": "타블렛 선물해줬어",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002758.1.15",
          "form": "와 완전 부럽네",
          "original_form": "와 완전 부럽네",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002758.1.16",
          "form": "타블렛 가격 좀 나가는데",
          "original_form": "타블렛 가격 좀 나가는데",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002758.1.17",
          "form": "돈 꽤 깨졌지",
          "original_form": "돈 꽤 깨졌지",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002758.1.18",
          "form": "8만원 넘게 들었는데",
          "original_form": "8만원 넘게 들었는데",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002758.1.19",
          "form": "얘가 타블렛을 안쓰네?^^",
          "original_form": "얘가 타블렛을 안쓰네?^^",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002758.1.20",
          "form": "헉.. 8만원 너무 비싼데",
          "original_form": "헉.. 8만원 너무 비싼데",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002758.1.21",
          "form": "나 연습용으로 산것도 3만원이야..",
          "original_form": "나 연습용으로 산것도 3만원이야..",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002758.1.22",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002758.1.23",
          "form": "와콤 타블렛 사줬어",
          "original_form": "와콤 타블렛 사줬어",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002758.1.24",
          "form": "와콤 메이커 있는거네",
          "original_form": "와콤 메이커 있는거네",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002758.1.25",
          "form": "젤 비싼 종류인데",
          "original_form": "젤 비싼 종류인데",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002758.1.26",
          "form": "얘가 열심히 안쓰면",
          "original_form": "얘가 열심히 안쓰면",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002758.1.27",
          "form": "뺐어올거야",
