      "utterance": [
        {
          "id": "MDRW2100017198.1.1",
          "form": "인생 드라마가 어케 되세요?",
          "original_form": "인생 드라마가 어케 되세요?",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017198.1.2",
          "form": "눈이부시게..?",
          "original_form": "눈이부시게..?",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017198.1.3",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017198.1.4",
          "form": "아아 보지는 않았지만",
          "original_form": "아아 보지는 않았지만",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017198.1.5",
          "form": "명대사 모음집에서 봤어요",
          "original_form": "명대사 모음집에서 봤어요",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017198.1.6",
          "form": "좋은 대사들이 많더라구요",
          "original_form": "좋은 대사들이 많더라구요",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017198.1.7",
          "form": "눈이부시게 작가님이 누구셨죠?",
          "original_form": "눈이부시게 작가님이 누구셨죠?",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017198.1.8",
          "form": "약간 노희경 작가님 느낌 났어요",
          "original_form": "약간 노희경 작가님 느낌 났어요",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017198.1.9",
          "form": "어 제가 작가는 잘 기억을 못하는 편이라",
          "original_form": "어 제가 작가는 잘 기억을 못하는 편이라",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017198.1.10",
          "form": "아아 괜찮아요 ㅎㅎ",
          "original_form": "아아 괜찮아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017198.1.11",
          "form": "제목만 기억하면 됐죠",
          "original_form": "제목만 기억하면 됐죠",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017198.1.12",
          "form": "아 저 2분만요",
          "original_form": "아 저 2분만요",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017198.1.13",
          "form": "네! ㅎㅎ",
          "original_form": "네! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017198.1.14",
          "form": "인생드라마 있으세요?",
          "original_form": "인생드라마 있으세요?",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017198.1.15",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.16",
          "form": "뭐였지",
          "original_form": "뭐였지",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.17",
          "form": "공효진 나오는",
          "original_form": "공효진 나오는",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.18",
          "form": "괜사",
          "original_form": "괜사",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.19",
          "form": "괜찮아 사랑이야",
          "original_form": "괜찮아 사랑이야",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.21",
          "form": "오오 아시네요",
          "original_form": "오오 아시네요",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.22",
          "form": "그것도 좋았어요",
          "original_form": "그것도 좋았어요",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.23",
          "form": "서로 다른 사람들 이해할 수 있는 게 좋았어요",
          "original_form": "서로 다른 사람들 이해할 수 있는 게 좋았어요",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.24",
          "form": "약간 나를 찾는 느낌",
          "original_form": "약간 나를 찾는 느낌",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.25",
          "form": "거기 캐릭터들 하나하나 다 이상하잖아요",
          "original_form": "거기 캐릭터들 하나하나 다 이상하잖아요",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.26",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017198.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017198.1.28",
          "form": "하나 하나 다 상처가있죠",
          "original_form": "하나 하나 다 상처가있죠",
