      "utterance": [
        {
          "id": "MDRW2100027670.1.1",
          "form": "정규직이 좋긴좋죠",
          "original_form": "정규직이 좋긴좋죠",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027670.1.2",
          "form": "안좋네요ㅋㅋㅋㅠ정년보장된다던데",
          "original_form": "안좋네요ㅋㅋㅋㅠ정년보장된다던데",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.3",
          "form": "근데 사실 무기계약직은",
          "original_form": "근데 사실 무기계약직은",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.4",
          "form": "사람들한테 잘 안 알려져있고",
          "original_form": "사람들한테 잘 안 알려져있고",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.5",
          "form": "계약직이란 말 때문에 더 인식이 안 좋게 느껴지는 것 같아요",
          "original_form": "계약직이란 말 때문에 더 인식이 안 좋게 느껴지는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.6",
          "form": "안그래도 지방공공기관 이번에 5배수 면접보라고 와서... 고민중이거든요",
          "original_form": "안그래도 지방공공기관 이번에 5배수 면접보라고 와서... 고민중이거든요",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.7",
          "form": "정규직을 우선으로 하니까 아무래도ㅎㅎㅎ",
          "original_form": "정규직을 우선으로 하니까 아무래도ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.8",
          "form": "맞아요 이름만 바꿔도 훨씬 좋을텐데!!",
          "original_form": "맞아요 이름만 바꿔도 훨씬 좋을텐데!!",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.9",
          "form": "정년 보장되면 괜찮은데요??",
          "original_form": "정년 보장되면 괜찮은데요??",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.10",
          "form": "무기 계약직이랑 정규직이랑 사실",
          "original_form": "무기 계약직이랑 정규직이랑 사실",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.11",
          "form": "워라벨은 더 좋지만 차별이 있겠죠ㅠ월급 200만원 받으면서 같은 일한다든지...",
          "original_form": "워라벨은 더 좋지만 차별이 있겠죠ㅠ월급 200만원 받으면서 같은 일한다든지...",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027670.1.12",
          "form": "계약 갱신하는 거 빼고는 크게 다른 거 있을까요..?",
          "original_form": "계약 갱신하는 거 빼고는 크게 다른 거 있을까요..?",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.13",
          "form": "ㅠㅠㅠㅠㅠ맞아요 돈 문제는 있죠",
          "original_form": "ㅠㅠㅠㅠㅠ맞아요 돈 문제는 있죠",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.14",
          "form": "연봉상승률이 다르긴 하대요!!!ㅠㅠㅠㅠ",
          "original_form": "연봉상승률이 다르긴 하대요!!!ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.15",
          "form": "대신 책임을 덜지구",
          "original_form": "대신 책임을 덜지구",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.16",
          "form": "저 대학때 인턴햇던데도",
          "original_form": "저 대학때 인턴햇던데도",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.17",
          "form": "아하 장단점이 있네요",
          "original_form": "아하 장단점이 있네요",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.18",
          "form": "정규직이 270인데 야근조금더하고",
          "original_form": "정규직이 270인데 야근조금더하고",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.19",
          "form": "무기계약직분은 190이구 6시칼퇴고",
          "original_form": "무기계약직분은 190이구 6시칼퇴고",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.20",
          "form": "사실 저도",
          "original_form": "사실 저도",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.21",
          "form": "일은 똑같구...그러더라구요",
          "original_form": "일은 똑같구...그러더라구요",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.23",
          "form": "워라밸 엄청 중시하는 편이라",
          "original_form": "워라밸 엄청 중시하는 편이라",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027670.1.24",
          "form": "듣고나니 무기계약직이 더 나아보이네요...",
          "original_form": "듣고나니 무기계약직이 더 나아보이네요...",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027670.1.25",
          "form": "ㅋㅋㅋㅋ그쵸!?!? 워라벨",
          "original_form": "ㅋㅋㅋㅋ그쵸!?!? 워라벨",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027670.1.26",
          "form": "법원직 준비하는 것도",
          "original_form": "법원직 준비하는 것도",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027670.1.27",
          "form": "자기 할 일만 다 하면 아무 상관 안하고",
          "original_form": "자기 할 일만 다 하면 아무 상관 안하고",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027670.1.28",
          "form": "휴가도 맘대로 쓸 수 있다고 해서 준비하고 있는게 좀 있어여ㅋㅋㅋㅋ",
          "original_form": "휴가도 맘대로 쓸 수 있다고 해서 준비하고 있는게 좀 있어여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027670.1.29",
          "form": "오ㅋㅋㅋ일반 행정직보다요??!",
          "original_form": "오ㅋㅋㅋ일반 행정직보다요??!",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.30",
          "form": "휴가 끝나고 오면 자기 일 쌓여있긴 하겠지만...",
          "original_form": "휴가 끝나고 오면 자기 일 쌓여있긴 하겠지만...",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.31",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.32",
          "form": "일행은 아무래도",
          "original_form": "일행은 아무래도",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.33",
          "form": "저는 주변에 다 일행으로 갔는데",
          "original_form": "저는 주변에 다 일행으로 갔는데",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.34",
          "form": "행정부 소속이라 좀 힘들죠",
          "original_form": "행정부 소속이라 좀 힘들죠",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.35",
          "form": "동사무소 이런데는 민원인도 그렇고",
          "original_form": "동사무소 이런데는 민원인도 그렇고",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.36",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.37",
          "form": "텃세도 있고,. 조직문화가 세더라고요",
          "original_form": "텃세도 있고,. 조직문화가 세더라고요",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027670.1.38",
          "form": "그대신 법원직 시험이조금더어렵죠?ㅠ",
          "original_form": "그대신 법원직 시험이조금더어렵죠?ㅠ",
