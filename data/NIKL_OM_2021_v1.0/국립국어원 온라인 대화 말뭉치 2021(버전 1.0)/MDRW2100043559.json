      "utterance": [
        {
          "id": "MDRW2100043559.1.1",
          "form": "건강을 위해서 무얼 하시나요",
          "original_form": "건강을 위해서 무얼 하시나요",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043559.1.2",
          "form": "운동이요",
          "original_form": "운동이요",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043559.1.3",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043559.1.4",
          "form": "어떤 운동 하시나요",
          "original_form": "어떤 운동 하시나요",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043559.1.5",
          "form": "필라테스해요",
          "original_form": "필라테스해요",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043559.1.6",
          "form": "주2회",
          "original_form": "주2회",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043559.1.7",
          "form": "하기 힘든 운동 아닌가요",
          "original_form": "하기 힘든 운동 아닌가요",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043559.1.8",
          "form": "좀 근력운동이 있긴한데",
          "original_form": "좀 근력운동이 있긴한데",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043559.1.9",
          "form": "하고나면 온몸이 시원해요",
          "original_form": "하고나면 온몸이 시원해요",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043559.1.10",
          "form": "보기에도 어렵고 힘들어 보이더라고요",
          "original_form": "보기에도 어렵고 힘들어 보이더라고요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043559.1.11",
          "form": "근데이젠 안하면 몸이 무거워요",
          "original_form": "근데이젠 안하면 몸이 무거워요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043559.1.12",
          "form": "따로하시는 운동은 없으세요?",
          "original_form": "따로하시는 운동은 없으세요?",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043559.1.13",
          "form": "전 동네 1시간 걷기를 해요",
          "original_form": "전 동네 1시간 걷기를 해요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043559.1.14",
          "form": "걷기도 좋죠",
          "original_form": "걷기도 좋죠",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100043559.1.15",
          "form": "여름에도 하셨나요?",
          "original_form": "여름에도 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100043559.1.16",
          "form": "여름엔 더 열심히 하게 되요",
          "original_form": "여름엔 더 열심히 하게 되요",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100043559.1.17",
          "form": "추울땐 더 하기 힘들 어요",
          "original_form": "추울땐 더 하기 힘들 어요",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100043559.1.18",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100043559.1.19",
          "form": "여름에가 더 하기힘들것같았어요",
          "original_form": "여름에가 더 하기힘들것같았어요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100043559.1.20",
          "form": "전 추운것 보단 땀많이나는 여름이 좋아요",
          "original_form": "전 추운것 보단 땀많이나는 여름이 좋아요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100043559.1.21",
          "form": "땀 흘리고 나면 개운해요",
          "original_form": "땀 흘리고 나면 개운해요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        }
      ]
