      "utterance": [
        {
          "id": "MDRW2100036429.1.1",
          "form": "안녕 오랜만이야",
          "original_form": "안녕 오랜만이야",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036429.1.2",
          "form": "나 떡보ㄸ이 먹었어",
          "original_form": "나 떡보ㄸ이 먹었어",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036429.1.3",
          "form": "신전떡볶이 알아?",
          "original_form": "신전떡볶이 알아?",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036429.1.4",
          "form": "응응 그거 별로 안맵다던데",
          "original_form": "응응 그거 별로 안맵다던데",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.5",
          "form": "너는 어떤 맛 먹어?",
          "original_form": "너는 어떤 맛 먹어?",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.6",
          "form": "나는 매운 맛 먹었어",
          "original_form": "나는 매운 맛 먹었어",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.7",
          "form": "나도 매운맛 먹어",
          "original_form": "나도 매운맛 먹어",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.8",
          "form": "치즈 추가해서",
          "original_form": "치즈 추가해서",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.9",
          "form": "많이 맵더라",
          "original_form": "많이 맵더라",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.10",
          "form": "아 정말? 보통맛 먹는고 아냐?",
          "original_form": "아 정말? 보통맛 먹는고 아냐?",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.11",
          "form": "치즈 추가하면 나중에 딱딱해지지않아?",
          "original_form": "치즈 추가하면 나중에 딱딱해지지않아?",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.12",
          "form": "응 나는 매운맛 마스터거든",
          "original_form": "응 나는 매운맛 마스터거든",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.13",
          "form": "이미 집에오니 굳어있더랴",
          "original_form": "이미 집에오니 굳어있더랴",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.14",
          "form": "가위로 치즈판응 잘라먹엇어",
          "original_form": "가위로 치즈판응 잘라먹엇어",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.15",
          "form": "매마라니 대단한걸",
          "original_form": "매마라니 대단한걸",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036429.1.16",
          "form": "매마라니 ㅌㅋㅋㅋ 웃기는 줄임맣이다",
          "original_form": "매마라니 ㅌㅋㅋㅋ 웃기는 줄임맣이다",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036429.1.17",
          "form": "나는 그래서 치즈추가는 안해",
          "original_form": "나는 그래서 치즈추가는 안해",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036429.1.18",
          "form": "요즘 유행하는 망이야",
          "original_form": "요즘 유행하는 망이야",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036429.1.19",
          "form": "너 거짓말 하는건 아니지?",
          "original_form": "너 거짓말 하는건 아니지?",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036429.1.20",
          "form": "응 당연하지 난 진실만말해",
          "original_form": "응 당연하지 난 진실만말해",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036429.1.21",
          "form": "차즈 뿌려먹는거 어제도 봤는걸",
          "original_form": "차즈 뿌려먹는거 어제도 봤는걸",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036429.1.22",
          "form": "뭐? 어떤음식에?",
          "original_form": "뭐? 어떤음식에?",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036429.1.23",
          "form": "모든 음식에",
          "original_form": "모든 음식에",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036429.1.24",
          "form": "너 치즈마니아라던데?",
          "original_form": "너 치즈마니아라던데?",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036429.1.25",
          "form": "그건 좀 너무 느끼할 것 같아",
          "original_form": "그건 좀 너무 느끼할 것 같아",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036429.1.26",
          "form": "치즈밥도 즐겨먹는다더라",
          "original_form": "치즈밥도 즐겨먹는다더라",
          "speaker_id": "2",
          "time": "20210830 23:02"
        }
      ]
    }
  ]
