      "utterance": [
        {
          "id": "MDRW2100004047.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004047.1.2",
          "form": "안녕하세용 ㅎㅎ 중고거래 해보신적있나요",
          "original_form": "안녕하세용 ㅎㅎ 중고거래 해보신적있나요",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004047.1.3",
          "form": "중고거래 한번 해봤어요!",
          "original_form": "중고거래 한번 해봤어요!",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004047.1.4",
          "form": "당근으로 닌텐도 게임 구입해봤었네요",
          "original_form": "당근으로 닌텐도 게임 구입해봤었네요",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004047.1.5",
          "form": "오 전 닌텐도 없어용",
          "original_form": "오 전 닌텐도 없어용",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004047.1.6",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004047.1.7",
          "form": "닌텐도 재밌어요!",
          "original_form": "닌텐도 재밌어요!",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004047.1.8",
          "form": "게임 관심 있으시면 꼭 해보셍ㅅ",
          "original_form": "게임 관심 있으시면 꼭 해보셍ㅅ",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004047.1.9",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004047.1.10",
          "form": "저도 무슨 기계를 그리 비싸게 주고 사냐",
          "original_form": "저도 무슨 기계를 그리 비싸게 주고 사냐",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004047.1.11",
          "form": "라고 생각했었는데 젤다 진짜 재밌더라구요...",
          "original_form": "라고 생각했었는데 젤다 진짜 재밌더라구요...",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004047.1.12",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004047.1.13",
          "form": "닌텐도 있는사람들은 팩?",
          "original_form": "닌텐도 있는사람들은 팩?",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004047.1.14",
          "form": "사는거 엄청 열심히던뎅 ㅋㅋ",
          "original_form": "사는거 엄청 열심히던뎅 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004047.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004047.1.16",
          "form": "저도 열심히 사고있죠...",
          "original_form": "저도 열심히 사고있죠...",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004047.1.17",
          "form": "뭔가 기계를 사니",
          "original_form": "뭔가 기계를 사니",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004047.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004047.1.19",
          "form": "열심히 플레이해야겠다",
          "original_form": "열심히 플레이해야겠다",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004047.1.20",
          "form": "싶은....",
          "original_form": "싶은....",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004047.1.21",
          "form": "닌텐도를 한번도 해본적이없어영 ..",
          "original_form": "닌텐도를 한번도 해본적이없어영 ..",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004047.1.22",
          "form": "궁금하당",
          "original_form": "궁금하당",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004047.1.23",
          "form": "요새",
          "original_form": "요새",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004047.1.24",
          "form": "당근으로 싸게 많이 올라오더라구요",
          "original_form": "당근으로 싸게 많이 올라오더라구요",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004047.1.25",
          "form": "잘 살펴보세요!",
          "original_form": "잘 살펴보세요!",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004047.1.26",
          "form": "중고나라 그런곳은 인터넷이라 좀 못믿어운데",
          "original_form": "중고나라 그런곳은 인터넷이라 좀 못믿어운데",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004047.1.27",
          "form": "당근은 직거래라 괜찮더라구요",
          "original_form": "당근은 직거래라 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004047.1.28",
          "form": "한번 살펴봐야겟어요",
          "original_form": "한번 살펴봐야겟어요",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004047.1.29",
          "form": "최근에 뭐구매하신거있어요?",
          "original_form": "최근에 뭐구매하신거있어요?",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004047.1.30",
          "form": "음 저는",
          "original_form": "음 저는",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004047.1.31",
          "form": "최근에는 주로 먹을것...",
          "original_form": "최근에는 주로 먹을것...",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004047.1.32",
          "form": "먹을것밖에 없네요...ㅎㅎㅎ",
          "original_form": "먹을것밖에 없네요...ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004047.1.33",
          "form": "어제는 파바에서 빵사구",
          "original_form": "어제는 파바에서 빵사구",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004047.1.34",
          "form": "엔젤리너스에서 반미샀네용",
          "original_form": "엔젤리너스에서 반미샀네용",
          "speaker_id": "1",
          "time": "20210821 15:04"
        }
      ]
    }
