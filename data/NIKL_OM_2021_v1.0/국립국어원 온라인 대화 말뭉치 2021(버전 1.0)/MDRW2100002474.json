      "utterance": [
        {
          "id": "MDRW2100002474.1.1",
          "form": "요즘 운동에 관심이 많이 생기더라",
          "original_form": "요즘 운동에 관심이 많이 생기더라",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002474.1.2",
          "form": "운동이 트렌드가 된 것 같아",
          "original_form": "운동이 트렌드가 된 것 같아",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002474.1.3",
          "form": "너는 운동에 관심 있어?",
          "original_form": "너는 운동에 관심 있어?",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002474.1.4",
          "form": "움직이는걸 싫어해서 없어",
          "original_form": "움직이는걸 싫어해서 없어",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002474.1.5",
          "form": "너는 운동 좋아해?",
          "original_form": "너는 운동 좋아해?",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002474.1.6",
          "form": "나는 수영 좋아해",
          "original_form": "나는 수영 좋아해",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002474.1.7",
          "form": "수영장 가고 싶은데",
          "original_form": "수영장 가고 싶은데",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002474.1.8",
          "form": "코로나때문에 못 가",
          "original_form": "코로나때문에 못 가",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002474.1.9",
          "form": "아무래도 수영장은 위험니까",
          "original_form": "아무래도 수영장은 위험니까",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002474.1.10",
          "form": "너무 슬퍼",
          "original_form": "너무 슬퍼",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002474.1.11",
          "form": "다른 운동은 좋아한다고 보긴 어렵고",
          "original_form": "다른 운동은 좋아한다고 보긴 어렵고",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002474.1.12",
          "form": "해야하니까 하는 수준이야",
          "original_form": "해야하니까 하는 수준이야",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002474.1.13",
          "form": "오 그래도 운동 하는구나",
          "original_form": "오 그래도 운동 하는구나",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002474.1.14",
          "form": "나는 정말 누워있어서",
          "original_form": "나는 정말 누워있어서",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002474.1.15",
          "form": "운동 때문에 누워있는것보다 더 피곤해",
          "original_form": "운동 때문에 누워있는것보다 더 피곤해",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002474.1.16",
          "form": "운동하면 그럴만하지",
          "original_form": "운동하면 그럴만하지",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002474.1.17",
          "form": "그래도 수영가고싶다",
          "original_form": "그래도 수영가고싶다",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002474.1.18",
          "form": "나 수영장 중학교 때가 마지막이라 신기해",
          "original_form": "나 수영장 중학교 때가 마지막이라 신기해",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002474.1.19",
          "form": "나랑 코로나 끝나고 가자",
          "original_form": "나랑 코로나 끝나고 가자",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002474.1.20",
          "form": "언제 끝날지 모르겠지만 그래",
          "original_form": "언제 끝날지 모르겠지만 그래",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002474.1.21",
          "form": "수영도 하고 테니스도 하자",
          "original_form": "수영도 하고 테니스도 하자",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002474.1.22",
          "form": "갑자기 테니스?",
          "original_form": "갑자기 테니스?",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002474.1.23",
          "form": "테니스 뭔가 간지나",
          "original_form": "테니스 뭔가 간지나",
          "speaker_id": "1",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002474.1.24",
          "form": "나 테니스는 티비로 본게 끝이라서 전혀 할 줄 몰라",
          "original_form": "나 테니스는 티비로 본게 끝이라서 전혀 할 줄 몰라",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002474.1.25",
          "form": "사실 나도 그래",
          "original_form": "사실 나도 그래",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002474.1.26",
          "form": "같이 배우러 학원가자",
          "original_form": "같이 배우러 학원가자",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002474.1.27",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210820 15:19"
        }
      ]
    }
  ]
