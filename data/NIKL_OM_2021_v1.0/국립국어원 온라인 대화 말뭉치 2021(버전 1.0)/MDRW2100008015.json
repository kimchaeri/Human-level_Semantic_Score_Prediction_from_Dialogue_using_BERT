      "utterance": [
        {
          "id": "MDRW2100008015.1.1",
          "form": "점심 식사는 하셨나요?",
          "original_form": "점심 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100008015.1.2",
          "form": "어머 벌써 시간이",
          "original_form": "어머 벌써 시간이",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008015.1.3",
          "form": "삼각김밥이랑 과자를 잔뜩 사왔어요 ㅎㅎ",
          "original_form": "삼각김밥이랑 과자를 잔뜩 사왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008015.1.4",
          "form": "그러고보니 삼각김밥\r\n먹어본지도 오래 되었네요:)",
          "original_form": "그러고보니 삼각김밥\r\n먹어본지도 오래 되었네요:)",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100008015.1.5",
          "form": "어떤거 사셨어요?",
          "original_form": "어떤거 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100008015.1.6",
          "form": "두개 붙어있는거요",
          "original_form": "두개 붙어있는거요",
          "speaker_id": "2",
          "time": "20210822 12:45"
        },
        {
          "id": "MDRW2100008015.1.7",
          "form": "간장 매콤 불고기세트 ㅎㅎ",
          "original_form": "간장 매콤 불고기세트 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:45"
        },
        {
          "id": "MDRW2100008015.1.8",
          "form": "맛있겠네요:)",
          "original_form": "맛있겠네요:)",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008015.1.9",
          "form": "저는 참치마요 좋아했었는데\r\n조만간 사먹어야겠어요",
          "original_form": "저는 참치마요 좋아했었는데\r\n조만간 사먹어야겠어요",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008015.1.10",
          "form": "은근 비싸요 1900원인데 깨알같이 100원 할인받았네요 ㅋㅋ",
          "original_form": "은근 비싸요 1900원인데 깨알같이 100원 할인받았네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008015.1.11",
          "form": "제가 삼각김밥을 안먹은지가\r\n오래되긴 했나봐요ㅋㅋ",
          "original_form": "제가 삼각김밥을 안먹은지가\r\n오래되긴 했나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008015.1.12",
          "form": "제가 기억하는 삼각김밥은\r\n700원인데...ㅋㅋ",
          "original_form": "제가 기억하는 삼각김밥은\r\n700원인데...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008015.1.13",
          "form": "저도 그 기억으로 갔는데 생각보다 비싸서 당황했어요 ㅋㅋ",
          "original_form": "저도 그 기억으로 갔는데 생각보다 비싸서 당황했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008015.1.14",
          "form": "그러네요ㅋㅋ 삼각김밥 3개인데\r\n왜 1900원 내고 2개 받아오셨어여ㅋㅋ",
          "original_form": "그러네요ㅋㅋ 삼각김밥 3개인데\r\n왜 1900원 내고 2개 받아오셨어여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008015.1.15",
          "form": "1개짜리는 1800  1500 1200하더라고여",
          "original_form": "1개짜리는 1800  1500 1200하더라고여",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008015.1.16",
          "form": "와...  너무하네요 증말",
          "original_form": "와...  너무하네요 증말",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008015.1.17",
          "form": "아님 크기가 그만큼 커진건가요?",
          "original_form": "아님 크기가 그만큼 커진건가요?",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008015.1.18",
          "form": "제 기억속의 삼각김밥과 그다지 다르지않아여 ㅋㅋ",
          "original_form": "제 기억속의 삼각김밥과 그다지 다르지않아여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008015.1.19",
          "form": "되려 작아진듯? ㅋㅋ",
          "original_form": "되려 작아진듯? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008015.1.20",
          "form": "주머니 사정은 똑같은데ㅋㅋ\r\n물가만 계속 오르네여",
          "original_form": "주머니 사정은 똑같은데ㅋㅋ\r\n물가만 계속 오르네여",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008015.1.21",
          "form": "벌써 또 저녁 먹을 시간이네요",
          "original_form": "벌써 또 저녁 먹을 시간이네요",
