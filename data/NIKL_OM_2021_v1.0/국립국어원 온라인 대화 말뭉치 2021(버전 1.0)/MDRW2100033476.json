          "original_form": "평생 불가능",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.27",
          "form": "ㄹㅇ 기상이변",
          "original_form": "ㄹㅇ 기상이변",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.29",
          "form": "저런거에 안웃는법",
          "original_form": "저런거에 안웃는법",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.30",
          "form": "제발 알려주세요",
          "original_form": "제발 알려주세요",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.31",
          "form": "님 오늘도 비왔어요",
          "original_form": "님 오늘도 비왔어요",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.32",
          "form": "제가 사는 지역에...",
          "original_form": "제가 사는 지역에...",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033476.1.33",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033476.1.34",
          "form": "글고 너무 꿉꿉해서",
          "original_form": "글고 너무 꿉꿉해서",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033476.1.35",
          "form": "제습기 틀었어요",
          "original_form": "제습기 틀었어요",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033476.1.36",
          "form": "비바람을 뚫고... 백신맞으러 간 저.",
          "original_form": "비바람을 뚫고... 백신맞으러 간 저.",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033476.1.37",
          "form": "하 진짜 저도요",
          "original_form": "하 진짜 저도요",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033476.1.38",
          "form": "저 오늘 샤워하지말라고 할까봐 미리 씻고 갔는데",
          "original_form": "저 오늘 샤워하지말라고 할까봐 미리 씻고 갔는데",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033476.1.39",
          "form": "비 덕에 2번씻고 이건 안씻으면 다른 병걸린다 싶어서 3번 씻었어요",
          "original_form": "비 덕에 2번씻고 이건 안씻으면 다른 병걸린다 싶어서 3번 씻었어요",
          "speaker_id": "1",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100033476.1.40",
          "form": "의미없는 선 샤워",
          "original_form": "의미없는 선 샤워",
          "speaker_id": "1",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100033476.1.41",
          "form": "아나 맞다 이날씨에 백신맞으러 가셨지 참",
          "original_form": "아나 맞다 이날씨에 백신맞으러 가셨지 참",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100033476.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100033476.1.43",
          "form": "엇 잠만 그럼 비맞고",
          "original_form": "엇 잠만 그럼 비맞고",
          "speaker_id": "2",
          "time": "20210825 00:41"
        },
