        },
        {
          "id": "MDRW2100033476.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100033476.1.19",
          "form": "저 진짜 그런거에 안 웃고 싶어요",
          "original_form": "저 진짜 그런거에 안 웃고 싶어요",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.20",
          "form": "ㄹㅇ 기상이변",
          "original_form": "ㄹㅇ 기상이변",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.22",
          "form": "언제 쯤이면..",
          "original_form": "언제 쯤이면..",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.23",
          "form": "엘리뇨 라니냐~!",
          "original_form": "엘리뇨 라니냐~!",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.24",
          "form": "안웃을 수 있을까요",
          "original_form": "안웃을 수 있을까요",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033476.1.25",
          "form": "하 찐짜웃기네",
          "original_form": "하 찐짜웃기네",
