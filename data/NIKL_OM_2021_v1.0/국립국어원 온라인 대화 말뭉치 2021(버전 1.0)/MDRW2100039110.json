      "utterance": [
        {
          "id": "MDRW2100039110.1.1",
          "form": "최근에 샀거나 살 예정인 물건 있나요?",
          "original_form": "최근에 샀거나 살 예정인 물건 있나요?",
          "speaker_id": "1",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100039110.1.2",
          "form": "가을옷 하나 샀어요",
          "original_form": "가을옷 하나 샀어요",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100039110.1.3",
          "form": "이제 가을 옷 살 시기죠",
          "original_form": "이제 가을 옷 살 시기죠",
          "speaker_id": "1",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039110.1.4",
          "form": "전 영양제 구입 예정입니다",
          "original_form": "전 영양제 구입 예정입니다",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039110.1.5",
          "form": "어떤 영양제 드세요?",
          "original_form": "어떤 영양제 드세요?",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039110.1.6",
          "form": "유산균 종합비타민 루테인지아잔틴 이요",
          "original_form": "유산균 종합비타민 루테인지아잔틴 이요",
          "speaker_id": "1",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039110.1.7",
          "form": "우와 많이 드시네요",
          "original_form": "우와 많이 드시네요",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039110.1.8",
          "form": "많은가요? ㅋㅋ 추가로 더 사야되요",
          "original_form": "많은가요? ㅋㅋ 추가로 더 사야되요",
          "speaker_id": "1",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039110.1.9",
          "form": "저는 센트룸 하나 먹다가 끊었어요",
          "original_form": "저는 센트룸 하나 먹다가 끊었어요",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039110.1.10",
          "form": "건강하면 영양제 사는 것보단 잘 먹는 게 낫죠 ㅎㅎ",
          "original_form": "건강하면 영양제 사는 것보단 잘 먹는 게 낫죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039110.1.11",
          "form": "건강한건 아니지만... 식단으로 해결하려고 하고 있습니다ㅎ",
          "original_form": "건강한건 아니지만... 식단으로 해결하려고 하고 있습니다ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039110.1.12",
          "form": "식사가 기본이죠",
          "original_form": "식사가 기본이죠",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039110.1.13",
          "form": "아무튼 전 추석혜택 노리고 있어요 ㅎㅎ",
          "original_form": "아무튼 전 추석혜택 노리고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039110.1.14",
          "form": "추석혜택이요?",
          "original_form": "추석혜택이요?",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039110.1.15",
          "form": "할인 하나요?",
          "original_form": "할인 하나요?",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039110.1.16",
          "form": "네 쿠폰 적립금 사은품 혜택이 많아요",
          "original_form": "네 쿠폰 적립금 사은품 혜택이 많아요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039110.1.17",
          "form": "와 한번 확인해봐야겠네요",
          "original_form": "와 한번 확인해봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039110.1.18",
          "form": "아마 사이트마다 추석 준비하라고 팝업창이나 공지로 알려줘요",
          "original_form": "아마 사이트마다 추석 준비하라고 팝업창이나 공지로 알려줘요",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039110.1.19",
          "form": "사이트 한바퀴 돌아보겠습니다",
          "original_form": "사이트 한바퀴 돌아보겠습니다",
