      "utterance": [
        {
          "id": "MDRW2100042872.1.1",
          "form": "내년에 결혼하셔요?",
          "original_form": "내년에 결혼하셔요?",
          "speaker_id": "1",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100042872.1.2",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100042872.1.3",
          "form": "와 ㅎ 식세기 좋죵 ㅎㅎ 꼭 리스트에 추가ㅎ",
          "original_form": "와 ㅎ 식세기 좋죵 ㅎㅎ 꼭 리스트에 추가ㅎ",
          "speaker_id": "1",
          "time": "20210907 10:34"
        },
        {
          "id": "MDRW2100042872.1.4",
          "form": "식세기 건조기 젤먼저 넣엇어요",
          "original_form": "식세기 건조기 젤먼저 넣엇어요",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100042872.1.5",
          "form": "로봇청소기도 살껀데",
          "original_form": "로봇청소기도 살껀데",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100042872.1.6",
          "form": "추천인가욤?",
          "original_form": "추천인가욤?",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100042872.1.7",
          "form": "아 바닥에 물건많이 없다면ㅋㅋ추천요",
          "original_form": "아 바닥에 물건많이 없다면ㅋㅋ추천요",
          "speaker_id": "1",
          "time": "20210907 10:40"
        },
        {
          "id": "MDRW2100042872.1.8",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210907 10:40"
        },
        {
          "id": "MDRW2100042872.1.9",
          "form": "애있으니 바닥에 뭐가 많아서 안되요ㅠ",
          "original_form": "애있으니 바닥에 뭐가 많아서 안되요ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:40"
        },
        {
          "id": "MDRW2100042872.1.10",
          "form": "그럴꺼같아요ㅜㅜ",
          "original_form": "그럴꺼같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.11",
          "form": "다치우고 돌려야하니",
          "original_form": "다치우고 돌려야하니",
          "speaker_id": "1",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.12",
          "form": "브라바 초창기 30마넌이나",
          "original_form": "브라바 초창기 30마넌이나",
          "speaker_id": "1",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.13",
          "form": "바닥에 매트도깔려잇고",
          "original_form": "바닥에 매트도깔려잇고",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.14",
          "form": "주고샀는데ㅠㅠ",
          "original_form": "주고샀는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.15",
          "form": "20번도안쓴것같네여ㅠ",
          "original_form": "20번도안쓴것같네여ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.16",
          "form": "안그래도ㅋㅋㅋ브라바",
          "original_form": "안그래도ㅋㅋㅋ브라바",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.17",
          "form": "살려구햇는데",
          "original_form": "살려구햇는데",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.18",
          "form": "직구로 샀는데ㅠ",
          "original_form": "직구로 샀는데ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.20",
          "form": "백스텝!!!!!!",
          "original_form": "백스텝!!!!!!",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.21",
          "form": "스타일러도",
          "original_form": "스타일러도",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.22",
          "form": "요새는 밀고 닦는것도",
          "original_form": "요새는 밀고 닦는것도",
          "speaker_id": "1",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.23",
          "form": "그닥일까요?",
          "original_form": "그닥일까요?",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042872.1.24",
          "form": "있잖아요 제꺼는 닦는것만ㅠ",
          "original_form": "있잖아요 제꺼는 닦는것만ㅠ",
