      "utterance": [
        {
          "id": "MDRW2100008666.1.1",
          "form": "아르바이트 해보셨나요",
          "original_form": "아르바이트 해보셨나요",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008666.1.2",
          "form": "넵 저 영화관 알바해봤어요 ㅋㅋㅋ",
          "original_form": "넵 저 영화관 알바해봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008666.1.3",
          "form": "엇 저도요",
          "original_form": "엇 저도요",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008666.1.4",
          "form": "전 메가박스",
          "original_form": "전 메가박스",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008666.1.5",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008666.1.6",
          "form": "헉 저는 경쟁사 cgv요",
          "original_form": "헉 저는 경쟁사 cgv요",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008666.1.7",
          "form": "미소지기였습니다 ㅋㅋㅋ",
          "original_form": "미소지기였습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008666.1.8",
          "form": "시지비 분위기는 어때요?",
          "original_form": "시지비 분위기는 어때요?",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008666.1.9",
          "form": "좀 자유로웠던 것 같아요",
          "original_form": "좀 자유로웠던 것 같아요",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008666.1.10",
          "form": "점바점이겠지만 전 좀 자유로운편이었어요",
          "original_form": "점바점이겠지만 전 좀 자유로운편이었어요",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008666.1.11",
          "form": "오 자유로우셨구나 어느파트에서 일하셨나요",
          "original_form": "오 자유로우셨구나 어느파트에서 일하셨나요",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008666.1.12",
          "form": "직영이 아니었나? 그래서 좀 더 자유로웠어요",
          "original_form": "직영이 아니었나? 그래서 좀 더 자유로웠어요",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008666.1.13",
          "form": "저희는 돌려가지고, 영사 담당만 빼고 다 했어요 ㅋㅋ",
          "original_form": "저희는 돌려가지고, 영사 담당만 빼고 다 했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008666.1.14",
          "form": "매점 해보셨나요",
          "original_form": "매점 해보셨나요",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008666.1.15",
          "form": "ㅎ 제일 힘든 파트 아닌가요",
          "original_form": "ㅎ 제일 힘든 파트 아닌가요",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008666.1.16",
          "form": "저 매점 첫날 들어가서 솥 태워먹었어요ㅎ",
          "original_form": "저 매점 첫날 들어가서 솥 태워먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008666.1.17",
          "form": "헉 태웠어요?????",
          "original_form": "헉 태웠어요?????",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008666.1.18",
          "form": "고의는 아니었는데.. 어쩌다보니 탔더라고요",
          "original_form": "고의는 아니었는데.. 어쩌다보니 탔더라고요",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008666.1.19",
          "form": "혼 안 나셨나요",
          "original_form": "혼 안 나셨나요",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008666.1.20",
          "form": "네.. 어쩌겠어요 1일차신입을 제대로 가르쳐주지않은 본인들을 탓해야지",
          "original_form": "네.. 어쩌겠어요 1일차신입을 제대로 가르쳐주지않은 본인들을 탓해야지",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008666.1.21",
          "form": "ㅋㅋㅋ 근데 진짜 고의는 아니었어요",
          "original_form": "ㅋㅋㅋ 근데 진짜 고의는 아니었어요",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008666.1.22",
          "form": "고의로 누가 태워요ㅠㅠ",
          "original_form": "고의로 누가 태워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008666.1.23",
          "form": "처음엔 할인 정보도 엄청 어려웠어요",
          "original_form": "처음엔 할인 정보도 엄청 어려웠어요",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008666.1.24",
          "form": "아 시지비는 할인이 많으니까",
          "original_form": "아 시지비는 할인이 많으니까",
          "speaker_id": "1",
          "time": "20210822 17:22"
        }
      ]
