      "utterance": [
        {
          "id": "MDRW2100017880.1.1",
          "form": "요근래에 코로나로 집에",
          "original_form": "요근래에 코로나로 집에",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017880.1.2",
          "form": "머무르는게 많아지니",
          "original_form": "머무르는게 많아지니",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017880.1.3",
          "form": "자연스럽게 책을 읽게 되더라구용 ㅎㅎ",
          "original_form": "자연스럽게 책을 읽게 되더라구용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017880.1.4",
          "form": "한번 나중에 사서 읽어보시는것도 좋고",
          "original_form": "한번 나중에 사서 읽어보시는것도 좋고",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017880.1.5",
          "form": "오오 좋은 자세에요",
          "original_form": "오오 좋은 자세에요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017880.1.6",
          "form": "그럴게요",
          "original_form": "그럴게요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017880.1.7",
          "form": "뭔가 나이 먹으니까",
          "original_form": "뭔가 나이 먹으니까",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017880.1.8",
          "form": "짧은 글들만 좋아져요 ㅠㅠ",
          "original_form": "짧은 글들만 좋아져요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017880.1.9",
          "form": "아 짧은 글 좋죠 ㅋㅋㅋ",
          "original_form": "아 짧은 글 좋죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017880.1.10",
          "form": "뭔가 함축적인게 좋더라고용",
          "original_form": "뭔가 함축적인게 좋더라고용",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017880.1.11",
          "form": "브런치 보세요?",
          "original_form": "브런치 보세요?",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017880.1.12",
          "form": "들어는 봣는데 보진않아용",
          "original_form": "들어는 봣는데 보진않아용",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017880.1.13",
          "form": "저는 주로 인스타나 페북에 나오는 짧은 그런 글들을 보곤해성",
          "original_form": "저는 주로 인스타나 페북에 나오는 짧은 그런 글들을 보곤해성",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017880.1.14",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017880.1.15",
          "form": "감성글 좋져 ㅎㅎ",
          "original_form": "감성글 좋져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017880.1.16",
          "form": "맞아요! 감성글 새벽에 보면",
          "original_form": "맞아요! 감성글 새벽에 보면",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017880.1.17",
          "form": "진짜 감성 다 젖어서 ㅋㅋㅋㅋ",
          "original_form": "진짜 감성 다 젖어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017880.1.18",
          "form": "새벽에 ㅋㅋㅋㅋ",
          "original_form": "새벽에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017880.1.19",
          "form": "시간대를 잘 찾으시네요",
          "original_form": "시간대를 잘 찾으시네요",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017880.1.20",
          "form": "뭔가 그 새벽감성 건드리기거",
          "original_form": "뭔가 그 새벽감성 건드리기거",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017880.1.21",
          "form": "건드리기가",
          "original_form": "건드리기가",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017880.1.22",
          "form": "폰트부터가 아련....",
          "original_form": "폰트부터가 아련....",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017880.1.23",
          "form": "딱 감성글 짧은글 보기에 딱 좋더라고요",
          "original_form": "딱 감성글 짧은글 보기에 딱 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017880.1.24",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017880.1.25",
          "form": "전 에스엔에스를 잘 안해서 ㅎㅎ",
          "original_form": "전 에스엔에스를 잘 안해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017880.1.26",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017880.1.27",
          "form": "대신 유튜브로 책 추천 글 추천 봐요",
          "original_form": "대신 유튜브로 책 추천 글 추천 봐요",
          "speaker_id": "1",
          "time": "20210826 21:34"
        }
      ]
    }
  ]
