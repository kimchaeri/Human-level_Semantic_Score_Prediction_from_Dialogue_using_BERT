      "utterance": [
        {
          "id": "MDRW2100017468.1.1",
          "form": "아 여기 썼네",
          "original_form": "아 여기 썼네",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.2",
          "form": "튜닝의 끝은 순정이다?",
          "original_form": "튜닝의 끝은 순정이다?",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.3",
          "form": "되게 까다롭네    말하고보니",
          "original_form": "되게 까다롭네    말하고보니",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.4",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.5",
          "form": "근데 그렇게 말하는 나는 사실",
          "original_form": "근데 그렇게 말하는 나는 사실",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.6",
          "form": "그러면 뭐가좋을까",
          "original_form": "그러면 뭐가좋을까",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.7",
          "form": "국물없는 라면을 가장 좋아한다는",
          "original_form": "국물없는 라면을 가장 좋아한다는",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.8",
          "form": "모순",
          "original_form": "모순",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017468.1.9",
          "form": "그러네?",
          "original_form": "그러네?",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.10",
          "form": "이정도면 순정이라고 봐줘",
          "original_form": "이정도면 순정이라고 봐줘",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.11",
          "form": "그러면 오늘 저녁은",
          "original_form": "그러면 오늘 저녁은",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.12",
          "form": "역시 국없라인가",
          "original_form": "역시 국없라인가",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.13",
          "form": "계량컵 물 550ml 딱맞춘",
          "original_form": "계량컵 물 550ml 딱맞춘",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.14",
          "form": "순정라면 어때",
          "original_form": "순정라면 어때",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.15",
          "form": "국없라 좀 없어보여 줄임말이",
          "original_form": "국없라 좀 없어보여 줄임말이",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.16",
          "form": "좋은 별명이 없을까",
          "original_form": "좋은 별명이 없을까",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.17",
          "form": "볶라",
          "original_form": "볶라",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017468.1.18",
          "form": "노우 라면은 볶라",
          "original_form": "노우 라면은 볶라",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017468.1.19",
          "form": "라면이즈 볶라!!",
          "original_form": "라면이즈 볶라!!",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017468.1.20",
          "form": "쓸데없이 고집있는편",
          "original_form": "쓸데없이 고집있는편",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017468.1.21",
          "form": "볶라는",
          "original_form": "볶라는",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017468.1.22",
          "form": "팔도비빔면 불닭볶음면",
          "original_form": "팔도비빔면 불닭볶음면",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
