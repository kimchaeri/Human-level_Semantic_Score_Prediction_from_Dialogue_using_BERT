      "utterance": [
        {
          "id": "MDRW2100002798.1.1",
          "form": "책봐요?",
          "original_form": "책봐요?",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002798.1.2",
          "form": "네 자주는 못봐요",
          "original_form": "네 자주는 못봐요",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002798.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002798.1.4",
          "form": "최근에 본책이 없네요 ㅋㅋㅋ",
          "original_form": "최근에 본책이 없네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002798.1.5",
          "form": "시간이 핑계오",
          "original_form": "시간이 핑계오",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002798.1.6",
          "form": "퇴근하고 집에오니 지금시간인데",
          "original_form": "퇴근하고 집에오니 지금시간인데",
          "speaker_id": "2",
          "time": "20210820 20:11"
        },
        {
          "id": "MDRW2100002798.1.7",
          "form": "책을 읽을수가...",
          "original_form": "책을 읽을수가...",
          "speaker_id": "2",
          "time": "20210820 20:11"
        },
        {
          "id": "MDRW2100002798.1.8",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100002798.1.9",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100002798.1.10",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100002798.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100002798.1.12",
          "form": "책읽어야지 하는 마음만 있네오",
          "original_form": "책읽어야지 하는 마음만 있네오",
          "speaker_id": "1",
          "time": "20210822 08:41"
        },
        {
          "id": "MDRW2100002798.1.13",
          "form": "노력해야겠네요",
          "original_form": "노력해야겠네요",
          "speaker_id": "1",
          "time": "20210825 07:50"
        },
        {
          "id": "MDRW2100002798.1.14",
          "form": "처음이 어렵죠",
          "original_form": "처음이 어렵죠",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100002798.1.15",
          "form": "도전",
          "original_form": "도전",
          "speaker_id": "1",
          "time": "20210828 08:58"
        },
        {
          "id": "MDRW2100002798.1.16",
          "form": "만화책 보고싶네요 ㅎㅎ",
          "original_form": "만화책 보고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 08:58"
        },
        {
          "id": "MDRW2100002798.1.17",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100002798.1.18",
          "form": "빌리는곳도 다 없어져서",
          "original_form": "빌리는곳도 다 없어져서",
          "speaker_id": "2",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100002798.1.19",
          "form": "보기가 쉽지 않네요",
          "original_form": "보기가 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100002798.1.20",
          "form": "만화책은 실물로 봐줘야하는데",
