      "utterance": [
        {
          "id": "MDRW2100025492.1.1",
          "form": "점심 드셨나요",
          "original_form": "점심 드셨나요",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025492.1.2",
          "form": "네 된장찌개 먹었어요",
          "original_form": "네 된장찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025492.1.3",
          "form": "집밥인가요 ㅎㅎ",
          "original_form": "집밥인가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025492.1.4",
          "form": "점심에 뭐 드셨어요?",
          "original_form": "점심에 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025492.1.5",
          "form": "삼계탕이요!! 3",
          "original_form": "삼계탕이요!! {emoji:❤}3{emoji:❤}",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025492.1.6",
          "form": "ㅋㅋ 맛있는 거 드셨네요",
          "original_form": "ㅋㅋ 맛있는 거 드셨네요",
          "speaker_id": "1",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025492.1.7",
          "form": "평소에 배달음식 자주 드시나요?",
          "original_form": "평소에 배달음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210901 15:02"
        },
        {
          "id": "MDRW2100025492.1.8",
          "form": "요새는 자주 먹는거 같아요",
          "original_form": "요새는 자주 먹는거 같아요",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025492.1.9",
          "form": "외식이 힘드니",
          "original_form": "외식이 힘드니",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025492.1.10",
          "form": "주로 어떤 거 시키세요?",
          "original_form": "주로 어떤 거 시키세요?",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025492.1.11",
          "form": "음 중국집이 제일 많고",
          "original_form": "음 중국집이 제일 많고",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025492.1.12",
          "form": "그 외는 백반!인듯 해요",
          "original_form": "그 외는 백반!인듯 해요",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025492.1.13",
          "form": "주로 뭐 시키시나요",
          "original_form": "주로 뭐 시키시나요",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025492.1.14",
          "form": "저는 자주 시키는 편은 아니라 시키면 치킨 아니면 피자가 대부분이에요",
          "original_form": "저는 자주 시키는 편은 아니라 시키면 치킨 아니면 피자가 대부분이에요",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025492.1.15",
          "form": "치킨피자가 노말하긴하죠",
          "original_form": "치킨피자가 노말하긴하죠",
          "speaker_id": "2",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100025492.1.16",
          "form": "지난주에 맵슐랭 처음 시켜봤는데 맛있더라구요",
          "original_form": "지난주에 맵슐랭 처음 시켜봤는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100025492.1.17",
