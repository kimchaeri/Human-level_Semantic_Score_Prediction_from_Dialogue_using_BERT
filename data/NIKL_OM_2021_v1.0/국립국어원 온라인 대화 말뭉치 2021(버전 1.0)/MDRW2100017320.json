      "utterance": [
        {
          "id": "MDRW2100017320.1.1",
          "form": "오늘 회사는 어떠셨나요",
          "original_form": "오늘 회사는 어떠셨나요",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017320.1.2",
          "form": "아 네... 욕을 쓰고싶지만",
          "original_form": "아 네... 욕을 쓰고싶지만",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017320.1.3",
          "form": "나름 잘 다녀왔어요",
          "original_form": "나름 잘 다녀왔어요",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017320.1.4",
          "form": "오랜만에 보는 사람들도 있어서 반가웠답니다",
          "original_form": "오랜만에 보는 사람들도 있어서 반가웠답니다",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017320.1.5",
          "form": "점심에 두찜 시켜먹었어요",
          "original_form": "점심에 두찜 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017320.1.6",
          "form": "법카찬스!!",
          "original_form": "법카찬스!!",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017320.1.7",
          "form": "우와 두찜!! 로제 드셨어요? 거기 묵은지찜닭도 맛있다던데 궁금하네요 ㅋㅋ 법카찬스가 정말 짱이죠",
          "original_form": "우와 두찜!! 로제 드셨어요? 거기 묵은지찜닭도 맛있다던데 궁금하네요 ㅋㅋ 법카찬스가 정말 짱이죠",
          "speaker_id": "1",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017320.1.8",
          "form": "로제는 월욜에 먹어서 오늘은 간장이랑 고추장!!",
          "original_form": "로제는 월욜에 먹어서 오늘은 간장이랑 고추장!!",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017320.1.9",
          "form": "저도 묵은지가 궁금했는데",
          "original_form": "저도 묵은지가 궁금했는데",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017320.1.10",
          "form": "다들 그건 말을 안하길래",
          "original_form": "다들 그건 말을 안하길래",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017320.1.11",
          "form": "짜져있었어요",
          "original_form": "짜져있었어요",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017320.1.12",
          "form": "저는 결정권이 없답니다~",
          "original_form": "저는 결정권이 없답니다~",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017320.1.13",
          "form": "회사란....ㅠㅠ 회사에서 그러면 식사메뉴같은건 주로 누가 결정하세요?",
          "original_form": "회사란....ㅠㅠ 회사에서 그러면 식사메뉴같은건 주로 누가 결정하세요?",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017320.1.14",
          "form": "목소리 큰사람이요~^^",
          "original_form": "목소리 큰사람이요~^^",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017320.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 전 배달어플이 없어서 ㅋㅋㅋㅋ걍 옆에서 구경만해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 전 배달어플이 없어서 ㅋㅋㅋㅋ걍 옆에서 구경만해요",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017320.1.16",
          "form": "ㅋㅋㅋ저희 회사랑 똑같네요 ㅋㅋㅋ목소리큰사람ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저희 회사랑 똑같네요 ㅋㅋㅋ목소리큰사람ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017320.1.17",
          "form": "이번주에 저희팀에서 어떤분이",
          "original_form": "이번주에 저희팀에서 어떤분이",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017320.1.18",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ어떤분이...에서 끝났지만 관심법으로 무슨말씀하시려는지 알았습니다",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ어떤분이...에서 끝났지만 관심법으로 무슨말씀하시려는지 알았습니다",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017320.1.19",
          "form": "퇴사하고 싶어요 저도",
          "original_form": "퇴사하고 싶어요 저도",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017320.1.20",
          "form": "누가 퇴사한다고 하니까 저도 퇴사하고싶더라구오",
          "original_form": "누가 퇴사한다고 하니까 저도 퇴사하고싶더라구오",
          "speaker_id": "1",
          "time": "20210826 19:15"
