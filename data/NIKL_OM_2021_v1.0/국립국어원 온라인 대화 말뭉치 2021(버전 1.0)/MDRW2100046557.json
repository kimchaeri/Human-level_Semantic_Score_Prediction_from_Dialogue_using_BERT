        {
          "id": "MDRW2100046557.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211029 14:47"
        },
        {
          "id": "MDRW2100046557.1.3",
          "form": "글램핑 가보신 적 있으신가요?",
          "original_form": "글램핑 가보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20211029 14:47"
        },
        {
          "id": "MDRW2100046557.1.4",
          "form": "네 몇번가봤지요",
          "original_form": "네 몇번가봤지요",
          "speaker_id": "1",
          "time": "20211029 14:47"
        },
        {
          "id": "MDRW2100046557.1.5",
          "form": "캠핑 좋아하시나봐요~!",
          "original_form": "캠핑 좋아하시나봐요~!",
          "speaker_id": "2",
          "time": "20211029 14:47"
        },
        {
          "id": "MDRW2100046557.1.6",
          "form": "어디가 제일 좋으셨어요? ㅎㅎ",
          "original_form": "어디가 제일 좋으셨어요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211029 14:47"
        },
        {
          "id": "MDRW2100046557.1.7",
          "form": "고기좋아해요",
          "original_form": "고기좋아해요",
          "speaker_id": "1",
          "time": "20211029 14:47"
        },
        {
          "id": "MDRW2100046557.1.8",
          "form": "캠핑 가서 구워먹는 바베큐요?",
          "original_form": "캠핑 가서 구워먹는 바베큐요?",
          "speaker_id": "2",
          "time": "20211029 14:48"
        },
        {
          "id": "MDRW2100046557.1.9",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20211029 14:48"
        },
        {
          "id": "MDRW2100046557.1.10",
          "form": "산이보이는곳이좋더라구요",
          "original_form": "산이보이는곳이좋더라구요",
          "speaker_id": "1",
          "time": "20211029 14:48"
        },
        {
          "id": "MDRW2100046557.1.11",
          "form": "크으 원래 나가서 먹으면 라면도 더 맛있게 느껴지잖아요 ㅎㅎ",
          "original_form": "크으 원래 나가서 먹으면 라면도 더 맛있게 느껴지잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211029 14:48"
        },
        {
          "id": "MDRW2100046557.1.12",
          "form": "네 라면 맛있게먹을려고 가는거죠ㅎ",
          "original_form": "네 라면 맛있게먹을려고 가는거죠ㅎ",
          "speaker_id": "1",
          "time": "20211029 14:49"
        },
        {
          "id": "MDRW2100046557.1.13",
          "form": "이번 가을엔 캠핑 계획 없으신가요~?",
          "original_form": "이번 가을엔 캠핑 계획 없으신가요~?",
          "speaker_id": "2",
          "time": "20211029 14:49"
        },
        {
          "id": "MDRW2100046557.1.14",
          "form": "계획은없는데 많이가고싶네요단풍구경도하고",
          "original_form": "계획은없는데 많이가고싶네요단풍구경도하고",
          "speaker_id": "1",
          "time": "20211029 14:49"
        },
        {
          "id": "MDRW2100046557.1.15",
          "form": "그쵸 가을이 짧아서 항상 아쉬워요",
          "original_form": "그쵸 가을이 짧아서 항상 아쉬워요",
          "speaker_id": "2",
          "time": "20211029 14:49"
        },
        {
          "id": "MDRW2100046557.1.16",
          "form": "캠핑 용품도 구매하셨나요?",
          "original_form": "캠핑 용품도 구매하셨나요?",
          "speaker_id": "2",
          "time": "20211029 14:49"
        },
        {
          "id": "MDRW2100046557.1.17",
          "form": "아뇨 사고싶은건많아요ㅎ",
          "original_form": "아뇨 사고싶은건많아요ㅎ",
          "speaker_id": "1",
          "time": "20211029 14:50"
        },
        {
          "id": "MDRW2100046557.1.18",
          "form": "저는 캠핑 의자가 너무 갖고싶어요 ㅋㅋ 엄청 편해보이더라구요",
