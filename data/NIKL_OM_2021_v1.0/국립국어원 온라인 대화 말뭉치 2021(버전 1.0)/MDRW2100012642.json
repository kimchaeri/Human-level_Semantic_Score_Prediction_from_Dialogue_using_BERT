      "utterance": [
        {
          "id": "MDRW2100012642.1.1",
          "form": "엄마 아직도 팔 아파?",
          "original_form": "엄마 아직도 팔 아파?",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012642.1.2",
          "form": "어제보단 괜찮아졌어.",
          "original_form": "어제보단 괜찮아졌어.",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012642.1.3",
          "form": "너는 귀 어때.",
          "original_form": "너는 귀 어때.",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012642.1.4",
          "form": "나도 이제 심하게 아프진 않은데 아직도 가끔 따끔 거려",
          "original_form": "나도 이제 심하게 아프진 않은데 아직도 가끔 따끔 거려",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012642.1.5",
          "form": "심하진 않아도 아직도 아프긴 해",
          "original_form": "심하진 않아도 아직도 아프긴 해",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012642.1.6",
          "form": "내일도 그러면 꼭 병원 가 봐.",
          "original_form": "내일도 그러면 꼭 병원 가 봐.",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012642.1.7",
          "form": "중이염이면 어떡해.",
          "original_form": "중이염이면 어떡해.",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012642.1.8",
          "form": "그러면 귀가 심하게 안 들리지 않았을까?",
          "original_form": "그러면 귀가 심하게 안 들리지 않았을까?",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012642.1.9",
          "form": "너무 걱정하지마 심한 건 아닌 것 같아",
          "original_form": "너무 걱정하지마 심한 건 아닌 것 같아",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012642.1.10",
          "form": "아까 저녁을 급하게 먹었더니 좀 체한 듯.",
          "original_form": "아까 저녁을 급하게 먹었더니 좀 체한 듯.",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012642.1.11",
          "form": "요즘 자주 그러네 엄마",
          "original_form": "요즘 자주 그러네 엄마",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012642.1.12",
          "form": "먹고 바로 눕지 말고 1시간 동안은 좀 앉아 있어~",
          "original_form": "먹고 바로 눕지 말고 1시간 동안은 좀 앉아 있어~",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012642.1.13",
          "form": "요즘엔 바로 안 누워",
          "original_form": "요즘엔 바로 안 누워",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012642.1.14",
          "form": "원래 소화기관이 좀 안 좋잖아",
          "original_form": "원래 소화기관이 좀 안 좋잖아",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012642.1.15",
          "form": "그래도... 집에 사이클 있잖아 그거라도 타",
          "original_form": "그래도... 집에 사이클 있잖아 그거라도 타",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012642.1.16",
          "form": "티비 보면서 타면 좋잖아",
          "original_form": "티비 보면서 타면 좋잖아",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012642.1.17",
          "form": "어. 살도 뺄 겸 그래야겠어",
          "original_form": "어. 살도 뺄 겸 그래야겠어",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012642.1.18",
          "form": "어제도 탔는데 힘들더라.",
          "original_form": "어제도 탔는데 힘들더라.",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012642.1.19",
          "form": "평소에 운동을 너무 안해서 그래~~",
          "original_form": "평소에 운동을 너무 안해서 그래~~",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012642.1.20",
          "form": "날 좋아지면 저녁 먹고 산책이라도 다녀야겠어",
          "original_form": "날 좋아지면 저녁 먹고 산책이라도 다녀야겠어",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012642.1.21",
          "form": "막내랑 같이 다녀. 걔도 요새 다이어트 하더라",
          "original_form": "막내랑 같이 다녀. 걔도 요새 다이어트 하더라",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012642.1.22",
          "form": "응, 요즘 살 뺀다고 매일 유튜브 보면서 혼자 운동해.",
          "original_form": "응, 요즘 살 뺀다고 매일 유튜브 보면서 혼자 운동해.",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012642.1.23",
          "form": "아빠 오늘 백신 주사 맞았다.",
          "original_form": "아빠 오늘 백신 주사 맞았다.",
          "speaker_id": "2",
          "time": "20210825 21:37"
        }
      ]
