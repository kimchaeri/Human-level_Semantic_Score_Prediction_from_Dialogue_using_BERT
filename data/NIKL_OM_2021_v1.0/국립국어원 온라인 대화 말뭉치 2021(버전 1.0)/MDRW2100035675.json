      "utterance": [
        {
          "id": "MDRW2100035675.1.1",
          "form": "딱 좋은 날이 한 일주일정도가죠",
          "original_form": "딱 좋은 날이 한 일주일정도가죠",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035675.1.2",
          "form": "그담에 훅덥고요ㅋㅋ",
          "original_form": "그담에 훅덥고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035675.1.3",
          "form": "네네ㅋㅋㅋ 반팔 꺼내기 시작하죠",
          "original_form": "네네ㅋㅋㅋ 반팔 꺼내기 시작하죠",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035675.1.4",
          "form": "사계절 내내 선크림 바르시나용?",
          "original_form": "사계절 내내 선크림 바르시나용?",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035675.1.5",
          "form": "넵 바릅니다",
          "original_form": "넵 바릅니다",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.6",
          "form": "습관이에요",
          "original_form": "습관이에요",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.7",
          "form": "전 겨울에도 자외선이 여름 못지않게 강하다는걸",
          "original_form": "전 겨울에도 자외선이 여름 못지않게 강하다는걸",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.8",
          "form": "얼마전에 알았어요",
          "original_form": "얼마전에 알았어요",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.9",
          "form": "겨울만큼은 믿고있었거든요ㅋㅋㅋ",
          "original_form": "겨울만큼은 믿고있었거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.10",
          "form": "좀 귀찮아져서 스틱형으로 바르기시작했지만요ㅋㅋ",
          "original_form": "좀 귀찮아져서 스틱형으로 바르기시작했지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.11",
          "form": "오오ㅋㅋㅋ 스틱형 간편하죵ㅋㅋㅋ",
          "original_form": "오오ㅋㅋㅋ 스틱형 간편하죵ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.12",
          "form": "친구가 스키를 타고왔는데",
          "original_form": "친구가 스키를 타고왔는데",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.13",
          "form": "name1님..! 자외선은..! name1님의 눈에 무언가 보인다? 싶으면 자외선이 있는겁니다..!",
          "original_form": "&name1&님..! 자외선은..! &name1&님의 눈에 무언가 보인다? 싶으면 자외선이 있는겁니다..!",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.14",
          "form": "겨울인데도 타서 왔더라구요",
          "original_form": "겨울인데도 타서 왔더라구요",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.15",
          "form": "헉ㅠㅠㅠ 슬퍼요ㅠㅠㅠ",
          "original_form": "헉ㅠㅠㅠ 슬퍼요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035675.1.16",
          "form": "눈앞에 사물이 식별될정도면 자외선이 있는거에요..!",
          "original_form": "눈앞에 사물이 식별될정도면 자외선이 있는거에요..!",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035675.1.17",
          "form": "겨울 믿으시면 안됩니다",
          "original_form": "겨울 믿으시면 안됩니다",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035675.1.18",
          "form": "그렇군요....ㅠ",
          "original_form": "그렇군요....ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035675.1.19",
          "form": "겨울의 배신ㅋㅋㅋ",
          "original_form": "겨울의 배신ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035675.1.20",
          "form": "스키타러가면 특히 잘타요ㅋㅋ",
          "original_form": "스키타러가면 특히 잘타요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035675.1.21",
          "form": "눈때문에 빛이 반사되서 그런걸까요?",
          "original_form": "눈때문에 빛이 반사되서 그런걸까요?",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035675.1.22",
          "form": "흰 눈에 햇빛이 반사되서 위아래로 두배로 받아요",
          "original_form": "흰 눈에 햇빛이 반사되서 위아래로 두배로 받아요",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035675.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035675.1.24",
          "form": "같은 원리로 여름 바닷가, 계곡에서도",
          "original_form": "같은 원리로 여름 바닷가, 계곡에서도",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035675.1.25",
          "form": "스키는 타러 안가야겠아요...ㅋㅋㅋㅋ",
          "original_form": "스키는 타러 안가야겠아요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035675.1.26",
          "form": "물에 반사되는 빛에 더 잘탑니다ㅋㅋ",
          "original_form": "물에 반사되는 빛에 더 잘탑니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035675.1.27",
