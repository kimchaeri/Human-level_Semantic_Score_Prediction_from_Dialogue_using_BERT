      "utterance": [
        {
          "id": "MDRW2100035597.1.1",
          "form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "original_form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:02"
        },
        {
          "id": "MDRW2100035597.1.2",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "2",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100035597.1.3",
          "form": "예전에 청거북이 키워봤어요",
          "original_form": "예전에 청거북이 키워봤어요",
          "speaker_id": "2",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100035597.1.4",
          "form": "오 저도 아주 어릴때 금붕어랑 거북이 키웠던 기억이나요 ㅋㅋ",
          "original_form": "오 저도 아주 어릴때 금붕어랑 거북이 키웠던 기억이나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100035597.1.5",
          "form": "아하 그러시군요",
          "original_form": "아하 그러시군요",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035597.1.6",
          "form": "그때 사료주고 가끔 돼지고기로 음식하면",
          "original_form": "그때 사료주고 가끔 돼지고기로 음식하면",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035597.1.7",
          "form": "자투리 고기 잘라서 생고기로 먹이줬어요",
          "original_form": "자투리 고기 잘라서 생고기로 먹이줬어요",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100035597.1.8",
          "form": "생각해보면 그건 제가 키운게 아니라",
          "original_form": "생각해보면 그건 제가 키운게 아니라",
          "speaker_id": "1",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100035597.1.9",
          "form": "저희 어머니께서",
          "original_form": "저희 어머니께서",
          "speaker_id": "1",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100035597.1.10",
          "form": "키우셨던거나 마찬가지였네요 ㅋㅋ",
          "original_form": "키우셨던거나 마찬가지였네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100035597.1.11",
          "form": "아 저도 그렇긴 그래요",
          "original_form": "아 저도 그렇긴 그래요",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100035597.1.12",
          "form": "저는 먹이나 가끔 뿌려줬죠",
          "original_form": "저는 먹이나 가끔 뿌려줬죠",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100035597.1.13",
          "form": "뭔가 저는 체험학습장 온것처럼 구경만 한거같아요",
          "original_form": "뭔가 저는 체험학습장 온것처럼 구경만 한거같아요",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035597.1.14",
          "form": "급 반성 ㅋㅋ",
          "original_form": "급 반성 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035597.1.15",
          "form": "ㅋㅋㅋㅋ그러셨군요",
          "original_form": "ㅋㅋㅋㅋ그러셨군요",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035597.1.16",
          "form": "음 키우고 싶으신건 있으세요?",
          "original_form": "음 키우고 싶으신건 있으세요?",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035597.1.17",
          "form": "강아지 키워보고싶어요",
          "original_form": "강아지 키워보고싶어요",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035597.1.18",
          "form": "오 특정 종?도 있으세요?",
          "original_form": "오 특정 종?도 있으세요?",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035597.1.19",
          "form": "나의 드림독?",
          "original_form": "나의 드림독?",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035597.1.20",
          "form": "같은 ㅋㅋ",
          "original_form": "같은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035597.1.21",
          "form": "포메요 ㅋㅋㅋ",
          "original_form": "포메요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035597.1.22",
          "form": "오 저는 검은개인데 흰색이나 갈색 눈썹이 있는 개를 키우고싶었어요",
          "original_form": "오 저는 검은개인데 흰색이나 갈색 눈썹이 있는 개를 키우고싶었어요",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035597.1.23",
          "form": "아 네이버에 사눈박이라고 치고 이미지 보심 아하 싶으실듯",
          "original_form": "아 네이버에 사눈박이라고 치고 이미지 보심 아하 싶으실듯",
          "speaker_id": "1",
          "time": "20210829 21:30"
        }
      ]
    }
