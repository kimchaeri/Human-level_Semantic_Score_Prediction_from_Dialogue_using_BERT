      "utterance": [
        {
          "id": "MDRW2100000960.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000960.1.2",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000960.1.3",
          "form": "아 뭐야 ㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나 요즘 부산 중독",
          "original_form": "아 뭐야 ㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나 요즘 부산 중독",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.4",
          "form": "ㅠㅠ다들 같은 마음이지",
          "original_form": "ㅠㅠ다들 같은 마음이지",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.5",
          "form": "최근에 간 청도도 진짜 좋았다",
          "original_form": "최근에 간 청도도 진짜 좋았다",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.6",
          "form": "부산 그래도 꽤 최근에 갔다오지않았음?",
          "original_form": "부산 그래도 꽤 최근에 갔다오지않았음?",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.7",
          "form": "근데 청도는 차가 없으니까 확실히.. 불편하더라",
          "original_form": "근데 청도는 차가 없으니까 확실히.. 불편하더라",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.8",
          "form": "맞아...빨리 누가 면허따주라",
          "original_form": "맞아...빨리 누가 면허따주라",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.9",
          "form": "맞아 근데 부산은 그 호텔가는 맛이라서 계속 거기서 쉬고싶어",
          "original_form": "맞아 근데 부산은 그 호텔가는 맛이라서 계속 거기서 쉬고싶어",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.10",
          "form": "면허 시간 돈 차 만 있으면 참 좋을텐데",
          "original_form": "면허 시간 돈 차 만 있으면 참 좋을텐데",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000960.1.11",
          "form": "맞아 숙소하나 잘 찾은거같아",
          "original_form": "맞아 숙소하나 잘 찾은거같아",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000960.1.12",
          "form": "그 숙소에서 먹은",
          "original_form": "그 숙소에서 먹은",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000960.1.13",
          "form": "홋헬이 안보내지네ㅠ",
          "original_form": "홋헬이 안보내지네ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000960.1.14",
          "form": "그.숙소에서 먹은 코스요리도 맛있었지",
          "original_form": "그.숙소에서 먹은 코스요리도 맛있었지",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000960.1.15",
          "form": "다음에 또 쉬러 가자",
          "original_form": "다음에 또 쉬러 가자",
          "speaker_id": "2",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000960.1.16",
          "form": "좋아 그때 그리고 회도 또 먹자",
          "original_form": "좋아 그때 그리고 회도 또 먹자",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000960.1.17",
          "form": "부산하면 회니까!",
          "original_form": "부산하면 회니까!",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000960.1.18",
          "form": "친구가 최근에 부산에서 캐논카메라로 사진찍어줬는데 좋았어",
          "original_form": "친구가 최근에 부산에서 캐논카메라로 사진찍어줬는데 좋았어",
          "speaker_id": "2",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000960.1.19",
          "form": "맞아 마늘 많이!",
          "original_form": "맞아 마늘 많이!",
          "speaker_id": "2",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000960.1.20",
          "form": "나는 필름카메라 들고갈께!",
          "original_form": "나는 필름카메라 들고갈께!",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000960.1.21",
          "form": "해외여행도 가고싶다",
          "original_form": "해외여행도 가고싶다",
          "speaker_id": "2",
          "time": "20210819 20:56"
        }
      ]
