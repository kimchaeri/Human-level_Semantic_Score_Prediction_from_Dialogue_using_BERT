      "utterance": [
        {
          "id": "MDRW2100043272.1.1",
          "form": "축구봐야지 뭔 영화야 ^^??",
          "original_form": "축구봐야지 뭔 영화야 ^^??",
          "speaker_id": "2",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.2",
          "form": "뭐보게요",
          "original_form": "뭐보게요",
          "speaker_id": "2",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.3",
          "form": "난 축구안봐",
          "original_form": "난 축구안봐",
          "speaker_id": "1",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.4",
          "form": "알아요..",
          "original_form": "알아요..",
          "speaker_id": "2",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.5",
          "form": "뭐보게요",
          "original_form": "뭐보게요",
          "speaker_id": "2",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.6",
          "form": "고민이야 기분이 좀 우울한게 뭘볼지",
          "original_form": "고민이야 기분이 좀 우울한게 뭘볼지",
          "speaker_id": "1",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.7",
          "form": "밥안먹어서그런가",
          "original_form": "밥안먹어서그런가",
          "speaker_id": "1",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.8",
          "form": "더우울해지는 디피를 추천드립니다",
          "original_form": "더우울해지는 디피를 추천드립니다",
          "speaker_id": "2",
          "time": "20210907 16:43"
        },
        {
          "id": "MDRW2100043272.1.9",
          "form": "밥먹으면서 봐",
          "original_form": "밥먹으면서 봐",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.10",
          "form": "그거드라마자나",
          "original_form": "그거드라마자나",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.11",
          "form": "6화밖에안햄",
          "original_form": "6화밖에안햄",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.12",
          "form": "하루만에다봤다",
          "original_form": "하루만에다봤다",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.13",
          "form": "우울한내용이야?",
          "original_form": "우울한내용이야?",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.14",
          "form": "갈수록?",
          "original_form": "갈수록?",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.15",
          "form": "시즌2예고해?",
          "original_form": "시즌2예고해?",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.16",
          "form": "2,3화는 좀 괜찮고",
          "original_form": "2,3화는 좀 괜찮고",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.17",
          "form": "시즌2나오냐고?",
          "original_form": "시즌2나오냐고?",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043272.1.18",
          "form": "나올거같기도하고...아직 주인공이 이병이라 ㅋㅋㅋㅋ",
          "original_form": "나올거같기도하고...아직 주인공이 이병이라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100043272.1.19",
          "form": "전역까지 500일남음...",
          "original_form": "전역까지 500일남음...",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100043272.1.20",
          "form": "아니면 영화 뭐보던데 사람들",
          "original_form": "아니면 영화 뭐보던데 사람들",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100043272.1.21",
          "form": "야야 그럼\r\n프로포즈봐",
          "original_form": "야야 그럼\r\n프로포즈봐",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100043272.1.22",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100043272.1.23",
          "form": "아냐 디피도보고싶은데",
          "original_form": "아냐 디피도보고싶은데",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100043272.1.24",
          "form": "디피도잼써 올해 봤던 드라마중에 제일 짜임새잇어",
          "original_form": "디피도잼써 올해 봤던 드라마중에 제일 짜임새잇어",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100043272.1.25",
          "form": "그정도야?",
          "original_form": "그정도야?",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100043272.1.26",
