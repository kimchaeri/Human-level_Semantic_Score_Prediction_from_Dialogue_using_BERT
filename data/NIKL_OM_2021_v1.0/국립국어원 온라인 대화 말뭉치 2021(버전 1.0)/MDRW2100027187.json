      "utterance": [
        {
          "id": "MDRW2100027187.1.1",
          "form": "name2은 하시는 운동 있으신가요?",
          "original_form": "&name2&은 하시는 운동 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027187.1.2",
          "form": "저 필라테스 다니고 있어요!",
          "original_form": "저 필라테스 다니고 있어요!",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027187.1.3",
          "form": "저도 필라테스 다녔었는데~",
          "original_form": "저도 필라테스 다녔었는데~",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027187.1.4",
          "form": "얼마나 다니셨어요?",
          "original_form": "얼마나 다니셨어요?",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027187.1.5",
          "form": "저 지금 3개월 끊어 놨던 게 이제 끝나가네요 ㅋㅋ",
          "original_form": "저 지금 3개월 끊어 놨던 게 이제 끝나가네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027187.1.6",
          "form": "오래 꾸준히 하셨네요!",
          "original_form": "오래 꾸준히 하셨네요!",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027187.1.7",
          "form": "튼튼해진 게 느껴지나요?",
          "original_form": "튼튼해진 게 느껴지나요?",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027187.1.8",
          "form": "저 너무 불성실하게 다녀서",
          "original_form": "저 너무 불성실하게 다녀서",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027187.1.9",
          "form": "지금 기간은 얼마 안 남았는데 횟슈가 너무 많이 남았어요",
          "original_form": "지금 기간은 얼마 안 남았는데 횟슈가 너무 많이 남았어요",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027187.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027187.1.11",
          "form": "헉 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027187.1.12",
          "form": "전 코어위주로 필라테스 수업 들었는데",
          "original_form": "전 코어위주로 필라테스 수업 들었는데",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027187.1.13",
          "form": "안 되던 동작이 한달만에 되는 거 보고...",
          "original_form": "안 되던 동작이 한달만에 되는 거 보고...",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027187.1.14",
          "form": "필라테스의 힘을 깨달았습니다",
          "original_form": "필라테스의 힘을 깨달았습니다",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027187.1.15",
          "form": "아 필라테스 수업을 부위별로 하나요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 필라테스 수업을 부위별로 하나요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027187.1.16",
          "form": "저는 시간표가 짜여져 있었는데",
          "original_form": "저는 시간표가 짜여져 있었는데",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027187.1.17",
          "form": "코어강화나",
          "original_form": "코어강화나",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027187.1.18",
          "form": "이런 식으로 진행됐던 거 같아요!",
          "original_form": "이런 식으로 진행됐던 거 같아요!",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027187.1.19",
          "form": "아 그렇구나 제가 다니는 센터는 도구별로 시간이 나뉘어져 있더라구요",
          "original_form": "아 그렇구나 제가 다니는 센터는 도구별로 시간이 나뉘어져 있더라구요",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027187.1.20",
          "form": "몇 시부터 몇 시까지는 리포머.. 이런 식으로요",
          "original_form": "몇 시부터 몇 시까지는 리포머.. 이런 식으로요",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027187.1.21",
          "form": "기구 필라테스 하시는군요!",
          "original_form": "기구 필라테스 하시는군요!",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027187.1.22",
          "form": "전 그건 안 해봤어요",
          "original_form": "전 그건 안 해봤어요",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027187.1.23",
          "form": "그럼 무슨 필라테스 하세요?",
          "original_form": "그럼 무슨 필라테스 하세요?",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027187.1.24",
          "form": "그냥 맨몸필라테스였는데",
          "original_form": "그냥 맨몸필라테스였는데",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027187.1.25",
          "form": "소도구 정도만 이용했어요",
          "original_form": "소도구 정도만 이용했어요",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027187.1.26",
          "form": "밴드나 무슨 요가링이나",
          "original_form": "밴드나 무슨 요가링이나",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027187.1.27",
          "form": "맨몸 필라테스 ㅋㅋㅋㅋㅋ매트 필라테스라고 보통 지칭하는 그것인가요",
          "original_form": "맨몸 필라테스 ㅋㅋㅋㅋㅋ매트 필라테스라고 보통 지칭하는 그것인가요",
