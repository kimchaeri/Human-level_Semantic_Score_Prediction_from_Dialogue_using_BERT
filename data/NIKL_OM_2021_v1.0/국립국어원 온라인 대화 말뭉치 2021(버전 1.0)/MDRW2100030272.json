      "utterance": [
        {
          "id": "MDRW2100030272.1.1",
          "form": "진짜 정치와 법임",
          "original_form": "진짜 정치와 법임",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030272.1.2",
          "form": "정치 그리고 법",
          "original_form": "정치 그리고 법",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030272.1.3",
          "form": "법은 가장 상위법인",
          "original_form": "법은 가장 상위법인",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030272.1.4",
          "form": "헌법 중심으로 많이 배워",
          "original_form": "헌법 중심으로 많이 배워",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030272.1.5",
          "form": "근데 진짜 재밌어",
          "original_form": "근데 진짜 재밌어",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.6",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.7",
          "form": "사회탐구 과목 중 제일 재밌음",
          "original_form": "사회탐구 과목 중 제일 재밌음",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.8",
          "form": "저거 다 외워야 해?",
          "original_form": "저거 다 외워야 해?",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.9",
          "form": "법 말이야",
          "original_form": "법 말이야",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.10",
          "form": "헌법을?",
          "original_form": "헌법을?",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.11",
          "form": "오우 아뇨",
          "original_form": "오우 아뇨",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.12",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.13",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.14",
          "form": "오 다행이다 다 외워냐 하는줄",
          "original_form": "오 다행이다 다 외워냐 하는줄",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.15",
          "form": "법의 내용을 중심으로 배우기보단",
          "original_form": "법의 내용을 중심으로 배우기보단",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.16",
          "form": "법의 정신?",
          "original_form": "법의 정신?",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.17",
          "form": "그런거랑",
          "original_form": "그런거랑",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030272.1.18",
          "form": "기본 원리",
          "original_form": "기본 원리",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030272.1.19",
          "form": "웅웅 법의 정신이라니",
          "original_form": "웅웅 법의 정신이라니",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030272.1.20",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030272.1.21",
          "form": "의미와 목적을 통해",
          "original_form": "의미와 목적을 통해",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030272.1.22",
          "form": "실현 방안을 중심으로 배워",
          "original_form": "실현 방안을 중심으로 배워",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030272.1.23",
          "form": "예를 들어 국민 주권주의가 있는데",
          "original_form": "예를 들어 국민 주권주의가 있는데",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030272.1.24",
          "form": "의미는 국가 의사를 결정하는 주권이 국민에게 있다라는 것이고",
          "original_form": "의미는 국가 의사를 결정하는 주권이 국민에게 있다라는 것이고",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030272.1.25",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030272.1.26",
          "form": "실현 방안은 참정권 보장이랑",
          "original_form": "실현 방안은 참정권 보장이랑",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030272.1.27",
          "form": "오 참정권 들어봣어",
          "original_form": "오 참정권 들어봣어",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030272.1.28",
          "form": "언론, 출판, 집회, 결사의 자유를 보장하는 것",
          "original_form": "언론, 출판, 집회, 결사의 자유를 보장하는 것",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030272.1.29",
          "form": "복수 정당제가 있어",
          "original_form": "복수 정당제가 있어",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030272.1.30",
          "form": "재밌지 않아?",
          "original_form": "재밌지 않아?",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.31",
          "form": "...ㅎ",
          "original_form": "...ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.32",
          "form": "....ㅎ 하나도 재미없어보임",
          "original_form": "....ㅎ 하나도 재미없어보임",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.33",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.34",
          "form": "우리 수능 50일 기념으로",
          "original_form": "우리 수능 50일 기념으로",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.35",
          "form": "25기 선배님들에게",
          "original_form": "25기 선배님들에게",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.36",
          "form": "텍스트 문자만 봐도 내잼없어",
          "original_form": "텍스트 문자만 봐도 내잼없어",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.37",
          "form": "선물 드린다",
          "original_form": "선물 드린다",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.38",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.39",
          "form": "와 뭐 주ㅜ?",
          "original_form": "와 뭐 주ㅜ?",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.40",
          "form": "학교 풍습같은 건데",
          "original_form": "학교 풍습같은 건데",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.41",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.42",
          "form": "각인 텀블러랑",
          "original_form": "각인 텀블러랑",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.43",
          "form": "초콜릿 안에 담아서",
          "original_form": "초콜릿 안에 담아서",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.44",
          "form": "편지랑 같이 드릴 예정",
          "original_form": "편지랑 같이 드릴 예정",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030272.1.45",
          "form": "이거 내 아이디어였어",
          "original_form": "이거 내 아이디어였어",
          "speaker_id": "2",
          "time": "20210903 23:29"
        }
      ]
    }
  ]
