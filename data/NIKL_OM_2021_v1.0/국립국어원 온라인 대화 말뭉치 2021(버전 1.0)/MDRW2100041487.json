        {
          "id": "MDRW2100041487.1.1",
          "form": "나 폰케이스 살거야",
          "original_form": "나 폰케이스 살거야",
          "speaker_id": "2",
          "time": "20210906 01:41"
        },
        {
          "id": "MDRW2100041487.1.2",
          "form": "또?",
          "original_form": "또?",
          "speaker_id": "1",
          "time": "20210906 01:41"
        },
        {
          "id": "MDRW2100041487.1.3",
          "form": "또라니 케이스는많을수록좋지",
          "original_form": "또라니 케이스는많을수록좋지",
          "speaker_id": "2",
          "time": "20210906 01:41"
        },
        {
          "id": "MDRW2100041487.1.4",
          "form": "어떤 케이스 보고 있어?",
          "original_form": "어떤 케이스 보고 있어?",
          "speaker_id": "1",
          "time": "20210906 01:41"
        },
        {
          "id": "MDRW2100041487.1.5",
          "form": "귀여운거",
          "original_form": "귀여운거",
          "speaker_id": "2",
          "time": "20210906 01:41"
        },
        {
          "id": "MDRW2100041487.1.6",
          "form": "귀여운거좋지",
          "original_form": "귀여운거좋지",
          "speaker_id": "1",
          "time": "20210906 01:41"
        },
        {
          "id": "MDRW2100041487.1.7",
          "form": "곰돌이모양살거야",
          "original_form": "곰돌이모양살거야",
          "speaker_id": "2",
          "time": "20210906 01:42"
        },
        {
          "id": "MDRW2100041487.1.8",
          "form": "우와 진짜 귀여울거같은데",
          "original_form": "우와 진짜 귀여울거같은데",
          "speaker_id": "1",
          "time": "20210906 01:42"
        },
        {
          "id": "MDRW2100041487.1.9",
          "form": "너도같이 살래?",
          "original_form": "너도같이 살래?",
          "speaker_id": "2",
          "time": "20210906 01:42"
        },
        {
          "id": "MDRW2100041487.1.10",
          "form": "내 기종 있는지 봐줘",
          "original_form": "내 기종 있는지 봐줘",
          "speaker_id": "1",
          "time": "20210906 01:42"
        },
        {
          "id": "MDRW2100041487.1.11",
          "form": "너가 갤럭시노트20이지?",
          "original_form": "너가 갤럭시노트20이지?",
          "speaker_id": "2",
          "time": "20210906 01:42"
        },
        {
          "id": "MDRW2100041487.1.12",
          "form": "맞아 한번확인해줘",
          "original_form": "맞아 한번확인해줘",
          "speaker_id": "1",
          "time": "20210906 01:42"
        },
        {
          "id": "MDRW2100041487.1.13",
          "form": "사진보냈어 봐봐",
          "original_form": "사진보냈어 봐봐",
          "speaker_id": "2",
          "time": "20210906 01:43"
