      "utterance": [
        {
          "id": "MDRW2100045784.1.1",
          "form": "저 시리얼요!",
          "original_form": "저 시리얼요!",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045784.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045784.1.3",
          "form": "저 막 씻었어요",
          "original_form": "저 막 씻었어요",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045784.1.4",
          "form": "저도 목을거애욬ㅋㅋㅋ",
          "original_form": "저도 목을거애욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045784.1.5",
          "form": "어떤 시리얼인가요",
          "original_form": "어떤 시리얼인가요",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045784.1.6",
          "form": "포스트 그래놀라요!",
          "original_form": "포스트 그래놀라요!",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045784.1.7",
          "form": "마쉿는거 드시네요",
          "original_form": "마쉿는거 드시네요",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045784.1.8",
          "form": "저는 오트밀먹을거에요",
          "original_form": "저는 오트밀먹을거에요",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045784.1.9",
          "form": "오오 그것도 맛있죠!",
          "original_form": "오오 그것도 맛있죠!",
          "speaker_id": "2",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100045784.1.10",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045784.1.11",
          "form": "네 아침 대용으로 자주 먹어요",
          "original_form": "네 아침 대용으로 자주 먹어요",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045784.1.12",
          "form": "오늘은 왜 그래놀라였어요? 좀 더 끌렸나용?",
          "original_form": "오늘은 왜 그래놀라였어요? 좀 더 끌렸나용?",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100045784.1.13",
          "form": "어제 새로 배송이 와서요!ㅋㅋㅋ",
          "original_form": "어제 새로 배송이 와서요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045784.1.14",
          "form": "아 맞네요 왔으면 먹어봐야죠 ㅋㅋㅋㅋ",
          "original_form": "아 맞네요 왔으면 먹어봐야죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045784.1.15",
          "form": "저는 오트밀 먹고 있어요 아침에 그레놀라만 먹고 괜찮아요?",
          "original_form": "저는 오트밀 먹고 있어요 아침에 그레놀라만 먹고 괜찮아요?",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045784.1.16",
          "form": "그리고 인터넷에서 자주 할인을 해서 좋아요",
          "original_form": "그리고 인터넷에서 자주 할인을 해서 좋아요",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045784.1.17",
          "form": "그래놀라랑 우유랑 먹고, 두유 몇 개 챙겨가서 배고프면 마셔줘요",
          "original_form": "그래놀라랑 우유랑 먹고, 두유 몇 개 챙겨가서 배고프면 마셔줘요",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045784.1.18",
          "form": "두유가 히든카드 군요",
          "original_form": "두유가 히든카드 군요",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045784.1.19",
          "form": "무슨 두유에요?",
          "original_form": "무슨 두유에요?",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045784.1.20",
          "form": "매일우유 두유요!",
          "original_form": "매일우유 두유요!",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045784.1.21",
          "form": "그거 초코맛도 있는데 얼려먹으면 존맛이에요",
          "original_form": "그거 초코맛도 있는데 얼려먹으면 존맛이에요",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045784.1.22",
          "form": "저두 고단백인가 있어욬ㅋㅋㅋ",
          "original_form": "저두 고단백인가 있어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045784.1.23",
          "form": "어떻게 얼려먹어요?",
          "original_form": "어떻게 얼려먹어요?",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045784.1.24",
          "form": "팩에 들었잖아요",
          "original_form": "팩에 들었잖아요",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045784.1.25",
          "form": "그거 팩을 펴서요",
          "original_form": "그거 팩을 펴서요",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045784.1.26",
          "form": "더위사냥 모양처럼 펼쳐서 얼리면 돼요!",
          "original_form": "더위사냥 모양처럼 펼쳐서 얼리면 돼요!",
          "speaker_id": "2",
          "time": "20210908 07:53"
        }
      ]
    }
  ]
