      "utterance": [
        {
          "id": "MDRW2100016014.1.1",
          "form": "염색해보신 적 있으세요",
          "original_form": "염색해보신 적 있으세요",
          "speaker_id": "2",
          "time": "20210826 07:40"
        },
        {
          "id": "MDRW2100016014.1.2",
          "form": "네 태어나서 딱 한번이요 ㅋㅋㅋ",
          "original_form": "네 태어나서 딱 한번이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 07:41"
        },
        {
          "id": "MDRW2100016014.1.3",
          "form": "어떤색으로 해보셨나요",
          "original_form": "어떤색으로 해보셨나요",
          "speaker_id": "2",
          "time": "20210826 07:41"
        },
        {
          "id": "MDRW2100016014.1.4",
          "form": "탈색없이 젤밝은브라운으로요~",
          "original_form": "탈색없이 젤밝은브라운으로요~",
          "speaker_id": "1",
          "time": "20210826 07:41"
        },
        {
          "id": "MDRW2100016014.1.5",
          "form": "브라운 만족하셨나요",
          "original_form": "브라운 만족하셨나요",
          "speaker_id": "2",
          "time": "20210826 07:41"
        },
        {
          "id": "MDRW2100016014.1.6",
          "form": "음 그닥 별로였어요,, 제 퍼스널컬러랑 안맞다는걸깨달은뒤부턴 염색안했어요",
          "original_form": "음 그닥 별로였어요,, 제 퍼스널컬러랑 안맞다는걸깨달은뒤부턴 염색안했어요",
          "speaker_id": "1",
          "time": "20210826 07:42"
        },
        {
          "id": "MDRW2100016014.1.7",
          "form": "으음.. 미용실가서 하신거죠?",
          "original_form": "으음.. 미용실가서 하신거죠?",
          "speaker_id": "2",
          "time": "20210826 07:42"
        },
        {
          "id": "MDRW2100016014.1.8",
          "form": "아무래도 셀프는 어려우니까",
          "original_form": "아무래도 셀프는 어려우니까",
          "speaker_id": "2",
          "time": "20210826 07:42"
        },
        {
          "id": "MDRW2100016014.1.9",
          "form": "아닌가 셀프로 하셨으려나",
          "original_form": "아닌가 셀프로 하셨으려나",
          "speaker_id": "2",
          "time": "20210826 07:42"
        },
        {
          "id": "MDRW2100016014.1.10",
          "form": "미용실가서했어요 ㅎㅎ",
          "original_form": "미용실가서했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 07:42"
        },
        {
          "id": "MDRW2100016014.1.11",
          "form": "머리가 길어서 혼자는 힘들겠더라구요",
          "original_form": "머리가 길어서 혼자는 힘들겠더라구요",
          "speaker_id": "1",
          "time": "20210826 07:43"
        },
        {
          "id": "MDRW2100016014.1.12",
          "form": "아하.. 미용실은 주로 커트하러가시나요",
          "original_form": "아하.. 미용실은 주로 커트하러가시나요",
          "speaker_id": "2",
          "time": "20210826 07:43"
        },
        {
          "id": "MDRW2100016014.1.13",
          "form": "네! 커트나 관리받으러가요 ㅎㅎ 근데 이시국이후론 미용실도 자주안갔어요,,",
          "original_form": "네! 커트나 관리받으러가요 ㅎㅎ 근데 이시국이후론 미용실도 자주안갔어요,,",
          "speaker_id": "1",
          "time": "20210826 07:44"
        },
        {
          "id": "MDRW2100016014.1.14",
          "form": "음 아무래도 코로나도 있고 미용실은 소통을 하니까",
          "original_form": "음 아무래도 코로나도 있고 미용실은 소통을 하니까",
          "speaker_id": "2",
          "time": "20210826 07:44"
        },
        {
          "id": "MDRW2100016014.1.15",
          "form": "불안하시면 안가는게 맞는거 같아요ㅎㅎ",
          "original_form": "불안하시면 안가는게 맞는거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 07:44"
        },
        {
          "id": "MDRW2100016014.1.16",
          "form": "관리는 어떤거 주로 받으셨어요",
          "original_form": "관리는 어떤거 주로 받으셨어요",
          "speaker_id": "2",
          "time": "20210826 07:45"
        },
        {
          "id": "MDRW2100016014.1.17",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 07:45"
        },
        {
          "id": "MDRW2100016014.1.18",
          "form": "저는 헤어보단 두피쪽케어 받았어요",
          "original_form": "저는 헤어보단 두피쪽케어 받았어요",
          "speaker_id": "1",
          "time": "20210826 07:45"
        },
        {
          "id": "MDRW2100016014.1.19",
          "form": "스케일링 받으셨구나",
          "original_form": "스케일링 받으셨구나",
          "speaker_id": "2",
          "time": "20210826 07:45"
        }
