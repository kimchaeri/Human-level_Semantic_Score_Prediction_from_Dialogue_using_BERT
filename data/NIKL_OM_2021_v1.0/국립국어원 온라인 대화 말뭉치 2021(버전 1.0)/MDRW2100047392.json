      "utterance": [
        {
          "id": "MDRW2100047392.1.1",
          "form": "학창시절 기억에 남는 에피소드 있너요",
          "original_form": "학창시절 기억에 남는 에피소드 있너요",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100047392.1.2",
          "form": "야자 시간에 땡땡이친 거요",
          "original_form": "야자 시간에 땡땡이친 거요",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047392.1.3",
          "form": "나갔다가 야자 끝날 땐 또 살며시 들어왔죠ㅎ",
          "original_form": "나갔다가 야자 끝날 땐 또 살며시 들어왔죠ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100047392.1.4",
          "form": "name1님은 야자하셨나요?",
          "original_form": "&name1&님은 야자하셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100047392.1.5",
          "form": "네 야자 했어요^^",
          "original_form": "네 야자 했어요^^",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100047392.1.6",
          "form": "지역이 어디였나요?",
          "original_form": "지역이 어디였나요?",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100047392.1.7",
          "form": "전북이요",
          "original_form": "전북이요",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100047392.1.8",
          "form": "아 지역마다 야자 유무가 다 다르더라고요",
          "original_form": "아 지역마다 야자 유무가 다 다르더라고요",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100047392.1.9",
          "form": "저 대학 가니 친구들 태반이 야자를 안해봐서 억울했어요ㅎ",
          "original_form": "저 대학 가니 친구들 태반이 야자를 안해봐서 억울했어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100047392.1.10",
          "form": "전북도 야자했군요",
          "original_form": "전북도 야자했군요",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100047392.1.11",
          "form": "지역 어디셨나요?",
          "original_form": "지역 어디셨나요?",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100047392.1.12",
          "form": "저는 수원이요",
          "original_form": "저는 수원이요",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100047392.1.13",
          "form": "요새는 야자가 많이 사라졌대요",
          "original_form": "요새는 야자가 많이 사라졌대요",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100047392.1.14",
          "form": "그렇죠 더 자율적으로 변한것 같죠",
          "original_form": "그렇죠 더 자율적으로 변한것 같죠",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100047392.1.15",
          "form": "사실 야자해도 공부하는 애들은 몇 안됐으니ㅎ",
          "original_form": "사실 야자해도 공부하는 애들은 몇 안됐으니ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100047392.1.16",
          "form": "그게 맞는 것 같아요",
          "original_form": "그게 맞는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100047392.1.17",
          "form": "그렇죠 야자는 다른 추억들이 더 많은 겋 같아요 놀고..먹고..",
          "original_form": "그렇죠 야자는 다른 추억들이 더 많은 겋 같아요 놀고..먹고..",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100047392.1.18",
          "form": "ㅎㅎ 쉴때 책상붙여놓고 티타임하고",
          "original_form": "ㅎㅎ 쉴때 책상붙여놓고 티타임하고",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100047392.1.19",
          "form": "학부모들이 피자치킨 같은 간식 넣어주고",
          "original_form": "학부모들이 피자치킨 같은 간식 넣어주고",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100047392.1.20",
          "form": "맞아요 야자때 먹은 야식 기억나요^^",
          "original_form": "맞아요 야자때 먹은 야식 기억나요^^",
