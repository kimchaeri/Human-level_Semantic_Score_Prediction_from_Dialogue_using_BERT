      "utterance": [
        {
          "id": "MDRW2100040625.1.1",
          "form": "선선해지는거넘좋은데",
          "original_form": "선선해지는거넘좋은데",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040625.1.2",
          "form": "일기예보너무엉망이라서",
          "original_form": "일기예보너무엉망이라서",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040625.1.3",
          "form": "괴롭다ㅋㅋㅋ",
          "original_form": "괴롭다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040625.1.4",
          "form": "아 그러게 일기예보 어이없어",
          "original_form": "아 그러게 일기예보 어이없어",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040625.1.5",
          "form": "응 밖에다니니까 진짜그게너무중요한데",
          "original_form": "응 밖에다니니까 진짜그게너무중요한데",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040625.1.6",
          "form": "이렇게나많이틀리닞ㅋㅋㄱㅋ..",
          "original_form": "이렇게나많이틀리닞ㅋㅋㄱㅋ..",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040625.1.7",
          "form": "아 그러네 그치",
          "original_form": "아 그러네 그치",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100040625.1.8",
          "form": "스트레스겠다ㅜㅠ",
          "original_form": "스트레스겠다ㅜㅠ",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040625.1.9",
          "form": "응응 비도오다말다 엉망이다아주ㅋㅋㅋㅋ",
          "original_form": "응응 비도오다말다 엉망이다아주ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040625.1.10",
          "form": "아랬다저랬다 난라",
          "original_form": "아랬다저랬다 난라",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040625.1.11",
          "form": "우산은 특하",
          "original_form": "우산은 특하",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040625.1.12",
          "form": "그래그러니내가우산을잃어버렸지",
          "original_form": "그래그러니내가우산을잃어버렸지",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040625.1.13",
          "form": "ㄱㅋㅈㄱㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㄱㅋㅈㄱㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040625.1.14",
          "form": "그러네 맬 들고다니다가 ㅋㅋ",
          "original_form": "그러네 맬 들고다니다가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040625.1.15",
          "form": "ㅋㅋㅋㅋㄱ망할놈의기억력",
          "original_form": "ㅋㅋㅋㅋㄱ망할놈의기억력",
          "speaker_id": "2",
          "time": "20210905 12:30"
        },
        {
          "id": "MDRW2100040625.1.16",
          "form": "오늘 해나더니 다시 흐려지네",
          "original_form": "오늘 해나더니 다시 흐려지네",
          "speaker_id": "1",
          "time": "20210905 12:53"
        },
        {
          "id": "MDRW2100040625.1.17",
          "form": "지금날이흐린가요 당근하러나가야하는데",
          "original_form": "지금날이흐린가요 당근하러나가야하는데",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100040625.1.18",
          "form": "와 당근 뭐 팔러가?",
          "original_form": "와 당근 뭐 팔러가?",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040625.1.19",
          "form": "좀 쌀쌀해",
          "original_form": "좀 쌀쌀해",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040625.1.20",
          "form": "앨범나눔ㅋㅈㅋ\n비오네ㅜㅜ",
          "original_form": "앨범나눔ㅋㅈㅋ\n비오네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100040625.1.21",
          "form": "계속이어서",
          "original_form": "계속이어서",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100040625.1.22",
