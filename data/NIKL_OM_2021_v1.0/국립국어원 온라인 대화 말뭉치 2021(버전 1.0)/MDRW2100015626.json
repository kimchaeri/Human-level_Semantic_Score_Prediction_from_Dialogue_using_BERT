      "utterance": [
        {
          "id": "MDRW2100015626.1.1",
          "form": "콩국수 좋아해요!",
          "original_form": "콩국수 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015626.1.2",
          "form": "저 근데 이번 여름에 콩국수 못먹었어요ㅠㅠㅠ",
          "original_form": "저 근데 이번 여름에 콩국수 못먹었어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015626.1.3",
          "form": "아ㅜㅜㅜ 저도요ㅜㅜㅜ",
          "original_form": "아ㅜㅜㅜ 저도요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015626.1.4",
          "form": "꾸덕한 콩국수 먹고싶어요",
          "original_form": "꾸덕한 콩국수 먹고싶어요",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015626.1.5",
          "form": "너무 좋죠..",
          "original_form": "너무 좋죠..",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015626.1.6",
          "form": "전 위에 오이 꼭 올려야해요",
          "original_form": "전 위에 오이 꼭 올려야해요",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015626.1.7",
          "form": "아ㅜㅜㅜㅜ너무좋아요 그리고 김치필수에요ㅋㅋ",
          "original_form": "아ㅜㅜㅜㅜ너무좋아요 그리고 김치필수에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015626.1.8",
          "form": "하 벌써 맛있다",
          "original_form": "하 벌써 맛있다",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015626.1.9",
          "form": "콩물도 진짜 걸죽하게 찐으로..",
          "original_form": "콩물도 진짜 걸죽하게 찐으로..",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015626.1.10",
          "form": "그러니까요ㅠㅠㅠ  오이하니까 비빔면도 먹고싶어서 오늘은 배홍동 비빔면 사왔어요ㅋㅋ",
          "original_form": "그러니까요ㅠㅠㅠ  오이하니까 비빔면도 먹고싶어서 오늘은 배홍동 비빔면 사왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100015626.1.11",
          "form": "오 그거 맛있어요?",
          "original_form": "오 그거 맛있어요?",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100015626.1.12",
          "form": "저 안먹어봤어요",
          "original_form": "저 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100015626.1.13",
          "form": "맛있어요!!! 원래 팔도비빔면좋아했는데 팔도가 맛이 밍밍해졌다고 생각할쯤 나온 배홍동이",
          "original_form": "맛있어요!!! 원래 팔도비빔면좋아했는데 팔도가 맛이 밍밍해졌다고 생각할쯤 나온 배홍동이",
          "speaker_id": "2",
          "time": "20210827 09:16"
        },
        {
          "id": "MDRW2100015626.1.14",
          "form": "예전팔도맛이에요ㅋㅋ 자극적이고 맛있어요!! 첫시작은 유느님 광고때문에 먹게됐어요ㅋ기",
          "original_form": "예전팔도맛이에요ㅋㅋ 자극적이고 맛있어요!! 첫시작은 유느님 광고때문에 먹게됐어요ㅋ기",
          "speaker_id": "2",
          "time": "20210827 09:16"
        },
        {
          "id": "MDRW2100015626.1.15",
          "form": "ㅋㅋㅋㅋㅋ아 저도 광고 봤어요",
          "original_form": "ㅋㅋㅋㅋㅋ아 저도 광고 봤어요",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100015626.1.16",
          "form": "너무 강렬한 광고..",
          "original_form": "너무 강렬한 광고..",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100015626.1.17",
          "form": "배흥동 다음에 먹어봐야겠어요",
          "original_form": "배흥동 다음에 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100015626.1.18",
          "form": "ㅋㅋㅋㅋ진짜광고웃기더라구요\r\n어제는 크림까르보 불닭볶음면 먹었는데 이것도맛있어요!",
          "original_form": "ㅋㅋㅋㅋ진짜광고웃기더라구요\r\n어제는 크림까르보 불닭볶음면 먹었는데 이것도맛있어요!",
          "speaker_id": "2",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100015626.1.19",
          "form": "오 저 이건 먹어봤어요!",
          "original_form": "오 저 이건 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100015626.1.20",
          "form": "불닭은 진짜 다 맛있는거 같아요ㅠㅠ실패가 없는",
          "original_form": "불닭은 진짜 다 맛있는거 같아요ㅠㅠ실패가 없는",
          "speaker_id": "1",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100015626.1.21",
          "form": "맞아요ㅜㅜ 저는 불닭 매워하는데 까르보 들어가면 먹을만한 매움이라 더 맛있어요ㅋㅋ",
          "original_form": "맞아요ㅜㅜ 저는 불닭 매워하는데 까르보 들어가면 먹을만한 매움이라 더 맛있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100015626.1.22",
          "form": "그쵸ㅋㅋㅋ아무래도 덜 맵더라구용",
          "original_form": "그쵸ㅋㅋㅋ아무래도 덜 맵더라구용",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100015626.1.23",
          "form": "하 얘기하니까 불닭 먹고싶네요ㅎㅎ",
          "original_form": "하 얘기하니까 불닭 먹고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:14"
        }
      ]
    }
