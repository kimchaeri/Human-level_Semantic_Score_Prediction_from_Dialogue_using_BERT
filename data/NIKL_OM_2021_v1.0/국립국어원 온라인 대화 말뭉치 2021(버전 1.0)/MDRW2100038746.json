      "utterance": [
        {
          "id": "MDRW2100038746.1.1",
          "form": "씨제이는 주문하셨어요?",
          "original_form": "씨제이는 주문하셨어요?",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038746.1.2",
          "form": "아뇨. 그건 참으려구요",
          "original_form": "아뇨. 그건 참으려구요",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038746.1.3",
          "form": "아 냉동실이 ㅎㅎ",
          "original_form": "아 냉동실이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038746.1.4",
          "form": "저는 꼬마돈까스 다먹어서 주문해야되는데",
          "original_form": "저는 꼬마돈까스 다먹어서 주문해야되는데",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038746.1.5",
          "form": "그거 하나땜에 다른걸 더 집어넣는거 같아서 ㅎㅎ",
          "original_form": "그거 하나땜에 다른걸 더 집어넣는거 같아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038746.1.6",
          "form": "ㅎㅎ 지금 넘 뒤죽박죽이라 정리좀해야하는데",
          "original_form": "ㅎㅎ 지금 넘 뒤죽박죽이라 정리좀해야하는데",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038746.1.7",
          "form": "어떤거 남았는지 잘 모르겠어요",
          "original_form": "어떤거 남았는지 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038746.1.8",
          "form": "이게 맞는건가 고민되요 ㅎㅎ",
          "original_form": "이게 맞는건가 고민되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038746.1.9",
          "form": "안먹는건 진짜 버려야되오 ㅎㅎ",
          "original_form": "안먹는건 진짜 버려야되오 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038746.1.10",
          "form": "그러게요.  버려야하는데 자꾸 속으로 숨어요^^",
          "original_form": "그러게요.  버려야하는데 자꾸 속으로 숨어요^^",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038746.1.11",
          "form": "위에꺼 먹고 다시 위로 채우고~~",
          "original_form": "위에꺼 먹고 다시 위로 채우고~~",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038746.1.12",
          "form": "속으로 ㅎㅎ",
          "original_form": "속으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038746.1.13",
          "form": "그게 알차고 좋죠 ㅎ",
          "original_form": "그게 알차고 좋죠 ㅎ",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038746.1.14",
          "form": "ㅎ 택배오면 행복하긴해요. 처리곤란이라도요",
          "original_form": "ㅎ 택배오면 행복하긴해요. 처리곤란이라도요",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038746.1.15",
          "form": "저두요 ㅎㅎ",
          "original_form": "저두요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038746.1.16",
          "form": "핫도그는 엄마집으로 보냈는데",
          "original_form": "핫도그는 엄마집으로 보냈는데",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038746.1.17",
          "form": "좋아하시더라구요 ㅋ",
          "original_form": "좋아하시더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038746.1.18",
          "form": "ㅎ 효녀시네요^^",
          "original_form": "ㅎ 효녀시네요^^",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038746.1.19",
          "form": "그냥 울집엔 안먹을것같아서 보낸건데",
          "original_form": "그냥 울집엔 안먹을것같아서 보낸건데",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038746.1.20",
          "form": "좋아할지몰랐어요 ㅎ",
          "original_form": "좋아할지몰랐어요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038746.1.21",
          "form": "안먹으면 안사면되는데",
          "original_form": "안먹으면 안사면되는데",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038746.1.22",
          "form": "11개에 천원인걸 보고는 안사면 손해인듯한 기분에 ㅋ",
          "original_form": "11개에 천원인걸 보고는 안사면 손해인듯한 기분에 ㅋ",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038746.1.23",
          "form": "담에 또 핫딜나옴 보내드림 좋겠네요",
          "original_form": "담에 또 핫딜나옴 보내드림 좋겠네요",
