      "utterance": [
        {
          "id": "MDRW2100023241.1.1",
          "form": "싸움하면은 친구 아니야",
          "original_form": "싸움하면은 친구 아니야",
          "speaker_id": "1",
          "time": "20210829 23:14"
        },
        {
          "id": "MDRW2100023241.1.2",
          "form": "사이좋게 지내요 ㅋㅋㅋㅋㅋ",
          "original_form": "사이좋게 지내요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:14"
        },
        {
          "id": "MDRW2100023241.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023241.1.4",
          "form": "엄마 말투네요",
          "original_form": "엄마 말투네요",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023241.1.5",
          "form": "name2 종교 있으세요? ㅋㅋㅋ",
          "original_form": "&name2& 종교 있으세요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023241.1.6",
          "form": "저도 엄마니 엄마 말투 ㅋㅋ",
          "original_form": "저도 엄마니 엄마 말투 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023241.1.7",
          "form": "전 무교입니다 ㅋㅋ",
          "original_form": "전 무교입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023241.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023241.1.9",
          "form": "종교를 한번도 가져본적 없으세요?",
          "original_form": "종교를 한번도 가져본적 없으세요?",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023241.1.10",
          "form": "어머님이 기독교이실거 같았는데ㅋㅋ",
          "original_form": "어머님이 기독교이실거 같았는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023241.1.11",
          "form": "엄마는 기독교아니고",
          "original_form": "엄마는 기독교아니고",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023241.1.12",
          "form": "불교",
          "original_form": "불교",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023241.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023241.1.14",
          "form": "아....어머님은 불교셨군요",
          "original_form": "아....어머님은 불교셨군요",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100023241.1.15",
          "form": "저는 기독교입니다 ㅋㅋ",
          "original_form": "저는 기독교입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100023241.1.16",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023241.1.17",
          "form": "그럼 오늘 교회 다녀오셨나요?",
          "original_form": "그럼 오늘 교회 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023241.1.18",
          "form": "아니요ㅋㅋ\r\n비대면 예배로 드리고 있어요",
          "original_form": "아니요ㅋㅋ\r\n비대면 예배로 드리고 있어요",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023241.1.19",
          "form": "천국도 방역수칙 지켜야 가요ㅋㅋㅋ",
          "original_form": "천국도 방역수칙 지켜야 가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023241.1.20",
          "form": "아 요즘 못가는군요 교회도",
          "original_form": "아 요즘 못가는군요 교회도",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023241.1.21",
          "form": "가시는 분들은 가시는데\r\n저는 19년 이후로는\r\n가본적ㅇㅣ 없네요ㅠㄱ",
          "original_form": "가시는 분들은 가시는데\r\n저는 19년 이후로는\r\n가본적ㅇㅣ 없네요ㅠㄱ",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023241.1.22",
          "form": "아이들있어서 더 조심스러우시죠?",
          "original_form": "아이들있어서 더 조심스러우시죠?",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023241.1.23",
          "form": "아무래도 더 그렇죠ㅠ",
          "original_form": "아무래도 더 그렇죠ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023241.1.24",
          "form": "둘째 임신중에 코로나 시작되고",
          "original_form": "둘째 임신중에 코로나 시작되고",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023241.1.25",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023241.1.26",
          "form": "으휴",
          "original_form": "으휴",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023241.1.27",
          "form": "코로나 언제 끝나나",
          "original_form": "코로나 언제 끝나나",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023241.1.28",
          "form": "이젠 코로나위드 라고....ㅋㅋ\r\n함께가는게 맞는거 같아요ㅠ\r\n끝은 없을 거 같고ㅜㅜ",
          "original_form": "이젠 코로나위드 라고....ㅋㅋ\r\n함께가는게 맞는거 같아요ㅠ\r\n끝은 없을 거 같고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100023241.1.29",
          "form": "주일날 예배마치고\r\n비슷한 또래 부부들끼리\r\n모임도 갖고 했는데 \r\n이젠 그 시간들도 그립네요",
          "original_form": "주일날 예배마치고\r\n비슷한 또래 부부들끼리\r\n모임도 갖고 했는데 \r\n이젠 그 시간들도 그립네요",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100023241.1.30",
          "form": "이제 코로나 끝날때까진.... 못하겠죠?",
          "original_form": "이제 코로나 끝날때까진.... 못하겠죠?",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023241.1.31",
          "form": "끝은 없을듯 하고 \r\n다들 접종도하고 단계도 좀\r\n낮아지면 할 수 있지 않을까요?",
          "original_form": "끝은 없을듯 하고 \r\n다들 접종도하고 단계도 좀\r\n낮아지면 할 수 있지 않을까요?",
          "speaker_id": "1",
          "time": "20210830 00:02"
        }
      ]
