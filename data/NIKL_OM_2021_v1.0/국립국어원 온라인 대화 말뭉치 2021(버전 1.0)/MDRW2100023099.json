      "utterance": [
        {
          "id": "MDRW2100023099.1.1",
          "form": "저거 못하니까",
          "original_form": "저거 못하니까",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023099.1.2",
          "form": "제발 딱딱하고 거리감있고 웃음기 쫙뺀",
          "original_form": "제발 딱딱하고 거리감있고 웃음기 쫙뺀",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.3",
          "form": "그런 기관으로",
          "original_form": "그런 기관으로",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.4",
          "form": "돌아와달라....",
          "original_form": "돌아와달라....",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.5",
          "form": "ㅇㅇ 그냥 딱딱한 홍보영상 만들어...",
          "original_form": "ㅇㅇ 그냥 딱딱한 홍보영상 만들어...",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.6",
          "form": "공공기관은 칭찬받을 생각이 아니라",
          "original_form": "공공기관은 칭찬받을 생각이 아니라",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.7",
          "form": "욕먹지 않을 생각으로 해야하는거 아닌가...",
          "original_form": "욕먹지 않을 생각으로 해야하는거 아닌가...",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.8",
          "form": "ㅇㅇ....공공기관이 sns에서 인기있어서",
          "original_form": "ㅇㅇ....공공기관이 sns에서 인기있어서",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.9",
          "form": "뭐할건데....",
          "original_form": "뭐할건데....",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.10",
          "form": "광고받을것도 아니잖아...",
          "original_form": "광고받을것도 아니잖아...",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023099.1.11",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023099.1.12",
          "form": "인기 있어서 사람 몰리면",
          "original_form": "인기 있어서 사람 몰리면",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.13",
          "form": "진상짓 일정률의 법칙에 따라",
          "original_form": "진상짓 일정률의 법칙에 따라",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.14",
          "form": "ㅇㅏ그래가지고",
          "original_form": "ㅇㅏ그래가지고",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.15",
          "form": "직원들만 피곤래짐",
          "original_form": "직원들만 피곤래짐",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.16",
          "form": "내가 트위터에서",
          "original_form": "내가 트위터에서",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.17",
          "form": "이낙연 제페토같은걸",
          "original_form": "이낙연 제페토같은걸",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.18",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.19",
          "form": "봐야하잖아...",
          "original_form": "봐야하잖아...",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.20",
          "form": "내 멘탈을 좀",
          "original_form": "내 멘탈을 좀",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.21",
          "form": "진짜 정치인 모에화",
          "original_form": "진짜 정치인 모에화",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.22",
          "form": "고려해달라고...",
          "original_form": "고려해달라고...",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.23",
          "form": "젤 이해 안가는 컨텐츠야...",
          "original_form": "젤 이해 안가는 컨텐츠야...",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.24",
          "form": "ㄹㅇ....",
          "original_form": "ㄹㅇ....",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.25",
          "form": "아니 정치인을 왜",
          "original_form": "아니 정치인을 왜",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023099.1.26",
          "form": "빨아먹지...?",
          "original_form": "빨아먹지...?",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023099.1.27",
          "form": "한창 정치인들 유튜브 했었는데",
          "original_form": "한창 정치인들 유튜브 했었는데",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023099.1.28",
          "form": "그때부터 이해 안갔어",
          "original_form": "그때부터 이해 안갔어",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023099.1.29",
          "form": "ㅇㅇ....정치인이면",
          "original_form": "ㅇㅇ....정치인이면",
          "speaker_id": "2",
          "time": "20210829 21:45"
        }
      ]
    }
