      "utterance": [
        {
          "id": "MDRW2100005000.1.1",
          "form": "아 맞아 저번에 물어봤던 우산",
          "original_form": "아 맞아 저번에 물어봤던 우산",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.2",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.3",
          "form": "찾음?",
          "original_form": "찾음?",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.4",
          "form": "그 뒤에 어디에 있었는지 찾았엌ㅋ",
          "original_form": "그 뒤에 어디에 있었는지 찾았엌ㅋ",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.5",
          "form": "어디?",
          "original_form": "어디?",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.6",
          "form": "엄마가 빌려갔었던 걸 까먹었더라고",
          "original_form": "엄마가 빌려갔었던 걸 까먹었더라고",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.8",
          "form": "애당초 우산을 안갖고왔던거야?",
          "original_form": "애당초 우산을 안갖고왔던거야?",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.9",
          "form": "찾아서 다행히 비올때 잘 썼다",
          "original_form": "찾아서 다행히 비올때 잘 썼다",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005000.1.10",
          "form": "그럼 오늘부터 장마 시작인건가?",
          "original_form": "그럼 오늘부터 장마 시작인건가?",
          "speaker_id": "2",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100005000.1.11",
          "form": "아님 우리 만나고 난 뒤 엄마가 가져갔던거야?",
          "original_form": "아님 우리 만나고 난 뒤 엄마가 가져갔던거야?",
          "speaker_id": "1",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100005000.1.12",
          "form": "여기는 이번주 내내 비온다더라ㅠ",
          "original_form": "여기는 이번주 내내 비온다더라ㅠ",
          "speaker_id": "1",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100005000.1.13",
          "form": "우리도...",
          "original_form": "우리도...",
          "speaker_id": "2",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100005000.1.14",
          "form": "장마 시작 맞나보다",
          "original_form": "장마 시작 맞나보다",
          "speaker_id": "2",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100005000.1.15",
          "form": "에이, 벌써부터 기분이 쳐지네",
          "original_form": "에이, 벌써부터 기분이 쳐지네",
          "speaker_id": "2",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100005000.1.16",
          "form": "빗소리는 좋은데 비오는 날 외출은 싫어",
          "original_form": "빗소리는 좋은데 비오는 날 외출은 싫어",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100005000.1.17",
          "form": "맞아ㅠㅠ",
          "original_form": "맞아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100005000.1.18",
          "form": "비 싫음ㅠ",
          "original_form": "비 싫음ㅠ",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100005000.1.19",
          "form": "근데 원래 이 맘때 비왔었나?",
          "original_form": "근데 원래 이 맘때 비왔었나?",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100005000.1.20",
          "form": "음...기억이 안나",
          "original_form": "음...기억이 안나",
          "speaker_id": "2",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005000.1.21",
          "form": "뒤늦게 태풍 엄청 왔었던거 같은데",
          "original_form": "뒤늦게 태풍 엄청 왔었던거 같은데",
          "speaker_id": "2",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005000.1.22",
          "form": "추석때쯤에 태풍오긴하지",
          "original_form": "추석때쯤에 태풍오긴하지",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005000.1.23",
          "form": "날씨 이상해ㅠㅠ",
          "original_form": "날씨 이상해ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005000.1.24",
          "form": "이렇게 지구종말이 다가오는 것인가",
          "original_form": "이렇게 지구종말이 다가오는 것인가",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005000.1.25",
          "form": "지구종말의 징조인가!",
          "original_form": "지구종말의 징조인가!",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005000.1.26",
          "form": "오늘 날씨 너무 끔찍하지 않니",
          "original_form": "오늘 날씨 너무 끔찍하지 않니",
          "speaker_id": "2",
          "time": "20210824 10:30"
        },
        {
          "id": "MDRW2100005000.1.27",
          "form": "어제부터 천둥번개에...",
          "original_form": "어제부터 천둥번개에...",
          "speaker_id": "2",
          "time": "20210824 10:30"
        },
        {
          "id": "MDRW2100005000.1.28",
          "form": "여름 끝나간다고 생각했는데 정말 갑작스럽게 찾아왔네",
          "original_form": "여름 끝나간다고 생각했는데 정말 갑작스럽게 찾아왔네",
          "speaker_id": "2",
          "time": "20210824 10:31"
        }
      ]
    }
