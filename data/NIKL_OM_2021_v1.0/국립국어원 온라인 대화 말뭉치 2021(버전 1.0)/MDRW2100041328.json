      "utterance": [
        {
          "id": "MDRW2100041328.1.1",
          "form": "거기 분위기도 그렇고",
          "original_form": "거기 분위기도 그렇고",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.2",
          "form": "왠지... 연대하는 여성들이 하는 데라서",
          "original_form": "왠지... 연대하는 여성들이 하는 데라서",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.3",
          "form": "돈 아끼면 안될 것 같고 (?)",
          "original_form": "돈 아끼면 안될 것 같고 (?)",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.4",
          "form": "내가 좀 분위기에 취하는 게 있거든요 ㅠㅠ",
          "original_form": "내가 좀 분위기에 취하는 게 있거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.5",
          "form": "맞아요 돈쭐내고싶고",
          "original_form": "맞아요 돈쭐내고싶고",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.6",
          "form": "돈도없는게ㅠ",
          "original_form": "돈도없는게ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.7",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.8",
          "form": "아니그니까요",
          "original_form": "아니그니까요",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.9",
          "form": "저 진짜 분위기 너무 휩쓸리는 편이라",
          "original_form": "저 진짜 분위기 너무 휩쓸리는 편이라",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.10",
          "form": "근데 대부분 그런걸까요ㅠ",
          "original_form": "근데 대부분 그런걸까요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.11",
          "form": "아 저도요!!!!",
          "original_form": "아 저도요!!!!",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041328.1.12",
          "form": "좀 안그러고 싶은데",
          "original_form": "좀 안그러고 싶은데",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.13",
          "form": "약간 그 자리에서는",
          "original_form": "약간 그 자리에서는",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.14",
          "form": "그게안돼요",
          "original_form": "그게안돼요",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.15",
          "form": "에이 그냥 사먹자~ 이런 생각",
          "original_form": "에이 그냥 사먹자~ 이런 생각",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.16",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.17",
          "form": "맞아요퓨ㅠㅠㅠㅠ",
          "original_form": "맞아요퓨ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.18",
          "form": "통장을 더 세분화해야하나...",
          "original_form": "통장을 더 세분화해야하나...",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.19",
          "form": "밥값 쓰는 통장...",
          "original_form": "밥값 쓰는 통장...",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.20",
          "form": "아 왓는데 먹자..이런기분",
          "original_form": "아 왓는데 먹자..이런기분",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.21",
          "form": "옷 사는 통장...",
          "original_form": "옷 사는 통장...",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.22",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.23",
          "form": "그니까요 저 그래서 적금늘",
          "original_form": "그니까요 저 그래서 적금늘",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.24",
          "form": "더 늘렸어요",
          "original_form": "더 늘렸어요",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.25",
          "form": "아예 돈 못쓰게",
          "original_form": "아예 돈 못쓰게",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.26",
          "form": "네네네 우와 잘하셨어요",
          "original_form": "네네네 우와 잘하셨어요",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.27",
          "form": "근데 신카를 써서.... 진짜 무슨 나는 쫌쫌따리 마넌 이마넌 썻는데",
          "original_form": "근데 신카를 써서.... 진짜 무슨 나는 쫌쫌따리 마넌 이마넌 썻는데",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.28",
          "form": "카드값 1억",
          "original_form": "카드값 1억",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041328.1.29",
          "form": "으앜ㅋㅋㅋㅋ 아니 뭔지 알아요",
          "original_form": "으앜ㅋㅋㅋㅋ 아니 뭔지 알아요",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041328.1.30",
          "form": "티끌 모아 1억 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "티끌 모아 1억 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041328.1.31",
          "form": "여기 대화 주제에",
          "original_form": "여기 대화 주제에",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041328.1.32",
          "form": "ㅋㅋㅋㅋ마통 될 기세.",
          "original_form": "ㅋㅋㅋㅋ마통 될 기세.",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041328.1.33",
          "form": "금융 경제 이런 것도 있으면 좋겠어요",
          "original_form": "금융 경제 이런 것도 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041328.1.34",
          "form": "ㅋㅋㅋㅋ오 맞네요",
          "original_form": "ㅋㅋㅋㅋ오 맞네요",
          "speaker_id": "2",
