      "utterance": [
        {
          "id": "MDRW2100013455.1.1",
          "form": "고기국수",
          "original_form": "고기국수",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013455.1.2",
          "form": "맛있던가요?",
          "original_form": "맛있던가요?",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013455.1.3",
          "form": "좀 느끼하다고 생각할수도 있을거같긴한데",
          "original_form": "좀 느끼하다고 생각할수도 있을거같긴한데",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013455.1.4",
          "form": "저는 면종류 음식을 좋아해서 그런지 맛있었어요 ㅎㅎ",
          "original_form": "저는 면종류 음식을 좋아해서 그런지 맛있었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013455.1.5",
          "form": "ㅋㅋㅋ저도먹어봤어요",
          "original_form": "ㅋㅋㅋ저도먹어봤어요",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013455.1.6",
          "form": "근데 맛집 찾아갔는데",
          "original_form": "근데 맛집 찾아갔는데",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013455.1.7",
          "form": "그날하필휴무더라고요?",
          "original_form": "그날하필휴무더라고요?",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013455.1.8",
          "form": "그래서 차선책으로 동문시장에 있는곳갔는데",
          "original_form": "그래서 차선책으로 동문시장에 있는곳갔는데",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013455.1.9",
          "form": "그냥그랬어서 슬펐어요",
          "original_form": "그냥그랬어서 슬펐어요",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100013455.1.10",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013455.1.11",
          "form": "맛집 휴무 진짜 마음찢기도록 슬프죠",
          "original_form": "맛집 휴무 진짜 마음찢기도록 슬프죠",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013455.1.12",
          "form": "ㅋㅋㅋ제주도가고싶네요 맛집탐방",
          "original_form": "ㅋㅋㅋ제주도가고싶네요 맛집탐방",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013455.1.13",
          "form": "해산물러버에겐 너무좋은여행지죠",
          "original_form": "해산물러버에겐 너무좋은여행지죠",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013455.1.14",
          "form": "맞아요 제가 해산물러버인지라 ㅋㅋㅋ",
          "original_form": "맞아요 제가 해산물러버인지라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013455.1.15",
          "form": "또 가고싶네요",
          "original_form": "또 가고싶네요",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013455.1.16",
          "form": "ㅋㅋㅋㅋ제가 진짜 찐이라고요 너무좋아해요",
          "original_form": "ㅋㅋㅋㅋ제가 진짜 찐이라고요 너무좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013455.1.17",
          "form": "제주도 한달살기하면 넘좋을것같아요\n식도락",
          "original_form": "제주도 한달살기하면 넘좋을것같아요\n식도락",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013455.1.18",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013455.1.19",
          "form": "배고프네요ㅠ",
          "original_form": "배고프네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013455.1.20",
          "form": "저도요! 점심을 적게먹어서ㅠㅠ",
          "original_form": "저도요! 점심을 적게먹어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013455.1.21",
          "form": "점심 뭐드셨나여?",
          "original_form": "점심 뭐드셨나여?",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013455.1.22",
          "form": "점심은 가볍게 라면이랑 수육 조금 먹었네요",
          "original_form": "점심은 가볍게 라면이랑 수육 조금 먹었네요",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013455.1.23",
          "form": "헐 그게 가볍게인가요?",
          "original_form": "헐 그게 가볍게인가요?",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013455.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013455.1.25",
          "form": "수육이 가볍게인가요?",
          "original_form": "수육이 가볍게인가요?",
