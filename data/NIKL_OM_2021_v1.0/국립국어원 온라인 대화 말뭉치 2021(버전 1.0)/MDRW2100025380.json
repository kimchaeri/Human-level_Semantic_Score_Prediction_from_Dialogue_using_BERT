          "speaker_id": "2",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100025380.1.3",
          "form": "재택은 무슨",
          "original_form": "재택은 무슨",
          "speaker_id": "2",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100025380.1.4",
          "form": "뭐 하라면 하겠다만",
          "original_form": "뭐 하라면 하겠다만",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.5",
          "form": "저희는 제조업종이라",
          "original_form": "저희는 제조업종이라",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.6",
          "form": "재택하는친구들보면 완전꿀빨던데",
          "original_form": "재택하는친구들보면 완전꿀빨던데",
          "speaker_id": "1",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.7",
          "form": "친구들도 몇명 있는데",
          "original_form": "친구들도 몇명 있는데",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.8",
          "form": "집중이 안된대요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "집중이 안된대요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.9",
          "form": "집에서 일하니깐",
          "original_form": "집에서 일하니깐",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.10",
          "form": "당연하죠 ㅋㅋㅋㅋ침대가옆에있는데",
          "original_form": "당연하죠 ㅋㅋㅋㅋ침대가옆에있는데",
          "speaker_id": "1",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.11",
          "form": "효율 쓰레기라고 커피집가던데",
          "original_form": "효율 쓰레기라고 커피집가던데",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.12",
          "form": "그런듯ㅋㅋㅋ",
          "original_form": "그런듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025380.1.13",
          "form": "??? 야 개편한데 일이 안들어와",
          "original_form": "??? 야 개편한데 일이 안들어와",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025380.1.14",
          "form": "재택하면 업무 밀리긴할듯ㅋㅋㅋㅋㅋ",
          "original_form": "재택하면 업무 밀리긴할듯ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025380.1.15",
          "form": "재택 뭐 장단점 있겠죠",
          "original_form": "재택 뭐 장단점 있겠죠",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025380.1.16",
          "form": "보니까 위엔 셔츠입고",
          "original_form": "보니까 위엔 셔츠입고",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025380.1.17",
          "form": "반바지입고 일한다는데",
          "original_form": "반바지입고 일한다는데",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025380.1.18",
          "form": "듣긴들었는데 그거진짜 갑질아님?ㅋㅋㅋㅋ",
          "original_form": "듣긴들었는데 그거진짜 갑질아님?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:34"
        },
        {
          "id": "MDRW2100025380.1.19",
          "form": "근데 화상으로 보고 있는데 후줄근한것도 좀 그러잖아요",
          "original_form": "근데 화상으로 보고 있는데 후줄근한것도 좀 그러잖아요",
          "speaker_id": "2",
          "time": "20210901 10:34"
        },
        {
          "id": "MDRW2100025380.1.20",
          "form": "셔츠까지 입을필요가 있나 싶은거죠",
          "original_form": "셔츠까지 입을필요가 있나 싶은거죠",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025380.1.21",
          "form": "셔츠가 편함",
          "original_form": "셔츠가 편함",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025380.1.22",
          "form": "아 뭐 회의면 인정",
          "original_form": "아 뭐 회의면 인정",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025380.1.23",
          "form": "괜찮나? 이정도면 됐나 생각 안해도 되고",
          "original_form": "괜찮나? 이정도면 됐나 생각 안해도 되고",
