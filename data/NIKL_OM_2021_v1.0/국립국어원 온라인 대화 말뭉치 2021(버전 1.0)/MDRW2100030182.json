      "utterance": [
        {
          "id": "MDRW2100030182.1.1",
          "form": "저두 대학생이랍니닷",
          "original_form": "저두 대학생이랍니닷",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030182.1.2",
          "form": "꺅 저는 대학교2학년이에요",
          "original_form": "꺅 저는 대학교2학년이에요",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030182.1.3",
          "form": "헐 저두요!! 사실 휴학해서 2학년에 멈춘거지만 곧 3학년이랍니다>,<",
          "original_form": "헐 저두요!! 사실 휴학해서 2학년에 멈춘거지만 곧 3학년이랍니다>,<",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030182.1.4",
          "form": "헉 휴학 멋져요 저는ㅋㅋㅋ 편입하고 안되면 휴학하려고요",
          "original_form": "헉 휴학 멋져요 저는ㅋㅋㅋ 편입하고 안되면 휴학하려고요",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030182.1.5",
          "form": "편입이 더 멋지신걸요 어느과로 편입 하시려구요!? 아까 도예과라고 하셔서 완전 궁금한",
          "original_form": "편입이 더 멋지신걸요 어느과로 편입 하시려구요!? 아까 도예과라고 하셔서 완전 궁금한",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030182.1.6",
          "form": "과는 같고 다른 학교로 가고싶어서요!!",
          "original_form": "과는 같고 다른 학교로 가고싶어서요!!",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030182.1.7",
          "form": "글쿤요!! 완전 대단하셔요 편입 준비하는 친구들 보니 다들 열심히 하던데^//^!!",
          "original_form": "글쿤요!! 완전 대단하셔요 편입 준비하는 친구들 보니 다들 열심히 하던데^//^!!",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030182.1.8",
          "form": "맞아요! 문제는 영어가...ㅜㅜ",
          "original_form": "맞아요! 문제는 영어가...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030182.1.9",
          "form": "토익이 너무 어려워요",
          "original_form": "토익이 너무 어려워요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030182.1.10",
          "form": "아 ㅠㅠㅠㅠ 영어 ㅠㅠㅠ 토익 어렵죠ㅠㅠ",
          "original_form": "아 ㅠㅠㅠㅠ 영어 ㅠㅠㅠ 토익 어렵죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030182.1.11",
          "form": "무슨과세요?? 아까 여쭤봤나...?",
          "original_form": "무슨과세요?? 아까 여쭤봤나...?",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030182.1.12",
          "form": "아까 안 여쭤보셨었어요!",
          "original_form": "아까 안 여쭤보셨었어요!",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030182.1.13",
          "form": "저는 만화애니과랍니당",
          "original_form": "저는 만화애니과랍니당",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030182.1.14",
          "form": "그래서 영어 포기하고도 어떻게 해서 학교 갔어요 ㅋㅋ!!",
          "original_form": "그래서 영어 포기하고도 어떻게 해서 학교 갔어요 ㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030182.1.15",
          "form": "만화애니과 멋져요!!!",
          "original_form": "만화애니과 멋져요!!!",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030182.1.16",
          "form": "같은 예술쪽이네요ㅜㅜ",
          "original_form": "같은 예술쪽이네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030182.1.17",
          "form": "그래서 아까 과 듣고 반가웠었어요 ㅎㅎ",
          "original_form": "그래서 아까 과 듣고 반가웠었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030182.1.18",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030182.1.19",
          "form": "입시미술 하셨..죠...?",
          "original_form": "입시미술 하셨..죠...?",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030182.1.20",
          "form": "했습니당..... 도예과도 입시 실기시험 하죠...??",
          "original_form": "했습니당..... 도예과도 입시 실기시험 하죠...??",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030182.1.21",
          "form": "그럼요... 기디했답니다...",
          "original_form": "그럼요... 기디했답니다...",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030182.1.22",
          "form": "도예과는 기디 하는줄 첨 알았어요 우와",
          "original_form": "도예과는 기디 하는줄 첨 알았어요 우와",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030182.1.23",
          "form": "기디 완전 어려워 보이던데 고생하셨겠어요 ㅠㅠㅠ",
          "original_form": "기디 완전 어려워 보이던데 고생하셨겠어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030182.1.24",
          "form": "추합이었거든용...",
          "original_form": "추합이었거든용...",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030182.1.25",
          "form": "물레시험이랑 점토성형 있었는데",
          "original_form": "물레시험이랑 점토성형 있었는데",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030182.1.26",
          "form": "제가 그 때 손가락을 베이는 바람에 망쳤어요..",
          "original_form": "제가 그 때 손가락을 베이는 바람에 망쳤어요..",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030182.1.27",
          "form": "기디할 때는 펜촉에도 찔려봤답니다...",
          "original_form": "기디할 때는 펜촉에도 찔려봤답니다...",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030182.1.28",
          "form": "에궁 ㅠㅠㅠㅠ 망친것도 눈물나는데 다쳤다고 하시니까 더 ㅠㅠㅠㅠ",
          "original_form": "에궁 ㅠㅠㅠㅠ 망친것도 눈물나는데 다쳤다고 하시니까 더 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030182.1.29",
          "form": "그래두 대학 붙으셔서 다행이에요ㅠㅠ",
          "original_form": "그래두 대학 붙으셔서 다행이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030182.1.30",
          "form": "저는 사실 수시때 별로 안 가고 싶던 곳 붙어서 잠시 재수 준비 했었거든요.. 그때 잠깐이긴 했지만 진짜 울면서 그림 그렸던..",
          "original_form": "저는 사실 수시때 별로 안 가고 싶던 곳 붙어서 잠시 재수 준비 했었거든요.. 그때 잠깐이긴 했지만 진짜 울면서 그림 그렸던..",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.31",
          "form": "히히 그쵸 편입할 기회가 주어졌다고 생각하고 열심히 하고있어요",
          "original_form": "히히 그쵸 편입할 기회가 주어졌다고 생각하고 열심히 하고있어요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.32",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.33",
          "form": "울지마세요ㅜㅜㅠㅜㅠ.. 학교가 어떻든 열심히하면 되는거니까요!!",
          "original_form": "울지마세요ㅜㅜㅠㅜㅠ.. 학교가 어떻든 열심히하면 되는거니까요!!",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.34",
          "form": "ㅎㅎ물론 저도 추합으로 대학 다른곳 붙어서 재수 준비 그만하고 대학 왔답니다!",
          "original_form": "ㅎㅎ물론 저도 추합으로 대학 다른곳 붙어서 재수 준비 그만하고 대학 왔답니다!",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.35",
          "form": "앗 다행이에요!!!!",
          "original_form": "앗 다행이에요!!!!",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.36",
          "form": "원하는곳 붙으셨나요??",
          "original_form": "원하는곳 붙으셨나요??",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.37",
          "form": "ㅋㅋㅋ제 기준 그럭저럭 괜찮은 곳 와서 만족하면서 다니구 있어요 !!",
          "original_form": "ㅋㅋㅋ제 기준 그럭저럭 괜찮은 곳 와서 만족하면서 다니구 있어요 !!",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.38",
          "form": "네!1",
          "original_form": "네!1",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030182.1.39",
          "form": "헉 다행이에요!!!!",
          "original_form": "헉 다행이에요!!!!",
