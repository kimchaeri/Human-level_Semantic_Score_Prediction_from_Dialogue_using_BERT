      "utterance": [
        {
          "id": "MDRW2100036533.1.1",
          "form": "저도 요즘 알림이 없네요",
          "original_form": "저도 요즘 알림이 없네요",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036533.1.2",
          "form": "한번은 이런 채팅이 왔는데",
          "original_form": "한번은 이런 채팅이 왔는데",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036533.1.3",
          "form": "만약 제가 오천원에 올려놨으면",
          "original_form": "만약 제가 오천원에 올려놨으면",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036533.1.4",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036533.1.5",
          "form": "삼천원에 쿨거래 해요",
          "original_form": "삼천원에 쿨거래 해요",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036533.1.6",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036533.1.7",
          "form": "뭔지알것같은",
          "original_form": "뭔지알것같은",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036533.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036533.1.9",
          "form": "그거보다 더심한거 있어요",
          "original_form": "그거보다 더심한거 있어요",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036533.1.10",
          "form": "제가 에어팟무선충전2를 내놨거든요",
          "original_form": "제가 에어팟무선충전2를 내놨거든요",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036533.1.11",
          "form": "저 진짜 괜찮은거 완전 저렴하게 올리거든요",
          "original_form": "저 진짜 괜찮은거 완전 저렴하게 올리거든요",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036533.1.12",
          "form": "새상품",
          "original_form": "새상품",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036533.1.13",
          "form": "근데 100,000 ok?",
          "original_form": "근데 100,000 ok?",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036533.1.14",
          "form": "이렇게 대뜸 온거에요",
          "original_form": "이렇게 대뜸 온거에요",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036533.1.15",
          "form": "제가 안됩니다 하니까",
          "original_form": "제가 안됩니다 하니까",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036533.1.16",
          "form": "어우.....",
          "original_form": "어우.....",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036533.1.17",
          "form": "진짜야",
          "original_form": "진짜야",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036533.1.18",
          "form": "이러길래 차단을..ㅋㅋ",
          "original_form": "이러길래 차단을..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036533.1.19",
          "form": "대뜸 진짜 ㅋㅋㅋㅋ",
