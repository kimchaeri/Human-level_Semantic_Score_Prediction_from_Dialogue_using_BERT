      "utterance": [
        {
          "id": "MDRW2100012976.1.1",
          "form": "요즘 드라마나 티비프로그램 보는거 있어?",
          "original_form": "요즘 드라마나 티비프로그램 보는거 있어?",
          "speaker_id": "1",
          "time": "20210825 09:54"
        },
        {
          "id": "MDRW2100012976.1.2",
          "form": "요새",
          "original_form": "요새",
          "speaker_id": "2",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100012976.1.3",
          "form": "슬의 보고있지 ㅋㅋㄱㄱㄱㅋ",
          "original_form": "슬의 보고있지 ㅋㅋㄱㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100012976.1.4",
          "form": "슬의말고는 보는게 없군",
          "original_form": "슬의말고는 보는게 없군",
          "speaker_id": "2",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100012976.1.5",
          "form": "뭐 보노??",
          "original_form": "뭐 보노??",
          "speaker_id": "2",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100012976.1.6",
          "form": "슬의생 본다ㅋ",
          "original_form": "슬의생 본다ㅋ",
          "speaker_id": "1",
          "time": "20210825 10:07"
        },
        {
          "id": "MDRW2100012976.1.7",
          "form": "펜트하우스3랑ㅋ",
          "original_form": "펜트하우스3랑ㅋ",
          "speaker_id": "1",
          "time": "20210825 10:07"
        },
        {
          "id": "MDRW2100012976.1.8",
          "form": "슬의생 울 첫째도 좋아함ㅋ",
          "original_form": "슬의생 울 첫째도 좋아함ㅋ",
          "speaker_id": "1",
          "time": "20210825 10:07"
        },
        {
          "id": "MDRW2100012976.1.9",
          "form": "재미있어ㅋ",
          "original_form": "재미있어ㅋ",
          "speaker_id": "1",
          "time": "20210825 10:07"
        },
        {
          "id": "MDRW2100012976.1.10",
          "form": "아 ㅋㅋㅋㅋ 첫째는 벌써 드라마를 즐겨보는군",
          "original_form": "아 ㅋㅋㅋㅋ 첫째는 벌써 드라마를 즐겨보는군",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012976.1.11",
          "form": "놀면뭐하니도 msg 워너비 할때",
          "original_form": "놀면뭐하니도 msg 워너비 할때",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012976.1.12",
          "form": "봤는데",
          "original_form": "봤는데",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012976.1.13",
          "form": "끝나서 안보고",
          "original_form": "끝나서 안보고",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012976.1.14",
          "form": "ㅋㅋ응응ㅋㅋ맞아ㅋㅋ",
          "original_form": "ㅋㅋ응응ㅋㅋ맞아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012976.1.15",
          "form": "우연히 한번봤는데",
          "original_form": "우연히 한번봤는데",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012976.1.16",
          "form": "너무좋아하더라구ㅋ",
          "original_form": "너무좋아하더라구ㅋ",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012976.1.17",
          "form": "오호 잘본다이",
          "original_form": "오호 잘본다이",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012976.1.18",
          "form": "빨리 내일이 기다려지는구먼",
          "original_form": "빨리 내일이 기다려지는구먼",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012976.1.19",
          "form": "곧 류준열 나오는 드라마한다길래",
          "original_form": "곧 류준열 나오는 드라마한다길래",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100012976.1.20",
          "form": "기다리고 있다",
          "original_form": "기다리고 있다",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100012976.1.21",
          "form": "오오 뭘까? 궁금하네ㅋ",
          "original_form": "오오 뭘까? 궁금하네ㅋ",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012976.1.22",
          "form": "그 누구더라 신민아 나오는것도",
          "original_form": "그 누구더라 신민아 나오는것도",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012976.1.23",
          "form": "재미있겠던데ㅋ",
          "original_form": "재미있겠던데ㅋ",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100012976.1.24",
          "form": "남주가 중요하다",
          "original_form": "남주가 중요하다",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100012976.1.25",
          "form": "나의 드라마 선택기준은",
          "original_form": "나의 드라마 선택기준은",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100012976.1.26",
          "form": "남주 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "남주 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100012976.1.27",
          "form": "류존열이랑 전도연 나온다대",
          "original_form": "류존열이랑 전도연 나온다대",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100012976.1.28",
          "form": "인간실격 이라고",
          "original_form": "인간실격 이라고",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100012976.1.29",
          "form": "ㅋㅋ아아",
          "original_form": "ㅋㅋ아아",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.30",
          "form": "전도연? 대박이네ㅋ",
          "original_form": "전도연? 대박이네ㅋ",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.31",
          "form": "엉 류준열이랑",
          "original_form": "엉 류준열이랑",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.32",
          "form": "전도연 나온다니",
          "original_form": "전도연 나온다니",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.33",
          "form": "잘어울림",
          "original_form": "잘어울림",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.34",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.35",
          "form": "대박 느낌",
          "original_form": "대박 느낌",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.36",
          "form": "기다리고 있음",
          "original_form": "기다리고 있음",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.37",
          "form": "봐야겠다",
          "original_form": "봐야겠다",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.38",
          "form": "어차피 본방은 못보고",
          "original_form": "어차피 본방은 못보고",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100012976.1.39",
          "form": "넷플로",
          "original_form": "넷플로",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100012976.1.40",
          "form": "맞어ㅋㅋ",
          "original_form": "맞어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100012976.1.41",
          "form": "슬의도 항상 넷플 ㅋㅋ",
          "original_form": "슬의도 항상 넷플 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100012976.1.42",
          "form": "응응ㅋㅋ",
          "original_form": "응응ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100012976.1.43",
          "form": "넷플 이용하는구만ㅋㅋ",
          "original_form": "넷플 이용하는구만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:41"
        }
      ]
