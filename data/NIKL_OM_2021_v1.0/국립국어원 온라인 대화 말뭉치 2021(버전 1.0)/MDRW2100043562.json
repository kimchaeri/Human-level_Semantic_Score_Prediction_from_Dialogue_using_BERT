      "utterance": [
        {
          "id": "MDRW2100043562.1.1",
          "form": "아 그렇군요 백신은 뭐 맞으세요?",
          "original_form": "아 그렇군요 백신은 뭐 맞으세요?",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043562.1.2",
          "form": "모더나 아님 화이자라던데 아직은 모르겠어요",
          "original_form": "모더나 아님 화이자라던데 아직은 모르겠어요",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043562.1.3",
          "form": "병원에 들어오는데로 맞으시겠네요",
          "original_form": "병원에 들어오는데로 맞으시겠네요",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043562.1.4",
          "form": "혹시 희망하는 백신 있으세요?",
          "original_form": "혹시 희망하는 백신 있으세요?",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043562.1.5",
          "form": "아녀! 사실 뭐가 좋은지 잘 모르겠어요 ㅋㅋㅋ",
          "original_form": "아녀! 사실 뭐가 좋은지 잘 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043562.1.6",
          "form": "백신 맞고 부작용 없이 효과가 좋으면 됐죠",
          "original_form": "백신 맞고 부작용 없이 효과가 좋으면 됐죠",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043562.1.7",
          "form": "잘 챙겨 먹고 잘 쉬세요",
          "original_form": "잘 챙겨 먹고 잘 쉬세요",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043562.1.8",
          "form": "ㅋㅋㅋ 부작용있응까봐 걱정이에요",
          "original_form": "ㅋㅋㅋ 부작용있응까봐 걱정이에요",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043562.1.9",
          "form": "아프다는데 얼마나 아픙지두 무섭구여ㅠㅠ",
          "original_form": "아프다는데 얼마나 아픙지두 무섭구여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043562.1.10",
          "form": "독감 주사보다는 안 아프데요",
          "original_form": "독감 주사보다는 안 아프데요",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043562.1.11",
          "form": "독감 주사를 맞아 본 적이 없어서..ㅋㅋ",
          "original_form": "독감 주사를 맞아 본 적이 없어서..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043562.1.12",
          "form": "저도요 신종플루 때 맞아본게 다에요ㅋㅋ",
          "original_form": "저도요 신종플루 때 맞아본게 다에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043562.1.13",
          "form": "전 신종플루때도 안 맞고 지나갔ㅇ요",
          "original_form": "전 신종플루때도 안 맞고 지나갔ㅇ요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043562.1.14",
          "form": "왜 안 맞았지.. 기억은 안나지만요",
          "original_form": "왜 안 맞았지.. 기억은 안나지만요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043562.1.15",
          "form": "학교에서 놔준다고 갔는데 친구는 달걀 알러지라서 저만 맞았어요",
          "original_form": "학교에서 놔준다고 갔는데 친구는 달걀 알러지라서 저만 맞았어요",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043562.1.16",
          "form": "그러고보니 신종픙루때도 난리였네요 ㅋㅋㅋㅋ",
          "original_form": "그러고보니 신종픙루때도 난리였네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043562.1.17",
          "form": "전 그 때 수능쳤었는데 막 신종플루 걸린 친구 따로 시험치구 그랬었어요",
          "original_form": "전 그 때 수능쳤었는데 막 신종플루 걸린 친구 따로 시험치구 그랬었어요",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043562.1.18",
          "form": "신종플루는 그래도 코로나보단 나았죠ㅠ",
          "original_form": "신종플루는 그래도 코로나보단 나았죠ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043562.1.19",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043562.1.20",
          "form": "전 코로나가 이렇게 오래 갈 줄 몰랐어요ㅠ 올 해는 끝날 줄 알았는데..",
          "original_form": "전 코로나가 이렇게 오래 갈 줄 몰랐어요ㅠ 올 해는 끝날 줄 알았는데..",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043562.1.21",
          "form": "그러게요 전 처음 나올 때 신종플루나 메르스처럼 지나갈 줄 알았어요",
          "original_form": "그러게요 전 처음 나올 때 신종플루나 메르스처럼 지나갈 줄 알았어요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        }
      ]
    }
