      "utterance": [
        {
          "id": "MDRW2100036981.1.1",
          "form": "여기두~~ 오랜만에 많이온당",
          "original_form": "여기두~~ 오랜만에 많이온당",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100036981.1.2",
          "form": "딱 목요일까지 계속 오고",
          "original_form": "딱 목요일까지 계속 오고",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036981.1.3",
          "form": "금욜부터는 안 온대~~",
          "original_form": "금욜부터는 안 온대~~",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036981.1.4",
          "form": "오옷! 다행이다~",
          "original_form": "오옷! 다행이다~",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100036981.1.5",
          "form": "비 넘 자주 와 ㅠ",
          "original_form": "비 넘 자주 와 ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100036981.1.6",
          "form": "웅웅...",
          "original_form": "웅웅...",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100036981.1.7",
          "form": "여름비보다 가을비가 더 많이 오는듯..",
          "original_form": "여름비보다 가을비가 더 많이 오는듯..",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100036981.1.8",
          "form": "그르게.. 뭔 장마가 이리 늦게",
          "original_form": "그르게.. 뭔 장마가 이리 늦게",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100036981.1.9",
          "form": "그나마 재택이라 다행인데",
          "original_form": "그나마 재택이라 다행인데",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100036981.1.10",
          "form": "출퇴근했었으면",
          "original_form": "출퇴근했었으면",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100036981.1.11",
          "form": "암울했을듯..ㅠ",
          "original_form": "암울했을듯..ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100036981.1.12",
          "form": "글게 ㅜ우산쓰는거 넘피곤 ㅋㅋ",
          "original_form": "글게 ㅜ우산쓰는거 넘피곤 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100036981.1.13",
          "form": "진짜 어쩔땐 우비를 입고싶다니까~ㅋㅋㅋ",
          "original_form": "진짜 어쩔땐 우비를 입고싶다니까~ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:41"
        },
        {
          "id": "MDRW2100036981.1.14",
          "form": "마자 ㅋㅋㅋ 우비 좋아",
          "original_form": "마자 ㅋㅋㅋ 우비 좋아",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036981.1.15",
          "form": "아직도 많이 오네 ㅠ",
          "original_form": "아직도 많이 오네 ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036981.1.16",
          "form": "웅우~",
          "original_form": "웅우~",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100036981.1.17",
          "form": "추워~",
          "original_form": "추워~",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100036981.1.18",
          "form": "방금 음쓰 버리러 잠깐 나갔다왔는데",
          "original_form": "방금 음쓰 버리러 잠깐 나갔다왔는데",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100036981.1.19",
          "form": "그새 흠뻑 젖음..ㅋㅋㅋ",
          "original_form": "그새 흠뻑 젖음..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100036981.1.20",
          "form": "으악 ㅠㅋㅋㅋ",
          "original_form": "으악 ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100036981.1.21",
          "form": "약간 추우면서도 습한 느낌..ㅋㅋㅋ",
          "original_form": "약간 추우면서도 습한 느낌..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100036981.1.22",
