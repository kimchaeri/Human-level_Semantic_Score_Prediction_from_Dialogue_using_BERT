      "utterance": [
        {
          "id": "MDRW2100038354.1.1",
          "form": "여긴 바람이 많이 불어요ㅜㅜ",
          "original_form": "여긴 바람이 많이 불어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100038354.1.2",
          "form": "영 아일랜드 태평양바람 어련하겠습니까",
          "original_form": "영 아일랜드 태평양바람 어련하겠습니까",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100038354.1.3",
          "form": "오늘도 여전합니다",
          "original_form": "오늘도 여전합니다",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100038354.1.4",
          "form": "우산이 의미없음",
          "original_form": "우산이 의미없음",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100038354.1.5",
          "form": "비안오는게 어디고",
          "original_form": "비안오는게 어디고",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100038354.1.6",
          "form": "비가 가로로 와요",
          "original_form": "비가 가로로 와요",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100038354.1.7",
          "form": "마저 ㅋㅋ",
          "original_form": "마저 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100038354.1.8",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038354.1.9",
          "form": "영아일랜드니까",
          "original_form": "영아일랜드니까",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100038354.1.10",
          "form": "여긴또 비가그쳤네요",
          "original_form": "여긴또 비가그쳤네요",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100038354.1.11",
          "form": "그쪽도 가로로비가 와요?",
          "original_form": "그쪽도 가로로비가 와요?",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100038354.1.12",
          "form": "여긴 영아일랜드 아니라서 세로로와요",
          "original_form": "여긴 영아일랜드 아니라서 세로로와요",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100038354.1.13",
          "form": "좋겠네요",
          "original_form": "좋겠네요",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100038354.1.14",
          "form": "비와서좋은건아니지만 name2님네보단 낫네요",
          "original_form": "비와서좋은건아니지만 &name2&님네보단 낫네요",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100038354.1.15",
          "form": "여긴 대신 여름에  시원했어요",
          "original_form": "여긴 대신 여름에  시원했어요",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100038354.1.16",
          "form": "아유 좋네요",
          "original_form": "아유 좋네요",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038354.1.17",
          "form": "부럽당",
          "original_form": "부럽당",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038354.1.18",
          "form": "부럽죠?",
          "original_form": "부럽죠?",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038354.1.19",
          "form": "겨울은 추워요",
          "original_form": "겨울은 추워요",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038354.1.20",
          "form": "그건 안부러워요",
          "original_form": "그건 안부러워요",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100038354.1.21",
          "form": "시원해요",
          "original_form": "시원해요",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100038354.1.22",
          "form": "겨울말이에요",
          "original_form": "겨울말이에요",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100038354.1.23",
          "form": "겨울은 시원해야죠",
          "original_form": "겨울은 시원해야죠",
          "speaker_id": "2",
          "time": "20210902 11:19"
        }
