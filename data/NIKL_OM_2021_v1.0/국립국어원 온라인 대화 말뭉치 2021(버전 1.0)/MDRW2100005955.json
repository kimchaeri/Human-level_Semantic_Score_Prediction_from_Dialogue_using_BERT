      "utterance": [
        {
          "id": "MDRW2100005955.1.1",
          "form": "저 깡통시장 갔었어요",
          "original_form": "저 깡통시장 갔었어요",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005955.1.2",
          "form": "깡통시장 먹거리많아서 재밌죠",
          "original_form": "깡통시장 먹거리많아서 재밌죠",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005955.1.3",
          "form": "야시장가신거예요?",
          "original_form": "야시장가신거예요?",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005955.1.4",
          "form": "아뇨 낮에 다녀왔어요 밤에는 해운대 갔다왔어서",
          "original_form": "아뇨 낮에 다녀왔어요 밤에는 해운대 갔다왔어서",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005955.1.5",
          "form": "그러셨구나",
          "original_form": "그러셨구나",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100005955.1.6",
          "form": "밤에 야시장을해서 대만 야시장처럼",
          "original_form": "밤에 야시장을해서 대만 야시장처럼",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100005955.1.7",
          "form": "먹거리트럭을 줄지어놓고 팔더라구요",
          "original_form": "먹거리트럭을 줄지어놓고 팔더라구요",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100005955.1.8",
          "form": "헐 저 야시장도 엄청 좋아하는데 다음에는 밤에 가봐야겠네요ㅠㅜ",
          "original_form": "헐 저 야시장도 엄청 좋아하는데 다음에는 밤에 가봐야겠네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100005955.1.9",
          "form": "관광지는 대부분 밤이 더 좋은거 같네요",
          "original_form": "관광지는 대부분 밤이 더 좋은거 같네요",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100005955.1.10",
          "form": "부산 여행가기 되게 좋은 도시잖아요 부산사시면 추천해주실만한곳 있으실까요??",
          "original_form": "부산 여행가기 되게 좋은 도시잖아요 부산사시면 추천해주실만한곳 있으실까요??",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100005955.1.11",
          "form": "음 흰여울길 가보셨나요?",
          "original_form": "음 흰여울길 가보셨나요?",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005955.1.12",
          "form": "아뇨 흰여울길이 여행가기 좋나요??",
          "original_form": "아뇨 흰여울길이 여행가기 좋나요??",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100005955.1.13",
          "form": "거기가 해안절벽이 있는곳이라 경치가 좋아요",
          "original_form": "거기가 해안절벽이 있는곳이라 경치가 좋아요",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100005955.1.14",
          "form": "카페들도 많은데 어딜가셔도 괜찮으시겠지만",
          "original_form": "카페들도 많은데 어딜가셔도 괜찮으시겠지만",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100005955.1.15",
          "form": "손목서가가유명하구요 에테르도 추천드려요",
          "original_form": "손목서가가유명하구요 에테르도 추천드려요",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100005955.1.16",
          "form": "헐 해안절벽 엄청 이쁠것같은데요??",
          "original_form": "헐 해안절벽 엄청 이쁠것같은데요??",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005955.1.17",
          "form": "엄청 이뻐요 부산살때 자주갔어요",
          "original_form": "엄청 이뻐요 부산살때 자주갔어요",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100005955.1.18",
          "form": "남포동도 가까워서 갔다가 남포동가서 저녁먹으면 딱이예요",
          "original_form": "남포동도 가까워서 갔다가 남포동가서 저녁먹으면 딱이예요",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100005955.1.19",
          "form": "헐 ㅠㅜ 다음에 부산 가게되면 가볼게요",
          "original_form": "헐 ㅠㅜ 다음에 부산 가게되면 가볼게요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100005955.1.20",
          "form": "전 천안 살고있는데 노잼도시인것같아요",
          "original_form": "전 천안 살고있는데 노잼도시인것같아요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        }
      ]
