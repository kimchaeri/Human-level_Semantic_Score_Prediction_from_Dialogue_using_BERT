      "utterance": [
        {
          "id": "MDRW2100009499.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009499.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009499.1.3",
          "form": "주로 무얼 사시나요?",
          "original_form": "주로 무얼 사시나요?",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009499.1.4",
          "form": "아무래도 옷같은거 사는거 좋아해요",
          "original_form": "아무래도 옷같은거 사는거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009499.1.5",
          "form": "아하? 저는 다른건 욕심이 없는데\r\n립 제품 애정자에요.",
          "original_form": "아하? 저는 다른건 욕심이 없는데\r\n립 제품 애정자에요.",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009499.1.6",
          "form": "서랍 가득 립스틱, 틴트, 립밤이 ㅠ",
          "original_form": "서랍 가득 립스틱, 틴트, 립밤이 ㅠ",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009499.1.7",
          "form": "오~ 기분전환하기에 립만한게 없죠",
          "original_form": "오~ 기분전환하기에 립만한게 없죠",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009499.1.8",
          "form": "요즘은 마스크를 해서 입술을 안발라도 되는데",
          "original_form": "요즘은 마스크를 해서 입술을 안발라도 되는데",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009499.1.9",
          "form": "욕심쟁이죠 ㅎㅎ",
          "original_form": "욕심쟁이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009499.1.10",
          "form": "저는 마스크속에도 틴트같은거살짝 발라요ㅋㅋ",
          "original_form": "저는 마스크속에도 틴트같은거살짝 발라요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009499.1.11",
          "form": "벗었을때 내가보니까요",
          "original_form": "벗었을때 내가보니까요",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009499.1.12",
          "form": "그니까요 맞아요 ㅋㅋㅋ \r\n잠깐 벗을 때의 칙칙함이 싫더라고요",
          "original_form": "그니까요 맞아요 ㅋㅋㅋ \r\n잠깐 벗을 때의 칙칙함이 싫더라고요",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009499.1.13",
          "form": "맞아요ㅋㅋ아파보이고 칙칙해보이고",
          "original_form": "맞아요ㅋㅋ아파보이고 칙칙해보이고",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009499.1.14",
          "form": "지금 여름 끝무렵인데 \r\n옷 사시면 뭐 사세요?",
          "original_form": "지금 여름 끝무렵인데 \r\n옷 사시면 뭐 사세요?",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009499.1.15",
          "form": "여름옷위에 걸칠바람막이랑",
          "original_form": "여름옷위에 걸칠바람막이랑",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009499.1.16",
          "form": "여름옷 세일하는거 ?\r\n아님 가을 옷?",
          "original_form": "여름옷 세일하는거 ?\r\n아님 가을 옷?",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009499.1.17",
          "form": "원피스샀어요ㅋ 세일하는거",
          "original_form": "원피스샀어요ㅋ 세일하는거",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009499.1.18",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009499.1.19",
          "form": "맞아요 반팔에 가디건, 점퍼 입으면 \r\n또 몇주 가니까요 ㅎㅎ",
          "original_form": "맞아요 반팔에 가디건, 점퍼 입으면 \r\n또 몇주 가니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009499.1.20",
          "form": "저는 집에 옷이 사이즈별로 다 있어요 ㅠ",
          "original_form": "저는 집에 옷이 사이즈별로 다 있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009499.1.21",
          "form": "살이 계속 쪄서 ㅠㅠ",
          "original_form": "살이 계속 쪄서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009499.1.22",
          "form": "저는 빅사이즈수집해요ㅋㅋㅋ",
          "original_form": "저는 빅사이즈수집해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009499.1.23",
          "form": "뺄 수 있다는 희망으로 작년 사이즈를 버리지 못하고",
          "original_form": "뺄 수 있다는 희망으로 작년 사이즈를 버리지 못하고",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009499.1.24",
          "form": "저두요ㅠ",
          "original_form": "저두요ㅠ",
          "speaker_id": "1",
          "time": "20210823 00:06"
        }
      ]
    }
  ]
