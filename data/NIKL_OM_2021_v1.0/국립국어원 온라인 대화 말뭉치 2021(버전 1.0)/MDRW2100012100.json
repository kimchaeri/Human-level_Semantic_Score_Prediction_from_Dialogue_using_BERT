      "utterance": [
        {
          "id": "MDRW2100012100.1.1",
          "form": "음식 뭐좋아해?",
          "original_form": "음식 뭐좋아해?",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012100.1.2",
          "form": "나 무말랭이",
          "original_form": "나 무말랭이",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012100.1.3",
          "form": "무말랭이? 난 별론데",
          "original_form": "무말랭이? 난 별론데",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012100.1.4",
          "form": "왜 좋아",
          "original_form": "왜 좋아",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012100.1.5",
          "form": "무말랭이 맛이써",
          "original_form": "무말랭이 맛이써",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012100.1.6",
          "form": "맛있어?",
          "original_form": "맛있어?",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012100.1.7",
          "form": "입맛이 특이하네",
          "original_form": "입맛이 특이하네",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012100.1.8",
          "form": "오늘 뭐먹었어?",
          "original_form": "오늘 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012100.1.9",
          "form": "나 치킨!",
          "original_form": "나 치킨!",
          "speaker_id": "2",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012100.1.10",
          "form": "무슨 치킨 먹었어?",
          "original_form": "무슨 치킨 먹었어?",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012100.1.11",
          "form": "근데 내가 입맛이 특이해?",
          "original_form": "근데 내가 입맛이 특이해?",
          "speaker_id": "2",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012100.1.12",
          "form": "아닌가 ㅇㅅㅇ",
          "original_form": "아닌가 ㅇㅅㅇ",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012100.1.13",
          "form": "난 무말랭이 시러",
          "original_form": "난 무말랭이 시러",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012100.1.14",
          "form": "무슨 치킨 먹엇어?",
          "original_form": "무슨 치킨 먹엇어?",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012100.1.15",
          "form": "그럼 치킨먹어",
          "original_form": "그럼 치킨먹어",
          "speaker_id": "2",
          "time": "20210824 19:12"
        },
