      "utterance": [
        {
          "id": "MDRW2100016621.1.1",
          "form": "오 어떻게 그러지",
          "original_form": "오 어떻게 그러지",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016621.1.2",
          "form": "아직도반페미이러는",
          "original_form": "아직도반페미이러는",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016621.1.3",
          "form": "여자분들도있잖아",
          "original_form": "여자분들도있잖아",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016621.1.4",
          "form": "아 맞지맞지",
          "original_form": "아 맞지맞지",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016621.1.5",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016621.1.6",
          "form": "근데요즘연애하고결혼하는사람은",
          "original_form": "근데요즘연애하고결혼하는사람은",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016621.1.7",
          "form": "신기하긴함",
          "original_form": "신기하긴함",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016621.1.8",
          "form": "엔번방때보고",
          "original_form": "엔번방때보고",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016621.1.9",
          "form": "깨닫는게",
          "original_form": "깨닫는게",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016621.1.10",
          "form": "없을까?",
          "original_form": "없을까?",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016621.1.11",
          "form": "엔번방도그렇게",
          "original_form": "엔번방도그렇게",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016621.1.12",
          "form": "고",
          "original_form": "고",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016621.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016621.1.14",
          "form": "너네학교에타도심함?",
          "original_form": "너네학교에타도심함?",
          "speaker_id": "1",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016621.1.15",
          "form": "우린 시사 토론 쪽이 심하고",
          "original_form": "우린 시사 토론 쪽이 심하고",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016621.1.16",
          "form": "우리 과에도 간간히 올라옴",
          "original_form": "우리 과에도 간간히 올라옴",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016621.1.17",
          "form": "시사토론게시판따로있음?",
          "original_form": "시사토론게시판따로있음?",
          "speaker_id": "1",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016621.1.18",
          "form": "ㅇㅇㅇ 따로 있음",
          "original_form": "ㅇㅇㅇ 따로 있음",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016621.1.19",
          "form": "전 학교에서는 없었어??",
          "original_form": "전 학교에서는 없었어??",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016621.1.20",
          "form": "ㅇㅇㅇ없어서",
          "original_form": "ㅇㅇㅇ없어서",
          "speaker_id": "1",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016621.1.21",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016621.1.22",
          "form": "실시간게시팜?",
          "original_form": "실시간게시팜?",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016621.1.23",
          "form": "추천많이받는거",
          "original_form": "추천많이받는거",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016621.1.24",
          "form": "그게시판에매일올라옴",
          "original_form": "그게시판에매일올라옴",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016621.1.25",
          "form": "아 진심",
          "original_form": "아 진심",
          "speaker_id": "2",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016621.1.26",
          "form": "차라리시사게가따로있는게",
          "original_form": "차라리시사게가따로있는게",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016621.1.27",
          "form": "나은거같기도하고",
          "original_form": "나은거같기도하고",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016621.1.28",
          "form": "좀 모어두는 효과는 있는 것 같음",
          "original_form": "좀 모어두는 효과는 있는 것 같음",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016621.1.29",
          "form": "ㅇㅇ인기글에 안 올라오는 것만으로도",
          "original_form": "ㅇㅇ인기글에 안 올라오는 것만으로도",
          "speaker_id": "1",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016621.1.30",
          "form": "근데 그 게시물이 인기글에 올라올 때가 있음",
          "original_form": "근데 그 게시물이 인기글에 올라올 때가 있음",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016621.1.31",
          "form": "그거안막았어?",
          "original_form": "그거안막았어?",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016621.1.32",
          "form": "막을 수도 있음?",
          "original_form": "막을 수도 있음?",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016621.1.33",
          "form": "지금학교는추천많아도인기글안올라오던데",
          "original_form": "지금학교는추천많아도인기글안올라오던데",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016621.1.34",
          "form": "모르겠음",
          "original_form": "모르겠음",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016621.1.35",
          "form": "있는줄",
          "original_form": "있는줄",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016621.1.36",
          "form": "오메",
          "original_form": "오메",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016621.1.37",
          "form": "내가잘못본건가",
          "original_form": "내가잘못본건가",
          "speaker_id": "1",
          "time": "20210826 14:17"
        }
      ]
    }
  ]
