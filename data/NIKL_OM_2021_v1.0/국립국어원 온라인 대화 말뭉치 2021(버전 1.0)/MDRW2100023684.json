      "utterance": [
        {
          "id": "MDRW2100023684.1.1",
          "form": "이번에 여름 휴가",
          "original_form": "이번에 여름 휴가",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023684.1.2",
          "form": "다녀오셨어요?",
          "original_form": "다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023684.1.3",
          "form": "이번에 경주월드 다녀왔어요!",
          "original_form": "이번에 경주월드 다녀왔어요!",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023684.1.4",
          "form": "시국이 시국인지라 조심스럽긴 했죠..ㅠㅜ",
          "original_form": "시국이 시국인지라 조심스럽긴 했죠..ㅠㅜ",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023684.1.5",
          "form": "와! 저 경주 좋아하는데",
          "original_form": "와! 저 경주 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023684.1.6",
          "form": "거기는 한번도 안가봤거든요ㅜ",
          "original_form": "거기는 한번도 안가봤거든요ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023684.1.7",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023684.1.8",
          "form": "에버랜드나 롯데월드에 비교하면 작긴 한데 탈 거리는 정말 많더라고요!",
          "original_form": "에버랜드나 롯데월드에 비교하면 작긴 한데 탈 거리는 정말 많더라고요!",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100023684.1.9",
          "form": "오히려 넓지 않아서 자유이용권 제대로 활용하고 온 것 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "오히려 넓지 않아서 자유이용권 제대로 활용하고 온 것 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100023684.1.10",
          "form": "경주를 가야할 이유가 또 늘었네요ㅋㅋㅋㅋ",
          "original_form": "경주를 가야할 이유가 또 늘었네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 06:13"
        },
        {
          "id": "MDRW2100023684.1.11",
          "form": "또 어디가셨어요?!",
          "original_form": "또 어디가셨어요?!",
          "speaker_id": "1",
          "time": "20210901 06:14"
        },
        {
          "id": "MDRW2100023684.1.12",
          "form": "이번에는 경주밖에 못 가봤어요ㅠㅠ",
          "original_form": "이번에는 경주밖에 못 가봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100023684.1.13",
          "form": "혹시 어디 가신 곳 있으신가요?",
          "original_form": "혹시 어디 가신 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100023684.1.14",
          "form": "전 올해는 패스예요ㅜ",
          "original_form": "전 올해는 패스예요ㅜ",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100023684.1.15",
          "form": "백신 맞기 전이라",
          "original_form": "백신 맞기 전이라",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100023684.1.16",
          "form": "주변 눈치도 보여서요",
          "original_form": "주변 눈치도 보여서요",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100023684.1.17",
          "form": "아 그렇죠ㅠㅠ",
          "original_form": "아 그렇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023684.1.18",
          "form": "아까 티비보는데 경주월드 나오더라고요!",
          "original_form": "아까 티비보는데 경주월드 나오더라고요!",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100023684.1.19",
          "form": "드라켄? 놀이기구 타는거 봤는데ㅋㅋㅋㅋ",
          "original_form": "드라켄? 놀이기구 타는거 봤는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100023684.1.20",
          "form": "혹시 그것도 타셨나요!",
          "original_form": "혹시 그것도 타셨나요!",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100023684.1.21",
          "form": "드라켄 재밌었어요ㅠㅠ",
          "original_form": "드라켄 재밌었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100023684.1.22",
          "form": "전 그게 제일 재밌었던 것 같아여",
          "original_form": "전 그게 제일 재밌었던 것 같아여",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100023684.1.23",
          "form": "후룸라이드랑!",
          "original_form": "후룸라이드랑!",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100023684.1.24",
          "form": "와!!! 후룸라이드도 있어요?!",
          "original_form": "와!!! 후룸라이드도 있어요?!",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100023684.1.25",
          "form": "진짜 가보고싶다",
          "original_form": "진짜 가보고싶다",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100023684.1.26",
          "form": "의외로 재밌는 게 좀 있더라고요ㅋㅋㅋㅋㅋ",
          "original_form": "의외로 재밌는 게 좀 있더라고요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100023684.1.27",
          "form": "혹시 가시게 되면 꼭 할인받아서 가세요! 제 값주고 가기엔... 돈 아까울 것 같기도 해요ㅎㅎ",
          "original_form": "혹시 가시게 되면 꼭 할인받아서 가세요! 제 값주고 가기엔... 돈 아까울 것 같기도 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:12"
        }
      ]
    }
