      "utterance": [
        {
          "id": "MDRW2100035278.1.1",
          "form": "파리 여행은 2-300 정도 드나용 ??",
          "original_form": "파리 여행은 2-300 정도 드나용 ??",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100035278.1.2",
          "form": "비행기 표값이 200정도 든 거 같아요!",
          "original_form": "비행기 표값이 200정도 든 거 같아요!",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100035278.1.3",
          "form": "저는 성수기 약간 걸쳐서 갔거든요ㅠ",
          "original_form": "저는 성수기 약간 걸쳐서 갔거든요ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100035278.1.4",
          "form": "헐 표값부터 비싸군요 ㅠㅠㅠ",
          "original_form": "헐 표값부터 비싸군요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100035278.1.5",
          "form": "보통 성수기가 언제에요?",
          "original_form": "보통 성수기가 언제에요?",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100035278.1.6",
          "form": "여행가기 딱 좋은 7-8월이랑 연말이요!\n우리나라 공항 기준이니까요ㅠㅠ ㅎㅎ",
          "original_form": "여행가기 딱 좋은 7-8월이랑 연말이요!\n우리나라 공항 기준이니까요ㅠㅠ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035278.1.7",
          "form": "아 딱 우리나라랑 똑같군요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "아 딱 우리나라랑 똑같군요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100035278.1.8",
          "form": "어떠셨어요 ???!",
          "original_form": "어떠셨어요 ???!",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100035278.1.9",
          "form": "디즈니랜드만 좋았고, 파리 자체는 별로였어요 ㅎㅎ",
          "original_form": "디즈니랜드만 좋았고, 파리 자체는 별로였어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100035278.1.10",
          "form": "헐 진짜요 ?? ㅜㅜ 어떤점이요 ??",
          "original_form": "헐 진짜요 ?? ㅜㅜ 어떤점이요 ??",
          "speaker_id": "1",
