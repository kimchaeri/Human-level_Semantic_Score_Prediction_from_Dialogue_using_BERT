        {
          "id": "MDRW2100005402.1.1",
          "form": "학교생활하니까",
          "original_form": "학교생활하니까",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005402.1.2",
          "form": "급식생각나네",
          "original_form": "급식생각나네",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005402.1.3",
          "form": "급식먹을때가 좋았음...",
          "original_form": "급식먹을때가 좋았음...",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005402.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ급식이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ급식이",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005402.1.5",
          "form": "알아서 영양성분 맞춰서주니까",
          "original_form": "알아서 영양성분 맞춰서주니까",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005402.1.6",
          "form": "편하긴 편햐",
          "original_form": "편하긴 편햐",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005402.1.7",
          "form": "ㅁㅈㅁㅈ 그리고 내가 뭐먹을지 고민 안해도 돼서",
          "original_form": "ㅁㅈㅁㅈ 그리고 내가 뭐먹을지 고민 안해도 돼서",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005402.1.8",
          "form": "편한게 있었어",
          "original_form": "편한게 있었어",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005402.1.9",
          "form": "맞아 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005402.1.10",
          "form": "그리고 가격도 싸고....",
          "original_form": "그리고 가격도 싸고....",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005402.1.11",
          "form": "급식이 진짜 좋기는 조아...",
          "original_form": "급식이 진짜 좋기는 조아...",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005402.1.12",
          "form": "맨날 급식 오늘은 뭐나오는지",
          "original_form": "맨날 급식 오늘은 뭐나오는지",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005402.1.13",
          "form": "메뉴 체크하는 낙으로",
          "original_form": "메뉴 체크하는 낙으로",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005402.1.14",
          "form": "학교 다니는 거지 뭐",
          "original_form": "학교 다니는 거지 뭐",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005402.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아맞아",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005402.1.16",
          "form": "유일한 낙이 급식시간 아니겄오",
          "original_form": "유일한 낙이 급식시간 아니겄오",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005402.1.17",
          "form": "근데 우리 학교는",
          "original_form": "근데 우리 학교는",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005402.1.18",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.19",
          "form": "아니... 학  니언이 나쁜말이면",
          "original_form": "아니... 학  니언이 나쁜말이면",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.20",
          "form": "학교얘길 어케해",
          "original_form": "학교얘길 어케해",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.21",
          "form": "이걸 거르네",
          "original_form": "이걸 거르네",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.23",
          "form": "어이없다 진짜",
          "original_form": "어이없다 진짜",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.24",
          "form": "니언..이 나와서",
          "original_form": "니언..이 나와서",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.25",
          "form": "그걸 거르나바",
          "original_form": "그걸 거르나바",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.26",
          "form": "어이가없따...",
          "original_form": "어이가없따...",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005402.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005402.1.28",
          "form": "암튼 학니연은",
          "original_form": "암튼 학니연은",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005402.1.29",
          "form": "왜 말씀하신건가용",
          "original_form": "왜 말씀하신건가용",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005402.1.30",
          "form": "학니언이 올라갈때마다 건물을 다르게 썼잖아",
          "original_form": "학니언이 올라갈때마다 건물을 다르게 썼잖아",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005402.1.31",
          "form": "그래서 1학년땐",
          "original_form": "그래서 1학년땐",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005402.1.32",
          "form": "1...학니언땐...",
          "original_form": "1...학니언땐...",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005402.1.33",
          "form": "급식실에서ㅠ제일 멀어서",
          "original_form": "급식실에서ㅠ제일 멀어서",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005402.1.34",
          "form": "언덕 두개를 넘어야했고...",
          "original_form": "언덕 두개를 넘어야했고...",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005402.1.35",
          "form": "힘들었는데",
          "original_form": "힘들었는데",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005402.1.36",
          "form": "고3때는",
          "original_form": "고3때는",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005402.1.37",
          "form": "그렇게 힘겹게 뛰어오는 고1 고2들 보면서",
          "original_form": "그렇게 힘겹게 뛰어오는 고1 고2들 보면서",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005402.1.38",
          "form": "여유롭게 밥먹으러가던 기억이나섴ㅋㅋㅋ",
          "original_form": "여유롭게 밥먹으러가던 기억이나섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005402.1.39",
          "form": "마자마자 ㅋㅋㅋ",
          "original_form": "마자마자 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005402.1.40",
          "form": "젤 가까웠어서",
          "original_form": "젤 가까웠어서",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005402.1.41",
          "form": "고삼 건물이",
          "original_form": "고삼 건물이",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005402.1.42",
          "form": "급식실이랑ㅋㅋ",
          "original_form": "급식실이랑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005402.1.43",
          "form": "고삼때는 에어컨도",
          "original_form": "고삼때는 에어컨도",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005402.1.44",
          "form": "빵빵하게 틀어줘서",
          "original_form": "빵빵하게 틀어줘서",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005402.1.45",
          "form": "여름에 담요덮고 다니면서",
          "original_form": "여름에 담요덮고 다니면서",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005402.1.46",
          "form": "더워하는 고1 고2들을",
          "original_form": "더워하는 고1 고2들을",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005402.1.47",
          "form": "관망하는 낙이있었지",
          "original_form": "관망하는 낙이있었지",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005402.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아",
