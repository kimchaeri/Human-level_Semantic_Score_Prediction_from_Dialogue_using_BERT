      "utterance": [
        {
          "id": "MDRW2100001002.1.1",
          "form": "넌 요즘 어떤 책 읽어?",
          "original_form": "넌 요즘 어떤 책 읽어?",
          "speaker_id": "1",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100001002.1.2",
          "form": "메이즈러너 다시 읽어보고 있어",
          "original_form": "메이즈러너 다시 읽어보고 있어",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100001002.1.3",
          "form": "메이즈러너 오랜만이다",
          "original_form": "메이즈러너 오랜만이다",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100001002.1.4",
          "form": "고등학생때 재미있게 읽었었는데",
          "original_form": "고등학생때 재미있게 읽었었는데",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100001002.1.5",
          "form": "나도 고등학교때 이후로 책을 안읽었는데 뒷권이 나왓더라구",
          "original_form": "나도 고등학교때 이후로 책을 안읽었는데 뒷권이 나왓더라구",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100001002.1.6",
          "form": "아 정말? 새로 나온지 몰랐네",
          "original_form": "아 정말? 새로 나온지 몰랐네",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100001002.1.7",
          "form": "새로 나온건 이름이 뭔데?",
          "original_form": "새로 나온건 이름이 뭔데?",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100001002.1.8",
          "form": "나온지는 좀 됐는데 킬오더랑 피버코드가 있더라고",
          "original_form": "나온지는 좀 됐는데 킬오더랑 피버코드가 있더라고",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100001002.1.9",
          "form": "오 처음들어본다",
          "original_form": "오 처음들어본다",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100001002.1.10",
          "form": "나도 안읽은 책이네",
          "original_form": "나도 안읽은 책이네",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100001002.1.11",
          "form": "너도 한번 사서 읽어보는거 어때",
          "original_form": "너도 한번 사서 읽어보는거 어때",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100001002.1.12",
          "form": "도서관 가서 찾아봐야겠어",
          "original_form": "도서관 가서 찾아봐야겠어",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100001002.1.13",
          "form": "요즘 소설 사는 돈이 아깝더라고 ㅜㅜ",
          "original_form": "요즘 소설 사는 돈이 아깝더라고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100001002.1.14",
          "form": "맞아.. 종이책은 무겁기도 하고 관리도 힘들구",
          "original_form": "맞아.. 종이책은 무겁기도 하고 관리도 힘들구",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100001002.1.15",
          "form": "이북리더기 사서 그런가",
          "original_form": "이북리더기 사서 그런가",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100001002.1.16",
          "form": "전자책이 더 좋아",
          "original_form": "전자책이 더 좋아",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100001002.1.17",
          "form": "나도 전자책 종류가 보기 편하더라구 그래서 요즘 웹소설이나 전자책 자주봐",
          "original_form": "나도 전자책 종류가 보기 편하더라구 그래서 요즘 웹소설이나 전자책 자주봐",
          "speaker_id": "2",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100001002.1.18",
          "form": "웹소설 재미있지",
          "original_form": "웹소설 재미있지",
          "speaker_id": "1",
          "time": "20210820 11:44"
        }
