        },
        {
          "id": "MDRW2100035610.1.6",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035610.1.7",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035610.1.8",
          "form": "아니요\r\nname1님은요?",
          "original_form": "아니요\r\n&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035610.1.9",
          "form": "저도 안키워요ㅠㅠ",
          "original_form": "저도 안키워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035610.1.10",
          "form": "동물은 어떤 동물 좋아하세요?",
          "original_form": "동물은 어떤 동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035610.1.11",
          "form": "저는 고양이나 다람쥐요",
          "original_form": "저는 고양이나 다람쥐요",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035610.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035610.1.13",
          "form": "전 다람쥐나 토끼요!\r\n강아지나 고양이도 좋아하구요\r\n보통 귀여운 동물들을 좋아해요",
          "original_form": "전 다람쥐나 토끼요!\r\n강아지나 고양이도 좋아하구요\r\n보통 귀여운 동물들을 좋아해요",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035610.1.14",
          "form": "그렇군요 그럼 반려동물로 키워보고 깊은 동물이 있으신가요?",
          "original_form": "그렇군요 그럼 반려동물로 키워보고 깊은 동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035610.1.15",
          "form": "음..반려동물 키우는건 생각해보지 않았어요\r\n아직 반려동물을 책임지고 키울 준비가 되어있지도 않고, 제가 동물을 귀여워하고 예뻐하면서도 한편으로는 겁내고 무서워해서요ㅜㅜ",
          "original_form": "음..반려동물 키우는건 생각해보지 않았어요\r\n아직 반려동물을 책임지고 키울 준비가 되어있지도 않고, 제가 동물을 귀여워하고 예뻐하면서도 한편으로는 겁내고 무서워해서요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035610.1.16",
          "form": "맞아요 사실 저도 동물들 귀여워한다고 하지만 막상 길가다가 보면 스스로도 모르게 두려워할 때가 많죠ㅠㅠ",
          "original_form": "맞아요 사실 저도 동물들 귀여워한다고 하지만 막상 길가다가 보면 스스로도 모르게 두려워할 때가 많죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100035610.1.17",
          "form": "맞아요 길에서 마주치면 무서울때가 있어요",
          "original_form": "맞아요 길에서 마주치면 무서울때가 있어요",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035610.1.18",
          "form": "사람을 보고 도망가거나 관심없는 동물들이면 괜찮은데",
          "original_form": "사람을 보고 도망가거나 관심없는 동물들이면 괜찮은데",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035610.1.19",
          "form": "예전에 한 번 검은 강아지가 갑자기 막 쫓아오는데 너무 무서웠어요ㅜㅜ",
          "original_form": "예전에 한 번 검은 강아지가 갑자기 막 쫓아오는데 너무 무서웠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100035610.1.20",
          "form": "맞아요 사실 저는 밤에 고양이나 강아지가 갑자기 짖으면 엄청 놀라요",
          "original_form": "맞아요 사실 저는 밤에 고양이나 강아지가 갑자기 짖으면 엄청 놀라요",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100035610.1.21",
          "form": "그쵸 늦은 밤에 갑자기 소리 들려오면 깜짝 놀라게 되더라구요 \r\n특히 여러 마리가 짖는다면..",
          "original_form": "그쵸 늦은 밤에 갑자기 소리 들려오면 깜짝 놀라게 되더라구요 \r\n특히 여러 마리가 짖는다면..",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100035610.1.22",
          "form": "그러니까요 마음과 몸이 다르네요ㅠㅠ",
          "original_form": "그러니까요 마음과 몸이 다르네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:38"
        }
      ]
