      "utterance": [
        {
          "id": "MDRW2100018726.1.1",
          "form": "원래 name3 친구 포함 세 명이서 놀거든",
          "original_form": "원래 &name3& 친구 포함 세 명이서 놀거든",
          "speaker_id": "2",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018726.1.2",
          "form": "세 명이서 서로 집을 왕래하는데",
          "original_form": "세 명이서 서로 집을 왕래하는데",
          "speaker_id": "2",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018726.1.3",
          "form": "같이하원하는 남자애가 또 있어",
          "original_form": "같이하원하는 남자애가 또 있어",
          "speaker_id": "2",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018726.1.4",
          "form": "근데 걔가 엄청 산만해",
          "original_form": "근데 걔가 엄청 산만해",
          "speaker_id": "2",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018726.1.5",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "1",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.6",
          "form": "그래서 걔는 보통 빼고 노는데",
          "original_form": "그래서 걔는 보통 빼고 노는데",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.7",
          "form": "우리끼리 집으로 가면",
          "original_form": "우리끼리 집으로 가면",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.8",
          "form": "걔도 맨날 오고 싶어서 따라 온단 말이야",
          "original_form": "걔도 맨날 오고 싶어서 따라 온단 말이야",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.9",
          "form": "가고싶어하죠ㅠ",
          "original_form": "가고싶어하죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.10",
          "form": "걔 엄마도 알거 아니야",
          "original_form": "걔 엄마도 알거 아니야",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.11",
          "form": "우리끼리 집에 가서 노는 지",
          "original_form": "우리끼리 집에 가서 노는 지",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.12",
          "form": "엊그제도 같이 노는 남자의 집에서 name3랑 둘이 놀았는데",
          "original_form": "엊그제도 같이 노는 남자의 집에서 &name3&랑 둘이 놀았는데",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.13",
          "form": "신고하는거 아님??ㅋㅋㅋ",
          "original_form": "신고하는거 아님??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.14",
          "form": "그때도 같이 오고 싶어서 난리",
          "original_form": "그때도 같이 오고 싶어서 난리",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.15",
          "form": "근데 어제 딱 집에 오는데 마주쳐가지고",
          "original_form": "근데 어제 딱 집에 오는데 마주쳐가지고",
          "speaker_id": "2",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018726.1.16",
          "form": "우리랑 놀이터에서 놀거라고 오는 거 뻔히 아는데",
          "original_form": "우리랑 놀이터에서 놀거라고 오는 거 뻔히 아는데",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.17",
          "form": "쫌 미안한 거야",
          "original_form": "쫌 미안한 거야",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.18",
          "form": "그래서 같이 놀자고 집으로 데려갔는데",
          "original_form": "그래서 같이 놀자고 집으로 데려갔는데",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.19",
          "form": "에고ㅠㅠ 애들이 같이 하원하니",
          "original_form": "에고ㅠㅠ 애들이 같이 하원하니",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.20",
          "form": "아 다신 안 데려와야지 생각했다",
          "original_form": "아 다신 안 데려와야지 생각했다",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.21",
          "form": "계속 싸움",
          "original_form": "계속 싸움",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.22",
          "form": "ㅋㅋㅋ그엄마도 느꼈을듯요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그엄마도 느꼈을듯요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.23",
          "form": "놀릴려고 데려가는건데~",
          "original_form": "놀릴려고 데려가는건데~",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.24",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.25",
          "form": "1시간 정도 놀았는데",
          "original_form": "1시간 정도 놀았는데",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.26",
          "form": "싸우면 같이 안노는게 맞죠잉~",
          "original_form": "싸우면 같이 안노는게 맞죠잉~",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.27",
          "form": "치 쳤다",
          "original_form": "치 쳤다",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018726.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018726.1.29",
          "form": "가고 나서 바로 name4한테  이제 친구 집에 데려오는 건 없다고 얘기했더니",
          "original_form": "가고 나서 바로 &name4&한테  이제 친구 집에 데려오는 건 없다고 얘기했더니",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018726.1.30",
          "form": "아니 name3한테",
          "original_form": "아니 &name3&한테",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018726.1.31",
          "form": "우리 원이 하는 말",
          "original_form": "우리 원이 하는 말",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018726.1.32",
          "form": "캐도 1시간 놀고 갔음 착하네요 ㅋㅋㅋㅋ",
          "original_form": "캐도 1시간 놀고 갔음 착하네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018726.1.33",
          "form": "엄마 갈 때 분명히 다음에 또 와라고 얘기했잖아라고 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "엄마 갈 때 분명히 다음에 또 와라고 얘기했잖아라고 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018726.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ엄마가 잘못햤네여 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ엄마가 잘못햤네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018726.1.35",
          "form": "그냥 잘가만 했어야지 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 잘가만 했어야지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018726.1.36",
          "form": "아 그건 그냥 인사치레였어 ㅋㅋㅋ",
          "original_form": "아 그건 그냥 인사치레였어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018726.1.37",
          "form": "가는 친구한테",
          "original_form": "가는 친구한테",
          "speaker_id": "2",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018726.1.38",
