      "utterance": [
        {
          "id": "MDRW2100015085.1.1",
          "form": "당근케이크 땡기네요",
          "original_form": "당근케이크 땡기네요",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015085.1.2",
          "form": "당근케이크는고소한맛으로먹나요",
          "original_form": "당근케이크는고소한맛으로먹나요",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015085.1.3",
          "form": "저는잘안먹어봐서",
          "original_form": "저는잘안먹어봐서",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015085.1.4",
          "form": "네 그리고 당근이 씹히는데 증말 맛있어요",
          "original_form": "네 그리고 당근이 씹히는데 증말 맛있어요",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015085.1.5",
          "form": "저는반당근파라",
          "original_form": "저는반당근파라",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015085.1.6",
          "form": "저는 티라미수케이크조아해요",
          "original_form": "저는 티라미수케이크조아해요",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015085.1.7",
          "form": "그럼 당근마켓도 이용 안하시나요?",
          "original_form": "그럼 당근마켓도 이용 안하시나요?",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015085.1.8",
          "form": "당근마켓에 티라미수도파나요",
          "original_form": "당근마켓에 티라미수도파나요",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015085.1.9",
          "form": "간혹 떨이로 팝니다",
          "original_form": "간혹 떨이로 팝니다",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015085.1.10",
          "form": "참좋은곳이네요",
          "original_form": "참좋은곳이네요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015085.1.11",
          "form": "케이크안먹은지오래되었어요",
          "original_form": "케이크안먹은지오래되었어요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015085.1.12",
          "form": "저도 케이크는 녹차,딸기말곤 다 좋아하는데",
          "original_form": "저도 케이크는 녹차,딸기말곤 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015085.1.13",
          "form": "마지막 케이크가 전남친생일날 먹은게 다라",
          "original_form": "마지막 케이크가 전남친생일날 먹은게 다라",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.14",
          "form": "기분이 더럽네요",
          "original_form": "기분이 더럽네요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.15",
          "form": "생크림케이크안먹은지도오래되었어요",
          "original_form": "생크림케이크안먹은지도오래되었어요",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.16",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.17",
          "form": "그러셧구나",
          "original_form": "그러셧구나",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.18",
          "form": "근데케이크가드시고싶으시구나",
          "original_form": "근데케이크가드시고싶으시구나",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.19",
          "form": "당근케이크를 혼자 다 먹으면서 새로운 추억을 만들어야죠",
          "original_form": "당근케이크를 혼자 다 먹으면서 새로운 추억을 만들어야죠",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.20",
          "form": "얼른드시고잊으세요",
          "original_form": "얼른드시고잊으세요",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.21",
          "form": "한입에 먹어볼까 싶습니다",
          "original_form": "한입에 먹어볼까 싶습니다",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.22",
          "form": "감사해오",
          "original_form": "감사해오",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015085.1.23",
          "form": "케이크만들어보셧나요",
          "original_form": "케이크만들어보셧나요",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015085.1.24",
          "form": "저는케이크만들고싶어요",
          "original_form": "저는케이크만들고싶어요",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015085.1.25",
          "form": "아 것도 전남친이랑 만들러갔는데",
          "original_form": "아 것도 전남친이랑 만들러갔는데",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015085.1.26",
          "form": "죄송해오",
          "original_form": "죄송해오",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015085.1.27",
          "form": "초면에 죄송하지만 사드세요",
          "original_form": "초면에 죄송하지만 사드세요",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015085.1.28",
          "form": "맛없습니다",
          "original_form": "맛없습니다",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015085.1.29",
          "form": "제가실례를",
          "original_form": "제가실례를",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015085.1.30",
          "form": "인간적으로 케이크 너무 비싸요",
