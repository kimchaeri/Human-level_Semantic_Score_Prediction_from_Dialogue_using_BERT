      "utterance": [
        {
          "id": "MDRW2100033615.1.1",
          "form": "드뎌 오늘 도착했어요",
          "original_form": "드뎌 오늘 도착했어요",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033615.1.2",
          "form": "워치4 사전예약한거가 왔네요",
          "original_form": "워치4 사전예약한거가 왔네요",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033615.1.3",
          "form": "뭐가도착했어요?",
          "original_form": "뭐가도착했어요?",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033615.1.4",
          "form": "오ㅎ 워치사셨어요?",
          "original_form": "오ㅎ 워치사셨어요?",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033615.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100033615.1.6",
          "form": "액티브2 워치사려다가",
          "original_form": "액티브2 워치사려다가",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033615.1.7",
          "form": "이번에 신제품인데 사전예약하니 할인이 많이되더라근요",
          "original_form": "이번에 신제품인데 사전예약하니 할인이 많이되더라근요",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033615.1.8",
          "form": "그래서 질렀네요",
          "original_form": "그래서 질렀네요",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033615.1.9",
          "form": "얼마주고요?",
          "original_form": "얼마주고요?",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033615.1.10",
          "form": "16만원정도",
          "original_form": "16만원정도",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033615.1.11",
          "form": "사은품도 줘요?",
          "original_form": "사은품도 줘요?",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033615.1.12",
          "form": "사은품은 없어요",
          "original_form": "사은품은 없어요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033615.1.13",
          "form": "딱 시계만오는군요",
          "original_form": "딱 시계만오는군요",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033615.1.14",
          "form": "그건 플립3 정도사야줄듯요 ㅎㅎ",
          "original_form": "그건 플립3 정도사야줄듯요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033615.1.15",
          "form": "11번가에서 샀는데 쿠폰주더라구요",
          "original_form": "11번가에서 샀는데 쿠폰주더라구요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033615.1.16",
          "form": "액세사리 쿠폰",
          "original_form": "액세사리 쿠폰",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100033615.1.17",
          "form": "시계로 문자도 보내지나요?",
          "original_form": "시계로 문자도 보내지나요?",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033615.1.18",
          "form": "9만원할인에 오케이 카드할인까지 받는거니",
          "original_form": "9만원할인에 오케이 카드할인까지 받는거니",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033615.1.19",
          "form": "사은품 안받아도 되요 ㅎ",
          "original_form": "사은품 안받아도 되요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033615.1.20",
          "form": "아직 개봉안해봤어요 ㅎ",
          "original_form": "아직 개봉안해봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033615.1.21",
          "form": "원래비싸군요ㅎ",
          "original_form": "원래비싸군요ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033615.1.22",
          "form": "할인 많이받으셨네요",
          "original_form": "할인 많이받으셨네요",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033615.1.23",
          "form": "동생이 40넘게준걸로 알아요",
          "original_form": "동생이 40넘게준걸로 알아요",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033615.1.24",
          "form": "워치3",
          "original_form": "워치3",
