      "utterance": [
        {
          "id": "MDRW2100040807.1.1",
          "form": "이번 추석때 좀 모이나요?",
          "original_form": "이번 추석때 좀 모이나요?",
          "speaker_id": "1",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100040807.1.2",
          "form": "아 할머니댁이 근처이긴 한데",
          "original_form": "아 할머니댁이 근처이긴 한데",
          "speaker_id": "2",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100040807.1.3",
          "form": "인원제한이 있어서 다같이 모이기는 힘들 것같아요ㅜ",
          "original_form": "인원제한이 있어서 다같이 모이기는 힘들 것같아요ㅜ",
          "speaker_id": "2",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100040807.1.4",
          "form": "근처에 계시는구나",
          "original_form": "근처에 계시는구나",
          "speaker_id": "1",
          "time": "20210905 08:03"
        },
        {
          "id": "MDRW2100040807.1.5",
          "form": "맞아요.. 저희도 이번 역시 작년처럼 그냥 가족끼리만 있을 듯해요",
          "original_form": "맞아요.. 저희도 이번 역시 작년처럼 그냥 가족끼리만 있을 듯해요",
          "speaker_id": "1",
          "time": "20210905 08:03"
        },
        {
          "id": "MDRW2100040807.1.6",
          "form": "네네 아무래도 코로나가 심하니ㅠ",
          "original_form": "네네 아무래도 코로나가 심하니ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100040807.1.7",
          "form": "친척들 못만난지도 오래네요",
          "original_form": "친척들 못만난지도 오래네요",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100040807.1.8",
          "form": "친척들과 모여서 같이 밥먹고 얘기나누던 때가 그리워요",
          "original_form": "친척들과 모여서 같이 밥먹고 얘기나누던 때가 그리워요",
          "speaker_id": "2",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040807.1.9",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040807.1.10",
          "form": "작년부터 올해까지 이렇게 오래 갈줄 몰랐어요",
          "original_form": "작년부터 올해까지 이렇게 오래 갈줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040807.1.11",
          "form": "네 가족들 만나는 것도 힘드니",
          "original_form": "네 가족들 만나는 것도 힘드니",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100040807.1.12",
          "form": "속상하네요",
          "original_form": "속상하네요",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100040807.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040807.1.14",
          "form": "동거 가족 아니면 만나기도 힘들고",
          "original_form": "동거 가족 아니면 만나기도 힘들고",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040807.1.15",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040807.1.16",
          "form": "내일부터는 좀 완화가 된다고 해서 그나마 다행이에요",
          "original_form": "내일부터는 좀 완화가 된다고 해서 그나마 다행이에요",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040807.1.17",
          "form": "전 수도권 사는데",
          "original_form": "전 수도권 사는데",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040807.1.18",
          "form": "그래도 추석에 친척들 모일 정도는 아니니까",
          "original_form": "그래도 추석에 친척들 모일 정도는 아니니까",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040807.1.19",
          "form": "다른 지역에 사는 가족들 오라고 하기도 찾아가기도 그렇더라고요",
          "original_form": "다른 지역에 사는 가족들 오라고 하기도 찾아가기도 그렇더라고요",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100040807.1.20",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040807.1.21",
          "form": "어느정도 백신 완료 되면 그때는 좀 괜찮을 것도 같고",
          "original_form": "어느정도 백신 완료 되면 그때는 좀 괜찮을 것도 같고",
          "speaker_id": "1",
          "time": "20210905 09:24"
        }
      ]
