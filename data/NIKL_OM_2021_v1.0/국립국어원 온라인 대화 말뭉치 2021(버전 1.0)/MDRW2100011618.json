      "utterance": [
        {
          "id": "MDRW2100011618.1.1",
          "form": "요새 뭐보세요? ㅎㅎ",
          "original_form": "요새 뭐보세요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011618.1.2",
          "form": "유튜브 자주봅니다",
          "original_form": "유튜브 자주봅니다",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011618.1.3",
          "form": "구독하시는 채널 같은거 있으신가요",
          "original_form": "구독하시는 채널 같은거 있으신가요",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100011618.1.4",
          "form": "빠니보틀 여행유튜브요!",
          "original_form": "빠니보틀 여행유튜브요!",
          "speaker_id": "1",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100011618.1.5",
          "form": "오 지금 시국에 해외신가요 그분은?",
          "original_form": "오 지금 시국에 해외신가요 그분은?",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011618.1.6",
          "form": "지금 미국 가있어요ㅋㅋㅋ",
          "original_form": "지금 미국 가있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011618.1.7",
          "form": "오 백신 맞고 가신건가",
          "original_form": "오 백신 맞고 가신건가",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011618.1.8",
          "form": "저 그런 종류는 한번도 본적없어서 좀신기해요",
          "original_form": "저 그런 종류는 한번도 본적없어서 좀신기해요",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100011618.1.9",
          "form": "대리만족 중입니다ㅎㅎ",
          "original_form": "대리만족 중입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100011618.1.10",
          "form": "미국사람들은 마스크 잘 안쓴다던데",
          "original_form": "미국사람들은 마스크 잘 안쓴다던데",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100011618.1.11",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100011618.1.12",
          "form": "와 진짜 생생한 미국모습을 볼수있을듯",
          "original_form": "와 진짜 생생한 미국모습을 볼수있을듯",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100011618.1.13",
          "form": "쓰긴 쓰더라구요 재밌어요ㅎㅎ",
          "original_form": "쓰긴 쓰더라구요 재밌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100011618.1.14",
          "form": "영상미랄까 진심 보는재미가 있겠어요",
          "original_form": "영상미랄까 진심 보는재미가 있겠어요",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100011618.1.15",
          "form": "혹시 csi아세요?",
          "original_form": "혹시 csi아세요?",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100011618.1.16",
          "form": "미드인가요?",
          "original_form": "미드인가요?",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100011618.1.17",
          "form": "네네 과학수사대라고 보심될듯요",
          "original_form": "네네 과학수사대라고 보심될듯요",
          "speaker_id": "2",
          "time": "20210824 18:53"
        },
        {
          "id": "MDRW2100011618.1.18",
          "form": "왜 같은 바다라도 거기 바다는 더 좋아보이던지 ㅋㅋ",
          "original_form": "왜 같은 바다라도 거기 바다는 더 좋아보이던지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:54"
        }
