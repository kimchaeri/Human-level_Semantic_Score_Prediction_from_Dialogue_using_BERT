      "utterance": [
        {
          "id": "MDRW2100030687.1.1",
          "form": "여행인가요",
          "original_form": "여행인가요",
          "speaker_id": "2",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.2",
          "form": "베트남에 안 와서 섭섭하네요",
          "original_form": "베트남에 안 와서 섭섭하네요",
          "speaker_id": "1",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.3",
          "form": "단풍 보러 놀러 가고 싶네요",
          "original_form": "단풍 보러 놀러 가고 싶네요",
          "speaker_id": "2",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.4",
          "form": "여행하기에는 좋죠",
          "original_form": "여행하기에는 좋죠",
          "speaker_id": "1",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.5",
          "form": "1년에 휴가가 많이 없어서 가기 힘들어요",
          "original_form": "1년에 휴가가 많이 없어서 가기 힘들어요",
          "speaker_id": "2",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.6",
          "form": "먹방 찍기에 딱 좋은 도시예요",
          "original_form": "먹방 찍기에 딱 좋은 도시예요",
          "speaker_id": "1",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.7",
          "form": "먹방 여행 좋지요",
          "original_form": "먹방 여행 좋지요",
          "speaker_id": "2",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.8",
          "form": "쌀국수가 정말 맛있거든요",
          "original_form": "쌀국수가 정말 맛있거든요",
          "speaker_id": "1",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100030687.1.9",
          "form": "식 재료가 참 다양했어요",
          "original_form": "식 재료가 참 다양했어요",
          "speaker_id": "1",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.10",
          "form": "재작년인가 더 전인가 부모랑 경주에 가서 한복 빌려 입고 사진 찍고 다녔어요",
          "original_form": "재작년인가 더 전인가 부모랑 경주에 가서 한복 빌려 입고 사진 찍고 다녔어요",
          "speaker_id": "2",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.11",
          "form": "한국에는 없는게 많았어요",
          "original_form": "한국에는 없는게 많았어요",
          "speaker_id": "1",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.12",
          "form": "해산물도 풍부하다면 서요",
          "original_form": "해산물도 풍부하다면 서요",
          "speaker_id": "2",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.13",
          "form": "사진 본 기억이 나네요",
          "original_form": "사진 본 기억이 나네요",
          "speaker_id": "1",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.14",
          "form": "이국적인 음식 먹어 보고 싶네요",
          "original_form": "이국적인 음식 먹어 보고 싶네요",
          "speaker_id": "2",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.15",
          "form": "랍스타가 아주 많죠",
          "original_form": "랍스타가 아주 많죠",
          "speaker_id": "1",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.16",
          "form": "랍스타 정말 맛있겠어요",
          "original_form": "랍스타 정말 맛있겠어요",
          "speaker_id": "2",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030687.1.17",
          "form": "베트남이 더워서 그렇지 풍부한 나라예요",
          "original_form": "베트남이 더워서 그렇지 풍부한 나라예요",
          "speaker_id": "1",
          "time": "20210904 11:28"
        }
