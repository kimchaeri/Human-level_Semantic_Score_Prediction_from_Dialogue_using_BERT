      "utterance": [
        {
          "id": "MDRW2100025677.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025677.1.2",
          "form": "여행가고 싶은 곳이 있으세요?",
          "original_form": "여행가고 싶은 곳이 있으세요?",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025677.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025677.1.4",
          "form": "지금은 안동가고싶어요",
          "original_form": "지금은 안동가고싶어요",
          "speaker_id": "2",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025677.1.5",
          "form": "안동 어디 가고싶으세요??",
          "original_form": "안동 어디 가고싶으세요??",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025677.1.6",
          "form": "월영교요!",
          "original_form": "월영교요!",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025677.1.7",
          "form": "아 완전 야경맛집이죠!",
          "original_form": "아 완전 야경맛집이죠!",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025677.1.8",
          "form": "친구들이랑 갔었는데 정말 이뻤어요 ㅎㅎ",
          "original_form": "친구들이랑 갔었는데 정말 이뻤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025677.1.9",
          "form": "저는 하동 가고싶어요",
          "original_form": "저는 하동 가고싶어요",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025677.1.10",
          "form": "다원에 차 마시러 ㅎㅎ",
          "original_form": "다원에 차 마시러 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025677.1.11",
          "form": "아 하동도 좋죠ㅠㅠ",
          "original_form": "아 하동도 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025677.1.12",
          "form": "제가 갔었을때는 사람이 없어서 너무 한적하고 좋았어요",
          "original_form": "제가 갔었을때는 사람이 없어서 너무 한적하고 좋았어요",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025677.1.13",
          "form": "사람 없는거 너무 좋죠 ㅠㅠ 나만 거기있는 느낌",
          "original_form": "사람 없는거 너무 좋죠 ㅠㅠ 나만 거기있는 느낌",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025677.1.14",
          "form": "그죠!! 제가 그게 좋아서 한 5~7년전에는 경주",
          "original_form": "그죠!! 제가 그게 좋아서 한 5~7년전에는 경주",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025677.1.15",
          "form": "3년전에는 안동 갔었어요",
          "original_form": "3년전에는 안동 갔었어요",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025677.1.16",
          "form": "요즘은 경주에 사람이 너무 많아요",
          "original_form": "요즘은 경주에 사람이 너무 많아요",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025677.1.17",
          "form": "맞아요 경주 넘 핫해졌어요 ㅠ",
          "original_form": "맞아요 경주 넘 핫해졌어요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025677.1.18",
          "form": "어딜가든 사람이 많더라구요",
          "original_form": "어딜가든 사람이 많더라구요",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025677.1.19",
          "form": "이것저것 생긴것도 많구요...",
          "original_form": "이것저것 생긴것도 많구요...",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025677.1.20",
          "form": "맛집없기로 유명한 경주에 식사할만한곳 생기는건 좋지만...",
          "original_form": "맛집없기로 유명한 경주에 식사할만한곳 생기는건 좋지만...",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025677.1.21",
          "form": "이제 자주 가진 않을거 같아요",
          "original_form": "이제 자주 가진 않을거 같아요",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025677.1.22",
          "form": "맛집이 없었어요? ㅋㅋㅋㅋ 몰랐어요",
          "original_form": "맛집이 없었어요? ㅋㅋㅋㅋ 몰랐어요",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025677.1.23",
          "form": "자주는 저도 안갈거같아요",
          "original_form": "자주는 저도 안갈거같아요",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025677.1.24",
          "form": "경주 맛집이 있었나요..?ㅠ 저는 갈때마다...",
          "original_form": "경주 맛집이 있었나요..?ㅠ 저는 갈때마다...",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025677.1.25",
          "form": "못찾았어요",
          "original_form": "못찾았어요",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025677.1.26",
          "form": "흠.. 가깝긴한데 자주 안가서 잘 모르겠어요 ㅠ",
          "original_form": "흠.. 가깝긴한데 자주 안가서 잘 모르겠어요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100025677.1.27",
          "form": "그래도 뭔가 먹을거리는 있었던거같은데 ㅠ",
          "original_form": "그래도 뭔가 먹을거리는 있었던거같은데 ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:26"
        }
      ]
    }
