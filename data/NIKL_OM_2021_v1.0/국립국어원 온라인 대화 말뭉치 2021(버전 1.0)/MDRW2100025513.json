      "utterance": [
        {
          "id": "MDRW2100025513.1.1",
          "form": "오늘은 카레랑 불고기랑 소세지입니다~",
          "original_form": "오늘은 카레랑 불고기랑 소세지입니다~",
          "speaker_id": "2",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025513.1.2",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "2",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025513.1.3",
          "form": "어제 샌드위치 남은거 먹엇슺니당",
          "original_form": "어제 샌드위치 남은거 먹엇슺니당",
          "speaker_id": "1",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100025513.1.4",
          "form": "카레카레 맛있는데",
          "original_form": "카레카레 맛있는데",
          "speaker_id": "1",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100025513.1.5",
          "form": "ㅋㅋ샌드위치 큰거삿어요? 왤케 쪼끔드시나요ㅠ",
          "original_form": "ㅋㅋ샌드위치 큰거삿어요? 왤케 쪼끔드시나요ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025513.1.6",
          "form": "후앙 ..",
          "original_form": "후앙 ..",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025513.1.7",
          "form": "속이더부룩해유",
          "original_form": "속이더부룩해유",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100025513.1.8",
          "form": "계속케익만무거서근가",
          "original_form": "계속케익만무거서근가",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100025513.1.9",
          "form": "ㅜㅠ 오늘은 밥드세요 김치찌개같은거 칼큼한거",
          "original_form": "ㅜㅠ 오늘은 밥드세요 김치찌개같은거 칼큼한거",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025513.1.10",
          "form": "으엉 ㅠㅠ근데 혼자 살다보니",
          "original_form": "으엉 ㅠㅠ근데 혼자 살다보니",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100025513.1.11",
          "form": "그런 밥먹기가 힘두네용",
          "original_form": "그런 밥먹기가 힘두네용",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100025513.1.12",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100025513.1.13",
          "form": "마자요...사실 저도 그래요",
          "original_form": "마자요...사실 저도 그래요",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025513.1.14",
          "form": "그래서 저도 회사밥이라도 열심히 먹으려고 합니다",
          "original_form": "그래서 저도 회사밥이라도 열심히 먹으려고 합니다",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025513.1.15",
          "form": "그나마 다양한 메뉴가 나와서요ㅠ",
          "original_form": "그나마 다양한 메뉴가 나와서요ㅠ",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025513.1.16",
          "form": "헝 ㅜㅜ 그러게요... 아 급식이그립네요",
          "original_form": "헝 ㅜㅜ 그러게요... 아 급식이그립네요",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025513.1.17",
          "form": "맞아요 급식 잘나오는게 최고인데.. 급식 너무 좋죠 영양사님이 영양계산도 잘 해주시구",
          "original_form": "맞아요 급식 잘나오는게 최고인데.. 급식 너무 좋죠 영양사님이 영양계산도 잘 해주시구",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025513.1.18",
          "form": "저 고등학교 급식이 진짜 잘 나왔던거같아요",
          "original_form": "저 고등학교 급식이 진짜 잘 나왔던거같아요",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025513.1.19",
          "form": "고등학교때 급식이 어떻게 좋았나요?",
          "original_form": "고등학교때 급식이 어떻게 좋았나요?",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025513.1.20",
          "form": "메유가 좋은? 아니면 푸짐함?",
          "original_form": "메유가 좋은? 아니면 푸짐함?",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025513.1.21",
          "form": "메뉴",
          "original_form": "메뉴",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025513.1.22",
          "form": "메뉴가 갠차낫던거같아요!간식도ㅋㅋㅋ",
          "original_form": "메뉴가 갠차낫던거같아요!간식도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025513.1.23",
          "form": "name2님은.어떠셨어요",
          "original_form": "&name2&님은.어떠셨어요",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025513.1.24",
          "form": "저도 고등학교때 푸짐하고 맛도좋았어요",
          "original_form": "저도 고등학교때 푸짐하고 맛도좋았어요",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025513.1.25",
          "form": "간식때 베스킨 아이스크림줄때가 젤 좋았네요 ㅋㅋㅋ",
          "original_form": "간식때 베스킨 아이스크림줄때가 젤 좋았네요 ㅋㅋㅋ",
          "speaker_id": "2",
