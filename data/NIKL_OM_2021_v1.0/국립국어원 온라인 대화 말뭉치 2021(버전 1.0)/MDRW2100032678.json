        {
          "id": "MDRW2100032678.1.1",
          "form": "동호회 있으신가요",
          "original_form": "동호회 있으신가요",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100032678.1.2",
          "form": "아니요 ㅋㅋ",
          "original_form": "아니요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100032678.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100032678.1.4",
          "form": "아뇨 ㅋㅋ모임도 없으세요?",
          "original_form": "아뇨 ㅋㅋ모임도 없으세요?",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100032678.1.5",
          "form": "뭐 친구들이랑 하는 계는 있죠",
          "original_form": "뭐 친구들이랑 하는 계는 있죠",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100032678.1.6",
          "form": "그쵸 친구들이랑 놀러가려고 돈모으고하는 ㅎㅎ",
          "original_form": "그쵸 친구들이랑 놀러가려고 돈모으고하는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100032678.1.7",
          "form": "아 저희는 먹으러",
          "original_form": "아 저희는 먹으러",
          "speaker_id": "1",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100032678.1.8",
          "form": "그것도 포함이용 ㅋ.ㅋ 먹는게 빠짐 안되죠!",
          "original_form": "그것도 포함이용 ㅋ.ㅋ 먹는게 빠짐 안되죠!",
          "speaker_id": "2",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100032678.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ먹기만해요 정말ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ먹기만해요 정말ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100032678.1.10",
          "form": "먹기위해 살죠 .. 맛있는거 먹으려구 ㅠ",
          "original_form": "먹기위해 살죠 .. 맛있는거 먹으려구 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100032678.1.11",
          "form": "마자요 ㅠㅠㅠㅠ",
          "original_form": "마자요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100032678.1.12",
          "form": "코로나가 빨리풀려야할텐데 ㅠㅠ놀러두 가구요",
          "original_form": "코로나가 빨리풀려야할텐데 ㅠㅠ놀러두 가구요",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100032678.1.13",
          "form": "정말 그랬으면 좋겠지만 ....",
          "original_form": "정말 그랬으면 좋겠지만 ....",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100032678.1.14",
          "form": "언제 풀릴지...",
          "original_form": "언제 풀릴지...",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
