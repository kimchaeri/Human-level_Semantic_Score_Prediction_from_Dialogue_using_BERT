      "utterance": [
        {
          "id": "MDRW2100019135.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019135.1.2",
          "form": "안녕하세요~ 로제 떡볶이 좋아하시나요 ㅎㅎ",
          "original_form": "안녕하세요~ 로제 떡볶이 좋아하시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019135.1.3",
          "form": "네 ㅋㅋㅋ 좋아하세요?",
          "original_form": "네 ㅋㅋㅋ 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019135.1.4",
          "form": "엄청 좋아해요 ㅋㅋ",
          "original_form": "엄청 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.5",
          "form": "어디꺼 젤 좋아하세요?",
          "original_form": "어디꺼 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.6",
          "form": "어디거 드세요?",
          "original_form": "어디거 드세요?",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.7",
          "form": "앗 ㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.8",
          "form": "저는 신전 좋아해요",
          "original_form": "저는 신전 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.9",
          "form": "헐 저두요 ㅋㅋㅋ !",
          "original_form": "헐 저두요 ㅋㅋㅋ !",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.10",
          "form": "오 너무 맛있지않나요 ㅠㅠ 가성비도 좋구여",
          "original_form": "오 너무 맛있지않나요 ㅠㅠ 가성비도 좋구여",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.11",
          "form": "맞아요 ㅠㅠ 그 신전 특유의 맛이랑 로제랑",
          "original_form": "맞아요 ㅠㅠ 그 신전 특유의 맛이랑 로제랑",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.12",
          "form": "잘어울려요",
          "original_form": "잘어울려요",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.13",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.14",
          "form": "후추향도 좀 나지않나요? ㅋㅋㅋ",
          "original_form": "후추향도 좀 나지않나요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019135.1.15",
          "form": "아 맞아요! 카레향 + 후추향 ㅋㅋ",
          "original_form": "아 맞아요! 카레향 + 후추향 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019135.1.16",
          "form": "근데 신전꺼는 호불호 갈리더라구요",
          "original_form": "근데 신전꺼는 호불호 갈리더라구요",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019135.1.17",
          "form": "맞아요 ㅋㅋㅋㅋ 다른덴 어디드셔보셨어요??",
          "original_form": "맞아요 ㅋㅋㅋㅋ 다른덴 어디드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019135.1.18",
          "form": "아 그래요??",
          "original_form": "아 그래요??",
          "speaker_id": "1",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019135.1.19",
          "form": "네네 싫어하는 사람도 많아요 ㅠ",
          "original_form": "네네 싫어하는 사람도 많아요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019135.1.20",
          "form": "다른데꺼 거의 다 먹어본거같은데",
          "original_form": "다른데꺼 거의 다 먹어본거같은데",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019135.1.21",
          "form": "저는 뭔가",
          "original_form": "저는 뭔가",
          "speaker_id": "1",
