      "utterance": [
        {
          "id": "MDRW2100019997.1.1",
          "form": "요즘 자주 듣는 노래 있으세요?",
          "original_form": "요즘 자주 듣는 노래 있으세요?",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100019997.1.2",
          "form": "저는 주로 출근할 때 음원사이트를 이용하는 편이라",
          "original_form": "저는 주로 출근할 때 음원사이트를 이용하는 편이라",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100019997.1.3",
          "form": "그때그때 상큼한 재즈나 팝음악 들어요",
          "original_form": "그때그때 상큼한 재즈나 팝음악 들어요",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100019997.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019997.1.5",
          "form": "저는 한곡에 꽂히면 그것만 듣는 스탈이랗ㅎㅎ",
          "original_form": "저는 한곡에 꽂히면 그것만 듣는 스탈이랗ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 03:55"
        },
        {
          "id": "MDRW2100019997.1.6",
          "form": "주로 아이돌노래 위주로 들어요 밝고 상큼한 곡들",
          "original_form": "주로 아이돌노래 위주로 들어요 밝고 상큼한 곡들",
          "speaker_id": "1",
          "time": "20210828 03:55"
        },
        {
          "id": "MDRW2100019997.1.7",
          "form": "아 여름엔 상큼한 노래들이 음원사이트 상위권에 있더라고요",
          "original_form": "아 여름엔 상큼한 노래들이 음원사이트 상위권에 있더라고요",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100019997.1.8",
          "form": "요즘 자주듣는 팝송 추천해주세요!",
          "original_form": "요즘 자주듣는 팝송 추천해주세요!",
          "speaker_id": "1",
          "time": "20210829 06:46"
        },
        {
          "id": "MDRW2100019997.1.9",
          "form": "저스틴비버껀가 피치스 좋더라고요ㅎㅎ",
          "original_form": "저스틴비버껀가 피치스 좋더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 06:46"
        },
        {
          "id": "MDRW2100019997.1.10",
          "form": "맞아요 저스틴비버 피치스",
          "original_form": "맞아요 저스틴비버 피치스",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100019997.1.11",
          "form": "저도 요즘 자주 들었어요",
          "original_form": "저도 요즘 자주 들었어요",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100019997.1.12",
          "form": "어제 그알 보셨나요?",
          "original_form": "어제 그알 보셨나요?",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100019997.1.13",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100019997.1.14",
          "form": "무슨 내용이었어요?",
          "original_form": "무슨 내용이었어요?",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100019997.1.15",
          "form": "요즘 한창 정치이슈얘긴데",
          "original_form": "요즘 한창 정치이슈얘긴데",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100019997.1.16",
          "form": "어떤 사기꾼이 가짜수산업자 행세하면서",
          "original_form": "어떤 사기꾼이 가짜수산업자 행세하면서",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100019997.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100019997.1.18",
          "form": "방송인 국회의원 정치인들 속이고",
          "original_form": "방송인 국회의원 정치인들 속이고",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100019997.1.19",
          "form": "그런내용인데ㅋㅋㅋ사기친 아이템이",
          "original_form": "그런내용인데ㅋㅋㅋ사기친 아이템이",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100019997.1.20",
          "form": "오징어였어요",
          "original_form": "오징어였어요",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100019997.1.21",
          "form": "엥? 오징어요?",
          "original_form": "엥? 오징어요?",
          "speaker_id": "2",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100019997.1.22",
          "form": "오징어를 저렴할때 잡아서",
          "original_form": "오징어를 저렴할때 잡아서",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100019997.1.23",
          "form": "얼렸다가 비싸지면 팔고",
          "original_form": "얼렸다가 비싸지면 팔고",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100019997.1.24",
          "form": "그럼 돈이 된다고",
          "original_form": "그럼 돈이 된다고",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100019997.1.25",
          "form": "86억이나",
          "original_form": "86억이나",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100019997.1.26",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100019997.1.27",
          "form": "투자를 했대요 하하",
          "original_form": "투자를 했대요 하하",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100019997.1.28",
          "form": "그런걸로도 사기를 치는군요",
          "original_form": "그런걸로도 사기를 치는군요",
          "speaker_id": "2",
