        },
        {
          "id": "MDRW2100031499.1.2",
          "form": "요즘 ott 이용하는거 있어요?",
          "original_form": "요즘 ott 이용하는거 있어요?",
          "speaker_id": "1",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100031499.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100031499.1.4",
          "form": "티빙만봐요",
          "original_form": "티빙만봐요",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100031499.1.5",
          "form": "심심님은요?",
          "original_form": "심심님은요?",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100031499.1.6",
          "form": "저는 왓챠랑 티빙이요!",
          "original_form": "저는 왓챠랑 티빙이요!",
          "speaker_id": "1",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100031499.1.7",
          "form": "티빙으로 최근에 본거 있어요?",
          "original_form": "티빙으로 최근에 본거 있어요?",
          "speaker_id": "1",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100031499.1.8",
          "form": "티빙으로 유퀴즈나 슬의생이요 ㅎ",
          "original_form": "티빙으로 유퀴즈나 슬의생이요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100031499.1.9",
          "form": "왓챠는 재밌는거 있나요?",
          "original_form": "왓챠는 재밌는거 있나요?",
          "speaker_id": "2",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100031499.1.10",
          "form": "왓챠는 넷플에 없는게 있어서 보고 있어요ㅋㅋ",
          "original_form": "왓챠는 넷플에 없는게 있어서 보고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031499.1.11",
          "form": "슬의 지금 방송분까지 다 보셨나요?",
          "original_form": "슬의 지금 방송분까지 다 보셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031499.1.12",
          "form": "아뇨 앞에만요ㅜㅜ",
          "original_form": "아뇨 앞에만요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031499.1.13",
          "form": "프로그램이 기니까 다 못봤어요",
          "original_form": "프로그램이 기니까 다 못봤어요",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031499.1.14",
          "form": "점점 길어지는것 같지 않아요?",
          "original_form": "점점 길어지는것 같지 않아요?",
          "speaker_id": "1",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031499.1.15",
          "form": "맞아요 그래서 시간 날때만 조금씩 봐요",
          "original_form": "맞아요 그래서 시간 날때만 조금씩 봐요",
          "speaker_id": "2",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100031499.1.16",
          "form": "사실 옛날보다 긴영상보는게 집중력이 떨어져요",
          "original_form": "사실 옛날보다 긴영상보는게 집중력이 떨어져요",
          "speaker_id": "2",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100031499.1.17",
          "form": "아 저도요 유튜브의 영향인지",
          "original_form": "아 저도요 유튜브의 영향인지",
          "speaker_id": "1",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031499.1.18",
          "form": "이젠 긴거 보면 힘들더라고요",
          "original_form": "이젠 긴거 보면 힘들더라고요",
          "speaker_id": "1",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031499.1.19",
          "form": "유투브는 자주 보세요?",
          "original_form": "유투브는 자주 보세요?",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031499.1.20",
          "form": "네 종종 구독해놓은 사람들 영상 뜨면 보는거 같아요",
          "original_form": "네 종종 구독해놓은 사람들 영상 뜨면 보는거 같아요",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031499.1.21",
          "form": "주로 보는 유튜버 있으세요?",
          "original_form": "주로 보는 유튜버 있으세요?",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031499.1.22",
          "form": "어떤 채널보세요?",
          "original_form": "어떤 채널보세요?",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031499.1.23",
          "form": "전 요즘 먹방봐요",
          "original_form": "전 요즘 먹방봐요",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031499.1.24",
          "form": "저는 예랑가랑봐요",
          "original_form": "저는 예랑가랑봐요",
          "speaker_id": "1",
          "time": "20210904 22:20"
        }
