      "utterance": [
        {
          "id": "MDRW2100006960.1.1",
          "form": "온라인 커뮤니티",
          "original_form": "온라인 커뮤니티",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006960.1.2",
          "form": "난 에타를 자주 본다",
          "original_form": "난 에타를 자주 본다",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006960.1.3",
          "form": "얼마전에 SNS에서 만난 친구들이랑",
          "original_form": "얼마전에 SNS에서 만난 친구들이랑",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006960.1.4",
          "form": "부산에서 정모한 적 있다",
          "original_form": "부산에서 정모한 적 있다",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006960.1.5",
          "form": "정모라고 하면 거창하고",
          "original_form": "정모라고 하면 거창하고",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006960.1.6",
          "form": "그냥 친한 친구들끼리 만나서 놀았다",
          "original_form": "그냥 친한 친구들끼리 만나서 놀았다",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006960.1.7",
          "form": "sns에서 친구를 만난 친구를 직접 만나다니",
          "original_form": "sns에서 친구를 만난 친구를 직접 만나다니",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006960.1.8",
          "form": "그정도면 정모지",
          "original_form": "그정도면 정모지",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006960.1.9",
          "form": "재밌었다",
          "original_form": "재밌었다",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006960.1.10",
          "form": "부산바다 구경하고 왔습니다",
          "original_form": "부산바다 구경하고 왔습니다",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006960.1.11",
          "form": "이열",
          "original_form": "이열",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006960.1.12",
          "form": "친구들이면 나이는 다 비슷한건가",
          "original_form": "친구들이면 나이는 다 비슷한건가",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006960.1.13",
          "form": "아뇨 94년생부터 01년생까지 있었습니다",
          "original_form": "아뇨 94년생부터 01년생까지 있었습니다",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006960.1.14",
          "form": "그래도 친구죠",
          "original_form": "그래도 친구죠",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006960.1.15",
          "form": "94..",
          "original_form": "94..",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006960.1.16",
          "form": "그럴수있지",
          "original_form": "그럴수있지",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006960.1.17",
          "form": "친구지",
          "original_form": "친구지",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006960.1.18",
          "form": "친구지",
          "original_form": "친구지",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006960.1.19",
          "form": "갓 태어난 누구랑도 친구하는데",
          "original_form": "갓 태어난 누구랑도 친구하는데",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006960.1.20",
          "form": "94년생쯤이야...",
          "original_form": "94년생쯤이야...",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006960.1.21",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006960.1.22",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006960.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006960.1.24",
          "form": "여대 에타는 좀 클린한 편인지...",
          "original_form": "여대 에타는 좀 클린한 편인지...",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006960.1.25",
          "form": "가끔 관음하는 사람..들이",
          "original_form": "가끔 관음하는 사람..들이",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006960.1.26",
          "form": "있긴 한데",
          "original_form": "있긴 한데",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006960.1.27",
          "form": "그거빼면 괜찮은 편",
          "original_form": "그거빼면 괜찮은 편",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006960.1.28",
          "form": "관음은 계정 사서 하는건가...",
          "original_form": "관음은 계정 사서 하는건가...",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006960.1.29",
          "form": "가끔 의견 나뉠 때도 있지만 클린하지",
          "original_form": "가끔 의견 나뉠 때도 있지만 클린하지",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006960.1.30",
          "form": "진짜 이상한 사람들 많네",
          "original_form": "진짜 이상한 사람들 많네",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006960.1.31",
          "form": "응 계정 파는 사람이 있더라",
          "original_form": "응 계정 파는 사람이 있더라",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006960.1.32",
          "form": "그 사람도 신기...",
          "original_form": "그 사람도 신기...",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006960.1.33",
          "form": "보통 글 쓰는 거나",
          "original_form": "보통 글 쓰는 거나",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006960.1.34",
          "form": "대충 감이 잡히는데",
          "original_form": "대충 감이 잡히는데",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006960.1.35",
          "form": "아 플리마켓게시판 같은 데서",
          "original_form": "아 플리마켓게시판 같은 데서",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006960.1.36",
          "form": "옵챗 파면",
          "original_form": "옵챗 파면",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006960.1.37",
          "form": "가끔 관음하는 사람들 들어온다더라..",
          "original_form": "가끔 관음하는 사람들 들어온다더라..",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006960.1.38",
          "form": "와... 진짜 이상한 사람들이다...",
          "original_form": "와... 진짜 이상한 사람들이다...",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006960.1.39",
          "form": "우리 학교 에타는 대놓고 이상한 사람들이 넘쳐서 내가 할 말은 아니긴 한데",
          "original_form": "우리 학교 에타는 대놓고 이상한 사람들이 넘쳐서 내가 할 말은 아니긴 한데",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006960.1.40",
          "form": "성별은 얘기 안하지만",
          "original_form": "성별은 얘기 안하지만",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006960.1.41",
          "form": "착샷을 찍어보내라고하더라",
          "original_form": "착샷을 찍어보내라고하더라",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006960.1.42",
          "form": "대놓고 이상한 사람들..",
          "original_form": "대놓고 이상한 사람들..",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006960.1.43",
          "form": "착샷 ㅎㅎㅎ...ㅎㅎㅎㅎ",
          "original_form": "착샷 ㅎㅎㅎ...ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006960.1.44",
          "form": "유리구슬이다",
          "original_form": "유리구슬이다",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006960.1.45",
          "form": "근데 그런 사람들 많아서",
          "original_form": "근데 그런 사람들 많아서",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006960.1.46",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006960.1.47",
          "form": "처음엔 사람들 관음하는 사람 글 가서 댓글 테러 했는데",
          "original_form": "처음엔 사람들 관음하는 사람 글 가서 댓글 테러 했는데",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006960.1.48",
          "form": "요즘엔 그냥 신고 눌러서 글 다 빛삭됨",
          "original_form": "요즘엔 그냥 신고 눌러서 글 다 빛삭됨",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006960.1.49",
          "form": "역시 먹이를 안 주는 게 답이지",
          "original_form": "역시 먹이를 안 주는 게 답이지",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006960.1.50",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006960.1.51",
          "form": "에타 본인인증 좀 제대로 하면 좋겠는데",
          "original_form": "에타 본인인증 좀 제대로 하면 좋겠는데",
          "speaker_id": "2",
