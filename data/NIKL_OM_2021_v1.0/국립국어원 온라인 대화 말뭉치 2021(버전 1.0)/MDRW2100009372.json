        },
        {
          "id": "MDRW2100009372.1.2",
          "form": "평소에 집안일 하세요?",
          "original_form": "평소에 집안일 하세요?",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009372.1.3",
          "form": "네 저 자취중이라 집안일 열심히 하고있어요",
          "original_form": "네 저 자취중이라 집안일 열심히 하고있어요",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009372.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009372.1.5",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009372.1.6",
          "form": "저랏 반만 비슷하시네요",
          "original_form": "저랏 반만 비슷하시네요",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009372.1.7",
          "form": "자취중인데 집안일을 미루고 있어요",
          "original_form": "자취중인데 집안일을 미루고 있어요",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009372.1.8",
          "form": "특히 화장실 청소요",
          "original_form": "특히 화장실 청소요",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009372.1.9",
          "form": "설거지와",
          "original_form": "설거지와",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009372.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009372.1.11",
          "form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ 사실 저도 말은 저렇게했지만",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ 사실 저도 말은 저렇게했지만",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009372.1.12",
          "form": "화장실청소까지 열심히하진않아요ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "화장실청소까지 열심히하진않아요ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009372.1.13",
          "form": "설거지랑 화장실청소가 젤 하기싫은거같아요ㅠ",
          "original_form": "설거지랑 화장실청소가 젤 하기싫은거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009372.1.14",
          "form": "아 정말요",
          "original_form": "아 정말요",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009372.1.15",
          "form": "저희 집은 주방분리형이라서",
          "original_form": "저희 집은 주방분리형이라서",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009372.1.16",
          "form": "방에만 에어컨이 있고 부엌은 덥거든요",
          "original_form": "방에만 에어컨이 있고 부엌은 덥거든요",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009372.1.17",
          "form": "집안일이고 뭐고 침대에만 있고 싶어요",
          "original_form": "집안일이고 뭐고 침대에만 있고 싶어요",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009372.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009372.1.19",
          "form": "룸메이트 있으세요?",
          "original_form": "룸메이트 있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009372.1.20",
          "form": "헐ㅠㅠㅠㅠ 근데 주방분리형이라니 부러운데요??",
          "original_form": "헐ㅠㅠㅠㅠ 근데 주방분리형이라니 부러운데요??",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009372.1.21",
          "form": "저는 1.5룸이여서 음식 한번 해먹으면 온데 다 냄새가ㅠㅠㅠㅠ",
          "original_form": "저는 1.5룸이여서 음식 한번 해먹으면 온데 다 냄새가ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009372.1.22",
          "form": "name1님은 좀 넓은데 사시나봐요??",
          "original_form": "&name1&님은 좀 넓은데 사시나봐요??",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009372.1.23",
          "form": "룸메는 없어요!ㅎㅎㅎ",
          "original_form": "룸메는 없어요!ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009372.1.24",
          "form": "방만 한 10평 정도 돼요",
          "original_form": "방만 한 10평 정도 돼요",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009372.1.25",
          "form": "집을 가성비 있게 잘 구했어요",
          "original_form": "집을 가성비 있게 잘 구했어요",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009372.1.26",
          "form": "서울이 아니고 지방이거든요",
          "original_form": "서울이 아니고 지방이거든요",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009372.1.27",
          "form": "헐 대박...b name1님 완전 좋은데서 자취하시는데요??",
          "original_form": "헐 대박...b &name1&님 완전 좋은데서 자취하시는데요??",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009372.1.28",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009372.1.29",
          "form": "저도 지방살아요! 근데도 비싸더라구요ㅠㅠ 전 부산인데 월세 45만원 정도 하네요",
          "original_form": "저도 지방살아요! 근데도 비싸더라구요ㅠㅠ 전 부산인데 월세 45만원 정도 하네요",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009372.1.30",
          "form": "이제 티비만 있으면 완벽해요",
          "original_form": "이제 티비만 있으면 완벽해요",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009372.1.31",
          "form": "부산은 도시라서 그런가봐요",
          "original_form": "부산은 도시라서 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009372.1.32",
          "form": "전 진주거든요",
          "original_form": "전 진주거든요",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009372.1.33",
          "form": "덜 도시죠",
          "original_form": "덜 도시죠",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009372.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009372.1.35",
          "form": "룸메이트 가져보신 적 있으세요?",
          "original_form": "룸메이트 가져보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009372.1.36",
          "form": "헐 저도 티비 없이 살아요 ㅋㅋㅋㅋ",
          "original_form": "헐 저도 티비 없이 살아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009372.1.37",
          "form": "name1님 저희 다음 주제는 자취생의 자취템 쇼핑 어떤가요 ㅋㅋㅋㅋㅋ",
          "original_form": "&name1&님 저희 다음 주제는 자취생의 자취템 쇼핑 어떤가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009372.1.38",
          "form": "앗 진주면 부산이랑 가깝지않나요? 놀러가본적 있는거같아요!ㅎㅎㅎㅎ",
          "original_form": "앗 진주면 부산이랑 가깝지않나요? 놀러가본적 있는거같아요!ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009372.1.39",
          "form": "아뇨아뇨 저는 독립하고나서는 항상 혼자서 살았어요! name1님은요?",
          "original_form": "아뇨아뇨 저는 독립하고나서는 항상 혼자서 살았어요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009372.1.40",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009372.1.41",
          "form": "근데 가끔 화장실에 핸드폰 없이 들어가면서",
          "original_form": "근데 가끔 화장실에 핸드폰 없이 들어가면서",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009372.1.42",
          "form": "그런 상상해요",
          "original_form": "그런 상상해요",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009372.1.43",
          "form": "문이 고장나서 갇히면 어떡하지?",
          "original_form": "문이 고장나서 갇히면 어떡하지?",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009372.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009372.1.45",
          "form": "그것 빼고 좋아요",
          "original_form": "그것 빼고 좋아요",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009372.1.46",
          "form": "아 맞아요ㅠㅠㅠ",
          "original_form": "아 맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009372.1.47",
          "form": "한번씩 인터넷 보다보면 화장실 문이 안열려서 갇혀서 사망",
          "original_form": "한번씩 인터넷 보다보면 화장실 문이 안열려서 갇혀서 사망",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009372.1.48",
          "form": "이런 글 보다보면 무서워요ㅠㅠㅠ",
          "original_form": "이런 글 보다보면 무서워요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009372.1.49",
          "form": "그 이후로 폰은 진짜 꼭꼭 화장실에 들고가는거같아요ㅠ",
          "original_form": "그 이후로 폰은 진짜 꼭꼭 화장실에 들고가는거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009372.1.50",
          "form": "같은 고민을 하시네요ㅜㅜ",
          "original_form": "같은 고민을 하시네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009372.1.51",
          "form": "전 친구가 갇힌 적 있어요",
          "original_form": "전 친구가 갇힌 적 있어요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009372.1.52",
          "form": "화장실은 아니고 방에서요",
          "original_form": "화장실은 아니고 방에서요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009372.1.53",
          "form": "핸드폰 없이 갇혔는데",
          "original_form": "핸드폰 없이 갇혔는데",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009372.1.54",
          "form": "제가 옆건물 살기도 하고 친해서",
          "original_form": "제가 옆건물 살기도 하고 친해서",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009372.1.55",
          "form": "같이 살다시피 하거든요",
          "original_form": "같이 살다시피 하거든요",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009372.1.56",
          "form": "점심 같이 먹으려고 갔다가 구해줬어요",
          "original_form": "점심 같이 먹으려고 갔다가 구해줬어요",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009372.1.57",
          "form": "간 김에 점심 먹고 설거지도 해주고요",
          "original_form": "간 김에 점심 먹고 설거지도 해주고요",
          "speaker_id": "1",
