      "utterance": [
        {
          "id": "MDRW2100008078.1.1",
          "form": "점심식사는 했나용?",
          "original_form": "점심식사는 했나용?",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008078.1.2",
          "form": "초밥먹었금돳",
          "original_form": "초밥먹었금돳",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008078.1.3",
          "form": "오 어제 초밥 얘기하시더니\r\n오늘 드셨군요ㅋㅋ",
          "original_form": "오 어제 초밥 얘기하시더니\r\n오늘 드셨군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008078.1.4",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008078.1.5",
          "form": "맛있네염 ㅋㅋㅋ",
          "original_form": "맛있네염 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008078.1.6",
          "form": "서비스로 유부초밥주샸는데",
          "original_form": "서비스로 유부초밥주샸는데",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008078.1.7",
          "form": "하필 내가 못먹는걸...ㅎ",
          "original_form": "하필 내가 못먹는걸...ㅎ",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008078.1.8",
          "form": "앜ㅋㅋㅋㅜㅜ \r\n다른거 달라 하시죠ㅠ",
          "original_form": "앜ㅋㅋㅋㅜㅜ \r\n다른거 달라 하시죠ㅠ",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008078.1.9",
          "form": "이미 늦아ㅛ더라구염 ..ㅎㅎ",
          "original_form": "이미 늦아ㅛ더라구염 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008078.1.10",
          "form": "남친이.많이먹음 ㅋ.ㅋ",
          "original_form": "남친이.많이먹음 ㅋ.ㅋ",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008078.1.11",
          "form": "남친은 유부를 좋아하나보네요ㅋ",
          "original_form": "남친은 유부를 좋아하나보네요ㅋ",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008078.1.12",
          "form": "우리 남편도 좋아합니당",
          "original_form": "우리 남편도 좋아합니당",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008078.1.13",
          "form": "name1",
          "original_form": "&name1&{emoji:♡}",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008078.1.14",
          "form": "name1ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "&name1&ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008078.1.15",
          "form": "점심은 뭐드셨어염??ㅋㅋ",
          "original_form": "점심은 뭐드셨어염??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008078.1.16",
          "form": "생선굽굽해서 조금 먹고\r\n집앞 빠바에서 빵사온거 먹었어용",
          "original_form": "생선굽굽해서 조금 먹고\r\n집앞 빠바에서 빵사온거 먹었어용",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008078.1.17",
          "form": "이제 곧 저녁 먹을 시간이 되가네용",
          "original_form": "이제 곧 저녁 먹을 시간이 되가네용",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008078.1.18",
          "form": "고기먹을라구욤 ㅎ.ㄹ",
          "original_form": "고기먹을라구욤 ㅎ.ㄹ",
          "speaker_id": "2",
          "time": "20210822 20:08"
        },
        {
          "id": "MDRW2100008078.1.19",
          "form": "오 저녁은 고기ㅋㅋ",
          "original_form": "오 저녁은 고기ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008078.1.20",
          "form": "고기는 언제나 옳죠",
          "original_form": "고기는 언제나 옳죠{emoji:♡}",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008078.1.21",
          "form": "아침은 드셨어염??",
          "original_form": "아침은 드셨어염??",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100008078.1.22",
          "form": "저흰.아침 거르고 점심 선정중입니당 ㅋㅋ",
          "original_form": "저흰.아침 거르고 점심 선정중입니당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100008078.1.23",
          "form": "저희는이라뇨....?",
          "original_form": "저희는이라뇨....?",
          "speaker_id": "1",
          "time": "20210823 10:15"
        },
        {
          "id": "MDRW2100008078.1.24",
          "form": "아침에 빠바 빵 먹었습니당ㅋㅋ",
          "original_form": "아침에 빠바 빵 먹었습니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:16"
        },
        {
          "id": "MDRW2100008078.1.25",
          "form": "무슨빵 드셨어염 ㅋㅋ",
          "original_form": "무슨빵 드셨어염 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:17"
        },
        {
          "id": "MDRW2100008078.1.26",
          "form": "꼬맹이들이 밤식빵 먹는다해서\r\n같이 밤식빵 먹었습니당ㅋㅋ",
          "original_form": "꼬맹이들이 밤식빵 먹는다해서\r\n같이 밤식빵 먹었습니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:20"
        },
        {
          "id": "MDRW2100008078.1.27",
          "form": "밤식빵좋아하는군용 ㅋㅋ",
          "original_form": "밤식빵좋아하는군용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:21"
        },
        {
          "id": "MDRW2100008078.1.28",
          "form": "네ㅋㅋ 꼬맹이들 입맛에 딱인가봐요",
          "original_form": "네ㅋㅋ 꼬맹이들 입맛에 딱인가봐요",
          "speaker_id": "1",
          "time": "20210823 10:24"
        },
        {
          "id": "MDRW2100008078.1.29",
          "form": "밤식빵 맛있쥬 ㅋㅋㅋ",
          "original_form": "밤식빵 맛있쥬 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100008078.1.30",
          "form": "점심 메뉴는 정하셨어요?",
          "original_form": "점심 메뉴는 정하셨어요?",
          "speaker_id": "1",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100008078.1.31",
          "form": "라멘먹었습니당",
          "original_form": "라멘먹었습니당",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100008078.1.32",
          "form": "대창 덮밥이랑",
          "original_form": "대창 덮밥이랑",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100008078.1.33",
          "form": "대창덮밥요?ㅋㅋ",
          "original_form": "대창덮밥요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100008078.1.34",
          "form": "맛...있나요?",
          "original_form": "맛...있나요?",
          "speaker_id": "1",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100008078.1.35",
          "form": "맛있을거 같은데\r\n먹어본적이 없어서\r\n상상이 안되는 맛 ㅋㅋ",
          "original_form": "맛있을거 같은데\r\n먹어본적이 없어서\r\n상상이 안되는 맛 ㅋㅋ",
