      "utterance": [
        {
          "id": "MDRW2100025324.1.1",
          "form": "새삼 능소화 이름이 trumpet creeper",
          "original_form": "새삼 능소화 이름이 trumpet creeper",
          "speaker_id": "2",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100025324.1.2",
          "form": "라고하니까",
          "original_form": "라고하니까",
          "speaker_id": "2",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100025324.1.3",
          "form": "특이하네",
          "original_form": "특이하네",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025324.1.4",
          "form": "트럼펫 비슷하게 꽃송이가 크긴 하지...",
          "original_form": "트럼펫 비슷하게 꽃송이가 크긴 하지...",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025324.1.5",
          "form": "오.....저지금",
          "original_form": "오.....저지금",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025324.1.6",
          "form": "성인버전보고있는데",
          "original_form": "성인버전보고있는데",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.7",
          "form": "아 바로?",
          "original_form": "아 바로?",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.8",
          "form": "소리안듣고 영상만보는중...",
          "original_form": "소리안듣고 영상만보는중...",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.10",
          "form": "아니 켰더니 그거부터나오더라고....",
          "original_form": "아니 켰더니 그거부터나오더라고....",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.11",
          "form": "그건 안예은 분장이 더 잔인했던거같은..",
          "original_form": "그건 안예은 분장이 더 잔인했던거같은..",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.12",
          "form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.13",
          "form": "근데얼굴에 능소화꽃이",
          "original_form": "근데얼굴에 능소화꽃이",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.14",
          "form": "너무 깜찍해서",
          "original_form": "너무 깜찍해서",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.15",
          "form": "떼주고싶다",
          "original_form": "떼주고싶다",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025324.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "1",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025324.1.17",
          "form": "나머지는 다 괜찮은데",
          "original_form": "나머지는 다 괜찮은데",
          "speaker_id": "2",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025324.1.18",
          "form": "얼굴에꽃만좀",
          "original_form": "얼굴에꽃만좀",
          "speaker_id": "2",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025324.1.19",
          "form": "아근데",
          "original_form": "아근데",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025324.1.20",
          "form": "저는 점프스케어랄게",
          "original_form": "저는 점프스케어랄게",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025324.1.21",
          "form": "한번크게있고",
          "original_form": "한번크게있고",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025324.1.22",
          "form": "나머진 견딜만하네여",
          "original_form": "나머진 견딜만하네여",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025324.1.23",
          "form": "앗 그정도라면 다행...",
          "original_form": "앗 그정도라면 다행...",
          "speaker_id": "1",
          "time": "20210901 09:33"
        },
        {
          "id": "MDRW2100025324.1.24",
          "form": "ㅋㅋㅋㄱㅋ아 그 웃으며 다가오는거",
          "original_form": "ㅋㅋㅋㄱㅋ아 그 웃으며 다가오는거",
          "speaker_id": "2",
          "time": "20210901 09:34"
        },
        {
          "id": "MDRW2100025324.1.25",
          "form": "첨에등장하고",
          "original_form": "첨에등장하고",
          "speaker_id": "2",
          "time": "20210901 09:34"
        },
        {
          "id": "MDRW2100025324.1.26",
          "form": "그게 깜짝",
          "original_form": "그게 깜짝",
          "speaker_id": "2",
          "time": "20210901 09:34"
        },
        {
          "id": "MDRW2100025324.1.27",
          "form": "전체화면안해서다행",
          "original_form": "전체화면안해서다행",
          "speaker_id": "2",
          "time": "20210901 09:34"
        },
        {
          "id": "MDRW2100025324.1.28",
          "form": "노래는 따로들을래",
          "original_form": "노래는 따로들을래",
          "speaker_id": "2",
          "time": "20210901 09:35"
        }
      ]
    }
  ]
