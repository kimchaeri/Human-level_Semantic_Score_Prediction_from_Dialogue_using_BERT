      "utterance": [
        {
          "id": "MDRW2100029928.1.1",
          "form": "오늘 날씨가 간만에 해가 났네요. 거긴 어떤가요?",
          "original_form": "오늘 날씨가 간만에 해가 났네요. 거긴 어떤가요?",
          "speaker_id": "1",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029928.1.2",
          "form": "여긴 흐리고 춥네요 ㅠ",
          "original_form": "여긴 흐리고 춥네요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100029928.1.3",
          "form": "구름이 그쪽으로 갔나봅니다",
          "original_form": "구름이 그쪽으로 갔나봅니다",
          "speaker_id": "1",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100029928.1.4",
          "form": "그런가봐요 ㅠ",
          "original_form": "그런가봐요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100029928.1.5",
          "form": "name2님은 어느 계절을 가장 좋아하시나요?",
          "original_form": "&name2&님은 어느 계절을 가장 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100029928.1.6",
          "form": "전 겨울이요 ㅎㅎ",
          "original_form": "전 겨울이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100029928.1.7",
          "form": "눈이 너무너무 좋아서",
          "original_form": "눈이 너무너무 좋아서",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100029928.1.8",
          "form": "겨울이 좋아요",
          "original_form": "겨울이 좋아요",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100029928.1.9",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100029928.1.10",
          "form": "눈오는 풍경 정말 멋있죠",
          "original_form": "눈오는 풍경 정말 멋있죠",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100029928.1.11",
          "form": "내리는 눈이 좋나요? 쌓인 눈이 좋나요?",
          "original_form": "내리는 눈이 좋나요? 쌓인 눈이 좋나요?",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100029928.1.12",
          "form": "둘다 좋은데...........",
          "original_form": "둘다 좋은데...........",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100029928.1.13",
          "form": "사실 못봐서 좋은거같기도하고",
          "original_form": "사실 못봐서 좋은거같기도하고",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100029928.1.14",
          "form": "그냥 눈이면 환장합니다",
          "original_form": "그냥 눈이면 환장합니다",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100029928.1.15",
          "form": "눈이 별로 안오는 지역에 사시는군요",
          "original_form": "눈이 별로 안오는 지역에 사시는군요",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100029928.1.16",
          "form": "네..부산에 살아요",
          "original_form": "네..부산에 살아요",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100029928.1.17",
          "form": "제 평생 눈 본적이 몇 번 없네요",
          "original_form": "제 평생 눈 본적이 몇 번 없네요",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100029928.1.18",
          "form": "손꼽을 수 있을거같아요",
          "original_form": "손꼽을 수 있을거같아요",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100029928.1.19",
          "form": "그렇군요 전 수도권이라 겨울마다 눈을 봅니다",
          "original_form": "그렇군요 전 수도권이라 겨울마다 눈을 봅니다",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100029928.1.20",
          "form": "아 그럼 싫으시겠다",
          "original_form": "아 그럼 싫으시겠다",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100029928.1.21",
          "form": "보통은 싫어하더라구요",
          "original_form": "보통은 싫어하더라구요",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100029928.1.22",
          "form": "하늘에서 내려오는 쓰레기..?",
          "original_form": "하늘에서 내려오는 쓰레기..?",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100029928.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100029928.1.24",
          "form": "운전하면서부터  현실적이 되더군요",
          "original_form": "운전하면서부터  현실적이 되더군요",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100029928.1.25",
          "form": "미끄러우니까 위험하답니다",
          "original_form": "미끄러우니까 위험하답니다",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100029928.1.26",
          "form": "아... 하긴 그렇겠네요",
          "original_form": "아... 하긴 그렇겠네요",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100029928.1.27",
          "form": "전 눈길에서도 운전할일이 정말 없겠네요",
          "original_form": "전 눈길에서도 운전할일이 정말 없겠네요",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100029928.1.28",
          "form": "그래도...뭔가...호기심이 생기긴한데",
          "original_form": "그래도...뭔가...호기심이 생기긴한데",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100029928.1.29",
          "form": "엄청 위험할거같아요",
          "original_form": "엄청 위험할거같아요",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100029928.1.30",
          "form": "눈이 얼면 정말 위험해요",
          "original_form": "눈이 얼면 정말 위험해요",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100029928.1.31",
          "form": "아 얼건 생각도 못했네요",
          "original_form": "아 얼건 생각도 못했네요",
          "speaker_id": "2",
          "time": "20210903 20:32"
        }
      ]
    }
