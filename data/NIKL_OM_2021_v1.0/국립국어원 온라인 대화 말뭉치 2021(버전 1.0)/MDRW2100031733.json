      "utterance": [
        {
          "id": "MDRW2100031733.1.1",
          "form": "김치찌개 된장찌개 어떤게 더 좋나요",
          "original_form": "김치찌개 된장찌개 어떤게 더 좋나요",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031733.1.2",
          "form": "악 넘 어려워요ㅠ",
          "original_form": "악 넘 어려워요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031733.1.3",
          "form": "평생 하나만 먹어야해요",
          "original_form": "평생 하나만 먹어야해요",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031733.1.4",
          "form": "그건 무리인데요ㅜ",
          "original_form": "그건 무리인데요ㅜ",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031733.1.5",
          "form": "그럼 평생 하나 포기하기!",
          "original_form": "그럼 평생 하나 포기하기!",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031733.1.6",
          "form": "김치찌개요ㅠ",
          "original_form": "김치찌개요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031733.1.7",
          "form": "와 전 된장찌개인데",
          "original_form": "와 전 된장찌개인데",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031733.1.8",
          "form": "김치요리는 많으니깐..",
          "original_form": "김치요리는 많으니깐..",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031733.1.9",
          "form": "아 그런가요ㅋㅋ",
          "original_form": "아 그런가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031733.1.10",
          "form": "네 김치찜으로 버티려고요",
          "original_form": "네 김치찜으로 버티려고요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031733.1.11",
          "form": "역시 배우신분ㅋㄱㄲ",
          "original_form": "역시 배우신분ㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031733.1.12",
          "form": "찌개중에는 뭐가 젤 좋아요",
          "original_form": "찌개중에는 뭐가 젤 좋아요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031733.1.13",
          "form": "저 순두부찌개도 너무 좋아요",
          "original_form": "저 순두부찌개도 너무 좋아요",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031733.1.14",
          "form": "해물순두부찌개가 진리죠",
          "original_form": "해물순두부찌개가 진리죠",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031733.1.15",
          "form": "역시 저흰 한국인이에요!!",
          "original_form": "역시 저흰 한국인이에요!!",
          "speaker_id": "1",
