      "utterance": [
        {
          "id": "MDRW2100003991.1.1",
          "form": "아침에 비가 많이 오더니 해가 덨네",
          "original_form": "아침에 비가 많이 오더니 해가 덨네",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003991.1.2",
          "form": "해가 나왔어 ? 커튼 치워봐야 겠다",
          "original_form": "해가 나왔어 ? 커튼 치워봐야 겠다",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003991.1.3",
          "form": "날씨도 모르고 잇니 ?",
          "original_form": "날씨도 모르고 잇니 ?",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003991.1.4",
          "form": "커튼 때문에 안보여",
          "original_form": "커튼 때문에 안보여",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003991.1.5",
          "form": "보슬비 오고 있는줄 알았네",
          "original_form": "보슬비 오고 있는줄 알았네",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003991.1.6",
          "form": "뉴스는 좀 보고",
          "original_form": "뉴스는 좀 보고",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003991.1.7",
          "form": "티비로는 잘 안챙겨보고",
          "original_form": "티비로는 잘 안챙겨보고",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003991.1.8",
          "form": "핸드폰으로는 보는데",
          "original_form": "핸드폰으로는 보는데",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003991.1.9",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003991.1.10",
          "form": "몇일동안 온다고 했다며 ?",
          "original_form": "몇일동안 온다고 했다며 ?",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003991.1.11",
          "form": "주말 내내 비래",
          "original_form": "주말 내내 비래",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003991.1.12",
          "form": "내일 약속 있어서 나가 봐야 하는데 얼마나 올려나",
          "original_form": "내일 약속 있어서 나가 봐야 하는데 얼마나 올려나",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003991.1.13",
          "form": "어딜가니 누구 만날려고",
          "original_form": "어딜가니 누구 만날려고",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003991.1.14",
          "form": "친구가 결혼한다고 만나자고 하네",
          "original_form": "친구가 결혼한다고 만나자고 하네",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003991.1.15",
          "form": "만나야 하는거니",
          "original_form": "만나야 하는거니",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003991.1.16",
          "form": "낼 아니면 언제보겠어",
          "original_form": "낼 아니면 언제보겠어",
          "speaker_id": "1",
          "time": "20210821 14:34"
        }
      ]
