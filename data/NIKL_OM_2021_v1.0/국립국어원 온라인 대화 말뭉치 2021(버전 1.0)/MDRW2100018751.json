      "utterance": [
        {
          "id": "MDRW2100018751.1.1",
          "form": "식사 하시고 항상 커피 드세요?",
          "original_form": "식사 하시고 항상 커피 드세요?",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018751.1.2",
          "form": "네 커피를 좋아하지는 않는데",
          "original_form": "네 커피를 좋아하지는 않는데",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018751.1.3",
          "form": "이제 루틴이 돼서 없으면 허전해요ㅎ",
          "original_form": "이제 루틴이 돼서 없으면 허전해요ㅎ",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018751.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018751.1.5",
          "form": "저는 아직 커피 맛을 모르겠어요...",
          "original_form": "저는 아직 커피 맛을 모르겠어요...",
          "speaker_id": "2",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018751.1.6",
          "form": "졸리면 어쩔 수 없이 마실 수밖에 없지만요...",
          "original_form": "졸리면 어쩔 수 없이 마실 수밖에 없지만요...",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018751.1.7",
          "form": "ㅎㅎ 카페인이 잘 듣는 편인가봐요",
          "original_form": "ㅎㅎ 카페인이 잘 듣는 편인가봐요",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018751.1.8",
          "form": "마시면 정말 잠이 좀 가시나요?",
          "original_form": "마시면 정말 잠이 좀 가시나요?",
          "speaker_id": "1",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018751.1.9",
          "form": "네니오... 이상하게 때에 따라 다르더라구요",
          "original_form": "네니오... 이상하게 때에 따라 다르더라구요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018751.1.10",
          "form": "점심시간이네요!",
          "original_form": "점심시간이네요!",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018751.1.11",
          "form": "맛점하세요~!",
          "original_form": "맛점하세요~!",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018751.1.12",
          "form": "네 콩국수 맛있게 드세요^^",
          "original_form": "네 콩국수 맛있게 드세요^^",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018751.1.13",
          "form": "커피 뭐 드셨어요?",
          "original_form": "커피 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018751.1.14",
          "form": "저는 도저히 잠 이 안 깨서 어쩔 수 없이 아메리카노 샀어요",
          "original_form": "저는 도저히 잠 이 안 깨서 어쩔 수 없이 아메리카노 샀어요",
          "speaker_id": "2",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018751.1.15",
          "form": "식곤증이 밀려올 때죠ㅜ",
          "original_form": "식곤증이 밀려올 때죠ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018751.1.16",
          "form": "저는 간만에 라떼 마셨어요",
          "original_form": "저는 간만에 라떼 마셨어요",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018751.1.17",
          "form": "폴바셋에 왔으니 아무래도 라떼를 마셔줘야ㅎ",
          "original_form": "폴바셋에 왔으니 아무래도 라떼를 마셔줘야ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018751.1.18",
          "form": "헉헉 저 폴바셋",
          "original_form": "헉헉 저 폴바셋",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018751.1.19",
          "form": "아이스크림라떼 최애에요...",
          "original_form": "아이스크림라떼 최애에요...",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018751.1.20",
          "form": "진짜 폴바셋 라떼 잘 하는 것 같아요",
          "original_form": "진짜 폴바셋 라떼 잘 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018751.1.21",
          "form": "어 아이스크림라떼라는 메뉴도 있군요!",
          "original_form": "어 아이스크림라떼라는 메뉴도 있군요!",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018751.1.22",
          "form": "아이스크림 따로 라떼 따로 먹었는데 합친 거라니",
          "original_form": "아이스크림 따로 라떼 따로 먹었는데 합친 거라니",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018751.1.23",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018751.1.24",
          "form": "폴바셋이 유제품 명가죠ㅎㅎ",
          "original_form": "폴바셋이 유제품 명가죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018751.1.25",
          "form": "회사 근처 카페는 스벅 외엔 그나마 폴바셋이 대항마같아요",
          "original_form": "회사 근처 카페는 스벅 외엔 그나마 폴바셋이 대항마같아요",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018751.1.26",
          "form": "탐탐은 텅텅ㅜ",
          "original_form": "탐탐은 텅텅ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018751.1.27",
          "form": "탐탐은 진짜 안 간 지 오래 된 것 같아요.",
          "original_form": "탐탐은 진짜 안 간 지 오래 된 것 같아요.",
          "speaker_id": "2",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018751.1.28",
          "form": "아이스크림만 팔기도 하는군요",
          "original_form": "아이스크림만 팔기도 하는군요",
          "speaker_id": "2",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018751.1.29",
          "form": "첨 알았어요",
          "original_form": "첨 알았어요",
          "speaker_id": "2",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018751.1.30",
          "form": "그래도 전 탐탐 프레즐 좋아하는데...사람들이 몰라줘 아쉽",
          "original_form": "그래도 전 탐탐 프레즐 좋아하는데...사람들이 몰라줘 아쉽",
          "speaker_id": "1",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018751.1.31",
          "form": "아 전 폴바셋을 아이스크림으로 첨 알았어요",
          "original_form": "아 전 폴바셋을 아이스크림으로 첨 알았어요",
          "speaker_id": "1",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018751.1.32",
          "form": "가격도 착해요ㅎ",
          "original_form": "가격도 착해요ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018751.1.33",
          "form": "프레즐... 잊지 않기로... 메모해둬야겠어요",
          "original_form": "프레즐... 잊지 않기로... 메모해둬야겠어요",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018751.1.34",
          "form": "아이스크림만 판다니",
          "original_form": "아이스크림만 판다니",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018751.1.35",
          "form": "그걸로 탑 쌓아 먹고 싶네요",
          "original_form": "그걸로 탑 쌓아 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018751.1.36",
          "form": "부드러운 풍미가 아이스크림 세손가락안에 듭니다 ㅎㅎ",
          "original_form": "부드러운 풍미가 아이스크림 세손가락안에 듭니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018751.1.37",
          "form": "탐탐 이탈리아프레즐은 완전 피자 같아요",
          "original_form": "탐탐 이탈리아프레즐은 완전 피자 같아요",
          "speaker_id": "1",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018751.1.38",
          "form": "끼니떼우기도 좋으니 드셔보세요ㅎ",
          "original_form": "끼니떼우기도 좋으니 드셔보세요ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018751.1.39",
          "form": "주문들어가면 알바생이 일일이 제조들어가는거라 퀄리티 높아요",
          "original_form": "주문들어가면 알바생이 일일이 제조들어가는거라 퀄리티 높아요",
          "speaker_id": "1",
          "time": "20210827 12:59"
        }
      ]
