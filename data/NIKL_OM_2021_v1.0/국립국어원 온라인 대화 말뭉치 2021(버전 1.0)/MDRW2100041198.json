      "utterance": [
        {
          "id": "MDRW2100041198.1.1",
          "form": "저녁식사 하셨어요?",
          "original_form": "저녁식사 하셨어요?",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100041198.1.2",
          "form": "굽네치킨 시켜먹엇어요",
          "original_form": "굽네치킨 시켜먹엇어요",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100041198.1.3",
          "form": "맛있었나요? 전 좀 맵더라구요",
          "original_form": "맛있었나요? 전 좀 맵더라구요",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100041198.1.4",
          "form": "저두 사실 별로 자극적인 맛 안좋아해서",
          "original_form": "저두 사실 별로 자극적인 맛 안좋아해서",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100041198.1.5",
          "form": "잘 안시켜먹는데 ㅌㅋㅋ",
          "original_form": "잘 안시켜먹는데 ㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100041198.1.6",
          "form": "양은 적은 편이덜라구요",
          "original_form": "양은 적은 편이덜라구요",
          "speaker_id": "1",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100041198.1.7",
          "form": "ㅋㅋㅋㅋ저희집은 치킨 막 되게 많이는 못먹어서",
          "original_form": "ㅋㅋㅋㅋ저희집은 치킨 막 되게 많이는 못먹어서",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041198.1.8",
          "form": "한마리 3명이서 겨우 먹는 편이라",
          "original_form": "한마리 3명이서 겨우 먹는 편이라",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041198.1.9",
          "form": "그럼 양은 괜찮았겠네요",
          "original_form": "그럼 양은 괜찮았겠네요",
          "speaker_id": "1",
          "time": "20210906 06:52"
        },
        {
          "id": "MDRW2100041198.1.10",
          "form": "저는 보고 둘이 먹을 양이다 생각했어요",
          "original_form": "저는 보고 둘이 먹을 양이다 생각했어요",
          "speaker_id": "1",
          "time": "20210906 06:53"
        },
        {
          "id": "MDRW2100041198.1.11",
          "form": "네네 ㅋㅋㅋ 적당햇어요!!",
          "original_form": "네네 ㅋㅋㅋ 적당햇어요!!",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041198.1.12",
          "form": "콜라를 너무 큰 걸 주네요",
          "original_form": "콜라를 너무 큰 걸 주네요",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041198.1.13",
          "form": "1리터 넘는거 주더라구요",
          "original_form": "1리터 넘는거 주더라구요",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041198.1.14",
          "form": "보통 배달음식 잘 시켜드세요?",
          "original_form": "보통 배달음식 잘 시켜드세요?",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041198.1.15",
          "form": "ㅋㅋㅋㅋ코로나 전까진 진짜 거의 안 시켜먹었는데",
          "original_form": "ㅋㅋㅋㅋ코로나 전까진 진짜 거의 안 시켜먹었는데",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041198.1.16",
          "form": "식당을 잘 못가면서부터 시켜먹게됐어요",
          "original_form": "식당을 잘 못가면서부터 시켜먹게됐어요",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041198.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100041198.1.18",
          "form": "식당 안가본지 꽤 됐네요",
          "original_form": "식당 안가본지 꽤 됐네요",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100041198.1.19",
          "form": "저도 코로나 이후로 식당을 못가고..",
          "original_form": "저도 코로나 이후로 식당을 못가고..",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041198.1.20",
          "form": "가끔 픽업은 해봤네요",
          "original_form": "가끔 픽업은 해봤네요",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041198.1.21",
          "form": "배달앱을 안깔아서요",
          "original_form": "배달앱을 안깔아서요",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041198.1.22",
          "form": "ㅋㅋㅋ오 현명하십니당",
          "original_form": "ㅋㅋㅋ오 현명하십니당",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100041198.1.23",
          "form": "저도 배달 끊고싶어요",
          "original_form": "저도 배달 끊고싶어요",
          "speaker_id": "2",
          "time": "20210906 19:36"
        }
      ]
