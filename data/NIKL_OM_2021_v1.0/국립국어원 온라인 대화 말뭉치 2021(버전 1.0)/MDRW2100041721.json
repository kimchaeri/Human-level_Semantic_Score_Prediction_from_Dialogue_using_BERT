      "utterance": [
        {
          "id": "MDRW2100041721.1.1",
          "form": "자꾸 생각 안나 드나??",
          "original_form": "자꾸 생각 안나 드나??",
          "speaker_id": "2",
          "time": "20210906 14:56"
        },
        {
          "id": "MDRW2100041721.1.2",
          "form": "응응 ㅋㅋ 괜찮더라 ㅋㅋ",
          "original_form": "응응 ㅋㅋ 괜찮더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100041721.1.3",
          "form": "나는 그카고 집에 오면 아 더 살 걸 생각 드는데ㅋㅋㅋㅋ",
          "original_form": "나는 그카고 집에 오면 아 더 살 걸 생각 드는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100041721.1.4",
          "form": "하나 괜찮은거 사서 그런지 미련도 없음",
          "original_form": "하나 괜찮은거 사서 그런지 미련도 없음",
          "speaker_id": "1",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100041721.1.5",
          "form": "푸핫 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "푸핫 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100041721.1.6",
          "form": "근데 애들이 진자 금방크잖아",
          "original_form": "근데 애들이 진자 금방크잖아",
          "speaker_id": "1",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100041721.1.7",
          "form": "나는 비싸게 준 옷들은 왜 그렇게 아까운지몰라. 차라리 그 돈으로 그냥 다른걸 사주는게 낫다 생각이 들거든",
          "original_form": "나는 비싸게 준 옷들은 왜 그렇게 아까운지몰라. 차라리 그 돈으로 그냥 다른걸 사주는게 낫다 생각이 들거든",
          "speaker_id": "1",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100041721.1.8",
          "form": "사람마다 소비하는게 다르니깐 뭐 ㅋㅋ",
          "original_form": "사람마다 소비하는게 다르니깐 뭐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:58"
        },
        {
          "id": "MDRW2100041721.1.9",
          "form": "맞다 그래~",
          "original_form": "맞다 그래~",
          "speaker_id": "2",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100041721.1.10",
          "form": "사람마다 다 돈 들이는 게 다르다ㅋㅋㅋㅋ",
          "original_form": "사람마다 다 돈 들이는 게 다르다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100041721.1.11",
          "form": "우리는 먹는 거에ㅋㅋㅋㅋ",
          "original_form": "우리는 먹는 거에ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100041721.1.12",
          "form": "ㅋㅋㅋㅋㅋ맞어 우리도 먹는거에 돈 많이 쓴다",
          "original_form": "ㅋㅋㅋㅋㅋ맞어 우리도 먹는거에 돈 많이 쓴다",
          "speaker_id": "1",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041721.1.13",
          "form": "장난아니게 깨짐 ㅠ",
          "original_form": "장난아니게 깨짐 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041721.1.14",
          "form": "그래 근데 우리 신랑은 옷에 돈 쓴다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 근데 우리 신랑은 옷에 돈 쓴다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041721.1.15",
          "form": "그래서 우린 둘 다 나간다ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 우린 둘 다 나간다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100041721.1.16",
          "form": "정말 아끼고 싶다ㅠ-",
          "original_form": "정말 아끼고 싶다ㅠ-",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041721.1.17",
          "form": "나도 나도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 나도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041721.1.18",
          "form": "아 웃겨",
          "original_form": "아 웃겨",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041721.1.19",
          "form": "이게 진짜 내 혼자 아낀다고 되는 게 아님ㅠ",
          "original_form": "이게 진짜 내 혼자 아낀다고 되는 게 아님ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041721.1.20",
          "form": "아무도 협조를 안 해준다ㅋㅋㅋㅋ",
          "original_form": "아무도 협조를 안 해준다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041721.1.21",
          "form": "이러다가 나도 고삐 풀리고ㅋㅋㅋㅋ",
          "original_form": "이러다가 나도 고삐 풀리고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100041721.1.22",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100041721.1.23",
          "form": "나도 사실은 요즘에 막 아끼고 있거든",
          "original_form": "나도 사실은 요즘에 막 아끼고 있거든",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100041721.1.24",
          "form": "근데 또 언제 고삐가 풀릴지 모르지",
          "original_form": "근데 또 언제 고삐가 풀릴지 모르지",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100041721.1.25",
          "form": "진짜 한번씩 막 소비하고싶을때가 많이 오잖아 ㅋㅋ그땐 막 산다",
          "original_form": "진짜 한번씩 막 소비하고싶을때가 많이 오잖아 ㅋㅋ그땐 막 산다",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100041721.1.26",
          "form": "내 쟁이는게 봤제??ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내 쟁이는게 봤제??ㅋㅋㅋㅋㅋㅋㅋㅋ",
