      "utterance": [
        {
          "id": "MDRW2100013201.1.1",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013201.1.2",
          "form": "네  중국집에서 짜장면이요",
          "original_form": "네  중국집에서 짜장면이요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013201.1.3",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013201.1.4",
          "form": "네 순대국이요",
          "original_form": "네 순대국이요",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013201.1.5",
          "form": "혼밥의 성지에 가셨군요",
          "original_form": "혼밥의 성지에 가셨군요",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013201.1.6",
          "form": "혼밥 하신거에요?",
          "original_form": "혼밥 하신거에요?",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013201.1.7",
          "form": "포장이요 ㅎ",
          "original_form": "포장이요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013201.1.8",
          "form": "순대국은 혼자먹기도 좋던데요 ㅎ",
          "original_form": "순대국은 혼자먹기도 좋던데요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013201.1.9",
          "form": "혼자드실일 많지 않나요? ㅎ",
          "original_form": "혼자드실일 많지 않나요? ㅎ",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013201.1.10",
          "form": "전 평일은 거의 혼밥이요",
          "original_form": "전 평일은 거의 혼밥이요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013201.1.11",
          "form": "오늘은 친구랑 먹었어요. ㅎ",
          "original_form": "오늘은 친구랑 먹었어요. ㅎ",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013201.1.12",
          "form": "수다 떨며 좋았겠어요",
          "original_form": "수다 떨며 좋았겠어요",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013201.1.13",
          "form": "배달은 자주하시나요?",
          "original_form": "배달은 자주하시나요?",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013201.1.14",
          "form": "삼실에서 배달은 한 달에 한 두 번 정도요.",
          "original_form": "삼실에서 배달은 한 달에 한 두 번 정도요.",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013201.1.15",
          "form": "맛집은 자주가시나요?",
          "original_form": "맛집은 자주가시나요?",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013201.1.16",
          "form": "블로그 찾아보고 아이들 데리고 한 번 가보긴 했어요.",
          "original_form": "블로그 찾아보고 아이들 데리고 한 번 가보긴 했어요.",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013201.1.17",
          "form": "최근에 고기 집 함 가봤는데 블로그 처럼 엄청 맛있다는 건 모르겠더라구요",
          "original_form": "최근에 고기 집 함 가봤는데 블로그 처럼 엄청 맛있다는 건 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013201.1.18",
          "form": "광곤가보네요 ㅜ",
          "original_form": "광곤가보네요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013201.1.19",
          "form": "외식안한지도 오래됐네요",
          "original_form": "외식안한지도 오래됐네요",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013201.1.20",
          "form": "광고 같진 않은데 고기도 식어서 나오고 전 그닥 이였는데 애들은 맛있다고 또 가자고 하던데요",
          "original_form": "광고 같진 않은데 고기도 식어서 나오고 전 그닥 이였는데 애들은 맛있다고 또 가자고 하던데요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013201.1.21",
          "form": "숯불고기가 좋더라고요",
          "original_form": "숯불고기가 좋더라고요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013201.1.22",
          "form": "네 제가 간데는 석갈비라고 초벌로 구워져 나오는 곳 이었어요",
          "original_form": "네 제가 간데는 석갈비라고 초벌로 구워져 나오는 곳 이었어요",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013201.1.23",
          "form": "초벌이면 또 익혀야하는거아닌가요?",
          "original_form": "초벌이면 또 익혀야하는거아닌가요?",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013201.1.24",
          "form": "인덕션에 데워먹는 거더라구요. 데워도 별로 안뜨거워져서 고기가 질기더라구요",
          "original_form": "인덕션에 데워먹는 거더라구요. 데워도 별로 안뜨거워져서 고기가 질기더라구요",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013201.1.25",
          "form": "별로네요 유명한곳 같은데",
          "original_form": "별로네요 유명한곳 같은데",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013201.1.26",
          "form": "제가 느끼기엔 그랬는데 그날만 그랬던 건지.. 사람들은 많이 가더라구요",
          "original_form": "제가 느끼기엔 그랬는데 그날만 그랬던 건지.. 사람들은 많이 가더라구요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013201.1.27",
          "form": "바빠서 그런걸수도 있겠네요",
          "original_form": "바빠서 그런걸수도 있겠네요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013201.1.28",
          "form": "3시쯤 가서 그랬나봐요",
          "original_form": "3시쯤 가서 그랬나봐요",
          "speaker_id": "2",
          "time": "20210825 19:16"
        }
      ]
    }
