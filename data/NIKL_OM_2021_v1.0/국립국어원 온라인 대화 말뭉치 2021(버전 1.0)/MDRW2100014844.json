      "utterance": [
        {
          "id": "MDRW2100014844.1.1",
          "form": "안녕하세요! 저녁 드셨나요?",
          "original_form": "안녕하세요! 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100014844.1.2",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100014844.1.3",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100014844.1.4",
          "form": "네 저도 먹었어요!",
          "original_form": "네 저도 먹었어요!",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100014844.1.5",
          "form": "맛있는 거 드셨나요?",
          "original_form": "맛있는 거 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100014844.1.6",
          "form": "사온 반찬에 먹었어요",
          "original_form": "사온 반찬에 먹었어요",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100014844.1.7",
          "form": "저는 양념 갈비 구워 먹었어요!",
          "original_form": "저는 양념 갈비 구워 먹었어요!",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100014844.1.8",
          "form": "갈비 좋아하시나요?",
          "original_form": "갈비 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100014844.1.9",
          "form": "좋아해요! 맛있었겠네요",
          "original_form": "좋아해요! 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100014844.1.10",
          "form": "잘못하면 금방 타잖아요",
          "original_form": "잘못하면 금방 타잖아요",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100014844.1.11",
          "form": "맞아요ㅠㅜ 그래서 불을 좀 약하게 해서 구웠어요",
          "original_form": "맞아요ㅠㅜ 그래서 불을 좀 약하게 해서 구웠어요",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100014844.1.12",
          "form": "반찬은 보통 사서 드시나요?",
          "original_form": "반찬은 보통 사서 드시나요?",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100014844.1.13",
          "form": "네. 요리를 잘 못하다 보니ㅎㅎ",
          "original_form": "네. 요리를 잘 못하다 보니ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100014844.1.14",
          "form": "요리 잘 하세요?",
          "original_form": "요리 잘 하세요?",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100014844.1.15",
          "form": "저도 잘 못하는데 할머니가 자주 해주세요",
          "original_form": "저도 잘 못하는데 할머니가 자주 해주세요",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100014844.1.16",
          "form": "어떤 반찬 제일 좋아하세요?",
          "original_form": "어떤 반찬 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100014844.1.17",
          "form": "오징어 젓갈이요",
          "original_form": "오징어 젓갈이요",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100014844.1.18",
          "form": "흰밥이랑 금방 먹어요ㅋㅋ",
          "original_form": "흰밥이랑 금방 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100014844.1.19",
          "form": "오징어 젓갈 맛있죠! 젓갈 양념 너무 맛있어요ㅋㅋㅋ 밥이 술술 들어가요",
          "original_form": "오징어 젓갈 맛있죠! 젓갈 양념 너무 맛있어요ㅋㅋㅋ 밥이 술술 들어가요",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100014844.1.20",
          "form": "맞아요. 젓갈은 다 맛있더라구요ㅋㅋ",
          "original_form": "맞아요. 젓갈은 다 맛있더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100014844.1.21",
          "form": "간단하게 간장이랑 계란 비벼먹는것두 맛있어요",
          "original_form": "간단하게 간장이랑 계란 비벼먹는것두 맛있어요",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100014844.1.22",
          "form": "간장계란밥 간단한데 너무 맛있어요!",
          "original_form": "간장계란밥 간단한데 너무 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 21:14"
        }
      ]
