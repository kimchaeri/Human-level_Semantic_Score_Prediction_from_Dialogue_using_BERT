      "utterance": [
        {
          "id": "MDRW2100039700.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039700.1.2",
          "form": "뭐 드셨나용",
          "original_form": "뭐 드셨나용",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039700.1.3",
          "form": "부대찌개먹었어요",
          "original_form": "부대찌개먹었어요",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039700.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039700.1.5",
          "form": "만드신건가요?",
          "original_form": "만드신건가요?",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039700.1.6",
          "form": "저는 땅땅치킨",
          "original_form": "저는 땅땅치킨",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039700.1.7",
          "form": "아뇨 밀키트요 ㅎ",
          "original_form": "아뇨 밀키트요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039700.1.8",
          "form": "땅땅치킨 맛있나요?",
          "original_form": "땅땅치킨 맛있나요?",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039700.1.9",
          "form": "오 요즘 밀키트 잘나오더라구요",
          "original_form": "오 요즘 밀키트 잘나오더라구요",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039700.1.10",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039700.1.11",
          "form": "모든 치킨이 먹으면 느끼해서 ㅎㅎ",
          "original_form": "모든 치킨이 먹으면 느끼해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039700.1.12",
          "form": "먹을만해요",
          "original_form": "먹을만해요",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039700.1.13",
          "form": "오 땅땅 안먹어봐서",
          "original_form": "오 땅땅 안먹어봐서",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039700.1.14",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039700.1.15",
          "form": "무슨 치킨드셨어요?",
          "original_form": "무슨 치킨드셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039700.1.16",
          "form": "후라이드?",
          "original_form": "후라이드?",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039700.1.17",
          "form": "오븐반 간장치킨반 세트에요 ㅎㅎ",
          "original_form": "오븐반 간장치킨반 세트에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039700.1.18",
          "form": "리뷰 떡볶이 도 주네요",
          "original_form": "리뷰 떡볶이 도 주네요",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039700.1.19",
          "form": "오 요즘은 그렇게도 해주나보네요",
          "original_form": "오 요즘은 그렇게도 해주나보네요",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039700.1.20",
          "form": "떡볶이는 맛있나요?",
          "original_form": "떡볶이는 맛있나요?",
          "speaker_id": "1",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039700.1.21",
          "form": "세트 라서 그런가바요",
          "original_form": "세트 라서 그런가바요",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039700.1.22",
          "form": "세트가 가성비 좋죠",
          "original_form": "세트가 가성비 좋죠",
          "speaker_id": "1",
          "time": "20210903 20:47"
        }
