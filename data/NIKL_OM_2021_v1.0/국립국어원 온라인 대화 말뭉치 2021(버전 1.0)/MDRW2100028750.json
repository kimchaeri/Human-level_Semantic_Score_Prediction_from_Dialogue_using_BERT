      "utterance": [
        {
          "id": "MDRW2100028750.1.1",
          "form": "좋아하는 책 장르가 뭐야?",
          "original_form": "좋아하는 책 장르가 뭐야?",
          "speaker_id": "1",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100028750.1.2",
          "form": "스릴러 미스테리 추리 좋아하지",
          "original_form": "스릴러 미스테리 추리 좋아하지",
          "speaker_id": "2",
          "time": "20210903 14:39"
        },
        {
          "id": "MDRW2100028750.1.3",
          "form": "넌 뭐 좋아하는데?",
          "original_form": "넌 뭐 좋아하는데?",
          "speaker_id": "2",
          "time": "20210903 14:39"
        },
        {
          "id": "MDRW2100028750.1.4",
          "form": "난 스릴러랑 판타지",
          "original_form": "난 스릴러랑 판타지",
          "speaker_id": "1",
          "time": "20210903 14:41"
        },
        {
          "id": "MDRW2100028750.1.5",
          "form": "판타지 넘 좋아 섀도우 앤 본 추천한다",
          "original_form": "판타지 넘 좋아 섀도우 앤 본 추천한다",
          "speaker_id": "2",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100028750.1.6",
          "form": "한 번 읽어볼게",
          "original_form": "한 번 읽어볼게",
          "speaker_id": "1",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100028750.1.7",
          "form": "아 드라마로도 나왔어 그만큼 재미가 있는 책이란 얘기지 ㅎㅎ",
          "original_form": "아 드라마로도 나왔어 그만큼 재미가 있는 책이란 얘기지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100028750.1.8",
          "form": "아 우리 책 반납일 9일 맞지??",
          "original_form": "아 우리 책 반납일 9일 맞지??",
          "speaker_id": "2",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100028750.1.9",
          "form": "9일 맞아",
          "original_form": "9일 맞아",
          "speaker_id": "1",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100028750.1.10",
          "form": "혹시 해리포터 읽어봤어?",
          "original_form": "혹시 해리포터 읽어봤어?",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100028750.1.11",
          "form": "해리포터 당연하지 이미 여러번 읽었지",
          "original_form": "해리포터 당연하지 이미 여러번 읽었지",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100028750.1.12",
          "form": "이건 왜 물어봐",
          "original_form": "이건 왜 물어봐",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100028750.1.13",
          "form": "기억력 좋아지는 책 읽어야 할 것 같아",
          "original_form": "기억력 좋아지는 책 읽어야 할 것 같아",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100028750.1.14",
          "form": "잘못 물어봤어. 빌려온 책은 다 읽었어?",
          "original_form": "잘못 물어봤어. 빌려온 책은 다 읽었어?",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100028750.1.15",
          "form": "아직 1권만 읽는 중이야",
          "original_form": "아직 1권만 읽는 중이야",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100028750.1.16",
          "form": "넌 다 읽었어??",
          "original_form": "넌 다 읽었어??",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100028750.1.17",
          "form": "아니.아직 2권밖에 못읽었어",
          "original_form": "아니.아직 2권밖에 못읽었어",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100028750.1.18",
          "form": "몇 권 빌렸더라? 3권??",
          "original_form": "몇 권 빌렸더라? 3권??",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100028750.1.19",
          "form": "4권 빌렸어",
          "original_form": "4권 빌렸어",
          "speaker_id": "1",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100028750.1.20",
          "form": "아 반절이나 읽었네 난 1권도 못 읽고...",
          "original_form": "아 반절이나 읽었네 난 1권도 못 읽고...",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100028750.1.21",
          "form": "9일에 반납하러 갈거야?",
          "original_form": "9일에 반납하러 갈거야?",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100028750.1.22",
          "form": "당연히 가지",
          "original_form": "당연히 가지",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100028750.1.23",
          "form": "못 읽으면 어떡하지? ㅠ 빨리 읽어야 겠다",
          "original_form": "못 읽으면 어떡하지? ㅠ 빨리 읽어야 겠다",
          "speaker_id": "2",
          "time": "20210903 15:58"
        }
      ]
