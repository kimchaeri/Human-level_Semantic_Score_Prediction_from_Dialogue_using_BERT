      "utterance": [
        {
          "id": "MDRW2100027149.1.1",
          "form": "오...그럼 가족이랑 같이 사시나봐요",
          "original_form": "오...그럼 가족이랑 같이 사시나봐요",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100027149.1.2",
          "form": "저도 저까지 가족구성원이 5명...",
          "original_form": "저도 저까지 가족구성원이 5명...",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100027149.1.3",
          "form": "저는 4명이요",
          "original_form": "저는 4명이요",
          "speaker_id": "2",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100027149.1.4",
          "form": "남동생 하나 있어요",
          "original_form": "남동생 하나 있어요",
          "speaker_id": "2",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100027149.1.5",
          "form": "아 저는 누나 두명있어요",
          "original_form": "아 저는 누나 두명있어요",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027149.1.6",
          "form": "집안일 정말 안해여...",
          "original_form": "집안일 정말 안해여...",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027149.1.7",
          "form": "ㅋㅋㅋㅋㅋ 집안일을 안하신다니",
          "original_form": "ㅋㅋㅋㅋㅋ 집안일을 안하신다니",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027149.1.8",
          "form": "너무하시네요",
          "original_form": "너무하시네요",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027149.1.9",
          "form": "지들 방만치우는데 그것도 자주안해여",
          "original_form": "지들 방만치우는데 그것도 자주안해여",
          "speaker_id": "1",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027149.1.10",
          "form": "방바닥도 하얀색이라 머리털 엄청 뿜고",
          "original_form": "방바닥도 하얀색이라 머리털 엄청 뿜고",
          "speaker_id": "1",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027149.1.11",
          "form": "머리카락 흘리는건 진짜 어쩔 수 없지만",
          "original_form": "머리카락 흘리는건 진짜 어쩔 수 없지만",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027149.1.12",
          "form": "안치우는건 진짜 아니네요",
          "original_form": "안치우는건 진짜 아니네요",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027149.1.13",
          "form": "으",
          "original_form": "으",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027149.1.14",
          "form": "약간 둘다 일하느라 바쁜건 알겠는데 자기먹은건 좀 버렸으면 좋겠어요",
          "original_form": "약간 둘다 일하느라 바쁜건 알겠는데 자기먹은건 좀 버렸으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027149.1.15",
          "form": "두 분 다 직장인이세요?",
          "original_form": "두 분 다 직장인이세요?",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027149.1.16",
          "form": "네 둘다 회사다녀요",
          "original_form": "네 둘다 회사다녀요",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027149.1.17",
          "form": "저는 아직 취준생이고 코로나 때매 독서실도 못가서 집에있다보니",
          "original_form": "저는 아직 취준생이고 코로나 때매 독서실도 못가서 집에있다보니",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027149.1.18",
          "form": "자연스럽게 집안일은 저의몫...",
          "original_form": "자연스럽게 집안일은 저의몫...",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027149.1.19",
          "form": "취준생이신데 집안일 하는건 진짜 힘들겠어요",
          "original_form": "취준생이신데 집안일 하는건 진짜 힘들겠어요",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027149.1.20",
          "form": "어쩔수없죠ㅠㅠ 화장실청소만 안하고 싶어요",
          "original_form": "어쩔수없죠ㅠㅠ 화장실청소만 안하고 싶어요",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027149.1.21",
          "form": "그건 저도요",
          "original_form": "그건 저도요",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027149.1.22",
          "form": "일부러 다른 청소 도맡아서 하고 있어요",
          "original_form": "일부러 다른 청소 도맡아서 하고 있어요",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027149.1.23",
          "form": "그니까요 냄새나는건 어쩔수없는데 락스칠하는게 힘든거같아요",
          "original_form": "그니까요 냄새나는건 어쩔수없는데 락스칠하는게 힘든거같아요",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027149.1.24",
          "form": "맞아요 락스 냄새도 독하고",
          "original_form": "맞아요 락스 냄새도 독하고",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027149.1.25",
          "form": "옷에묻으면 또 안지워지구",
          "original_form": "옷에묻으면 또 안지워지구",
          "speaker_id": "1",
          "time": "20210902 10:24"
        }
      ]
    }
  ]
