      "utterance": [
        {
          "id": "MDRW2100044797.1.1",
          "form": "ㅋㅋㅋㅋ그건 인정",
          "original_form": "ㅋㅋㅋㅋ그건 인정",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044797.1.2",
          "form": "동남아 음식 잘먹는 편?",
          "original_form": "동남아 음식 잘먹는 편?",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044797.1.3",
          "form": "오 향신료 완전 못먹음",
          "original_form": "오 향신료 완전 못먹음",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044797.1.4",
          "form": "나 쌀국수 국물도 뮷머금",
          "original_form": "나 쌀국수 국물도 뮷머금",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044797.1.5",
          "form": "ㅋㅋㅋㅋ난 현지화된 거만 가능",
          "original_form": "ㅋㅋㅋㅋ난 현지화된 거만 가능",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044797.1.6",
          "form": "베트남 쌀국수가 그렇게 맛있다미...",
          "original_form": "베트남 쌀국수가 그렇게 맛있다미...",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044797.1.7",
          "form": "ㅋㅋㅋㅋㅋ그렇군",
          "original_form": "ㅋㅋㅋㅋㅋ그렇군",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044797.1.8",
          "form": "먼데 그냥쌀국수 아니여??",
          "original_form": "먼데 그냥쌀국수 아니여??",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044797.1.9",
          "form": "다른거??",
          "original_form": "다른거??",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044797.1.10",
          "form": "그 국가마다 좀 다르대",
          "original_form": "그 국가마다 좀 다르대",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044797.1.11",
          "form": "오 맛이다르구나",
          "original_form": "오 맛이다르구나",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044797.1.12",
          "form": "일식은 조아해?",
          "original_form": "일식은 조아해?",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044797.1.13",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044797.1.14",
          "form": "혹시 싫어하니",
          "original_form": "혹시 싫어하니",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044797.1.15",
          "form": "초밥 무슨 초밥조아해",
          "original_form": "초밥 무슨 초밥조아해",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044797.1.16",
          "form": "나 조아해",
          "original_form": "나 조아해",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044797.1.17",
          "form": "흠 생연어?",
          "original_form": "흠 생연어?",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044797.1.18",
          "form": "두툼쓰",
          "original_form": "두툼쓰",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044797.1.19",
          "form": "오 연어초밪조아하는구나",
          "original_form": "오 연어초밪조아하는구나",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044797.1.20",
          "form": "연어10개 다 먹어?",
          "original_form": "연어10개 다 먹어?",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044797.1.21",
          "form": "아 노노 그건 아님ㅋㅋㅋ",
          "original_form": "아 노노 그건 아님ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044797.1.22",
          "form": "ㅋㅋㅋㅋ좀 느끼하지",
          "original_form": "ㅋㅋㅋㅋ좀 느끼하지",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044797.1.23",
          "form": "반 구우면 10개 가능",
          "original_form": "반 구우면 10개 가능",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044797.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇군 나는 광어 조아함",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇군 나는 광어 조아함",
          "speaker_id": "1",
          "time": "20210907 23:50"
        }
