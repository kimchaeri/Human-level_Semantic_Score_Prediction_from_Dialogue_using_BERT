      "utterance": [
        {
          "id": "MDRW2100017077.1.1",
          "form": "카레 맛있게 되었네",
          "original_form": "카레 맛있게 되었네",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017077.1.2",
          "form": "그르겡",
          "original_form": "그르겡",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017077.1.3",
          "form": "엄마가 해주는 건 다 맛있지",
          "original_form": "엄마가 해주는 건 다 맛있지",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017077.1.4",
          "form": "당연하지~ㅋㅋ 카레에 뭐뭐 넣은거얌?",
          "original_form": "당연하지~ㅋㅋ 카레에 뭐뭐 넣은거얌?",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017077.1.5",
          "form": "사과도 넣고 양파도 많이 넣고 야채 많이 넣고",
          "original_form": "사과도 넣고 양파도 많이 넣고 야채 많이 넣고",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017077.1.6",
          "form": "오 사과가 들어갔구만 ㅋㅋ 새콤한게 있더니만",
          "original_form": "오 사과가 들어갔구만 ㅋㅋ 새콤한게 있더니만",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017077.1.7",
          "form": "그래 사과 넣으면 맛이 좋다",
          "original_form": "그래 사과 넣으면 맛이 좋다",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017077.1.8",
          "form": "옹 넘 맛있었옹 ㅎㅎ",
          "original_form": "옹 넘 맛있었옹 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017077.1.9",
          "form": "많이 끓여 놓았으니 내일도 먹어라",
          "original_form": "많이 끓여 놓았으니 내일도 먹어라",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017077.1.10",
          "form": "구래구램 ㅋ 좋군여",
          "original_form": "구래구램 ㅋ 좋군여",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017077.1.11",
          "form": "내일은 고기 더 넣어서 먹어야겠다",
          "original_form": "내일은 고기 더 넣어서 먹어야겠다",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017077.1.12",
          "form": "오 좋다 ㅋㅋ",
          "original_form": "오 좋다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017077.1.13",
