      "utterance": [
        {
          "id": "MDRW2100006598.1.1",
          "form": "가정폭력이나",
          "original_form": "가정폭력이나",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.2",
          "form": "강간도 있어요 책에",
          "original_form": "강간도 있어요 책에",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.3",
          "form": "흥미로운 케이스는",
          "original_form": "흥미로운 케이스는",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.4",
          "form": "옛날에는 그런거 인정받기가 어려웠을 것 같아요",
          "original_form": "옛날에는 그런거 인정받기가 어려웠을 것 같아요",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.5",
          "form": "고립된 해자와 납치범인데",
          "original_form": "고립된 해자와 납치범인데",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.7",
          "form": "피해자",
          "original_form": "피해자",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.8",
          "form": "해자?",
          "original_form": "해자?",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006598.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.10",
          "form": "사람은 타인이 필요해서",
          "original_form": "사람은 타인이 필요해서",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.11",
          "form": "사회적 동물이니까 나 랑 남이 필요하거든요",
          "original_form": "사회적 동물이니까 나 랑 남이 필요하거든요",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.12",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.13",
          "form": "그게 심리학적으로 엄청 중요하다던데",
          "original_form": "그게 심리학적으로 엄청 중요하다던데",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.14",
          "form": "이 남이 아무도 없으면 그 범인이 된데요",
          "original_form": "이 남이 아무도 없으면 그 범인이 된데요",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.15",
          "form": "네 그래서 스톡홀름같은게 일어나죠",
          "original_form": "네 그래서 스톡홀름같은게 일어나죠",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.16",
          "form": "아 그거 뭔지 알것 같아요",
          "original_form": "아 그거 뭔지 알것 같아요",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006598.1.17",
          "form": "그래요 그거 ㅋㅋㅋㅋ 이름이 이제 기억나네요",
          "original_form": "그래요 그거 ㅋㅋㅋㅋ 이름이 이제 기억나네요",
          "speaker_id": "1",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006598.1.18",
          "form": "반이나 차지한.ㄴ거예요 범인의 사고방식이",
          "original_form": "반이나 차지한.ㄴ거예요 범인의 사고방식이",
          "speaker_id": "2",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006598.1.19",
          "form": "완전 위험한데요",
          "original_form": "완전 위험한데요",
          "speaker_id": "1",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006598.1.20",
          "form": "나 빼고 상호작용할 타인이 범인 뿐이니까 영향력이 커지죠",
          "original_form": "나 빼고 상호작용할 타인이 범인 뿐이니까 영향력이 커지죠",
          "speaker_id": "2",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006598.1.21",
          "form": "그래서 다른 사람들도 범인처럼 생각할꺼라고 생각하데더ㅣㄴ데요",
          "original_form": "그래서 다른 사람들도 범인처럼 생각할꺼라고 생각하데더ㅣㄴ데요",
          "speaker_id": "2",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006598.1.22",
          "form": "재밌죠",
          "original_form": "재밌죠",
          "speaker_id": "2",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006598.1.23",
          "form": "와 그러면 탈출이 더 힘들어지겠네요",
          "original_form": "와 그러면 탈출이 더 힘들어지겠네요",
          "speaker_id": "1",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.24",
          "form": "그걸 다시 때내고 정상적인 타인과 애착을 형성하며",
          "original_form": "그걸 다시 때내고 정상적인 타인과 애착을 형성하며",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.25",
          "form": "이후에도 다른 사람들을 범인 마인드로 볼거고",
          "original_form": "이후에도 다른 사람들을 범인 마인드로 볼거고",
          "speaker_id": "1",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.26",
          "form": "회복하는게 힘든 일이래요",
          "original_form": "회복하는게 힘든 일이래요",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.27",
          "form": "내내",
          "original_form": "내내",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.28",
          "form": "그래서 피해자중에는",
          "original_form": "그래서 피해자중에는",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.29",
          "form": "그렇죠 한번 망가졌으니.. 고치기가 쉬울리가요",
          "original_form": "그렇죠 한번 망가졌으니.. 고치기가 쉬울리가요",
          "speaker_id": "1",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.30",
          "form": "범인들이 늙거나 힘이 없을때 자기집에 부르고",
          "original_form": "범인들이 늙거나 힘이 없을때 자기집에 부르고",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.31",
          "form": "병수발도 들어준대요",
          "original_form": "병수발도 들어준대요",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006598.1.32",
          "form": "가정폭력같은 케슨.ㄴ",
          "original_form": "가정폭력같은 케슨.ㄴ",
          "speaker_id": "2",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006598.1.33",
          "form": "심각하네요...",
          "original_form": "심각하네요...",
          "speaker_id": "1",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006598.1.34",
          "form": "신기하죠",
          "original_form": "신기하죠",
          "speaker_id": "2",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006598.1.35",
          "form": "엄청 보편적인 케이스에요",
          "original_form": "엄청 보편적인 케이스에요",
          "speaker_id": "2",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006598.1.36",
          "form": "책에 지뢰가 많아서",
          "original_form": "책에 지뢰가 많아서",
          "speaker_id": "1",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006598.1.37",
          "form": "심약자는 보기 힘들것 같아요",
          "original_form": "심약자는 보기 힘들것 같아요",
          "speaker_id": "1",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006598.1.38",
          "form": "범인맘에 들어서 상황을 조절하려는 시도를 한.ㄴ게",
          "original_form": "범인맘에 들어서 상황을 조절하려는 시도를 한.ㄴ게",
          "speaker_id": "2",
