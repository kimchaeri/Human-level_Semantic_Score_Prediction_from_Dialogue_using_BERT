      "utterance": [
        {
          "id": "MDRW2100007030.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007030.1.2",
          "form": "퍼즐이나 짝맞추기 이런건 종종해요",
          "original_form": "퍼즐이나 짝맞추기 이런건 종종해요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007030.1.3",
          "form": "사천성 아세요?",
          "original_form": "사천성 아세요?",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007030.1.4",
          "form": "스타 이런건 전혀 모르겠더라구요",
          "original_form": "스타 이런건 전혀 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007030.1.5",
          "form": "네네 예전에 많이했죠ㅎ",
          "original_form": "네네 예전에 많이했죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007030.1.6",
          "form": "저도 그런게임 좋아해서 예전엔 진짜 많이 했었는데",
          "original_form": "저도 그런게임 좋아해서 예전엔 진짜 많이 했었는데",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007030.1.7",
          "form": "애니팡 엄청했죠ㅋ",
          "original_form": "애니팡 엄청했죠ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007030.1.8",
          "form": "애니팡도 진짜 핫했죠 ㅎ",
          "original_form": "애니팡도 진짜 핫했죠 ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007030.1.9",
          "form": "지금 k쇼핑에 게임하는데 안빠지고 하고 있어요",
          "original_form": "지금 k쇼핑에 게임하는데 안빠지고 하고 있어요",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007030.1.10",
          "form": "저희 사장님 요즘도 하시던데요ㅋ",
          "original_form": "저희 사장님 요즘도 하시던데요ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007030.1.11",
          "form": "아 애니팡이랑 비슷하죠 그겜",
          "original_form": "아 애니팡이랑 비슷하죠 그겜",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007030.1.12",
          "form": "네 3개만 맞추면 터지는 게임",
          "original_form": "네 3개만 맞추면 터지는 게임",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007030.1.13",
          "form": "은근 중독성이 있어요",
          "original_form": "은근 중독성이 있어요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007030.1.14",
          "form": "저 애니팡중독자였어요ㅎ",
          "original_form": "저 애니팡중독자였어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007030.1.15",
          "form": "점수대박이였는데ㅋ",
          "original_form": "점수대박이였는데ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007030.1.16",
          "form": "진짜요? 잘하실듯해요",
          "original_form": "진짜요? 잘하실듯해요",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007030.1.17",
          "form": "k쇼핑에 게임도 도전해보세요",
          "original_form": "k쇼핑에 게임도 도전해보세요",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007030.1.18",
          "form": "고런 게임은 잘되더라구요",
          "original_form": "고런 게임은 잘되더라구요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007030.1.19",
          "form": "안그래도 k쇼핑겜 530등정도 찍었어요ㅎㅎ",
          "original_form": "안그래도 k쇼핑겜 530등정도 찍었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007030.1.20",
          "form": "오~~ 그럼 적립금얼마예요?",
          "original_form": "오~~ 그럼 적립금얼마예요?",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007030.1.21",
          "form": "5000원준대요",
          "original_form": "5000원준대요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007030.1.22",
          "form": "근데 점점 밀리고있어요ㅎ",
          "original_form": "근데 점점 밀리고있어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007030.1.23",
          "form": "앗 그것도 밀릴수도 있군요",
          "original_form": "앗 그것도 밀릴수도 있군요",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007030.1.24",
          "form": "우와 5000원 대단하시네요",
          "original_form": "우와 5000원 대단하시네요",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007030.1.25",
          "form": "말까지 잘 버티길ㅎㅎ",
          "original_form": "말까지 잘 버티길ㅎㅎ",
