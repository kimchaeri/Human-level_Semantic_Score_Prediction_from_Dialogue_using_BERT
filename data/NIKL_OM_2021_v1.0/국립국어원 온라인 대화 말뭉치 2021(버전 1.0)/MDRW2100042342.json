      "utterance": [
        {
          "id": "MDRW2100042342.1.1",
          "form": "집에 오면",
          "original_form": "집에 오면",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042342.1.2",
          "form": "본인도 단 게 먹고 싶으니까",
          "original_form": "본인도 단 게 먹고 싶으니까",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042342.1.3",
          "form": "초콜릿도 먹고 과자도 먹고 오예스 같은 거 막 허겁지겁 서너 개씩 막 먹거든",
          "original_form": "초콜릿도 먹고 과자도 먹고 오예스 같은 거 막 허겁지겁 서너 개씩 막 먹거든",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042342.1.4",
          "form": "그러고 나니 밥맛이 없지",
          "original_form": "그러고 나니 밥맛이 없지",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042342.1.5",
          "form": "으휴.....",
          "original_form": "으휴.....",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042342.1.6",
          "form": "악순환이네",
          "original_form": "악순환이네",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042342.1.7",
          "form": "진짜 입맛이 없는가 봐요",
          "original_form": "진짜 입맛이 없는가 봐요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042342.1.8",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.9",
          "form": "밥이 맛이 없나보다",
          "original_form": "밥이 맛이 없나보다",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.10",
          "form": "비쩍 말라가지고",
          "original_form": "비쩍 말라가지고",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.11",
          "form": "우리 첫째랑 똑같구만요",
          "original_form": "우리 첫째랑 똑같구만요",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.12",
          "form": "지코 방 오면",
          "original_form": "지코 방 오면",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.13",
          "form": "밥이 그래 먹기 싫음",
          "original_form": "밥이 그래 먹기 싫음",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.14",
          "form": "지코라고 쓰고 가라",
          "original_form": "지코라고 쓰고 가라",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.15",
          "form": "지코 방ㅋㅋㅋㅋㅋㅋ",
          "original_form": "지코 방ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.16",
          "form": "하염없이 기다릴 수 있으니ㅋㅋㅋㅋㄱ",
          "original_form": "하염없이 기다릴 수 있으니ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.17",
          "form": "그래 왜 이래 안먹겠노 ㅋㅋ",
          "original_form": "그래 왜 이래 안먹겠노 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042342.1.18",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042342.1.19",
          "form": "name3는 학교 밥이 맛없고 시간 정해져 있으니 많이 못 먹는 거 같더라고요",
          "original_form": "&name3&는 학교 밥이 맛없고 시간 정해져 있으니 많이 못 먹는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042342.1.20",
          "form": "아침은 맨날 밥달라 그래요",
          "original_form": "아침은 맨날 밥달라 그래요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042342.1.21",
          "form": "갈비뼈가 보이겠음",
          "original_form": "갈비뼈가 보이겠음",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042342.1.22",
          "form": "자기도 점심 못 먹는 거 아니까",
          "original_form": "자기도 점심 못 먹는 거 아니까",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042342.1.23",
          "form": "우린 갈비뼈가 보여요ㅋㅋㅋㅋㅋ",
          "original_form": "우린 갈비뼈가 보여요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.24",
          "form": "아 진짜 밥이 맛없는가",
          "original_form": "아 진짜 밥이 맛없는가",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.25",
          "form": "메뉴는 엄청 좋아 보이던데",
          "original_form": "메뉴는 엄청 좋아 보이던데",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.26",
          "form": "무릎이 너무 앙상하고ㅋㅋ",
          "original_form": "무릎이 너무 앙상하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.27",
          "form": "냉동식품 이런 거 많이 나오잖아요",
          "original_form": "냉동식품 이런 거 많이 나오잖아요",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.28",
          "form": "우리 name4도 종이 짝같다",
          "original_form": "우리 &name4&도 종이 짝같다",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.29",
          "form": "name3는 그런 거 잘 안 먹어요ㅠ",
          "original_form": "&name3&는 그런 거 잘 안 먹어요ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.30",
          "form": "메뉴에 속고 있었네",
          "original_form": "메뉴에 속고 있었네",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.31",
          "form": "난 진짜 맛있는 줄 알았거든",
          "original_form": "난 진짜 맛있는 줄 알았거든",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.32",
          "form": "할머니가 해주는 된장찌개",
          "original_form": "할머니가 해주는 된장찌개",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.33",
          "form": "감 센 거 좋아함",
          "original_form": "감 센 거 좋아함",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042342.1.34",
          "form": "아침은 무조건 밥 줘야 되겠다",
          "original_form": "아침은 무조건 밥 줘야 되겠다",
