      "utterance": [
        {
          "id": "MDRW2100027530.1.1",
          "form": "name2님이당",
          "original_form": "&name2&님이당",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027530.1.2",
          "form": "헐 name1이다",
          "original_form": "헐 &name1&이다",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027530.1.3",
          "form": "걸어다니는 name1\r\nㄴname1은 원래 걸어다니는데요",
          "original_form": "걸어다니는 &name1&\r\nㄴ&name1&은 원래 걸어다니는데요",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027530.1.4",
          "form": "강아지도 자고 일어나면 머리 뻗치는 거 너무 웃겨요ㅠㅠ",
          "original_form": "강아지도 자고 일어나면 머리 뻗치는 거 너무 웃겨요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027530.1.5",
          "form": "털뻗치는 견",
          "original_form": "털뻗치는 견",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027530.1.6",
          "form": "털도 눌려요 막",
          "original_form": "털도 눌려요 막",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027530.1.7",
          "form": "한쪽 주디만 털 눌려있고 그래요",
          "original_form": "한쪽 주디만 털 눌려있고 그래요",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027530.1.8",
          "form": "아놔ㅠㅠ 너무 귀여워요",
          "original_form": "아놔ㅠㅠ 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027530.1.9",
          "form": "그럼 털 막.. 빗겨줘야 되나요",
          "original_form": "그럼 털 막.. 빗겨줘야 되나요",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027530.1.10",
          "form": "시간지나면 다시 털이 빵빵해지긴 해서 걍 내비둬요",
          "original_form": "시간지나면 다시 털이 빵빵해지긴 해서 걍 내비둬요",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027530.1.11",
          "form": "그리고 털을빗으면 역정내는 견이라..............",
          "original_form": "그리고 털을빗으면 역정내는 견이라..............",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027530.1.12",
          "form": "악 다시 빵빵해지면 너무 귀여울 것 같아요",
          "original_form": "악 다시 빵빵해지면 너무 귀여울 것 같아요",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027530.1.13",
          "form": "겨울이도 푸들이라 털 잘 안 빠지나요? ",
          "original_form": "겨울이도 푸들이라 털 잘 안 빠지나요? ",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027530.1.14",
          "form": "내 털 인간이 더 많이 빠져요",
          "original_form": "내 털 인간이 더 많이 빠져요",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027530.1.15",
          "form": "개털 사람털보다 덜빠진다!",
          "original_form": "개털 사람털보다 덜빠진다!",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027530.1.16",
          "form": "아ㅠ 인간도 털 좀 덜 빠졌으면 좋겠어요 푸들처럼 머리 볶으면 덜 빠지려나",
          "original_form": "아ㅠ 인간도 털 좀 덜 빠졌으면 좋겠어요 푸들처럼 머리 볶으면 덜 빠지려나",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027530.1.17",
          "form": "인간푸들 name2님",
          "original_form": "인간푸들 &name2&님",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027530.1.18",
          "form": "복슬한..  인간이 되실지도",
          "original_form": "복슬한..  인간이 되실지도",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027530.1.19",
          "form": "그래도 빗으면 털 좀 나와요",
          "original_form": "그래도 빗으면 털 좀 나와요",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027530.1.20",
          "form": "전 곱슬머리라 볶으면 폭탄되더라구여 흑흑",
          "original_form": "전 곱슬머리라 볶으면 폭탄되더라구여 흑흑",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027530.1.21",
          "form": "푸들 털공 만들고싶다..",
          "original_form": "푸들 털공 만들고싶다..",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027530.1.22",
          "form": "저도요 걸어다니는 폭탄되어버림",
          "original_form": "저도요 걸어다니는 폭탄되어버림",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027530.1.23",
          "form": "견... 다른푸들은 몰겟는데",
          "original_form": "견... 다른푸들은 몰겟는데",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027530.1.24",
          "form": "장개는 머리빼고 내려갈수록 직모돼서 털공이 기묘해져요",
          "original_form": "장개는 머리빼고 내려갈수록 직모돼서 털공이 기묘해져요",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027530.1.25",
          "form": "개의 tmi",
          "original_form": "개의 tmi",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027530.1.26",
          "form": "?!?! 신기하다..",
          "original_form": "?!?! 신기하다..",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027530.1.27",
          "form": "개 tmi 털기..",
          "original_form": "개 tmi 털기..",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027530.1.28",
          "form": "신상이 털리고있는 강아지",
