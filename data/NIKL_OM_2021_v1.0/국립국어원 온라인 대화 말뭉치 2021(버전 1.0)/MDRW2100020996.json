      "utterance": [
        {
          "id": "MDRW2100020996.1.1",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020996.1.2",
          "form": "그냥 동네산책만요",
          "original_form": "그냥 동네산책만요",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020996.1.3",
          "form": "하시는 운동있으세요?",
          "original_form": "하시는 운동있으세요?",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020996.1.4",
          "form": "저는 실내자전거만 타요 ㅋㅋㅋ",
          "original_form": "저는 실내자전거만 타요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020996.1.5",
          "form": "와 실내자전거",
          "original_form": "와 실내자전거",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020996.1.6",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020996.1.7",
          "form": "안지루한가요?",
          "original_form": "안지루한가요?",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020996.1.8",
          "form": "지금도 타는데 힘드네요",
          "original_form": "지금도 타는데 힘드네요",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020996.1.9",
          "form": "항상 티비나 폰보면서 해서 지루하진 않아요",
          "original_form": "항상 티비나 폰보면서 해서 지루하진 않아요",
          "speaker_id": "1",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020996.1.10",
          "form": "오 저는 여기저기가서",
          "original_form": "오 저는 여기저기가서",
          "speaker_id": "2",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020996.1.11",
          "form": "구경하는 재미로",
          "original_form": "구경하는 재미로",
          "speaker_id": "2",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020996.1.12",
          "form": "걸어요",
          "original_form": "걸어요",
          "speaker_id": "2",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020996.1.13",
          "form": "오 그거도 좋죠",
          "original_form": "오 그거도 좋죠",
          "speaker_id": "1",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020996.1.14",
          "form": "날씨 좋으면 기분 좋잖아요 ㅋㅋ",
          "original_form": "날씨 좋으면 기분 좋잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020996.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020996.1.16",
          "form": "비오면 괜히 핑계대고",
          "original_form": "비오면 괜히 핑계대고",
          "speaker_id": "2",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020996.1.17",
          "form": "맞아여 ㅋㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020996.1.18",
          "form": "오늘은 운동안해 이러고 막",
          "original_form": "오늘은 운동안해 이러고 막",
          "speaker_id": "2",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020996.1.19",
          "form": "모두가 같은 핑계를 대는 군요 ㅋㅋㅋㅋ",
          "original_form": "모두가 같은 핑계를 대는 군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020996.1.20",
          "form": "실내자전거는 매일하세요?",
          "original_form": "실내자전거는 매일하세요?",
          "speaker_id": "2",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020996.1.21",
          "form": "네 몇년 됐어요",
          "original_form": "네 몇년 됐어요",
          "speaker_id": "1",
          "time": "20210828 14:52"
        }
      ]
