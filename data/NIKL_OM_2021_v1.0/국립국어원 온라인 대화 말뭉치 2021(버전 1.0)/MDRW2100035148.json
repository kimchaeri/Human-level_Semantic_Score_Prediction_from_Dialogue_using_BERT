      "utterance": [
        {
          "id": "MDRW2100035148.1.1",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "1",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100035148.1.2",
          "form": "넹 집반찬으로 소소하게!",
          "original_form": "넹 집반찬으로 소소하게!",
          "speaker_id": "2",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100035148.1.3",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100035148.1.4",
          "form": "오늘은 안먹었어욥!",
          "original_form": "오늘은 안먹었어욥!",
          "speaker_id": "1",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100035148.1.5",
          "form": "어제 너무 먹었더니 지금까지 배부른 기분이네요ㅋㅋ",
          "original_form": "어제 너무 먹었더니 지금까지 배부른 기분이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100035148.1.6",
          "form": "ㅋㅋㅋ저도 오랜만에 나들이할거라 저녁에 맛있는 거 먹으려고",
          "original_form": "ㅋㅋㅋ저도 오랜만에 나들이할거라 저녁에 맛있는 거 먹으려고",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100035148.1.7",
          "form": "간단히 먹고 있어요",
          "original_form": "간단히 먹고 있어요",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100035148.1.8",
          "form": "여행갈땐 계획을 얼마나 세우시는 편인가요",
          "original_form": "여행갈땐 계획을 얼마나 세우시는 편인가요",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100035148.1.9",
          "form": "전에는 세세하게 세웠는데 다녀와보니 그렇게 안된다는걸 깨닫고 꼭 가고픈곳 빼고는 널널하게 세우고 있어요!",
          "original_form": "전에는 세세하게 세웠는데 다녀와보니 그렇게 안된다는걸 깨닫고 꼭 가고픈곳 빼고는 널널하게 세우고 있어요!",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100035148.1.10",
          "form": "계획 세우시는 편이신가요?",
          "original_form": "계획 세우시는 편이신가요?",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100035148.1.11",
          "form": "넹 저두 대략 어디 갈지 행선지 정도만 세우고ㅋㅋㅋ",
          "original_form": "넹 저두 대략 어디 갈지 행선지 정도만 세우고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100035148.1.12",
          "form": "시간은 넉넉하게 잡아둬요",
          "original_form": "시간은 넉넉하게 잡아둬요",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100035148.1.13",
          "form": "오~ 확실히 갔다오면 어느정도 계획성이 잡히는거 같아요",
          "original_form": "오~ 확실히 갔다오면 어느정도 계획성이 잡히는거 같아요",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100035148.1.14",
          "form": "저 여행 갈때마다 계획표 엄청 화려하게 만드는 편인데",
          "original_form": "저 여행 갈때마다 계획표 엄청 화려하게 만드는 편인데",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100035148.1.15",
          "form": "하나도 안 지켜요",
          "original_form": "하나도 안 지켜요",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100035148.1.16",
          "form": "계회은 와장창 깨라고 있는 것",
          "original_form": "계회은 와장창 깨라고 있는 것",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100035148.1.17",
          "form": "ㅋㄲㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㄲㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100035148.1.18",
          "form": "저도 프린트해서 가는데 도착하면 종이쪼가리ㅋㅋ",
          "original_form": "저도 프린트해서 가는데 도착하면 종이쪼가리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100035148.1.19",
          "form": "네ㅌㅋㅌ꼭 여행이 계획대로 되지않는 신기함이 있더라구요",
          "original_form": "네ㅌㅋㅌ꼭 여행이 계획대로 되지않는 신기함이 있더라구요",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100035148.1.20",
          "form": "맞아요ㅋㅋㅋ 그게 매력인거겠죠?",
          "original_form": "맞아요ㅋㅋㅋ 그게 매력인거겠죠?",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035148.1.21",
          "form": "이야기할수록 더 가고싶네요ㅋㅋ",
          "original_form": "이야기할수록 더 가고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100035148.1.22",
          "form": "아 그니까요 ㅠㅠ 지금은 여행계획을 짜도 갈수 없을 확률이 더 크니깐 ㅜㅜ",
          "original_form": "아 그니까요 ㅠㅠ 지금은 여행계획을 짜도 갈수 없을 확률이 더 크니깐 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100035148.1.23",
          "form": "맞아요ㅜㅠ",
          "original_form": "맞아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 12:37"
        }
      ]
    }
