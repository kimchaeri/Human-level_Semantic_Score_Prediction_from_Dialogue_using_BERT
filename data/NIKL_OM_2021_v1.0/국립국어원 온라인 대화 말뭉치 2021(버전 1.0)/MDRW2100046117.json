      "utterance": [
        {
          "id": "MDRW2100046117.1.1",
          "form": "혼자 사시나요??",
          "original_form": "혼자 사시나요??",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046117.1.2",
          "form": "언니랑 살고있어요",
          "original_form": "언니랑 살고있어요",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046117.1.3",
          "form": "직장다니시나요??",
          "original_form": "직장다니시나요??",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046117.1.4",
          "form": "넵넵 둘다 직장인이라 시간이 잘안나네요",
          "original_form": "넵넵 둘다 직장인이라 시간이 잘안나네요",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046117.1.5",
          "form": "아무래도 돈도 많이들고 시간도 부족하죠ㅠㅠ",
          "original_form": "아무래도 돈도 많이들고 시간도 부족하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046117.1.6",
          "form": "저흰 가족이 많아서 키워요",
          "original_form": "저흰 가족이 많아서 키워요",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046117.1.7",
          "form": "맞아요 ㅠㅠ 그래서 반려동물키우시는분들너무부러워요",
          "original_form": "맞아요 ㅠㅠ 그래서 반려동물키우시는분들너무부러워요",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046117.1.8",
          "form": "돌아가면서 케어해 줄 수 있어서요!",
          "original_form": "돌아가면서 케어해 줄 수 있어서요!",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046117.1.9",
          "form": "그건진짜부러워요 산책 자주가시나요?",
          "original_form": "그건진짜부러워요 산책 자주가시나요?",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046117.1.10",
          "form": "근데 떠나보내면 너무 슬퍼서 저희도 이제 안키울거에요ㅠㅠ",
          "original_form": "근데 떠나보내면 너무 슬퍼서 저희도 이제 안키울거에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046117.1.11",
          "form": "산책은 매일가죠!",
          "original_form": "산책은 매일가죠!",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046117.1.12",
          "form": "앗 맞아요 ㅠ떠나보내는게 너무힘들죠 ㅠ진짜가족이라서",
          "original_form": "앗 맞아요 ㅠ떠나보내는게 너무힘들죠 ㅠ진짜가족이라서",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046117.1.13",
          "form": "ㅠㅠ 한번 떠나보내고나니 너무 슬퍼요",
          "original_form": "ㅠㅠ 한번 떠나보내고나니 너무 슬퍼요",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046117.1.14",
          "form": "그리고 돈도 진짜 많이 들어요..",
          "original_form": "그리고 돈도 진짜 많이 들어요..",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046117.1.15",
          "form": "이번에 강아지 슬개골탈구라고 뒷다리가 안좋아져서 뒷다리 두개 다 수술했는데",
          "original_form": "이번에 강아지 슬개골탈구라고 뒷다리가 안좋아져서 뒷다리 두개 다 수술했는데",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046117.1.16",
          "form": "200깨졌어요ㅠㅠ",
          "original_form": "200깨졌어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046117.1.17",
          "form": "병원비 장난아닐거같아요 ㅠ",
          "original_form": "병원비 장난아닐거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046117.1.18",
          "form": "헉 ㅠㅠ",
          "original_form": "헉 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046117.1.19",
          "form": "슬개골탈구 많이온다던데",
          "original_form": "슬개골탈구 많이온다던데",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046117.1.20",
          "form": "그렇다고 수술 안시킬수도없고..",
          "original_form": "그렇다고 수술 안시킬수도없고..",
          "speaker_id": "2",
          "time": "20210908 10:33"
        }
      ]
