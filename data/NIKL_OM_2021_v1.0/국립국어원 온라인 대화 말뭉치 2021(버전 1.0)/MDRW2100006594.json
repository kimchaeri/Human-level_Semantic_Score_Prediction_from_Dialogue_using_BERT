      "utterance": [
        {
          "id": "MDRW2100006594.1.1",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006594.1.2",
          "form": "책 이전에는 그런 진잔코드가 없어서",
          "original_form": "책 이전에는 그런 진잔코드가 없어서",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006594.1.3",
          "form": "다들그냥 사고로 약해진 미치광이였어요",
          "original_form": "다들그냥 사고로 약해진 미치광이였어요",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006594.1.4",
          "form": "어 그러게요 그러면 뭐라그랴요?",
          "original_form": "어 그러게요 그러면 뭐라그랴요?",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006594.1.5",
          "form": "미치광이...",
          "original_form": "미치광이...",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.6",
          "form": "그래서 욕도먹고 치료도 못받아요",
          "original_form": "그래서 욕도먹고 치료도 못받아요",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.7",
          "form": "느낌이 확 달라지네요",
          "original_form": "느낌이 확 달라지네요",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.8",
          "form": "전쟁군인들이 이런게 심해서",
          "original_form": "전쟁군인들이 이런게 심해서",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.9",
          "form": "그쵸... 전쟁인데",
          "original_form": "그쵸... 전쟁인데",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.10",
          "form": "정신적 충격이 크죠",
          "original_form": "정신적 충격이 크죠",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.11",
          "form": "나약한 군인취급 받았대요 못 극복하면",
          "original_form": "나약한 군인취급 받았대요 못 극복하면",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.12",
          "form": "아니... 너무하네..",
          "original_form": "아니... 너무하네..",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.13",
          "form": "그리고 국가도 그런 인간을 치워버리고 싶은거예요",
          "original_form": "그리고 국가도 그런 인간을 치워버리고 싶은거예요",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.14",
          "form": "꼭 극복해야 하는 것도 아닌데",
          "original_form": "꼭 극복해야 하는 것도 아닌데",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006594.1.15",
          "form": "와... 국가 뭐냐 왜 있냐 ㅠ",
          "original_form": "와... 국가 뭐냐 왜 있냐 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006594.1.16",
          "form": "넘하네 ㅠ",
          "original_form": "넘하네 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006594.1.17",
          "form": "훌륭하고 용가한 군인상을 못보여주니까",
          "original_form": "훌륭하고 용가한 군인상을 못보여주니까",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006594.1.18",
          "form": "아 전쟁에 악 영향을 주는군요",
          "original_form": "아 전쟁에 악 영향을 주는군요",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006594.1.19",
          "form": "그래서 pstd 를 진단 못하게 막았대요",
          "original_form": "그래서 pstd 를 진단 못하게 막았대요",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006594.1.20",
          "form": "약한인간만 그러는게 아고",
          "original_form": "약한인간만 그러는게 아고",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006594.1.21",
          "form": "하긴 군인들이 PTSD 많이 걸린다고 하면",
          "original_form": "하긴 군인들이 PTSD 많이 걸린다고 하면",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006594.1.22",
          "form": "누가 군대에 지원입대해요",
          "original_form": "누가 군대에 지원입대해요",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006594.1.23",
          "form": "당연하다는걸 알면 사람들이 무서워하니까",
          "original_form": "당연하다는걸 알면 사람들이 무서워하니까",
          "speaker_id": "1",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006594.1.24",
          "form": "명예로운 승전국의 정치인이 신경쓸 문제는 아닌거죠",
          "original_form": "명예로운 승전국의 정치인이 신경쓸 문제는 아닌거죠",
          "speaker_id": "1",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006594.1.25",
          "form": "아무도 지원 안하지...",
          "original_form": "아무도 지원 안하지...",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006594.1.26",
          "form": "맞아요 그래서 최대한 감추려했어요",
          "original_form": "맞아요 그래서 최대한 감추려했어요",
          "speaker_id": "1",
          "time": "20210908 02:59"
        }
      ]
