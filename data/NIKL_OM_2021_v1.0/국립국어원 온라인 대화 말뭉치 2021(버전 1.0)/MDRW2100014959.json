      "utterance": [
        {
          "id": "MDRW2100014959.1.1",
          "form": "영화자주보세요?",
          "original_form": "영화자주보세요?",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014959.1.2",
          "form": "어쩌다 가끔 보는 편이에요",
          "original_form": "어쩌다 가끔 보는 편이에요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014959.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014959.1.4",
          "form": "저는 다운받아서 봐요ㅎ",
          "original_form": "저는 다운받아서 봐요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014959.1.5",
          "form": "코로나라 밖에나갈수없으니ㅎㅎ",
          "original_form": "코로나라 밖에나갈수없으니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014959.1.6",
          "form": "최근에는 뭐 보셨어요?",
          "original_form": "최근에는 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014959.1.7",
          "form": "영화는 마블영화였는데 제목은 기억이안나네요ㅎㅎ",
          "original_form": "영화는 마블영화였는데 제목은 기억이안나네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014959.1.8",
          "form": "스칼렛 요한슨나오는거요ㅎㅎ",
          "original_form": "스칼렛 요한슨나오는거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014959.1.9",
          "form": "아 제목이 기억 안 나네요 ㅎㅎ",
          "original_form": "아 제목이 기억 안 나네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014959.1.10",
          "form": "저는 미나리 봤어요",
          "original_form": "저는 미나리 봤어요",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014959.1.11",
          "form": "미나리 재밌나요?",
          "original_form": "미나리 재밌나요?",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100014959.1.12",
          "form": "전 좋았어요.",
          "original_form": "전 좋았어요.",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100014959.1.13",
          "form": "감독 가족의 자전적 이야기라고 하더라구요",
          "original_form": "감독 가족의 자전적 이야기라고 하더라구요",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100014959.1.14",
          "form": "ㅎㅎ 저도 한번 챙겨보겟습니다ㅎㅎ",
          "original_form": "ㅎㅎ 저도 한번 챙겨보겟습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100014959.1.15",
          "form": "다들 재밌다고하니깐ㅎㅎ",
          "original_form": "다들 재밌다고하니깐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100014959.1.16",
          "form": "극장 영화 관람하고싶네요ㅠ",
          "original_form": "극장 영화 관람하고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014959.1.17",
          "form": "코로나 때문에 영화 시장 많이 죽은 것 같더라구요 ㅠ",
          "original_form": "코로나 때문에 영화 시장 많이 죽은 것 같더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014959.1.18",
          "form": "맞아요ㅎㅍ 요즘 영화관도 힘들거애요ㅎ",
          "original_form": "맞아요ㅎㅍ 요즘 영화관도 힘들거애요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100014959.1.19",
          "form": "그립네요ㅠㅠ 마스크 없이 영화보던 시절이 ㅠ",
          "original_form": "그립네요ㅠㅠ 마스크 없이 영화보던 시절이 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100014959.1.20",
          "form": "영화 개봉도 자꾸 미뤄지는 것 같고..",
          "original_form": "영화 개봉도 자꾸 미뤄지는 것 같고..",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100014959.1.21",
          "form": "영화배우들도 힘들겠어요",
          "original_form": "영화배우들도 힘들겠어요",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100014959.1.22",
          "form": "이러다 영화 안나오는거 아닌가몰라요 ㅠ",
          "original_form": "이러다 영화 안나오는거 아닌가몰라요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014959.1.23",
          "form": "다들 어려운 시기네요ㅠ\r\n제작 자체가 무산되는 경우도 많겠죠",
          "original_form": "다들 어려운 시기네요ㅠ\r\n제작 자체가 무산되는 경우도 많겠죠",
          "speaker_id": "1",
          "time": "20210825 22:15"
        }
      ]
    }
