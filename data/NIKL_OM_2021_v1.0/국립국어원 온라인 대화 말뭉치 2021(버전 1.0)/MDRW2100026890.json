      "utterance": [
        {
          "id": "MDRW2100026890.1.1",
          "form": "야식 드시나요",
          "original_form": "야식 드시나요",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026890.1.2",
          "form": "야식은 왠만하면 안 먹으려는 편이에요.",
          "original_form": "야식은 왠만하면 안 먹으려는 편이에요.",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026890.1.3",
          "form": "좋은 습관이에요",
          "original_form": "좋은 습관이에요",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026890.1.4",
          "form": "저도 끊고 있어요 ㅎㅎ",
          "original_form": "저도 끊고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026890.1.5",
          "form": "대신 뭐 먹고싶을 때 껌 씹다보니까 하루에 1,2번은 씹게 되더라구요 ㅋㅋ",
          "original_form": "대신 뭐 먹고싶을 때 껌 씹다보니까 하루에 1,2번은 씹게 되더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026890.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ 껌도 안좋지 않나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 껌도 안좋지 않나요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026890.1.7",
          "form": "ㅋㅋ 다른 거 먹는 거 보단 낫겠지 싶긴 한데 껌 중독 비슷하게 됐네요",
          "original_form": "ㅋㅋ 다른 거 먹는 거 보단 낫겠지 싶긴 한데 껌 중독 비슷하게 됐네요",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026890.1.8",
          "form": "ㅋㅋ 뭐 칼로리는 없으니까용",
          "original_form": "ㅋㅋ 뭐 칼로리는 없으니까용",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026890.1.9",
          "form": "저녁으로는 뭐 드셨어요?",
          "original_form": "저녁으로는 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100026890.1.10",
          "form": "소고기 먹었어욥",
          "original_form": "소고기 먹었어욥",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100026890.1.11",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100026890.1.12",
          "form": "맛있는 거 드셨네요 ㅋㅋ",
          "original_form": "맛있는 거 드셨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100026890.1.13",
          "form": "전 순두부찌개 먹었어요",
          "original_form": "전 순두부찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100026890.1.14",
          "form": "순두부찌개도",
          "original_form": "순두부찌개도",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100026890.1.15",
          "form": "얼큰하니 맛있죠",
          "original_form": "얼큰하니 맛있죠",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100026890.1.16",
          "form": "마트에서 다담 양념같은 거 사서 재료넣고 끓여도 맛있더라구요",
          "original_form": "마트에서 다담 양념같은 거 사서 재료넣고 끓여도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100026890.1.17",
          "form": "그 한알?",
          "original_form": "그 한알?",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100026890.1.18",
          "form": "넣는 그거 좋던데요",
          "original_form": "넣는 그거 좋던데요",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100026890.1.19",
          "form": "아 육수내는 대신 그것도 편하고 맛있더라구요",
          "original_form": "아 육수내는 대신 그것도 편하고 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100026890.1.20",
          "form": "나중에 보셔요",
          "original_form": "나중에 보셔요",
          "speaker_id": "2",
