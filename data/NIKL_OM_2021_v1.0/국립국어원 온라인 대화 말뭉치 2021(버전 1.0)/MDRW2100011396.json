      "utterance": [
        {
          "id": "MDRW2100011396.1.1",
          "form": "무교입니다 ㅋㅋ",
          "original_form": "무교입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011396.1.2",
          "form": "저도없고 부모님은불교요",
          "original_form": "저도없고 부모님은불교요",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100011396.1.3",
          "form": "했던이야기 같은건..기분탓이겠죠?ㅎㅎ",
          "original_form": "했던이야기 같은건..기분탓이겠죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011396.1.4",
          "form": "곰곰히 생각해보니 했네요ㅎㅎ",
          "original_form": "곰곰히 생각해보니 했네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011396.1.5",
          "form": "성당은 안가보셨죠?",
          "original_form": "성당은 안가보셨죠?",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100011396.1.6",
          "form": "네ㅎㅎ 한번도",
          "original_form": "네ㅎㅎ 한번도",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100011396.1.7",
          "form": "꼭한번가보고싶네요",
          "original_form": "꼭한번가보고싶네요",
          "speaker_id": "1",
          "time": "20210825 07:05"
        },
        {
          "id": "MDRW2100011396.1.8",
          "form": "어떤곳이지",
          "original_form": "어떤곳이지",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100011396.1.9",
          "form": "요즘은 코로나 때문에 종교계도 조용할듯 해요",
          "original_form": "요즘은 코로나 때문에 종교계도 조용할듯 해요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100011396.1.10",
          "form": "교회는 아니져",
          "original_form": "교회는 아니져",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100011396.1.11",
          "form": "ㅠㅠ 왜그렇게 못 모여서 안달들인지",
          "original_form": "ㅠㅠ 왜그렇게 못 모여서 안달들인지",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100011396.1.12",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100011396.1.13",
          "form": "ㅠㅠ 행사도 하는곳 보면 이해안되요ㅠ",
          "original_form": "ㅠㅠ 행사도 하는곳 보면 이해안되요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100011396.1.14",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100011396.1.15",
          "form": "모이지 말라하는데 청개구리를 삶아먹은건지.\nㅜ",
