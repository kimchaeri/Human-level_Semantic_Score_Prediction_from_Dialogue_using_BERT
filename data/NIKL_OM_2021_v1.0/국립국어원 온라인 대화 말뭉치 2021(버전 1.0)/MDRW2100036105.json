      "utterance": [
        {
          "id": "MDRW2100036105.1.1",
          "form": "저녁에뭐드세여?",
          "original_form": "저녁에뭐드세여?",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036105.1.2",
          "form": "저녁에 뭐드세여?",
          "original_form": "저녁에 뭐드세여?",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036105.1.3",
          "form": "모르겠어뇨",
          "original_form": "모르겠어뇨",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036105.1.4",
          "form": "약속한것도 없나여?",
          "original_form": "약속한것도 없나여?",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036105.1.5",
          "form": "맨날 일정이 바껴서",
          "original_form": "맨날 일정이 바껴서",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036105.1.6",
          "form": "어 그러시구나..",
          "original_form": "어 그러시구나..",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036105.1.7",
          "form": "멋대로 확정짓는다고 화내더라고요",
          "original_form": "멋대로 확정짓는다고 화내더라고요",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036105.1.8",
          "form": "그래도 뭘 하려고 생각하고잇진않을까여",
          "original_form": "그래도 뭘 하려고 생각하고잇진않을까여",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036105.1.9",
          "form": "괜히 말했다간 혼나요",
          "original_form": "괜히 말했다간 혼나요",
          "speaker_id": "2",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036105.1.10",
          "form": "아.. 나쁜사람이네요",
          "original_form": "아.. 나쁜사람이네요",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036105.1.11",
          "form": "입조심 해야돼요",
          "original_form": "입조심 해야돼요",
          "speaker_id": "2",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036105.1.12",
          "form": "식사하시고 양치하셧어여?",
          "original_form": "식사하시고 양치하셧어여?",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036105.1.13",
          "form": "지금도 제 언행을 감시하고 있을지 몰라요",
          "original_form": "지금도 제 언행을 감시하고 있을지 몰라요",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100036105.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100036105.1.15",
          "form": "무서워요",
          "original_form": "무서워요",
          "speaker_id": "2",
          "time": "20210830 16:27"
        }
      ]
