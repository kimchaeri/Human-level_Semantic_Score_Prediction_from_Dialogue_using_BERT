      "utterance": [
        {
          "id": "MDRW2100019998.1.1",
          "form": "베이킹도 좋아하시나요?",
          "original_form": "베이킹도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019998.1.2",
          "form": "그거는 준비할것도 넘 많고",
          "original_form": "그거는 준비할것도 넘 많고",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019998.1.3",
          "form": "공간이 여유있어야해서",
          "original_form": "공간이 여유있어야해서",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019998.1.4",
          "form": "유튜브로만 봐요ㅠ 해보고 싶긴하지만",
          "original_form": "유튜브로만 봐요ㅠ 해보고 싶긴하지만",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019998.1.5",
          "form": "저는 내일 베이킹 원데이클래스 하러 가요",
          "original_form": "저는 내일 베이킹 원데이클래스 하러 가요",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019998.1.6",
          "form": "파운드케이크 만들 예정입니다ㅎㅎ",
          "original_form": "파운드케이크 만들 예정입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019998.1.7",
          "form": "우와~~~ 파운드케이크 맛있잖아요~~",
          "original_form": "우와~~~ 파운드케이크 맛있잖아요~~",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019998.1.8",
          "form": "원데이클래스 자주 하세요?",
          "original_form": "원데이클래스 자주 하세요?",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019998.1.9",
          "form": "베이킹 수업 많긴 하더라고요",
          "original_form": "베이킹 수업 많긴 하더라고요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019998.1.10",
          "form": "지난번에 떡케이크 만들기 해봤는데 재밌더라고요",
          "original_form": "지난번에 떡케이크 만들기 해봤는데 재밌더라고요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019998.1.11",
          "form": "그러다 이번에 베이킹 이벤트 클래스 발견해서 신청했어요",
          "original_form": "그러다 이번에 베이킹 이벤트 클래스 발견해서 신청했어요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019998.1.12",
          "form": "베이킹 레시피도 다 알려주고 재료도 주는거죠?!",
          "original_form": "베이킹 레시피도 다 알려주고 재료도 주는거죠?!",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019998.1.13",
          "form": "몸만 가서 만들면 되나요?!",
          "original_form": "몸만 가서 만들면 되나요?!",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019998.1.14",
          "form": "네네~ 돈과 몸ㅎㅎ",
          "original_form": "네네~ 돈과 몸ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019998.1.15",
          "form": "베이킹 레시피도 알려주고 재료랑 조리도구도 다 제공해준대요",
          "original_form": "베이킹 레시피도 알려주고 재료랑 조리도구도 다 제공해준대요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019998.1.16",
          "form": "오... 빵 좋아하세요?",
          "original_form": "오... 빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019998.1.17",
          "form": "빵 좋아하는 편이예요",
          "original_form": "빵 좋아하는 편이예요",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100019998.1.18",
          "form": "디저트류도 좋아하고",
          "original_form": "디저트류도 좋아하고",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100019998.1.19",
          "form": "혹시 수도권 사세요?",
          "original_form": "혹시 수도권 사세요?",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019998.1.20",
          "form": "빵맛집 진짜 많잖아요ㅝ",
          "original_form": "빵맛집 진짜 많잖아요ㅝ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019998.1.21",
          "form": "네 수도권에 살아요",
          "original_form": "네 수도권에 살아요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019998.1.22",
          "form": "근데 요즘은 빵맛집들 택배도 많이 해주더라고요",
          "original_form": "근데 요즘은 빵맛집들 택배도 많이 해주더라고요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100019998.1.23",
          "form": "저도 빵 좋아해서 생활의달인 같은데 나오면 주문해먹어요ㅋㅋㅋ",
          "original_form": "저도 빵 좋아해서 생활의달인 같은데 나오면 주문해먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019998.1.24",
          "form": "괜히 맛집이 아니더라고요~",
          "original_form": "괜히 맛집이 아니더라고요~",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019998.1.25",
          "form": "맞아요 맛집 빵은 달라요",
          "original_form": "맞아요 맛집 빵은 달라요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        }
      ]
