      "utterance": [
        {
          "id": "MDRW2100011706.1.1",
          "form": "그러까",
          "original_form": "그러까",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011706.1.2",
          "form": "나중에 여유되면",
          "original_form": "나중에 여유되면",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011706.1.3",
          "form": "name3이 데리고 놀러 댕기자",
          "original_form": "&name3&이 데리고 놀러 댕기자",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011706.1.4",
          "form": "탈레반 무서워서 여행가겠나",
          "original_form": "탈레반 무서워서 여행가겠나",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011706.1.5",
          "form": "늙는건 슬프다ㅜ",
          "original_form": "늙는건 슬프다ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011706.1.6",
          "form": "어 ㅜ 세자매 같이 다니자 여행",
          "original_form": "어 ㅜ 세자매 같이 다니자 여행",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011706.1.7",
          "form": "무서운데 안 가면 되디",
          "original_form": "무서운데 안 가면 되디",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011706.1.8",
          "form": "그래..여행은 마음 편한 사람들끼리 가야된다",
          "original_form": "그래..여행은 마음 편한 사람들끼리 가야된다",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011706.1.9",
          "form": "탈레반 전세계에 흩어져있나?",
          "original_form": "탈레반 전세계에 흩어져있나?",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011706.1.10",
          "form": "몰라 왜?",
          "original_form": "몰라 왜?",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.11",
          "form": "여행갈려니.. 유럽쪽은 중국사람이라고 오해할까봐",
          "original_form": "여행갈려니.. 유럽쪽은 중국사람이라고 오해할까봐",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.12",
          "form": "무서워서 여행이",
          "original_form": "무서워서 여행이",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.13",
          "form": "아직은 좀 위험하지",
          "original_form": "아직은 좀 위험하지",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.14",
          "form": "잘사는곳은 괜찮다더라...",
          "original_form": "잘사는곳은 괜찮다더라...",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.15",
          "form": "맘편히 비행기타고",
          "original_form": "맘편히 비행기타고",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.16",
          "form": "여행가고싶네",
          "original_form": "여행가고싶네",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.17",
          "form": "호텔 좋은 곳에서",
          "original_form": "호텔 좋은 곳에서",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.18",
          "form": "얘들 크는게 아쉽다",
          "original_form": "얘들 크는게 아쉽다",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011706.1.19",
          "form": "같이 가고싶다",
          "original_form": "같이 가고싶다",
          "speaker_id": "1",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011706.1.20",
          "form": "우리나라 여행도 좋을듯",
          "original_form": "우리나라 여행도 좋을듯",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011706.1.21",
          "form": "호캉스 가고싶네",
          "original_form": "호캉스 가고싶네",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011706.1.22",
          "form": "부산이라도 좋지싶다",
          "original_form": "부산이라도 좋지싶다",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011706.1.23",
          "form": "여행 생각하면서 기분 풀어보자",
          "original_form": "여행 생각하면서 기분 풀어보자",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100011706.1.24",
          "form": "어디 놀러갔을때 가장 좋았어?",
          "original_form": "어디 놀러갔을때 가장 좋았어?",
          "speaker_id": "1",
          "time": "20210824 22:23"
        }
      ]
