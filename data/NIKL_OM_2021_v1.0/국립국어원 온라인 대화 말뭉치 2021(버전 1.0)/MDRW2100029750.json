      "utterance": [
        {
          "id": "MDRW2100029750.1.1",
          "form": "네 백반 먹었어요. 식사하셨나요?",
          "original_form": "네 백반 먹었어요. 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029750.1.2",
          "form": "ㅋㅋ네 집밥 먹었어요",
          "original_form": "ㅋㅋ네 집밥 먹었어요",
          "speaker_id": "1",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029750.1.3",
          "form": "간만에 본가와서",
          "original_form": "간만에 본가와서",
          "speaker_id": "1",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100029750.1.4",
          "form": "집밥이 먹고나서 속도 편하고 좋지요ㅎㅎ",
          "original_form": "집밥이 먹고나서 속도 편하고 좋지요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029750.1.5",
          "form": "ㅋㅋ집의 그리움...",
          "original_form": "ㅋㅋ집의 그리움...",
          "speaker_id": "1",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100029750.1.6",
          "form": "평소에 요리도 하시나요?",
          "original_form": "평소에 요리도 하시나요?",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029750.1.7",
          "form": "자주합니다 ...",
          "original_form": "자주합니다 ...",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100029750.1.8",
          "form": "자신있는 메뉴는 뭔가요?",
          "original_form": "자신있는 메뉴는 뭔가요?",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100029750.1.9",
          "form": "계란찜이오!ㅋㅋ",
          "original_form": "계란찜이오!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100029750.1.10",
          "form": "초보에요 ㅋㅋ",
          "original_form": "초보에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100029750.1.11",
          "form": "ㅋㅋ 유튜브 레시피 보고 따라하면 할만하더라구요",
          "original_form": "ㅋㅋ 유튜브 레시피 보고 따라하면 할만하더라구요",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100029750.1.12",
          "form": "ㅋㅋ네 대충 먹을정도는",
          "original_form": "ㅋㅋ네 대충 먹을정도는",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029750.1.13",
          "form": "가능합니다",
          "original_form": "가능합니다",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029750.1.14",
          "form": "요리 유튜브도 엄청 많더라구요",
          "original_form": "요리 유튜브도 엄청 많더라구요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100029750.1.15",
          "form": "ㅋㅋ 그 야매요리",
          "original_form": "ㅋㅋ 그 야매요리",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100029750.1.16",
          "form": "재밌던데요",
          "original_form": "재밌던데요",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100029750.1.17",
          "form": "정통야매요리 채널인가요?",
          "original_form": "정통야매요리 채널인가요?",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100029750.1.18",
          "form": "네 3분요리",
          "original_form": "네 3분요리",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100029750.1.19",
          "form": "막 무보까",
          "original_form": "막 무보까",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100029750.1.20",
          "form": "뭐더라",
          "original_form": "뭐더라",
