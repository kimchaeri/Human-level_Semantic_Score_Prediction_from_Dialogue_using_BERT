      "utterance": [
        {
          "id": "MDRW2100032539.1.1",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032539.1.2",
          "form": "저베이킹하는사람들에대한",
          "original_form": "저베이킹하는사람들에대한",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032539.1.3",
          "form": "로망이약간있어요ㅎㅎㅎㅎ",
          "original_form": "로망이약간있어요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032539.1.4",
          "form": "재밌어요!!! 제빵학원 잠시 다녔는데",
          "original_form": "재밌어요!!! 제빵학원 잠시 다녔는데",
          "speaker_id": "1",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100032539.1.5",
          "form": "빵 부푸는게 되게 신기해요 ㅋㅋㅋ",
          "original_form": "빵 부푸는게 되게 신기해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100032539.1.6",
          "form": "ㅎㅎㅎ갓구운빵냄새도좋을것같아요",
          "original_form": "ㅎㅎㅎ갓구운빵냄새도좋을것같아요",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032539.1.7",
          "form": "금손이시네요",
          "original_form": "금손이시네요",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032539.1.8",
          "form": "마카롱도 한번 배워 보고싶어요 ㅋㅋㅋㅋ",
          "original_form": "마카롱도 한번 배워 보고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032539.1.9",
          "form": "만들기 까다롭다고 하네요 ㅠ",
          "original_form": "만들기 까다롭다고 하네요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032539.1.10",
          "form": "우왕 맞아요",
          "original_form": "우왕 맞아요",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032539.1.11",
          "form": "꼬끄?그게어렵다고하더라고요",
          "original_form": "꼬끄?그게어렵다고하더라고요",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032539.1.12",
          "form": "핫케잌 만드는것도 재밌어요 ㅋㅋ",
          "original_form": "핫케잌 만드는것도 재밌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032539.1.13",
          "form": "제일 간단하규",
          "original_form": "제일 간단하규",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032539.1.14",
          "form": "하유 다 맛있겠어요",
          "original_form": "하유 다 맛있겠어요",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032539.1.15",
          "form": "어떤빵제일좋아하세요?",
          "original_form": "어떤빵제일좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032539.1.16",
          "form": "저는 안가리긴 하는데 슈크림빵 피자빵 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 안가리긴 하는데 슈크림빵 피자빵 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032539.1.17",
          "form": "빵집가면 항상 고민해요 ㅋㅋ",
          "original_form": "빵집가면 항상 고민해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032539.1.18",
          "form": "앗 슈크림빵 저도 좋아해요",
          "original_form": "앗 슈크림빵 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100032539.1.19",
          "form": "실패하지않죠",
          "original_form": "실패하지않죠",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100032539.1.20",
          "form": "네네 ㅋㅋㅋ 편의점에서 파는것도 맛있어요!!",
          "original_form": "네네 ㅋㅋㅋ 편의점에서 파는것도 맛있어요!!",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032539.1.21",
          "form": "맞아요 씨유가 잘하더라고요",
          "original_form": "맞아요 씨유가 잘하더라고요",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100032539.1.22",
          "form": "편의점에 롤케잌? 비슷한거 파는데 진짜 맛있어요 !",
          "original_form": "편의점에 롤케잌? 비슷한거 파는데 진짜 맛있어요 !",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032539.1.23",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032539.1.24",
          "form": "살만안찌면여러개먹고싶어요",
          "original_form": "살만안찌면여러개먹고싶어요",
