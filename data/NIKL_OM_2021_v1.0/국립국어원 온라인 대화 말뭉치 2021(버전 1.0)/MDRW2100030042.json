        {
          "id": "MDRW2100030042.1.1",
          "form": "그럼 그 애들 미술을 더 안하지 않나요?",
          "original_form": "그럼 그 애들 미술을 더 안하지 않나요?",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100030042.1.2",
          "form": "전 동호회는없고 봉사만",
          "original_form": "전 동호회는없고 봉사만",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100030042.1.3",
          "form": "봉사 활동 하세요??",
          "original_form": "봉사 활동 하세요??",
          "speaker_id": "1",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100030042.1.4",
          "form": "일년에 한번 연탄봉사요",
          "original_form": "일년에 한번 연탄봉사요",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100030042.1.5",
          "form": "애들데리고",
          "original_form": "애들데리고",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100030042.1.6",
          "form": "어디로요??",
          "original_form": "어디로요??",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030042.1.7",
          "form": "저 그런거 한번도 안해봤어요",
          "original_form": "저 그런거 한번도 안해봤어요",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030042.1.8",
          "form": "차가못들어가는곳요",
          "original_form": "차가못들어가는곳요",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100030042.1.9",
          "form": "사람들이 한줄로서서 연탄 나르는거예요",
          "original_form": "사람들이 한줄로서서 연탄 나르는거예요",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100030042.1.10",
          "form": "아~ 아이들한테도 좋은 경험이겠는데요",
          "original_form": "아~ 아이들한테도 좋은 경험이겠는데요",
          "speaker_id": "1",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100030042.1.11",
          "form": "봉사찾아보면 많아요..아이들크면 하세요",
          "original_form": "봉사찾아보면 많아요..아이들크면 하세요",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100030042.1.12",
          "form": "지금은 어려서 가도 사고만 치겠죠?",
          "original_form": "지금은 어려서 가도 사고만 치겠죠?",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030042.1.13",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100030042.1.14",
          "form": "아이들과 하기 좋은 봉사 뭐가 있을까요?",
          "original_form": "아이들과 하기 좋은 봉사 뭐가 있을까요?",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100030042.1.15",
          "form": "아직어려서 하기가좀",
          "original_form": "아직어려서 하기가좀",
