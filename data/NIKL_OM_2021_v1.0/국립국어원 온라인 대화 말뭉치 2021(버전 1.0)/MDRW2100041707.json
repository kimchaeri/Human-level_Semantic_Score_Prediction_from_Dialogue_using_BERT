      "utterance": [
        {
          "id": "MDRW2100041707.1.1",
          "form": "안녕하세요 좋아하는 음식은 어떤것 이신가요?",
          "original_form": "안녕하세요 좋아하는 음식은 어떤것 이신가요?",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041707.1.2",
          "form": "저 매운거 좋아해요.",
          "original_form": "저 매운거 좋아해요.",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041707.1.3",
          "form": "매운거 잘 드시나요?",
          "original_form": "매운거 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041707.1.4",
          "form": "아니요ㅠㅠ 매운거 유행이 너무 슬픈 맵찔이에요........",
          "original_form": "아니요ㅠㅠ 매운거 유행이 너무 슬픈 맵찔이에요........",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041707.1.5",
          "form": "잘드신다니 부러워요....",
          "original_form": "잘드신다니 부러워요....",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041707.1.6",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041707.1.7",
          "form": "네 좋아해요ㅎ",
          "original_form": "네 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041707.1.8",
          "form": "그래도 매운거 자주 먹진 않아요ㅋㅋ",
          "original_form": "그래도 매운거 자주 먹진 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041707.1.9",
          "form": "저는 매운거 잘 못먹긴 하지만 좋아해서ㅋㅋㅋ울면서 먹어요ㅋㅋㅋㅋ",
          "original_form": "저는 매운거 잘 못먹긴 하지만 좋아해서ㅋㅋㅋ울면서 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041707.1.10",
          "form": "ㅋㅋㅋ스트레스 있음 너무 땡기잖아요",
          "original_form": "ㅋㅋㅋ스트레스 있음 너무 땡기잖아요",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041707.1.11",
          "form": "맞아요ㅋㅋㅋㅋ 하..또 얘기하니까 땡기네요 중독인가봐요",
          "original_form": "맞아요ㅋㅋㅋㅋ 하..또 얘기하니까 땡기네요 중독인가봐요",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041707.1.12",
          "form": "배달 주로 시켜드세요?",
          "original_form": "배달 주로 시켜드세요?",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041707.1.13",
          "form": "네 보통 배달 먹는거 같아요",
          "original_form": "네 보통 배달 먹는거 같아요",
          "speaker_id": "2",
          "time": "20210906 14:56"
        },
        {
          "id": "MDRW2100041707.1.14",
          "form": "저도 그래요 배달비가 아깝긴 하지만 편해서 끊을 수가 없네요ㅠㅠ",
          "original_form": "저도 그래요 배달비가 아깝긴 하지만 편해서 끊을 수가 없네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041707.1.15",
          "form": "맞아요ㅠㅠ 조금이라도 할인되는거 찾아보구요ㅋㅋ",
          "original_form": "맞아요ㅠㅠ 조금이라도 할인되는거 찾아보구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041707.1.16",
          "form": "저도ㅋㅋ 쿠폰 줄때 열심히 배달시켜먹어요",
          "original_form": "저도ㅋㅋ 쿠폰 줄때 열심히 배달시켜먹어요",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041707.1.17",
          "form": "맞아요ㅋㅋ 피자 프랜차이즈 넘 비싸서 쿠폰 나오면 꼭 먹는거 같아요",
          "original_form": "맞아요ㅋㅋ 피자 프랜차이즈 넘 비싸서 쿠폰 나오면 꼭 먹는거 같아요",
          "speaker_id": "2",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100041707.1.18",
          "form": "예전에는 피자가 비싼 음식이고 치킨이 좀 저렴하다는 인상이 있었는데 요즘은 치킨이 너무너무 비싼 것 같아요",
          "original_form": "예전에는 피자가 비싼 음식이고 치킨이 좀 저렴하다는 인상이 있었는데 요즘은 치킨이 너무너무 비싼 것 같아요",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041707.1.19",
          "form": "쿠폰도 없구ㅜㅜ",
          "original_form": "쿠폰도 없구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041707.1.20",
          "form": "그렇긴해요. 전 시장 통닭도 잘 사다먹어서 가격 괜찮더라구요ㅋㅋ",
          "original_form": "그렇긴해요. 전 시장 통닭도 잘 사다먹어서 가격 괜찮더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041707.1.21",
          "form": "아~맞아요 시장 통닭 저렴하더라구요! 저는 순살을 좋아해서ㅠㅠ",
          "original_form": "아~맞아요 시장 통닭 저렴하더라구요! 저는 순살을 좋아해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 16:54"
        }
      ]
    }
