      "utterance": [
        {
          "id": "MDRW2100018239.1.1",
          "form": "얀센은 한번맞음 끝이죠?",
          "original_form": "얀센은 한번맞음 끝이죠?",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018239.1.2",
          "form": "네 그렇긴한데 고를 수 있었다면",
          "original_form": "네 그렇긴한데 고를 수 있었다면",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018239.1.3",
          "form": "화이자를 ㅎㅎ",
          "original_form": "화이자를 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018239.1.4",
          "form": "화이자도 자꾸 사망해서 ㅎ",
          "original_form": "화이자도 자꾸 사망해서 ㅎ",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018239.1.5",
          "form": "전 예약만해놓고 저~말쯤에",
          "original_form": "전 예약만해놓고 저~말쯤에",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018239.1.6",
          "form": "그때가서 맞을지 고민하려구요",
          "original_form": "그때가서 맞을지 고민하려구요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018239.1.7",
          "form": "부작용없었어요?",
          "original_form": "부작용없었어요?",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018239.1.8",
          "form": "네 맞은 부위 살짝 뻐근한거?",
          "original_form": "네 맞은 부위 살짝 뻐근한거?",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018239.1.9",
          "form": "그리고 맞고 나니까 쫌 안심되서 ㅎㅎ",
          "original_form": "그리고 맞고 나니까 쫌 안심되서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018239.1.10",
          "form": "맞긴해야겠고 맞자니 무섭고ㅎ",
          "original_form": "맞긴해야겠고 맞자니 무섭고ㅎ",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018239.1.11",
          "form": "살려고맞는건데 죽을까봐",
          "original_form": "살려고맞는건데 죽을까봐",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018239.1.12",
          "form": "괜찮을거에요",
          "original_form": "괜찮을거에요",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018239.1.13",
          "form": "뉴스는 진짜 극히 일부일거에용ㅎㅎ",
          "original_form": "뉴스는 진짜 극히 일부일거에용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018239.1.14",
          "form": "회사 동생 건너건너 지인이 화이자2차 맞고 죽었대요",
          "original_form": "회사 동생 건너건너 지인이 화이자2차 맞고 죽었대요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018239.1.15",
          "form": "청원올라오고 뉴스기사떴던데",
          "original_form": "청원올라오고 뉴스기사떴던데",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018239.1.16",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018239.1.17",
          "form": "네 뭔가 모르는사람이 뉴스뜨면그런보다할텐데",
          "original_form": "네 뭔가 모르는사람이 뉴스뜨면그런보다할텐데",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018239.1.18",
          "form": "아는지인이라니가 겁나던데요",
          "original_form": "아는지인이라니가 겁나던데요",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018239.1.19",
          "form": "아 그러게요 지인이면 무섭죠",
          "original_form": "아 그러게요 지인이면 무섭죠",
          "speaker_id": "2",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100018239.1.20",
          "form": "맞긴해야겠죠?",
          "original_form": "맞긴해야겠죠?",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018239.1.21",
          "form": "네 ㅎㅎ 괜찮을거에요",
          "original_form": "네 ㅎㅎ 괜찮을거에요",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018239.1.22",
          "form": "다들맞는거보고 그때가서 결정해야겠어요",
          "original_form": "다들맞는거보고 그때가서 결정해야겠어요",
          "speaker_id": "1",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018239.1.23",
          "form": "집순이라 안맞아도 될듯한데ㅎㅎ",
          "original_form": "집순이라 안맞아도 될듯한데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018239.1.24",
          "form": "그래도 가족 있으니까 ㅎㅎ",
          "original_form": "그래도 가족 있으니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018239.1.25",
          "form": "코로나 지긋지긋하네요 ㅎ언제쯤 정상화될지",
          "original_form": "코로나 지긋지긋하네요 ㅎ언제쯤 정상화될지",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018239.1.26",
          "form": "얀센은 6개월뒤 또맞아야되지않아요?",
          "original_form": "얀센은 6개월뒤 또맞아야되지않아요?",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018239.1.27",
          "form": "아 뭐 6개월 뒤에는",
          "original_form": "아 뭐 6개월 뒤에는",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018239.1.28",
          "form": "백신 좀 넉넉하지 싶어서요",
          "original_form": "백신 좀 넉넉하지 싶어서요",
          "speaker_id": "2",
          "time": "20210827 01:45"
        }
      ]
    }
  ]
