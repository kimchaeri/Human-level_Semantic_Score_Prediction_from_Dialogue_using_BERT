      "utterance": [
        {
          "id": "MDRW2100010258.1.1",
          "form": "저도 불닭좋아하구용",
          "original_form": "저도 불닭좋아하구용",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010258.1.2",
          "form": "뭐 요즘엔 다 맵기조절이되서",
          "original_form": "뭐 요즘엔 다 맵기조절이되서",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010258.1.3",
          "form": "떡볶이 짬뽕 마라탕",
          "original_form": "떡볶이 짬뽕 마라탕",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010258.1.4",
          "form": "다 좋아해용 ~~",
          "original_form": "다 좋아해용 ~~",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010258.1.5",
          "form": "아아 짬뽕 ㅋㅋㅋ 혹시 신길동 매운짬뽕아세요?",
          "original_form": "아아 짬뽕 ㅋㅋㅋ 혹시 신길동 매운짬뽕아세요?",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010258.1.6",
          "form": "짬뽕 매운맛하니까 생각났는데",
          "original_form": "짬뽕 매운맛하니까 생각났는데",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010258.1.7",
          "form": "들어만 봤어요 !",
          "original_form": "들어만 봤어요 !",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.8",
          "form": "거기 짬뽕 진....짜 캡사이신 맛으로 매워요",
          "original_form": "거기 짬뽕 진....짜 캡사이신 맛으로 매워요",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.9",
          "form": "여긴 여주인데 여기도 유가장이라고 유명한 매운짬뽕있어요 !!",
          "original_form": "여긴 여주인데 여기도 유가장이라고 유명한 매운짬뽕있어요 !!",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.10",
          "form": "ㅋㅋㅋㅋㅋ 무도나와서 유명해졋대요",
          "original_form": "ㅋㅋㅋㅋㅋ 무도나와서 유명해졋대요",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.11",
          "form": "두번 갔는데... 진짜 왜 두번이나 갔는지ㅜㅜ",
          "original_form": "두번 갔는데... 진짜 왜 두번이나 갔는지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.12",
          "form": "오호! ㅋㅋㅋ",
          "original_form": "오호! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.13",
          "form": "거기도 캡사이신맛인가요??",
          "original_form": "거기도 캡사이신맛인가요??",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.14",
          "form": "담에 한번 먹으러가봐야겟어용",
          "original_form": "담에 한번 먹으러가봐야겟어용",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.15",
          "form": "움 아뇽 캡사이신맛이",
          "original_form": "움 아뇽 캡사이신맛이",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.16",
          "form": "확 나진않는데",
          "original_form": "확 나진않는데",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010258.1.17",
          "form": "조금은 들어간거같아용 ㅁ",
          "original_form": "조금은 들어간거같아용 ㅁ",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010258.1.18",
          "form": "아~ 신길동은 매우면 무리하지말라고 먹고 토한다고 하더라구요..  오마이 ...",
          "original_form": "아~ 신길동은 매우면 무리하지말라고 먹고 토한다고 하더라구요..  오마이 ...",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010258.1.19",
          "form": "근데 너무 유명해서 방송국에서 촬영온 사진들 엄청있고...",
          "original_form": "근데 너무 유명해서 방송국에서 촬영온 사진들 엄청있고...",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010258.1.20",
          "form": "요즘날씨에 짬뽕국물도 최고져!!",
          "original_form": "요즘날씨에 짬뽕국물도 최고져!!",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010258.1.21",
          "form": "혹시 샤브샤브는 어떤 체인점 선호하세요?",
          "original_form": "혹시 샤브샤브는 어떤 체인점 선호하세요?",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010258.1.22",
          "form": "저는 안가려용 샤브매니아 채선당 다 좋아해용 ~",
          "original_form": "저는 안가려용 샤브매니아 채선당 다 좋아해용 ~",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010258.1.23",
          "form": "저는 등촌샤브샤브 좋아해요! ㅎㅎ",
          "original_form": "저는 등촌샤브샤브 좋아해요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010258.1.24",
          "form": "볶음밥까지 제입맛에 딱 ㅜㅜ",
          "original_form": "볶음밥까지 제입맛에 딱 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010258.1.25",
          "form": "현대백화점에 1인샤브도 있는데 요기도 맛있어요~~",
          "original_form": "현대백화점에 1인샤브도 있는데 요기도 맛있어요~~",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010258.1.26",
          "form": "아 등촌도있네용 ㄹㄹ",
          "original_form": "아 등촌도있네용 ㄹㄹ",
          "speaker_id": "2",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010258.1.27",
          "form": "여기 미나리 들어간것도 좋고 ㅜㅜ 국물 예술",
          "original_form": "여기 미나리 들어간것도 좋고 ㅜㅜ 국물 예술",
          "speaker_id": "1",
          "time": "20210823 16:16"
        },
        {
          "id": "MDRW2100010258.1.28",
          "form": "비오니깐 샤브샤브 더 땡기네용",
          "original_form": "비오니깐 샤브샤브 더 땡기네용",
