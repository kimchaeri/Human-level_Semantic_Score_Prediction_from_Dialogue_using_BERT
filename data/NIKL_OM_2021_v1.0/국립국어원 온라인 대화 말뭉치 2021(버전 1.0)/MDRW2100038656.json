      "utterance": [
        {
          "id": "MDRW2100038656.1.1",
          "form": "전 집에 있는거 대충 먹을거같아요 점심 때 먹은 볶음밥이 아직도ㅜ",
          "original_form": "전 집에 있는거 대충 먹을거같아요 점심 때 먹은 볶음밥이 아직도ㅜ",
          "speaker_id": "2",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100038656.1.2",
          "form": "아하 어떤 볶음밥요",
          "original_form": "아하 어떤 볶음밥요",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038656.1.3",
          "form": "김볶에 체다랑 까망베르 올려먹었어요ㅋㅋ",
          "original_form": "김볶에 체다랑 까망베르 올려먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100038656.1.4",
          "form": "와 치즈가 다양하게 구비됐네요 ㅋㅋㅋ",
          "original_form": "와 치즈가 다양하게 구비됐네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038656.1.5",
          "form": "네ㅋㅋ인터넷에서 쟁인 치즈들이에요",
          "original_form": "네ㅋㅋ인터넷에서 쟁인 치즈들이에요",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100038656.1.6",
          "form": "까망베르 맛있어요! 추천!",
          "original_form": "까망베르 맛있어요! 추천!",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100038656.1.7",
          "form": "와... 김볶에 까망베르",
          "original_form": "와... 김볶에 까망베르",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100038656.1.8",
          "form": "부러워용...",
          "original_form": "부러워용...",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100038656.1.9",
          "form": "마싯겟다",
          "original_form": "마싯겟다",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100038656.1.10",
          "form": "치즈가 임박제품으로 자주 나오더라고요! ㅋㅋ그때 쟁여놔요ㅎㅎ",
          "original_form": "치즈가 임박제품으로 자주 나오더라고요! ㅋㅋ그때 쟁여놔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038656.1.11",
          "form": "오! 임박치즈",
          "original_form": "오! 임박치즈",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038656.1.12",
          "form": "젤 조아하는치즈 머에용",
          "original_form": "젤 조아하는치즈 머에용",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100038656.1.13",
          "form": "모짜요ㅋㅋ 기본템ㅋㅋ",
          "original_form": "모짜요ㅋㅋ 기본템ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100038656.1.14",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100038656.1.15",
          "form": "ㅋㅋㅋ 저도 모짜저아해요",
          "original_form": "ㅋㅋㅋ 저도 모짜저아해요",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038656.1.16",
          "form": "생모짜는 더맛잇..",
          "original_form": "생모짜는 더맛잇..",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038656.1.17",
          "form": "맞아요 치즈 너무 좋아요",
          "original_form": "맞아요 치즈 너무 좋아요",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038656.1.18",
          "form": "제가 그래서 조식뷔페좋아하는데... 요즘 ㅜㅠ",
          "original_form": "제가 그래서 조식뷔페좋아하는데... 요즘 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038656.1.19",
          "form": "ㅜㅜㅜ 조식뷔페 어디조아하셧어요",
          "original_form": "ㅜㅜㅜ 조식뷔페 어디조아하셧어요",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100038656.1.20",
          "form": "빵뷔페나 아님 숙박했을때 껴주는ㅋㅋ",
          "original_form": "빵뷔페나 아님 숙박했을때 껴주는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038656.1.21",
          "form": "진짜 왠만한 곳은 다 만족해요ㅋㅋ 불만없고요",
          "original_form": "진짜 왠만한 곳은 다 만족해요ㅋㅋ 불만없고요",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100038656.1.22",
          "form": "빵뷔페도 잇어요??",
          "original_form": "빵뷔페도 잇어요??",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100038656.1.23",
          "form": "네 근데 빵뷔페 단가 안맞는다고 많이들 닫았더라고요ㅜ",
          "original_form": "네 근데 빵뷔페 단가 안맞는다고 많이들 닫았더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210902 18:57"
        }
      ]
    }
