      "utterance": [
        {
          "id": "MDRW2100005657.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005657.1.2",
          "form": "저는 무교입니다",
          "original_form": "저는 무교입니다",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005657.1.3",
          "form": "저도 무교예요!",
          "original_form": "저도 무교예요!",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005657.1.4",
          "form": "가족들도 다 무교세요?",
          "original_form": "가족들도 다 무교세요?",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005657.1.5",
          "form": "저희집은 종교가 다 달라서ㅎㅎ",
          "original_form": "저희집은 종교가 다 달라서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005657.1.6",
          "form": "가족들은 다 기독교예요",
          "original_form": "가족들은 다 기독교예요",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005657.1.7",
          "form": "저도 어릴 때 기독교였는데",
          "original_form": "저도 어릴 때 기독교였는데",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005657.1.8",
          "form": "어릴 때 그냥 교회 다닌 거라..",
          "original_form": "어릴 때 그냥 교회 다닌 거라..",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005657.1.9",
          "form": "저도 어릴 때 억지로 다녔었어요",
          "original_form": "저도 어릴 때 억지로 다녔었어요",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005657.1.10",
          "form": "일요일 아침이 너무 싫었던 기억이 있네요",
          "original_form": "일요일 아침이 너무 싫었던 기억이 있네요",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005657.1.11",
          "form": "저도요ㅋㅋㅋㅋㅋ ㅜㅠ",
          "original_form": "저도요ㅋㅋㅋㅋㅋ ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005657.1.12",
          "form": "그래도 가끔 거기서 부르던 노래는 생각나요ㅋㅋㅋㅋ 아무생각 없이 부르다가 흠칫하게 되는 그런...",
          "original_form": "그래도 가끔 거기서 부르던 노래는 생각나요ㅋㅋㅋㅋ 아무생각 없이 부르다가 흠칫하게 되는 그런...",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005657.1.13",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005657.1.14",
          "form": "근데 또 요즘은",
          "original_form": "근데 또 요즘은",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005657.1.15",
          "form": "오히려 불교 정신이 좋더라고요",
          "original_form": "오히려 불교 정신이 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005657.1.16",
          "form": "아직 무교이긴 하지만",
          "original_form": "아직 무교이긴 하지만",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005657.1.17",
          "form": "불교 정신은 종교를 떠나서 되게 건강한 거 같아요!",
          "original_form": "불교 정신은 종교를 떠나서 되게 건강한 거 같아요!",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005657.1.18",
          "form": "종교는 또 언제든 바뀔 수도 있고 그런 거니까",
          "original_form": "종교는 또 언제든 바뀔 수도 있고 그런 거니까",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005657.1.19",
          "form": "신앙심이 생기면 그것도 나름 좋을 거 같기도 해요 요즘은",
          "original_form": "신앙심이 생기면 그것도 나름 좋을 거 같기도 해요 요즘은",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005657.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005657.1.21",
          "form": "뭔가 불교를 정말 믿고 그런 것보다는",
          "original_form": "뭔가 불교를 정말 믿고 그런 것보다는",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005657.1.22",
          "form": "삶을 더 이롭게 해줄 것 같아서",
          "original_form": "삶을 더 이롭게 해줄 것 같아서",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005657.1.23",
          "form": "관심 가네요",
          "original_form": "관심 가네요",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005657.1.24",
          "form": "그쵸 불교 생각하면 사람 마음이 좀 평온해 지는? 그런 기분이 들어서 좋기도 해요!\r\n어릴 땐 몰랐는데 이제는 절도 너무 멋있더라구요",
          "original_form": "그쵸 불교 생각하면 사람 마음이 좀 평온해 지는? 그런 기분이 들어서 좋기도 해요!\r\n어릴 땐 몰랐는데 이제는 절도 너무 멋있더라구요",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005657.1.25",
          "form": "맞아요 종교는 신앙심에서 비롯되는 거라지만 불교는 뭔가",
          "original_form": "맞아요 종교는 신앙심에서 비롯되는 거라지만 불교는 뭔가",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005657.1.26",
          "form": "절 가서 음식 먹고 내려오는 것까지",
          "original_form": "절 가서 음식 먹고 내려오는 것까지",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005657.1.27",
          "form": "힐링될 것 같더라규요",
          "original_form": "힐링될 것 같더라규요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005657.1.28",
          "form": "맞아요 절밥두 맛있고 건강하잖아요!",
          "original_form": "맞아요 절밥두 맛있고 건강하잖아요!",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005657.1.29",
          "form": "그렇죠ㅜ",
          "original_form": "그렇죠ㅜ",
          "speaker_id": "2",
          "time": "20210821 23:52"
        }
      ]
