        {
          "id": "MDRW2100014615.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014615.1.2",
          "form": "레몬맘",
          "original_form": "레몬맘",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014615.1.3",
          "form": "어머어머",
          "original_form": "어머어머",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014615.1.4",
          "form": "신기하군요",
          "original_form": "신기하군요",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014615.1.5",
          "form": "반모해 반모",
          "original_form": "반모해 반모",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014615.1.6",
          "form": "미용과 건강에 대해 말하래",
          "original_form": "미용과 건강에 대해 말하래",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.7",
          "form": "요새 다이어트 하니",
          "original_form": "요새 다이어트 하니",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.8",
          "form": "난 하는 중이야",
          "original_form": "난 하는 중이야",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.10",
          "form": "나는 안해..",
          "original_form": "나는 안해..",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.11",
          "form": "링피트를 시작했지",
          "original_form": "링피트를 시작했지",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.12",
          "form": "넌",
          "original_form": "넌",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.13",
          "form": "안하는구나",
          "original_form": "안하는구나",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.14",
          "form": "운동해야하긴 하는데",
          "original_form": "운동해야하긴 하는데",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.15",
          "form": "고민 중이야",
          "original_form": "고민 중이야",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.16",
          "form": "링피트는 어때?",
          "original_form": "링피트는 어때?",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.17",
          "form": "고민을 몇 년 했는지 질문해도 될까",
          "original_form": "고민을 몇 년 했는지 질문해도 될까",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014615.1.19",
          "form": "음~",
          "original_form": "음~",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.20",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.21",
          "form": "그래도 일하면서 쪘던 살은 뺐어",
          "original_form": "그래도 일하면서 쪘던 살은 뺐어",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.22",
          "form": "오 대단한걸",
          "original_form": "오 대단한걸",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.23",
          "form": "그것만으로도 만족해",
          "original_form": "그것만으로도 만족해",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.24",
          "form": "도너츠가 빠지다니",
          "original_form": "도너츠가 빠지다니",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.25",
          "form": "그럼 됐지",
          "original_form": "그럼 됐지",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.26",
          "form": "대단하지",
          "original_form": "대단하지",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.27",
          "form": "대단한일이야",
          "original_form": "대단한일이야",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.28",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.29",
          "form": "난 어제 운동해서",
          "original_form": "난 어제 운동해서",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.30",
          "form": "아침에 허벅지가 없었는데",
          "original_form": "아침에 허벅지가 없었는데",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014615.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.32",
          "form": "지금은 종아리가 없어",
          "original_form": "지금은 종아리가 없어",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.33",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.34",
          "form": "웃으면 안돼",
          "original_form": "웃으면 안돼",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.35",
          "form": "말을해야해",
          "original_form": "말을해야해",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.36",
          "form": "종아리 운동했구나",
          "original_form": "종아리 운동했구나",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.37",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.38",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.39",
          "form": "구나만 하는 구나",
          "original_form": "구나만 하는 구나",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.40",
          "form": "오늘까지 해본 결과",
          "original_form": "오늘까지 해본 결과",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.41",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.42",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.43",
          "form": "힘들어",
          "original_form": "힘들어",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.44",
          "form": "후회스러워",
          "original_form": "후회스러워",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014615.1.45",
          "form": "후회까지 하는구나",
          "original_form": "후회까지 하는구나",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.46",
          "form": "샀으니 운동해야하는데",
          "original_form": "샀으니 운동해야하는데",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.47",
          "form": "갈길이 머네",
          "original_form": "갈길이 머네",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.48",
          "form": "그래도 게임이 있어서",
          "original_form": "그래도 게임이 있어서",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.49",
          "form": "조금은 재밌지않아?",
          "original_form": "조금은 재밌지않아?",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.50",
          "form": "몬스터를 죽이고 싶어져",
          "original_form": "몬스터를 죽이고 싶어져",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.51",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.52",
          "form": "최선을 다하지",
          "original_form": "최선을 다하지",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014615.1.53",
          "form": "친구",
          "original_form": "친구",
          "speaker_id": "1",
          "time": "20210825 20:04"
        }
      ]
    }
  ]
