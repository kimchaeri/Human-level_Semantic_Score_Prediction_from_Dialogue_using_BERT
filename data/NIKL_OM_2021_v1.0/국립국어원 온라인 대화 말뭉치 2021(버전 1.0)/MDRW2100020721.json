      "utterance": [
        {
          "id": "MDRW2100020721.1.1",
          "form": "어떤 음식 좋아하세요??",
          "original_form": "어떤 음식 좋아하세요??",
          "speaker_id": "1",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020721.1.2",
          "form": "제 최애는 순대국입니다",
          "original_form": "제 최애는 순대국입니다",
          "speaker_id": "2",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020721.1.3",
          "form": "순대국 좋아해요",
          "original_form": "순대국 좋아해요",
          "speaker_id": "1",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100020721.1.4",
          "form": "예전에 많이 먹었는데",
          "original_form": "예전에 많이 먹었는데",
          "speaker_id": "1",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100020721.1.5",
          "form": "고기만요 순대만요",
          "original_form": "고기만요 순대만요",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100020721.1.6",
          "form": "둘다 다좋아해요",
          "original_form": "둘다 다좋아해요",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100020721.1.7",
          "form": "골고루 다먹었어요",
          "original_form": "골고루 다먹었어요",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100020721.1.8",
          "form": "전 고기파 ㅎㅎ",
          "original_form": "전 고기파 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100020721.1.9",
          "form": "고기는 당연히 사랑하죠",
          "original_form": "고기는 당연히 사랑하죠",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100020721.1.10",
          "form": "또 뭐 좋아하세요",
          "original_form": "또 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100020721.1.11",
          "form": "치킨도 완전 좋아하구요",
          "original_form": "치킨도 완전 좋아하구요",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100020721.1.12",
          "form": "피자 햄버거......",
          "original_form": "피자 햄버거......",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100020721.1.13",
          "form": "초딩입맛이에요",
          "original_form": "초딩입맛이에요",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100020721.1.14",
          "form": "맛있으니까 그게 정답입니다",
          "original_form": "맛있으니까 그게 정답입니다",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100020721.1.15",
          "form": "그니깐요 맛있으니깐 먹는거죠",
          "original_form": "그니깐요 맛있으니깐 먹는거죠",
          "speaker_id": "1",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100020721.1.16",
          "form": "저녁에는 치킨먹었어요",
          "original_form": "저녁에는 치킨먹었어요",
          "speaker_id": "1",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100020721.1.17",
          "form": "후식으로 스벅가서 음료먹구요",
          "original_form": "후식으로 스벅가서 음료먹구요",
          "speaker_id": "1",
          "time": "20210830 09:12"
        },
        {
          "id": "MDRW2100020721.1.18",
          "form": "ㅋㅋ스벅 좋아하세요?",
          "original_form": "ㅋㅋ스벅 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100020721.1.19",
          "form": "네 스벅 완전 좋아합니다",
          "original_form": "네 스벅 완전 좋아합니다",
          "speaker_id": "1",
