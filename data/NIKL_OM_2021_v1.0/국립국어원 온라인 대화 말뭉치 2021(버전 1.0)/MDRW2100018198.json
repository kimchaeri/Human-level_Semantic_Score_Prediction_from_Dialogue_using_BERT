      "utterance": [
        {
          "id": "MDRW2100018198.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018198.1.2",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018198.1.3",
          "form": "연애하시나요;",
          "original_form": "연애하시나요;",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018198.1.4",
          "form": "넵 남자친구 있어요ㅎㅎ님은요?",
          "original_form": "넵 남자친구 있어요ㅎㅎ님은요?",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018198.1.5",
          "form": "ㅋㅋㅋ 없어염",
          "original_form": "ㅋㅋㅋ 없어염",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018198.1.6",
          "form": "ㅋㅋ좋아하는사람은요?",
          "original_form": "ㅋㅋ좋아하는사람은요?",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018198.1.7",
          "form": "없습니다ㅜㅜ",
          "original_form": "없습니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018198.1.8",
          "form": "처음에 어떻게 만나셨나용",
          "original_form": "처음에 어떻게 만나셨나용",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018198.1.9",
          "form": "소개받았어요~",
          "original_form": "소개받았어요~",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018198.1.10",
          "form": "부럽네요 확실히 학생 벗어나면",
          "original_form": "부럽네요 확실히 학생 벗어나면",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018198.1.11",
          "form": "아무래도 확실히 자연스럽게 사귀기는 힘들어지는것같네여..",
          "original_form": "아무래도 확실히 자연스럽게 사귀기는 힘들어지는것같네여..",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018198.1.12",
          "form": "맞아요 뭐 모임이나 친구들 소개로 만나는 방법밖엔",
          "original_form": "맞아요 뭐 모임이나 친구들 소개로 만나는 방법밖엔",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018198.1.13",
          "form": "딱히 없더라구요",
          "original_form": "딱히 없더라구요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018198.1.14",
          "form": "하핫 얼마나 사귀셨나용",
          "original_form": "하핫 얼마나 사귀셨나용",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018198.1.15",
          "form": "전 마지막 연애가 2년 전이네요~!!!!ㅋㅋㅋ",
          "original_form": "전 마지막 연애가 2년 전이네요~!!!!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018198.1.16",
          "form": "저는 곧 1년이예요!!!",
          "original_form": "저는 곧 1년이예요!!!",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018198.1.17",
          "form": "오 얼마나 사귀셨어요?",
          "original_form": "오 얼마나 사귀셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018198.1.18",
          "form": "저 그때 5개월?",
          "original_form": "저 그때 5개월?",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018198.1.19",
          "form": "취준할때라 되게 쪼잔한 연애를 했네염 ㅋㅋ",
          "original_form": "취준할때라 되게 쪼잔한 연애를 했네염 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018198.1.20",
          "form": "아하ㅎㅎ뭐 그런것두 다 추억이고 그런거죠",
          "original_form": "아하ㅎㅎ뭐 그런것두 다 추억이고 그런거죠",
