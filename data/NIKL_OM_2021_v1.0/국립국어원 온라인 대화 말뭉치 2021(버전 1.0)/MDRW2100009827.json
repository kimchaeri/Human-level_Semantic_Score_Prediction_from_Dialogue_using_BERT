      "utterance": [
        {
          "id": "MDRW2100009827.1.1",
          "form": "오늘 점심은 뭐야?",
          "original_form": "오늘 점심은 뭐야?",
          "speaker_id": "2",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009827.1.2",
          "form": "오늘 우린 북어국 ㅎㅎ",
          "original_form": "오늘 우린 북어국 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009827.1.3",
          "form": "오 나도 좋아하는데 ㅎㅎ",
          "original_form": "오 나도 좋아하는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009827.1.4",
          "form": "신랑은 회사에서 맛있는 거 먹어용 ^^",
          "original_form": "신랑은 회사에서 맛있는 거 먹어용 ^^",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009827.1.5",
          "form": "북어국에 김치 넣어 먹으면 완전 꿀맛인데",
          "original_form": "북어국에 김치 넣어 먹으면 완전 꿀맛인데",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009827.1.6",
          "form": "ㅎㅎ 맛있지~ 안그래도 김치 썰어놨어",
          "original_form": "ㅎㅎ 맛있지~ 안그래도 김치 썰어놨어",
          "speaker_id": "1",
          "time": "20210823 10:36"
        },
        {
          "id": "MDRW2100009827.1.7",
          "form": "집김치 잘 익었던데 맛있겠다",
          "original_form": "집김치 잘 익었던데 맛있겠다",
          "speaker_id": "2",
          "time": "20210823 10:36"
        },
        {
          "id": "MDRW2100009827.1.8",
          "form": "애들 수업 끝나면 바로 밥먹어야 해서 지금 계속 준비중이야",
          "original_form": "애들 수업 끝나면 바로 밥먹어야 해서 지금 계속 준비중이야",
          "speaker_id": "1",
          "time": "20210823 10:36"
        },
        {
          "id": "MDRW2100009827.1.9",
          "form": "고생이 많네",
          "original_form": "고생이 많네",
          "speaker_id": "2",
          "time": "20210823 10:36"
        },
        {
          "id": "MDRW2100009827.1.10",
          "form": "ㅎㅎ 요즘 항상 바쁘네",
          "original_form": "ㅎㅎ 요즘 항상 바쁘네",
          "speaker_id": "1",
          "time": "20210823 10:38"
        },
        {
          "id": "MDRW2100009827.1.11",
          "form": "글치 국에 반찬 몇 가지만 해도 오전이 훅 지나가니",
          "original_form": "글치 국에 반찬 몇 가지만 해도 오전이 훅 지나가니",
          "speaker_id": "2",
          "time": "20210823 10:38"
        },
        {
          "id": "MDRW2100009827.1.12",
          "form": "ㅇㅇ 요즘 좀 정신이 없어",
          "original_form": "ㅇㅇ 요즘 좀 정신이 없어",
          "speaker_id": "1",
          "time": "20210823 10:38"
        },
        {
          "id": "MDRW2100009827.1.13",
          "form": "간단음식으로 먹어",
          "original_form": "간단음식으로 먹어",
          "speaker_id": "2",
          "time": "20210823 10:38"
        },
        {
          "id": "MDRW2100009827.1.14",
          "form": "볶음밥 카레 이런거 너무 많이 줘서 ㅎㅎ",
          "original_form": "볶음밥 카레 이런거 너무 많이 줘서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:39"
        },
        {
          "id": "MDRW2100009827.1.15",
          "form": "북어국만 먹어도 영양만점이지 다 들어있잖아 ㅎㅎ",
          "original_form": "북어국만 먹어도 영양만점이지 다 들어있잖아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:39"
        },
        {
          "id": "MDRW2100009827.1.16",
          "form": "북어국에 두부, 계란, 무, 북어 들어있으니 영양만점이긴 하지 ㅎㅎ",
          "original_form": "북어국에 두부, 계란, 무, 북어 들어있으니 영양만점이긴 하지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:40"
        }
      ]
