      "utterance": [
        {
          "id": "MDRW2100045803.1.1",
          "form": "퓰리처상이 언론사사진 맞죠",
          "original_form": "퓰리처상이 언론사사진 맞죠",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045803.1.2",
          "form": "네 그렇게 알고 있어요",
          "original_form": "네 그렇게 알고 있어요",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045803.1.3",
          "form": "뜻깊은 전시네요",
          "original_form": "뜻깊은 전시네요",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045803.1.4",
          "form": "그리고 이건희 전이요",
          "original_form": "그리고 이건희 전이요",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045803.1.5",
          "form": "아 소장품",
          "original_form": "아 소장품",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045803.1.6",
          "form": "네 경쟁이 치열해 더라고요",
          "original_form": "네 경쟁이 치열해 더라고요",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045803.1.7",
          "form": "예약 힘들다고 들었어요",
          "original_form": "예약 힘들다고 들었어요",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045803.1.8",
          "form": "금방 관심이 식을 줄 알았는데",
          "original_form": "금방 관심이 식을 줄 알았는데",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045803.1.9",
          "form": "아무래도 워낙 방대한 작품이라",
          "original_form": "아무래도 워낙 방대한 작품이라",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045803.1.10",
          "form": "지금 벌써 한 달이 지나도록 경쟁이 엄청 치열해요",
          "original_form": "지금 벌써 한 달이 지나도록 경쟁이 엄청 치열해요",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045803.1.11",
          "form": "삼성이 이미지세탁 제대로 했죠",
          "original_form": "삼성이 이미지세탁 제대로 했죠",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045803.1.12",
          "form": "맞아요 사회환원은 무슨ㅋㅋ",
          "original_form": "맞아요 사회환원은 무슨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045803.1.13",
          "form": "어쩔수없이 내놓은걸 무슨 숭고한 결정처럼",
          "original_form": "어쩔수없이 내놓은걸 무슨 숭고한 결정처럼",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045803.1.14",
          "form": "저도 그런 이야기들을 때 웃기더라구요",
          "original_form": "저도 그런 이야기들을 때 웃기더라구요",
