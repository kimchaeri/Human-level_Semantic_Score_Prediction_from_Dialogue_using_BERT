        },
        {
          "id": "MDRW2100029660.1.2",
          "form": "안하시나요",
          "original_form": "안하시나요",
          "speaker_id": "2",
          "time": "20210903 11:28"
        },
        {
          "id": "MDRW2100029660.1.3",
          "form": "고스톱할때 생각합니다",
          "original_form": "고스톱할때 생각합니다",
          "speaker_id": "1",
          "time": "20210903 11:28"
        },
        {
          "id": "MDRW2100029660.1.4",
          "form": "고스톱은",
          "original_form": "고스톱은",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.5",
          "form": "저돈이 내돈이엇으면",
          "original_form": "저돈이 내돈이엇으면",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.7",
          "form": "게임머니만 부자입니다",
          "original_form": "게임머니만 부자입니다",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.8",
          "form": "돈을많이따면",
          "original_form": "돈을많이따면",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.9",
          "form": "건물도 사겟어요",
          "original_form": "건물도 사겟어요",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.10",
          "form": "기본단위가 높은곳으로",
          "original_form": "기본단위가 높은곳으로",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.11",
          "form": "올라가야해서",
          "original_form": "올라가야해서",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.12",
          "form": "현타옵니다",
          "original_form": "현타옵니다",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.13",
          "form": "그러다 거지되지요",
          "original_form": "그러다 거지되지요",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.14",
          "form": "바로 잃던데요",
          "original_form": "바로 잃던데요",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.15",
          "form": "실력이 어중간",
          "original_form": "실력이 어중간",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.16",
          "form": "인생 한방이죠",
          "original_form": "인생 한방이죠",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.17",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029660.1.18",
