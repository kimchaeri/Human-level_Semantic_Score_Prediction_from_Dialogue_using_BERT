      "utterance": [
        {
          "id": "MDRW2100042692.1.1",
          "form": "니 친구는 연락왔냐",
          "original_form": "니 친구는 연락왔냐",
          "speaker_id": "2",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042692.1.2",
          "form": "ㅇㅇ아깐 정신없는지 확인도 못하더니",
          "original_form": "ㅇㅇ아깐 정신없는지 확인도 못하더니",
          "speaker_id": "1",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042692.1.3",
          "form": "연락옴",
          "original_form": "연락옴",
          "speaker_id": "1",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042692.1.4",
          "form": "어느병원이래?",
          "original_form": "어느병원이래?",
          "speaker_id": "2",
          "time": "20210907 01:47"
        },
        {
          "id": "MDRW2100042692.1.5",
          "form": "광안 좋은강안병원이래",
          "original_form": "광안 좋은강안병원이래",
          "speaker_id": "1",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100042692.1.6",
          "form": "아 거기..",
          "original_form": "아 거기..",
          "speaker_id": "2",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100042692.1.7",
          "form": "거기도 크지",
          "original_form": "거기도 크지",
          "speaker_id": "2",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100042692.1.8",
          "form": "거기 고신대에서",
          "original_form": "거기 고신대에서",
          "speaker_id": "1",
          "time": "20210907 01:53"
        },
        {
          "id": "MDRW2100042692.1.9",
          "form": "이번에 새로 스카웃해온 의사가 잘본대",
          "original_form": "이번에 새로 스카웃해온 의사가 잘본대",
          "speaker_id": "1",
          "time": "20210907 01:54"
        },
        {
          "id": "MDRW2100042692.1.10",
          "form": "고신대가 암으로 유명하긴하지",
          "original_form": "고신대가 암으로 유명하긴하지",
          "speaker_id": "2",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042692.1.11",
          "form": "ㅇㅇ그렇다대",
          "original_form": "ㅇㅇ그렇다대",
          "speaker_id": "1",
          "time": "20210907 01:55"
        },
        {
          "id": "MDRW2100042692.1.12",
          "form": "name3?그 교수라던데 유방암 잘본대",
          "original_form": "&name3&?그 교수라던데 유방암 잘본대",
          "speaker_id": "1",
          "time": "20210907 01:56"
        },
        {
          "id": "MDRW2100042692.1.13",
          "form": "검색 좀 해보자",
          "original_form": "검색 좀 해보자",
          "speaker_id": "2",
          "time": "20210907 01:56"
        },
        {
          "id": "MDRW2100042692.1.14",
          "form": "잘하는 의사 맞나보다",
          "original_form": "잘하는 의사 맞나보다",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042692.1.15",
          "form": "유명하네",
          "original_form": "유명하네",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042692.1.16",
          "form": "ㅇㅇ유방암쪽 유명의사라는데",
          "original_form": "ㅇㅇ유방암쪽 유명의사라는데",
          "speaker_id": "1",
          "time": "20210907 02:00"
        },
        {
          "id": "MDRW2100042692.1.17",
          "form": "암 거의 확신했다니까 진짜 걱정될듯",
          "original_form": "암 거의 확신했다니까 진짜 걱정될듯",
          "speaker_id": "1",
          "time": "20210907 02:01"
        },
        {
          "id": "MDRW2100042692.1.18",
          "form": "아직 조직검사 결과는 안나왔다며",
          "original_form": "아직 조직검사 결과는 안나왔다며",
          "speaker_id": "2",
          "time": "20210907 02:01"
        },
        {
          "id": "MDRW2100042692.1.19",
          "form": "근데 초음파 모양보고 확신했대ㅠㅜ",
          "original_form": "근데 초음파 모양보고 확신했대ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 02:02"
        },
        {
          "id": "MDRW2100042692.1.20",
          "form": "목욜 입원 잡았다더라",
