      "utterance": [
        {
          "id": "MDRW2100007404.1.1",
          "form": "저는 딱한번 찍고",
          "original_form": "저는 딱한번 찍고",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007404.1.2",
          "form": "완전 멋있어...",
          "original_form": "완전 멋있어...{emoji:☆}",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007404.1.3",
          "form": "계속 하락세라... 맘잡고 복구해보려고요...ㅋㅋㅋ",
          "original_form": "계속 하락세라... 맘잡고 복구해보려고요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007404.1.4",
          "form": "앗... ㅋㅋㅋ",
          "original_form": "앗... ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007404.1.5",
          "form": "하락세는 올라갈일만 남았다는 뜻입니다..!",
          "original_form": "하락세는 올라갈일만 남았다는 뜻입니다..!",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007404.1.6",
          "form": "감사해요!!!! 2학기 힘냅시다 ㅎㅎ",
          "original_form": "감사해요!!{emoji:♡}!! 2학기 힘냅시다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007404.1.7",
          "form": "2학기 저희는 대면 강행한다는데",
          "original_form": "2학기 저희는 대면 강행한다는데",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007404.1.8",
          "form": "전체 비대면 하시나요??",
          "original_form": "전체 비대면 하시나요??",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007404.1.9",
          "form": "저는 이론과목은 비대면이라 녹강이고",
          "original_form": "저는 이론과목은 비대면이라 녹강이고",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007404.1.10",
          "form": "교내실습은 대면이라",
          "original_form": "교내실습은 대면이라",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007404.1.11",
          "form": "1주일에 2일 정도 나가용",
          "original_form": "1주일에 2일 정도 나가용",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007404.1.12",
          "form": "뭔가 부러워요..",
          "original_form": "뭔가 부러워요..",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007404.1.13",
          "form": "저흰 이론 수업만 하는데 4일 나가요...",
          "original_form": "저흰 이론 수업만 하는데 4일 나가요...",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007404.1.14",
          "form": "아니 대면 장난하는건가...",
          "original_form": "아니 대면 장난하는건가...",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007404.1.15",
          "form": "근데 저희 1주일만 저렇게 하는거고",
          "original_form": "근데 저희 1주일만 저렇게 하는거고",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007404.1.16",
          "form": "상황보고 정한대요 ...",
          "original_form": "상황보고 정한대요 ...",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007404.1.17",
          "form": "근데 지금 대면은 정말",
          "original_form": "근데 지금 대면은 정말",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007404.1.18",
          "form": "아닌거같아요...ㅜㅜ",
          "original_form": "아닌거같아요...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007404.1.19",
          "form": "눈치게임 하는 느낌..?",
          "original_form": "눈치게임 하는 느낌..?",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007404.1.20",
          "form": "진짜 이론수업 대면 강행한다고 수업의 질에 크게 차이는 없는데",
          "original_form": "진짜 이론수업 대면 강행한다고 수업의 질에 크게 차이는 없는데",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007404.1.21",
          "form": "그냥 등록금 깎아주기 싫어서라고 밖에 안 보여요...ㅎ",
          "original_form": "그냥 등록금 깎아주기 싫어서라고 밖에 안 보여요...ㅎ",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007404.1.22",
          "form": "그쵸... 대면 비대면 다해봤는데 저는 오히려 비대면일때가 나은거 같아요...",
          "original_form": "그쵸... 대면 비대면 다해봤는데 저는 오히려 비대면일때가 나은거 같아요...",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007404.1.23",
          "form": "별로 깎아주지도 않으면서...그냥 전면 비대면 했으면ㅜㅜ",
          "original_form": "별로 깎아주지도 않으면서...그냥 전면 비대면 했으면ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007404.1.24",
          "form": "저희 시험은 강경대면으로 치는데",
          "original_form": "저희 시험은 강경대면으로 치는데",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007404.1.25",
          "form": "왜 그러는지 진짜 모르겠어요",
          "original_form": "왜 그러는지 진짜 모르겠어요",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007404.1.26",
          "form": "저희도 시험은 대면으로 꼭 보는데",
          "original_form": "저희도 시험은 대면으로 꼭 보는데",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007404.1.27",
          "form": "근데 비대면으로 한번 봤을때",
          "original_form": "근데 비대면으로 한번 봤을때",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007404.1.28",
          "form": "전 좀 불편했어요 ㅜㅜ",
          "original_form": "전 좀 불편했어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007404.1.29",
          "form": "시험 비대면은 은근 까다롭더라고요",
          "original_form": "시험 비대면은 은근 까다롭더라고요",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007404.1.30",
          "form": "저희도 레포트로 받고 화상 시험으로 치고 해봤는데",
          "original_form": "저희도 레포트로 받고 화상 시험으로 치고 해봤는데",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007404.1.31",
          "form": "레포트가 그나마 나았던거 같아요",
          "original_form": "레포트가 그나마 나았던거 같아요",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007404.1.32",
          "form": "그리구 절대평가로 바꿔서 점수좀 퍼주면 좋을거 같은데",
          "original_form": "그리구 절대평가로 바꿔서 점수좀 퍼주면 좋을거 같은데",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007404.1.33",
          "form": "절대 안바꿔주더라구요 ...ㅜ",
          "original_form": "절대 안바꿔주더라구요 ...ㅜ",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007404.1.34",
          "form": "진짜 교수님들 너무하세요",
          "original_form": "진짜 교수님들 너무하세요",
