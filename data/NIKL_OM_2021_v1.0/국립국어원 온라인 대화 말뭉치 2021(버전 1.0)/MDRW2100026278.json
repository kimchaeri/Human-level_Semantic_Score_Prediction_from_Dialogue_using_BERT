      "utterance": [
        {
          "id": "MDRW2100026278.1.1",
          "form": "요즘 유튭 뭐 보세여?",
          "original_form": "요즘 유튭 뭐 보세여?",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026278.1.2",
          "form": "먹방봤어요",
          "original_form": "먹방봤어요",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026278.1.3",
          "form": "먹방.. 주로 누구보세요?",
          "original_form": "먹방.. 주로 누구보세요?",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026278.1.4",
          "form": "음....천뚱도보고 쯔양도보고 햇님도보고 히밥도보고",
          "original_form": "음....천뚱도보고 쯔양도보고 햇님도보고 히밥도보고",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026278.1.5",
          "form": "그런걱ㅌ어요",
          "original_form": "그런걱ㅌ어요",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026278.1.6",
          "form": "name1님은 뭐보시나요",
          "original_form": "&name1&님은 뭐보시나요",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026278.1.7",
          "form": "전 고양이 봐요",
          "original_form": "전 고양이 봐요",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026278.1.8",
          "form": "요즘은 어몽어스빠져서",
          "original_form": "요즘은 어몽어스빠져서",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026278.1.9",
          "form": "한가득 군림보만 봤어요",
          "original_form": "한가득 군림보만 봤어요",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026278.1.10",
          "form": "ㅋㅋㅋ어몽어스 재밌나요",
          "original_form": "ㅋㅋㅋ어몽어스 재밌나요",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026278.1.11",
          "form": "재미는있는데",
          "original_form": "재미는있는데",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026278.1.12",
          "form": "못해서 슬퍼요",
          "original_form": "못해서 슬퍼요",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026278.1.13",
          "form": "플레이를 잘해야하는데",
          "original_form": "플레이를 잘해야하는데",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026278.1.14",
          "form": "플레이쫄보",
          "original_form": "플레이쫄보",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026278.1.15",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026278.1.16",
          "form": "음악은 듣나요?",
          "original_form": "음악은 듣나요?",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026278.1.17",
          "form": "유튜브로요?",
          "original_form": "유튜브로요?",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026278.1.18",
          "form": "아무거나요 ㅎㅎ",
          "original_form": "아무거나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026278.1.19",
          "form": "ㄷ들어요",
          "original_form": "ㄷ들어요",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026278.1.20",
          "form": "폰바꿔서",
          "original_form": "폰바꿔서",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026278.1.21",
          "form": "...아놔",
          "original_form": "...아놔",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026278.1.22",
          "form": "지니를 강제로 쓰고있어요",
          "original_form": "지니를 강제로 쓰고있어요",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026278.1.23",
          "form": "아하 지니군요",
          "original_form": "아하 지니군요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026278.1.24",
          "form": "어떤노래듣나요",
          "original_form": "어떤노래듣나요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026278.1.25",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026278.1.26",
          "form": "요즘 아이유 악동뮤지션의 낙하",
          "original_form": "요즘 아이유 악동뮤지션의 낙하",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026278.1.27",
          "form": "들어요",
          "original_form": "들어요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026278.1.28",
          "form": "중독성잇더라구요",
          "original_form": "중독성잇더라구요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        }
      ]
    }
