          "id": "MDRW2100016027.1.1",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 08:13"
        },
        {
          "id": "MDRW2100016027.1.2",
          "form": "저는 원래 먹지 않았다가",
          "original_form": "저는 원래 먹지 않았다가",
          "speaker_id": "2",
          "time": "20210826 08:13"
        },
        {
          "id": "MDRW2100016027.1.3",
          "form": "잘 안드시는구나",
          "original_form": "잘 안드시는구나",
          "speaker_id": "1",
          "time": "20210826 08:13"
        },
        {
          "id": "MDRW2100016027.1.4",
          "form": "최근에 먹어요",
          "original_form": "최근에 먹어요",
          "speaker_id": "2",
          "time": "20210826 08:13"
        },
        {
          "id": "MDRW2100016027.1.5",
          "form": "아-점-간",
          "original_form": "아-점-간",
          "speaker_id": "2",
          "time": "20210826 08:13"
        },
        {
          "id": "MDRW2100016027.1.6",
          "form": "그럼 아침 메뉴는 정했나요",
          "original_form": "그럼 아침 메뉴는 정했나요",
          "speaker_id": "1",
          "time": "20210826 08:13"
        },
        {
          "id": "MDRW2100016027.1.7",
          "form": "아-(간)-저",
          "original_form": "아-(간)-저",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.8",
          "form": "이런식으로ㅎㅎ",
          "original_form": "이런식으로ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.9",
          "form": "저 해장해야돼서",
          "original_form": "저 해장해야돼서",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.10",
          "form": "아 중간에 간식타임을 갖으시는구나",
          "original_form": "아 중간에 간식타임을 갖으시는구나",
          "speaker_id": "1",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.11",
          "form": "콩나물국?",
          "original_form": "콩나물국?",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.12",
          "form": "해장국?",
          "original_form": "해장국?",
          "speaker_id": "1",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.13",
          "form": "끓여야겠네요",
          "original_form": "끓여야겠네요",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.14",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100016027.1.15",
          "form": "아 청 양 고 추 잔 뜩 넣 어서 끓 여 드 세 요",
          "original_form": "아 청 양 고 추 잔 뜩 넣 어서 끓 여 드 세 요",
          "speaker_id": "1",
          "time": "20210826 08:15"
        },
        {
          "id": "MDRW2100016027.1.16",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 08:15"
        },
        {
          "id": "MDRW2100016027.1.17",
          "form": "네에 감사합니다",
          "original_form": "네에 감사합니다",
          "speaker_id": "2",
          "time": "20210826 08:15"
        },
        {
          "id": "MDRW2100016027.1.18",
          "form": "모르겠어요 끓 여드세여",
          "original_form": "모르겠어요 끓 여드세여",
          "speaker_id": "1",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100016027.1.19",
          "form": "네에ㅜ",
          "original_form": "네에ㅜ",
          "speaker_id": "2",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100016027.1.20",
          "form": "끓여먹어야쥬",
          "original_form": "끓여먹어야쥬",
          "speaker_id": "2",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100016027.1.21",
