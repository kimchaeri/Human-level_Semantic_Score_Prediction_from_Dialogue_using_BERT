      "utterance": [
        {
          "id": "MDRW2100039399.1.1",
          "form": "저는 필리핀도 좋았어요",
          "original_form": "저는 필리핀도 좋았어요",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039399.1.2",
          "form": "어떤 섬갔는데",
          "original_form": "어떤 섬갔는데",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039399.1.3",
          "form": "아바타영화아세요?",
          "original_form": "아바타영화아세요?",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039399.1.4",
          "form": "네!!! 아바타 엄청 재밌게 봤어요!!",
          "original_form": "네!!! 아바타 엄청 재밌게 봤어요!!",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039399.1.5",
          "form": "거기 감독이 폭포보고 영감을 얻었대요",
          "original_form": "거기 감독이 폭포보고 영감을 얻었대요",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039399.1.6",
          "form": "이름을 까먹었는데..",
          "original_form": "이름을 까먹었는데..",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039399.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039399.1.8",
          "form": "엄청 멋있을 것 같아요!!",
          "original_form": "엄청 멋있을 것 같아요!!",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100039399.1.9",
          "form": "네 멋있었어요!!",
          "original_form": "네 멋있었어요!!",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100039399.1.10",
          "form": "고래상어도 보고",
          "original_form": "고래상어도 보고",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100039399.1.11",
          "form": "고래상어가 뭔지 방금 찾아봤는데",
          "original_form": "고래상어가 뭔지 방금 찾아봤는데",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100039399.1.12",
          "form": "저 상어들이 돌아다녀도 안전한가요?",
          "original_form": "저 상어들이 돌아다녀도 안전한가요?",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100039399.1.13",
          "form": "네네 근데 쟤네를 만지면 안된대요",
          "original_form": "네네 근데 쟤네를 만지면 안된대요",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039399.1.14",
          "form": "조그만 새우만 먹는대요",
          "original_form": "조그만 새우만 먹는대요",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039399.1.15",
          "form": "완전 크고..신기해요",
          "original_form": "완전 크고..신기해요",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039399.1.16",
          "form": "아 새우만 먹어요? 그래서 고래상어군요..! 신기해요",
          "original_form": "아 새우만 먹어요? 그래서 고래상어군요..! 신기해요",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039399.1.17",
          "form": "코로나 끝나면 꼭 가고 싶어요ㅠ",
          "original_form": "코로나 끝나면 꼭 가고 싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039399.1.18",
          "form": "일본 말고 가본곳이 없어서ㅓㅜㅠㅠㅠㅠ",
          "original_form": "일본 말고 가본곳이 없어서ㅓㅜㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039399.1.19",
          "form": "그죠ㅜㅠㅜ",
          "original_form": "그죠ㅜㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039399.1.20",
          "form": "자도 저기 둘이 끝이에요ㅠㅠ",
          "original_form": "자도 저기 둘이 끝이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039399.1.21",
          "form": "특별히 가고 싶은 나라 있나요?",
          "original_form": "특별히 가고 싶은 나라 있나요?",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039399.1.22",
          "form": "저는..홍콩??",
          "original_form": "저는..홍콩??",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039399.1.23",
          "form": "name1님 어디가고싶으세요?",
          "original_form": "&name1&님 어디가고싶으세요?",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039399.1.24",
          "form": "전 프랑스요!! 사실 지금 돈 모으는 중입니다ㅋㅋ",
          "original_form": "전 프랑스요!! 사실 지금 돈 모으는 중입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039399.1.25",
          "form": "3년 후에 파리 올림픽을 하는데",
          "original_form": "3년 후에 파리 올림픽을 하는데",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039399.1.26",
          "form": "여름에 꼭 가려고요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "여름에 꼭 가려고요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039399.1.27",
          "form": "이번 올림픽 보면서 느꼈어요. 다음엔 꼭 직관을 가야지..",
          "original_form": "이번 올림픽 보면서 느꼈어요. 다음엔 꼭 직관을 가야지..{emoji:☆}",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039399.1.28",
          "form": "우와!!!!!",
          "original_form": "우와!!!!!",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039399.1.29",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039399.1.30",
          "form": "얼마정도 목표에요??",
          "original_form": "얼마정도 목표에요??",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039399.1.31",
          "form": "최소 2주 정도 있다 오고 싶어요!! 그때 괜찮다면 한 달까지 있고 싶은데....",
          "original_form": "최소 2주 정도 있다 오고 싶어요!! 그때 괜찮다면 한 달까지 있고 싶은데....",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039399.1.32",
          "form": "어떻게 될진 아직 모르겠네요ㅎㅎㅎ",
          "original_form": "어떻게 될진 아직 모르겠네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039399.1.33",
          "form": "오 얼마정도 모을예정이에요??",
          "original_form": "오 얼마정도 모을예정이에요??",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039399.1.34",
          "form": "얼마나 모아야할까여",
          "original_form": "얼마나 모아야할까여",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039399.1.35",
          "form": "음 최대한 아껴서 비행기표 빼고 500 생각중이에요! 근데 숙소를 싸게 가면 많이 줄어들것 같아요",
          "original_form": "음 최대한 아껴서 비행기표 빼고 500 생각중이에요! 근데 숙소를 싸게 가면 많이 줄어들것 같아요",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100039399.1.36",
          "form": "유럽 주변국들 돌면 더 들어갈 것 같기도 하구요!",
          "original_form": "유럽 주변국들 돌면 더 들어갈 것 같기도 하구요!",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039399.1.37",
          "form": "우와 좋네요~~!!",
          "original_form": "우와 좋네요~~!!",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039399.1.38",
          "form": "진짜 여행가면 돈이 문제인데",
          "original_form": "진짜 여행가면 돈이 문제인데",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039399.1.39",
          "form": "너무 좋을것같아여",
          "original_form": "너무 좋을것같아여",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039399.1.40",
          "form": "돈을 정말 최대한 아끼려구요ㅎㅎㅎ",
          "original_form": "돈을 정말 최대한 아끼려구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039399.1.41",
          "form": "가성비 여행-",
          "original_form": "가성비 여행-{emoji:☆}",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039399.1.42",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039399.1.43",
          "form": "여행 저두 가고 싶네요ㅠㅠ",
          "original_form": "여행 저두 가고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039399.1.44",
          "form": "혼자가실 생각이신가요?",
          "original_form": "혼자가실 생각이신가요?",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039399.1.45",
          "form": "음 혼자는 조금 무서워서 친구랑 가려고요",
          "original_form": "음 혼자는 조금 무서워서 친구랑 가려고요",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039399.1.46",
          "form": "근데 워낙 비용과 시간이 많이 들어서",
          "original_form": "근데 워낙 비용과 시간이 많이 들어서",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039399.1.47",
          "form": "누구랑 갈지 모르겠어요",
          "original_form": "누구랑 갈지 모르겠어요",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039399.1.48",
          "form": "마자요ㅠㅜ",
          "original_form": "마자요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039399.1.49",
          "form": "계모임처럼 돈모아서 가는것도 좋울것겉아요",
          "original_form": "계모임처럼 돈모아서 가는것도 좋울것겉아요",
