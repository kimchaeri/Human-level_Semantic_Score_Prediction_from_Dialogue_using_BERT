      "utterance": [
        {
          "id": "MDRW2100003964.1.1",
          "form": "점심에 뭐 드셨나요?",
          "original_form": "점심에 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003964.1.2",
          "form": "저 방금 먹었는데요",
          "original_form": "저 방금 먹었는데요",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003964.1.3",
          "form": "고등어조림 서대구이요",
          "original_form": "고등어조림 서대구이요",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003964.1.4",
          "form": "우련히 들어 갔는데 맛집이네요",
          "original_form": "우련히 들어 갔는데 맛집이네요",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003964.1.5",
          "form": "뭐 드셨어요 ?",
          "original_form": "뭐 드셨어요 ?",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003964.1.6",
          "form": "저도 방금 다 먹었어요",
          "original_form": "저도 방금 다 먹었어요",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003964.1.7",
          "form": "저는 닭개장이랑 감자볶음이요",
          "original_form": "저는 닭개장이랑 감자볶음이요",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003964.1.8",
          "form": "오 완전 맛나겠어요",
          "original_form": "오 완전 맛나겠어요",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003964.1.9",
          "form": "지금은 디저트로 이디야 레몬에이드 마셔요 ㅎㅎ",
          "original_form": "지금은 디저트로 이디야 레몬에이드 마셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003964.1.10",
          "form": "달달구리도 먹고싶은데 호응을 안해주네요",
          "original_form": "달달구리도 먹고싶은데 호응을 안해주네요",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003964.1.11",
          "form": "레몬에이드요? 와 새콤달콤하겠어요",
          "original_form": "레몬에이드요? 와 새콤달콤하겠어요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003964.1.12",
          "form": "입가심으로 딱이네요",
          "original_form": "입가심으로 딱이네요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003964.1.13",
          "form": "저 새콤이 좋아해요",
          "original_form": "저 새콤이 좋아해요",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003964.1.14",
          "form": "전 믹스커피랑 우유, 얼음으로 라떼 만들어 먹으려구요",
          "original_form": "전 믹스커피랑 우유, 얼음으로 라떼 만들어 먹으려구요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003964.1.15",
          "form": "요새 맛들렸거든요",
          "original_form": "요새 맛들렸거든요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003964.1.16",
          "form": "세상에 최고네요 ㅎㅎ저 라떼 짱좋아해요",
          "original_form": "세상에 최고네요 ㅎㅎ저 라떼 짱좋아해요",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003964.1.17",
          "form": "저도 해먹어 볼래요 ㅎㅎ",
          "original_form": "저도 해먹어 볼래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003964.1.18",
          "form": "엄마한텐 식후땡으로 카누랑 믹스커피 타 주었어요",
          "original_form": "엄마한텐 식후땡으로 카누랑 믹스커피 타 주었어요",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003964.1.19",
          "form": "전 커피 좋아하거든요 자주 마셔서 문제인듯",
          "original_form": "전 커피 좋아하거든요 자주 마셔서 문제인듯",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003964.1.20",
          "form": "전 아이가 바리스타2급이라 정말 잘타줘요 ㅎㅎ",
          "original_form": "전 아이가 바리스타2급이라 정말 잘타줘요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.21",
          "form": "초등인데 따서 맨날 돈받고 해줘요푸ㅜ",
          "original_form": "초등인데 따서 맨날 돈받고 해줘요푸ㅜ",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.22",
          "form": "우와 멋있네요",
          "original_form": "우와 멋있네요",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.23",
          "form": "녀석 치사해요 ㅋㅋ",
          "original_form": "녀석 치사해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.24",
          "form": "초등학생도 있어요?",
          "original_form": "초등학생도 있어요?",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.25",
          "form": "엄마가 좋아 하시겠어요 ㅎㅎ",
          "original_form": "엄마가 좋아 하시겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.26",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.27",
          "form": "용돈벌이네요",
          "original_form": "용돈벌이네요",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.28",
          "form": "귀엽고 웃기겠어요",
          "original_form": "귀엽고 웃기겠어요",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.29",
          "form": "네 ㅋㅋ아주 부자예요",
          "original_form": "네 ㅋㅋ아주 부자예요",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003964.1.30",
          "form": "네 돈독이 올랐네요ㅋㅋ",
          "original_form": "네 돈독이 올랐네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003964.1.31",
          "form": "전 아메리카노는 연하게 먹고",
          "original_form": "전 아메리카노는 연하게 먹고",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003964.1.32",
          "form": "라떼는 달게 먹어요",
          "original_form": "라떼는 달게 먹어요",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003964.1.33",
          "form": "주로 뭐 해주나요?",
          "original_form": "주로 뭐 해주나요?",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003964.1.34",
          "form": "제가 아메리카노 라떼 해달라해요",
          "original_form": "제가 아메리카노 라떼 해달라해요",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003964.1.35",
          "form": "역시 라떼는 달달구리하게 먹어야 제맛이죠",
          "original_form": "역시 라떼는 달달구리하게 먹어야 제맛이죠",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003964.1.36",
          "form": "그렇죠 ㅋㅋ시럽왕창",
          "original_form": "그렇죠 ㅋㅋ시럽왕창",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003964.1.37",
          "form": "전 아메리카노는 진하게 마셔요",
          "original_form": "전 아메리카노는 진하게 마셔요",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003964.1.38",
          "form": "우와 전용(?) 바리스타네요",
          "original_form": "우와 전용(?) 바리스타네요",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003964.1.39",
          "form": "네 ㅎㅎ비싸요",
          "original_form": "네 ㅎㅎ비싸요",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003964.1.40",
          "form": "전 디저트도 또 먹어요 ㅎㅎ",
          "original_form": "전 디저트도 또 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003964.1.41",
          "form": "케이크 아 못끊겠어요",
          "original_form": "케이크 아 못끊겠어요",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100003964.1.42",
          "form": "커피숍 코스예요 ㅎㅎ",
          "original_form": "커피숍 코스예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:36"
        }
      ]
    }
