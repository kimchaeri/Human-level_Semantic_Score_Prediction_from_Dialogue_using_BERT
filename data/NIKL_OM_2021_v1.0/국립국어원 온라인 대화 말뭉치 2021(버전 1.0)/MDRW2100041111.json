      "utterance": [
        {
          "id": "MDRW2100041111.1.1",
          "form": "헉 간장계란밥도맛있고",
          "original_form": "헉 간장계란밥도맛있고",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100041111.1.2",
          "form": "함박스테이크에 곁들이면",
          "original_form": "함박스테이크에 곁들이면",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100041111.1.3",
          "form": "짱맛있죠",
          "original_form": "짱맛있죠",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100041111.1.4",
          "form": "맞아요!  함박스테이크에 곁들일때는 \r\n노른자를 툭 터트려서 먹어줘야죠",
          "original_form": "맞아요!  함박스테이크에 곁들일때는 \r\n노른자를 툭 터트려서 먹어줘야죠",
          "speaker_id": "1",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100041111.1.5",
          "form": "반숙완숙 다좋아하세요?!",
          "original_form": "반숙완숙 다좋아하세요?!",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100041111.1.6",
          "form": "저는 반숙보다는 완숙을 좋아하는데,\r\n너무 완숙인건 싫어해요ㅋㅋㅋ",
          "original_form": "저는 반숙보다는 완숙을 좋아하는데,\r\n너무 완숙인건 싫어해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100041111.1.7",
          "form": "삶은계란도 좋아하세요?!!",
          "original_form": "삶은계란도 좋아하세요?!!",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100041111.1.8",
          "form": "네 삶은계란 좋아해요!\r\n계란 삶아서 계란장 만드는것도 좋아하구요",
          "original_form": "네 삶은계란 좋아해요!\r\n계란 삶아서 계란장 만드는것도 좋아하구요",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100041111.1.9",
          "form": "헉 계란장 정말맛있더라고요",
          "original_form": "헉 계란장 정말맛있더라고요",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100041111.1.10",
          "form": "밥도둑",
          "original_form": "밥도둑",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100041111.1.11",
          "form": "그쵸!! 저 어제 계란장 만들어서 냉장고에 넣어놨어요",
          "original_form": "그쵸!! 저 어제 계란장 만들어서 냉장고에 넣어놨어요",
          "speaker_id": "1",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100041111.1.12",
          "form": "반찬 이것저것 없을때 먹기 좋더라구요",
          "original_form": "반찬 이것저것 없을때 먹기 좋더라구요",
          "speaker_id": "1",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100041111.1.13",
          "form": "헉 세상에 부러워요,,,!",
          "original_form": "헉 세상에 부러워요,,,!",
          "speaker_id": "2",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100041111.1.14",
          "form": "계란장,,",
          "original_form": "계란장,,",
          "speaker_id": "2",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100041111.1.15",
          "form": "저양파장아찌는 할줄아는데,,,",
          "original_form": "저양파장아찌는 할줄아는데,,,",
          "speaker_id": "2",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100041111.1.16",
          "form": "양파장아찌도 맛있죠!! \r\n저는 계란 장 담그면서 양파도 썰어서 함께 넣어놔요ㅋㅋㅋ",
          "original_form": "양파장아찌도 맛있죠!! \r\n저는 계란 장 담그면서 양파도 썰어서 함께 넣어놔요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100041111.1.17",
          "form": "양파장아찌 어떤 식으로 만드세요?",
          "original_form": "양파장아찌 어떤 식으로 만드세요?",
          "speaker_id": "1",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100041111.1.18",
          "form": "식초물설탕 일대일대일?! 넣고 끓여서 양파에 부어요!",
          "original_form": "식초물설탕 일대일대일?! 넣고 끓여서 양파에 부어요!",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041111.1.19",
          "form": "오오 그렇게 하시는군요 \r\n저도 양파만 따로 장아찌 담글까 하고 생각하고 있어요",
          "original_form": "오오 그렇게 하시는군요 \r\n저도 양파만 따로 장아찌 담글까 하고 생각하고 있어요",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100041111.1.20",
          "form": "저는 양파 두세개만 갖고 조금만들어서 오이도 그냥 넣어버려요!",
          "original_form": "저는 양파 두세개만 갖고 조금만들어서 오이도 그냥 넣어버려요!",
          "speaker_id": "2",
          "time": "20210905 17:34"
        }
      ]
    }
