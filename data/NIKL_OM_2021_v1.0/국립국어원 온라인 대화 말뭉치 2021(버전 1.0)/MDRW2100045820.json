      "utterance": [
        {
          "id": "MDRW2100045820.1.1",
          "form": "말을 바꿔야 해요",
          "original_form": "말을 바꿔야 해요",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045820.1.2",
          "form": "어떡해요?",
          "original_form": "어떡해요?",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045820.1.3",
          "form": "올바른 인식이 확산되어야 할 텐데",
          "original_form": "올바른 인식이 확산되어야 할 텐데",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045820.1.4",
          "form": "그쵸 그쵸",
          "original_form": "그쵸 그쵸",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045820.1.5",
          "form": "그런 사람들에 가로막히면",
          "original_form": "그런 사람들에 가로막히면",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.6",
          "form": "정말 큰일",
          "original_form": "정말 큰일",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.7",
          "form": "그래서 요즘은 기후 위기라고 말을 바꾸 더라고요",
          "original_form": "그래서 요즘은 기후 위기라고 말을 바꾸 더라고요",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.8",
          "form": "위기",
          "original_form": "위기",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.9",
          "form": "지구 온난화라고 하면",
          "original_form": "지구 온난화라고 하면",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.11",
          "form": "그냥 따뜻해진다",
          "original_form": "그냥 따뜻해진다",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.12",
          "form": "이런 느낌이니",
          "original_form": "이런 느낌이니",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045820.1.13",
          "form": "아 글쿤요",
          "original_form": "아 글쿤요",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045820.1.14",
          "form": "경각심을 가지도록",
          "original_form": "경각심을 가지도록",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045820.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045820.1.16",
          "form": "아예 기후 위기라고",
          "original_form": "아예 기후 위기라고",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045820.1.17",
          "form": "맞아요 지금 이런 날씨 정상이 아니긴 해요",
          "original_form": "맞아요 지금 이런 날씨 정상이 아니긴 해요",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045820.1.18",
          "form": "여름에도 에어컨들을 틀어대니",
          "original_form": "여름에도 에어컨들을 틀어대니",
          "speaker_id": "2",
