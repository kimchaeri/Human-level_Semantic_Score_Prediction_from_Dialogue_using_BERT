      "utterance": [
        {
          "id": "MDRW2100011250.1.1",
          "form": "응~울name1 잘한다이 ㅋㅋㅋㅋ",
          "original_form": "응~울&name1& 잘한다이 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011250.1.2",
          "form": "일단 차타고 가서 대리불러서 들온다하는데 ..",
          "original_form": "일단 차타고 가서 대리불러서 들온다하는데 ..",
          "speaker_id": "2",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011250.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011250.1.4",
          "form": "못따라가니 걱정이다..이노무 남자들은.",
          "original_form": "못따라가니 걱정이다..이노무 남자들은.",
          "speaker_id": "2",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.5",
          "form": "내놓음 걱정이다ㅠ",
          "original_form": "내놓음 걱정이다ㅠ",
          "speaker_id": "2",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.6",
          "form": "그러니깐 정말",
          "original_form": "그러니깐 정말",
          "speaker_id": "1",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.7",
          "form": "걱정되겠다ㅠㅠ",
          "original_form": "걱정되겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.8",
          "form": "다 회복될때까지 계속 시내까지 가야는데..",
          "original_form": "다 회복될때까지 계속 시내까지 가야는데..",
          "speaker_id": "2",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.9",
          "form": "정말..좀 아푸면 병원거야된다ㅠ",
          "original_form": "정말..좀 아푸면 병원거야된다ㅠ",
          "speaker_id": "2",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.10",
          "form": "내가 친정식구들 걱정하는게 ㅋㅋㅋㅋㅋ 비슷하네",
          "original_form": "내가 친정식구들 걱정하는게 ㅋㅋㅋㅋㅋ 비슷하네",
          "speaker_id": "1",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.11",
          "form": "name1야..난 니맘 백번안다ㅠ",
          "original_form": "&name1&야..난 니맘 백번안다ㅠ",
          "speaker_id": "2",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.12",
          "form": "맞다맞다",
          "original_form": "맞다맞다",
          "speaker_id": "1",
          "time": "20210824 10:00"
        },
        {
          "id": "MDRW2100011250.1.13",
          "form": "조금만 아프면 가야되는데",
          "original_form": "조금만 아프면 가야되는데",
          "speaker_id": "1",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100011250.1.14",
          "form": "남자들은 그게안되니",
          "original_form": "남자들은 그게안되니",
          "speaker_id": "1",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100011250.1.15",
          "form": "난도 첫째 3살때 엄마 만난거니~",
          "original_form": "난도 첫째 3살때 엄마 만난거니~",
          "speaker_id": "2",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100011250.1.16",
          "form": "아빠 혼자있으니 더 걱정이고ㅠ",
          "original_form": "아빠 혼자있으니 더 걱정이고ㅠ",
          "speaker_id": "2",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100011250.1.17",
          "form": "응응 그러니깐",
          "original_form": "응응 그러니깐",
          "speaker_id": "1",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100011250.1.18",
          "form": "맞아. 차라리 여자친구분계시면",
          "original_form": "맞아. 차라리 여자친구분계시면",
          "speaker_id": "1",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100011250.1.19",
          "form": "덜한데",
          "original_form": "덜한데",
          "speaker_id": "1",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100011250.1.20",
          "form": "그러니~걱정은 덜되는데ㅠ",
          "original_form": "그러니~걱정은 덜되는데ㅠ",
          "speaker_id": "2",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.21",
          "form": "응응 지금은 아버지 여자친구분 계시거든",
          "original_form": "응응 지금은 아버지 여자친구분 계시거든",
          "speaker_id": "1",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.22",
          "form": "울 시아버님은 진짜 건강관리잘하고 집안일 혼자 다하는데..",
          "original_form": "울 시아버님은 진짜 건강관리잘하고 집안일 혼자 다하는데..",
          "speaker_id": "2",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.23",
          "form": "그래서 솔직히 신경 덜 쓰인다",
          "original_form": "그래서 솔직히 신경 덜 쓰인다",
          "speaker_id": "1",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.24",
          "form": "그래 맞다~여자가 있아야된다 ㅋㅋㅋ",
          "original_form": "그래 맞다~여자가 있아야된다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.25",
          "form": "글고 건강해야되고~",
          "original_form": "글고 건강해야되고~",
          "speaker_id": "2",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.26",
          "form": "와 시아버님 멋지시다",
          "original_form": "와 시아버님 멋지시다",
          "speaker_id": "1",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.27",
          "form": "혼자 다하시고",
          "original_form": "혼자 다하시고",
          "speaker_id": "1",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.28",
          "form": "울아빤 여기저기 아파서ㅠ",
          "original_form": "울아빤 여기저기 아파서ㅠ",
          "speaker_id": "2",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100011250.1.29",
          "form": "근데 울신랑은 왜 안그렇노??",
          "original_form": "근데 울신랑은 왜 안그렇노??",
          "speaker_id": "2",
          "time": "20210824 10:03"
        },
        {
          "id": "MDRW2100011250.1.30",
          "form": "맞아 맞아",
          "original_form": "맞아 맞아",
          "speaker_id": "1",
          "time": "20210824 10:03"
        },
        {
          "id": "MDRW2100011250.1.31",
          "form": "주워왔나??",
          "original_form": "주워왔나??",
