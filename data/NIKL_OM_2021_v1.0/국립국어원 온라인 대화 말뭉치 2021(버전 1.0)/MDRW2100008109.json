      "utterance": [
        {
          "id": "MDRW2100008109.1.1",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008109.1.2",
          "form": "배달음식좋아하세요?",
          "original_form": "배달음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008109.1.3",
          "form": "넹 배달 피자 어제도 먹었음",
          "original_form": "넹 배달 피자 어제도 먹었음",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.4",
          "form": "무슨피자요",
          "original_form": "무슨피자요",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.5",
          "form": "난 피자헛좋아해",
          "original_form": "난 피자헛좋아해",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.6",
          "form": "이태리하우스에서 시킨 불고기피자요",
          "original_form": "이태리하우스에서 시킨 불고기피자요",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.7",
          "form": "거기 도너츠가 맛있음",
          "original_form": "거기 도너츠가 맛있음",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.8",
          "form": "도너츠맛있지",
          "original_form": "도너츠맛있지",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.9",
          "form": "난 찹쌀도너츠좋아해",
          "original_form": "난 찹쌀도너츠좋아해",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.10",
          "form": "오늘은 와플시켜 먹고싶은데 자제중",
          "original_form": "오늘은 와플시켜 먹고싶은데 자제중",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008109.1.11",
          "form": "찹쌀도너츠도 맛있지",
          "original_form": "찹쌀도너츠도 맛있지",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008109.1.12",
          "form": "와플사줘",
          "original_form": "와플사줘",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008109.1.13",
          "form": "커피도",
          "original_form": "커피도",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008109.1.14",
          "form": "오늘은 니가 좀 사줘도 될 것 같네요",
          "original_form": "오늘은 니가 좀 사줘도 될 것 같네요",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008109.1.15",
          "form": "와플은 사과잼와플이 짱",
          "original_form": "와플은 사과잼와플이 짱",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008109.1.16",
          "form": "사과반 생크림반",
          "original_form": "사과반 생크림반",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008109.1.17",
          "form": "사과쨈와플 인정 생크림은 좀 적어야해",
          "original_form": "사과쨈와플 인정 생크림은 좀 적어야해",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008109.1.18",
          "form": "거기에 아아 한 잔 먹으면 최고지",
          "original_form": "거기에 아아 한 잔 먹으면 최고지",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008109.1.19",
          "form": "학교앞에서파는와플이짱",
          "original_form": "학교앞에서파는와플이짱",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008109.1.20",
          "form": "배달로 시켜먹는 와플은 대부분 눅눅해져서 실망스러움",
          "original_form": "배달로 시켜먹는 와플은 대부분 눅눅해져서 실망스러움",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008109.1.21",
          "form": "배달의 시대인데",
          "original_form": "배달의 시대인데",
          "speaker_id": "2",
          "time": "20210822 13:26"
        }
      ]
