      "utterance": [
        {
          "id": "MDRW2100038303.1.1",
          "form": "그러면 카트라이더 잘 하세요?",
          "original_form": "그러면 카트라이더 잘 하세요?",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038303.1.2",
          "form": "잘하진 않아요ㅋㅋ",
          "original_form": "잘하진 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100038303.1.3",
          "form": "ㅋㅋ 응원과 실력은 다르죠",
          "original_form": "ㅋㅋ 응원과 실력은 다르죠",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038303.1.4",
          "form": "그쵸. 선수들은 넘사벽이고요ㅋㅋㅋ",
          "original_form": "그쵸. 선수들은 넘사벽이고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100038303.1.5",
          "form": "그래서 프로인거 같아요",
          "original_form": "그래서 프로인거 같아요",
          "speaker_id": "1",
          "time": "20210902 09:53"
        },
        {
          "id": "MDRW2100038303.1.6",
          "form": "그렇긴 하죠",
          "original_form": "그렇긴 하죠",
          "speaker_id": "2",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100038303.1.7",
          "form": "하는 게임 있어요?",
          "original_form": "하는 게임 있어요?",
          "speaker_id": "2",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100038303.1.8",
          "form": "저는 롤이랑 모바일겜 몇개 하고 있어요",
          "original_form": "저는 롤이랑 모바일겜 몇개 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038303.1.9",
          "form": "롤 인기많잖아요",
          "original_form": "롤 인기많잖아요",
          "speaker_id": "2",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038303.1.10",
          "form": "많죠 그래서 매칭잡기 좋아요",
          "original_form": "많죠 그래서 매칭잡기 좋아요",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038303.1.11",
          "form": "잘하시나요?",
          "original_form": "잘하시나요?",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038303.1.12",
          "form": "못해요ㅋㅋ",
          "original_form": "못해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038303.1.13",
          "form": "좋아하는 선수랑 괴리감이 있죠",
          "original_form": "좋아하는 선수랑 괴리감이 있죠",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038303.1.14",
          "form": "좋아하는 선수가 누구에요?",
          "original_form": "좋아하는 선수가 누구에요?",
          "speaker_id": "2",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100038303.1.15",
          "form": "미드 쵸비 선수 좋아해요",
          "original_form": "미드 쵸비 선수 좋아해요",
          "speaker_id": "1",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038303.1.16",
          "form": "한화 이스포츠팀 계정에서 봤어요ㅋㅋㅋ",
          "original_form": "한화 이스포츠팀 계정에서 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038303.1.17",
          "form": "만화도 그려서 선수 홍보해주더라구요",
          "original_form": "만화도 그려서 선수 홍보해주더라구요",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038303.1.18",
          "form": "맞아요 만화 엄청 자주 올리더라구요",
          "original_form": "맞아요 만화 엄청 자주 올리더라구요",
          "speaker_id": "1",
          "time": "20210902 10:14"
