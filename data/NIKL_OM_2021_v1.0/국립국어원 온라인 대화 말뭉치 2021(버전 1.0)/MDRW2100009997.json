      "utterance": [
        {
          "id": "MDRW2100009997.1.1",
          "form": "학과 단톡 너무 시끄러워서 그냥 꺼둠",
          "original_form": "학과 단톡 너무 시끄러워서 그냥 꺼둠",
          "speaker_id": "2",
          "time": "20210823 12:57"
        },
        {
          "id": "MDRW2100009997.1.2",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210823 12:57"
        },
        {
          "id": "MDRW2100009997.1.3",
          "form": "아마도??",
          "original_form": "아마도??",
          "speaker_id": "2",
          "time": "20210823 12:57"
        },
        {
          "id": "MDRW2100009997.1.4",
          "form": "동아리하라고강요안함?",
          "original_form": "동아리하라고강요안함?",
          "speaker_id": "1",
          "time": "20210823 12:57"
        },
        {
          "id": "MDRW2100009997.1.5",
          "form": "응 그런 거 없어",
          "original_form": "응 그런 거 없어",
          "speaker_id": "2",
          "time": "20210823 12:57"
        },
        {
          "id": "MDRW2100009997.1.6",
          "form": "학기 중에믄 글쓸 시간도 부족한데",
          "original_form": "학기 중에믄 글쓸 시간도 부족한데",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009997.1.7",
          "form": "하긴너네학과는워낙바쁘니까",
          "original_form": "하긴너네학과는워낙바쁘니까",
          "speaker_id": "1",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009997.1.8",
          "form": "갑자기강요하니까",
          "original_form": "갑자기강요하니까",
          "speaker_id": "1",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009997.1.9",
          "form": "너학생화비",
          "original_form": "너학생화비",
          "speaker_id": "1",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009997.1.10",
          "form": "냈음?",
          "original_form": "냈음?",
          "speaker_id": "1",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009997.1.11",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.12",
          "form": "무슨 학생회비를 냐라 그래서 그냥 안냄",
          "original_form": "무슨 학생회비를 냐라 그래서 그냥 안냄",
          "speaker_id": "2",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.13",
          "form": "그거내라고엄청강요하지않음?",
          "original_form": "그거내라고엄청강요하지않음?",
          "speaker_id": "1",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.14",
          "form": "은은하게 강요하긴 함",
          "original_form": "은은하게 강요하긴 함",
          "speaker_id": "2",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.15",
          "form": "아니 근데 그렇게 혜택이 있는 것 같지도 않은데 굳이 내야 하나",
          "original_form": "아니 근데 그렇게 혜택이 있는 것 같지도 않은데 굳이 내야 하나",
          "speaker_id": "2",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.16",
          "form": "우리학과는",
          "original_form": "우리학과는",
          "speaker_id": "1",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.17",
          "form": "6년치",
          "original_form": "6년치",
          "speaker_id": "1",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.18",
          "form": "다내라고",
          "original_form": "다내라고",
          "speaker_id": "1",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009997.1.19",
          "form": "십만원넘게내라고함",
          "original_form": "십만원넘게내라고함",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.20",
          "form": "뭐야 제정신인가",
          "original_form": "뭐야 제정신인가",
          "speaker_id": "2",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.21",
          "form": "개인별로전화도옴",
          "original_form": "개인별로전화도옴",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.22",
          "form": "뭐야 그거 그냥 돈 뺏기지",
          "original_form": "뭐야 그거 그냥 돈 뺏기지",
          "speaker_id": "2",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.23",
          "form": "학과장? 이런사람이 전화해서",
          "original_form": "학과장? 이런사람이 전화해서",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.24",
          "form": "낼생각없냐고",
          "original_form": "낼생각없냐고",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.25",
          "form": "안내면혜택없다고",
          "original_form": "안내면혜택없다고",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.26",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.27",
          "form": "으으",
          "original_form": "으으",
          "speaker_id": "2",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009997.1.28",
          "form": "뭐야 학생회가 학생 돈 뜯어내라고 있는덴가",
          "original_form": "뭐야 학생회가 학생 돈 뜯어내라고 있는덴가",
          "speaker_id": "2",
          "time": "20210823 13:01"
        },
        {
          "id": "MDRW2100009997.1.29",
          "form": "학생 복지는 등록금에서 알아서 해야 하는 거 아니야??",
          "original_form": "학생 복지는 등록금에서 알아서 해야 하는 거 아니야??",
          "speaker_id": "2",
          "time": "20210823 13:01"
        },
        {
          "id": "MDRW2100009997.1.30",
          "form": "새내기들은",
          "original_form": "새내기들은",
          "speaker_id": "1",
          "time": "20210823 13:01"
        },
        {
          "id": "MDRW2100009997.1.31",
          "form": "잘모르니까",
          "original_form": "잘모르니까",
          "speaker_id": "1",
          "time": "20210823 13:01"
        },
        {
          "id": "MDRW2100009997.1.32",
          "form": "더 전화하고 강요하는듯",
          "original_form": "더 전화하고 강요하는듯",
          "speaker_id": "1",
          "time": "20210823 13:01"
        },
        {
          "id": "MDRW2100009997.1.33",
          "form": "으으 사람들이 참",
          "original_form": "으으 사람들이 참",
          "speaker_id": "2",
          "time": "20210823 13:02"
        },
        {
          "id": "MDRW2100009997.1.34",
          "form": "나는중간에정책이바뀌지않으면",
          "original_form": "나는중간에정책이바뀌지않으면",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100009997.1.35",
          "form": "1년미국대학교다녀야함",
          "original_form": "1년미국대학교다녀야함",
          "speaker_id": "1",
          "time": "20210824 13:33"
        }
      ]
    }
