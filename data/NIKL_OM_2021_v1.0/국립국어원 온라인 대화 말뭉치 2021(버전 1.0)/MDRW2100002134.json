      "utterance": [
        {
          "id": "MDRW2100002134.1.1",
          "form": "스벅에 새로나온 음료 드셔보셨어요?",
          "original_form": "스벅에 새로나온 음료 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002134.1.2",
          "form": "아이스크림 들어간거 말씀하시는거죠?",
          "original_form": "아이스크림 들어간거 말씀하시는거죠?",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002134.1.3",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002134.1.4",
          "form": "이름이 아이스크림 콜드브루였나요?",
          "original_form": "이름이 아이스크림 콜드브루였나요?",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002134.1.5",
          "form": "그거 별 더준다고 할때 먹어보고 반해서 스벅가면 자꾸 그거시켜요",
          "original_form": "그거 별 더준다고 할때 먹어보고 반해서 스벅가면 자꾸 그거시켜요",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002134.1.6",
          "form": "별3개 줄때 몇번 먹었는데 맛있더라구요",
          "original_form": "별3개 줄때 몇번 먹었는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002134.1.7",
          "form": "아이스크림 블렌딩 콜드브루요",
          "original_form": "아이스크림 블렌딩 콜드브루요",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002134.1.8",
          "form": "가격이 비싸서 글치ㅋ",
          "original_form": "가격이 비싸서 글치ㅋ",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002134.1.9",
          "form": "얼음이 안들어가서 그런가 오래둬도 물도 안생기고 부드럽고 맛있어요",
          "original_form": "얼음이 안들어가서 그런가 오래둬도 물도 안생기고 부드럽고 맛있어요",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002134.1.10",
          "form": "부드러운 맛이 좋더라구요",
          "original_form": "부드러운 맛이 좋더라구요",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002134.1.11",
          "form": "가격은 좀 사악하죠 ㅎㅎ",
          "original_form": "가격은 좀 사악하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002134.1.12",
          "form": "그니까요 다들 비싼 더위사냥이라고ㅎ",
          "original_form": "그니까요 다들 비싼 더위사냥이라고ㅎ",
          "speaker_id": "1",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002134.1.13",
          "form": "품절될정도로 맛있으니 올해 스벅실적올리는데 한몫한거같아요",
          "original_form": "품절될정도로 맛있으니 올해 스벅실적올리는데 한몫한거같아요",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002134.1.14",
          "form": "전 더위사냥 보다 훨 맛나던데요",
          "original_form": "전 더위사냥 보다 훨 맛나던데요",
          "speaker_id": "1",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002134.1.15",
          "form": "맞아요 딱 그맛이예요 더위사냥\r\n완전공감되요",
          "original_form": "맞아요 딱 그맛이예요 더위사냥\r\n완전공감되요",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002134.1.16",
          "form": "1일 한정판매라 늦게가면 품절이예요ㅎㅎ",
          "original_form": "1일 한정판매라 늦게가면 품절이예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:58"
        }
