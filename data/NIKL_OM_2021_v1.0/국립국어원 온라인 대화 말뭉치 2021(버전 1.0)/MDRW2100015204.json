      "utterance": [
        {
          "id": "MDRW2100015204.1.1",
          "form": "동호회 활동 해보신거 있나요?",
          "original_form": "동호회 활동 해보신거 있나요?",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015204.1.2",
          "form": "오 동호회 쫌쫌따리 대학교때나 고등학교때 했던거 같아요 name1님은요?",
          "original_form": "오 동호회 쫌쫌따리 대학교때나 고등학교때 했던거 같아요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015204.1.3",
          "form": "저도 전체동아리 이런건 안해보고 그냥 과 안에 있는 연극부 했었네요",
          "original_form": "저도 전체동아리 이런건 안해보고 그냥 과 안에 있는 연극부 했었네요",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015204.1.4",
          "form": "연 극 부!!!!!",
          "original_form": "연 극 부!!!!!",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015204.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015204.1.6",
          "form": "재능있으시다",
          "original_form": "재능있으시다",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015204.1.7",
          "form": "아니에요 그냥 다 가입할수 있어요 ㅋㅋㅋㅋ",
          "original_form": "아니에요 그냥 다 가입할수 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015204.1.8",
          "form": "무대 뛰셨어요??",
          "original_form": "무대 뛰셨어요??",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015204.1.9",
          "form": "ㅋㅋㅋㅋㅋ 발표회 하긴 해요 과 사람들이랑 교수님들 모아서..",
          "original_form": "ㅋㅋㅋㅋㅋ 발표회 하긴 해요 과 사람들이랑 교수님들 모아서..",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015204.1.10",
          "form": "와 어떤 제목이었나요?",
          "original_form": "와 어떤 제목이었나요?",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015204.1.11",
          "form": "기억 안나요 ㅋㅋㅋ 전공이 일본어라 일본어로 하는 거였거든요..",
          "original_form": "기억 안나요 ㅋㅋㅋ 전공이 일본어라 일본어로 하는 거였거든요..",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015204.1.12",
          "form": "사실 연극보다 분장이 흑역사예요",
          "original_form": "사실 연극보다 분장이 흑역사예요",
