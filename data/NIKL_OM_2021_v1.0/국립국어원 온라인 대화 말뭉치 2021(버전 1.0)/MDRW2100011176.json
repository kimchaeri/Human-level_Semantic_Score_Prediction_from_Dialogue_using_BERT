      "utterance": [
        {
          "id": "MDRW2100011176.1.1",
          "form": "운동 하시는거 있으신가요?",
          "original_form": "운동 하시는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 02:28"
        },
        {
          "id": "MDRW2100011176.1.2",
          "form": "저 이제 헬스 시작하려구요!",
          "original_form": "저 이제 헬스 시작하려구요!",
          "speaker_id": "2",
          "time": "20210824 02:29"
        },
        {
          "id": "MDRW2100011176.1.3",
          "form": "헬스장끊으실려구요?",
          "original_form": "헬스장끊으실려구요?",
          "speaker_id": "1",
          "time": "20210824 02:29"
        },
        {
          "id": "MDRW2100011176.1.4",
          "form": "요즘 코로나인데 괜찮나요?",
          "original_form": "요즘 코로나인데 괜찮나요?",
          "speaker_id": "1",
          "time": "20210824 02:30"
        },
        {
          "id": "MDRW2100011176.1.5",
          "form": "아 헬스장은 무서워서",
          "original_form": "아 헬스장은 무서워서",
          "speaker_id": "2",
          "time": "20210824 02:30"
        },
        {
          "id": "MDRW2100011176.1.6",
          "form": "홈트부터 하려구요",
          "original_form": "홈트부터 하려구요",
          "speaker_id": "2",
          "time": "20210824 02:30"
        },
        {
          "id": "MDRW2100011176.1.7",
          "form": "아 홈트면 주로 무슨운동해요?",
          "original_form": "아 홈트면 주로 무슨운동해요?",
          "speaker_id": "1",
          "time": "20210824 02:31"
        },
        {
          "id": "MDRW2100011176.1.8",
          "form": "푸시업이랑 철봉부터 시작하려구요ㅎㅎ",
          "original_form": "푸시업이랑 철봉부터 시작하려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:32"
        },
        {
          "id": "MDRW2100011176.1.9",
          "form": "오 철봉도요?",
          "original_form": "오 철봉도요?",
          "speaker_id": "1",
          "time": "20210824 02:32"
        },
        {
          "id": "MDRW2100011176.1.10",
          "form": "아 남성분이시군요ㅎ",
          "original_form": "아 남성분이시군요ㅎ",
          "speaker_id": "1",
          "time": "20210824 02:32"
        },
        {
          "id": "MDRW2100011176.1.11",
          "form": "아 네네 좋아하는운동 있으세요?",
          "original_form": "아 네네 좋아하는운동 있으세요?",
          "speaker_id": "2",
          "time": "20210824 02:33"
        },
        {
          "id": "MDRW2100011176.1.12",
          "form": "전 음악들음서 하는게 그나마 낫더라구요",
          "original_form": "전 음악들음서 하는게 그나마 낫더라구요",
          "speaker_id": "1",
          "time": "20210824 02:34"
        },
        {
          "id": "MDRW2100011176.1.13",
          "form": "요즘은 안하고 스피닝 줌바해봤어요",
          "original_form": "요즘은 안하고 스피닝 줌바해봤어요",
          "speaker_id": "1",
          "time": "20210824 02:35"
        },
        {
          "id": "MDRW2100011176.1.14",
          "form": "와 그거 엄청 힘들지않나요",
          "original_form": "와 그거 엄청 힘들지않나요",
          "speaker_id": "2",
          "time": "20210824 02:35"
        },
        {
          "id": "MDRW2100011176.1.15",
          "form": "둘다 힘들어요ㅎ",
          "original_form": "둘다 힘들어요ㅎ",
          "speaker_id": "1",
          "time": "20210824 02:35"
        },
        {
          "id": "MDRW2100011176.1.16",
          "form": "특히 스피닝",
          "original_form": "특히 스피닝",
          "speaker_id": "1",
          "time": "20210824 02:35"
        },
        {
          "id": "MDRW2100011176.1.17",
          "form": "체력소모 엄청날듯 한데",
          "original_form": "체력소모 엄청날듯 한데",
          "speaker_id": "2",
          "time": "20210824 02:36"
        },
        {
          "id": "MDRW2100011176.1.18",
          "form": "그래서 했는데 잘못 함 근육파열되기도 한다더라구요",
          "original_form": "그래서 했는데 잘못 함 근육파열되기도 한다더라구요",
          "speaker_id": "1",
          "time": "20210824 02:36"
        },
        {
          "id": "MDRW2100011176.1.19",
          "form": "근육이 녹는다나",
          "original_form": "근육이 녹는다나",
          "speaker_id": "1",
          "time": "20210824 02:37"
        }
      ]
