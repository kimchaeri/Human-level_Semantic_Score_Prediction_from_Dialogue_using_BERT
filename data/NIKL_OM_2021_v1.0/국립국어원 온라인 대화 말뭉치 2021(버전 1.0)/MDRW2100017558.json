      "utterance": [
        {
          "id": "MDRW2100017558.1.1",
          "form": "여름동안 수박쥬스 엄청 먹었는데 이제 여름 끝나가네요",
          "original_form": "여름동안 수박쥬스 엄청 먹었는데 이제 여름 끝나가네요",
          "speaker_id": "2",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100017558.1.2",
          "form": "저는 냉장고가 작아서",
          "original_form": "저는 냉장고가 작아서",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017558.1.3",
          "form": "이번엔 수박 못 먹었어요ㅠ",
          "original_form": "이번엔 수박 못 먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017558.1.4",
          "form": "썰어진거 사려니 너무 비싼거있죠",
          "original_form": "썰어진거 사려니 너무 비싼거있죠",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017558.1.5",
          "form": "수박쥬스 맛있겠다ㅠ",
          "original_form": "수박쥬스 맛있겠다ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017558.1.6",
          "form": "수박 이번에 진짜 비싸더라구요",
          "original_form": "수박 이번에 진짜 비싸더라구요",
          "speaker_id": "2",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017558.1.7",
          "form": "그래서 수박쥬스만 먹었어요..",
          "original_form": "그래서 수박쥬스만 먹었어요..",
          "speaker_id": "2",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017558.1.8",
          "form": "쥬씨에서 드셨어요?",
          "original_form": "쥬씨에서 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017558.1.9",
          "form": "아니요ㅋㅋㅋㅋ사실 이것도 로컬 맛집이라",
          "original_form": "아니요ㅋㅋㅋㅋ사실 이것도 로컬 맛집이라",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017558.1.10",
          "form": "우리 동네는 맛집이 없어서ㅜㅜ",
          "original_form": "우리 동네는 맛집이 없어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017558.1.11",
          "form": "요즘 슬세권 탐방하거든요 슬리퍼 신고 다닐 수 있는 맛집 탐방",
          "original_form": "요즘 슬세권 탐방하거든요 슬리퍼 신고 다닐 수 있는 맛집 탐방",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017558.1.12",
          "form": "앜ㅋㅋㅋㅋ진짜 다행인건 저도 슬리퍼신고 1분거리에",
          "original_form": "앜ㅋㅋㅋㅋ진짜 다행인건 저도 슬리퍼신고 1분거리에",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017558.1.13",
          "form": "곱창전골이랑 칼국수맛집이있어욬ㅋㅋㅋㅋ",
          "original_form": "곱창전골이랑 칼국수맛집이있어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017558.1.14",
          "form": "헐 너무 맛있는 메뉴인데요",
          "original_form": "헐 너무 맛있는 메뉴인데요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017558.1.15",
          "form": "지역에서도 알아주는 진짜 맛집이라",
          "original_form": "지역에서도 알아주는 진짜 맛집이라",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017558.1.16",
          "form": "포장해서 먹어도 맛있어요ㅠ",
          "original_form": "포장해서 먹어도 맛있어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017558.1.17",
          "form": "헐 군침돈다.. 거기두 로컬맛집인가봐요",
          "original_form": "헐 군침돈다.. 거기두 로컬맛집인가봐요",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017558.1.18",
          "form": "주택가 골목에 있는데",
          "original_form": "주택가 골목에 있는데",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017558.1.19",
          "form": "아는사람들만 아는 맛집이랄까...?",
          "original_form": "아는사람들만 아는 맛집이랄까...?",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017558.1.20",
          "form": "그중에 하나가 저!",
          "original_form": "그중에 하나가 저!",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017558.1.21",
          "form": "유명해졌으면 하면서도 유명해지면 섭섭하겠네여 ㅋㅋㅋ",
          "original_form": "유명해졌으면 하면서도 유명해지면 섭섭하겠네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017558.1.22",
          "form": "맛집주민들은 다 알거예요ㅋㅋㅋㅋ",
          "original_form": "맛집주민들은 다 알거예요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017558.1.23",
          "form": "대표적인게 골목식당에 나왔던 연돈!",
          "original_form": "대표적인게 골목식당에 나왔던 연돈!",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017558.1.24",
          "form": "연돈은 이제 갈수없는 ㅋㅋㅋ",
          "original_form": "연돈은 이제 갈수없는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017558.1.25",
          "form": "연돈 드셔보셨어요?!",
          "original_form": "연돈 드셔보셨어요?!",
          "speaker_id": "1",
          "time": "20210826 22:14"
        }
      ]
    }
