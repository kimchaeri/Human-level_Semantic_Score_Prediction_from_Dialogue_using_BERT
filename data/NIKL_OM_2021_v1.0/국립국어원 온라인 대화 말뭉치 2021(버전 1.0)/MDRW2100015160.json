      "utterance": [
        {
          "id": "MDRW2100015160.1.1",
          "form": "안녕하세요 실례지만 혹시 불자이신가요?",
          "original_form": "안녕하세요 실례지만 혹시 불자이신가요?",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015160.1.2",
          "form": "아니요 무교에요 불교 이신가요?",
          "original_form": "아니요 무교에요 불교 이신가요?",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015160.1.3",
          "form": "아 name2이라는 아이디가 뭔가 불교의 느낌이 들어 여쭤봤어요",
          "original_form": "아 &name2&이라는 아이디가 뭔가 불교의 느낌이 들어 여쭤봤어요",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015160.1.4",
          "form": "저는 불교입니다",
          "original_form": "저는 불교입니다",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015160.1.5",
          "form": "앗 불교군요 뒤에껄 늦게 봤네요ㅠ",
          "original_form": "앗 불교군요 뒤에껄 늦게 봤네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015160.1.6",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015160.1.7",
          "form": "가족분들도 불교신가요?",
          "original_form": "가족분들도 불교신가요?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015160.1.8",
          "form": "네 집안이 불교쪽이에요",
          "original_form": "네 집안이 불교쪽이에요",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015160.1.9",
          "form": "막 열심히 절에 다니고 막 불공 드리는것까지는 아닌데",
          "original_form": "막 열심히 절에 다니고 막 불공 드리는것까지는 아닌데",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015160.1.10",
          "form": "마음이 향해있는?",
          "original_form": "마음이 향해있는?",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015160.1.11",
          "form": "아하 근데 종종 산타면 절 있어서 뭔가 은근 친숙한 느낌이에요 산에 있어서 그런가 분위기가 평화로워서 맘이 차분해지는 느낌이라 좋아해요",
          "original_form": "아하 근데 종종 산타면 절 있어서 뭔가 은근 친숙한 느낌이에요 산에 있어서 그런가 분위기가 평화로워서 맘이 차분해지는 느낌이라 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015160.1.12",
          "form": "사찰이 풍수좋은데에만 있어서 진짜 그런듯해요",
          "original_form": "사찰이 풍수좋은데에만 있어서 진짜 그런듯해요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015160.1.13",
          "form": "종소리도 좋고",
          "original_form": "종소리도 좋고",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015160.1.14",
          "form": "독경소리도 좋고",
          "original_form": "독경소리도 좋고",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015160.1.15",
          "form": "풍경도 고즈넉학",
          "original_form": "풍경도 고즈넉학{emoji:‥}",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015160.1.16",
          "form": "하고",
          "original_form": "하고",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015160.1.17",
          "form": "맞아요 요새는 아예 힐링코스에 절체험도 들어가있던데 그만큼 힐링되는 공간인 것 같아요",
          "original_form": "맞아요 요새는 아예 힐링코스에 절체험도 들어가있던데 그만큼 힐링되는 공간인 것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015160.1.18",
          "form": "그쵸 몸과 마음이 모두 힐링되는",
          "original_form": "그쵸 몸과 마음이 모두 힐링되는",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015160.1.19",
          "form": "템플스테이",
          "original_form": "템플스테이",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015160.1.20",
          "form": "해보신적있으신가요?",
          "original_form": "해보신적있으신가요?",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015160.1.21",
          "form": "네 한번 다녀와봤어요",
          "original_form": "네 한번 다녀와봤어요",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015160.1.22",
          "form": "맛난거먹고 다도도 배우고",
          "original_form": "맛난거먹고 다도도 배우고",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015160.1.23",
          "form": "나름 좋은 경험이었어요",
          "original_form": "나름 좋은 경험이었어요",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015160.1.24",
          "form": "오 전 은근 절이 멀리 있어서 한번 가려면 마음 먹고 가야되더라구요ㅠㅠ",
          "original_form": "오 전 은근 절이 멀리 있어서 한번 가려면 마음 먹고 가야되더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015160.1.25",
          "form": "그게 딱 하나 단점인듯해요",
          "original_form": "그게 딱 하나 단점인듯해요",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015160.1.26",
          "form": "접근성이 떨어져 큰 맘먹고 준비해야한다는거",
          "original_form": "접근성이 떨어져 큰 맘먹고 준비해야한다는거",
          "speaker_id": "1",
          "time": "20210825 22:16"
        }
      ]
