      "utterance": [
        {
          "id": "MDRW2100044416.1.1",
          "form": "엄마 내가 챙겨준  영양제 잘 먹고 있어요",
          "original_form": "엄마 내가 챙겨준  영양제 잘 먹고 있어요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044416.1.2",
          "form": "어떤   영양제",
          "original_form": "어떤   영양제",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044416.1.3",
          "form": "오메가 쓰리랑 마그네슘",
          "original_form": "오메가 쓰리랑 마그네슘",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044416.1.4",
          "form": "먹을려고 하는데 자꾸 까먹네",
          "original_form": "먹을려고 하는데 자꾸 까먹네",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044416.1.5",
          "form": "먹고 주니 먹어야 된다니까",
          "original_form": "먹고 주니 먹어야 된다니까",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044416.1.6",
          "form": "유산균도 먹고 있어요",
          "original_form": "유산균도 먹고 있어요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044416.1.7",
          "form": "유산균은 빈속에 먹으라매",
          "original_form": "유산균은 빈속에 먹으라매",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044416.1.8",
          "form": "응 유산균은 빈속에 먹는 게 좋아요",
          "original_form": "응 유산균은 빈속에 먹는 게 좋아요",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044416.1.9",
          "form": "아침에 밥 먹기 전에 먹고 있어",
          "original_form": "아침에 밥 먹기 전에 먹고 있어",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044416.1.10",
          "form": "오메가 쓰리도 먹어요",
          "original_form": "오메가 쓰리도 먹어요",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044416.1.11",
          "form": "그건 언제 먹는 건데",
          "original_form": "그건 언제 먹는 건데",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044416.1.12",
          "form": "그건 아무 때나",
          "original_form": "그건 아무 때나",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044416.1.13",
          "form": "속 안 비어 있을 때 먹으면 돼요",
          "original_form": "속 안 비어 있을 때 먹으면 돼요",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044416.1.14",
          "form": "알았어",
          "original_form": "알았어",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044416.1.15",
          "form": "비타민 씨는 계속 먹고 있어",
          "original_form": "비타민 씨는 계속 먹고 있어",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044416.1.16",
          "form": "잘하고 있네요 비타민 씨는 빈속에 먹으면 안 돼요",
          "original_form": "잘하고 있네요 비타민 씨는 빈속에 먹으면 안 돼요",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044416.1.17",
          "form": "응 그건 알고 있어",
          "original_form": "응 그건 알고 있어",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044416.1.18",
          "form": "먹을 때마다 물도 많이 먹어주구",
          "original_form": "먹을 때마다 물도 많이 먹어주구",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044416.1.19",
          "form": "응 알들이 커서",
          "original_form": "응 알들이 커서",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044416.1.20",
          "form": "물을 많이 먹게 돼",
          "original_form": "물을 많이 먹게 돼",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044416.1.21",
          "form": "그건 좋네",
          "original_form": "그건 좋네",
          "speaker_id": "1",
