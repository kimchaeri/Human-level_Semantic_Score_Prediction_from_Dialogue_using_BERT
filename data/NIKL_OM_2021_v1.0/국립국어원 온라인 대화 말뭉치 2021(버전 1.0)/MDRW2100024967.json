      "utterance": [
        {
          "id": "MDRW2100024967.1.1",
          "form": "버뜨 건성은 겨울에 화장이 굳어서 쩍쩍 갈라진다는 거ㅋㅋㅋㅋ",
          "original_form": "버뜨 건성은 겨울에 화장이 굳어서 쩍쩍 갈라진다는 거ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024967.1.2",
          "form": "수분 부족 지성 증말 할말하않...",
          "original_form": "수분 부족 지성 증말 할말하않...",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024967.1.3",
          "form": "남들 얼굴에 기름 닦아낼때 난 기름을 바르짘ㅋㅋㅋ",
          "original_form": "남들 얼굴에 기름 닦아낼때 난 기름을 바르짘ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024967.1.4",
          "form": "다 각자의 고충이ㅜㅜㅋㅋㅋ",
          "original_form": "다 각자의 고충이ㅜㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024967.1.5",
          "form": "돈만 있음 피부과에 출근도장 찍고싶네ㅋㅋ",
          "original_form": "돈만 있음 피부과에 출근도장 찍고싶네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024967.1.6",
          "form": "돈있음 뭔들 ㅋㅋㅋㅋ",
          "original_form": "돈있음 뭔들 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024967.1.7",
          "form": "돈 대신 부지런으로 때워야 ㅋㅋㅋㅋ",
          "original_form": "돈 대신 부지런으로 때워야 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024967.1.8",
          "form": "부지런도 돈이 있어야ㅋㅋㅋ",
          "original_form": "부지런도 돈이 있어야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024967.1.9",
          "form": "홈케어로 부지런해야지",
          "original_form": "홈케어로 부지런해야지",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024967.1.10",
          "form": "난 것도 못함ㅋㅋㅋㅋ",
          "original_form": "난 것도 못함ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024967.1.11",
          "form": "마스크팩도 자주 하니까 돈이더라ㅋㅋㅋ",
          "original_form": "마스크팩도 자주 하니까 돈이더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024967.1.12",
          "form": "마스크팩은 난 돈보다 부지런함이 더 필요한 너낌 ㅋㅋㅋㅋㅋ",
          "original_form": "마스크팩은 난 돈보다 부지런함이 더 필요한 너낌 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024967.1.13",
          "form": "돈보다 어려운게 부지런함이구나ㅋㅋ",
          "original_form": "돈보다 어려운게 부지런함이구나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024967.1.14",
          "form": "마스크팩 정도의 투자는 가넝하다는 뭐 그런 소리 ㅋㅋㅋㅋ",
          "original_form": "마스크팩 정도의 투자는 가넝하다는 뭐 그런 소리 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100024967.1.15",
          "form": "너 투자자였구나ㅎㅎㅎ",
          "original_form": "너 투자자였구나ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100024967.1.16",
          "form": "일단 집에 팩은 쌓여 있으니 ㅋㅋㅋ",
          "original_form": "일단 집에 팩은 쌓여 있으니 ㅋㅋㅋ",
