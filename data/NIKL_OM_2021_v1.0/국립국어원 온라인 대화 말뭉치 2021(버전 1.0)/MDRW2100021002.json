      "utterance": [
        {
          "id": "MDRW2100021002.1.1",
          "form": "아 소주는 못먹어서 맥주랑 먹어요 ㅋㅋㅋㅋ",
          "original_form": "아 소주는 못먹어서 맥주랑 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021002.1.2",
          "form": "막창이랑 소주 좋아하시나봐요?!",
          "original_form": "막창이랑 소주 좋아하시나봐요?!",
          "speaker_id": "2",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021002.1.3",
          "form": "저두 맥주파에요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저두 맥주파에요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100021002.1.4",
          "form": "소주는 너무 금방 취해서 잘 안 먹어요",
          "original_form": "소주는 너무 금방 취해서 잘 안 먹어요",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100021002.1.5",
          "form": "name2님은 술 잘 드세요??",
          "original_form": "&name2&님은 술 잘 드세요??",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100021002.1.6",
          "form": "ㅋㅋㅋㅋ아니요 잘 못먹어요 ㅠ 맥주 조금 마셔요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아니요 잘 못먹어요 ㅠ 맥주 조금 마셔요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021002.1.7",
          "form": "소주는 알콜맛나서 싫어해욬ㅋㅋㅋ",
          "original_form": "소주는 알콜맛나서 싫어해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021002.1.8",
          "form": "저두요 소주 마시며뉴진짜 화학약품먹는 느낌",
          "original_form": "저두요 소주 마시며뉴진짜 화학약품먹는 느낌",
          "speaker_id": "1",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100021002.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100021002.1.10",
          "form": "맥주랑 섞으면 좀 낫더라구요",
          "original_form": "맥주랑 섞으면 좀 낫더라구요",
          "speaker_id": "1",
          "time": "20210829 14:48"
        },
        {
          "id": "MDRW2100021002.1.11",
          "form": "맥주는 어떤거 좋아하세요??",
          "original_form": "맥주는 어떤거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210829 14:48"
        },
        {
          "id": "MDRW2100021002.1.12",
          "form": "아 저두 소맥은 먹어욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 저두 소맥은 먹어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100021002.1.13",
          "form": "저는 맥주 레몬맥주같은거 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 맥주 레몬맥주같은거 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100021002.1.14",
          "form": "아니면 그냥 생맥!",
          "original_form": "아니면 그냥 생맥!",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100021002.1.15",
          "form": "오 저 깔라만시 소주는 괜찮더라구요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 저 깔라만시 소주는 괜찮더라구요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100021002.1.16",
          "form": "아 그거 저도 좋아해욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 그거 저도 좋아해욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100021002.1.17",
          "form": "저는 최근에 테라 먹어봤는데 괜찮더라구요",
          "original_form": "저는 최근에 테라 먹어봤는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100021002.1.18",
          "form": "크게 알콜향안나구 괜찮더라구욬ㅋㅋㅋ",
          "original_form": "크게 알콜향안나구 괜찮더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100021002.1.19",
          "form": "맞아요 훨씬 낫더라구요ㅋㅋㅋㅋ",
          "original_form": "맞아요 훨씬 낫더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100021002.1.20",
          "form": "맞아요 테라 인기있는이유를 알겟더라구요",
          "original_form": "맞아요 테라 인기있는이유를 알겟더라구요",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100021002.1.21",
          "form": "맞아요 저 최근에야 먹었거든요",
          "original_form": "맞아요 저 최근에야 먹었거든요",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100021002.1.22",
          "form": "제가 맥주 톡쏘는 시큼한 맛을 싫어하는데",
          "original_form": "제가 맥주 톡쏘는 시큼한 맛을 싫어하는데",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100021002.1.23",
          "form": "테라가 시큼한 맛이 없더라구요",
          "original_form": "테라가 시큼한 맛이 없더라구요",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100021002.1.24",
          "form": "아 저두요 그런부분이 좀뱔로에요",
          "original_form": "아 저두요 그런부분이 좀뱔로에요",
