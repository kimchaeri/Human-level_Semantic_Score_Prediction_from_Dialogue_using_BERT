      "utterance": [
        {
          "id": "MDRW2100038121.1.1",
          "form": "좁고 긴 시간을 할애못하는 사람에게 맞는",
          "original_form": "좁고 긴 시간을 할애못하는 사람에게 맞는",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038121.1.2",
          "form": "애완동물은",
          "original_form": "애완동물은",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038121.1.3",
          "form": "다마고치라는 결론을 내렸습니다",
          "original_form": "다마고치라는 결론을 내렸습니다",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038121.1.4",
          "form": "ㅋㅋㅋㅋㅋ앗 적절하네요",
          "original_form": "ㅋㅋㅋㅋㅋ앗 적절하네요",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038121.1.5",
          "form": "저도 다마고치를 사야겠어요!",
          "original_form": "저도 다마고치를 사야겠어요!",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038121.1.6",
          "form": "요즘 디자인도 귀엽던데",
          "original_form": "요즘 디자인도 귀엽던데",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038121.1.7",
          "form": "닌텐도 게임기가 있으시면",
          "original_form": "닌텐도 게임기가 있으시면",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038121.1.8",
          "form": "거기 동물들이 좀 더 똑똑하고 픽셀도 많아요",
          "original_form": "거기 동물들이 좀 더 똑똑하고 픽셀도 많아요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038121.1.9",
          "form": "아님 동물의 숲도 좋겠어요",
          "original_form": "아님 동물의 숲도 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038121.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038121.1.11",
          "form": "동숲 적절하네요",
          "original_form": "동숲 적절하네요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038121.1.12",
          "form": "저는 개인적으로 조건이되면",
          "original_form": "저는 개인적으로 조건이되면",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038121.1.13",
          "form": "햄스터는 꼭 키워보고 싶어요",
          "original_form": "햄스터는 꼭 키워보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038121.1.14",
          "form": "동숲과는 감정적 교류도 할 수 있으니깐요!",
          "original_form": "동숲과는 감정적 교류도 할 수 있으니깐요!",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038121.1.15",
          "form": "아 전 햄스터는 어릴때 키웠어요",
          "original_form": "아 전 햄스터는 어릴때 키웠어요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038121.1.16",
          "form": "근데 name1님은 케이지 3개는 이어놓은 그런 환경을 바라시는거죠?",
          "original_form": "근데 &name1&님은 케이지 3개는 이어놓은 그런 환경을 바라시는거죠?",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038121.1.17",
          "form": "네 집이 컸으면 좋겠어요",
          "original_form": "네 집이 컸으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038121.1.18",
          "form": "리빙박스 큰것 정도?",
          "original_form": "리빙박스 큰것 정도?",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038121.1.19",
          "form": "아 큰거 하나면 쉽게 하실수 있을텐데",
          "original_form": "아 큰거 하나면 쉽게 하실수 있을텐데",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038121.1.20",
          "form": "그래도 자는 곳과 햄스터리빙박스는 문 하나는 좀 막혀있어야해요",
          "original_form": "그래도 자는 곳과 햄스터리빙박스는 문 하나는 좀 막혀있어야해요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038121.1.21",
          "form": "오 햄은 좀 폐쇄된곳을 좋아해서 그렇죠?",
          "original_form": "오 햄은 좀 폐쇄된곳을 좋아해서 그렇죠?",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100038121.1.22",
          "form": "아뇨,, 야행성이라",
          "original_form": "아뇨,, 야행성이라",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038121.1.23",
          "form": "사람이 자려면요",
          "original_form": "사람이 자려면요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038121.1.24",
          "form": "아하ㅋㅋㅋ시끄러워서였군요",
          "original_form": "아하ㅋㅋㅋ시끄러워서였군요",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038121.1.25",
          "form": "넴 밤에 게속 갉갉하고 뛰어요 ㅋㅋ",
          "original_form": "넴 밤에 게속 갉갉하고 뛰어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038121.1.26",
          "form": "ㅋㅋㅋ참고할게요 너무 귀여운 이유네요",
          "original_form": "ㅋㅋㅋ참고할게요 너무 귀여운 이유네요",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038121.1.27",
          "form": "네 ㅋㅋ 인간과 좀 생체시계가 안 맞아요",
          "original_form": "네 ㅋㅋ 인간과 좀 생체시계가 안 맞아요",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038121.1.28",
          "form": "그래서 워너룸 고양이가 안좋은 이유는",
          "original_form": "그래서 워너룸 고양이가 안좋은 이유는",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038121.1.29",
          "form": "사람의 수면을 많이 방해해요",
          "original_form": "사람의 수면을 많이 방해해요",
          "speaker_id": "2",
