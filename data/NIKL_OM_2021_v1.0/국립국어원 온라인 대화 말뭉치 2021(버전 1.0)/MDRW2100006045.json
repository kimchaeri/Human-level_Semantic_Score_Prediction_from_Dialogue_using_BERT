      "utterance": [
        {
          "id": "MDRW2100006045.1.1",
          "form": "유튜브같은건 보시나요?",
          "original_form": "유튜브같은건 보시나요?",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100006045.1.2",
          "form": "잘 안봅니다",
          "original_form": "잘 안봅니다",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100006045.1.3",
          "form": "name1님은 보나요?",
          "original_form": "&name1&님은 보나요?",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100006045.1.4",
          "form": "엇 요즘 저는 티비보다 유튜브를 많이 봐요",
          "original_form": "엇 요즘 저는 티비보다 유튜브를 많이 봐요",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100006045.1.5",
          "form": "주로 경제채널 봅니당",
          "original_form": "주로 경제채널 봅니당",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100006045.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100006045.1.7",
          "form": "전 아직 티비 보는 세대라",
          "original_form": "전 아직 티비 보는 세대라",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100006045.1.8",
          "form": "아 스마트폰으로 유튜브 한 번 봐보셔쇼ㅋㅊ관심 분야로",
          "original_form": "아 스마트폰으로 유튜브 한 번 봐보셔쇼ㅋㅊ관심 분야로",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100006045.1.9",
          "form": "티비도 바둑채널만 봅니다",
          "original_form": "티비도 바둑채널만 봅니다",
          "speaker_id": "2",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100006045.1.10",
          "form": "아... 바둑 좋아하시는군요... 저는 장기ㅋ 근데 실력이 안 늡니다",
          "original_form": "아... 바둑 좋아하시는군요... 저는 장기ㅋ 근데 실력이 안 늡니다",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100006045.1.11",
          "form": "가장 즐기는 오락이기도 합니다",
          "original_form": "가장 즐기는 오락이기도 합니다",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006045.1.12",
          "form": "어렸을 때부터 바둑을 좋아했어요",
          "original_form": "어렸을 때부터 바둑을 좋아했어요",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006045.1.13",
          "form": "바둑 두시는 분들 아이큐가 높다던데요. 영재들의 스포츠",
          "original_form": "바둑 두시는 분들 아이큐가 높다던데요. 영재들의 스포츠",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006045.1.14",
          "form": "기원도 다니시나요?",
          "original_form": "기원도 다니시나요?",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006045.1.15",
          "form": "기원을 다니지 못하고 독학으로 배웠습니다",
          "original_form": "기원을 다니지 못하고 독학으로 배웠습니다",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100006045.1.16",
          "form": "요새 바둑전문 채널이 있어서 많이 도움 됩니다",
          "original_form": "요새 바둑전문 채널이 있어서 많이 도움 됩니다",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100006045.1.17",
          "form": "우와 대국은 어떻게 하세요?",
          "original_form": "우와 대국은 어떻게 하세요?",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100006045.1.18",
          "form": "이번에 그래서 바둑 게임을 깔았습니다",
          "original_form": "이번에 그래서 바둑 게임을 깔았습니다",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100006045.1.19",
          "form": "온라인 대국 하고 있지요",
          "original_form": "온라인 대국 하고 있지요",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100006045.1.20",
          "form": "아하ㅋ 온라인에서 몇단정도 되셔요",
          "original_form": "아하ㅋ 온라인에서 몇단정도 되셔요",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100006045.1.21",
          "form": "최고단수까지 올라갔습니다",
          "original_form": "최고단수까지 올라갔습니다",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100006045.1.22",
          "form": "헉! 9단..그럼 진짜 잘 하시는 거 아닌가요",
          "original_form": "헉! 9단..그럼 진짜 잘 하시는 거 아닌가요",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100006045.1.23",
          "form": "일단 온라인에서 그렇습니다",
          "original_form": "일단 온라인에서 그렇습니다",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006045.1.24",
          "form": "진짜 기원 같은데서 대국해본적은 없네요",
          "original_form": "진짜 기원 같은데서 대국해본적은 없네요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006045.1.25",
          "form": "우왕 진짜 대단하십니당",
          "original_form": "우왕 진짜 대단하십니당",
