      "utterance": [
        {
          "id": "MDRW2100043707.1.1",
          "form": "최근에 영화보신게 있으세요?",
          "original_form": "최근에 영화보신게 있으세요?",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043707.1.2",
          "form": "최근엔 영화관 가진 못했고",
          "original_form": "최근엔 영화관 가진 못했고",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043707.1.3",
          "form": "영화관 가서 본 건 크루엘라",
          "original_form": "영화관 가서 본 건 크루엘라",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043707.1.4",
          "form": "가 최근이네요",
          "original_form": "가 최근이네요",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043707.1.5",
          "form": "오 그것도 재미있었어요",
          "original_form": "오 그것도 재미있었어요",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043707.1.6",
          "form": "정말 최근엔 가신적이없네요 ㅎㅎ",
          "original_form": "정말 최근엔 가신적이없네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043707.1.7",
          "form": "네 5월말에...",
          "original_form": "네 5월말에...",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043707.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043707.1.9",
          "form": "보고싶으신게없으셨나요?",
          "original_form": "보고싶으신게없으셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043707.1.10",
          "form": "아뇨 코로나가 심해서",
          "original_form": "아뇨 코로나가 심해서",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043707.1.11",
          "form": "영화관 가기 망설여지더라고요",
          "original_form": "영화관 가기 망설여지더라고요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043707.1.12",
          "form": "아 ㅠㅠ그건그렇죠 ㅠㅠ",
          "original_form": "아 ㅠㅠ그건그렇죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043707.1.13",
          "form": "코로나가 4단계고 이러니",
          "original_form": "코로나가 4단계고 이러니",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043707.1.14",
          "form": "네 그래서...",
          "original_form": "네 그래서...",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043707.1.15",
          "form": "공연 보러 다니는 것도 좋아하는데 ㅇ",
          "original_form": "공연 보러 다니는 것도 좋아하는데 ㅇ",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043707.1.16",
          "form": "못간지 오래네요ㅜ",
          "original_form": "못간지 오래네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043707.1.17",
          "form": "ㅠㅠㅠㅠ맞아요 요즘 다 안하고 그러니 ㅠ",
          "original_form": "ㅠㅠㅠㅠ맞아요 요즘 다 안하고 그러니 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043707.1.18",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043707.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043707.1.20",
          "form": "랜선 공연으로 만족해요",
          "original_form": "랜선 공연으로 만족해요",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043707.1.21",
          "form": "다행이네요 ㅎㅎ",
          "original_form": "다행이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043707.1.22",
          "form": "어떤 공연을 보셨나요?",
          "original_form": "어떤 공연을 보셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043707.1.23",
          "form": "뮤지컬 좋아해서",
          "original_form": "뮤지컬 좋아해서",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043707.1.24",
          "form": "최근엔 레드북이란 뮤지컬 봤어요",
          "original_form": "최근엔 레드북이란 뮤지컬 봤어요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043707.1.25",
          "form": "레드북 ㅎㅎ",
          "original_form": "레드북 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043707.1.26",
          "form": "어떤 내용인가요?",
          "original_form": "어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043707.1.27",
          "form": "쇼케이스에서 중계도 해주는군요",
          "original_form": "쇼케이스에서 중계도 해주는군요",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043707.1.28",
          "form": "그런쪽으론 아무것도 몰라서 ㅎㅎ",
          "original_form": "그런쪽으론 아무것도 몰라서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:17"
        }
      ]
    }
