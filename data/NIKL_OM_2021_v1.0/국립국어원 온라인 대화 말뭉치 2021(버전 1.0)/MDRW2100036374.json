      "utterance": [
        {
          "id": "MDRW2100036374.1.1",
          "form": "소설이 원작이네 홍천기",
          "original_form": "소설이 원작이네 홍천기",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036374.1.2",
          "form": "아 혹시 김유정 안나옴?",
          "original_form": "아 혹시 김유정 안나옴?",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036374.1.3",
          "form": "신령한 힘을 가진 여화공이름이 홍천기",
          "original_form": "신령한 힘을 가진 여화공이름이 홍천기",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036374.1.4",
          "form": "예고편 잠시 본 듯",
          "original_form": "예고편 잠시 본 듯",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036374.1.5",
          "form": "맞다 김유정 역시 잘 아네",
          "original_form": "맞다 김유정 역시 잘 아네",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036374.1.6",
          "form": "그냥 예고편 언뜻 기억나네. 남자는 키큰앤데 이름 기억 안나네 ㅋㅋ",
          "original_form": "그냥 예고편 언뜻 기억나네. 남자는 키큰앤데 이름 기억 안나네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036374.1.7",
          "form": "안효섭이 남주 서브남주가 공명",
          "original_form": "안효섭이 남주 서브남주가 공명",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036374.1.8",
          "form": "서브가 공명이네",
          "original_form": "서브가 공명이네",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036374.1.9",
          "form": "채시라 여동생 채국희 이분도 조연으로 나오네",
          "original_form": "채시라 여동생 채국희 이분도 조연으로 나오네",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036374.1.10",
          "form": "둘다 김유정 좋아하겠지?",
          "original_form": "둘다 김유정 좋아하겠지?",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036374.1.11",
          "form": "김광규도 나온다 ㅋㅋ",
          "original_form": "김광규도 나온다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036374.1.12",
          "form": "조연도 빵빵하네",
          "original_form": "조연도 빵빵하네",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036374.1.13",
          "form": "화공에 여자는 없잖아 또 남장하는건가",
          "original_form": "화공에 여자는 없잖아 또 남장하는건가",
