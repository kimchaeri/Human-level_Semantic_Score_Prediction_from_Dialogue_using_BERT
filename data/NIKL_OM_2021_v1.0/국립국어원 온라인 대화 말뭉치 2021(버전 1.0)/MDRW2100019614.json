      "utterance": [
        {
          "id": "MDRW2100019614.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019614.1.2",
          "form": "게임은 직접 하는것 보다 겜방보는거 좋아해요",
          "original_form": "게임은 직접 하는것 보다 겜방보는거 좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019614.1.3",
          "form": "아 그러시구나 저는 고전게임 좋아해요",
          "original_form": "아 그러시구나 저는 고전게임 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019614.1.4",
          "form": "서커스 마리오 이렁거 ㅋㅋ",
          "original_form": "서커스 마리오 이렁거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019614.1.5",
          "form": "오 어릴때 많이 하던거네요 ㅋㅋ 재밌거했었죠",
          "original_form": "오 어릴때 많이 하던거네요 ㅋㅋ 재밌거했었죠",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019614.1.6",
          "form": "저는 하면 모바일게임 하는편이네요 캔디크러쉬사가나 꿈의정원 스도쿠 이런거요",
          "original_form": "저는 하면 모바일게임 하는편이네요 캔디크러쉬사가나 꿈의정원 스도쿠 이런거요",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019614.1.7",
          "form": "오오 꿈의정원은 뭔지 모르겠어요 찾아뷰ㅏ야겠어요",
          "original_form": "오오 꿈의정원은 뭔지 모르겠어요 찾아뷰ㅏ야겠어요",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019614.1.8",
          "form": "그냥 간단한 게임이에요 ㅎㅎ 머리비울때 게임해서 간단한게 좋더라구요",
          "original_form": "그냥 간단한 게임이에요 ㅎㅎ 머리비울때 게임해서 간단한게 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019614.1.9",
          "form": "오그런가요 도전",
          "original_form": "오그런가요 도전",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019614.1.10",
          "form": "겜방 유튜버 보시는분 계신가요?",
          "original_form": "겜방 유튜버 보시는분 계신가요?",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019614.1.11",
          "form": "아뇨 겜방 잘 몰라요",
          "original_form": "아뇨 겜방 잘 몰라요",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019614.1.12",
          "form": "저는 직접하는게 좋아용",
          "original_form": "저는 직접하는게 좋아용",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019614.1.13",
          "form": "최근에 제일 재밌게 하신 게임 있나요?",
          "original_form": "최근에 제일 재밌게 하신 게임 있나요?",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019614.1.14",
          "form": "게임은 아닌데 제페토 앱이요",
          "original_form": "게임은 아닌데 제페토 앱이요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019614.1.15",
          "form": "그건 어떤 앱인가요??",
          "original_form": "그건 어떤 앱인가요??",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019614.1.16",
          "form": "메타버스 앱이에요",
          "original_form": "메타버스 앱이에요",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019614.1.17",
          "form": "가상세계 아바타",
          "original_form": "가상세계 아바타",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019614.1.18",
          "form": "지금 검색해 봤는데 신기하네요 저도 한번 해봐야겠어요",
          "original_form": "지금 검색해 봤는데 신기하네요 저도 한번 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019614.1.19",
          "form": "오 정말요",
          "original_form": "오 정말요",
          "speaker_id": "1",
          "time": "20210827 23:22"
        }
