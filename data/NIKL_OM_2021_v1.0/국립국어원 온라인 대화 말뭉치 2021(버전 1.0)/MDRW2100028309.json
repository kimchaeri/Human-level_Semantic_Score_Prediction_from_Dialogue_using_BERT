      "utterance": [
        {
          "id": "MDRW2100028309.1.1",
          "form": "땅끄 어떤 운동",
          "original_form": "땅끄 어떤 운동",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028309.1.2",
          "form": "보세요?",
          "original_form": "보세요?",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028309.1.3",
          "form": "저는 헬스다니면서 운동 중이라서 다른 운동 다하고 유산소 전에 땅끄 복근 하고있어용 ㅎㅎ",
          "original_form": "저는 헬스다니면서 운동 중이라서 다른 운동 다하고 유산소 전에 땅끄 복근 하고있어용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028309.1.4",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028309.1.5",
          "form": "좋아하진 않지만",
          "original_form": "좋아하진 않지만",
          "speaker_id": "1",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028309.1.6",
          "form": "하려고 해요 ㅠㅠ",
          "original_form": "하려고 해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028309.1.7",
          "form": "ㅜㅜ 그쵸 저도 사실 운동하기 싫은데 살려고해요ㅋㅋㅋ",
          "original_form": "ㅜㅜ 그쵸 저도 사실 운동하기 싫은데 살려고해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028309.1.8",
          "form": "당뇨 안걸릴려는 발악..",
          "original_form": "당뇨 안걸릴려는 발악..",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028309.1.9",
          "form": "근육량 유지가 어렵네요",
          "original_form": "근육량 유지가 어렵네요",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028309.1.10",
          "form": "당뇨 관리 중이세요?",
          "original_form": "당뇨 관리 중이세요?",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028309.1.11",
          "form": "그건 아닌데 단걸 좋아해서요 ㅎㅎ 탄수화물도 좋아하고요",
          "original_form": "그건 아닌데 단걸 좋아해서요 ㅎㅎ 탄수화물도 좋아하고요",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028309.1.12",
          "form": "아하 ㅠ 탄수화물 저도요",
          "original_form": "아하 ㅠ 탄수화물 저도요",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028309.1.13",
          "form": "식단조절이 너무어려워욬ㅋㅋㅋ 체지방량 느는거 너무 한순간이네용",
          "original_form": "식단조절이 너무어려워욬ㅋㅋㅋ 체지방량 느는거 너무 한순간이네용",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028309.1.14",
          "form": "ㅠ 진짜요",
          "original_form": "ㅠ 진짜요",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028309.1.15",
          "form": "살 왜이리 쉽게 찌는지..",
          "original_form": "살 왜이리 쉽게 찌는지..",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028309.1.16",
          "form": "그쵸...ㅜ 근육은 잘빠지고 체지방은 잘찌고 문명이 너무 빨리 발전했어요",
          "original_form": "그쵸...ㅜ 근육은 잘빠지고 체지방은 잘찌고 문명이 너무 빨리 발전했어요",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028309.1.17",
          "form": "ㅠ 진짜요 ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠ 진짜요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028309.1.18",
          "form": "운동하기 너무 싫은데요",
          "original_form": "운동하기 너무 싫은데요",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028309.1.19",
          "form": "ㅋㅋ저도요 시간도 너무 부족해여",
          "original_form": "ㅋㅋ저도요 시간도 너무 부족해여",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028309.1.20",
          "form": "평생운동하고 살려면 9 to4 정돈 돼야 할것 같애요",
          "original_form": "평생운동하고 살려면 9 to4 정돈 돼야 할것 같애요",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028309.1.21",
          "form": "ㅜ아 그렇죠 ㅠㅠ 흑",
          "original_form": "ㅜ아 그렇죠 ㅠㅠ 흑",
