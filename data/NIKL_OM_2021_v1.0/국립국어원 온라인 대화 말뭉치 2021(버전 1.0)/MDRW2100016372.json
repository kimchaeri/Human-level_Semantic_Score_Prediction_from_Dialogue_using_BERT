      "utterance": [
        {
          "id": "MDRW2100016372.1.1",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016372.1.2",
          "form": "다양하게 좋아해요ㅋㅋㅋㅋㅋ",
          "original_form": "다양하게 좋아해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016372.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016372.1.4",
          "form": "저도 다양하게 좋아하는 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 다양하게 좋아하는 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016372.1.5",
          "form": "ㅋㅋㅋㅋ가수 누구좋아하세여",
          "original_form": "ㅋㅋㅋㅋ가수 누구좋아하세여",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016372.1.6",
          "form": "저는 샤이니 좋아합니다!",
          "original_form": "저는 샤이니 좋아합니다!",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016372.1.7",
          "form": "앗 샤이니 좋아하시는구나 제 주변에 샤이니 좋아하는 사람 여럿 있어서 저도 샤이니 친숙해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 샤이니 좋아하시는구나 제 주변에 샤이니 좋아하는 사람 여럿 있어서 저도 샤이니 친숙해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016372.1.8",
          "form": "ㅋㅋㅋㅋㅋ샤이니 수록곡 맛집ㅠㅠ 노래도 많이 들어보셧어요?",
          "original_form": "ㅋㅋㅋㅋㅋ샤이니 수록곡 맛집ㅠㅠ 노래도 많이 들어보셧어요?",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016372.1.9",
          "form": "잘 아는건 아니지만 ㅋㅋㅋㅋ 오드아이?",
          "original_form": "잘 아는건 아니지만 ㅋㅋㅋㅋ 오드아이?",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016372.1.10",
          "form": "아아 네 좋아요ㅋㅋㅋㅋ",
          "original_form": "아아 네 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016372.1.11",
          "form": "드라마는 많이 보시나여",
          "original_form": "드라마는 많이 보시나여",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016372.1.12",
          "form": "슬의 보고 있어요 ㅋㅋㅋ",
          "original_form": "슬의 보고 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016372.1.13",
          "form": "ㅋㅋㅋㅋ저두 슬의봐요 펜트하우스두 봅니다",
          "original_form": "ㅋㅋㅋㅋ저두 슬의봐요 펜트하우스두 봅니다",
          "speaker_id": "1",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016372.1.14",
          "form": "펜트하우스 아직 재밌나요?ㅋㅋㅋ",
          "original_form": "펜트하우스 아직 재밌나요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016372.1.15",
          "form": "이번시즌은 약간 노잼이긴한데 그래두 볼만하더라구요ㅋㅋㅋㅋ",
          "original_form": "이번시즌은 약간 노잼이긴한데 그래두 볼만하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016372.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 보다가 놓아버렸어요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 보다가 놓아버렸어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016372.1.17",
          "form": "ㅋㅋㅋㅋㅋ너무 막장이라서요??ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ너무 막장이라서요??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016372.1.18",
          "form": "막장인건 괜찮은데 잔인한장면이 너무 많이나와서요 ㅋㅋㅋㅋㅋ",
          "original_form": "막장인건 괜찮은데 잔인한장면이 너무 많이나와서요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100016372.1.19",
          "form": "아아맞아요 ㅋㅋㅋㅋㅋ좀 그래요",
          "original_form": "아아맞아요 ㅋㅋㅋㅋㅋ좀 그래요",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100016372.1.20",
          "form": "잔인한거 나오는거 안보세요??",
          "original_form": "잔인한거 나오는거 안보세요??",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016372.1.21",
          "form": "볼때는 괜찮은데 보고나면 꿈에 나와요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "볼때는 괜찮은데 보고나면 꿈에 나와요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016372.1.22",
          "form": "아아 그러면 괴롭겟네요ㄷㄷㄷ",
          "original_form": "아아 그러면 괴롭겟네요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016372.1.23",
          "form": "악몽의 단골소재가 되더라구요 ㅋㅋㅋ그래서 잘 안봐요",
          "original_form": "악몽의 단골소재가 되더라구요 ㅋㅋㅋ그래서 잘 안봐요",
          "speaker_id": "2",
          "time": "20210826 13:32"
        }
      ]
