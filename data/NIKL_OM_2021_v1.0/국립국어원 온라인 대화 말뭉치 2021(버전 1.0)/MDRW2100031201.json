      "utterance": [
        {
          "id": "MDRW2100031201.1.1",
          "form": "베트남 여행은 어땠어요",
          "original_form": "베트남 여행은 어땠어요",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031201.1.2",
          "form": "좋은 경험이었어요",
          "original_form": "좋은 경험이었어요",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031201.1.3",
          "form": "베트남에서 어디 가셨어요",
          "original_form": "베트남에서 어디 가셨어요",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031201.1.4",
          "form": "호지민이랑 하노이를 갔어요",
          "original_form": "호지민이랑 하노이를 갔어요",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031201.1.5",
          "form": "거기는대 도시인가요",
          "original_form": "거기는대 도시인가요",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031201.1.6",
          "form": "내 오토바이랑 차가 많아요",
          "original_form": "내 오토바이랑 차가 많아요",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031201.1.7",
          "form": "하노이가 수도인가요",
          "original_form": "하노이가 수도인가요",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031201.1.8",
          "form": "맞아요 하노이 있다",
          "original_form": "맞아요 하노이 있다",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031201.1.9",
          "form": "하노이 맞아요",
          "original_form": "하노이 맞아요",
          "speaker_id": "2",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100031201.1.10",
          "form": "하노이는 어땟나요",
          "original_form": "하노이는 어땟나요",
          "speaker_id": "1",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100031201.1.11",
          "form": "11월에 가서 쌀쌀했어요",
          "original_form": "11월에 가서 쌀쌀했어요",
          "speaker_id": "2",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100031201.1.12",
          "form": "하노이는 겨울이 있나요",
          "original_form": "하노이는 겨울이 있나요",
          "speaker_id": "1",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100031201.1.13",
          "form": "4계절이 있어요",
          "original_form": "4계절이 있어요",
          "speaker_id": "2",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100031201.1.14",
          "form": "단 건기와 우기로 나뉘어요",
          "original_form": "단 건기와 우기로 나뉘어요",
          "speaker_id": "2",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100031201.1.15",
          "form": "한국이랑 비슷하군요",
          "original_form": "한국이랑 비슷하군요",
          "speaker_id": "1",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100031201.1.16",
          "form": "지금은 우기이지요?",
          "original_form": "지금은 우기이지요?",
          "speaker_id": "1",
          "time": "20210904 18:55"
        },
        {
          "id": "MDRW2100031201.1.17",
          "form": "네 매일비가내려요",
          "original_form": "네 매일비가내려요",
          "speaker_id": "2",
          "time": "20210904 18:55"
        },
        {
          "id": "MDRW2100031201.1.18",
          "form": "여행 가고 싶은 나라가 어디예요",
          "original_form": "여행 가고 싶은 나라가 어디예요",
