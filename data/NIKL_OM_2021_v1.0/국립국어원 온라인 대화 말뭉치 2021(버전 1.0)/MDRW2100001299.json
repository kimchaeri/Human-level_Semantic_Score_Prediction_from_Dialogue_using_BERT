      "utterance": [
        {
          "id": "MDRW2100001299.1.1",
          "form": "도대체 어디갔니",
          "original_form": "도대체 어디갔니",
          "speaker_id": "1",
          "time": "20210517 16:02"
        },
        {
          "id": "MDRW2100001299.1.2",
          "form": "놀이터니?",
          "original_form": "놀이터니?",
          "speaker_id": "1",
          "time": "20210517 16:02"
        },
        {
          "id": "MDRW2100001299.1.3",
          "form": "이제 들어가는 길이지",
          "original_form": "이제 들어가는 길이지",
          "speaker_id": "2",
          "time": "20210517 16:19"
        },
        {
          "id": "MDRW2100001299.1.4",
          "form": "이제 집에서도 이걸로 말해야 하나?",
          "original_form": "이제 집에서도 이걸로 말해야 하나?",
          "speaker_id": "2",
          "time": "20210517 16:20"
        },
        {
          "id": "MDRW2100001299.1.5",
          "form": "알아서 해 ㅎㅎ",
          "original_form": "알아서 해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210517 16:20"
        },
        {
          "id": "MDRW2100001299.1.6",
          "form": "name3이 안젖었나",
          "original_form": "&name3&이 안젖었나",
          "speaker_id": "1",
          "time": "20210517 16:20"
        },
        {
          "id": "MDRW2100001299.1.7",
          "form": "많이 젖었지",
          "original_form": "많이 젖었지",
          "speaker_id": "2",
          "time": "20210517 16:20"
        },
        {
          "id": "MDRW2100001299.1.8",
          "form": "우리 집 거실",
          "original_form": "우리 집 거실",
          "speaker_id": "1",
          "time": "20210517 16:22"
        },
        {
          "id": "MDRW2100001299.1.9",
          "form": "지금 북카페 같음",
          "original_form": "지금 북카페 같음",
          "speaker_id": "1",
          "time": "20210517 16:22"
        },
        {
          "id": "MDRW2100001299.1.10",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "1",
          "time": "20210517 16:22"
        },
        {
          "id": "MDRW2100001299.1.11",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "2",
          "time": "20210517 16:22"
        },
        {
          "id": "MDRW2100001299.1.12",
          "form": "이렇게  며치 써보자",
          "original_form": "이렇게  며치 써보자",
          "speaker_id": "2",
          "time": "20210517 16:22"
        },
        {
          "id": "MDRW2100001299.1.13",
          "form": "쟈는 왜이래 우는공...",
          "original_form": "쟈는 왜이래 우는공...",
          "speaker_id": "1",
          "time": "20210517 16:23"
        },
        {
          "id": "MDRW2100001299.1.14",
          "form": "젖도 먹었는디 ㅋㅋ",
          "original_form": "젖도 먹었는디 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210517 16:23"
        },
        {
          "id": "MDRW2100001299.1.15",
          "form": "이제 나갈라고.",
          "original_form": "이제 나갈라고.",
          "speaker_id": "2",
          "time": "20210517 16:44"
        },
        {
          "id": "MDRW2100001299.1.16",
          "form": "공부하다가  좀 늦었군. 지금 출발함",
          "original_form": "공부하다가  좀 늦었군. 지금 출발함",
          "speaker_id": "2",
          "time": "20210518 21:11"
        },
        {
          "id": "MDRW2100001299.1.17",
          "form": "천천히 오삼",
          "original_form": "천천히 오삼",
          "speaker_id": "1",
          "time": "20210518 21:24"
        },
        {
          "id": "MDRW2100001299.1.18",
          "form": "name3이 낮잠 자나? 애들 잘 놀고 있나?",
          "original_form": "&name3&이 낮잠 자나? 애들 잘 놀고 있나?",
