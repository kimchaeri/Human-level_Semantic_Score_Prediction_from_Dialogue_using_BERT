      "utterance": [
        {
          "id": "MDRW2100040784.1.1",
          "form": "날이 많이 선선하네요",
          "original_form": "날이 많이 선선하네요",
          "speaker_id": "1",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100040784.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040784.1.3",
          "form": "완연 가을날씨네요~!",
          "original_form": "완연 가을날씨네요~!",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040784.1.4",
          "form": "네네 가을이 성큼",
          "original_form": "네네 가을이 성큼",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100040784.1.5",
          "form": "요며칠 하늘도 파랗고 날씨가 맑더라고요",
          "original_form": "요며칠 하늘도 파랗고 날씨가 맑더라고요",
          "speaker_id": "1",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100040784.1.6",
          "form": "이 좋은날 오래가면좋을텐데ㅜㅜ",
          "original_form": "이 좋은날 오래가면좋을텐데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040784.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100040784.1.8",
          "form": "한동안 비가 계속 와서...",
          "original_form": "한동안 비가 계속 와서...",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100040784.1.9",
          "form": "그쳐ㅠ 비가오면 축 쳐지더라구여ㅠ",
          "original_form": "그쳐ㅠ 비가오면 축 쳐지더라구여ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040784.1.10",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040784.1.11",
          "form": "비 오면 축축하고 기분도 늘어져요",
          "original_form": "비 오면 축축하고 기분도 늘어져요",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040784.1.12",
          "form": "맞아여ㅠㅠ 한번씩시원하게 오는겅 좋지만 계속오는건 별루에요ㅠㅠ",
          "original_form": "맞아여ㅠㅠ 한번씩시원하게 오는겅 좋지만 계속오는건 별루에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100040784.1.13",
          "form": "네 가을장마라니",
          "original_form": "네 가을장마라니",
          "speaker_id": "1",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100040784.1.14",
          "form": "전 요새 환절기에 비염이 심해서",
          "original_form": "전 요새 환절기에 비염이 심해서",
          "speaker_id": "2",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100040784.1.15",
          "form": "힘드네요ㅠㅠ아침저녁찬바람에 ㅠ",
          "original_form": "힘드네요ㅠㅠ아침저녁찬바람에 ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100040784.1.16",
          "form": "아 환절기엔 비염 심해지죠ㅜㅜ",
          "original_form": "아 환절기엔 비염 심해지죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 07:25"
        },
        {
          "id": "MDRW2100040784.1.17",
          "form": "저도 환절기엔 알러지 기운이 생기더라고요",
          "original_form": "저도 환절기엔 알러지 기운이 생기더라고요",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100040784.1.18",
          "form": "눈도 피부도 간지럽고ㅠ",
          "original_form": "눈도 피부도 간지럽고ㅠ",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100040784.1.19",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100040784.1.20",
          "form": "좁살트러블도 올라오고ㅠㅠ",
          "original_form": "좁살트러블도 올라오고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100040784.1.21",
          "form": "네네 환절기엔 그렇죠ㅜ",
          "original_form": "네네 환절기엔 그렇죠ㅜ",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100040784.1.22",
          "form": "오늘은 온종일 비가 내리네요",
          "original_form": "오늘은 온종일 비가 내리네요",
          "speaker_id": "1",
          "time": "20210907 17:22"
        }
      ]
