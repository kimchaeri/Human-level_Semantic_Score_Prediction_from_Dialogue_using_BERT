      "utterance": [
        {
          "id": "MDRW2100036845.1.1",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:09"
        },
        {
          "id": "MDRW2100036845.1.2",
          "form": "저 학교에서 일해서 급식 먹고있는데",
          "original_form": "저 학교에서 일해서 급식 먹고있는데",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036845.1.3",
          "form": "오늘 깐풍기 나왔어요 ㅋㅋㅋ",
          "original_form": "오늘 깐풍기 나왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036845.1.4",
          "form": "깐풍기 좋죠 화요일인데도 급식이 잘나오는군요",
          "original_form": "깐풍기 좋죠 화요일인데도 급식이 잘나오는군요",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036845.1.5",
          "form": "ㅋㅋㅋㅋㅋ name2님도 수요일이 맛잇는 날이엇나요",
          "original_form": "ㅋㅋㅋㅋㅋ &name2&님도 수요일이 맛잇는 날이엇나요",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100036845.1.6",
          "form": "네ㅋㅋ 초중고 전부 수다날이었어요",
          "original_form": "네ㅋㅋ 초중고 전부 수다날이었어요",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100036845.1.7",
          "form": "수요일은 다 먹는 날ㅋㅋㅋㅋ",
          "original_form": "수요일은 다 먹는 날ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100036845.1.8",
          "form": "오 맞아요 ㅋㅋㅋㅋ 저도 그래서 내일 급식 기대중이에요",
          "original_form": "오 맞아요 ㅋㅋㅋㅋ 저도 그래서 내일 급식 기대중이에요",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100036845.1.9",
          "form": "그래서 오늘 급식은 어땠나요!",
          "original_form": "그래서 오늘 급식은 어땠나요!",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100036845.1.10",
          "form": "오늘 수요일이라 짜장면이랑",
          "original_form": "오늘 수요일이라 짜장면이랑",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100036845.1.11",
          "form": "탕수육 나왔어요!!!",
          "original_form": "탕수육 나왔어요!!!",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100036845.1.12",
          "form": "학교에서 면 나오면 많이 불어있지 않나요?",
          "original_form": "학교에서 면 나오면 많이 불어있지 않나요?",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100036845.1.13",
          "form": "아 저는 일등으로먹어서 불진 않앗는데",
          "original_form": "아 저는 일등으로먹어서 불진 않앗는데",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100036845.1.14",
          "form": "늦게가면 불겠네요 ㅜㅜ",
          "original_form": "늦게가면 불겠네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100036845.1.15",
          "form": "학생 신분이었던지라 매번 분것만 먹었던 기억이 있어요ㅠ",
          "original_form": "학생 신분이었던지라 매번 분것만 먹었던 기억이 있어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100036845.1.16",
          "form": "으 그럼 면요리나오는날이 싫으셨겠어요",
          "original_form": "으 그럼 면요리나오는날이 싫으셨겠어요",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100036845.1.17",
          "form": "그런 날에는 따로 밥을 부탁드리기도 했어요",
          "original_form": "그런 날에는 따로 밥을 부탁드리기도 했어요",
