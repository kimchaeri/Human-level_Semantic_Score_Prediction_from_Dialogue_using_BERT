      "utterance": [
        {
          "id": "MDRW2100000562.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210819 16:51"
        },
        {
          "id": "MDRW2100000562.1.2",
          "form": "네! 강아지 한마리 있어요ㅎㅎ",
          "original_form": "네! 강아지 한마리 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000562.1.3",
          "form": "저희집 막내예요ㅠㅜ",
          "original_form": "저희집 막내예요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000562.1.4",
          "form": "끄앙 넘귀엽겠어요 전 고양이키우는 집사입니다",
          "original_form": "끄앙 넘귀엽겠어요 전 고양이키우는 집사입니다",
          "speaker_id": "1",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000562.1.5",
          "form": "고양이키우니까 강쥐키우시는분들 궁금했는데 잘됐네여",
          "original_form": "고양이키우니까 강쥐키우시는분들 궁금했는데 잘됐네여",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000562.1.6",
          "form": "강쥐들은 산책 매일매일나가야되져?",
          "original_form": "강쥐들은 산책 매일매일나가야되져?",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000562.1.7",
          "form": "네네ㅋㅋㅋ근데 그것도 강아지들 성격마다 다른 것 같아요ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ근데 그것도 강아지들 성격마다 다른 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000562.1.8",
          "form": "저희집 애는 더울때 나가는 거 싫어하고 누워있는 걸 좋아해요ㅋㅋㅋ",
          "original_form": "저희집 애는 더울때 나가는 거 싫어하고 누워있는 걸 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000562.1.9",
          "form": "강아지들은 유아차같은거 태우기도하던데",
          "original_form": "강아지들은 유아차같은거 태우기도하던데",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000562.1.10",
          "form": "가끔나가서보명 너무귀여워서 실례지만 강쥐들뻔히쳐다봅니다ㅜ",
          "original_form": "가끔나가서보명 너무귀여워서 실례지만 강쥐들뻔히쳐다봅니다ㅜ",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000562.1.11",
          "form": "맞아요ㅋㅋㅋㅋㅋ저희집에도 있어요ㅋㅋㅋㅋㅋ근데 막 나가고 싶어해서 별로 써본적은 없어요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ저희집에도 있어요ㅋㅋㅋㅋㅋ근데 막 나가고 싶어해서 별로 써본적은 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000562.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사놓고 안쓰게되는거 몇몇있져",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사놓고 안쓰게되는거 몇몇있져",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000562.1.13",
          "form": "저도 캣타워같은거 맘먹고 사놨는데 안쓰고 장롱위에만 올라가요ㅋㅋㅜ..",
          "original_form": "저도 캣타워같은거 맘먹고 사놨는데 안쓰고 장롱위에만 올라가요ㅋㅋㅜ..",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000562.1.14",
          "form": "진짜ㅋㅋ물어보고 살 수 있는 게 아니라서ㅋㅋ사놓고 못쓰는 것도 많고ㅋㅋㅋㅋ걍 쓰던말던 산 거는 또 좋아하고ㅋㅋㅋ아주 자기들 마음대로예요ㅋㅋ",
          "original_form": "진짜ㅋㅋ물어보고 살 수 있는 게 아니라서ㅋㅋ사놓고 못쓰는 것도 많고ㅋㅋㅋㅋ걍 쓰던말던 산 거는 또 좋아하고ㅋㅋㅋ아주 자기들 마음대로예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000562.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 아주 맘대로 상전이따로없습니다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 아주 맘대로 상전이따로없습니다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000562.1.16",
          "form": "그래도 어쩌겠어요ㅋㅋㅋ넘 귀여워서 용서 가능한ㅠㅜㅋㅋㅋㅋ",
          "original_form": "그래도 어쩌겠어요ㅋㅋㅋ넘 귀여워서 용서 가능한ㅠㅜㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000562.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 얘땜에 사는 정도에여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 얘땜에 사는 정도에여",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000562.1.18",
          "form": "진짜 보고있는데도 보고싶어져요ㅠㅜㅋㅋㅋ",
          "original_form": "진짜 보고있는데도 보고싶어져요ㅠㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000562.1.19",
          "form": "ㅋㅋㅋㅋㅋ맞아요 괜히 있다가 쭈왑하고와요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 괜히 있다가 쭈왑하고와요",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000562.1.20",
          "form": "저만 그런게 아닌가보네요ㅋㅋㅋㅋ",
          "original_form": "저만 그런게 아닌가보네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:25"
        }
      ]
