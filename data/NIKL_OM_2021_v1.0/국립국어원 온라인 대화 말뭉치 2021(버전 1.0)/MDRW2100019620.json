      "utterance": [
        {
          "id": "MDRW2100019620.1.1",
          "form": "오 무슨 만두 먹으셨는데요?",
          "original_form": "오 무슨 만두 먹으셨는데요?",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019620.1.2",
          "form": "풀무원 얇은피만두요!!",
          "original_form": "풀무원 얇은피만두요!!",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019620.1.3",
          "form": "냉동만두류 중에서 가장 맛있었어요",
          "original_form": "냉동만두류 중에서 가장 맛있었어요",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019620.1.4",
          "form": "진짜요? 냉동만두는 어떤거 드셔보셨어요?",
          "original_form": "진짜요? 냉동만두는 어떤거 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019620.1.5",
          "form": "비비고가 젤유명해서 비비고랑 한만두?? 먹어봤었어요 ㅋㅋㅋㅋ",
          "original_form": "비비고가 젤유명해서 비비고랑 한만두?? 먹어봤었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:26"
        },
        {
          "id": "MDRW2100019620.1.6",
          "form": "오 ㅎㅎ 비비고 중에 한섬만두요?",
          "original_form": "오 ㅎㅎ 비비고 중에 한섬만두요?",
          "speaker_id": "2",
          "time": "20210828 12:26"
        },
        {
          "id": "MDRW2100019620.1.7",
          "form": "아니면 한성기업 만두?",
          "original_form": "아니면 한성기업 만두?",
          "speaker_id": "2",
          "time": "20210828 12:26"
        },
        {
          "id": "MDRW2100019620.1.8",
          "form": "아 아니요 한만두라는 브랜드가 따로있어요 ㅋㅋㅋ",
          "original_form": "아 아니요 한만두라는 브랜드가 따로있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100019620.1.9",
          "form": "아 진짜요? 처음들어보네용 ㅎㅎ",
          "original_form": "아 진짜요? 처음들어보네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100019620.1.10",
          "form": "개성왕만두는 아세요? 저 그거 좋아해요 감자만두",
          "original_form": "개성왕만두는 아세요? 저 그거 좋아해요 감자만두",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100019620.1.11",
          "form": "오오 들어봤어요!! 다음에 먹어봐야겠네요",
          "original_form": "오오 들어봤어요!! 다음에 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100019620.1.12",
          "form": "개성왕만두 감자만두 ㅋㅋ",
          "original_form": "개성왕만두 감자만두 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100019620.1.13",
          "form": "집에 찜기 있어요?",
          "original_form": "집에 찜기 있어요?",
          "speaker_id": "2",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100019620.1.14",
          "form": "네 간편찜기있어요!!",
          "original_form": "네 간편찜기있어요!!",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100019620.1.15",
          "form": "꼭 쪄서 드세요",
          "original_form": "꼭 쪄서 드세요",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100019620.1.16",
          "form": "구워먹거나 에어프라이기 돌려먹으면 맛이 좀 덜해요",
          "original_form": "구워먹거나 에어프라이기 돌려먹으면 맛이 좀 덜해요",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100019620.1.17",
          "form": "얘는 만두피가 쫄깃한게 매력이라서 ㅎㅎ",
          "original_form": "얘는 만두피가 쫄깃한게 매력이라서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100019620.1.18",
          "form": "헐 그렇구나 하긴 맞아요 감자만두류는 그 쫄깃함이 생명이죠 ㅋㅋ 꼭 쪄서 먹어볼게요",
          "original_form": "헐 그렇구나 하긴 맞아요 감자만두류는 그 쫄깃함이 생명이죠 ㅋㅋ 꼭 쪄서 먹어볼게요",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100019620.1.19",
          "form": "네 ㅎㅎ 거긴 포장이 지퍼백인 것도 맘에 들어요",
          "original_form": "네 ㅎㅎ 거긴 포장이 지퍼백인 것도 맘에 들어요",
