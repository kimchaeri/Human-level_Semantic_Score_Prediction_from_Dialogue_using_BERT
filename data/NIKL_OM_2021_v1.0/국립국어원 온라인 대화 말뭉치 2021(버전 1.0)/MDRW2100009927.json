      "utterance": [
        {
          "id": "MDRW2100009927.1.1",
          "form": "언니는 고양이 키우고 싶음?",
          "original_form": "언니는 고양이 키우고 싶음?",
          "speaker_id": "2",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009927.1.2",
          "form": "키운다 안키운다 딱 골라야한다면?",
          "original_form": "키운다 안키운다 딱 골라야한다면?",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009927.1.3",
          "form": "고양이 좋아 하지만 키울자신이 엄서",
          "original_form": "고양이 좋아 하지만 키울자신이 엄서",
          "speaker_id": "1",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009927.1.4",
          "form": "나도 키울자신이 없음",
          "original_form": "나도 키울자신이 없음",
          "speaker_id": "2",
          "time": "20210823 12:00"
        },
        {
          "id": "MDRW2100009927.1.5",
          "form": "룸메가 고양이 키웠었는데 그때 간접키우는 생활 했었음",
          "original_form": "룸메가 고양이 키웠었는데 그때 간접키우는 생활 했었음",
          "speaker_id": "1",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100009927.1.6",
          "form": "행복했다",
          "original_form": "행복했다",
          "speaker_id": "1",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100009927.1.7",
          "form": "ㅋㅋ 딱 그정도가 좋은거일지도",
          "original_form": "ㅋㅋ 딱 그정도가 좋은거일지도",
          "speaker_id": "2",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009927.1.8",
          "form": "펫시터같은거 해보고싶기도함",
          "original_form": "펫시터같은거 해보고싶기도함",
          "speaker_id": "2",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009927.1.9",
          "form": "넘 행복하다 펫시터",
          "original_form": "넘 행복하다 펫시터",
          "speaker_id": "1",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009927.1.10",
          "form": "당근에 개산책알바 안올라옴?",
          "original_form": "당근에 개산책알바 안올라옴?",
          "speaker_id": "1",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009927.1.11",
          "form": "그런것도 당근올라옴?",
          "original_form": "그런것도 당근올라옴?",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009927.1.12",
          "form": "신기",
          "original_form": "신기",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009927.1.13",
          "form": "역쉬 서울",
          "original_form": "역쉬 서울",
          "speaker_id": "2",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009927.1.14",
          "form": "동네마다 특색이 다르제",
          "original_form": "동네마다 특색이 다르제",
          "speaker_id": "1",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009927.1.15",
          "form": "나는요새 고양이 사료 들고다님 만나면 주려고",
          "original_form": "나는요새 고양이 사료 들고다님 만나면 주려고",
          "speaker_id": "1",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009927.1.16",
          "form": "음 그건 잘생각해보아여",
          "original_form": "음 그건 잘생각해보아여",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009927.1.17",
          "form": "주택사시는분들 힘들어하기도하시더라는",
          "original_form": "주택사시는분들 힘들어하기도하시더라는",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009927.1.18",
          "form": "마자 장소를 잘보고 줘야함ㅜㅜ",
          "original_form": "마자 장소를 잘보고 줘야함ㅜㅜ",
          "speaker_id": "1",
