      "utterance": [
        {
          "id": "MDRW2100011442.1.1",
          "form": "결혼에대해 어떻게생각하세요?",
          "original_form": "결혼에대해 어떻게생각하세요?",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011442.1.2",
          "form": "전 하면 하는데 굳이 하고 싶진 않아요",
          "original_form": "전 하면 하는데 굳이 하고 싶진 않아요",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011442.1.3",
          "form": "누누님은요?",
          "original_form": "누누님은요?",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011442.1.4",
          "form": "저도 그렇게생각하는데",
          "original_form": "저도 그렇게생각하는데",
          "speaker_id": "1",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011442.1.5",
          "form": "전 좀 긍정적으로 생각하는편이예요",
          "original_form": "전 좀 긍정적으로 생각하는편이예요",
          "speaker_id": "1",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011442.1.6",
          "form": "그럼 마음이 맞으면 바로 직진하실 계획이신가요?",
          "original_form": "그럼 마음이 맞으면 바로 직진하실 계획이신가요?",
          "speaker_id": "2",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100011442.1.7",
          "form": "근데 요즘은 그런세상이아니죠ㅋㅋㅋ",
          "original_form": "근데 요즘은 그런세상이아니죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100011442.1.8",
          "form": "금전적인게 준비가되어야죠",
          "original_form": "금전적인게 준비가되어야죠",
          "speaker_id": "1",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100011442.1.9",
          "form": "맞아요 아무래도 비용적인 측면을 무시할 수 없을 테니까요",
          "original_form": "맞아요 아무래도 비용적인 측면을 무시할 수 없을 테니까요",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011442.1.10",
          "form": "맞아요ㅠㅠ 집값이 너무",
          "original_form": "맞아요ㅠㅠ 집값이 너무",
          "speaker_id": "1",
          "time": "20210824 13:02"
        },
        {
          "id": "MDRW2100011442.1.11",
          "form": "엄청나졌죠",
          "original_form": "엄청나졌죠",
          "speaker_id": "1",
          "time": "20210824 13:02"
        },
        {
          "id": "MDRW2100011442.1.12",
          "form": "맞아요 그리고 가구나 결혼식 같은 비용도 엄청나니까요",
          "original_form": "맞아요 그리고 가구나 결혼식 같은 비용도 엄청나니까요",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011442.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011442.1.14",
          "form": "선뜻 결혼하는사람들보면",
          "original_form": "선뜻 결혼하는사람들보면",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011442.1.15",
          "form": "대단하더라고요",
          "original_form": "대단하더라고요",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011442.1.16",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011442.1.17",
          "form": "이제 같이 살아야 할 사람을 딱 결정하는게 아무래도 어렵긴하죠",
          "original_form": "이제 같이 살아야 할 사람을 딱 결정하는게 아무래도 어렵긴하죠",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011442.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100011442.1.19",
