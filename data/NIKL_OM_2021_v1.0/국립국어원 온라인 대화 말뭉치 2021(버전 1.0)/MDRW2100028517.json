      "utterance": [
        {
          "id": "MDRW2100028517.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100028517.1.2",
          "form": "환절기인데 건강관리 어떻게 하시나요?",
          "original_form": "환절기인데 건강관리 어떻게 하시나요?",
          "speaker_id": "1",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100028517.1.3",
          "form": "요즘 코로나때문에 마스크를 더 잘 쓰고 다니고 있어요",
          "original_form": "요즘 코로나때문에 마스크를 더 잘 쓰고 다니고 있어요",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028517.1.4",
          "form": "더워도 겉옷을 챙기기도 하고요",
          "original_form": "더워도 겉옷을 챙기기도 하고요",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028517.1.5",
          "form": "아 그쵸 마스크ㅠㅠ",
          "original_form": "아 그쵸 마스크ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028517.1.6",
          "form": "요새 미세먼지 심해져서 마스크 더 잘 쓰게 되더라구요ㅠㅠ 호흡기가ㅠㅠㅠ",
          "original_form": "요새 미세먼지 심해져서 마스크 더 잘 쓰게 되더라구요ㅠㅠ 호흡기가ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028517.1.7",
          "form": "보통 봄에만 그랬는데 요즘은 사시사철 미세먼지가 문제네요ㅠ",
          "original_form": "보통 봄에만 그랬는데 요즘은 사시사철 미세먼지가 문제네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100028517.1.8",
          "form": "날도 일교차가 너무 심하고",
          "original_form": "날도 일교차가 너무 심하고",
          "speaker_id": "2",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100028517.1.9",
          "form": "맞아요 미세먼지가 아주 그냥....",
          "original_form": "맞아요 미세먼지가 아주 그냥....",
          "speaker_id": "1",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100028517.1.10",
          "form": "여름에는 잠잠하더니 좀 시원해지자마자 심해지저라구요",
          "original_form": "여름에는 잠잠하더니 좀 시원해지자마자 심해지저라구요",
          "speaker_id": "1",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100028517.1.11",
          "form": "이제 곧 가을이라고 환기시키고",
          "original_form": "이제 곧 가을이라고 환기시키고",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100028517.1.12",
          "form": "청소기 돌렸는데 먼지가 어마어마해요",
          "original_form": "청소기 돌렸는데 먼지가 어마어마해요",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100028517.1.13",
          "form": "아 맞아요 먼지가....",
          "original_form": "아 맞아요 먼지가....",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100028517.1.14",
          "form": "어쩐지 매일 청소해도 먼지가 쌓이더라구요ㅠㅠ",
          "original_form": "어쩐지 매일 청소해도 먼지가 쌓이더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100028517.1.15",
          "form": "호흡기 질환이 걱정되더라구요ㅠㅠ",
          "original_form": "호흡기 질환이 걱정되더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100028517.1.16",
          "form": "집에서 마스크 쓸 수도 없고 그렇다고 환기를 안 할 수도 없고",
          "original_form": "집에서 마스크 쓸 수도 없고 그렇다고 환기를 안 할 수도 없고",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100028517.1.17",
          "form": "정말 걱정이네요ㅠ",
          "original_form": "정말 걱정이네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100028517.1.18",
          "form": "맞아요........",
          "original_form": "맞아요........",
          "speaker_id": "1",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100028517.1.19",
          "form": "혹시 비염은 없으세요??",
          "original_form": "혹시 비염은 없으세요??",
          "speaker_id": "1",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100028517.1.20",
          "form": "알레르기성 비염이에요",
          "original_form": "알레르기성 비염이에요",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100028517.1.21",
          "form": "환절기엔 내내 고생이죠",
          "original_form": "환절기엔 내내 고생이죠",
          "speaker_id": "2",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100028517.1.22",
          "form": "아ㅠㅠㅠㅠ 지금이 가장 힘들 때 시겠네요ㅠㅠㅠ",
          "original_form": "아ㅠㅠㅠㅠ 지금이 가장 힘들 때 시겠네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100028517.1.23",
          "form": "저도 비염인이라 ㅋㅋㅋ",
          "original_form": "저도 비염인이라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100028517.1.24",
          "form": "병원은 다니시나요???",
          "original_form": "병원은 다니시나요???",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100028517.1.25",
          "form": "어릴때는 병원을 다니다가",
          "original_form": "어릴때는 병원을 다니다가",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100028517.1.26",
          "form": "요즘은 알레르기성이라 그래서 청소만 자주 하고 있어요ㅎㅎ",
          "original_form": "요즘은 알레르기성이라 그래서 청소만 자주 하고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100028517.1.27",
          "form": "먼지 알러지시면 진짜 신경쓰실게 많으시겠네요",
          "original_form": "먼지 알러지시면 진짜 신경쓰실게 많으시겠네요",
          "speaker_id": "1",
          "time": "20210903 10:29"
        }
      ]
    }
