      "utterance": [
        {
          "id": "MDRW2100005537.1.1",
          "form": "오늘은 저녁도 간단히 초밥 치킨 먹어서 편하네요",
          "original_form": "오늘은 저녁도 간단히 초밥 치킨 먹어서 편하네요",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005537.1.2",
          "form": "설거지는 아직 안했어요 ㅋㅋ",
          "original_form": "설거지는 아직 안했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005537.1.3",
          "form": "다 치우셨어요?",
          "original_form": "다 치우셨어요?",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005537.1.4",
          "form": "근데 집안일에 다 치운단 개념은 없는거 같아요",
          "original_form": "근데 집안일에 다 치운단 개념은 없는거 같아요",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005537.1.5",
          "form": "네 전 치우고 쉬는걸 조아해서 ㅎㅎ",
          "original_form": "네 전 치우고 쉬는걸 조아해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005537.1.6",
          "form": "주말도 남편은 쉬어도 전 계속 집안일하고 ㅡㅡ",
          "original_form": "주말도 남편은 쉬어도 전 계속 집안일하고 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005537.1.7",
          "form": "햇꽃게 나와서  한박스 사서 손질하니 이시간이더라구요 ㅎㅎ",
          "original_form": "햇꽃게 나와서  한박스 사서 손질하니 이시간이더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005537.1.8",
          "form": "제가 ㅋㅋ게을러요",
          "original_form": "제가 ㅋㅋ게을러요",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005537.1.9",
          "form": "저도 게을를땐  한없이 게을러서 그걸알길에 ㅋ 빨리 해요",
          "original_form": "저도 게을를땐  한없이 게을러서 그걸알길에 ㅋ 빨리 해요",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.10",
          "form": "세상에 꽃게 벌써 나왔군요",
          "original_form": "세상에 꽃게 벌써 나왔군요",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.11",
          "form": "전 손질 못해요 ㅋㅋ",
          "original_form": "전 손질 못해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.12",
          "form": "오 그게 답이죠",
          "original_form": "오 그게 답이죠",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.13",
          "form": "네네 찜으로 먹었는데 달아요 달아",
          "original_form": "네네 찜으로 먹었는데 달아요 달아",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.14",
          "form": "저희 식구는 귀찮다고 안먹어서",
          "original_form": "저희 식구는 귀찮다고 안먹어서",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.15",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.16",
          "form": "전 좋아해요",
          "original_form": "전 좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.17",
          "form": "헉 ㅋ 이맛걸 ㅜㅜㅋ",
          "original_form": "헉 ㅋ 이맛걸 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005537.1.18",
          "form": "혼자 먹기도 그래서 못사요",
          "original_form": "혼자 먹기도 그래서 못사요",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005537.1.19",
          "form": "저 불쌍한 사람 입니다",
          "original_form": "저 불쌍한 사람 입니다",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005537.1.20",
          "form": "맞죠 혼자 좋아하는건 선뜻 못사죠",
          "original_form": "맞죠 혼자 좋아하는건 선뜻 못사죠",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005537.1.21",
          "form": "네 오늘도 뭐 살라했더니",
          "original_form": "네 오늘도 뭐 살라했더니",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005537.1.22",
          "form": "남편이 참 이거 맛없다 안먹는다 안산더",
          "original_form": "남편이 참 이거 맛없다 안먹는다 안산더",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005537.1.23",
          "form": "내가 먹고싶다 뭐라했네요 ㅋㅋ",
          "original_form": "내가 먹고싶다 뭐라했네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005537.1.24",
          "form": "헉 !! 그래도 소확행을 위해 콩시미님꺼도 사세요 !!",
          "original_form": "헉 !! 그래도 소확행을 위해 콩시미님꺼도 사세요 !!",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005537.1.25",
          "form": "혼자 먹는건 이래서 안사요",
          "original_form": "혼자 먹는건 이래서 안사요",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005537.1.26",
          "form": "복수 하겠어요 ㅋㅋ",
          "original_form": "복수 하겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005537.1.27",
          "form": "그러려고요",
          "original_form": "그러려고요",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005537.1.28",
          "form": "낼 혼자 디저트 먹으러 가겠어요 ㅋㅋ",
          "original_form": "낼 혼자 디저트 먹으러 가겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005537.1.29",
          "form": "ㅎㅎㅎㅎ  맛있겟어요",
          "original_form": "ㅎㅎㅎㅎ  맛있겟어요",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005537.1.30",
          "form": "낼 스벅 갑니다 ㅋㅋ",
          "original_form": "낼 스벅 갑니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005537.1.31",
          "form": "오늘은 애들이 자기방에서 친구랑 톡하고 놀아서",
          "original_form": "오늘은 애들이 자기방에서 친구랑 톡하고 놀아서",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005537.1.32",
          "form": "더 혼자 노네요",
          "original_form": "더 혼자 노네요",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005537.1.33",
          "form": "이제 커서 맨날 혼자 놀긴해요",
          "original_form": "이제 커서 맨날 혼자 놀긴해요",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005537.1.34",
          "form": "좀더 커야 엄마 친구 하겠죠",
          "original_form": "좀더 커야 엄마 친구 하겠죠",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005537.1.35",
          "form": "다크면 살짝 서운하겠어요",
          "original_form": "다크면 살짝 서운하겠어요",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005537.1.36",
          "form": "서운해요",
          "original_form": "서운해요",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005537.1.37",
