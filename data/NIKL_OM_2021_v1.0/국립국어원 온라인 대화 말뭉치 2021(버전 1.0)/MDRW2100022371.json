      "utterance": [
        {
          "id": "MDRW2100022371.1.1",
          "form": "크고 아삭하게 어떻게 맛있게 김치를 담구죠?",
          "original_form": "크고 아삭하게 어떻게 맛있게 김치를 담구죠?",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022371.1.2",
          "form": "어.. 김치는 안담궈봐서",
          "original_form": "어.. 김치는 안담궈봐서",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022371.1.3",
          "form": "김치냉장고넣으면 맛있어지던데",
          "original_form": "김치냉장고넣으면 맛있어지던데",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022371.1.4",
          "form": "식당에서 깍두기 판매 해줬음 좋겠어요 ㅠㅠ",
          "original_form": "식당에서 깍두기 판매 해줬음 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022371.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022371.1.6",
          "form": "그랬으면 저 그거 사다먹었을듯",
          "original_form": "그랬으면 저 그거 사다먹었을듯",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.7",
          "form": "김치냉장고가 숙성도 도와줘요?",
          "original_form": "김치냉장고가 숙성도 도와줘요?",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.9",
          "form": "그래서 김치냉장고인걸요",
          "original_form": "그래서 김치냉장고인걸요",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.10",
          "form": "오~ 전 살얼음 잔뜩 낀거밖에 못봤어요 ㅠㅠ",
          "original_form": "오~ 전 살얼음 잔뜩 낀거밖에 못봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.11",
          "form": "우리 엄마가 조작을 잘못하셨나",
          "original_form": "우리 엄마가 조작을 잘못하셨나",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.12",
          "form": "앗.. 보관을 잘못하신건가",
          "original_form": "앗.. 보관을 잘못하신건가",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.13",
          "form": "온도가 많이 낮나봐요",
          "original_form": "온도가 많이 낮나봐요",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.14",
          "form": "막 배추김치가 통째로 얼어있는데 넘 웃겨갖곸ㅋㅋ",
          "original_form": "막 배추김치가 통째로 얼어있는데 넘 웃겨갖곸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.15",
          "form": "통채로요?",
          "original_form": "통채로요?",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022371.1.16",
          "form": "그래서 웃었다고 등짝스매싱 맞았어요 ㅠㅠ",
          "original_form": "그래서 웃었다고 등짝스매싱 맞았어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022371.1.17",
          "form": "그정도면 무기인데",
          "original_form": "그정도면 무기인데",
          "speaker_id": "1",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022371.1.18",
          "form": "ㅋㅋㅋㅋ 저같아도 웃길거같아요",
          "original_form": "ㅋㅋㅋㅋ 저같아도 웃길거같아요",
          "speaker_id": "1",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022371.1.19",
          "form": "그니까욬ㅋㅋㅋ 지금은 잘 보관하고 계신듯해요",
          "original_form": "그니까욬ㅋㅋㅋ 지금은 잘 보관하고 계신듯해요",
          "speaker_id": "2",
          "time": "20210829 13:14"
        }
      ]
