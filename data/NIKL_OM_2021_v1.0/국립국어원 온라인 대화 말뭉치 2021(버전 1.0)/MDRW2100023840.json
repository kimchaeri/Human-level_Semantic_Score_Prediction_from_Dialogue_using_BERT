      "utterance": [
        {
          "id": "MDRW2100023840.1.1",
          "form": "내내 자다가 이제 일어나서 밥 먹었엉",
          "original_form": "내내 자다가 이제 일어나서 밥 먹었엉",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023840.1.2",
          "form": "고마엉 이제 정신이 좀 드는데",
          "original_form": "고마엉 이제 정신이 좀 드는데",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023840.1.3",
          "form": "다행...",
          "original_form": "다행...",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023840.1.4",
          "form": "병원은 안갔어?",
          "original_form": "병원은 안갔어?",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023840.1.5",
          "form": "그냥 생리통이랑 피곤한거 겹쳐서 그런거 같아서",
          "original_form": "그냥 생리통이랑 피곤한거 겹쳐서 그런거 같아서",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023840.1.6",
          "form": "병원은 안갔어...",
          "original_form": "병원은 안갔어...",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023840.1.7",
          "form": "갈걸 그랬나",
          "original_form": "갈걸 그랬나",
          "speaker_id": "2",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023840.1.8",
          "form": "그럼 그냥 진통제만 먹은거야?",
          "original_form": "그럼 그냥 진통제만 먹은거야?",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023840.1.9",
          "form": "내일 출근이 넘 무섭다 ㅋㅋㅋ큐ㅠ",
          "original_form": "내일 출근이 넘 무섭다 ㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023840.1.10",
          "form": "열나?",
          "original_form": "열나?",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023840.1.11",
          "form": "그냥 진통제 먹고 잤어 ㅇㅇ",
          "original_form": "그냥 진통제 먹고 잤어 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023840.1.12",
          "form": "열은 안나! 다행이지..",
          "original_form": "열은 안나! 다행이지..",
          "speaker_id": "2",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023840.1.13",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023840.1.14",
          "form": "죽먹었어?",
          "original_form": "죽먹었어?",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023840.1.15",
          "form": "난 약간 아픈 기운 있으면",
          "original_form": "난 약간 아픈 기운 있으면",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023840.1.16",
          "form": "플라시보 효과를 위해서(?) 죽먹어 ㅋㅋㅋ",
          "original_form": "플라시보 효과를 위해서(?) 죽먹어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023840.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 죽먹을까 하다가 그냥 밥먹긴 했엌\n플라시보 효괔ㅋㅋㅋ 그거 괜찮겠는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 죽먹을까 하다가 그냥 밥먹긴 했엌\n플라시보 효괔ㅋㅋㅋ 그거 괜찮겠는데",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023840.1.18",
          "form": "내일도 아프면 죽먹어야지(?",
          "original_form": "내일도 아프면 죽먹어야지(?",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023840.1.19",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023840.1.20",
          "form": "건강한게 최고야",
          "original_form": "건강한게 최고야",
          "speaker_id": "1",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023840.1.21",
          "form": "맞아..건강이 최고야..",
          "original_form": "맞아..건강이 최고야..",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023840.1.22",
          "form": "있을땐 모르지만 없으면 소중한 그것..",
          "original_form": "있을땐 모르지만 없으면 소중한 그것..",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023840.1.23",
          "form": "언니 필라테스 열심히 해...",
          "original_form": "언니 필라테스 열심히 해...",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023840.1.24",
          "form": "(아련",
          "original_form": "(아련",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023840.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023840.1.26",
          "form": "나 방금 충격적인 글 봤어",
          "original_form": "나 방금 충격적인 글 봤어",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023840.1.27",
          "form": "근육이 없으면",
          "original_form": "근육이 없으면",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023840.1.28",
          "form": "물을 아무리 많이 마셔도 체수분이 안올라간대",
          "original_form": "물을 아무리 많이 마셔도 체수분이 안올라간대",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023840.1.29",
          "form": "헉 그럼 그냥 그대로 다시 배출되는건가",
          "original_form": "헉 그럼 그냥 그대로 다시 배출되는건가",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100023840.1.30",
          "form": "그렇대 ㅋㅋㅋㅋㅋ 인체의 신비...",
          "original_form": "그렇대 ㅋㅋㅋㅋㅋ 인체의 신비...",
          "speaker_id": "1",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100023840.1.31",
          "form": "ㅋㅋㅋㅋㅋ 내가 지금껏 물 마신건 변비에만 좋았다는 말이네.... ㅋㅋㅋㅋㅋㅋ 근육이제일 중요한 거였어",
          "original_form": "ㅋㅋㅋㅋㅋ 내가 지금껏 물 마신건 변비에만 좋았다는 말이네.... ㅋㅋㅋㅋㅋㅋ 근육이제일 중요한 거였어",
