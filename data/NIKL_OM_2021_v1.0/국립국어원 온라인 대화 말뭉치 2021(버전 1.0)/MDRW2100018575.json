      "utterance": [
        {
          "id": "MDRW2100018575.1.1",
          "form": "라멘 좋아하세요?",
          "original_form": "라멘 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 08:37"
        },
        {
          "id": "MDRW2100018575.1.2",
          "form": "좋아해요!특히 돈코츠라멘",
          "original_form": "좋아해요!특히 돈코츠라멘",
          "speaker_id": "1",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100018575.1.3",
          "form": "어느 라멘 좋아하세요",
          "original_form": "어느 라멘 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100018575.1.4",
          "form": "앗 저두요. 돈코츠라멘을 알기 전의 나는 풋내기였죠",
          "original_form": "앗 저두요. 돈코츠라멘을 알기 전의 나는 풋내기였죠",
          "speaker_id": "2",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100018575.1.5",
          "form": "지금은 고수인가요ㅎㅎ",
          "original_form": "지금은 고수인가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018575.1.6",
          "form": "수원에 진짜 맛집 있는데.. 가게가 워낙 작아서..",
          "original_form": "수원에 진짜 맛집 있는데.. 가게가 워낙 작아서..",
          "speaker_id": "2",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018575.1.7",
          "form": "오오 이름이 뭔가요",
          "original_form": "오오 이름이 뭔가요",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018575.1.8",
          "form": "요즘은 갈 수가 없네요",
          "original_form": "요즘은 갈 수가 없네요",
          "speaker_id": "2",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018575.1.9",
          "form": "키와마루아지",
          "original_form": "키와마루아지",
          "speaker_id": "2",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018575.1.10",
          "form": "배민에 입점해줘라줘",
          "original_form": "배민에 입점해줘라줘",
          "speaker_id": "2",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018575.1.11",
          "form": "검색해보니 분점도 있네요!",
          "original_form": "검색해보니 분점도 있네요!",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018575.1.12",
          "form": "가봐야겠어요",
          "original_form": "가봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018575.1.13",
          "form": "추천메뉴가 뭔가요",
          "original_form": "추천메뉴가 뭔가요",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018575.1.14",
          "form": "돈코츠라멘요ㅎㅎㅎ",
          "original_form": "돈코츠라멘요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018575.1.15",
          "form": "꼭 온천계란 추가해서 드셔주세용",
          "original_form": "꼭 온천계란 추가해서 드셔주세용",
          "speaker_id": "2",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018575.1.16",
          "form": "네 온천계란은 사랑입니다",
          "original_form": "네 온천계란은 사랑입니다",
          "speaker_id": "1",
          "time": "20210827 08:43"
        },
        {
          "id": "MDRW2100018575.1.17",
          "form": "우동에도 꼭 넣어요",
          "original_form": "우동에도 꼭 넣어요",
          "speaker_id": "1",
          "time": "20210827 08:43"
        },
        {
          "id": "MDRW2100018575.1.18",
          "form": "차슈도 직접 만드시는데 적당히 짭쪼롬하고 맛있어요",
          "original_form": "차슈도 직접 만드시는데 적당히 짭쪼롬하고 맛있어요",
          "speaker_id": "2",
          "time": "20210827 08:43"
        },
        {
          "id": "MDRW2100018575.1.19",
          "form": "차슈...차슈덮밥도 먹고 싶네요",
          "original_form": "차슈...차슈덮밥도 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018575.1.20",
          "form": "차슈 잘하는데 드문데",
          "original_form": "차슈 잘하는데 드문데",
          "speaker_id": "1",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018575.1.21",
          "form": "둘 다 먹읍시다..",
          "original_form": "둘 다 먹읍시다..",
          "speaker_id": "2",
          "time": "20210827 08:46"
        },
        {
          "id": "MDRW2100018575.1.22",
          "form": "먹을 수 있어요",
          "original_form": "먹을 수 있어요",
          "speaker_id": "2",
          "time": "20210827 08:46"
        },
        {
          "id": "MDRW2100018575.1.23",
          "form": "ㅎㅎ 덮밥  미니사이즈는 안파나요",
          "original_form": "ㅎㅎ 덮밥  미니사이즈는 안파나요",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018575.1.24",
          "form": "곁들이로 먹을수 있게",
          "original_form": "곁들이로 먹을수 있게",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018575.1.25",
          "form": "면이랑 밥은 별개입니다(정색)",
          "original_form": "면이랑 밥은 별개입니다(정색)",
          "speaker_id": "2",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018575.1.26",
          "form": "위대하신가봐요ㅎㅎ",
          "original_form": "위대하신가봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018575.1.27",
          "form": "디저트배도 따로겠지요?",
          "original_form": "디저트배도 따로겠지요?",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018575.1.28",
          "form": "그건 국룰입니다",
          "original_form": "그건 국룰입니다",
          "speaker_id": "2",
          "time": "20210827 08:50"
        }
      ]
    }
