      "utterance": [
        {
          "id": "MDRW2100020485.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020485.1.2",
          "form": "넵넵 필라테스 다녀요",
          "original_form": "넵넵 필라테스 다녀요",
          "speaker_id": "2",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020485.1.3",
          "form": "운동 좋아하시나요",
          "original_form": "운동 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020485.1.4",
          "form": "네 홈트요",
          "original_form": "네 홈트요",
          "speaker_id": "1",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020485.1.5",
          "form": "와 집에서 하시는군요 대단하시네요",
          "original_form": "와 집에서 하시는군요 대단하시네요",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020485.1.6",
          "form": "매일요",
          "original_form": "매일요",
          "speaker_id": "1",
          "time": "20210828 08:57"
        },
        {
          "id": "MDRW2100020485.1.7",
          "form": "유튜브 같은거 보면서 해요?",
          "original_form": "유튜브 같은거 보면서 해요?",
          "speaker_id": "2",
          "time": "20210828 08:59"
        },
        {
          "id": "MDRW2100020485.1.8",
          "form": "네 동기부여요",
          "original_form": "네 동기부여요",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100020485.1.9",
          "form": "헬스장엔 잘 안가셔요?",
          "original_form": "헬스장엔 잘 안가셔요?",
          "speaker_id": "2",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100020485.1.10",
          "form": "거리두기라ㅜ아직",
          "original_form": "거리두기라ㅜ아직",
          "speaker_id": "1",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100020485.1.11",
          "form": "코로나땜에 홈트를 시작하셨군요!",
          "original_form": "코로나땜에 홈트를 시작하셨군요!",
          "speaker_id": "2",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020485.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020485.1.13",
          "form": "저는 집에선 절대 안되길래 소규모 레슨이라도 다녀요",
          "original_form": "저는 집에선 절대 안되길래 소규모 레슨이라도 다녀요",
          "speaker_id": "2",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100020485.1.14",
          "form": "효과는요?",
          "original_form": "효과는요?",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100020485.1.15",
          "form": "주변에서 좀 슬림해졌다고 알아봐쥬더라구요!",
          "original_form": "주변에서 좀 슬림해졌다고 알아봐쥬더라구요!",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020485.1.16",
          "form": "오 부럽네요",
          "original_form": "오 부럽네요",
