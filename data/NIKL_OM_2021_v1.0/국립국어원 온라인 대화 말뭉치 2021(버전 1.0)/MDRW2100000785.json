      "utterance": [
        {
          "id": "MDRW2100000785.1.1",
          "form": "예술의전당이었나?",
          "original_form": "예술의전당이었나?",
          "speaker_id": "2",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000785.1.2",
          "form": "벤허가 그리스보다 나았어",
          "original_form": "벤허가 그리스보다 나았어",
          "speaker_id": "1",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000785.1.3",
          "form": "응 그건그래 주인공이 박은태자나",
          "original_form": "응 그건그래 주인공이 박은태자나",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000785.1.4",
          "form": "박은태  목소리 너무 좋음",
          "original_form": "박은태  목소리 너무 좋음",
          "speaker_id": "1",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000785.1.5",
          "form": "모차르트때부터 팬임",
          "original_form": "모차르트때부터 팬임",
          "speaker_id": "2",
          "time": "20210819 19:26"
        },
        {
          "id": "MDRW2100000785.1.6",
          "form": "모차르트가 제일 재밌었어",
          "original_form": "모차르트가 제일 재밌었어",
          "speaker_id": "1",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100000785.1.7",
          "form": "그 유영숙인가그분도 잘부름",
          "original_form": "그 유영숙인가그분도 잘부름",
          "speaker_id": "2",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100000785.1.8",
          "form": "ㅇㅇ 맞아 그분이 성량은 제일 좋았어",
          "original_form": "ㅇㅇ 맞아 그분이 성량은 제일 좋았어",
          "speaker_id": "1",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100000785.1.9",
          "form": "황금별 노래 다운받음",
          "original_form": "황금별 노래 다운받음",
          "speaker_id": "2",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000785.1.10",
          "form": "요즘은 뮤지컬 안하나?",
          "original_form": "요즘은 뮤지컬 안하나?",
          "speaker_id": "1",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000785.1.11",
          "form": "할걸 큰거는",
          "original_form": "할걸 큰거는",
          "speaker_id": "2",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000785.1.12",
          "form": "작은것들은 다죽은듯ㅜ",
          "original_form": "작은것들은 다죽은듯ㅜ",
          "speaker_id": "1",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000785.1.13",
          "form": "그니까 소극장들은 문닫을듯",
          "original_form": "그니까 소극장들은 문닫을듯",
          "speaker_id": "2",
