      "utterance": [
        {
          "id": "MDRW2100016827.1.1",
          "form": "좋아하는음식있으세요",
          "original_form": "좋아하는음식있으세요",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016827.1.2",
          "form": "저는 요새 한창 브리또에 빠져살아요 ㅋㅋ",
          "original_form": "저는 요새 한창 브리또에 빠져살아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016827.1.3",
          "form": "오~맛있겠어요!",
          "original_form": "오~맛있겠어요!",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016827.1.4",
          "form": "맛있어요ㅜ일주일째먹고있어요 ㅋㅋㅋ",
          "original_form": "맛있어요ㅜ일주일째먹고있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016827.1.5",
          "form": "또띠아로 직접만드시는거에요~!?",
          "original_form": "또띠아로 직접만드시는거에요~!?",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016827.1.6",
          "form": "아뇨ㅜ 아쉽게도 테이크아웃으로 먹고있어요",
          "original_form": "아뇨ㅜ 아쉽게도 테이크아웃으로 먹고있어요",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016827.1.7",
          "form": "사먹음...더맛잇죠",
          "original_form": "사먹음...더맛잇죠",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016827.1.8",
          "form": "집에서 하는것도 재료비랑노력생각하면 ㅠㅠ",
          "original_form": "집에서 하는것도 재료비랑노력생각하면 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016827.1.9",
          "form": "맞아요.. 홈메이드 여간 번거로운게 아니라서",
          "original_form": "맞아요.. 홈메이드 여간 번거로운게 아니라서",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016827.1.10",
          "form": "맞아요 ㅠ 집에서 하묜 더싸다싶어도",
          "original_form": "맞아요 ㅠ 집에서 하묜 더싸다싶어도",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016827.1.11",
          "form": "식구가 많우면 몰라도 1~2가구눈 재료값이 더나가요ㅠ",
          "original_form": "식구가 많우면 몰라도 1~2가구눈 재료값이 더나가요ㅠ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016827.1.12",
          "form": "그런것같더라구요..",
          "original_form": "그런것같더라구요..",
          "speaker_id": "1",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016827.1.13",
          "form": "집에선 거의 과일만 혼자 깎아먹는거같아요",
          "original_form": "집에선 거의 과일만 혼자 깎아먹는거같아요",
          "speaker_id": "1",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016827.1.14",
          "form": "전 복숭아 완전좋아해요ㅠㅠ",
          "original_form": "전 복숭아 완전좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016827.1.15",
          "form": "맞아요~ 요새 황도가 어쩜그리 맛있던지",
          "original_form": "맞아요~ 요새 황도가 어쩜그리 맛있던지",
          "speaker_id": "1",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016827.1.16",
          "form": "맞아요ㅠ~ 전 품종안가리고 다좋아하능데 말랑파이긴해요 !!",
          "original_form": "맞아요ㅠ~ 전 품종안가리고 다좋아하능데 말랑파이긴해요 !!",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016827.1.17",
          "form": "아하~ 저는 쫄복젤좋아해요 ㅎㅎ!",
          "original_form": "아하~ 저는 쫄복젤좋아해요 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100016827.1.18",
          "form": "점심드셨나요~",
          "original_form": "점심드셨나요~",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100016827.1.19",
          "form": "저는 육개장 먹었답니다",
