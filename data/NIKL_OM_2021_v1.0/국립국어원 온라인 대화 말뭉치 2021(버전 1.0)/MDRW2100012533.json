      "utterance": [
        {
          "id": "MDRW2100012533.1.1",
          "form": "커뮤니티 활동 뭐하세요?",
          "original_form": "커뮤니티 활동 뭐하세요?",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012533.1.2",
          "form": "음..전 딱히 활동 안하는 것 같은데",
          "original_form": "음..전 딱히 활동 안하는 것 같은데",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012533.1.3",
          "form": "하시는 거 있으세요?",
          "original_form": "하시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012533.1.4",
          "form": "그냥 맘카페?",
          "original_form": "그냥 맘카페?",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012533.1.5",
          "form": "저도 맘카페 주식카페",
          "original_form": "저도 맘카페 주식카페",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012533.1.6",
          "form": "활동해요",
          "original_form": "활동해요",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012533.1.7",
          "form": "엄마들 다 그렇죠",
          "original_form": "엄마들 다 그렇죠",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012533.1.8",
          "form": "오 주식하시는구나!",
          "original_form": "오 주식하시는구나!",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012533.1.9",
          "form": "전 주린이...완전 쌩초보에요.",
          "original_form": "전 주린이...완전 쌩초보에요.",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.10",
          "form": "공모주만요",
          "original_form": "공모주만요",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.11",
          "form": "블로그 카페 정보 없음 못해요",
          "original_form": "블로그 카페 정보 없음 못해요",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.12",
          "form": "아빠가 해보라고 하는데 모르겠어요ㅋㅋㅋㅋ",
          "original_form": "아빠가 해보라고 하는데 모르겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.13",
          "form": "오 하세요?",
          "original_form": "오 하세요?",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.14",
          "form": "아빠가 하셔요",
          "original_form": "아빠가 하셔요",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.15",
          "form": "공모주 하세요 요새 정보",
          "original_form": "공모주 하세요 요새 정보",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.16",
          "form": "블로그에서 보면돼요",
          "original_form": "블로그에서 보면돼요",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012533.1.17",
          "form": "내일까지 하는 일진도 엄청좋다고 블로그에서 찬사해서",
          "original_form": "내일까지 하는 일진도 엄청좋다고 블로그에서 찬사해서",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012533.1.18",
          "form": "넣으려하는데 도전해 보세요",
          "original_form": "넣으려하는데 도전해 보세요",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012533.1.19",
          "form": "오 정보 감사드려요\n한번 알아볼게요!",
          "original_form": "오 정보 감사드려요\n한번 알아볼게요!",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012533.1.20",
          "form": "아 전 카페로 아이 스터디해요ㅋㅋ",
          "original_form": "아 전 카페로 아이 스터디해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012533.1.21",
          "form": "영어 신문 같은거~",
          "original_form": "영어 신문 같은거~",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012533.1.22",
          "form": "네 낼마지막이니 삼성 미래 대신중 있음하세요",
          "original_form": "네 낼마지막이니 삼성 미래 대신중 있음하세요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012533.1.23",
          "form": "아 대단하세요",
          "original_form": "아 대단하세요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012533.1.24",
          "form": "전 능력이 안돼요",
          "original_form": "전 능력이 안돼요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012533.1.25",
          "form": "카페에서 그런것도 하는군요 서로 도움주고 하나봐요",
          "original_form": "카페에서 그런것도 하는군요 서로 도움주고 하나봐요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012533.1.26",
          "form": "네~스터디 비용 내고 자료 받아서 과제 내고 해요ㅎㅎ",
          "original_form": "네~스터디 비용 내고 자료 받아서 과제 내고 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012533.1.27",
          "form": "저도 겨우겨우 따라가요:)",
          "original_form": "저도 겨우겨우 따라가요:)",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012533.1.28",
          "form": "아 비용을 내는군요",
          "original_form": "아 비용을 내는군요",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012533.1.29",
          "form": "에이 잘하실것 같아요",
          "original_form": "에이 잘하실것 같아요",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012533.1.30",
          "form": "ㅎㅎ그렇게 봐주시니 감사해요~",
          "original_form": "ㅎㅎ그렇게 봐주시니 감사해요~",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012533.1.31",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012533.1.32",
          "form": "전 주식만 보며 좀 공부하요 ㅎㅎ",
          "original_form": "전 주식만 보며 좀 공부하요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012533.1.33",
          "form": "요샌 책보다 커뮤니티 활동으로 정보 얻는거 같아요",
          "original_form": "요샌 책보다 커뮤니티 활동으로 정보 얻는거 같아요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012533.1.34",
          "form": "맞아요 책보다 정보가 훨씬 빠르죠!",
          "original_form": "맞아요 책보다 정보가 훨씬 빠르죠!",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012533.1.35",
          "form": "내",
          "original_form": "내",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012533.1.36",
          "form": "그리고 지루하지 않아요 ㅋㅋ",
          "original_form": "그리고 지루하지 않아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012533.1.37",
          "form": "영상도 올라와서요 ㅎㅎ",
          "original_form": "영상도 올라와서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012533.1.38",
          "form": "낼 일진 하이 솔루션 도전해보세요 ㅎㅎ",
          "original_form": "낼 일진 하이 솔루션 도전해보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012533.1.39",
          "form": "영상도 올라오는군요!",
          "original_form": "영상도 올라오는군요!",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012533.1.40",
          "form": "넵 감사해요:)",
          "original_form": "넵 감사해요:)",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012533.1.41",
          "form": "네 요샌 기본인가봐요",
          "original_form": "네 요샌 기본인가봐요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        }
      ]
    }
  ]
