      "utterance": [
        {
          "id": "MDRW2100013692.1.1",
          "form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기\r\n어떠신가요?",
          "original_form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기\r\n어떠신가요?",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013692.1.2",
          "form": "음 전자요",
          "original_form": "음 전자요",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013692.1.3",
          "form": "ㅋㅋㅋ이유는요???.",
          "original_form": "ㅋㅋㅋ이유는요???.",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013692.1.4",
          "form": "단무지 없이는 해봐서요ㅋㅋㅋ",
          "original_form": "단무지 없이는 해봐서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013692.1.5",
          "form": "ㅋㅋㅋ 아무래도 저도",
          "original_form": "ㅋㅋㅋ 아무래도 저도",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013692.1.6",
          "form": "라면 김치없인 잘먹는답니다",
          "original_form": "라면 김치없인 잘먹는답니다",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013692.1.7",
          "form": "ㅋㅋ 라면에 김치가 없었더라면~~",
          "original_form": "ㅋㅋ 라면에 김치가 없었더라면~~",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013692.1.8",
          "form": "노래도 있는데",
          "original_form": "노래도 있는데",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013692.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100013692.1.10",
          "form": "저녁은드셨나요?",
          "original_form": "저녁은드셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100013692.1.11",
          "form": "네 친구들 만나서 ㅎㅎ",
          "original_form": "네 친구들 만나서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100013692.1.12",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100013692.1.13",
