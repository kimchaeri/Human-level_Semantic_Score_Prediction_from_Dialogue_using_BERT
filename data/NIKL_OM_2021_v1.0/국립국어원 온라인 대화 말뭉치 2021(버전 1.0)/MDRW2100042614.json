        {
          "id": "MDRW2100042614.1.1",
          "form": "네 황태국이요!",
          "original_form": "네 황태국이요!",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042614.1.2",
          "form": "오 황태국 맛있겠어요ㅠㅠㅠㅠ",
          "original_form": "오 황태국 맛있겠어요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100042614.1.3",
          "form": "평소 아침 드시는 편이신가요?",
          "original_form": "평소 아침 드시는 편이신가요?",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100042614.1.4",
          "form": "네 꼬박꼬박 ㅋㅋ",
          "original_form": "네 꼬박꼬박 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100042614.1.5",
          "form": "챙기시나요??",
          "original_form": "챙기시나요??",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100042614.1.6",
          "form": "저는 잠이 많아서 아침은 포기하는편이에요ㅎㅎ",
          "original_form": "저는 잠이 많아서 아침은 포기하는편이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100042614.1.7",
          "form": "아침 간단하게 드시나요?!",
          "original_form": "아침 간단하게 드시나요?!",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100042614.1.8",
          "form": "거하게 먹어요ㅋㅋ 고기도 가능합니다",
          "original_form": "거하게 먹어요ㅋㅋ 고기도 가능합니다",
          "speaker_id": "2",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100042614.1.9",
          "form": "오 오늘 아침엔 무얼 드셨나요?!",
          "original_form": "오 오늘 아침엔 무얼 드셨나요?!",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100042614.1.10",
          "form": "오늘은 간단하게 찰밥이요\r\nㅋㅋ",
          "original_form": "오늘은 간단하게 찰밥이요\r\nㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100042614.1.11",
          "form": "우와 맛있는거 드셨네요!!",
          "original_form": "우와 맛있는거 드셨네요!!",
          "speaker_id": "1",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100042614.1.12",
          "form": "보통 직접 해드시나요?",
          "original_form": "보통 직접 해드시나요?",
          "speaker_id": "1",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100042614.1.13",
          "form": "아뇨ㅋㅋ 엄마가ㅋㅋ",
          "original_form": "아뇨ㅋㅋ 엄마가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042614.1.14",
          "form": "전 불량식품느낌 잘만들어요ㅋㅋ",
          "original_form": "전 불량식품느낌 잘만들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100042614.1.15",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100042614.1.16",
          "form": "저도 요리 한다고 하는데",
          "original_form": "저도 요리 한다고 하는데",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100042614.1.17",
          "form": "맨날 결과물이 이상해요...ㅎㅎ",
          "original_form": "맨날 결과물이 이상해요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100042614.1.18",
          "form": "ㅋㅋname1님이 먹기엔 맛있어요?",
          "original_form": "ㅋㅋ&name1&님이 먹기엔 맛있어요?",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100042614.1.19",
          "form": "제 입에도 그냥 그래요ㅎㅎ",
          "original_form": "제 입에도 그냥 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100042614.1.20",
          "form": "그래서 요리를 잘안한답니다ㅎㅎ",
          "original_form": "그래서 요리를 잘안한답니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100042614.1.21",
