      "utterance": [
        {
          "id": "MDRW2100028327.1.1",
          "form": "싸이월드 부활했대요",
          "original_form": "싸이월드 부활했대요",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028327.1.2",
          "form": "싸이월드가 뭐냐??",
          "original_form": "싸이월드가 뭐냐??",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028327.1.3",
          "form": "개인 홈페이지? 블로그 같은 거예요",
          "original_form": "개인 홈페이지? 블로그 같은 거예요",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028327.1.4",
          "form": "부활이라니",
          "original_form": "부활이라니",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028327.1.5",
          "form": "그게 예전에 서비스 종료 했었거든요",
          "original_form": "그게 예전에 서비스 종료 했었거든요",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028327.1.6",
          "form": "대표가 돈들고 튄걸로 알고있어요",
          "original_form": "대표가 돈들고 튄걸로 알고있어요",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028327.1.7",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028327.1.8",
          "form": "그런 사이트도 그렇게 튀니?",
          "original_form": "그런 사이트도 그렇게 튀니?",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028327.1.9",
          "form": "요즘 세상 무서워서... 근데 정확히는 모르겠어요",
          "original_form": "요즘 세상 무서워서... 근데 정확히는 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028327.1.10",
          "form": "부활한거 보니 아닐수도 있고?",
          "original_form": "부활한거 보니 아닐수도 있고?",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028327.1.11",
          "form": "제대로 알고 말해야지",
          "original_form": "제대로 알고 말해야지",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028327.1.12",
          "form": "ㅋㅋㅋ 네 아무튼 막 도토리 같은거 주고받고 그랬어요",
          "original_form": "ㅋㅋㅋ 네 아무튼 막 도토리 같은거 주고받고 그랬어요",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028327.1.13",
          "form": "방명록도 남기고",
          "original_form": "방명록도 남기고",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028327.1.14",
          "form": "먹을거 주고받은거야?",
          "original_form": "먹을거 주고받은거야?",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028327.1.15",
          "form": "그건 아니고 사이버 코인같은거네요",
          "original_form": "그건 아니고 사이버 코인같은거네요",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028327.1.16",
          "form": "충전해서 쓰는거?",
          "original_form": "충전해서 쓰는거?",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028327.1.17",
          "form": "네네 그걸로 홈페이지 꾸미고!! 재밌었는데",
          "original_form": "네네 그걸로 홈페이지 꾸미고!! 재밌었는데",
          "speaker_id": "1",
          "time": "20210902 19:12"
        }
