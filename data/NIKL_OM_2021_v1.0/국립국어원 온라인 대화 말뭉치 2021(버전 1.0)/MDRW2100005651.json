      "utterance": [
        {
          "id": "MDRW2100005651.1.1",
          "form": "결혼은 빨리 하는게 좋은건가..?",
          "original_form": "결혼은 빨리 하는게 좋은건가..?",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005651.1.2",
          "form": "안하는게 좋지 않을까",
          "original_form": "안하는게 좋지 않을까",
          "speaker_id": "1",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100005651.1.3",
          "form": "ㅋㅋㅋㅋ다들 안하는게 좋다고 하더라",
          "original_form": "ㅋㅋㅋㅋ다들 안하는게 좋다고 하더라",
          "speaker_id": "2",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100005651.1.4",
          "form": "무섭지 않냐 막 결혼한다는게",
          "original_form": "무섭지 않냐 막 결혼한다는게",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100005651.1.5",
          "form": "내가 집을 책임져야되는 위치가 된다는거잖아",
          "original_form": "내가 집을 책임져야되는 위치가 된다는거잖아",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100005651.1.6",
          "form": "맞아 근데 나는 아이만 없으면 각자 시간도 챙기고 같이 있는 시간도 챙기고 좋을 것 같아!",
          "original_form": "맞아 근데 나는 아이만 없으면 각자 시간도 챙기고 같이 있는 시간도 챙기고 좋을 것 같아!",
          "speaker_id": "2",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100005651.1.7",
          "form": "아이가 없으면 생각할만 하겠다",
          "original_form": "아이가 없으면 생각할만 하겠다",
          "speaker_id": "1",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100005651.1.8",
          "form": "근데 그럴거면 동거가 낫지않나?",
          "original_form": "근데 그럴거면 동거가 낫지않나?",
          "speaker_id": "1",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100005651.1.9",
          "form": "흠 그런가",
          "original_form": "흠 그런가",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100005651.1.10",
          "form": "근데 요즘 결혼하묜 혜택들이 있어서 차라리 동거보단 결혼이 낫지 않은가~싶긴 한데",
          "original_form": "근데 요즘 결혼하묜 혜택들이 있어서 차라리 동거보단 결혼이 낫지 않은가~싶긴 한데",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100005651.1.11",
          "form": "잘 모르겠당ㅋㅋ",
          "original_form": "잘 모르겠당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100005651.1.12",
          "form": "아직 결혼은 생각해본적 없어서 모르겠디",
          "original_form": "아직 결혼은 생각해본적 없어서 모르겠디",
          "speaker_id": "1",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100005651.1.13",
          "form": "이제 곧 생각은 해봐야될텐데",
          "original_form": "이제 곧 생각은 해봐야될텐데",
          "speaker_id": "1",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100005651.1.14",
          "form": "고민이 많아!!",
          "original_form": "고민이 많아!!",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100005651.1.15",
          "form": "이래저래 걱정도 많구 그렇당",
          "original_form": "이래저래 걱정도 많구 그렇당",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100005651.1.16",
          "form": "언젠간 하겠지 뭐",
          "original_form": "언젠간 하겠지 뭐",
          "speaker_id": "1",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100005651.1.17",
          "form": "난 아직 어린애같은데",
          "original_form": "난 아직 어린애같은데",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100005651.1.18",
          "form": "벌써 이런 고민을 하네",
          "original_form": "벌써 이런 고민을 하네",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100005651.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그니까;;나도 어린데;;",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그니까;;나도 어린데;;",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100005651.1.20",
          "form": "연애 근황은 어때",
          "original_form": "연애 근황은 어때",
          "speaker_id": "1",
          "time": "20210823 18:59"
        },
        {
          "id": "MDRW2100005651.1.21",
          "form": "완전 잘 만나는중",
          "original_form": "완전 잘 만나는중",
          "speaker_id": "2",
          "time": "20210823 18:59"
        },
        {
          "id": "MDRW2100005651.1.22",
          "form": "1주년이야 곧",
          "original_form": "1주년이야 곧",
          "speaker_id": "2",
          "time": "20210823 18:59"
        },
        {
          "id": "MDRW2100005651.1.23",
          "form": "1주년 기념 선물은 준비한거 있어?",
          "original_form": "1주년 기념 선물은 준비한거 있어?",
          "speaker_id": "1",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100005651.1.24",
          "form": "커플링 맞추기로 했어!!!",
          "original_form": "커플링 맞추기로 했어!!!",
          "speaker_id": "2",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100005651.1.25",
          "form": "브랜드로 맞추고싶은데 어디꺼 사야할지 완전 고민중이여",
          "original_form": "브랜드로 맞추고싶은데 어디꺼 사야할지 완전 고민중이여",
          "speaker_id": "2",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100005651.1.26",
          "form": "난 친구 여자친구가 금은방해서 거기서 맞췄었는데",
          "original_form": "난 친구 여자친구가 금은방해서 거기서 맞췄었는데",
          "speaker_id": "1",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100005651.1.27",
          "form": "로이체였나 뭐 암튼 그런거",
          "original_form": "로이체였나 뭐 암튼 그런거",
          "speaker_id": "1",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100005651.1.28",
          "form": "옹 맘에드는걸로 골랐어?",
          "original_form": "옹 맘에드는걸로 골랐어?",
          "speaker_id": "2",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100005651.1.29",
          "form": "여자친구가 결정 못내려서 마음속으로 코카콜라 했으",
          "original_form": "여자친구가 결정 못내려서 마음속으로 코카콜라 했으",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100005651.1.30",
          "form": "고민하는척하면서",
          "original_form": "고민하는척하면서",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100005651.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100005651.1.32",
          "form": "멋지다",
