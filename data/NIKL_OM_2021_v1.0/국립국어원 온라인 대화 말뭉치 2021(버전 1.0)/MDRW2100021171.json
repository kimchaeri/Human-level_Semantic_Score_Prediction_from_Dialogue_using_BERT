      "utterance": [
        {
          "id": "MDRW2100021171.1.1",
          "form": "자주보는 유튜브영상이나 프로그램있으세요?",
          "original_form": "자주보는 유튜브영상이나 프로그램있으세요?",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021171.1.2",
          "form": "여행유튭이요",
          "original_form": "여행유튭이요",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100021171.1.3",
          "form": "오 저랑같네오",
          "original_form": "오 저랑같네오",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100021171.1.4",
          "form": "전 해외여행 유튜버ㅜㅜ",
          "original_form": "전 해외여행 유튜버ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100021171.1.5",
          "form": "대리만족하려구요",
          "original_form": "대리만족하려구요",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100021171.1.6",
          "form": "헉 저 빠니보틀이요",
          "original_form": "헉 저 빠니보틀이요",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100021171.1.7",
          "form": "전 곽튜브",
          "original_form": "전 곽튜브",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100021171.1.8",
          "form": "잼있어요",
          "original_form": "잼있어요",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100021171.1.9",
          "form": "곽튜브가 빠니보틀하고 친하던데 ㅋㅋㅋ",
          "original_form": "곽튜브가 빠니보틀하고 친하던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100021171.1.10",
          "form": "저 곽튜브도 봅니다ㅎㅎㅎ",
          "original_form": "저 곽튜브도 봅니다ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100021171.1.11",
          "form": "어 보시는군여",
          "original_form": "어 보시는군여",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100021171.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100021171.1.13",
          "form": "별거없는데 계속 보게되더라구여 ㅋㅋ",
          "original_form": "별거없는데 계속 보게되더라구여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100021171.1.14",
          "form": "러시아편 잼써요ㅎㅎ",
          "original_form": "러시아편 잼써요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100021171.1.15",
          "form": "마자요 ㅋㅋㅋ저도 최근에 봤어요",
          "original_form": "마자요 ㅋㅋㅋ저도 최근에 봤어요",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100021171.1.16",
          "form": "러시아어 엄청 잘하던데",
          "original_form": "러시아어 엄청 잘하던데",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100021171.1.17",
          "form": "마자요 그래서 더 재밌는듯",
          "original_form": "마자요 그래서 더 재밌는듯",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100021171.1.18",
          "form": "지금 여행할수있다는거 부럽",
          "original_form": "지금 여행할수있다는거 부럽",
