      "utterance": [
        {
          "id": "MDRW2100023884.1.1",
          "form": "오 어디까지 가봣어요",
          "original_form": "오 어디까지 가봣어요",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.2",
          "form": "젤 가고싶은건 바르셀로난데",
          "original_form": "젤 가고싶은건 바르셀로난데",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.3",
          "form": "솔로몬 제도나 바티칸시국 이런데 빼고요",
          "original_form": "솔로몬 제도나 바티칸시국 이런데 빼고요",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.4",
          "form": "와 많이 다니셨구나",
          "original_form": "와 많이 다니셨구나",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.6",
          "form": "메시의 고향 바르셀로나에 오신걸 환영합니다.",
          "original_form": "메시의 고향 바르셀로나에 오신걸 환영합니다.",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.8",
          "form": "바르셀로나는 사실 도시 이름인거 아시죠?",
          "original_form": "바르셀로나는 사실 도시 이름인거 아시죠?",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023884.1.10",
          "form": "스페인 여행으로 유명하구요 5월이나 6월 정도에 가시는게 좋아요",
          "original_form": "스페인 여행으로 유명하구요 5월이나 6월 정도에 가시는게 좋아요",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023884.1.11",
          "form": "오 왜요",
          "original_form": "오 왜요",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023884.1.12",
          "form": "비행기 직항 타시면 13시간 정도 걸리니까 비행기에서 낮잠잘 도구들 챙겨가시면 더 좋더라구요",
          "original_form": "비행기 직항 타시면 13시간 정도 걸리니까 비행기에서 낮잠잘 도구들 챙겨가시면 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023884.1.13",
          "form": "5월 6월 절대 못가는데",
          "original_form": "5월 6월 절대 못가는데",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023884.1.14",
          "form": "은퇴하고나 가겟네요",
          "original_form": "은퇴하고나 가겟네요",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023884.1.15",
          "form": "그때 날씨가 좀 온화하고 쾌적해요. 우리나라 초여름이랑 비슷한데",
          "original_form": "그때 날씨가 좀 온화하고 쾌적해요. 우리나라 초여름이랑 비슷한데",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023884.1.16",
          "form": "7월이나 8월은.. 현지인들이 휴가를 잘 떠나는 시기라서 가게 문 많이 닫아요 ㅜㅜ",
          "original_form": "7월이나 8월은.. 현지인들이 휴가를 잘 떠나는 시기라서 가게 문 많이 닫아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.17",
          "form": "그리고 다들 이때 여행 와가지구 여행객 엄청 많아요",
          "original_form": "그리고 다들 이때 여행 와가지구 여행객 엄청 많아요",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.18",
          "form": "그렇구나 은퇴하고 갈게요...",
          "original_form": "그렇구나 은퇴하고 갈게요...",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.19",
          "form": "봄방학 없으신가요?",
          "original_form": "봄방학 없으신가요?",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.20",
          "form": "봄방학 있지만",
          "original_form": "봄방학 있지만",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.21",
          "form": "은근 시간 안나던걸요",
          "original_form": "은근 시간 안나던걸요",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.22",
          "form": "아 그렇구나 ㅜㅜ",
          "original_form": "아 그렇구나 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.23",
          "form": "바르셀로나 물가는 한국이랑 비슷해서 돈 펑펑 쓰기 좋아요",
          "original_form": "바르셀로나 물가는 한국이랑 비슷해서 돈 펑펑 쓰기 좋아요",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023884.1.24",
          "form": "혹시나 가게 되면 일단 여기저기 무작정 걸어보시는걸 추천해요",
          "original_form": "혹시나 가게 되면 일단 여기저기 무작정 걸어보시는걸 추천해요",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023884.1.25",
          "form": "건물 너무 멋진게 많더라구오",
          "original_form": "건물 너무 멋진게 많더라구오",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023884.1.26",
          "form": "맞아요, 어디까지 알아보고 오셨어요?",
          "original_form": "맞아요, 어디까지 알아보고 오셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023884.1.27",
          "form": "뭐 한번 랜선투어는 햇으니가",
          "original_form": "뭐 한번 랜선투어는 햇으니가",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023884.1.28",
          "form": "가장 유명한게 아마 로마 가톨릭 성당인 사그라다 파밀리아 성당이 아닐까 싶어요",
          "original_form": "가장 유명한게 아마 로마 가톨릭 성당인 사그라다 파밀리아 성당이 아닐까 싶어요",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023884.1.29",
          "form": "복습합니다 지금",
          "original_form": "복습합니다 지금",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023884.1.30",
          "form": "콜럼버스 동상이 있는 람블라 거리도 뭐 아실거구",
          "original_form": "콜럼버스 동상이 있는 람블라 거리도 뭐 아실거구",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023884.1.31",
          "form": "시장으로는 보케리아 시장이 유명하겠네용",
          "original_form": "시장으로는 보케리아 시장이 유명하겠네용",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023884.1.32",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023884.1.33",
          "form": "며칠이나 다녀오셧나요",
          "original_form": "며칠이나 다녀오셧나요",
          "speaker_id": "2",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023884.1.34",
          "form": "근데 사람들은 뭐지 호셉마켓? 이렇게 부르더군요",
          "original_form": "근데 사람들은 뭐지 호셉마켓? 이렇게 부르더군요",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023884.1.35",
          "form": "얼마나 머물렀는 지는 기억이 잘 안납니다",
          "original_form": "얼마나 머물렀는 지는 기억이 잘 안납니다",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023884.1.36",
          "form": "어디가 젤 좋앗어요",
          "original_form": "어디가 젤 좋앗어요",
          "speaker_id": "2",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023884.1.37",
          "form": "해외여행 중에서요?",
          "original_form": "해외여행 중에서요?",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023884.1.38",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023884.1.39",
          "form": "여행지가 좋아서라기 보다는",
          "original_form": "여행지가 좋아서라기 보다는",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023884.1.40",
          "form": "함께 했던 사람들이 좋아서인데.. 중국이요",
          "original_form": "함께 했던 사람들이 좋아서인데.. 중국이요",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
