      "utterance": [
        {
          "id": "MDRW2100037969.1.1",
          "form": "응응 늦었넹",
          "original_form": "응응 늦었넹",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037969.1.2",
          "form": "어 요새 일이 많다더라고",
          "original_form": "어 요새 일이 많다더라고",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037969.1.3",
          "form": "우리 신랑은 지금 원 때문에 걱정이 많다",
          "original_form": "우리 신랑은 지금 원 때문에 걱정이 많다",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037969.1.4",
          "form": "캠핑은 취소했으",
          "original_form": "캠핑은 취소했으",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037969.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037969.1.6",
          "form": "왜ㅜㅜ수랑 ?",
          "original_form": "왜ㅜㅜ수랑 ?",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037969.1.7",
          "form": "그렇게해서?",
          "original_form": "그렇게해서?",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037969.1.8",
          "form": "어 수랑 가는 캠핑 취소했다",
          "original_form": "어 수랑 가는 캠핑 취소했다",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037969.1.9",
          "form": "이번에는 안 가기로 했어",
          "original_form": "이번에는 안 가기로 했어",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037969.1.10",
          "form": "응 잘했다잉",
          "original_form": "응 잘했다잉",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.11",
          "form": "이번에는 패스하고",
          "original_form": "이번에는 패스하고",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.12",
          "form": "담에 괜찮아지면 가면되징",
          "original_form": "담에 괜찮아지면 가면되징",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.13",
          "form": "근데 오늘 우리 원이 와서 얘기하는데\r\n수한테 수한테 캠핑을 가자고 했다는 거야",
          "original_form": "근데 오늘 우리 원이 와서 얘기하는데\r\n수한테 수한테 캠핑을 가자고 했다는 거야",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.14",
          "form": "근데 수가 캠핑하기 싫다고 했대",
          "original_form": "근데 수가 캠핑하기 싫다고 했대",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.15",
          "form": "그래서 원이 캠핑 같이 가자고 계속 얘기하니까",
          "original_form": "그래서 원이 캠핑 같이 가자고 계속 얘기하니까",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.16",
          "form": "무한에 계단에서 있는 무슨 캐릭터 쿠폰을 달라고 했다 돼",
          "original_form": "무한에 계단에서 있는 무슨 캐릭터 쿠폰을 달라고 했다 돼",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.17",
          "form": "그거 주면은 같이 간다고",
          "original_form": "그거 주면은 같이 간다고",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037969.1.18",
          "form": "그래서  원이 그 쿠폰 주기로 하고 같이 캠핑을 가기로 했다는 거야",
          "original_form": "그래서  원이 그 쿠폰 주기로 하고 같이 캠핑을 가기로 했다는 거야",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037969.1.19",
          "form": "근데 계속 이러면 원이 좀 끌려 다닐 거 같아 가지고",
          "original_form": "근데 계속 이러면 원이 좀 끌려 다닐 거 같아 가지고",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037969.1.20",
          "form": "그냥 안 가는 게 낫다고 얘기했음",
          "original_form": "그냥 안 가는 게 낫다고 얘기했음",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037969.1.21",
          "form": "아 그랬구나",
          "original_form": "아 그랬구나",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037969.1.22",
          "form": "우리 신랑도 이제 소 아빠하고 전화하면서",
          "original_form": "우리 신랑도 이제 소 아빠하고 전화하면서",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.23",
          "form": "아구 ㅜ또 그런일이ㄷㄷ",
          "original_form": "아구 ㅜ또 그런일이ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.24",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.25",
          "form": "더이상 애들 문제에 개입하지 말자고",
          "original_form": "더이상 애들 문제에 개입하지 말자고",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.26",
          "form": "맞어맞어",
          "original_form": "맞어맞어",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.27",
          "form": "애들끼리 싸우고 지지고 볶게 놔두자고",
          "original_form": "애들끼리 싸우고 지지고 볶게 놔두자고",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.28",
          "form": "얘기했다 더라고",
          "original_form": "얘기했다 더라고",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.29",
          "form": "나도 그게 나을 거 같아서",
          "original_form": "나도 그게 나을 거 같아서",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.30",
          "form": "맞다 개입하면안된다ㅜㅜ",
          "original_form": "맞다 개입하면안된다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.31",
          "form": "이제 원한테 자세히 안 물어보려고",
          "original_form": "이제 원한테 자세히 안 물어보려고",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037969.1.32",
          "form": "개입하게되면 어른들만 진짜 어색해진다니깐ㅜ",
          "original_form": "개입하게되면 어른들만 진짜 어색해진다니깐ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100037969.1.33",
          "form": "어른들이 서로 친하니깐 더한것같아ㅜ",
          "original_form": "어른들이 서로 친하니깐 더한것같아ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        }
      ]
