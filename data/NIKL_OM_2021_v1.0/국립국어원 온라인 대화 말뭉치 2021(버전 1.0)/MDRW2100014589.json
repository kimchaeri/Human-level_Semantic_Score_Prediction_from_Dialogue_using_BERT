      "utterance": [
        {
          "id": "MDRW2100014589.1.1",
          "form": "요즘 먹는 영양제있나요?",
          "original_form": "요즘 먹는 영양제있나요?",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014589.1.2",
          "form": "다이어트관심잇나요?",
          "original_form": "다이어트관심잇나요?",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014589.1.3",
          "form": "저는  유산균 마그네슘 먹어요",
          "original_form": "저는  유산균 마그네슘 먹어요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014589.1.4",
          "form": "오~ 전 비타민 디 챙겨 먹어요",
          "original_form": "오~ 전 비타민 디 챙겨 먹어요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014589.1.5",
          "form": "영원한 다이어터입니다",
          "original_form": "영원한 다이어터입니다",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014589.1.6",
          "form": "말만이지만",
          "original_form": "말만이지만",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014589.1.7",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014589.1.8",
          "form": "다이어트는 내일부터요",
          "original_form": "다이어트는 내일부터요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014589.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014589.1.10",
          "form": "요즘은 다이어트때문에 영양제로 연명하는 사람도 많은것 같아요",
          "original_form": "요즘은 다이어트때문에 영양제로 연명하는 사람도 많은것 같아요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014589.1.11",
          "form": "그런가요 ㅋㅋㅋㅌ",
          "original_form": "그런가요 ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014589.1.12",
          "form": "건강이 최고죠 그래도",
          "original_form": "건강이 최고죠 그래도",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014589.1.13",
          "form": "그쵸? 영양제는 어디서 구매하세요",
          "original_form": "그쵸? 영양제는 어디서 구매하세요",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014589.1.14",
          "form": "전 아이허브 들락많이했었는데",
          "original_form": "전 아이허브 들락많이했었는데",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014589.1.15",
          "form": "저는  지인추천으로  그냥 사요",
          "original_form": "저는  지인추천으로  그냥 사요",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014589.1.16",
          "form": "아이허브는 뭐에요",
          "original_form": "아이허브는 뭐에요",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014589.1.17",
          "form": "해외직구사이트인데 영양제 쪽이 많아요",
          "original_form": "해외직구사이트인데 영양제 쪽이 많아요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014589.1.18",
          "form": "귤젤리가 젤 유명",
          "original_form": "귤젤리가 젤 유명",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014589.1.19",
          "form": "할인도 많이하구요",
          "original_form": "할인도 많이하구요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014589.1.20",
          "form": "어머  너무 긔엽다  귤 젤리라니",
          "original_form": "어머  너무 긔엽다  귤 젤리라니",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014589.1.21",
          "form": "새로운  정보 감사합니다",
          "original_form": "새로운  정보 감사합니다",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014589.1.22",
          "form": "요즘 올리브영에서 살 수있어요",
          "original_form": "요즘 올리브영에서 살 수있어요",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014589.1.23",
          "form": "진짜요 ㅋ  나중에  가보도록 할게요 ㅋㅋ",
          "original_form": "진짜요 ㅋ  나중에  가보도록 할게요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014589.1.24",
          "form": "귤먹고 싶네요 ㅋㅋ",
          "original_form": "귤먹고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:13"
        }
      ]
