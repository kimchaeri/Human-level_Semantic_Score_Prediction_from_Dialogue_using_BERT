      "utterance": [
        {
          "id": "MDRW2100024802.1.1",
          "form": "집에서는 주로 뭐해",
          "original_form": "집에서는 주로 뭐해",
          "speaker_id": "2",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100024802.1.2",
          "form": "야구봐",
          "original_form": "야구봐",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024802.1.3",
          "form": "ㅋㅋㅋㅋ밥먹니?",
          "original_form": "ㅋㅋㅋㅋ밥먹니?",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024802.1.4",
          "form": "오늘은 뭐해주셔써",
          "original_form": "오늘은 뭐해주셔써",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024802.1.5",
          "form": "아직 집 도착 안했어 ㅋㅋㅋㅋ",
          "original_form": "아직 집 도착 안했어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:12"
        },
        {
          "id": "MDRW2100024802.1.6",
          "form": "아니 무슨일이야",
          "original_form": "아니 무슨일이야",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024802.1.7",
          "form": "비안오지?",
          "original_form": "비안오지?",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024802.1.8",
          "form": "원래 칼퇴해도 한 7시 15~20분쯤 도착해",
          "original_form": "원래 칼퇴해도 한 7시 15~20분쯤 도착해",
          "speaker_id": "2",
          "time": "20210831 19:41"
        },
        {
          "id": "MDRW2100024802.1.9",
          "form": "ㅇㅇ 오늘 한밤중에 온대",
          "original_form": "ㅇㅇ 오늘 한밤중에 온대",
          "speaker_id": "2",
          "time": "20210831 19:41"
        },
        {
          "id": "MDRW2100024802.1.10",
          "form": "이제 저녁먹었다 ㅋㅋㅋ 좀 쉬다 설거지해야지",
          "original_form": "이제 저녁먹었다 ㅋㅋㅋ 좀 쉬다 설거지해야지",
          "speaker_id": "2",
          "time": "20210831 19:41"
        },
        {
          "id": "MDRW2100024802.1.11",
          "form": "ㅋㅋㅋㅋ고생했다",
          "original_form": "ㅋㅋㅋㅋ고생했다",
          "speaker_id": "1",
          "time": "20210831 19:42"
        },
        {
          "id": "MDRW2100024802.1.12",
          "form": "뭐먹어써",
          "original_form": "뭐먹어써",
          "speaker_id": "1",
          "time": "20210831 19:42"
        },
        {
          "id": "MDRW2100024802.1.13",
          "form": "어제 남은 수육이랑 있는 반찬 ㅋㅋㅋ",
          "original_form": "어제 남은 수육이랑 있는 반찬 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:45"
        },
        {
          "id": "MDRW2100024802.1.14",
          "form": "수육..",
          "original_form": "수육..",
          "speaker_id": "1",
          "time": "20210831 19:46"
        },
        {
          "id": "MDRW2100024802.1.15",
          "form": "나 니가 고기얘기해서",
          "original_form": "나 니가 고기얘기해서",
          "speaker_id": "1",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100024802.1.16",
          "form": "목살구워먹음 어제ㅋㅋㅋ",
          "original_form": "목살구워먹음 어제ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100024802.1.17",
          "form": "어억 ㅋㅋㅋㅋ 몸보신했네",
          "original_form": "어억 ㅋㅋㅋㅋ 몸보신했네",
          "speaker_id": "2",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024802.1.18",
          "form": "집에 오면 암것두 하기 싫지 않냐",
          "original_form": "집에 오면 암것두 하기 싫지 않냐",
          "speaker_id": "2",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024802.1.19",
          "form": "ㄹㅇ 설거지미뤄둔거봐",
          "original_form": "ㄹㅇ 설거지미뤄둔거봐",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024802.1.20",
          "form": "설거지 바로 안하면 한도끝도 없이 밀림 ",
          "original_form": "설거지 바로 안하면 한도끝도 없이 밀림 ",
          "speaker_id": "2",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100024802.1.21",
          "form": "맞앜ㅋㅋㅋㅋ",
          "original_form": "맞앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100024802.1.22",
          "form": "아니근데난 물에좀 불리고하고싶거든",
          "original_form": "아니근데난 물에좀 불리고하고싶거든",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100024802.1.23",
          "form": "근데엄마가 너무너무싫어함",
          "original_form": "근데엄마가 너무너무싫어함",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100024802.1.24",
          "form": "ㅋㅋㅋㅋ 아 나도",
          "original_form": "ㅋㅋㅋㅋ 아 나도",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100024802.1.25",
          "form": "좀 불리고 하려하면 엄마가 바로 해버려서",
          "original_form": "좀 불리고 하려하면 엄마가 바로 해버려서",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100024802.1.26",
          "form": "기다리지말고 바로 해야함 ㄷㄷ",
          "original_form": "기다리지말고 바로 해야함 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100024802.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100024802.1.28",
          "form": "싱크대에 물있는걸 시러해",
          "original_form": "싱크대에 물있는걸 시러해",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100024802.1.29",
          "form": "근데좀 불려야 힘도덜들고..",
          "original_form": "근데좀 불려야 힘도덜들고..",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100024802.1.30",
          "form": "배도좀꺼지고",
          "original_form": "배도좀꺼지고",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100024802.1.31",
          "form": "하지않나..",
          "original_form": "하지않나..",
          "speaker_id": "1",
          "time": "20210901 18:42"
        }
      ]
    }
  ]
