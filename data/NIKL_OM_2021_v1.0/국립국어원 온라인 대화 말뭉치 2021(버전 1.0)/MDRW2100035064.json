      "utterance": [
        {
          "id": "MDRW2100035064.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035064.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035064.1.3",
          "form": "피자 몇번드세요? 일년에",
          "original_form": "피자 몇번드세요? 일년에",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035064.1.4",
          "form": "음 피자라..",
          "original_form": "음 피자라..",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035064.1.5",
          "form": "안드시구나",
          "original_form": "안드시구나",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035064.1.6",
          "form": "냉동피자+피자집에서 구매 까지 합한다면",
          "original_form": "냉동피자+피자집에서 구매 까지 합한다면",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035064.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035064.1.8",
          "form": "8~10번 정도 아닐까 싶어요",
          "original_form": "8~10번 정도 아닐까 싶어요",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035064.1.9",
          "form": "오.... 생각보다 자주 드시네여",
          "original_form": "오.... 생각보다 자주 드시네여",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035064.1.10",
          "form": "무슨피자드세여?",
          "original_form": "무슨피자드세여?",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035064.1.11",
          "form": "브랜드",
          "original_form": "브랜드",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035064.1.12",
          "form": "가끔 세일하면 오뚜기피자였던가 그거 냉동보관 해두었다가 먹고, 사먹게되면 동네 피자투어가 있어서 포장해서요",
          "original_form": "가끔 세일하면 오뚜기피자였던가 그거 냉동보관 해두었다가 먹고, 사먹게되면 동네 피자투어가 있어서 포장해서요",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100035064.1.13",
          "form": "다른 브랜드는 일년에 1~2번 정도요",
          "original_form": "다른 브랜드는 일년에 1~2번 정도요",
          "speaker_id": "2",
          "time": "20210829 01:37"
        },
        {
          "id": "MDRW2100035064.1.14",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210829 01:37"
        },
        {
          "id": "MDRW2100035064.1.15",
          "form": "풀무원더 맛잇다더라고여",
          "original_form": "풀무원더 맛잇다더라고여",
          "speaker_id": "1",
          "time": "20210829 01:37"
        },
        {
          "id": "MDRW2100035064.1.16",
          "form": "노엣지피자여",
          "original_form": "노엣지피자여",
          "speaker_id": "1",
          "time": "20210829 01:37"
        },
        {
          "id": "MDRW2100035064.1.17",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100035064.1.18",
          "form": "다음에 냉동피자 구매하게 된다면 참고할게요",
          "original_form": "다음에 냉동피자 구매하게 된다면 참고할게요",
          "speaker_id": "2",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100035064.1.19",
          "form": "가끔 딜뜨면 종종 알려드릴게여",
          "original_form": "가끔 딜뜨면 종종 알려드릴게여",
          "speaker_id": "1",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100035064.1.20",
          "form": "언젠가 필요하겟져",
          "original_form": "언젠가 필요하겟져",
          "speaker_id": "1",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100035064.1.21",
          "form": "갑자기 피자얘기하니까 배가고프네요",
          "original_form": "갑자기 피자얘기하니까 배가고프네요",
          "speaker_id": "1",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100035064.1.22",
          "form": "네! 저녁먹고나면 늦게 잠드니까 항상 12~2시 사이쯤 배고픈거같아요",
          "original_form": "네! 저녁먹고나면 늦게 잠드니까 항상 12~2시 사이쯤 배고픈거같아요",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100035064.1.23",
          "form": "아까는 소화가안되서 베아제 주황색먹엇는데 이젠 배고프다니 ㅋㅋ당황햇어요",
          "original_form": "아까는 소화가안되서 베아제 주황색먹엇는데 이젠 배고프다니 ㅋㅋ당황햇어요",
          "speaker_id": "1",
          "time": "20210829 01:41"
        }
      ]
