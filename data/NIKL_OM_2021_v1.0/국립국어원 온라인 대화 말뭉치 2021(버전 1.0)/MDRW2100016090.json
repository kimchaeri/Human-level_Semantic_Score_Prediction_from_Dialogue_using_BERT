      "utterance": [
        {
          "id": "MDRW2100016090.1.1",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016090.1.2",
          "form": "이사다니고 새로 사귀다보니",
          "original_form": "이사다니고 새로 사귀다보니",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016090.1.3",
          "form": "연락처도 모르겠네요",
          "original_form": "연락처도 모르겠네요",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016090.1.4",
          "form": "만나세요?",
          "original_form": "만나세요?",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016090.1.5",
          "form": "저도 거의ㅋㅋㅋ",
          "original_form": "저도 거의ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016090.1.6",
          "form": "그땐 아무 이유 없이 친했었는데",
          "original_form": "그땐 아무 이유 없이 친했었는데",
          "speaker_id": "1",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016090.1.7",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 10:11"
        },
        {
          "id": "MDRW2100016090.1.8",
          "form": "친구가 그런거죠",
          "original_form": "친구가 그런거죠",
          "speaker_id": "2",
          "time": "20210826 10:11"
        },
        {
          "id": "MDRW2100016090.1.9",
          "form": "크고나니 친구도 아이들 엄마 친구 를 사귀게 돼요",
          "original_form": "크고나니 친구도 아이들 엄마 친구 를 사귀게 돼요",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.10",
          "form": "전 고딩때 야자시간에 탈출 ㅋㅋ",
          "original_form": "전 고딩때 야자시간에 탈출 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.11",
          "form": "아 그렇겠네요. 아이 학교가 가정의 연장이군요",
          "original_form": "아 그렇겠네요. 아이 학교가 가정의 연장이군요",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.12",
          "form": "그재미ㅋㅋ",
          "original_form": "그재미ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.13",
          "form": "노래방 갔다 걸려서 ㅋㅋ",
          "original_form": "노래방 갔다 걸려서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.14",
          "form": "엄청 혼났네요 ㅋㅋ",
          "original_form": "엄청 혼났네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.15",
          "form": "어떻게 걸리셨어요ㅋㅋ",
          "original_form": "어떻게 걸리셨어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.16",
          "form": "그땐 체벌이 있어서",
          "original_form": "그땐 체벌이 있어서",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016090.1.17",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016090.1.18",
          "form": "친구랑 같이가니 넘 비잖아요",
          "original_form": "친구랑 같이가니 넘 비잖아요",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016090.1.19",
          "form": "걸리죠 ㅋㅋ",
          "original_form": "걸리죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016090.1.20",
          "form": "ㅋㅋ 과감하셨네요",
          "original_form": "ㅋㅋ 과감하셨네요",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016090.1.21",
          "form": "딱한번요 ㅋㅋ",
          "original_form": "딱한번요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016090.1.22",
          "form": "웃신거 저 선도부 ㅋㅋ",
          "original_form": "웃신거 저 선도부 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016090.1.23",
          "form": "ㅋㅋㅋ 더 혼나지 않나요?ㅋㅋ",
          "original_form": "ㅋㅋㅋ 더 혼나지 않나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016090.1.24",
          "form": "더혼났죠",
          "original_form": "더혼났죠",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.25",
          "form": "엉덩이 불났죠 ㅠㅠ",
          "original_form": "엉덩이 불났죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.26",
          "form": "그땐 참 성적 내려도 때려",
          "original_form": "그땐 참 성적 내려도 때려",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.27",
          "form": "떠들어도 때려",
          "original_form": "떠들어도 때려",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.28",
          "form": "전 만화책 봤다고..",
          "original_form": "전 만화책 봤다고..",
          "speaker_id": "1",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.29",
          "form": "요새 애들 믿지도 않을걸요",
          "original_form": "요새 애들 믿지도 않을걸요",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.30",
          "form": "전 소설",
          "original_form": "전 소설",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.31",
          "form": "뺏기고 ㅜㅜ",
          "original_form": "뺏기고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.32",
          "form": "그쵸? 요즘 체벌 없죠?",
          "original_form": "그쵸? 요즘 체벌 없죠?",
          "speaker_id": "1",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100016090.1.33",
          "form": "없죠",
          "original_form": "없죠",
          "speaker_id": "2",
          "time": "20210826 10:15"
        },
        {
          "id": "MDRW2100016090.1.34",
          "form": "요즘은 소설 읽으면 학교서 칭찬해주자나요ㅋㅋ",
          "original_form": "요즘은 소설 읽으면 학교서 칭찬해주자나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:15"
        }
      ]
    }
