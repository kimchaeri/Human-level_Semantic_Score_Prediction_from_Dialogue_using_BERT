      "utterance": [
        {
          "id": "MDRW2100005813.1.1",
          "form": "해줄게 무궁무짆ㅐ요",
          "original_form": "해줄게 무궁무짆ㅐ요",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100005813.1.2",
          "form": "간식시장도 너무 커졌고요",
          "original_form": "간식시장도 너무 커졌고요",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100005813.1.3",
          "form": "전용스파도 있어요",
          "original_form": "전용스파도 있어요",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100005813.1.4",
          "form": "지인보니 납골당에 안치해서 추모도 하더라구요",
          "original_form": "지인보니 납골당에 안치해서 추모도 하더라구요",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100005813.1.5",
          "form": "동물은 동물이에요",
          "original_form": "동물은 동물이에요",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100005813.1.6",
          "form": "저도 그렇게 생각합니다",
          "original_form": "저도 그렇게 생각합니다",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100005813.1.7",
          "form": "강아지를 사람보다 더아껴요",
          "original_form": "강아지를 사람보다 더아껴요",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100005813.1.8",
          "form": "공원에 개 유모차보면 기함하겠어요",
          "original_form": "공원에 개 유모차보면 기함하겠어요",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100005813.1.9",
          "form": "저는 이해못해요",
          "original_form": "저는 이해못해요",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005813.1.10",
          "form": "지인보니 사막여우를 키우더라구요",
          "original_form": "지인보니 사막여우를 키우더라구요",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005813.1.11",
          "form": "우리나라에서요?",
          "original_form": "우리나라에서요?",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005813.1.12",
          "form": "네 시중가를 보니 50~100만원이래요",
          "original_form": "네 시중가를 보니 50~100만원이래요",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005813.1.13",
          "form": "별걸다 키우네요",
          "original_form": "별걸다 키우네요",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005813.1.14",
          "form": "암만봐도 보여주기 식으로들 키워서 저는 별로에ㅛ",
          "original_form": "암만봐도 보여주기 식으로들 키워서 저는 별로에ㅛ",
          "speaker_id": "1",
          "time": "20210907 13:18"
        }
