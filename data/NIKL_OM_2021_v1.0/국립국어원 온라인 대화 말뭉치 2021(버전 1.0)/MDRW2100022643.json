      "utterance": [
        {
          "id": "MDRW2100022643.1.1",
          "form": "와 아침 챙겨먹는 게 생각보다 더 어려운 일이구나",
          "original_form": "와 아침 챙겨먹는 게 생각보다 더 어려운 일이구나",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022643.1.2",
          "form": "싶더라구요",
          "original_form": "싶더라구요",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022643.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022643.1.4",
          "form": "잠이 더 중요해요",
          "original_form": "잠이 더 중요해요",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022643.1.5",
          "form": "밥 한술 뜰 시간에 잠 5분이 더 중요한거죠,,",
          "original_form": "밥 한술 뜰 시간에 잠 5분이 더 중요한거죠,,",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022643.1.6",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022643.1.7",
          "form": "배고픔은 참아도",
          "original_form": "배고픔은 참아도",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022643.1.8",
          "form": "피곤하고 졸린 건 진짜 힘들더라구요",
          "original_form": "피곤하고 졸린 건 진짜 힘들더라구요",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022643.1.9",
          "form": "근데 밥 굶으면 체력이 더 떨어지는",
          "original_form": "근데 밥 굶으면 체력이 더 떨어지는",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022643.1.10",
          "form": "악순환의 굴레",
          "original_form": "악순환의 굴레",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022643.1.11",
          "form": "맞아요 먹고가면 아침이 다른데",
          "original_form": "맞아요 먹고가면 아침이 다른데",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022643.1.12",
          "form": "먹는게 참 힘들어요",
          "original_form": "먹는게 참 힘들어요",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022643.1.13",
          "form": "ㅋㅋㅋ그래서 한동안 하루야채라도 시켜먹었거든요",
          "original_form": "ㅋㅋㅋ그래서 한동안 하루야채라도 시켜먹었거든요",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022643.1.14",
          "form": "사무실에서 직원들이랑 같이 시키면 자리에 가져다주셨어요!",
          "original_form": "사무실에서 직원들이랑 같이 시키면 자리에 가져다주셨어요!",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022643.1.15",
          "form": "근데 코로나 터지면서 외부인 출입이 금지되니까.. 그마저도...ㅎ",
          "original_form": "근데 코로나 터지면서 외부인 출입이 금지되니까.. 그마저도...ㅎ",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022643.1.16",
          "form": "오 하루야채 좋네요!",
          "original_form": "오 하루야채 좋네요!",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022643.1.17",
          "form": "진짜 아예 빈 속이랑 가볍게 뭐라도 먹는거랑 다르더라구요",
          "original_form": "진짜 아예 빈 속이랑 가볍게 뭐라도 먹는거랑 다르더라구요",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022643.1.18",
          "form": "아 아쉽네요 코로나 거리두기만 아니었으면..ㅠ",
          "original_form": "아 아쉽네요 코로나 거리두기만 아니었으면..ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022643.1.19",
          "form": "그 전날 밀프랩인가",
          "original_form": "그 전날 밀프랩인가",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022643.1.20",
          "form": "저녁에 준비해서 만들어 갖고 다니시는 분들도 계시던데",
          "original_form": "저녁에 준비해서 만들어 갖고 다니시는 분들도 계시던데",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022643.1.21",
          "form": "저는 정말 p타입...",
          "original_form": "저는 정말 p타입...",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022643.1.22",
          "form": "귀차니즘..",
          "original_form": "귀차니즘..",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022643.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022643.1.24",
          "form": "요즘엔 이런말안쓰죠?ㅠ 귀차니즘이런거ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "요즘엔 이런말안쓰죠?ㅠ 귀차니즘이런거ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022643.1.25",
          "form": "잘 모르겠어요ㅋㅋㅋㅋㅋ저도 요즘 어린이들 용어를 몰라서",
          "original_form": "잘 모르겠어요ㅋㅋㅋㅋㅋ저도 요즘 어린이들 용어를 몰라서",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022643.1.26",
          "form": "제가 아는 거 다 끌어다 쓰고 있잖아요",
          "original_form": "제가 아는 거 다 끌어다 쓰고 있잖아요",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022643.1.27",
          "form": "대충 뜻만 통하면 되겠지! 하고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대충 뜻만 통하면 되겠지! 하고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022643.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022643.1.29",
          "form": "아까 미디어 말했던게 떠오르는데",
          "original_form": "아까 미디어 말했던게 떠오르는데",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022643.1.30",
          "form": "요즘 어린학생들 사이에서",
          "original_form": "요즘 어린학생들 사이에서",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022643.1.31",
          "form": "프로아나라고 극심하게 다이어트 하는 게",
          "original_form": "프로아나라고 극심하게 다이어트 하는 게",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022643.1.32",
          "form": "유행이라더라구요",
          "original_form": "유행이라더라구요",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022643.1.33",
          "form": "프로아나요?",
          "original_form": "프로아나요?",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022643.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022643.1.35",
          "form": "프로(찬성)+아노렉시아(거식증)",
          "original_form": "프로(찬성)+아노렉시아(거식증)",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022643.1.36",
          "form": "이 합성어 신조어라는데",
          "original_form": "이 합성어 신조어라는데",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022643.1.37",
          "form": "미디어에서 거의 극저체중 연예인들이",
          "original_form": "미디어에서 거의 극저체중 연예인들이",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022643.1.38",
          "form": "어머...",
          "original_form": "어머...",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022643.1.39",
          "form": "멋진 모습으로 비춰지니까",
          "original_form": "멋진 모습으로 비춰지니까",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022643.1.40",
          "form": "미의식이 점점 건강을 심하게 해치는 방향으로 가고있다더라구요",
          "original_form": "미의식이 점점 건강을 심하게 해치는 방향으로 가고있다더라구요",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022643.1.41",
          "form": "왜 저희때도 렛미인 이런 방송 많았잖아요",
          "original_form": "왜 저희때도 렛미인 이런 방송 많았잖아요",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022643.1.42",
          "form": "아 맞아요 맞아요",
          "original_form": "아 맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022643.1.43",
          "form": "와 오랜만에 들어봐요",
          "original_form": "와 오랜만에 들어봐요",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022643.1.44",
          "form": "매체가 참 어린 여성들한테 해로운거같아요",
          "original_form": "매체가 참 어린 여성들한테 해로운거같아요",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022643.1.45",
          "form": "성인 여성들한테도 해롭긴 마찬가지지만",
          "original_form": "성인 여성들한테도 해롭긴 마찬가지지만",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022643.1.46",
          "form": "좀 더 가치관이 세워지지 못한 시기잖아요",
          "original_form": "좀 더 가치관이 세워지지 못한 시기잖아요",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022643.1.47",
          "form": "옷은 점점 더 작아지고 짧아지고...",
          "original_form": "옷은 점점 더 작아지고 짧아지고...",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022643.1.48",
          "form": "그러게 말이예요..",
          "original_form": "그러게 말이예요..",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022643.1.49",
          "form": "특히 10대때는 아이돌이 정말",
          "original_form": "특히 10대때는 아이돌이 정말",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022643.1.50",
          "form": "방송에서는 손바닥만한 옷을 입은 여자아이돌을 찬양하고",
          "original_form": "방송에서는 손바닥만한 옷을 입은 여자아이돌을 찬양하고",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022643.1.51",
          "form": "유행을 주도하는 역할인데",
          "original_form": "유행을 주도하는 역할인데",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022643.1.52",
          "form": "제 말이요!",
          "original_form": "제 말이요!",
          "speaker_id": "2",
