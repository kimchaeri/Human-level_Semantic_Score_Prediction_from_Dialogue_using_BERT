      "utterance": [
        {
          "id": "MDRW2100000757.1.1",
          "form": "언니야 요즘에 홈트하잖아",
          "original_form": "언니야 요즘에 홈트하잖아",
          "speaker_id": "1",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000757.1.2",
          "form": "다이어트목적이야?",
          "original_form": "다이어트목적이야?",
          "speaker_id": "1",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000757.1.3",
          "form": "건강목적이야?",
          "original_form": "건강목적이야?",
          "speaker_id": "1",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000757.1.4",
          "form": "음..둘다야",
          "original_form": "음..둘다야",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000757.1.5",
          "form": "건강도 챙기고 다이어트도 하잣!",
          "original_form": "건강도 챙기고 다이어트도 하잣!",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000757.1.6",
          "form": "이런거야",
          "original_form": "이런거야",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000757.1.7",
          "form": "너도 요즘에 운동하기로 결심했잖아??",
          "original_form": "너도 요즘에 운동하기로 결심했잖아??",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000757.1.8",
          "form": "그랬었지 한창 홈트 유행이니까",
          "original_form": "그랬었지 한창 홈트 유행이니까",
          "speaker_id": "1",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000757.1.9",
          "form": "나도 흐름을따라",
          "original_form": "나도 흐름을따라",
          "speaker_id": "1",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000757.1.10",
          "form": "ㅋㅋㅋㅋ좋다",
          "original_form": "ㅋㅋㅋㅋ좋다",
          "speaker_id": "2",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000757.1.11",
          "form": "우리가족 모두다 운동하자~~",
          "original_form": "우리가족 모두다 운동하자~~",
          "speaker_id": "2",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000757.1.12",
          "form": "아자아자 홧팅이닷!",
          "original_form": "아자아자 홧팅이닷!",
          "speaker_id": "2",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000757.1.13",
          "form": "무슨 운동할거야?",
          "original_form": "무슨 운동할거야?",
          "speaker_id": "2",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000757.1.14",
          "form": "나는 언니야가하는",
          "original_form": "나는 언니야가하는",
          "speaker_id": "1",
          "time": "20210819 19:06"
        },
        {
          "id": "MDRW2100000757.1.15",
          "form": "댄스홈트",
          "original_form": "댄스홈트",
          "speaker_id": "1",
          "time": "20210819 19:06"
        },
        {
          "id": "MDRW2100000757.1.16",
          "form": "그게 신나고좋더라고",
          "original_form": "그게 신나고좋더라고",
          "speaker_id": "1",
          "time": "20210819 19:06"
        },
        {
          "id": "MDRW2100000757.1.17",
          "form": "댄스홈트가 뭐지???",
          "original_form": "댄스홈트가 뭐지???",
          "speaker_id": "2",
          "time": "20210819 19:06"
        },
        {
          "id": "MDRW2100000757.1.18",
          "form": "누구꺼 얘기하는거야?",
          "original_form": "누구꺼 얘기하는거야?",
          "speaker_id": "2",
          "time": "20210819 19:06"
        },
        {
          "id": "MDRW2100000757.1.19",
          "form": "나도 잘모르겠어",
          "original_form": "나도 잘모르겠어",
          "speaker_id": "1",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000757.1.20",
          "form": "막 춤추는거있었는데",
          "original_form": "막 춤추는거있었는데",
          "speaker_id": "1",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000757.1.21",
          "form": "춤추는것도 칼로리가 엄청소모된대",
          "original_form": "춤추는것도 칼로리가 엄청소모된대",
          "speaker_id": "1",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000757.1.22",
          "form": "혹시 그거 타바타??",
          "original_form": "혹시 그거 타바타??",
          "speaker_id": "2",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000757.1.23",
          "form": "엇 맞아 그 운동이였어!!!",
          "original_form": "엇 맞아 그 운동이였어!!!",
          "speaker_id": "1",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100000757.1.24",
          "form": "나도 나증에 그거 해볼라구",
          "original_form": "나도 나증에 그거 해볼라구",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.25",
          "form": "오키오키 좋아",
          "original_form": "오키오키 좋아",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.26",
          "form": "에어컨 틀고 가치하자!!",
          "original_form": "에어컨 틀고 가치하자!!",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.27",
          "form": "킼킼 조아조아",
          "original_form": "킼킼 조아조아",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.28",
          "form": "나도 이제 건강해질래",
          "original_form": "나도 이제 건강해질래",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.29",
          "form": "너무 먹기만 했다",
          "original_form": "너무 먹기만 했다",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.30",
          "form": "건강챙기는거 매우조아여",
          "original_form": "건강챙기는거 매우조아여",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.31",
          "form": "그래야 공부도 더열심히 할수있어!!!",
          "original_form": "그래야 공부도 더열심히 할수있어!!!",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100000757.1.32",
          "form": "마자....",
          "original_form": "마자....",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
