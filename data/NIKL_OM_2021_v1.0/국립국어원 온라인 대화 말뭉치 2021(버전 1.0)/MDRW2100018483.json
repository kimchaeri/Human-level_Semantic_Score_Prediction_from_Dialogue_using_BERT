        },
        {
          "id": "MDRW2100018483.1.3",
          "form": "네 대학생입니다",
          "original_form": "네 대학생입니다",
          "speaker_id": "1",
          "time": "20210827 02:58"
        },
        {
          "id": "MDRW2100018483.1.4",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210827 02:58"
        },
        {
          "id": "MDRW2100018483.1.5",
          "form": "아니요 전 졸업했어요!",
          "original_form": "아니요 전 졸업했어요!",
          "speaker_id": "2",
          "time": "20210827 02:59"
        },
        {
          "id": "MDRW2100018483.1.6",
          "form": "아직 개강 안 했겠네요",
          "original_form": "아직 개강 안 했겠네요",
          "speaker_id": "2",
          "time": "20210827 02:59"
        },
        {
