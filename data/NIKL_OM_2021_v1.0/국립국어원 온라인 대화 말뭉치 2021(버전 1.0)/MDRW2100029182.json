      "utterance": [
        {
          "id": "MDRW2100029182.1.1",
          "form": "군대 한때는",
          "original_form": "군대 한때는",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029182.1.2",
          "form": "2년2개월이었는데",
          "original_form": "2년2개월이었는데",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029182.1.3",
          "form": "1년이  적당한거 같아요",
          "original_form": "1년이  적당한거 같아요",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029182.1.4",
          "form": "지금너무짧음",
          "original_form": "지금너무짧음",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029182.1.5",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.6",
          "form": "심지어",
          "original_form": "심지어",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.7",
          "form": "몇년가야 만족?",
          "original_form": "몇년가야 만족?",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.8",
          "form": "휴대폰도 사용함",
          "original_form": "휴대폰도 사용함",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.9",
          "form": "그건 별로'..",
          "original_form": "그건 별로'..",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.10",
          "form": "내 사촌동생",
          "original_form": "내 사촌동생",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.11",
          "form": "첫째는",
          "original_form": "첫째는",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.12",
          "form": "군대가서 전화로 통화했고",
          "original_form": "군대가서 전화로 통화했고",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.13",
          "form": "둘째는 처음에 군대에서 제공하는",
          "original_form": "둘째는 처음에 군대에서 제공하는",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029182.1.14",
          "form": "근데 그래야 군에서 폭력이나",
          "original_form": "근데 그래야 군에서 폭력이나",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.15",
          "form": "어플로 사용하다가",
          "original_form": "어플로 사용하다가",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.16",
          "form": "걍화상통화 자유롭게함",
          "original_form": "걍화상통화 자유롭게함",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.17",
          "form": "사망사건이 줄어들것 같아요",
          "original_form": "사망사건이 줄어들것 같아요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.18",
          "form": "자기폰한다고",
          "original_form": "자기폰한다고",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.19",
          "form": "후임들 괴롭히는거",
          "original_form": "후임들 괴롭히는거",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.20",
          "form": "오오 좋아졌네요",
          "original_form": "오오 좋아졌네요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.21",
          "form": "많이 줄었다고 함",
          "original_form": "많이 줄었다고 함",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.22",
          "form": "왕따가 어디든 문제",
          "original_form": "왕따가 어디든 문제",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.23",
          "form": "금정적 영향이네요",
          "original_form": "금정적 영향이네요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.24",
          "form": "요즘믄 들어가면",
          "original_form": "요즘믄 들어가면",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029182.1.25",
          "form": "운동 무조건시키고 그런거 아니고",
          "original_form": "운동 무조건시키고 그런거 아니고",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.26",
          "form": "그럼 뭐 시켜요",
          "original_form": "그럼 뭐 시켜요",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.27",
          "form": "하고싶은사람만하는데",
          "original_form": "하고싶은사람만하는데",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.28",
          "form": "폰한다고",
          "original_form": "폰한다고",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.29",
          "form": "다들 안해서",
          "original_form": "다들 안해서",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.30",
          "form": "어 그건 좀 그런데요",
          "original_form": "어 그건 좀 그런데요",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.31",
          "form": "살쪄서나온다고 함",
          "original_form": "살쪄서나온다고 함",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.32",
          "form": "어쨌든 훈련을 하고 나와야 되는데",
          "original_form": "어쨌든 훈련을 하고 나와야 되는데",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.33",
          "form": "라떼는 동기들 군대가면",
          "original_form": "라떼는 동기들 군대가면",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.34",
          "form": "칼각잡고 나오고",
          "original_form": "칼각잡고 나오고",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.35",
          "form": "그리고 오늘 제대 그런 얘기 엄청 하겠네",
          "original_form": "그리고 오늘 제대 그런 얘기 엄청 하겠네",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.36",
          "form": "걸을때 팔올라가고",
          "original_form": "걸을때 팔올라가고",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.37",
          "form": "엄청 그런 라떼는 이런 얘기 많이 하겠네요",
          "original_form": "엄청 그런 라떼는 이런 얘기 많이 하겠네요",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.38",
          "form": "뒷짐지고 있고",
          "original_form": "뒷짐지고 있고",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029182.1.39",
          "form": "개편이겠네요",
          "original_form": "개편이겠네요",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029182.1.40",
          "form": "통화하고싶어서",
          "original_form": "통화하고싶어서",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029182.1.41",
          "form": "수신자부담전화하고",
          "original_form": "수신자부담전화하고",
          "speaker_id": "2",
          "time": "20210903 00:11"
        }
      ]
    }
