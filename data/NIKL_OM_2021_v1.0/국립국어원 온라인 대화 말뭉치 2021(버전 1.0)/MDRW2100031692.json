      "utterance": [
        {
          "id": "MDRW2100031692.1.1",
          "form": "바람 부네요",
          "original_form": "바람 부네요",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031692.1.2",
          "form": "그러게요ㅎ",
          "original_form": "그러게요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031692.1.3",
          "form": "요즘날씨가 추워요ㅎ",
          "original_form": "요즘날씨가 추워요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031692.1.4",
          "form": "오늘은 노 에어컨",
          "original_form": "오늘은 노 에어컨",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031692.1.5",
          "form": "저희는 그냥 틀었어요ㅎ",
          "original_form": "저희는 그냥 틀었어요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031692.1.6",
          "form": "선선해서 좋은데ㅎ 그래도 아이가 맨날뛰놀고 하니깐 땀이 많이 나더라구요 ㅡㅎ",
          "original_form": "선선해서 좋은데ㅎ 그래도 아이가 맨날뛰놀고 하니깐 땀이 많이 나더라구요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031692.1.7",
          "form": "계속비오다 오늘은 굳",
          "original_form": "계속비오다 오늘은 굳",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031692.1.8",
          "form": "맞아요ㅎ 계속 축축했는데ㅎ",
          "original_form": "맞아요ㅎ 계속 축축했는데ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031692.1.9",
          "form": "오늘은 바람도 선선하고ㅎ 애랑 산책가기 좋은 날씨였어요ㅎ",
          "original_form": "오늘은 바람도 선선하고ㅎ 애랑 산책가기 좋은 날씨였어요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031692.1.10",
          "form": "선선한바람 와 너무좋쳐 가을인가",
          "original_form": "선선한바람 와 너무좋쳐 가을인가",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031692.1.11",
          "form": "이제 가을이죠뭐ㅎㅎ",
          "original_form": "이제 가을이죠뭐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031692.1.12",
          "form": "태풍만 안왔으면 좋겟내요",
          "original_form": "태풍만 안왔으면 좋겟내요",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031692.1.13",
          "form": "이제 가을이 시작인가요??",
          "original_form": "이제 가을이 시작인가요??",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031692.1.14",
          "form": "태풍",
          "original_form": "태풍",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031692.1.15",
          "form": "가을태풍이 엄청쎄잖아요ㅎ",
          "original_form": "가을태풍이 엄청쎄잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031692.1.16",
          "form": "근데 올해는 진짜 태풍도 많이 안오긴하네요",
          "original_form": "근데 올해는 진짜 태풍도 많이 안오긴하네요",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031692.1.17",
          "form": "아 가을이구나ㅋ 까먹음ㅋ",
          "original_form": "아 가을이구나ㅋ 까먹음ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100031692.1.18",
          "form": "가을지요ㅎㅎ 이젠ㅎㅎ",
          "original_form": "가을지요ㅎㅎ 이젠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031692.1.19",
          "form": "가을에는 미세먼지도없고ㅎㅎ",
          "original_form": "가을에는 미세먼지도없고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031692.1.20",
          "form": "살찌는계절",
          "original_form": "살찌는계절",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031692.1.21",
          "form": "천고마비",
          "original_form": "천고마비",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031692.1.22",
          "form": "암요ㅎ 알죠ㅎㅎ",
          "original_form": "암요ㅎ 알죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031692.1.23",
          "form": "트렌치 코트 입고 ~ㅎㅎ",
          "original_form": "트렌치 코트 입고 ~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031692.1.24",
          "form": "떡볶이코트ㅋ",
          "original_form": "떡볶이코트ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031692.1.25",
