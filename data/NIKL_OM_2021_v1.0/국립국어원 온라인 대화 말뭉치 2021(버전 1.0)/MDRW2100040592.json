      "utterance": [
        {
          "id": "MDRW2100040592.1.1",
          "form": "저는 가방 팔았었는데 싸게 팔아서 그런가 금방 팔리긴하더라고요",
          "original_form": "저는 가방 팔았었는데 싸게 팔아서 그런가 금방 팔리긴하더라고요",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040592.1.2",
          "form": "네네 저도 가방",
          "original_form": "네네 저도 가방",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040592.1.3",
          "form": "당근도 은근 스트레스에요",
          "original_form": "당근도 은근 스트레스에요",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040592.1.4",
          "form": "산다해놓고 보고는 안사고ㅠㅠ",
          "original_form": "산다해놓고 보고는 안사고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040592.1.5",
          "form": "중고라 해도 새상품급 원하나봐요",
          "original_form": "중고라 해도 새상품급 원하나봐요",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040592.1.6",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "1",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100040592.1.7",
          "form": "제가 귀찮아서 사놓고. 가방을 잘 안들고 다녀서 그런가 거의 새거였거든요",
          "original_form": "제가 귀찮아서 사놓고. 가방을 잘 안들고 다녀서 그런가 거의 새거였거든요",
          "speaker_id": "1",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100040592.1.8",
          "form": "올리자마자 여러명한테 연락와서 신기했었네요",
          "original_form": "올리자마자 여러명한테 연락와서 신기했었네요",
          "speaker_id": "1",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100040592.1.9",
          "form": "연락갑자기 안오면 스트레스 인거같아여",
          "original_form": "연락갑자기 안오면 스트레스 인거같아여",
          "speaker_id": "1",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100040592.1.10",
          "form": "우와 정말요",
          "original_form": "우와 정말요",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040592.1.11",
          "form": "맞아 안오면 스트레스에요 ㅋㅋ",
          "original_form": "맞아 안오면 스트레스에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040592.1.12",
          "form": "살거겉아서 다른사람한테도 사실분있다고 말했는데 갑자기 연락 뜸하고 그러면...어우",
          "original_form": "살거겉아서 다른사람한테도 사실분있다고 말했는데 갑자기 연락 뜸하고 그러면...어우",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100040592.1.13",
          "form": "맞아요 당근도 은근 어려워요",
          "original_form": "맞아요 당근도 은근 어려워요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040592.1.14",
          "form": "차라리 그냥 의류 수거함이 편해요ㅠ",
          "original_form": "차라리 그냥 의류 수거함이 편해요ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040592.1.15",
          "form": "진짜 어려워요ㅠㅠ",
          "original_form": "진짜 어려워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100040592.1.16",
          "form": "저는 고물상? 에 팔아요",
          "original_form": "저는 고물상? 에 팔아요",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100040592.1.17",
          "form": "고물상이 있나요 근처에??",
          "original_form": "고물상이 있나요 근처에??",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100040592.1.18",
          "form": "네네 집근처에 있어서 갖다줘요 ㅎ 많이는 못받아도  당근에 파는건 너무 귀찮더라고요",
          "original_form": "네네 집근처에 있어서 갖다줘요 ㅎ 많이는 못받아도  당근에 파는건 너무 귀찮더라고요",
          "speaker_id": "1",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100040592.1.19",
          "form": "아 맞아요 약속도 잡아야하고",
          "original_form": "아 맞아요 약속도 잡아야하고",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100040592.1.20",
          "form": "맞아여ㅠ",
          "original_form": "맞아여ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100040592.1.21",
          "form": "그게 제일 귀챃죠",
          "original_form": "그게 제일 귀챃죠",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100040592.1.22",
          "form": "시간 맞춰야하고",
          "original_form": "시간 맞춰야하고",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100040592.1.23",
          "form": "그러고 안사가는 사람도있어서 ㅋㅋㅋ",
          "original_form": "그러고 안사가는 사람도있어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100040592.1.24",
          "form": "맞아요 진짜 ㅋㅋㅋㅋ 한참 연락 잘하다가 연락두절되면 얼마나 빡치던지",
          "original_form": "맞아요 진짜 ㅋㅋㅋㅋ 한참 연락 잘하다가 연락두절되면 얼마나 빡치던지",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100040592.1.25",
          "form": "그쵸 ㅋㅋㅋ 당근은 시간과의 싸움이에여",
          "original_form": "그쵸 ㅋㅋㅋ 당근은 시간과의 싸움이에여",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100040592.1.26",
          "form": "중고마켓인데 새상품 급 원한다고 보고 안사간 사람도 있옸어요ㅠㅠ",
          "original_form": "중고마켓인데 새상품 급 원한다고 보고 안사간 사람도 있옸어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100040592.1.27",
          "form": "아... ㅎㅎ 중고인데 새상품을 원하다니",
          "original_form": "아... ㅎㅎ 중고인데 새상품을 원하다니",
          "speaker_id": "1",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100040592.1.28",
          "form": "저도 보면 거의 새거인 상품만 잘 팔린듯요",
          "original_form": "저도 보면 거의 새거인 상품만 잘 팔린듯요",
          "speaker_id": "1",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100040592.1.29",
          "form": "아 그쵸 중고마켓의 의미를 잃었어요",
          "original_form": "아 그쵸 중고마켓의 의미를 잃었어요",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100040592.1.30",
          "form": "ㅋㅋㅋ그니깐요",
          "original_form": "ㅋㅋㅋ그니깐요",
          "speaker_id": "1",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100040592.1.31",
          "form": "요즘 구매하신거 있나여~?",
          "original_form": "요즘 구매하신거 있나여~?",
          "speaker_id": "1",
          "time": "20210907 11:42"
        },
        {
          "id": "MDRW2100040592.1.32",
          "form": "저 당근에서 물결고데기 샀어요",
          "original_form": "저 당근에서 물결고데기 샀어요",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100040592.1.33",
          "form": "저도 새상품샀네여 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 새상품샀네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100040592.1.34",
          "form": "남 뭐라할것도 없어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "남 뭐라할것도 없어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100040592.1.35",
          "form": "새상품을 좀 더 싸게 샀어요 근처에서",
          "original_form": "새상품을 좀 더 싸게 샀어요 근처에서",
          "speaker_id": "2",
          "time": "20210907 12:12"
        }
