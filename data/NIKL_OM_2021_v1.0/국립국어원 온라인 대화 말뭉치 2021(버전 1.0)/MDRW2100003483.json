      "utterance": [
        {
          "id": "MDRW2100003483.1.1",
          "form": "내일은 아침에 일어나서 맛있는거 만들어묵자",
          "original_form": "내일은 아침에 일어나서 맛있는거 만들어묵자",
          "speaker_id": "2",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.2",
          "form": "일어나서 엄마가 해주는 계란 후라이 먹든동",
          "original_form": "일어나서 엄마가 해주는 계란 후라이 먹든동",
          "speaker_id": "2",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.3",
          "form": "그래 동생님이 요리를 함 해봐라",
          "original_form": "그래 동생님이 요리를 함 해봐라",
          "speaker_id": "1",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.4",
          "form": "아니면 미숫가루 만들어 드셈 나는 안무거도된다",
          "original_form": "아니면 미숫가루 만들어 드셈 나는 안무거도된다",
          "speaker_id": "2",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.5",
          "form": "기대하고 있을게",
          "original_form": "기대하고 있을게",
          "speaker_id": "1",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.6",
          "form": "맛난 스파게티를 좀 만들어보게나",
          "original_form": "맛난 스파게티를 좀 만들어보게나",
          "speaker_id": "1",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.7",
          "form": "요리할줄모름 콘프로스트",
          "original_form": "요리할줄모름 콘프로스트",
          "speaker_id": "2",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.8",
          "form": "됐다 그런건",
          "original_form": "됐다 그런건",
          "speaker_id": "1",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.9",
          "form": "말아줄까? 코코볼도 가능함",
          "original_form": "말아줄까? 코코볼도 가능함",
          "speaker_id": "2",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.10",
          "form": "스파게티 요리",
          "original_form": "스파게티 요리",
          "speaker_id": "1",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.11",
          "form": "레시피 검색하면 다 나옴",
          "original_form": "레시피 검색하면 다 나옴",
          "speaker_id": "1",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003483.1.12",
          "form": "집에 코코볼없다",
          "original_form": "집에 코코볼없다",
          "speaker_id": "1",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003483.1.13",
          "form": "아침부터 우에 스파게티를 만드노",
          "original_form": "아침부터 우에 스파게티를 만드노",
          "speaker_id": "2",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003483.1.14",
          "form": "스파게티 소스 다 있음 스파게티 ㄱㄱ",
          "original_form": "스파게티 소스 다 있음 스파게티 ㄱㄱ",
          "speaker_id": "1",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003483.1.15",
          "form": "아침부터 못 먹는게 어딨어",
          "original_form": "아침부터 못 먹는게 어딨어",
          "speaker_id": "1",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003483.1.16",
          "form": "스파게티면까르보나라 고고",
          "original_form": "스파게티면까르보나라 고고",
          "speaker_id": "2",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003483.1.17",
          "form": "그래 재료 다있다",
          "original_form": "그래 재료 다있다",
          "speaker_id": "1",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003483.1.18",
          "form": "구래 아침부터 스파게티에 술도마시자",
          "original_form": "구래 아침부터 스파게티에 술도마시자",
          "speaker_id": "2",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003483.1.19",
          "form": "한번 솜씨를 발휘해보게",
          "original_form": "한번 솜씨를 발휘해보게",
          "speaker_id": "1",
          "time": "20210820 23:39"
        }
      ]
