      "utterance": [
        {
          "id": "MDRW2100033328.1.1",
          "form": "책읽으세요?",
          "original_form": "책읽으세요?",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033328.1.2",
          "form": "지금 책을 읽으려고 초반 몇장만 읽고 내비둔 게 몇 개 있네요 ㅋㅋㅋ",
          "original_form": "지금 책을 읽으려고 초반 몇장만 읽고 내비둔 게 몇 개 있네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033328.1.3",
          "form": "ㅎㅎ어떤장르인가요",
          "original_form": "ㅎㅎ어떤장르인가요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033328.1.4",
          "form": "에세이도 있고 소설도 있어요",
          "original_form": "에세이도 있고 소설도 있어요",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033328.1.5",
          "form": "재밌는장르네요ㅎㅎ국내껀가요",
          "original_form": "재밌는장르네요ㅎㅎ국내껀가요",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100033328.1.6",
          "form": "소설은 재밌긴한데 계속 끊어읽게 되더라구요 ㅋㄱㄱ",
          "original_form": "소설은 재밌긴한데 계속 끊어읽게 되더라구요 ㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100033328.1.7",
          "form": "저는반대롷ㅎㅎ끝까지읽어요",
          "original_form": "저는반대롷ㅎㅎ끝까지읽어요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100033328.1.8",
          "form": "계속생각나더라고요",
          "original_form": "계속생각나더라고요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100033328.1.9",
          "form": "와 그러시구나 저도 원래 그런편이었는데 요즘 집중력이 너무 안좋아져서 그런지 읽다가 끊기더라구요",
          "original_form": "와 그러시구나 저도 원래 그런편이었는데 요즘 집중력이 너무 안좋아져서 그런지 읽다가 끊기더라구요",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100033328.1.10",
          "form": "그럴수있죠ㅎㅎㅎ전그러다포기한적도있어요",
          "original_form": "그럴수있죠ㅎㅎㅎ전그러다포기한적도있어요",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100033328.1.11",
          "form": "ㅋㄱㅋ언젠가 읽겠죠?",
          "original_form": "ㅋㄱㅋ언젠가 읽겠죠?",
          "speaker_id": "2",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100033328.1.12",
          "form": "그져그져ㅎㅎㅎ",
          "original_form": "그져그져ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100033328.1.13",
          "form": "독서는아니지만웹툰봐요..",
          "original_form": "독서는아니지만웹툰봐요..",
          "speaker_id": "1",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100033328.1.14",
          "form": "맞아요 저도 웹툰은 안밀리고 보고 있어요",
          "original_form": "맞아요 저도 웹툰은 안밀리고 보고 있어요",
          "speaker_id": "2",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100033328.1.15",
          "form": "어떤거 보세요??",
          "original_form": "어떤거 보세요??",
          "speaker_id": "2",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100033328.1.16",
          "form": "저네이버웹툰",
          "original_form": "저네이버웹툰",
          "speaker_id": "1",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100033328.1.17",
          "form": "순위높은거다봐요ㅎㅎ",
          "original_form": "순위높은거다봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:23"
        }
