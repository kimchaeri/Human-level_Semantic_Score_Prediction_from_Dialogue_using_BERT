      "utterance": [
        {
          "id": "MDRW2100045052.1.1",
          "form": "우리랑 너무 달라서 신기해",
          "original_form": "우리랑 너무 달라서 신기해",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045052.1.2",
          "form": "연예계 규제도 어찌나 하는지",
          "original_form": "연예계 규제도 어찌나 하는지",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045052.1.3",
          "form": "인기있으면 그 장르 규제함 ㅋ기",
          "original_form": "인기있으면 그 장르 규제함 ㅋ기",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045052.1.4",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045052.1.5",
          "form": "신기하너",
          "original_form": "신기하너",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045052.1.6",
          "form": "웹드도 규제하는데 공중파보단 덜할뿐이라",
          "original_form": "웹드도 규제하는데 공중파보단 덜할뿐이라",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045052.1.7",
          "form": "귀신 나오면안되고",
          "original_form": "귀신 나오면안되고",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045052.1.8",
          "form": "타임슬립금지",
          "original_form": "타임슬립금지",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045052.1.9",
          "form": "그나마 제작해서 보여줄수있음이 다행인가ㅋ",
          "original_form": "그나마 제작해서 보여줄수있음이 다행인가ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045052.1.10",
          "form": "학생들 연애하는거. 나오면안되고",
          "original_form": "학생들 연애하는거. 나오면안되고",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045052.1.11",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045052.1.12",
          "form": "불량학생안됨",
          "original_form": "불량학생안됨",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.13",
          "form": "학생들 연애ㅋ",
          "original_form": "학생들 연애ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.14",
          "form": "현실부점하너",
          "original_form": "현실부점하너",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.15",
          "form": "그래서 중드 청춘물은 다 열공해",
          "original_form": "그래서 중드 청춘물은 다 열공해",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.16",
          "form": "너무 웃김 ㅋㅋㅋㅋ",
          "original_form": "너무 웃김 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.17",
          "form": "죄다 공부함 ㅋㅋ",
          "original_form": "죄다 공부함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.18",
          "form": "와우 청춘물 사기고",
          "original_form": "와우 청춘물 사기고",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.19",
          "form": "둘이 사귀는거같은데",
          "original_form": "둘이 사귀는거같은데",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.20",
          "form": "절대 사귀지않고",
          "original_form": "절대 사귀지않고",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.21",
          "form": "어른되면사겨",
          "original_form": "어른되면사겨",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045052.1.22",
          "form": "아~그런식이구나",
          "original_form": "아~그런식이구나",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045052.1.23",
          "form": "어른되고 갑자기 키쑤해",
          "original_form": "어른되고 갑자기 키쑤해",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045052.1.24",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045052.1.25",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045052.1.26",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045052.1.27",
          "form": "너무 뻔하것다",
          "original_form": "너무 뻔하것다",
