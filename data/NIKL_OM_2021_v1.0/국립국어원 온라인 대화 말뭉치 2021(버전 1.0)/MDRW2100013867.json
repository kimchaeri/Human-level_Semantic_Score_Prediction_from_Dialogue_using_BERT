      "utterance": [
        {
          "id": "MDRW2100013867.1.1",
          "form": "가족과 함께 지내시나봐요",
          "original_form": "가족과 함께 지내시나봐요",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013867.1.2",
          "form": "네 저는 부모님집에 살아요! name1은 주부시랬죠",
          "original_form": "네 저는 부모님집에 살아요! &name1&은 주부시랬죠",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013867.1.3",
          "form": "네 저는 아들 하나있어요",
          "original_form": "네 저는 아들 하나있어요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013867.1.4",
          "form": "청소하고 밥하고 하면 금방 하루가 끝나시죠",
          "original_form": "청소하고 밥하고 하면 금방 하루가 끝나시죠",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013867.1.5",
          "form": "어머나 얼마나 예쁠까요",
          "original_form": "어머나 얼마나 예쁠까요",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013867.1.6",
          "form": "네 맞아요 하는것없이 잘지나가요",
          "original_form": "네 맞아요 하는것없이 잘지나가요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013867.1.7",
          "form": "하루가 얼마나빠른지.. 한번씩은 우울해요 한것없구",
          "original_form": "하루가 얼마나빠른지.. 한번씩은 우울해요 한것없구",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013867.1.8",
          "form": "네 이쁘죠 이제크면...  뭘하고살지..ㅎ",
          "original_form": "네 이쁘죠 이제크면...  뭘하고살지..ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013867.1.9",
          "form": "저도 집에 있어서 그런지 name1과 거의 같은 느낌이에요",
          "original_form": "저도 집에 있어서 그런지 &name1&과 거의 같은 느낌이에요",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013867.1.10",
          "form": "집안일은 티도 안나고 뭐하다보면 금방 하루가 지나가죠",
          "original_form": "집안일은 티도 안나고 뭐하다보면 금방 하루가 지나가죠",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013867.1.11",
          "form": "뭐가 제일 하기 싫으세요?",
          "original_form": "뭐가 제일 하기 싫으세요?",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013867.1.12",
          "form": "아..그렇죠.. 폰만들여다 보게 되구요",
          "original_form": "아..그렇죠.. 폰만들여다 보게 되구요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013867.1.13",
          "form": "전...화장실청소요",
          "original_form": "전...화장실청소요",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013867.1.14",
          "form": "밑반찬하는것도 싫어요 ㅠ",
          "original_form": "밑반찬하는것도 싫어요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013867.1.15",
          "form": "아 설거지 생각하고 있었는데 다 맞아요",
          "original_form": "아 설거지 생각하고 있었는데 다 맞아요",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013867.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013867.1.17",
          "form": "밑반찬 요즘 뭐 해드세요?",
          "original_form": "밑반찬 요즘 뭐 해드세요?",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013867.1.18",
          "form": "네 ㅎ  너무싫어요",
          "original_form": "네 ㅎ  너무싫어요",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013867.1.19",
          "form": "거의메인 위주로 해먹어요",
          "original_form": "거의메인 위주로 해먹어요",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013867.1.20",
          "form": "밑반찬이 손이많이가요 ㅠ",
          "original_form": "밑반찬이 손이많이가요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013867.1.21",
          "form": "밑반찬진짜 귀찮고 그렇다고 잘 먹는것도 아니구 번거롭죠ㅠㅠ",
          "original_form": "밑반찬진짜 귀찮고 그렇다고 잘 먹는것도 아니구 번거롭죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013867.1.22",
          "form": "아기는 몇살이에요?",
          "original_form": "아기는 몇살이에요?",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013867.1.23",
          "form": "11살이요 \r\n아기가 아니네요 ㅎ 4학년입니다",
          "original_form": "11살이요 \r\n아기가 아니네요 ㅎ 4학년입니다",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013867.1.24",
          "form": "20대 후반도 부모님껜 아기인걸요ㅋㅋㅋ 4학년이면 다컸겠네요 같이 집안일 도와주나요?",
          "original_form": "20대 후반도 부모님껜 아기인걸요ㅋㅋㅋ 4학년이면 다컸겠네요 같이 집안일 도와주나요?",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013867.1.25",
          "form": "남자 아이인데\r\n도와주는걸 좋아해요",
          "original_form": "남자 아이인데\r\n도와주는걸 좋아해요",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013867.1.26",
          "form": "아직 아기같은면도 있고 든든할때도 있고 그래요 ㅎ",
          "original_form": "아직 아기같은면도 있고 든든할때도 있고 그래요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013867.1.27",
          "form": "일이 조금 줄어서 좋으시겠어요",
          "original_form": "일이 조금 줄어서 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013867.1.28",
          "form": "확실히 아들이 든든한가봐요",
          "original_form": "확실히 아들이 든든한가봐요",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013867.1.29",
          "form": "저희 엄마도 든든하다고 좋아하시더라구요..",
          "original_form": "저희 엄마도 든든하다고 좋아하시더라구요..",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013867.1.30",
          "form": "저는 조금 서운했지만 ㅋㅋㅋ",
          "original_form": "저는 조금 서운했지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013867.1.31",
          "form": "아 장 단점이 확실하죠\r\n딸은 얼마나 이뻐요^^",
          "original_form": "아 장 단점이 확실하죠\r\n딸은 얼마나 이뻐요^^",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013867.1.32",
          "form": "결혼해서도 주변에 보면 딸이 더잘해요",
          "original_form": "결혼해서도 주변에 보면 딸이 더잘해요",
