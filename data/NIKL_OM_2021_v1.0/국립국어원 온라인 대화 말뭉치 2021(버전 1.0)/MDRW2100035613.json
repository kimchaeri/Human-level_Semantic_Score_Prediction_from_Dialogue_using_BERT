      "utterance": [
        {
          "id": "MDRW2100035613.1.1",
          "form": "그죠ㅋㅋ그분들도 한 학력하시죠",
          "original_form": "그죠ㅋㅋ그분들도 한 학력하시죠",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.2",
          "form": "얼마전까지만해도",
          "original_form": "얼마전까지만해도",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.3",
          "form": "얘기하면 사람들이 놀렸어요ㅠ",
          "original_form": "얘기하면 사람들이 놀렸어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.4",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.5",
          "form": "sg워너비 좋아하는데",
          "original_form": "sg워너비 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.6",
          "form": "노래좋잖아요",
          "original_form": "노래좋잖아요",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.7",
          "form": "다들 한물간 가수라고 놀리더라구요",
          "original_form": "다들 한물간 가수라고 놀리더라구요",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.8",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.9",
          "form": "요즘 워너비 젤잘나가는뎁!",
          "original_form": "요즘 워너비 젤잘나가는뎁!",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035613.1.10",
          "form": "놀면 뭐하니 프로그램에서 나와서 재조명 받기전까지만해도",
          "original_form": "놀면 뭐하니 프로그램에서 나와서 재조명 받기전까지만해도",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.11",
          "form": "사람들이 멀 모르시네",
          "original_form": "사람들이 멀 모르시네",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.12",
          "form": "다들 잊혀진 가수 한물간 가수",
          "original_form": "다들 잊혀진 가수 한물간 가수",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.13",
          "form": "막 이렇게 생각해서 속상하더라구요ㅠㅠ",
          "original_form": "막 이렇게 생각해서 속상하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.14",
          "form": "전 김용준 좋아하는데요!",
          "original_form": "전 김용준 좋아하는데요!",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.15",
          "form": "미성 최고",
          "original_form": "미성 최고",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.16",
          "form": "음반도 꽤 자주 냈는데ㅠㅠ",
          "original_form": "음반도 꽤 자주 냈는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.17",
          "form": "ㅎㅎㅎ 감사해요",
          "original_form": "ㅎㅎㅎ 감사해요",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.18",
          "form": "골프 그만치고 소처럼 앨범 내줬으면..",
          "original_form": "골프 그만치고 소처럼 앨범 내줬으면..",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.19",
          "form": "그러게요ㅎㅎㅎ",
          "original_form": "그러게요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035613.1.20",
          "form": "음반 많이 내줬으면 좋겠어요",
          "original_form": "음반 많이 내줬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035613.1.21",
          "form": "요즘 들을 노래가 떨어지고있어서ㅋㅋㅋ",
          "original_form": "요즘 들을 노래가 떨어지고있어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035613.1.22",
          "form": "이번에 신곡도 좋던데",
          "original_form": "이번에 신곡도 좋던데",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035613.1.23",
          "form": "앗 들어보셨구나!",
          "original_form": "앗 들어보셨구나!",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035613.1.24",
          "form": "노래 좋더라구요 ㅡㅎㅎ",
          "original_form": "노래 좋더라구요 ㅡㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:18"
        }
      ]
    }
  ]
