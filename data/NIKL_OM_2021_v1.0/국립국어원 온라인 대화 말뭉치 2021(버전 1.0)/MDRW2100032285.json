      "utterance": [
        {
          "id": "MDRW2100032285.1.1",
          "form": "오늘 점심은 뭐 먹을래?",
          "original_form": "오늘 점심은 뭐 먹을래?",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032285.1.2",
          "form": "우리 점심 이미 먹었어",
          "original_form": "우리 점심 이미 먹었어",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032285.1.3",
          "form": "사케동 먹었잖아~",
          "original_form": "사케동 먹었잖아~",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032285.1.4",
          "form": "답이 너무 느려요%%%",
          "original_form": "답이 너무 느려요%%%",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032285.1.5",
          "form": "저도 제 일상이 있는걸요",
          "original_form": "저도 제 일상이 있는걸요",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032285.1.6",
          "form": "저녁은 뭐 먹을거야",
          "original_form": "저녁은 뭐 먹을거야",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032285.1.7",
          "form": "양식 한식 중식",
          "original_form": "양식 한식 중식",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032285.1.8",
          "form": "저녁은 생각 좀 해봐야 해",
          "original_form": "저녁은 생각 좀 해봐야 해",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.9",
          "form": "날씨도",
          "original_form": "날씨도",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.10",
          "form": "어두컴컴",
          "original_form": "어두컴컴",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.11",
          "form": "우중충한데",
          "original_form": "우중충한데",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.12",
          "form": "밥이라도 산뜻한 거 먹자",
          "original_form": "밥이라도 산뜻한 거 먹자",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.13",
          "form": "이런 날씨 좋아하잖아 name1이",
          "original_form": "이런 날씨 좋아하잖아 &name1&이",
          "speaker_id": "2",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.14",
          "form": "그래도 날이 좀 시원해져서",
          "original_form": "그래도 날이 좀 시원해져서",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.15",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.16",
          "form": "여름이 다 갔어",
          "original_form": "여름이 다 갔어",
          "speaker_id": "2",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.17",
          "form": "근데 왜 모기가 있지",
          "original_form": "근데 왜 모기가 있지",
          "speaker_id": "2",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032285.1.18",
          "form": "원래",
          "original_form": "원래",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.19",
          "form": "한창 더울 때보다",
          "original_form": "한창 더울 때보다",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.20",
          "form": "날씨 좀 풀이면 많아지잖아",
          "original_form": "날씨 좀 풀이면 많아지잖아",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.21",
          "form": "어젯밤에 엄청 커다란 모기가 내 발을 물었어",
          "original_form": "어젯밤에 엄청 커다란 모기가 내 발을 물었어",
          "speaker_id": "2",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.22",
          "form": "풀리면",
          "original_form": "풀리면",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.23",
          "form": "간지러웠어",
          "original_form": "간지러웠어",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.24",
          "form": "간지러웠어~~~",
          "original_form": "간지러웠어~~~",
          "speaker_id": "2",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.25",
          "form": "버물리 발랐어",
          "original_form": "버물리 발랐어",
          "speaker_id": "2",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032285.1.26",
          "form": "내일은 온도가 어떠려나",
          "original_form": "내일은 온도가 어떠려나",
          "speaker_id": "2",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100032285.1.27",
          "form": "요즘 출퇴근할 때 반팔 입어도 더워?",
          "original_form": "요즘 출퇴근할 때 반팔 입어도 더워?",
          "speaker_id": "2",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100032285.1.28",
          "form": "아침에 계단 오르면 땀나는데",
          "original_form": "아침에 계단 오르면 땀나는데",
          "speaker_id": "1",
          "time": "20210905 18:45"
        },
        {
          "id": "MDRW2100032285.1.29",
          "form": "그냥 걸어다니면 괜찮아",
          "original_form": "그냥 걸어다니면 괜찮아",
          "speaker_id": "1",
          "time": "20210905 18:45"
        },
        {
          "id": "MDRW2100032285.1.30",
          "form": "아아~~ 땀 안 나고 시원한 정도야",
          "original_form": "아아~~ 땀 안 나고 시원한 정도야",
          "speaker_id": "2",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100032285.1.31",
          "form": "응 요즘은 에어컨도 안틀고 있고",
          "original_form": "응 요즘은 에어컨도 안틀고 있고",
          "speaker_id": "1",
          "time": "20210905 23:30"
        },
        {
          "id": "MDRW2100032285.1.32",
          "form": "미세먼지도 없이 날씨도 맑아서",
          "original_form": "미세먼지도 없이 날씨도 맑아서",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100032285.1.33",
          "form": "창문 열어놓고 있는데",
          "original_form": "창문 열어놓고 있는데",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100032285.1.34",
          "form": "창문만 열어도 시원해서 너무 좋아~",
          "original_form": "창문만 열어도 시원해서 너무 좋아~",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100032285.1.35",
          "form": "오옹 이런 날 야외에서 맥주 마셔야하는데",
          "original_form": "오옹 이런 날 야외에서 맥주 마셔야하는데",
          "speaker_id": "2",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032285.1.36",
          "form": "코로나라 아쉽다~~",
          "original_form": "코로나라 아쉽다~~",
          "speaker_id": "2",
          "time": "20210905 23:46"
        }
      ]
