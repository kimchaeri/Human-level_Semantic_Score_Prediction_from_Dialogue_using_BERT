      "utterance": [
        {
          "id": "MDRW2100015505.1.1",
          "form": "유튜브로 요즘 뭐 보세요",
          "original_form": "유튜브로 요즘 뭐 보세요",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015505.1.2",
          "form": "저는잘보진않지만 웃긴거많이봐요",
          "original_form": "저는잘보진않지만 웃긴거많이봐요",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015505.1.3",
          "form": "name1님은뭐봐용",
          "original_form": "&name1&님은뭐봐용",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015505.1.4",
          "form": "저는 차은우 향수 씨에프 봤어요",
          "original_form": "저는 차은우 향수 씨에프 봤어요",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015505.1.5",
          "form": "악ㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015505.1.6",
          "form": "아쿠아향수궁금한데..",
          "original_form": "아쿠아향수궁금한데..",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015505.1.7",
          "form": "차은우향수..흑흑",
          "original_form": "차은우향수..흑흑",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015505.1.8",
          "form": "걸리몀 졸겟다",
          "original_form": "걸리몀 졸겟다",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015505.1.9",
          "form": "올리브영에있대요",
          "original_form": "올리브영에있대요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015505.1.10",
          "form": "오?시향해보고싶네요 차은우향수",
          "original_form": "오?시향해보고싶네요 차은우향수",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015505.1.11",
          "form": "가티가봐요",
          "original_form": "가티가봐요",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015505.1.12",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015505.1.13",
          "form": "요새티비는뭐봐용",
          "original_form": "요새티비는뭐봐용",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015505.1.14",
          "form": "아 요즘 티비 안보네요",
          "original_form": "아 요즘 티비 안보네요",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015505.1.15",
          "form": "아스트로!",
          "original_form": "아스트로!",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015505.1.16",
          "form": "아스트로밖에모르는바보..",
          "original_form": "아스트로밖에모르는바보..",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015505.1.17",
          "form": "아바보입니다",
          "original_form": "아바보입니다",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015505.1.18",
          "form": "아바보ㅋㅋㅋㅋㅋ",
          "original_form": "아바보ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015505.1.19",
          "form": "난잡덕이라",
          "original_form": "난잡덕이라",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015505.1.20",
          "form": "ㄱㅋㅈㄱㅈㄱㅈㅋㅈㄱ",
          "original_form": "ㄱㅋㅈㄱㅈㄱㅈㅋㅈㄱ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015505.1.21",
          "form": "아스트로버블있다구했나여?",
          "original_form": "아스트로버블있다구했나여?",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015505.1.22",
          "form": "유니버스입니다",
          "original_form": "유니버스입니다",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015505.1.23",
          "form": "버블같은건아닐텐데",
          "original_form": "버블같은건아닐텐데",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015505.1.24",
          "form": "사실 잘 몰라요 저는",
          "original_form": "사실 잘 몰라요 저는",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015505.1.25",
          "form": "아 유니버스",
          "original_form": "아 유니버스",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015505.1.26",
          "form": "비슷한거같아요그것도",
          "original_form": "비슷한거같아요그것도",
