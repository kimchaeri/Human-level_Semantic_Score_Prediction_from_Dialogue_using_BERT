      "utterance": [
        {
          "id": "MDRW2100041784.1.1",
          "form": "니가 지웠냐고 하니까 name3이 자기가 지웠다고 장난이야",
          "original_form": "니가 지웠냐고 하니까 &name3&이 자기가 지웠다고 장난이야",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041784.1.2",
          "form": "라고 얘기를 했지",
          "original_form": "라고 얘기를 했지",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041784.1.3",
          "form": "그래서 name4가 울고불고 난리가 났어",
          "original_form": "그래서 &name4&가 울고불고 난리가 났어",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041784.1.4",
          "form": "언니한테 전화해가지고 울고",
          "original_form": "언니한테 전화해가지고 울고",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.5",
          "form": "울면서 집에 갔는데",
          "original_form": "울면서 집에 갔는데",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.6",
          "form": "name4 아빠가 집에 있었지",
          "original_form": "&name4& 아빠가 집에 있었지",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.7",
          "form": "일이 커졌네",
          "original_form": "일이 커졌네",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.8",
          "form": "그런데 name4 아빠도 보통 애들이 싸우면",
          "original_form": "그런데 &name4& 아빠도 보통 애들이 싸우면",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.9",
          "form": "들이 싸우면서 크는 거지 하면서 별로 신경을 안 쓰거든",
          "original_form": "들이 싸우면서 크는 거지 하면서 별로 신경을 안 쓰거든",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.10",
          "form": "근데 게임을 지우고 카톡을 지운 거는 좀 심했다고 생각을 한 거야",
          "original_form": "근데 게임을 지우고 카톡을 지운 거는 좀 심했다고 생각을 한 거야",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.11",
          "form": "우리도 되게 크게 충격을 받았지",
          "original_form": "우리도 되게 크게 충격을 받았지",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041784.1.12",
          "form": "그래서 name4 아빠가 name3한테 왜 지웠냐",
          "original_form": "그래서 &name4& 아빠가 &name3&한테 왜 지웠냐",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.14",
          "form": "그럼 name3 너도 지워라",
          "original_form": "그럼 &name3& 너도 지워라",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.15",
          "form": "하니까 당연히 name3이 싫다고 하지",
          "original_form": "하니까 당연히 &name3&이 싫다고 하지",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.16",
          "form": "그래서 일단 name4 아빠가 우리 신랑한테 전화가 왔어",
          "original_form": "그래서 일단 &name4& 아빠가 우리 신랑한테 전화가 왔어",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.17",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.18",
          "form": "이런이런 일이 있었는데 혼내라고 하는 건 아니고",
          "original_form": "이런이런 일이 있었는데 혼내라고 하는 건 아니고",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.19",
          "form": "알고는 있어야 될 거 같아서 얘기를 한다",
          "original_form": "알고는 있어야 될 거 같아서 얘기를 한다",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.20",
          "form": "조심을 좀 시켜달라",
          "original_form": "조심을 좀 시켜달라",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.21",
          "form": "이미 언니가 알고 있는데",
          "original_form": "이미 언니가 알고 있는데",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.22",
          "form": "그래서 우리가 인제 name3이를 혼내려니까",
          "original_form": "그래서 우리가 인제 &name3&이를 혼내려니까",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041784.1.23",
          "form": "네 네",
