      "utterance": [
        {
          "id": "MDRW2100017617.1.1",
          "form": "유튜브 자주 보세요?",
          "original_form": "유튜브 자주 보세요?",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017617.1.2",
          "form": "네! 삶의질향상을위해 프리미엄가입까지했어요 ㅋㅋ",
          "original_form": "네! 삶의질향상을위해 프리미엄가입까지했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017617.1.3",
          "form": "ㅋㅋ 저두요 한번 무료로 이용했다가 해지했는데 광고 보는 거 답답하더라구요",
          "original_form": "ㅋㅋ 저두요 한번 무료로 이용했다가 해지했는데 광고 보는 거 답답하더라구요",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017617.1.4",
          "form": "주로 어떤 영상을 보세요?",
          "original_form": "주로 어떤 영상을 보세요?",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017617.1.5",
          "form": "그쵸~ 저는 예능 클립 찾아서봐욮ㅋㅋ",
          "original_form": "그쵸~ 저는 예능 클립 찾아서봐욮ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017617.1.6",
          "form": "최근에 라스에 올림픽선수나온편 잼나게봣어용",
          "original_form": "최근에 라스에 올림픽선수나온편 잼나게봣어용",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017617.1.7",
          "form": "그랬군요 또 재밌게 본 클립있나요?",
          "original_form": "그랬군요 또 재밌게 본 클립있나요?",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017617.1.8",
          "form": "음 어젠가 올라온 영국남자영상 배꼽잡으면서봤어요",
          "original_form": "음 어젠가 올라온 영국남자영상 배꼽잡으면서봤어요",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017617.1.9",
          "form": "어떤 내용이었는데요?",
          "original_form": "어떤 내용이었는데요?",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017617.1.10",
          "form": "영국학생들에게 불닭챌린지시키는건데 웃겨요 ㅋㅋ",
          "original_form": "영국학생들에게 불닭챌린지시키는건데 웃겨요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017617.1.11",
          "form": "추천해요!",
          "original_form": "추천해요!",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017617.1.12",
          "form": "아 한번 찾아서 봐봐야겠네요 ㅋㅋㅋ",
          "original_form": "아 한번 찾아서 봐봐야겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017617.1.13",
          "form": "또 추천해줄 만한 유튜버있나요?",
          "original_form": "또 추천해줄 만한 유튜버있나요?",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017617.1.14",
          "form": "음 전요새 웃소 재밌게봐요 ㅋㅋ",
          "original_form": "음 전요새 웃소 재밌게봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017617.1.15",
          "form": "어떤 내용의 영상을 주로 올리는 채널인가요?",
          "original_form": "어떤 내용의 영상을 주로 올리는 채널인가요?",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017617.1.16",
          "form": "그냥 웃긴영상들이요~ 소소잼으로 보기좋아요 ! 추천추천",
          "original_form": "그냥 웃긴영상들이요~ 소소잼으로 보기좋아요 ! 추천추천",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017617.1.17",
          "form": "넵 꼭 봐볼게요!!",
          "original_form": "넵 꼭 봐볼게요!!",
          "speaker_id": "1",
          "time": "20210826 20:38"
        }
