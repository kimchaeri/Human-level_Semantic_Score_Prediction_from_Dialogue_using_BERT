      "utterance": [
        {
          "id": "MDRW2100023493.1.1",
          "form": "아침엔 확실히 샐러드 먹으니까 속이 편해",
          "original_form": "아침엔 확실히 샐러드 먹으니까 속이 편해",
          "speaker_id": "2",
          "time": "20210830 09:20"
        },
        {
          "id": "MDRW2100023493.1.2",
          "form": "글치~ 더 야채 위주니",
          "original_form": "글치~ 더 야채 위주니",
          "speaker_id": "1",
          "time": "20210830 09:20"
        },
        {
          "id": "MDRW2100023493.1.3",
          "form": "과일 야채에 닭고기 올려 먹으니 딱 좋네~ 속도 편하고 ㅎㅎ",
          "original_form": "과일 야채에 닭고기 올려 먹으니 딱 좋네~ 속도 편하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 09:20"
        },
        {
          "id": "MDRW2100023493.1.4",
          "form": "키위랑 바나나도 엄청 달고 맛있더라",
          "original_form": "키위랑 바나나도 엄청 달고 맛있더라",
          "speaker_id": "1",
          "time": "20210830 09:20"
        },
        {
          "id": "MDRW2100023493.1.5",
          "form": "커피까지 마시니 뭔가 힐링되는 기분이야 ㅋㅋ",
          "original_form": "커피까지 마시니 뭔가 힐링되는 기분이야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:21"
        },
        {
          "id": "MDRW2100023493.1.6",
          "form": "ㅎㅎㅎ 사는 것보다 훨씬 비싼 샐러드야",
          "original_form": "ㅎㅎㅎ 사는 것보다 훨씬 비싼 샐러드야",
          "speaker_id": "1",
          "time": "20210830 09:21"
        },
        {
          "id": "MDRW2100023493.1.7",
          "form": "그렇지 들어가는 재료가 많으니",
          "original_form": "그렇지 들어가는 재료가 많으니",
          "speaker_id": "2",
          "time": "20210830 09:21"
        },
        {
          "id": "MDRW2100023493.1.8",
          "form": "키위 바나나 사과 고구마 블루베리 아몬드 닭고기가 재료니 진짜 많이 들어감",
          "original_form": "키위 바나나 사과 고구마 블루베리 아몬드 닭고기가 재료니 진짜 많이 들어감",
          "speaker_id": "1",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100023493.1.9",
          "form": "재료비만 만원 나오겠다 ㅎㅎ",
          "original_form": "재료비만 만원 나오겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023493.1.10",
          "form": "거의 그럴걸~  그래도 넷이 나눠 먹는 거니까",
          "original_form": "거의 그럴걸~  그래도 넷이 나눠 먹는 거니까",
          "speaker_id": "1",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023493.1.11",
          "form": "ㅎㅎㅎ 아침이 건강해서 좋아",
          "original_form": "ㅎㅎㅎ 아침이 건강해서 좋아",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023493.1.12",
          "form": "아침은 건강하게 먹었는데 눈이 그래서 ㅡㅜ",
          "original_form": "아침은 건강하게 먹었는데 눈이 그래서 ㅡㅜ",
          "speaker_id": "1",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023493.1.13",
          "form": "곧 좋아질거야 걱정마",
          "original_form": "곧 좋아질거야 걱정마",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023493.1.14",
          "form": "애들이랑 오늘 점심엔 뭐 먹어?",
          "original_form": "애들이랑 오늘 점심엔 뭐 먹어?",
          "speaker_id": "2",
          "time": "20210830 09:42"
        }
