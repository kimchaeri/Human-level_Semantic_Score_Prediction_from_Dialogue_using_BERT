      "utterance": [
        {
          "id": "MDRW2100024252.1.1",
          "form": "안녕하세요 여행 좋아하세요?",
          "original_form": "안녕하세요 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024252.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024252.1.3",
          "form": "여름휴가는 다녀오셨어요?",
          "original_form": "여름휴가는 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024252.1.4",
          "form": "이번엔 그냥 수영장(?) 2회요 애들이닌ㆍㅇ",
          "original_form": "이번엔 그냥 수영장(?) 2회요 애들이닌ㆍㅇ",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024252.1.5",
          "form": "아무래도 코시국 때문에 여행은 힘드네요^^",
          "original_form": "아무래도 코시국 때문에 여행은 힘드네요^^",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024252.1.6",
          "form": "그렇죠 필리핀가고싶다",
          "original_form": "그렇죠 필리핀가고싶다",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024252.1.7",
          "form": "필리핀 가보신적 있나봐요",
          "original_form": "필리핀 가보신적 있나봐요",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024252.1.8",
          "form": "네 혼자도가보고\n애들이랑도가보고",
          "original_form": "네 혼자도가보고\n애들이랑도가보고",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024252.1.9",
          "form": "골프나 스쿠버다이빙 좋아하시는분들이 주로 가던데 취미가 그쪽이신가보네요",
          "original_form": "골프나 스쿠버다이빙 좋아하시는분들이 주로 가던데 취미가 그쪽이신가보네요",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024252.1.10",
          "form": "아뇨^^그냥 풀빌라가 애들이랑가기좋아서요",
          "original_form": "아뇨^^그냥 풀빌라가 애들이랑가기좋아서요",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024252.1.11",
          "form": "국내풀빌라 가격이나 동남아 풀빌라 가격이나 비슷하긴하더라고요ㅎ 이번 여름 휴가때 저는 풀빌라로 갔었거든요",
          "original_form": "국내풀빌라 가격이나 동남아 풀빌라 가격이나 비슷하긴하더라고요ㅎ 이번 여름 휴가때 저는 풀빌라로 갔었거든요",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024252.1.12",
          "form": "아 국내요?",
          "original_form": "아 국내요?",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024252.1.13",
          "form": "가족여행이신가요?",
          "original_form": "가족여행이신가요?",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024252.1.14",
          "form": "네 가까운곳 검색해서 갔다왔어요ㅎㅎ",
          "original_form": "네 가까운곳 검색해서 갔다왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024252.1.15",
          "form": "가족끼리요(?) 부럽다",
          "original_form": "가족끼리요(?) 부럽다",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024252.1.16",
          "form": "내일또이야기해요~늦어서 오늘은..",
          "original_form": "내일또이야기해요~늦어서 오늘은..",
          "speaker_id": "2",
