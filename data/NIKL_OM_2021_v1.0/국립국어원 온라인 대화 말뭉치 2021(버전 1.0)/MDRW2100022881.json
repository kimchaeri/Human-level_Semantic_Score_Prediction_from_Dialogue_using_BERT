        },
        {
          "id": "MDRW2100022881.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022881.1.3",
          "form": "드셧나요?",
          "original_form": "드셧나요?",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022881.1.4",
          "form": "네 방금 먹었어요",
          "original_form": "네 방금 먹었어요",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022881.1.5",
          "form": "아하 뭐드셨어요?",
          "original_form": "아하 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022881.1.6",
          "form": "케이크요!",
          "original_form": "케이크요!",
          "speaker_id": "1",
