      "utterance": [
        {
          "id": "MDRW2100001818.1.1",
          "form": "달달한 카라멜 마끼야토 먹고싶네요",
          "original_form": "달달한 카라멜 마끼야토 먹고싶네요",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001818.1.2",
          "form": "달달한거 좋아하세요? 저도 무조건 시럽들어간거 좋아해요",
          "original_form": "달달한거 좋아하세요? 저도 무조건 시럽들어간거 좋아해요",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001818.1.3",
          "form": "커피맛을 몰라서 무조건 달달한거 좋아해요",
          "original_form": "커피맛을 몰라서 무조건 달달한거 좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001818.1.4",
          "form": "오늘 점심에 샤브샤브먹고 스벅가서 더블에스프레쏘 프라푸치노 먹었어요ㅎ",
          "original_form": "오늘 점심에 샤브샤브먹고 스벅가서 더블에스프레쏘 프라푸치노 먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001818.1.5",
          "form": "프라푸치노도 맛있죠 달달한게",
          "original_form": "프라푸치노도 맛있죠 달달한게",
          "speaker_id": "2",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001818.1.6",
          "form": "별쿠폰있어서 자바칩 추가왕창해서 먹었죠ㅎㅎ",
          "original_form": "별쿠폰있어서 자바칩 추가왕창해서 먹었죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001818.1.7",
          "form": "ㅎㅎ엄청 달았겠어요",
          "original_form": "ㅎㅎ엄청 달았겠어요",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001818.1.8",
          "form": "맛있던데요ㅎㅎ 요즘근데 블론드 바닐라 더블샷 마끼아또 그거에 빠졌어요 추천요",
          "original_form": "맛있던데요ㅎㅎ 요즘근데 블론드 바닐라 더블샷 마끼아또 그거에 빠졌어요 추천요",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001818.1.9",
          "form": "그래요 계속 새제품이 나오는데 먹던거만 먹으니",
          "original_form": "그래요 계속 새제품이 나오는데 먹던거만 먹으니",
          "speaker_id": "2",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001818.1.10",
          "form": "8300원짜리도 있던데요 ㅎ",
          "original_form": "8300원짜리도 있던데요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001818.1.11",
          "form": "밥값보다 비싸서 못사먹겠더라구요",
          "original_form": "밥값보다 비싸서 못사먹겠더라구요",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001818.1.12",
          "form": "헉 8300원이면 너무 비싼듯하네요",
          "original_form": "헉 8300원이면 너무 비싼듯하네요",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001818.1.13",
          "form": "리유저블 컵을주나봐요 청포도맛도있고 세가지 음료있던데 많이들 먹더라구요",
          "original_form": "리유저블 컵을주나봐요 청포도맛도있고 세가지 음료있던데 많이들 먹더라구요",
          "speaker_id": "1",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001818.1.14",
          "form": "청포도 맛있게요 산뜻하고",
          "original_form": "청포도 맛있게요 산뜻하고",
          "speaker_id": "2",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001818.1.15",
          "form": "함 사먹어볼까 하다가 패스했어요 넘 비싸서",
          "original_form": "함 사먹어볼까 하다가 패스했어요 넘 비싸서",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001818.1.16",
          "form": "너무 비싸서 쉽게 못마실듯해요",
          "original_form": "너무 비싸서 쉽게 못마실듯해요",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001818.1.17",
          "form": "저도요ㅎ커피 얘기하니 또 먹고싶네요",
          "original_form": "저도요ㅎ커피 얘기하니 또 먹고싶네요",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001818.1.18",
          "form": "전 아메리카노 물 희석 많이 해서 마셨어요",
          "original_form": "전 아메리카노 물 희석 많이 해서 마셨어요",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001818.1.19",
          "form": "아메리카노는 전 맛없더라구요ㅎ 무조건 달달해야",
          "original_form": "아메리카노는 전 맛없더라구요ㅎ 무조건 달달해야",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001818.1.20",
          "form": "콜라대신 느낌함 없앨려고 마셨어요",
          "original_form": "콜라대신 느낌함 없앨려고 마셨어요",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001818.1.21",
          "form": "다들 아메마심 그얘기하더라구요 텁텁하지않아서",
          "original_form": "다들 아메마심 그얘기하더라구요 텁텁하지않아서",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001818.1.22",
          "form": "그 맛에 먹는거 같아요 갈증도 안나고",
          "original_form": "그 맛에 먹는거 같아요 갈증도 안나고",
          "speaker_id": "2",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001818.1.23",
          "form": "전 물자체를 잘안마셔서 잘 안넘어가요ㅋ",
          "original_form": "전 물자체를 잘안마셔서 잘 안넘어가요ㅋ",
          "speaker_id": "1",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001818.1.24",
          "form": "음료대신 물을 충분히 마셔야되는데~",
          "original_form": "음료대신 물을 충분히 마셔야되는데~",
          "speaker_id": "2",
          "time": "20210819 21:47"
        }
      ]
    }
