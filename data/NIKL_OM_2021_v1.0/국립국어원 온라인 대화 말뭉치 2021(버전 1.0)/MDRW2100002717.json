      "utterance": [
        {
          "id": "MDRW2100002717.1.1",
          "form": "해외여행 가신다면 어디 가보고싶으세요?",
          "original_form": "해외여행 가신다면 어디 가보고싶으세요?",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002717.1.2",
          "form": "휴양지요",
          "original_form": "휴양지요",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002717.1.3",
          "form": "하와이 괌 이런",
          "original_form": "하와이 괌 이런",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002717.1.4",
          "form": "저두 다음 여행지 계획에 하와이예요",
          "original_form": "저두 다음 여행지 계획에 하와이예요",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002717.1.5",
          "form": "티비에서 보면 너무좋아보여서요",
          "original_form": "티비에서 보면 너무좋아보여서요",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002717.1.6",
          "form": "꼭 가보고싶어요",
          "original_form": "꼭 가보고싶어요",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002717.1.7",
          "form": "저두요 그냥 바다만 보고 와도 좋을거 같아요",
          "original_form": "저두요 그냥 바다만 보고 와도 좋을거 같아요",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002717.1.8",
          "form": "네 보면 해변에서 누워서 석양보는 그런거 많잖아요  해보고싶네요",
          "original_form": "네 보면 해변에서 누워서 석양보는 그런거 많잖아요  해보고싶네요",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002717.1.9",
          "form": "인생사진이겠어요",
          "original_form": "인생사진이겠어요",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002717.1.10",
          "form": "언제나 코로나 끝나서 갈수 있을지",
          "original_form": "언제나 코로나 끝나서 갈수 있을지",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002717.1.11",
          "form": "몇년동안 힘들듯 해요 인종차별 범죄가 많아서",
          "original_form": "몇년동안 힘들듯 해요 인종차별 범죄가 많아서",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002717.1.12",
          "form": "그것도 그렇네요 해외여행은 조심할게 많네요",
          "original_form": "그것도 그렇네요 해외여행은 조심할게 많네요",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002717.1.13",
          "form": "소매치기도 많고요",
          "original_form": "소매치기도 많고요",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002717.1.14",
          "form": "맞아요 그런것도 조심스럽긴 해요",
          "original_form": "맞아요 그런것도 조심스럽긴 해요",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002717.1.15",
          "form": "거기다 언어소통이 힘드니 곤란한 일 생겼을때 난처하고요",
          "original_form": "거기다 언어소통이 힘드니 곤란한 일 생겼을때 난처하고요",
          "speaker_id": "1",
          "time": "20210820 15:17"
        }
