      "utterance": [
        {
          "id": "MDRW2100030851.1.1",
          "form": "샤핑! 백화점 가고싶네요",
          "original_form": "샤핑! 백화점 가고싶네요",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030851.1.2",
          "form": "ㅎㅎ근처백화점 어떤거 있으세요?",
          "original_form": "ㅎㅎ근처백화점 어떤거 있으세요?",
          "speaker_id": "1",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100030851.1.3",
          "form": "현대백화점 있어요",
          "original_form": "현대백화점 있어요",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100030851.1.4",
          "form": "합정이라 여의도 더현대",
          "original_form": "합정이라 여의도 더현대",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100030851.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100030851.1.6",
          "form": "거기핫하죠",
          "original_form": "거기핫하죠",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100030851.1.7",
          "form": "맛집이많다는소문만들었어요",
          "original_form": "맛집이많다는소문만들었어요",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100030851.1.8",
          "form": "안가봤는데 줄서서 입장한데요 사람이 넘많아서",
          "original_form": "안가봤는데 줄서서 입장한데요 사람이 넘많아서",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100030851.1.9",
          "form": "와...줄서서",
          "original_form": "와...줄서서",
          "speaker_id": "1",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100030851.1.10",
          "form": "최근에쇼핑한목록은무엇인가요?",
          "original_form": "최근에쇼핑한목록은무엇인가요?",
          "speaker_id": "1",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100030851.1.11",
          "form": "음 전 책을샀어요",
          "original_form": "음 전 책을샀어요",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100030851.1.12",
          "form": "강방천의 관점",
          "original_form": "강방천의 관점",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100030851.1.13",
          "form": "오 에세인가요?",
          "original_form": "오 에세인가요?",
          "speaker_id": "1",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100030851.1.14",
          "form": "주식책이에요..ㅋㅋ",
          "original_form": "주식책이에요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100030851.1.15",
          "form": "ㅋㅋㅋㅋ오",
          "original_form": "ㅋㅋㅋㅋ오",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100030851.1.16",
          "form": "주식",
          "original_form": "주식",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100030851.1.17",
          "form": "주식하세요?",
          "original_form": "주식하세요?",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100030851.1.18",
          "form": "네 한지 한달됬어요",
          "original_form": "네 한지 한달됬어요",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100030851.1.19",
          "form": "수익 조금씩 보고있네요",
          "original_form": "수익 조금씩 보고있네요",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100030851.1.20",
          "form": "재테크해요?",
          "original_form": "재테크해요?",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100030851.1.21",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100030851.1.22",
          "form": "음 쪼끔해요ㅎㅎ주식살짝",
          "original_form": "음 쪼끔해요ㅎㅎ주식살짝",
