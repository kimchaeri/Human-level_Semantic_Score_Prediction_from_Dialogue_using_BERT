      "utterance": [
        {
          "id": "MDRW2100007339.1.1",
          "form": "완전 공감해요",
          "original_form": "완전 공감해요",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007339.1.2",
          "form": "생각난 김에 나중에 꼬꼬무 다른편 더 찾아봐야겠어요",
          "original_form": "생각난 김에 나중에 꼬꼬무 다른편 더 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007339.1.3",
          "form": "동반 oo 이라는거에대해서도 다룬적있었거든요",
          "original_form": "동반 oo 이라는거에대해서도 다룬적있었거든요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007339.1.4",
          "form": "그것도 의미있는 프로그램이였어요",
          "original_form": "그것도 의미있는 프로그램이였어요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007339.1.5",
          "form": "아 이거 단어가 안 보내지네요",
          "original_form": "아 이거 단어가 안 보내지네요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007339.1.6",
          "form": "검색해보고 왔어요!!",
          "original_form": "검색해보고 왔어요!!",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007339.1.7",
          "form": "대충 글 읽어봤는데...이것도 꼭 찾아봐야겠어요",
          "original_form": "대충 글 읽어봤는데...이것도 꼭 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007339.1.8",
          "form": "네 저도 그래서 저렇게 땡땡으로 ㅎㅎ",
          "original_form": "네 저도 그래서 저렇게 땡땡으로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007339.1.9",
          "form": "눈물없이는 볼수없어요 ㅜㅠ",
          "original_form": "눈물없이는 볼수없어요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007339.1.10",
          "form": "헉 내용이 많이 슬픈가봐요ㅠㅠ",
          "original_form": "헉 내용이 많이 슬픈가봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007339.1.11",
          "form": "네 슬퍼요\r\n아이 이야기 아이입장에서 생각하면",
          "original_form": "네 슬퍼요\r\n아이 이야기 아이입장에서 생각하면",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007339.1.12",
          "form": "너무가슴아프고 나도 착각하고있었구나 싶더라구요",
          "original_form": "너무가슴아프고 나도 착각하고있었구나 싶더라구요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007339.1.13",
          "form": "아차 싶어요",
          "original_form": "아차 싶어요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007339.1.14",
          "form": "진짜 너무 충격적이고 슬프네요ㅠㅠ",
          "original_form": "진짜 너무 충격적이고 슬프네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007339.1.15",
          "form": "어떻게 저런 일이.. ㅠㅠ",
          "original_form": "어떻게 저런 일이.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007339.1.16",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007339.1.17",
          "form": "아이입장에서 생각해본사람은 진짜 드물지싶어요",
          "original_form": "아이입장에서 생각해본사람은 진짜 드물지싶어요",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007339.1.18",
          "form": "꼬꼬무보면 기도안차는것들 진짜많아요",
          "original_form": "꼬꼬무보면 기도안차는것들 진짜많아요",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007339.1.19",
          "form": "그쵸 저는 아직 삼풍백화점 편만 봤지만",
          "original_form": "그쵸 저는 아직 삼풍백화점 편만 봤지만",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007339.1.20",
          "form": "그거도 진짜 충격적이었어요...",
          "original_form": "그거도 진짜 충격적이었어요...",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007339.1.21",
          "form": "휴거도 있어요 ㅎ",
          "original_form": "휴거도 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007339.1.22",
          "form": "휴거가 휴먼시아 거지?그건가요?",
          "original_form": "휴거가 휴먼시아 거지?그건가요?",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007339.1.23",
          "form": "ㅋㅋㅋ 아뇨 지구종말론자요",
          "original_form": "ㅋㅋㅋ 아뇨 지구종말론자요",
