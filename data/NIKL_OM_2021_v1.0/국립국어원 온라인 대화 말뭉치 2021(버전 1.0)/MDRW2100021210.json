      "utterance": [
        {
          "id": "MDRW2100021210.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021210.1.2",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021210.1.3",
          "form": "아니요. 키우세요?",
          "original_form": "아니요. 키우세요?",
          "speaker_id": "2",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021210.1.4",
          "form": "고양이 키우다 지금은 안키워요.",
          "original_form": "고양이 키우다 지금은 안키워요.",
          "speaker_id": "1",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021210.1.5",
          "form": "고양이 너무 좋은데 남편 반대로 언니집을 보냈어요",
          "original_form": "고양이 너무 좋은데 남편 반대로 언니집을 보냈어요",
          "speaker_id": "1",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021210.1.6",
          "form": "아 안타깝네요",
          "original_form": "아 안타깝네요",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021210.1.7",
          "form": "장모종 키웠더니 털날림이 너무 심해 남편 퇴직하면 다시 키워보려구요.",
          "original_form": "장모종 키웠더니 털날림이 너무 심해 남편 퇴직하면 다시 키워보려구요.",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021210.1.8",
          "form": "근데 제 친구도 키우는데 털이 엄청 나드라구요",
          "original_form": "근데 제 친구도 키우는데 털이 엄청 나드라구요",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021210.1.9",
          "form": "그리고 돈도 많이 들지 않나요?",
          "original_form": "그리고 돈도 많이 들지 않나요?",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021210.1.10",
          "form": "병원비가 좀 나가긴해요.",
          "original_form": "병원비가 좀 나가긴해요.",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021210.1.11",
          "form": "웃기지만 고양이 병원비 하려고 알바했었죠 ㅎㅎ",
          "original_form": "웃기지만 고양이 병원비 하려고 알바했었죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021210.1.12",
          "form": "사료값.간식비.병원비",
          "original_form": "사료값.간식비.병원비",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021210.1.13",
          "form": "그쵸?  방광염도 걸리고 그러던데",
          "original_form": "그쵸?  방광염도 걸리고 그러던데",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021210.1.14",
          "form": "맞아요. 반려동물은 아이하나 키우는 비용 들기때문에 신중하게 생각하고 들여야 하는거 같아요",
          "original_form": "맞아요. 반려동물은 아이하나 키우는 비용 들기때문에 신중하게 생각하고 들여야 하는거 같아요",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021210.1.15",
          "form": "제 직장동료도 고양이 때문에 적금도 들더라구요",
          "original_form": "제 직장동료도 고양이 때문에 적금도 들더라구요",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021210.1.16",
          "form": "그런데 너무 사랑스러워서 꼭 다시 키우려구요",
          "original_form": "그런데 너무 사랑스러워서 꼭 다시 키우려구요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021210.1.17",
          "form": "그럴거 같아요.",
          "original_form": "그럴거 같아요.",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021210.1.18",
          "form": "ㅎㅎ 맞아요. 급하게 몫돈 들어갈때 있어요",
          "original_form": "ㅎㅎ 맞아요. 급하게 몫돈 들어갈때 있어요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        }
      ]
