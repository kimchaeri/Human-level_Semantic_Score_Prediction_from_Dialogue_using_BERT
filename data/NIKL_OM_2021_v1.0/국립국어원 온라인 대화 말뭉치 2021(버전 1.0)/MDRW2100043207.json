      "utterance": [
        {
          "id": "MDRW2100043207.1.1",
          "form": "튀긴거진짜맛있어",
          "original_form": "튀긴거진짜맛있어",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043207.1.2",
          "form": "아까 근데 우리애기했던거 다 기름지고 살찌는거잖아요",
          "original_form": "아까 근데 우리애기했던거 다 기름지고 살찌는거잖아요",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043207.1.3",
          "form": "못참아",
          "original_form": "못참아",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043207.1.4",
          "form": "맞아 근데 튀김은 사실 나는 느끼해서잘못먹어",
          "original_form": "맞아 근데 튀김은 사실 나는 느끼해서잘못먹어",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043207.1.5",
          "form": "야채도 튀겨먹는민족이라 이거에요",
          "original_form": "야채도 튀겨먹는민족이라 이거에요",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043207.1.6",
          "form": "곁들이는 음식이지",
          "original_form": "곁들이는 음식이지",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043207.1.7",
          "form": "계란튀김먹고싶다",
          "original_form": "계란튀김먹고싶다",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043207.1.8",
          "form": "나도 메인은 치킨만 가능",
          "original_form": "나도 메인은 치킨만 가능",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043207.1.9",
          "form": "야 나 츠케멘 너무 먹고싶다",
          "original_form": "야 나 츠케멘 너무 먹고싶다",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043207.1.10",
          "form": "나도.. 청주에서도먹었는데 그맛은 못따라가",
          "original_form": "나도.. 청주에서도먹었는데 그맛은 못따라가",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
