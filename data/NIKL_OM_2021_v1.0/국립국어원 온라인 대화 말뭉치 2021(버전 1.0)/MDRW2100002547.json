      "utterance": [
        {
          "id": "MDRW2100002547.1.1",
          "form": "운동이요?",
          "original_form": "운동이요?",
          "speaker_id": "1",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002547.1.2",
          "form": "음 운동..을 사실 좋아하지않아서.. 숨쉬기운동..ㅎ",
          "original_form": "음 운동..을 사실 좋아하지않아서.. 숨쉬기운동..ㅎ",
          "speaker_id": "1",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002547.1.3",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002547.1.4",
          "form": "저도 제가하는 유일한 운동은 숨쉬기운동...",
          "original_form": "저도 제가하는 유일한 운동은 숨쉬기운동...",
          "speaker_id": "2",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002547.1.5",
          "form": "앗 통했네요ㅋㅋㅋㅋㅋ",
          "original_form": "앗 통했네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002547.1.6",
          "form": "움직이는게 좀.. 아 힘들더라구요",
          "original_form": "움직이는게 좀.. 아 힘들더라구요",
          "speaker_id": "1",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002547.1.7",
          "form": "근데 해야한다고 하잖아요..",
          "original_form": "근데 해야한다고 하잖아요..",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002547.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002547.1.9",
          "form": "운동하는분들 너무 대단한듯해여",
          "original_form": "운동하는분들 너무 대단한듯해여",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002547.1.10",
          "form": "가끔 보면 몸 만들고 이러는거 대단하다 하는데",
          "original_form": "가끔 보면 몸 만들고 이러는거 대단하다 하는데",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002547.1.11",
          "form": "그게 제가되고싶진않아욬ㅋㅋㅋ 넘 힘들어보여서",
          "original_form": "그게 제가되고싶진않아욬ㅋㅋㅋ 넘 힘들어보여서",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002547.1.12",
          "form": "특히 식이까지 하는분들",
          "original_form": "특히 식이까지 하는분들",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002547.1.13",
          "form": "인간의 경지를 뛰어넘은듯...",
          "original_form": "인간의 경지를 뛰어넘은듯...",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.14",
          "form": "식이 어찌 포기하지",
          "original_form": "식이 어찌 포기하지",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.15",
          "form": "전 절대 포기가안되서 평생 불가능할거같은..",
          "original_form": "전 절대 포기가안되서 평생 불가능할거같은..",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.16",
          "form": "인생의 즐거움인데",
          "original_form": "인생의 즐거움인데",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.17",
          "form": "먹는것도 제대로 못먹느면",
          "original_form": "먹는것도 제대로 못먹느면",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.18",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.19",
          "form": "뭔 낙으로살어",
          "original_form": "뭔 낙으로살어",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.20",
          "form": "사는 재미가 없을듯해서 엄두가 안나여ㅋㅋ",
          "original_form": "사는 재미가 없을듯해서 엄두가 안나여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002547.1.21",
          "form": "우리는 운동과 거리가 먼 걸로..ㅎ",
          "original_form": "우리는 운동과 거리가 먼 걸로..ㅎ",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002547.1.22",
          "form": "숨쉬기 운동 앞으로도 열심히합시다...ㅎ",
          "original_form": "숨쉬기 운동 앞으로도 열심히합시다...ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002547.1.23",
          "form": "아주 좋은 생각입니다ㅎ",
          "original_form": "아주 좋은 생각입니다ㅎ",
          "speaker_id": "1",
          "time": "20210820 13:33"
        }
      ]
