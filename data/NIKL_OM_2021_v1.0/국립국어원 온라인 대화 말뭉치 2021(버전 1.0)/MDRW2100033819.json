      "utterance": [
        {
          "id": "MDRW2100033819.1.1",
          "form": "일하고 계시나요???",
          "original_form": "일하고 계시나요???",
          "speaker_id": "2",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100033819.1.2",
          "form": "네ㅠ name2님은요?",
          "original_form": "네ㅠ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100033819.1.3",
          "form": "네 저두요 ㅠㅠ",
          "original_form": "네 저두요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100033819.1.4",
          "form": "고생하시네요ㅠㅠ",
          "original_form": "고생하시네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033819.1.5",
          "form": "오늘 업무 많이 힘드셨나요",
          "original_form": "오늘 업무 많이 힘드셨나요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033819.1.6",
          "form": "퇴사하고 싶을 뿐이에요 ~~~",
          "original_form": "퇴사하고 싶을 뿐이에요 ~~~",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033819.1.7",
          "form": "네 그냥저냥요",
          "original_form": "네 그냥저냥요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033819.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033819.1.9",
          "form": "저는 재택중인데 그냥저냥...그렇죠",
          "original_form": "저는 재택중인데 그냥저냥...그렇죠",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100033819.1.10",
          "form": "오늘은 평소보다 일이 좀많았어요",
          "original_form": "오늘은 평소보다 일이 좀많았어요",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100033819.1.11",
          "form": "아하 저는 회사가요!!",
          "original_form": "아하 저는 회사가요!!",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033819.1.12",
          "form": "재택하시니 부럽네요",
          "original_form": "재택하시니 부럽네요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033819.1.13",
          "form": "아침잠이 많아서 재택이 좋긴해요",
          "original_form": "아침잠이 많아서 재택이 좋긴해요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033819.1.14",
          "form": "저는 이직 준비중인데",
          "original_form": "저는 이직 준비중인데",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033819.1.15",
          "form": "name2님은 이직 준비하시나요?",
          "original_form": "&name2&님은 이직 준비하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033819.1.16",
          "form": "전 그냥 퇴사 생각이여!!!!",
          "original_form": "전 그냥 퇴사 생각이여!!!!",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100033819.1.17",
          "form": "어느 쪽으로 이직하려구요?",
          "original_form": "어느 쪽으로 이직하려구요?",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100033819.1.18",
          "form": "데이터쪽으로 이직하려구요 공부하고있어요",
          "original_form": "데이터쪽으로 이직하려구요 공부하고있어요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100033819.1.19",
          "form": "오 멋있네요 !!!",
          "original_form": "오 멋있네요 !!!",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100033819.1.20",
          "form": "저도 뭔가를 해야하지만",
          "original_form": "저도 뭔가를 해야하지만",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100033819.1.21",
          "form": "하기싫네요 흑",
          "original_form": "하기싫네요 흑",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100033819.1.22",
          "form": "안쉽죠ㅠㅠ 일다니시는것만해도",
          "original_form": "안쉽죠ㅠㅠ 일다니시는것만해도",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100033819.1.23",
          "form": "엄청성실하신건데",
          "original_form": "엄청성실하신건데",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100033819.1.24",
          "form": "더시간내고 에너지내기가...너무 힘들죠",
          "original_form": "더시간내고 에너지내기가...너무 힘들죠",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100033819.1.25",
          "form": "그렇죠 ㅠㅠㅠㅠㅠ",
          "original_form": "그렇죠 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
