      "utterance": [
        {
          "id": "MDRW2100017635.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017635.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017635.1.3",
          "form": "최근에 본 영화가 있으신가요?",
          "original_form": "최근에 본 영화가 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017635.1.4",
          "form": "저는 최근에 왓챠로 일주일간 친구 잼나게봤어요 ㅎㅎ",
          "original_form": "저는 최근에 왓챠로 일주일간 친구 잼나게봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017635.1.5",
          "form": "뭔지 몰라서 검색해보고 왔어요!",
          "original_form": "뭔지 몰라서 검색해보고 왔어요!",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017635.1.6",
          "form": "일주일마다 기억이 리셋된다니 살기 힘들 거 같아요ㅠㅠㅋㅋㅋ",
          "original_form": "일주일마다 기억이 리셋된다니 살기 힘들 거 같아요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017635.1.7",
          "form": "맞아요.. 눈물콧물빼면서 영화봣어요 ㅜㅜ근데 추천해요! 배우들 외모도 상당히 빼어나서 볼맛나요 ㅎㅎ",
          "original_form": "맞아요.. 눈물콧물빼면서 영화봣어요 ㅜㅜ근데 추천해요! 배우들 외모도 상당히 빼어나서 볼맛나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017635.1.8",
          "form": "헉 많이 슬픈 영환가요?? 혹시 막 누가 죽거나 하진 않는지ㅠㅠ",
          "original_form": "헉 많이 슬픈 영환가요?? 혹시 막 누가 죽거나 하진 않는지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017635.1.9",
          "form": "아뇨 ㅋㅋㅋ 내용이 감동적이라 슬펐어요ㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋ 내용이 감동적이라 슬펐어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017635.1.10",
          "form": "죽지않아요~",
          "original_form": "죽지않아요~",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017635.1.11",
          "form": "다행이네요ㅋㅋㅋㅋ",
          "original_form": "다행이네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017635.1.12",
          "form": "저 어릴 때 병으로 죽는 로맨스를 너무 많이 봐가지고ㅋㅋㅋ",
          "original_form": "저 어릴 때 병으로 죽는 로맨스를 너무 많이 봐가지고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017635.1.13",
          "form": "이제 더는 보고 싶지 않았습니다...",
          "original_form": "이제 더는 보고 싶지 않았습니다...",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017635.1.14",
          "form": "왓챠에서 또 재밌게 보신 영화는 없으세요?",
          "original_form": "왓챠에서 또 재밌게 보신 영화는 없으세요?",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017635.1.15",
          "form": "아 새드앤딩응 저두 싫어요 ㅜㅜ",
          "original_form": "아 새드앤딩응 저두 싫어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017635.1.16",
          "form": "아 저는 헝거게임도 얼마전에 몰아서봣어요ㅋㅋ",
          "original_form": "아 저는 헝거게임도 얼마전에 몰아서봣어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017635.1.17",
          "form": "앗 헝거게임! 저도 재밌게 봤던 영화예요.",
          "original_form": "앗 헝거게임! 저도 재밌게 봤던 영화예요.",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017635.1.18",
          "form": "저는 영화관에서 봤어요ㅋㅋㅋ",
          "original_form": "저는 영화관에서 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017635.1.19",
          "form": "해피엔딩이긴 했는데 여운이 많이 남는 영화였어요.",
          "original_form": "해피엔딩이긴 했는데 여운이 많이 남는 영화였어요.",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017635.1.20",
          "form": "맞아요 ㅋㅋ 딩한느낌~근데 재밌었어요",
          "original_form": "맞아요 ㅋㅋ 딩한느낌~근데 재밌었어요",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017635.1.21",
          "form": "판타지물들 좋아요 ㅋㅋ",
          "original_form": "판타지물들 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017635.1.22",
          "form": "맞아요. 현실 배경이면 몰입감은 더 높은데 그런만큼 또 힘들더라구요ㅠㅠ",
          "original_form": "맞아요. 현실 배경이면 몰입감은 더 높은데 그런만큼 또 힘들더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017635.1.23",
          "form": "액션도 판타지 쪽 스케일이 크구요ㅎㅎ",
          "original_form": "액션도 판타지 쪽 스케일이 크구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017635.1.24",
          "form": "맞아요! 마블 이쪽 워낙 고퀄이잖아요~",
          "original_form": "맞아요! 마블 이쪽 워낙 고퀄이잖아요~",
          "speaker_id": "1",
          "time": "20210826 20:52"
        }
      ]
    }
