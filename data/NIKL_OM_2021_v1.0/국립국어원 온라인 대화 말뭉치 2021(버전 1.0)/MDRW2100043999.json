      "utterance": [
        {
          "id": "MDRW2100043999.1.1",
          "form": "네 아직 캥거루입니다!",
          "original_form": "네 아직 캥거루입니다!",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043999.1.2",
          "form": "집밥최고죠ㅜㅜ",
          "original_form": "집밥최고죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043999.1.3",
          "form": "크 좋으시겠어용 !",
          "original_form": "크 좋으시겠어용 !",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043999.1.4",
          "form": "독립하셨나봐요~",
          "original_form": "독립하셨나봐요~",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043999.1.5",
          "form": "저는 결혼해서용 ㅠㅠ",
          "original_form": "저는 결혼해서용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043999.1.6",
          "form": "오오오 그러시군요",
          "original_form": "오오오 그러시군요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043999.1.7",
          "form": "넹 엄마랑 같이살긴하는데",
          "original_form": "넹 엄마랑 같이살긴하는데",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043999.1.8",
          "form": "엄마가 좀 편찮으셔서",
          "original_form": "엄마가 좀 편찮으셔서",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043999.1.9",
          "form": "제가 다 요리하거등요",
          "original_form": "제가 다 요리하거등요",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043999.1.10",
          "form": "와 엄청 대단하시네요ㅜㅜ",
          "original_form": "와 엄청 대단하시네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043999.1.11",
          "form": "저야그냥저먹을것만할줄알지 가족을위해서는못하는데",
          "original_form": "저야그냥저먹을것만할줄알지 가족을위해서는못하는데",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043999.1.12",
          "form": "ㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043999.1.13",
          "form": "그게참어려워요",
          "original_form": "그게참어려워요",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043999.1.14",
          "form": "제입맛에만 맞게는",
          "original_form": "제입맛에만 맞게는",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043999.1.15",
          "form": "이것저것 할텐데 말이죠 ㅠ",
          "original_form": "이것저것 할텐데 말이죠 ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043999.1.16",
          "form": "존경합니다ㅜㅜ",
          "original_form": "존경합니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043999.1.17",
          "form": "사람마다 취향이달라서 ㅠ참 ..",
          "original_form": "사람마다 취향이달라서 ㅠ참 ..",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043999.1.18",
          "form": "맞아요 저도 어쩌다 부모님해드리면 반응이..!",
          "original_form": "맞아요 저도 어쩌다 부모님해드리면 반응이..!",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043999.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 진짜 반응이 별로면 또 서운하죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 진짜 반응이 별로면 또 서운하죠",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043999.1.20",
          "form": "그러니까요ㅜㅜ",
          "original_form": "그러니까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:24"
        }
      ]
