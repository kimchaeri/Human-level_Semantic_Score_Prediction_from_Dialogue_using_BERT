        {
          "id": "MDRW2100016614.1.1",
          "form": "그쪽 날씨어떤가요",
          "original_form": "그쪽 날씨어떤가요",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016614.1.2",
          "form": "온통구름이에요",
          "original_form": "온통구름이에요",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016614.1.3",
          "form": "비는 안오네요",
          "original_form": "비는 안오네요",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016614.1.4",
          "form": "거기는요?",
          "original_form": "거기는요?",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016614.1.5",
          "form": "음 여긴쨍쨍해요!",
          "original_form": "음 여긴쨍쨍해요!",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016614.1.6",
          "form": "해났어요?",
          "original_form": "해났어요?",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100016614.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100016614.1.8",
          "form": "이제 해가빨리지네요",
          "original_form": "이제 해가빨리지네요",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100016614.1.9",
          "form": "이제곧 가을이네요!",
          "original_form": "이제곧 가을이네요!",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100016614.1.10",
          "form": "시간 너무빨리지나가요...",
          "original_form": "시간 너무빨리지나가요...",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100016614.1.11",
          "form": "가을하늘은 좋은데",
          "original_form": "가을하늘은 좋은데",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100016614.1.12",
          "form": "또금세 겨울오겠죠",
          "original_form": "또금세 겨울오겠죠",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100016614.1.13",
          "form": "그럼 올해도 금방..",
          "original_form": "그럼 올해도 금방..",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100016614.1.14",
          "form": "이번여름으너모더웠어요",
          "original_form": "이번여름으너모더웠어요",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100016614.1.15",
          "form": "그만큼겨울은 추울거라네요 ..ㄷㄷ",
          "original_form": "그만큼겨울은 추울거라네요 ..ㄷㄷ",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100016614.1.16",
          "form": "헉 벌써무섭네요",
          "original_form": "헉 벌써무섭네요",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100016614.1.17",
