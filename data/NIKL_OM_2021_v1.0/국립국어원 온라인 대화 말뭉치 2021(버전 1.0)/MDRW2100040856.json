      "utterance": [
        {
          "id": "MDRW2100040856.1.1",
          "form": "지금 하고 있는 운동이나 전에 해봤던 운동 있나요?",
          "original_form": "지금 하고 있는 운동이나 전에 해봤던 운동 있나요?",
          "speaker_id": "1",
          "time": "20210905 10:01"
        },
        {
          "id": "MDRW2100040856.1.2",
          "form": "일주일에 세번 필라테스 하고 있어요",
          "original_form": "일주일에 세번 필라테스 하고 있어요",
          "speaker_id": "2",
          "time": "20210905 10:04"
        },
        {
          "id": "MDRW2100040856.1.3",
          "form": "필라테스 전  헬스장 다닐때 한켠에서 강습하는거 참여해봤어요",
          "original_form": "필라테스 전  헬스장 다닐때 한켠에서 강습하는거 참여해봤어요",
          "speaker_id": "1",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040856.1.4",
          "form": "유연성을 키워주잖아요",
          "original_form": "유연성을 키워주잖아요",
          "speaker_id": "1",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040856.1.5",
          "form": "오 어떠셨어요?",
          "original_form": "오 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040856.1.6",
          "form": "저는 기구필라테스하는 곳에서 하고 있어요",
          "original_form": "저는 기구필라테스하는 곳에서 하고 있어요",
          "speaker_id": "2",
          "time": "20210905 10:12"
        },
        {
          "id": "MDRW2100040856.1.7",
          "form": "기구필라테스는 좀더 전문적이네요",
          "original_form": "기구필라테스는 좀더 전문적이네요",
          "speaker_id": "1",
          "time": "20210905 10:12"
        },
        {
          "id": "MDRW2100040856.1.8",
          "form": "유연성은 좀 있는편이라 필라테스 잘 따라하고 재미있었어요",
          "original_form": "유연성은 좀 있는편이라 필라테스 잘 따라하고 재미있었어요",
          "speaker_id": "1",
          "time": "20210905 10:12"
        },
        {
          "id": "MDRW2100040856.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 10:12"
        },
        {
          "id": "MDRW2100040856.1.10",
          "form": "오 유연성이 있으시구나",
          "original_form": "오 유연성이 있으시구나",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040856.1.11",
          "form": "저는 유연성은 좀 떨어져서",
          "original_form": "저는 유연성은 좀 떨어져서",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040856.1.12",
          "form": "겨우겨우 따라가요ㅜ",
          "original_form": "겨우겨우 따라가요ㅜ",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040856.1.13",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040856.1.14",
          "form": "수영을 10년넘게 하면서 운동은 쭉 했기 때문에",
          "original_form": "수영을 10년넘게 하면서 운동은 쭉 했기 때문에",
          "speaker_id": "1",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040856.1.15",
          "form": "헬스랑 수영을 해서 운동은 좀 했어요",
          "original_form": "헬스랑 수영을 해서 운동은 좀 했어요",
          "speaker_id": "1",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040856.1.16",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "2",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100040856.1.17",
          "form": "저도 수영 좋아하는데",
          "original_form": "저도 수영 좋아하는데",
          "speaker_id": "2",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100040856.1.18",
          "form": "요즘 코로나 때문에 수영장이 계속 휴관중이예요",
          "original_form": "요즘 코로나 때문에 수영장이 계속 휴관중이예요",
          "speaker_id": "2",
          "time": "20210905 11:56"
        },
        {
          "id": "MDRW2100040856.1.19",
          "form": "그렇죠 아무래도",
          "original_form": "그렇죠 아무래도",
          "speaker_id": "1",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040856.1.20",
          "form": "휴관 안했어도 아마 안갔을거에요",
          "original_form": "휴관 안했어도 아마 안갔을거에요",
          "speaker_id": "1",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040856.1.21",
          "form": "코로나 터지고 나서 다중이용시설을 피하고 있거든요",
          "original_form": "코로나 터지고 나서 다중이용시설을 피하고 있거든요",
          "speaker_id": "1",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040856.1.22",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100040856.1.23",
          "form": "저도 다중이용시설 이용은 피하게 되더라고요",
          "original_form": "저도 다중이용시설 이용은 피하게 되더라고요",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100040856.1.24",
          "form": "수영장에선 마스크도 못쓰고",
          "original_form": "수영장에선 마스크도 못쓰고",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100040856.1.25",
          "form": "수영을 거의 20년 가까이 다녔었는데 작년부터 못가니 많이 아쉽더라구요",
          "original_form": "수영을 거의 20년 가까이 다녔었는데 작년부터 못가니 많이 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100040856.1.26",
          "form": "언제쯤 다시 할 수 있을런지",
          "original_form": "언제쯤 다시 할 수 있을런지",
          "speaker_id": "1",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100040856.1.27",
          "form": "와 진짜 오래 하셨네요",
          "original_form": "와 진짜 오래 하셨네요",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040856.1.28",
          "form": "하다 안하면 더 생각나실텐데",
          "original_form": "하다 안하면 더 생각나실텐데",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040856.1.29",
          "form": "네 수영이 가장 오래한 운동이에요",
          "original_form": "네 수영이 가장 오래한 운동이에요",
          "speaker_id": "1",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040856.1.30",
          "form": "수영 좋죠",
          "original_form": "수영 좋죠",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040856.1.31",
          "form": "무리는 안가면서 운동효과는 좋고",
          "original_form": "무리는 안가면서 운동효과는 좋고",
          "speaker_id": "2",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100040856.1.32",
          "form": "그래서 오래했는데 대신 골다공증 예방에는 별로래요",
          "original_form": "그래서 오래했는데 대신 골다공증 예방에는 별로래요",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100040856.1.33",
          "form": "뼈 건강을 위해서는 헬스같은 거 해야한대요",
          "original_form": "뼈 건강을 위해서는 헬스같은 거 해야한대요",
          "speaker_id": "1",
          "time": "20210905 12:06"
        }
      ]
