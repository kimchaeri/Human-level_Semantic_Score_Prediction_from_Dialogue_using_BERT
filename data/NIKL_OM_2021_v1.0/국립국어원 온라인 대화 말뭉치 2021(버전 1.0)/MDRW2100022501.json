      "utterance": [
        {
          "id": "MDRW2100022501.1.1",
          "form": "도지마롤 좋아하시면 꼭 가서드셔보세요ㅠㅠ",
          "original_form": "도지마롤 좋아하시면 꼭 가서드셔보세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100022501.1.2",
          "form": "네ㅋㅋ추천 감사해요!",
          "original_form": "네ㅋㅋ추천 감사해요!",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022501.1.3",
          "form": "일본 가서 맛있었던 음식 또 있나요?",
          "original_form": "일본 가서 맛있었던 음식 또 있나요?",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022501.1.4",
          "form": "저는 아무래도 세이로무시가 제일 맛있었어요",
          "original_form": "저는 아무래도 세이로무시가 제일 맛있었어요",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022501.1.5",
          "form": "찾아보니까",
          "original_form": "찾아보니까",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022501.1.6",
          "form": "편백찜인가요??",
          "original_form": "편백찜인가요??",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022501.1.7",
          "form": "네! 편백찜비슷해요!",
          "original_form": "네! 편백찜비슷해요!",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022501.1.8",
          "form": "일본에서 먹어봤는데 진짜 맛있었어요",
          "original_form": "일본에서 먹어봤는데 진짜 맛있었어요",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022501.1.9",
          "form": "오..나중에 가면 먹어봐야겠어요!",
          "original_form": "오..나중에 가면 먹어봐야겠어요!",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022501.1.10",
          "form": "그밖에는 다코야끼나 소바 덮밥 돈까스가 맛있었어요",
          "original_form": "그밖에는 다코야끼나 소바 덮밥 돈까스가 맛있었어요",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022501.1.11",
          "form": "와 다 제가 좋아하는 것들이에요",
          "original_form": "와 다 제가 좋아하는 것들이에요",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022501.1.12",
          "form": "일식 좋아하시는구나!",
          "original_form": "일식 좋아하시는구나!",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022501.1.13",
          "form": "안 빼먹고 다 먹어볼게요ㅎㅎㅎㅎ",
          "original_form": "안 빼먹고 다 먹어볼게요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022501.1.14",
          "form": "네 저 일식 제일 좋아해요!",
          "original_form": "네 저 일식 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022501.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022501.1.16",
          "form": "좋아요 저두요! 일식좋아해요",
          "original_form": "좋아요 저두요! 일식좋아해요",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022501.1.17",
          "form": "그래서 일본에서 행복했어요",
          "original_form": "그래서 일본에서 행복했어요",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022501.1.18",
          "form": "그랬겠어요ㅠㅠㅠ",
          "original_form": "그랬겠어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022501.1.19",
          "form": "저도 코로나 이후에 다녀와야겠어요",
          "original_form": "저도 코로나 이후에 다녀와야겠어요",
          "speaker_id": "2",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022501.1.20",
          "form": "코로나가 빨리 끝나면 좋겠네요",
          "original_form": "코로나가 빨리 끝나면 좋겠네요",
          "speaker_id": "1",
          "time": "20210829 20:01"
        }
