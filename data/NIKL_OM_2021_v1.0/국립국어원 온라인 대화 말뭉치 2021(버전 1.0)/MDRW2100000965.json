      "utterance": [
        {
          "id": "MDRW2100000965.1.1",
          "form": "집 오자마자 설거지하고 빨래하니 9시....밥 먹기도 귀찮네요",
          "original_form": "집 오자마자 설거지하고 빨래하니 9시....밥 먹기도 귀찮네요",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000965.1.2",
          "form": "아직도 식사 안하셨나요???!!!",
          "original_form": "아직도 식사 안하셨나요???!!!",
          "speaker_id": "2",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000965.1.3",
          "form": "빨리 드세요ㅠㅠㅠ",
          "original_form": "빨리 드세요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000965.1.4",
          "form": "집안일 하다보면 시간이 빨리 가긴 하더라구요ㅠㅠ",
          "original_form": "집안일 하다보면 시간이 빨리 가긴 하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000965.1.5",
          "form": "그니까요ㅠㅠㅠㅜ퇴근하고 집안일하면...너무 시간이 훌쩍",
          "original_form": "그니까요ㅠㅠㅠㅜ퇴근하고 집안일하면...너무 시간이 훌쩍",
          "speaker_id": "1",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000965.1.6",
          "form": "진짜 집안일만 누가 대신해주면좋겠어요",
          "original_form": "진짜 집안일만 누가 대신해주면좋겠어요",
          "speaker_id": "1",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000965.1.7",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000965.1.8",
          "form": "먹고나면 설거지 쌓여서 먹기도 싫고...",
          "original_form": "먹고나면 설거지 쌓여서 먹기도 싫고...",
          "speaker_id": "2",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000965.1.9",
          "form": "누가 해주면 참 좋을텐데ㅠㅠㅠ",
          "original_form": "누가 해주면 참 좋을텐데ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000965.1.10",
          "form": "name1님은 그나마 할만한 집안일이 뭔가요?",
          "original_form": "&name1&님은 그나마 할만한 집안일이 뭔가요?",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000965.1.11",
          "form": "음....이불정리?ㅋㅋㅋ",
          "original_form": "음....이불정리?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000965.1.12",
          "form": "아니면 세탁이요ㅠㅜ건조말고",
          "original_form": "아니면 세탁이요ㅠㅜ건조말고",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000965.1.13",
          "form": "세탁기가해주니까..",
          "original_form": "세탁기가해주니까..",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000965.1.14",
          "form": "아ㅋㅋㅋㅋ저도 세탁이 젤 좋네요ㅎㅎ",
          "original_form": "아ㅋㅋㅋㅋ저도 세탁이 젤 좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000965.1.15",
          "form": "name1님은 설거지거리 생기면 바로바로 하는 편이신가요?",
          "original_form": "&name1&님은 설거지거리 생기면 바로바로 하는 편이신가요?",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000965.1.16",
          "form": "한번에하는게좋은데",
          "original_form": "한번에하는게좋은데",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000965.1.17",
          "form": "혼자 살다보니까",
          "original_form": "혼자 살다보니까",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000965.1.18",
          "form": "바로바로 하게됩니다ㅠ",
          "original_form": "바로바로 하게됩니다ㅠ",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000965.1.19",
          "form": "부지런하시네요!",
          "original_form": "부지런하시네요!",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000965.1.20",
          "form": "저는 혼자 살면 쌓아두고 몰아서 할거같아요ㅠㅠ",
          "original_form": "저는 혼자 살면 쌓아두고 몰아서 할거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.21",
          "form": "전에 저도 몰아했는데",
          "original_form": "전에 저도 몰아했는데",
          "speaker_id": "1",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.22",
          "form": "날파리들이랑 동거했어요",
          "original_form": "날파리들이랑 동거했어요",
          "speaker_id": "1",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.23",
          "form": "지금은 부모님이 계셔서 먹자마자 하는데ㅠㅠ",
          "original_form": "지금은 부모님이 계셔서 먹자마자 하는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.24",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.25",
          "form": "그럼 바로 해야죠",
          "original_form": "그럼 바로 해야죠",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.26",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.27",
          "form": "귀찮은거보다 더 싫은게 날파리...",
          "original_form": "귀찮은거보다 더 싫은게 날파리...",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000965.1.28",
          "form": "마자여....번식력이 너무 심하구.....",
          "original_form": "마자여....번식력이 너무 심하구.....",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000965.1.29",
          "form": "맞아요..없다싶으면 바로 나오고...",
          "original_form": "맞아요..없다싶으면 바로 나오고...",
          "speaker_id": "2",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100000965.1.30",
          "form": "여름엔 날파리 때문에 음식물 처리하기 너무 힘들어요ㅠㅠ",
          "original_form": "여름엔 날파리 때문에 음식물 처리하기 너무 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100000965.1.31",
          "form": "맞아요ㅠㅠㅠ냉동해두긴하는데ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ냉동해두긴하는데ㅠㅠㅠ",
