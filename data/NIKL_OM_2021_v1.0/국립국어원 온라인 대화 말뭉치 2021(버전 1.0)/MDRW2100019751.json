      "utterance": [
        {
          "id": "MDRW2100019751.1.1",
          "form": "ㅋㅋㅋ 이번엔 먹는거네요",
          "original_form": "ㅋㅋㅋ 이번엔 먹는거네요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019751.1.2",
          "form": "저는 먹는거 좋아합니다",
          "original_form": "저는 먹는거 좋아합니다",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019751.1.3",
          "form": "엄청",
          "original_form": "엄청",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019751.1.4",
          "form": "먹을거 어떤거 좋아하세요?",
          "original_form": "먹을거 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019751.1.5",
          "form": "먹는건 전부다? ㅎ",
          "original_form": "먹는건 전부다? ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019751.1.6",
          "form": "생으로 먹는거는 조금 별로이긴했는데",
          "original_form": "생으로 먹는거는 조금 별로이긴했는데",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019751.1.7",
          "form": "요즘엔 회도 그렇고 다 좋아해요",
          "original_form": "요즘엔 회도 그렇고 다 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019751.1.8",
          "form": "저는 고기가 제일  좋아요",
          "original_form": "저는 고기가 제일  좋아요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019751.1.9",
          "form": "고기는 언제나 옳습니다",
          "original_form": "고기는 언제나 옳습니다",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019751.1.10",
          "form": "요새는 항정살이 맛있더라고요 ㅋㅋ",
          "original_form": "요새는 항정살이 맛있더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019751.1.11",
          "form": "항정살엔 계란찜이죠 ㅎㅎㅎ",
          "original_form": "항정살엔 계란찜이죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019751.1.12",
          "form": "숯불에서 구운 고기가 정말 맛있어요.",
          "original_form": "숯불에서 구운 고기가 정말 맛있어요.",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019751.1.13",
          "form": "넵 ㅎ 고기는 언제나 옳고 숯불은 진리죠 ㅎㅎㅎㅎ",
          "original_form": "넵 ㅎ 고기는 언제나 옳고 숯불은 진리죠 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019751.1.14",
          "form": "동그랗게 올라온 계란찜이 정말 맛있는 데, 맛있는 집이 별로 없어요ㅎㅎ",
          "original_form": "동그랗게 올라온 계란찜이 정말 맛있는 데, 맛있는 집이 별로 없어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019751.1.15",
          "form": "저두 캠핑가서 숯불에 구운 고기가 젤로 맛있더라구요",
          "original_form": "저두 캠핑가서 숯불에 구운 고기가 젤로 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019751.1.16",
          "form": "고기 드시러 캠핑을 하시는군요?",
          "original_form": "고기 드시러 캠핑을 하시는군요?",
          "speaker_id": "1",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019751.1.17",
