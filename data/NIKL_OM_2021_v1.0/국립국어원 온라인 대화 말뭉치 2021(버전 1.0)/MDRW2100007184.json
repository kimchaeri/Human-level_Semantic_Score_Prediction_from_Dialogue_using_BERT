      "utterance": [
        {
          "id": "MDRW2100007184.1.1",
          "form": "지금 방학이야?",
          "original_form": "지금 방학이야?",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007184.1.2",
          "form": "나 올해 졸업했어 ...",
          "original_form": "나 올해 졸업했어 ...",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007184.1.3",
          "form": "방학이야 ..?",
          "original_form": "방학이야 ..?",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007184.1.4",
          "form": "나는 사실 학교를 졸업한지 10년돼서 몰라",
          "original_form": "나는 사실 학교를 졸업한지 10년돼서 몰라",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007184.1.5",
          "form": "그럼 취업한거야?",
          "original_form": "그럼 취업한거야?",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007184.1.6",
          "form": "아직은 백수야 ..",
          "original_form": "아직은 백수야 ..",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007184.1.7",
          "form": "학생때 동아리 활동 해봤어?",
          "original_form": "학생때 동아리 활동 해봤어?",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007184.1.8",
          "form": "응 책 만드는 동아리! 너는?",
          "original_form": "응 책 만드는 동아리! 너는?",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007184.1.9",
          "form": "나는 베이킹동아리!",
          "original_form": "나는 베이킹동아리!",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007184.1.10",
          "form": "그럼 책을 직접 썼어?",
          "original_form": "그럼 책을 직접 썼어?",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007184.1.11",
          "form": "그냥 인터뷰하고 싣는 잡지 같은거 만들었어! 기사는 썼지ㅋㅋ",
          "original_form": "그냥 인터뷰하고 싣는 잡지 같은거 만들었어! 기사는 썼지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007184.1.12",
          "form": "베이킹 멋지다. 케이크나 쿠키 같은 거 만드나?",
          "original_form": "베이킹 멋지다. 케이크나 쿠키 같은 거 만드나?",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007184.1.13",
          "form": "멋지다 나는 글솜씨가 별로 없거든..",
          "original_form": "멋지다 나는 글솜씨가 별로 없거든..",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007184.1.14",
          "form": "응 난 베이킹하는 걸 좋아해서 들어갔어 쿠키랑 머핀 마들렌 이런거 만들어!",
          "original_form": "응 난 베이킹하는 걸 좋아해서 들어갔어 쿠키랑 머핀 마들렌 이런거 만들어!",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007184.1.15",
          "form": "나도 그런 동아리 들어보고 싶었어.\r\n지금은 베이킹 책을 사긴 했는데 한번 해보니까 너무 어려워ㅋㅋ\r\n컵케익 사먹는 게 안전하더라ㅜㅜ",
          "original_form": "나도 그런 동아리 들어보고 싶었어.\r\n지금은 베이킹 책을 사긴 했는데 한번 해보니까 너무 어려워ㅋㅋ\r\n컵케익 사먹는 게 안전하더라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007184.1.16",
          "form": "베이킹이 점점 배울수록 어렵더라구ㅜㅜ 오븐에서 잘못 구우면 망치기도 하고",
          "original_form": "베이킹이 점점 배울수록 어렵더라구ㅜㅜ 오븐에서 잘못 구우면 망치기도 하고",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007184.1.17",
          "form": "대학다닐때 수업은 어땠어?",
          "original_form": "대학다닐때 수업은 어땠어?",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007184.1.18",
          "form": "재밌었는데 이제 와서 생각해보면 그때 좀더 열심히 할걸..  싶어\r\n학점 조금만 더 올릴걸!ㅜㅜ",
          "original_form": "재밌었는데 이제 와서 생각해보면 그때 좀더 열심히 할걸..  싶어\r\n학점 조금만 더 올릴걸!ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007184.1.19",
          "form": "맞아 졸업하고 나니까 조금 후회되는 일들이 많더라고ㅠ 수업 더 열심히 들을걸 ..시험 좀 잘칠걸...",
          "original_form": "맞아 졸업하고 나니까 조금 후회되는 일들이 많더라고ㅠ 수업 더 열심히 들을걸 ..시험 좀 잘칠걸...",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007184.1.20",
          "form": "학교생활할때가 재밌었던 것 같아",
          "original_form": "학교생활할때가 재밌었던 것 같아",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007184.1.21",
          "form": "그리고 학교에서 사람 스트레스가 아직까지 이어져서\r\n그때 손절할걸 하는 사람도 있어ㅜㅜ",
          "original_form": "그리고 학교에서 사람 스트레스가 아직까지 이어져서\r\n그때 손절할걸 하는 사람도 있어ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007184.1.22",
          "form": "그런 사람들은 진짜 초반에 끊는게 낫더라고ㅜㅜ",
          "original_form": "그런 사람들은 진짜 초반에 끊는게 낫더라고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007184.1.23",
          "form": "학교다니면서 이상한 사람들 진짜 많았어.. ㅠㅠ",
          "original_form": "학교다니면서 이상한 사람들 진짜 많았어.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007184.1.24",
          "form": "괜히 학교 인연 끌면 회사 다니면서도 1년에 한번씩은 만나야 되고 경조사도 가야 돼ㅜㅜ",
          "original_form": "괜히 학교 인연 끌면 회사 다니면서도 1년에 한번씩은 만나야 되고 경조사도 가야 돼ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007184.1.25",
          "form": "진짜 문제다 ㅜㅜ 맞지 않는 사람이랑 관계 이어나가는게 에너지가 엄청 소모되는 일이잖아..",
          "original_form": "진짜 문제다 ㅜㅜ 맞지 않는 사람이랑 관계 이어나가는게 에너지가 엄청 소모되는 일이잖아..",
          "speaker_id": "2",
          "time": "20210822 01:18"
        }
      ]
