      "utterance": [
        {
          "id": "MDRW2100004700.1.1",
          "form": "어머... 이번 도쿄올림픽 여자배구 보셨나요",
          "original_form": "어머... 이번 도쿄올림픽 여자배구 보셨나요",
          "speaker_id": "2",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004700.1.2",
          "form": "봣슴니다",
          "original_form": "봣슴니다",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004700.1.3",
          "form": "한일전... 생방으로 달리셨나요",
          "original_form": "한일전... 생방으로 달리셨나요",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.4",
          "form": "김연경짱 김희진짱 박정아짱 이소영짱..",
          "original_form": "김연경짱 김희진짱 박정아짱 이소영짱..",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.5",
          "form": "녜ㅜㅜ",
          "original_form": "녜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.6",
          "form": "그 전율..!",
          "original_form": "그 전율..!",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.7",
          "form": "안될줄알았는데 뒤집었다구요",
          "original_form": "안될줄알았는데 뒤집었다구요",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.8",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ저 진짜 공 계속 살아나는 거 보면서 좀비냐고 소리쳤어요",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ저 진짜 공 계속 살아나는 거 보면서 좀비냐고 소리쳤어요",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.9",
          "form": "그니까요 하",
          "original_form": "그니까요 하",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.10",
          "form": "하 감동감동",
          "original_form": "하 감동감동",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004700.1.11",
          "form": "곧 코보컵도 해요",
          "original_form": "곧 코보컵도 해요",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004700.1.12",
          "form": "후회없이 해보자는 말에서 울컥했잖아요",
          "original_form": "후회없이 해보자는 말에서 울컥했잖아요",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004700.1.13",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ갓연경",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ갓연경",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004700.1.14",
          "form": "맞아요 8월 23일",
          "original_form": "맞아요 8월 23일",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004700.1.15",
          "form": "코보컵 보실건가요??",
          "original_form": "코보컵 보실건가요??",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004700.1.16",
          "form": "전 김희진 선수님한테 빠져서... 아비케 응원할 예정입니다",
          "original_form": "전 김희진 선수님한테 빠져서... 아비케 응원할 예정입니다",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004700.1.17",
          "form": "기은 좋져",
          "original_form": "기은 좋져",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004700.1.18",
          "form": "전 이소영선수 응원해서 인삼 응원할거예요",
          "original_form": "전 이소영선수 응원해서 인삼 응원할거예요",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004700.1.19",
          "form": "첫날 첫경기가 지에스랑 인삼이라구요ㅠㅠ",
          "original_form": "첫날 첫경기가 지에스랑 인삼이라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004700.1.20",
          "form": "하 소영선배...",
          "original_form": "하 소영선배...",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004700.1.21",
          "form": "코보 넘 잔인",
          "original_form": "코보 넘 잔인",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004700.1.22",
          "form": "소영선배 사란훼",
          "original_form": "소영선배 사란훼",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004700.1.23",
          "form": "기은 첫경기는 어디랑 하죠?",
          "original_form": "기은 첫경기는 어디랑 하죠?",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004700.1.24",
          "form": "현건이랑 해요",
          "original_form": "현건이랑 해요",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004700.1.25",
          "form": "아앗 지윤이..",
          "original_form": "아앗 지윤이..",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004700.1.26",
          "form": "라이트 대 라이트인가요",
          "original_form": "라이트 대 라이트인가요",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004700.1.27",
          "form": "이번에 코보컵 외국인 없는거 아세용?",
          "original_form": "이번에 코보컵 외국인 없는거 아세용?",
          "speaker_id": "1",
          "time": "20210821 18:07"
        }
      ]
    }
  ]
