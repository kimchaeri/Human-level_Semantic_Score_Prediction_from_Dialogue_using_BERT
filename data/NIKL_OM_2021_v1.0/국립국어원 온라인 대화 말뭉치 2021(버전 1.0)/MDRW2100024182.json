      "utterance": [
        {
          "id": "MDRW2100024182.1.1",
          "form": "여행가면 주로 뭐 사오세요?",
          "original_form": "여행가면 주로 뭐 사오세요?",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024182.1.2",
          "form": "음 저... 일본 참 자주가서",
          "original_form": "음 저... 일본 참 자주가서",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024182.1.3",
          "form": "그렇게 동전파스랑 휴족시간 부자였어요... 왜그렇게 샀을까",
          "original_form": "그렇게 동전파스랑 휴족시간 부자였어요... 왜그렇게 샀을까",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024182.1.4",
          "form": "보브상도 아닌데... 후회 ㅋㅋㅋㅋㅋㅋㅋ 나도모르게 중국인 따라사기",
          "original_form": "보브상도 아닌데... 후회 ㅋㅋㅋㅋㅋㅋㅋ 나도모르게 중국인 따라사기",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024182.1.5",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024182.1.6",
          "form": "동전파스는 너무아프더라구요ㅜㅜ",
          "original_form": "동전파스는 너무아프더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024182.1.7",
          "form": "화상입는거  처럼ㅜㅜ",
          "original_form": "화상입는거  처럼ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024182.1.8",
          "form": "ㅠㅠ 저 샤론파스파입니다",
          "original_form": "ㅠㅠ 저 샤론파스파입니다",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024182.1.9",
          "form": "아! 그거 괜찮나요?",
          "original_form": "아! 그거 괜찮나요?",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024182.1.10",
          "form": "안써봐서",
          "original_form": "안써봐서",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024182.1.11",
          "form": "네 잘 떨어지고 작아서 붙이기 좋더라구요 ㅋㅋ",
          "original_form": "네 잘 떨어지고 작아서 붙이기 좋더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024182.1.12",
          "form": "아! 써봐야겠네요!",
          "original_form": "아! 써봐야겠네요!",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100024182.1.13",
          "form": "사봐야 나중에 거를줄도 아는것 같아요ㅋㅋ",
          "original_form": "사봐야 나중에 거를줄도 아는것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100024182.1.14",
          "form": "맞아요 ㅋㅋ 근데 그 비슷한게 한국제품에도 판다고 하더라구요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 근데 그 비슷한게 한국제품에도 판다고 하더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024182.1.15",
          "form": "아 진짜요?\r\n검색해서 찾아봐야겠네요ㅋㅋㄱ",
          "original_form": "아 진짜요?\r\n검색해서 찾아봐야겠네요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024182.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 좋네요 서로 좋은거 있음 공유를해보아요 ㅋㅋ name1님ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 좋네요 서로 좋은거 있음 공유를해보아요 ㅋㅋ &name1&님ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024182.1.17",
          "form": "네 유익한 정보네요 정말ㅋㅋㅋ",
          "original_form": "네 유익한 정보네요 정말ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024182.1.18",
          "form": "그쵸! ㅋㅋㅋ 쇼핑 좋아하시면 ㅋㅋㅋ 공유 좋아요 ㅋㅋㅋ",
          "original_form": "그쵸! ㅋㅋㅋ 쇼핑 좋아하시면 ㅋㅋㅋ 공유 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024182.1.19",
          "form": "전 일본갔다오면 대부분 스타킹이나 레깅스 사서 왔던거 같아요ㅋㅋ",
          "original_form": "전 일본갔다오면 대부분 스타킹이나 레깅스 사서 왔던거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024182.1.20",
          "form": "촉감이 너무 좋아서ㅎㅎ",
          "original_form": "촉감이 너무 좋아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024182.1.21",
          "form": "아 저두 스타킹많이 샀었는데ㅋㅋㅋ",
          "original_form": "아 저두 스타킹많이 샀었는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024182.1.22",
          "form": "너무 부드럽죠?ㅋㅋㅋ",
          "original_form": "너무 부드럽죠?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024182.1.23",
          "form": "네 ㅋㅋㅋㅋㅋ 또 뭘 사왔더랑 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ 또 뭘 사왔더랑 ㅋㅋ",
