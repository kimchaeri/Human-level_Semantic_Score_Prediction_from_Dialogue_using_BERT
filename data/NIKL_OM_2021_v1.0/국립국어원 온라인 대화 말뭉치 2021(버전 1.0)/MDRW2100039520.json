      "utterance": [
        {
          "id": "MDRW2100039520.1.1",
          "form": "사실 저 떡볶이 시켰어요",
          "original_form": "사실 저 떡볶이 시켰어요",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039520.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 몸에 안좋아!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 몸에 안좋아!!!",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039520.1.3",
          "form": "닭퍽퍽살이랑 누들 먹었는데 배고파서요...",
          "original_form": "닭퍽퍽살이랑 누들 먹었는데 배고파서요...",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039520.1.4",
          "form": "토피넛 라떼 마싯는데",
          "original_form": "토피넛 라떼 마싯는데",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039520.1.5",
          "form": "꼬르륵꼬륵...",
          "original_form": "꼬르륵꼬륵...",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039520.1.6",
          "form": "토피넛라떼 무슨 맛이에요?",
          "original_form": "토피넛라떼 무슨 맛이에요?",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100039520.1.7",
          "form": "어... 먹은지 넘 오래되긴했는데? 고소한맛이던가",
          "original_form": "어... 먹은지 넘 오래되긴했는데? 고소한맛이던가",
          "speaker_id": "2",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100039520.1.8",
          "form": "지금 사머거볼가(?)",
          "original_form": "지금 사머거볼가(?)",
          "speaker_id": "2",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100039520.1.9",
          "form": "앗 콜드브루 디카페인도 있다",
          "original_form": "앗 콜드브루 디카페인도 있다",
          "speaker_id": "2",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100039520.1.10",
          "form": "맞아 거긴 디카페인 팔더라고 카페인 못 먹어도 마실 수 있어요",
          "original_form": "맞아 거긴 디카페인 팔더라고 카페인 못 먹어도 마실 수 있어요",
          "speaker_id": "1",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100039520.1.11",
          "form": "토피넛이니까 무슨 견과류일 거 같은데 그게 뭘까",
          "original_form": "토피넛이니까 무슨 견과류일 거 같은데 그게 뭘까",
          "speaker_id": "1",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100039520.1.12",
          "form": "이건 안먹어봤으니 먹어본다는 말의 어디가 나쁜말이지? 웅 메가커피 디카페인 파네요",
          "original_form": "이건 안먹어봤으니 먹어본다는 말의 어디가 나쁜말이지? 웅 메가커피 디카페인 파네요",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039520.1.13",
          "form": "스벅인줄 알았는데",
          "original_form": "스벅인줄 알았는데",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039520.1.14",
          "form": "메가커피도 디카페인을 팔다니",
          "original_form": "메가커피도 디카페인을 팔다니",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039520.1.15",
          "form": "더 싸고 좋겠군요",
          "original_form": "더 싸고 좋겠군요",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039520.1.16",
          "form": "어머 견과류가 아니네",
          "original_form": "어머 견과류가 아니네",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039520.1.17",
          "form": "그러게요 의외로 견과류가 아니다",
          "original_form": "그러게요 의외로 견과류가 아니다",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039520.1.18",
          "form": "디카페인 특이한 맛이네..",
          "original_form": "디카페인 특이한 맛이네..",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039520.1.19",
          "form": "훨 밍밍한 느낌",
          "original_form": "훨 밍밍한 느낌",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100039520.1.20",
          "form": "엥 전 못 느꼈어요 디카페인이라고 밍밍한가",
          "original_form": "엥 전 못 느꼈어요 디카페인이라고 밍밍한가",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039520.1.21",
          "form": "근데 저 배고파요 배달시킨 떡볶 언제와",
          "original_form": "근데 저 배고파요 배달시킨 떡볶 언제와",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039520.1.22",
          "form": "어.. 디카페인 시켰는데 목이 붓는 느낌나",
          "original_form": "어.. 디카페인 시켰는데 목이 붓는 느낌나",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039520.1.23",
          "form": "왜지ㅣ",
          "original_form": "왜지ㅣ",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039520.1.24",
          "form": "카페인에 알러지가 있는 거였나요?",
          "original_form": "카페인에 알러지가 있는 거였나요?",
          "speaker_id": "1",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039520.1.25",
          "form": "아님 커피 자체에 알러지 있는 거 아냐?",
          "original_form": "아님 커피 자체에 알러지 있는 거 아냐?",
          "speaker_id": "1",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039520.1.26",
          "form": "목 붓는거면 큰일나는데",
          "original_form": "목 붓는거면 큰일나는데",
          "speaker_id": "1",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039520.1.27",
          "form": "전엔 없었는디.. 한모금하고 부각되는 느낌이라 버렸어요",
          "original_form": "전엔 없었는디.. 한모금하고 부각되는 느낌이라 버렸어요",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039520.1.28",
          "form": "궁중떡볶이 먹고싶다",
          "original_form": "궁중떡볶이 먹고싶다",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039520.1.29",
          "form": "저는 떡볶이를 먹고있어요",
          "original_form": "저는 떡볶이를 먹고있어요",
          "speaker_id": "1",
          "time": "20210903 19:18"
        }
      ]
    }
