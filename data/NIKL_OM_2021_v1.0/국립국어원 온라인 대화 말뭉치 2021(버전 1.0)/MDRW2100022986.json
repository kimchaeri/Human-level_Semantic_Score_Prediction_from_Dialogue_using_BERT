      "utterance": [
        {
          "id": "MDRW2100022986.1.1",
          "form": "동생이요 그게 누구죠",
          "original_form": "동생이요 그게 누구죠",
          "speaker_id": "2",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100022986.1.2",
          "form": "대화하지않습니다",
          "original_form": "대화하지않습니다",
          "speaker_id": "2",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100022986.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100022986.1.4",
          "form": "친하게지내야죠",
          "original_form": "친하게지내야죠",
          "speaker_id": "1",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100022986.1.5",
          "form": "남의 가정사에 개입하지 마시죠",
          "original_form": "남의 가정사에 개입하지 마시죠",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100022986.1.6",
          "form": "name1 형제자매있나여",
          "original_form": "&name1& 형제자매있나여",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100022986.1.7",
          "form": "에구궁 ; 야박하셔라 name2과 꽤 가깝다고 생각했는데..",
          "original_form": "에구궁 ; 야박하셔라 &name2&과 꽤 가깝다고 생각했는데..",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100022986.1.8",
          "form": "저는 오빠있습니다",
          "original_form": "저는 오빠있습니다",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100022986.1.9",
          "form": "아.. 오빠..오빠면 좋을 것 같네요 저도 오빠나 언니나 여동생 있었으면 좋겠어요",
          "original_form": "아.. 오빠..오빠면 좋을 것 같네요 저도 오빠나 언니나 여동생 있었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100022986.1.10",
          "form": "좋간 개뿔요",
          "original_form": "좋간 개뿔요",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100022986.1.11",
          "form": "가족을 보듬어줍시다 우리",
          "original_form": "가족을 보듬어줍시다 우리",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100022986.1.12",
          "form": "오빠한테 용돈받아봤어요?",
          "original_form": "오빠한테 용돈받아봤어요?",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100022986.1.13",
          "form": "저도 믿음직한 언니 or 귀여운 여동생 or 애교넘치는 남동생 or 잘생기고 동생한테 퍼주는 오빠",
          "original_form": "저도 믿음직한 언니 or 귀여운 여동생 or 애교넘치는 남동생 or 잘생기고 동생한테 퍼주는 오빠",
          "speaker_id": "1",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100022986.1.14",
          "form": "가지고싶어요",
          "original_form": "가지고싶어요",
          "speaker_id": "1",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100022986.1.15",
          "form": "뭐...진짜 머 한두번... ㅎ",
          "original_form": "뭐...진짜 머 한두번... ㅎ",
          "speaker_id": "1",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100022986.1.16",
          "form": "한두번이 어딥니까!!!",
          "original_form": "한두번이 어딥니까!!!",
          "speaker_id": "2",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100022986.1.17",
          "form": "애교넘치는 남동생있으면 재밌을거같아요 ㅋㅋㅋ",
          "original_form": "애교넘치는 남동생있으면 재밌을거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100022986.1.18",
          "form": "그죠 심부름도 잘하구",
          "original_form": "그죠 심부름도 잘하구",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100022986.1.19",
          "form": "돈벌어서 누나용돈도 주고",
          "original_form": "돈벌어서 누나용돈도 주고",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100022986.1.20",
          "form": "선물사주고",
          "original_form": "선물사주고",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100022986.1.21",
          "form": "아이구 그럼 정말.. 업고 다닙니다!!",
          "original_form": "아이구 그럼 정말.. 업고 다닙니다!!",
          "speaker_id": "2",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100022986.1.22",
          "form": "나한텐 왜 그런 형제가 없는걸까유",
          "original_form": "나한텐 왜 그런 형제가 없는걸까유",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100022986.1.23",
          "form": "휴 방금 분리수거 하고왔어요 힘드네여",
          "original_form": "휴 방금 분리수거 하고왔어요 힘드네여",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100022986.1.24",
