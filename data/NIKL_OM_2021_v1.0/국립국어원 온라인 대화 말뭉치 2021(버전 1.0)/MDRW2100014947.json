      "utterance": [
        {
          "id": "MDRW2100014947.1.1",
          "form": "취업을 꼭.. 해야되나",
          "original_form": "취업을 꼭.. 해야되나",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014947.1.2",
          "form": "돈이 많았으면 좋겠다",
          "original_form": "돈이 많았으면 좋겠다",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014947.1.3",
          "form": "진짜.. 요즘 간절히 느끼는 중",
          "original_form": "진짜.. 요즘 간절히 느끼는 중",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014947.1.4",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014947.1.5",
          "form": "초딩때부터",
          "original_form": "초딩때부터",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014947.1.6",
          "form": "돈을 모아놧어야햇는데",
          "original_form": "돈을 모아놧어야햇는데",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014947.1.7",
          "form": "뭘하면 돈을 많이 벌 수 있을까",
          "original_form": "뭘하면 돈을 많이 벌 수 있을까",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014947.1.8",
          "form": "난 돈이 좋아",
          "original_form": "난 돈이 좋아",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014947.1.9",
          "form": "너도",
          "original_form": "너도",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014947.1.10",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014947.1.11",
          "form": "옛날엔 안이랫는데",
          "original_form": "옛날엔 안이랫는데",
          "speaker_id": "2",
          "time": "20210825 21:19"
