        },
        {
          "id": "MDRW2100033636.1.2",
          "form": "어디꺼 좋아하세용",
          "original_form": "어디꺼 좋아하세용",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100033636.1.3",
          "form": "전 bbq 후라이드랑",
          "original_form": "전 bbq 후라이드랑",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100033636.1.4",
          "form": "교촌 레드콤보를 선호해요",
          "original_form": "교촌 레드콤보를 선호해요",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100033636.1.5",
          "form": "아시는지요",
          "original_form": "아시는지요",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.6",
          "form": "당연히 알죠",
          "original_form": "당연히 알죠",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.7",
          "form": "근데 레드콤보",
          "original_form": "근데 레드콤보",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.8",
          "form": "맵지 않나요?",
          "original_form": "맵지 않나요?",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.9",
          "form": "아 이게 첫입엔 안매운데",
          "original_form": "아 이게 첫입엔 안매운데",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.10",
          "form": "계속먹으면 너무맵더라구요",
          "original_form": "계속먹으면 너무맵더라구요",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.11",
          "form": "치킨무 필수에요",
          "original_form": "치킨무 필수에요",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.12",
          "form": "인정이요ㅠ",
          "original_form": "인정이요ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.13",
          "form": "후라이드가 짱인거같아요",
          "original_form": "후라이드가 짱인거같아요",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.14",
          "form": "그래서 그거랑 허니랑 먹으면 딱이에요",
          "original_form": "그래서 그거랑 허니랑 먹으면 딱이에요",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100033636.1.15",
