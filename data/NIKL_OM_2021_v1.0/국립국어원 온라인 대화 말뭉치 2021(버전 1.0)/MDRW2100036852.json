      "utterance": [
        {
          "id": "MDRW2100036852.1.1",
          "form": "얼죽아 이런말은 누가 만든걸까요",
          "original_form": "얼죽아 이런말은 누가 만든걸까요",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036852.1.2",
          "form": "재밌어요 ㅋㅋ",
          "original_form": "재밌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036852.1.3",
          "form": "전 한 겨울에도 냉장고 물 먹어요.",
          "original_form": "전 한 겨울에도 냉장고 물 먹어요.",
          "speaker_id": "2",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100036852.1.4",
          "form": "겨울에도 아아 팔면 좋겠는데 진상이라고 하겠죠 ㅋ",
          "original_form": "겨울에도 아아 팔면 좋겠는데 진상이라고 하겠죠 ㅋ",
          "speaker_id": "2",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100036852.1.5",
          "form": "헐 누가요",
          "original_form": "헐 누가요",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100036852.1.6",
          "form": "겨울에 팔지않나요?",
          "original_form": "겨울에 팔지않나요?",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100036852.1.7",
          "form": "아 그래요? 겨울에도 아이스커피 팔아요? 스벅 이런데서요?",
          "original_form": "아 그래요? 겨울에도 아이스커피 팔아요? 스벅 이런데서요?",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100036852.1.8",
          "form": "팔지않나요??? 계절 구별하는건 못봐서요",
          "original_form": "팔지않나요??? 계절 구별하는건 못봐서요",
          "speaker_id": "1",
          "time": "20210831 13:57"
        },
        {
          "id": "MDRW2100036852.1.9",
          "form": "그래요?? 겨울엔 따신 것만 파는 줄 알고 안가서 ㅋㅋㅋ",
          "original_form": "그래요?? 겨울엔 따신 것만 파는 줄 알고 안가서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100036852.1.10",
          "form": "아ㅡㅡ 바부같네요 엉엉",
          "original_form": "아ㅡㅡ 바부같네요 엉엉",
          "speaker_id": "2",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100036852.1.11",
          "form": "억 아니에요 아이스아메리카노 팔아요",
          "original_form": "억 아니에요 아이스아메리카노 팔아요",
          "speaker_id": "1",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100036852.1.12",
          "form": "커피믹스 좋아하세요?",
          "original_form": "커피믹스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100036852.1.13",
          "form": "남들 먹으면 따라먹는 정도요 ㅎㅎ",
          "original_form": "남들 먹으면 따라먹는 정도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100036852.1.14",
          "form": "슈프림골드 인가 새로 나온거 마셔보고 싶더라구요",
          "original_form": "슈프림골드 인가 새로 나온거 마셔보고 싶더라구요",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100036852.1.15",
          "form": "맥심에서 나왔어요?",
          "original_form": "맥심에서 나왔어요?",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100036852.1.16",
          "form": "네 맥심",
          "original_form": "네 맥심",
          "speaker_id": "2",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100036852.1.17",
          "form": "맛있을지 한 번 사봐야 겠어요",
          "original_form": "맛있을지 한 번 사봐야 겠어요",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036852.1.18",
          "form": "네 맛있으면 얘기해주세요 ㅎㅎ",
          "original_form": "네 맛있으면 얘기해주세요 ㅎㅎ",
          "speaker_id": "1",
