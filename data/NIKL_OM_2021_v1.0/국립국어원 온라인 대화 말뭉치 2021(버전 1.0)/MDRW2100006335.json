      "utterance": [
        {
          "id": "MDRW2100006335.1.1",
          "form": "선동렬이 투수",
          "original_form": "선동렬이 투수",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006335.1.2",
          "form": "투수들 너무 혹사시켜서",
          "original_form": "투수들 너무 혹사시켜서",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006335.1.3",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006335.1.4",
          "form": "뭐 다른감독도 다 그러지먀",
          "original_form": "뭐 다른감독도 다 그러지먀",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006335.1.5",
          "form": "만",
          "original_form": "만",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006335.1.6",
          "form": "투수 혹사는 어쩔 수 없는 부분일까요?ㅠ",
          "original_form": "투수 혹사는 어쩔 수 없는 부분일까요?ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006335.1.7",
          "form": "스트라이크 존 늘리면 좀 나을 것 같은데요. 경기 시간도 줄어들고",
          "original_form": "스트라이크 존 늘리면 좀 나을 것 같은데요. 경기 시간도 줄어들고",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006335.1.8",
          "form": "잘던지는투수를 계속 쓰게되니까",
          "original_form": "잘던지는투수를 계속 쓰게되니까",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006335.1.9",
          "form": "어쩔수없는거 같아요",
          "original_form": "어쩔수없는거 같아요",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006335.1.10",
          "form": "팀마다 잘 하는 투수는 몇 명 없고..",
          "original_form": "팀마다 잘 하는 투수는 몇 명 없고..",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100006335.1.11",
          "form": "요즘은 엘지나 kt가 투수 좋은 것 같더라구요",
          "original_form": "요즘은 엘지나 kt가 투수 좋은 것 같더라구요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100006335.1.12",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100006335.1.13",
          "form": "상위권이죠",
          "original_form": "상위권이죠",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100006335.1.14",
          "form": "올해가 kt우승 적기 같은데",
          "original_form": "올해가 kt우승 적기 같은데",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100006335.1.15",
          "form": "저도 kt에 한표ㅋㅋ",
          "original_form": "저도 kt에 한표ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100006335.1.16",
          "form": "슥은 ㅜㅜ",
          "original_form": "슥은 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006335.1.17",
          "form": "선발도없고",
          "original_form": "선발도없고",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006335.1.18",
          "form": "지금으로서는 건강만 했으면..ㅋㅋㅠ",
          "original_form": "지금으로서는 건강만 했으면..ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006335.1.19",
          "form": "추.신수가  와도",
          "original_form": "추.신수가  와도",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006335.1.20",
          "form": "투수없으니까",
          "original_form": "투수없으니까",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006335.1.21",
          "form": "안되네요 ㅜㅜ",
          "original_form": "안되네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006335.1.22",
          "form": "투수가 진짜 중요하더라구요ㅠ",
          "original_form": "투수가 진짜 중요하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006335.1.23",
          "form": "야구는 투수놀음",
          "original_form": "야구는 투수놀음",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006335.1.24",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006335.1.25",
          "form": "드래프트 때 좋은 선수 많이 건졌으면 좋겠네요 ㅎㅎ",
          "original_form": "드래프트 때 좋은 선수 많이 건졌으면 좋겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006335.1.26",
          "form": "물론 신인들은 두고 봐야지만요ㅋㅋ",
          "original_form": "물론 신인들은 두고 봐야지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006335.1.27",
          "form": "이번에 슥은 신인 투수 뽑았더라구요",
          "original_form": "이번에 슥은 신인 투수 뽑았더라구요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006335.1.28",
          "form": "사이드암 윤태현! ㅋㅋ 왜 이름도 다 알고 있는지",
          "original_form": "사이드암 윤태현! ㅋㅋ 왜 이름도 다 알고 있는지",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006335.1.29",
          "form": "서울에서 좋은선수들이 많이 나오는거 같아요",
          "original_form": "서울에서 좋은선수들이 많이 나오는거 같아요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006335.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006335.1.31",
          "form": "다아시네",
          "original_form": "다아시네",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006335.1.32",
          "form": "두산이 야구를 못하니까...",
          "original_form": "두산이 야구를 못하니까...",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006335.1.33",
          "form": "ㅋㅋㅋㅋㅋ 두산 지금까지 쭉 잘하다가 올해만 못하는거자나요",
          "original_form": "ㅋㅋㅋㅋㅋ 두산 지금까지 쭉 잘하다가 올해만 못하는거자나요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006335.1.34",
          "form": "한화를 생각하면....ㅋㅋ",
          "original_form": "한화를 생각하면....ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
