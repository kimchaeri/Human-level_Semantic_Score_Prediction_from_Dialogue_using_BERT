      "utterance": [
        {
          "id": "MDRW2100012059.1.1",
          "form": "가끔특산물소개할때",
          "original_form": "가끔특산물소개할때",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012059.1.2",
          "form": "농장주께서 여러 음식들만들고 소개할때",
          "original_form": "농장주께서 여러 음식들만들고 소개할때",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012059.1.3",
          "form": "그런게 나와요ㅋㅋ",
          "original_form": "그런게 나와요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012059.1.4",
          "form": "분명 드실거 같지 않은 메뉴들이",
          "original_form": "분명 드실거 같지 않은 메뉴들이",
          "speaker_id": "1",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012059.1.5",
          "form": "등장하는게 신기해요",
          "original_form": "등장하는게 신기해요",
          "speaker_id": "1",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012059.1.6",
          "form": "방송을 위한 제조일까 고민하게 됩니다",
          "original_form": "방송을 위한 제조일까 고민하게 됩니다",
          "speaker_id": "1",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012059.1.7",
          "form": "ㅋㅋ그런음식들은 리포터들이 보통 씹기도 전에 맛있다그러더라고요",
          "original_form": "ㅋㅋ그런음식들은 리포터들이 보통 씹기도 전에 맛있다그러더라고요",
          "speaker_id": "2",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012059.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ씹기전부터 맛있다니 정말 맛있나보네요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ씹기전부터 맛있다니 정말 맛있나보네요~",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012059.1.9",
          "form": "저번에 맛있는 녀석들에서도 시골 휴가편에서 아로니아밥 나와서 띠용 했었어요",
          "original_form": "저번에 맛있는 녀석들에서도 시골 휴가편에서 아로니아밥 나와서 띠용 했었어요",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012059.1.10",
          "form": "ㅋㅋㅋㅋㄱ 맛녀석들이 맛있게드셨나요",
          "original_form": "ㅋㅋㅋㅋㄱ 맛녀석들이 맛있게드셨나요",
          "speaker_id": "2",
          "time": "20210824 18:59"
        },
        {
          "id": "MDRW2100012059.1.11",
          "form": "네 아로니아밥뿐 아니라 고기반찬도 나와서",
          "original_form": "네 아로니아밥뿐 아니라 고기반찬도 나와서",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012059.1.12",
          "form": "잘 먹더라고요 ㅋㅋㅋ",
          "original_form": "잘 먹더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012059.1.13",
          "form": "일단 한솥 다 드시긴 했습니더",
          "original_form": "일단 한솥 다 드시긴 했습니더",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012059.1.14",
          "form": "보통 두솥 드시던데",
          "original_form": "보통 두솥 드시던데",
          "speaker_id": "2",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100012059.1.15",
          "form": "그런점에서 아로니아밥의 신뢰도가 조금 떨어지네요",
          "original_form": "그런점에서 아로니아밥의 신뢰도가 조금 떨어지네요",
          "speaker_id": "2",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100012059.1.16",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100012059.1.17",
          "form": "예리하세요",
          "original_form": "예리하세요",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012059.1.18",
          "form": "ㅋㅋ저는 조기 구이에  미역국 먹었습니다",
          "original_form": "ㅋㅋ저는 조기 구이에  미역국 먹었습니다",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012059.1.19",
          "form": "마치 생일상같은..",
          "original_form": "마치 생일상같은..",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012059.1.20",
          "form": "오늘 생선데이군요 굳굳",
          "original_form": "오늘 생선데이군요 굳굳",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012059.1.21",
          "form": "미역국에 뭐 넣으시나요!",
          "original_form": "미역국에 뭐 넣으시나요!",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012059.1.22",
          "form": "소고기입죠",
          "original_form": "소고기입죠",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012059.1.23",
          "form": "근본아입니까",
          "original_form": "근본아입니까",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012059.1.24",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "1",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012059.1.25",
          "form": "근본있는 미역국이네요",
          "original_form": "근본있는 미역국이네요",
          "speaker_id": "1",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012059.1.26",
          "form": "사실 아버지 생신인데",
          "original_form": "사실 아버지 생신인데",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012059.1.27",
          "form": "아무리봐도 매미님 저녁식단 양반댁 메뉴에요",
          "original_form": "아무리봐도 매미님 저녁식단 양반댁 메뉴에요",
          "speaker_id": "1",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012059.1.28",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012059.1.29",
          "form": "아버지 시골 가셔서",
          "original_form": "아버지 시골 가셔서",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012059.1.30",
          "form": "앗 정말 생신이시군요! 축하드립니다",
          "original_form": "앗 정말 생신이시군요! 축하드립니다",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012059.1.31",
          "form": "아버지 없는 아버지 생신상...",
          "original_form": "아버지 없는 아버지 생신상...",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012059.1.32",
          "form": "주인없는 고깃국..!",
          "original_form": "주인없는 고깃국..!",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012059.1.33",
          "form": "먹는 사람이 임자 아니겠습니다 불효자같나요",
          "original_form": "먹는 사람이 임자 아니겠습니다 불효자같나요",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012059.1.34",
          "form": "매미님이 잘 드시고 건강하면",
          "original_form": "매미님이 잘 드시고 건강하면",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012059.1.35",
          "form": "그게 효도아닌가요?",
          "original_form": "그게 효도아닌가요?",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012059.1.36",
          "form": "진정한 효를 아시는 분이네요",
          "original_form": "진정한 효를 아시는 분이네요",
          "speaker_id": "2",
          "time": "20210824 19:07"
        }
      ]
    }
