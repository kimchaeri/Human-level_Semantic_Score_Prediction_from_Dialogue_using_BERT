      "utterance": [
        {
          "id": "MDRW2100044653.1.1",
          "form": "커피는 아아만 마시나",
          "original_form": "커피는 아아만 마시나",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044653.1.2",
          "form": "그건 아닌데",
          "original_form": "그건 아닌데",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044653.1.3",
          "form": "라떼도 죻아",
          "original_form": "라떼도 죻아",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044653.1.4",
          "form": "올 글쿠나 바닐라라떼는?",
          "original_form": "올 글쿠나 바닐라라떼는?",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044653.1.5",
          "form": "바닐라라떼는 그 맛있는 거 말구",
          "original_form": "바닐라라떼는 그 맛있는 거 말구",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044653.1.6",
          "form": "인공적인 단맛 나는 건 좀...",
          "original_form": "인공적인 단맛 나는 건 좀...",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044653.1.7",
          "form": "아 그런거잇다 맛없는 바닐라시럽쓰는곳",
          "original_form": "아 그런거잇다 맛없는 바닐라시럽쓰는곳",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044653.1.8",
          "form": "ㅇㅇ그런건 너무 맛없음",
          "original_form": "ㅇㅇ그런건 너무 맛없음",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044653.1.9",
          "form": "입맛배려",
          "original_form": "입맛배려",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044653.1.10",
          "form": "마자 거기핸즈커피가곱넼ㅋㅋㅋ",
          "original_form": "마자 거기핸즈커피가곱넼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044653.1.11",
          "form": "ㅋㅋㅋ핸즈커피 맛있징ㅇㅇ",
          "original_form": "ㅋㅋㅋ핸즈커피 맛있징ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044653.1.12",
          "form": "요즘도 집에서 커피 내려마시나",
          "original_form": "요즘도 집에서 커피 내려마시나",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044653.1.13",
          "form": "마자마자 모카포트 최고시다",
          "original_form": "마자마자 모카포트 최고시다",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044653.1.14",
          "form": "오 모카포트 잘 쓰고 있구만",
          "original_form": "오 모카포트 잘 쓰고 있구만",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044653.1.15",
          "form": "마자마자 너도 모카포트이용해바",
          "original_form": "마자마자 너도 모카포트이용해바",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044653.1.16",
          "form": "원두 골라먹는재미잇다",
          "original_form": "원두 골라먹는재미잇다",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044653.1.17",
          "form": "ㅋㅋ그거 가스레인지로 하는거 아녀?",
          "original_form": "ㅋㅋ그거 가스레인지로 하는거 아녀?",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044653.1.18",
          "form": "마저마저 근데 그거뭐냐 인덕션용도잇오",
          "original_form": "마저마저 근데 그거뭐냐 인덕션용도잇오",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044653.1.19",
          "form": "오 근데 인덕션 못미더운뎈ㅋㅋㅋㅋㅋ",
          "original_form": "오 근데 인덕션 못미더운뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        }
