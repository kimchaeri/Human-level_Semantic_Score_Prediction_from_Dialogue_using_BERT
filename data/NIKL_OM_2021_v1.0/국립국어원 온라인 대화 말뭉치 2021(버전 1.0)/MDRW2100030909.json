      "utterance": [
        {
          "id": "MDRW2100030909.1.1",
          "form": "귀찮아서 나가기 힘들어요",
          "original_form": "귀찮아서 나가기 힘들어요",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030909.1.2",
          "form": "코로나때문에 가기가 그러네요",
          "original_form": "코로나때문에 가기가 그러네요",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030909.1.3",
          "form": "무조건 많이 발라 줘야 될 거 같아요",
          "original_form": "무조건 많이 발라 줘야 될 거 같아요",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.4",
          "form": "팩이라도 해야 할 텐데 집에 수면팩 또 있고 올리는 팩도 있는데 잘 안 하게 돼요",
          "original_form": "팩이라도 해야 할 텐데 집에 수면팩 또 있고 올리는 팩도 있는데 잘 안 하게 돼요",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.5",
          "form": "크림이랑 수면팩",
          "original_form": "크림이랑 수면팩",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.6",
          "form": "맞아요 귀찮지 귀차니즘 보단 코르나 때문인 것 같아요",
          "original_form": "맞아요 귀찮지 귀차니즘 보단 코르나 때문인 것 같아요",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.7",
          "form": "수면팩 편하긴 해요 그런데 그거 바르는 것도 참",
          "original_form": "수면팩 편하긴 해요 그런데 그거 바르는 것도 참",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.8",
          "form": "건조해져요 몸도",
          "original_form": "건조해져요 몸도",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.9",
          "form": "앞머리가 있다 보니 바르기가 쉽지 않네요",
          "original_form": "앞머리가 있다 보니 바르기가 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.10",
          "form": "그것마저 귀찮긴 하죠",
          "original_form": "그것마저 귀찮긴 하죠",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.11",
          "form": "부지런 해야 하고 앞머리 머리띠 하세요",
          "original_form": "부지런 해야 하고 앞머리 머리띠 하세요",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.12",
          "form": "샤워할 때 매번 샤워젤 쓰지 마시고 물로 꼼꼼히 씻어도 괜찮아요",
          "original_form": "샤워할 때 매번 샤워젤 쓰지 마시고 물로 꼼꼼히 씻어도 괜찮아요",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.13",
          "form": "저는 원래 비누로만 씻어요",
          "original_form": "저는 원래 비누로만 씻어요",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030909.1.14",
          "form": "요즘에는 너무 더워서 머리를 묶고 다닌다고 질렀는데요",
          "original_form": "요즘에는 너무 더워서 머리를 묶고 다닌다고 질렀는데요",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030909.1.15",
          "form": "바디로션안 바르지만",
          "original_form": "바디로션안 바르지만",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030909.1.16",
          "form": "머리를 질렀는데요",
          "original_form": "머리를 질렀는데요",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030909.1.17",
          "form": "길",
          "original_form": "길",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030909.1.18",
          "form": "기르니까 편한가요",
          "original_form": "기르니까 편한가요",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030909.1.19",
          "form": "바람이 차가워 지니 다시 머리를 자르고 싶네요",
          "original_form": "바람이 차가워 지니 다시 머리를 자르고 싶네요",
          "speaker_id": "2",
          "time": "20210904 14:10"
        }
      ]
