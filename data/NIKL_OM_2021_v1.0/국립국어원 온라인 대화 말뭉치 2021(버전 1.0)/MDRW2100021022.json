      "utterance": [
        {
          "id": "MDRW2100021022.1.1",
          "form": "운동 조아하시나요",
          "original_form": "운동 조아하시나요",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100021022.1.2",
          "form": "네 좋아하죠!",
          "original_form": "네 좋아하죠!",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021022.1.3",
          "form": "어떤운동하세요?",
          "original_form": "어떤운동하세요?",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021022.1.4",
          "form": "축구같은 구기종목 주로해요!",
          "original_form": "축구같은 구기종목 주로해요!",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100021022.1.5",
          "form": "오 조기축구나가시나요",
          "original_form": "오 조기축구나가시나요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021022.1.6",
          "form": "할까했는데 코로나땜에 포기했죠 ㅠㅠ",
          "original_form": "할까했는데 코로나땜에 포기했죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021022.1.7",
          "form": "옛날 학창시절때 축구엄청 좋아해서 자주하긴했는데",
          "original_form": "옛날 학창시절때 축구엄청 좋아해서 자주하긴했는데",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021022.1.8",
          "form": "마자요 코로나땜에ㅠ",
          "original_form": "마자요 코로나땜에ㅠ",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021022.1.9",
          "form": "그래서 요즘은",
          "original_form": "그래서 요즘은",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021022.1.10",
          "form": "홈트만 하고있네요",
          "original_form": "홈트만 하고있네요",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021022.1.11",
          "form": "오 저도 홈트시작하려구하는데",
          "original_form": "오 저도 홈트시작하려구하는데",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021022.1.12",
          "form": "유튜브 있는거 보면서 하구있어요!",
          "original_form": "유튜브 있는거 보면서 하구있어요!",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021022.1.13",
          "form": "뭐가 좋은질 잘 모르겠어서 ㅠㅠ",
          "original_form": "뭐가 좋은질 잘 모르겠어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021022.1.14",
          "form": "저는 푸시업바랑 문틈철봉샀어요",
          "original_form": "저는 푸시업바랑 문틈철봉샀어요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021022.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021022.1.16",
          "form": "문틈철봉 조심하세요! 그거 사고 엄청 일어난다고 하더라구여 ㅠㅠ",
          "original_form": "문틈철봉 조심하세요! 그거 사고 엄청 일어난다고 하더라구여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021022.1.17",
          "form": "아 맞아요떨어지면ㅠㅠ",
          "original_form": "아 맞아요떨어지면ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:09"
        }
