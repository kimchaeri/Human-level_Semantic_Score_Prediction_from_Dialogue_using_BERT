      "utterance": [
        {
          "id": "MDRW2100019478.1.1",
          "form": "안녕하세요 날씨가 많이 흐리죠ㅠ",
          "original_form": "안녕하세요 날씨가 많이 흐리죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019478.1.2",
          "form": "네 여기는 비가 오다 안오다 하네요 ㅠ",
          "original_form": "네 여기는 비가 오다 안오다 하네요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019478.1.3",
          "form": "거긴 어떤가요",
          "original_form": "거긴 어떤가요",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019478.1.4",
          "form": "낮에는 부슬비가 내렸는데 지금은 그쳤어요",
          "original_form": "낮에는 부슬비가 내렸는데 지금은 그쳤어요",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019478.1.5",
          "form": "여긴 저녁에도 잠깐 비가 왔어요",
          "original_form": "여긴 저녁에도 잠깐 비가 왔어요",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019478.1.6",
          "form": "춥진 않으세요? 저는 긴팔 긴바지 입고 있어요",
          "original_form": "춥진 않으세요? 저는 긴팔 긴바지 입고 있어요",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019478.1.7",
          "form": "저도 긴팔이랑 긴바지 입어요",
          "original_form": "저도 긴팔이랑 긴바지 입어요",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019478.1.8",
          "form": "금방 가을인거같아요",
          "original_form": "금방 가을인거같아요",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019478.1.9",
          "form": "곧있음 낙엽 지겠죠? 시간 빠르네요",
          "original_form": "곧있음 낙엽 지겠죠? 시간 빠르네요",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019478.1.10",
          "form": "아직 8월인데 날씨 체감은 9월이네요 ㅋㅋㅋ",
          "original_form": "아직 8월인데 날씨 체감은 9월이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019478.1.11",
          "form": "맞아요 아직 8월이었죠",
          "original_form": "맞아요 아직 8월이었죠",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019478.1.12",
          "form": "벌써 이렇게 싸늘하면 겨울은 어떨지 무서워요",
          "original_form": "벌써 이렇게 싸늘하면 겨울은 어떨지 무서워요",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019478.1.13",
          "form": "그쵸 작년에도 겨울이 추웠어서 좀 걱정이에요",
          "original_form": "그쵸 작년에도 겨울이 추웠어서 좀 걱정이에요",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019478.1.14",
          "form": "옷을 새로 사둬야 할까봐요",
          "original_form": "옷을 새로 사둬야 할까봐요",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019478.1.15",
          "form": "저는 롱패딩 안으로 껴입을거 장만하려구요",
          "original_form": "저는 롱패딩 안으로 껴입을거 장만하려구요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019478.1.16",
          "form": "니트같은거요?",
          "original_form": "니트같은거요?",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019478.1.17",
          "form": "니트나 기모들어간 옷이요",
          "original_form": "니트나 기모들어간 옷이요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019478.1.18",
          "form": "요즘은 얇은 목폴라도 잘나오더라구요",
          "original_form": "요즘은 얇은 목폴라도 잘나오더라구요",
          "speaker_id": "2",
          "time": "20210827 19:50"
        }
      ]
