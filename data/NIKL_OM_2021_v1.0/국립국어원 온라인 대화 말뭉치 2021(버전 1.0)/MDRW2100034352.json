      "utterance": [
        {
          "id": "MDRW2100034352.1.1",
          "form": "늦게까지 먹으니 아직까지 배부르네요",
          "original_form": "늦게까지 먹으니 아직까지 배부르네요",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100034352.1.2",
          "form": "앜ㅋㅋㅋ포만감있고 야식생각안나니 좋져뭐~",
          "original_form": "앜ㅋㅋㅋ포만감있고 야식생각안나니 좋져뭐~",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100034352.1.3",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100034352.1.4",
          "form": "후식으로는 설빙을 먹었어요ㅋㅋ",
          "original_form": "후식으로는 설빙을 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100034352.1.5",
          "form": "오 빙수좋아하시나봐요!",
          "original_form": "오 빙수좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100034352.1.6",
          "form": "추울수록 생각나는거 같아요ㅋㅋ",
          "original_form": "추울수록 생각나는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100034352.1.7",
          "form": "아하 ㅋㅋ 마치라잌 여름에삼계탕!",
          "original_form": "아하 ㅋㅋ 마치라잌 여름에삼계탕!",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100034352.1.8",
          "form": "넹ㅋㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100034352.1.9",
          "form": "겨울엔 냉면!",
          "original_form": "겨울엔 냉면!",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100034352.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 마쟈요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 마쟈요~",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100034352.1.11",
          "form": "냉면은 물냉파 혹은 비냉파??",
          "original_form": "냉면은 물냉파 혹은 비냉파??",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100034352.1.12",
          "form": "비냉이요!",
          "original_form": "비냉이요!",
          "speaker_id": "2",
          "time": "20210828 06:32"
        },
        {
          "id": "MDRW2100034352.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 06:33"
        },
        {
          "id": "MDRW2100034352.1.14",
          "form": "오~ 저두요!!",
          "original_form": "오~ 저두요!!",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100034352.1.15",
          "form": "자고로 양념파라 ㅎㅎ",
          "original_form": "자고로 양념파라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100034352.1.16",
          "form": "크으 양념 최고죠",
          "original_form": "크으 양념 최고죠",
          "speaker_id": "2",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100034352.1.17",
          "form": "근데 생각보다 맛있는곳이 많이 없어요ㅜ",
          "original_form": "근데 생각보다 맛있는곳이 많이 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100034352.1.18",
          "form": "맞아요.. 저도 냉면맛집은 아는곳이 잘 없어요",
