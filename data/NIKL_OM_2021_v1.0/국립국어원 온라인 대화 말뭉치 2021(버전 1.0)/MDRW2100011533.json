      "utterance": [
        {
          "id": "MDRW2100011533.1.1",
          "form": "오늘 습도 엄청 높지 ㅎㅎ",
          "original_form": "오늘 습도 엄청 높지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011533.1.2",
          "form": "ㅇㅇ 집안 습도가 88프로야",
          "original_form": "ㅇㅇ 집안 습도가 88프로야",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011533.1.3",
          "form": "훌 보일러 좀 돌려",
          "original_form": "훌 보일러 좀 돌려",
          "speaker_id": "2",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011533.1.4",
          "form": "보일러 돌리면 더울텐데",
          "original_form": "보일러 돌리면 더울텐데",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011533.1.5",
          "form": "그 정도면 에어컨 제습도 같이 돌려야겠는데",
          "original_form": "그 정도면 에어컨 제습도 같이 돌려야겠는데",
          "speaker_id": "2",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011533.1.6",
          "form": "와 진짜.. 집 안에서 비오게 생김",
          "original_form": "와 진짜.. 집 안에서 비오게 생김",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011533.1.7",
          "form": "장난 아니네..",
          "original_form": "장난 아니네..",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011533.1.8",
          "form": "최근에 90프로 육박한 건 처음 보는 것 같아",
          "original_form": "최근에 90프로 육박한 건 처음 보는 것 같아",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011533.1.9",
          "form": "거의 구름 속에 있는 수준 ㅎㅎ",
          "original_form": "거의 구름 속에 있는 수준 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011533.1.10",
          "form": "진짜 제습기 하나 사던지 해야지 벽지 다 들떴어",
          "original_form": "진짜 제습기 하나 사던지 해야지 벽지 다 들떴어",
          "speaker_id": "1",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011533.1.11",
          "form": "그러다 벽지 떨어지겠어 ㅠ",
          "original_form": "그러다 벽지 떨어지겠어 ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011533.1.12",
          "form": "여기저기 우글거려 ㅠㅜ",
          "original_form": "여기저기 우글거려 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011533.1.13",
          "form": "훌.. 해뜨면 펴지겠지",
          "original_form": "훌.. 해뜨면 펴지겠지",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011533.1.14",
          "form": "이번주 내내 습도가 이러면 안펴질듯",
          "original_form": "이번주 내내 습도가 이러면 안펴질듯",
