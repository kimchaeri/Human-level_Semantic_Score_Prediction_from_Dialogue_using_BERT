      "utterance": [
        {
          "id": "MDRW2100005643.1.1",
          "form": "글치",
          "original_form": "글치",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005643.1.2",
          "form": "아까 name3이 딱밤 때린거 미안해죽긋네",
          "original_form": "아까 &name3&이 딱밤 때린거 미안해죽긋네",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005643.1.3",
          "form": "내가 맞아보니 너무 아프노 ㅠ",
          "original_form": "내가 맞아보니 너무 아프노 ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005643.1.4",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005643.1.5",
          "form": "언니 힘조절못함",
          "original_form": "언니 힘조절못함",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005643.1.6",
          "form": "그래 당황했다 ㅠ",
          "original_form": "그래 당황했다 ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005643.1.7",
          "form": "애교로 쿵했는데ㅋ",
          "original_form": "애교로 쿵했는데ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005643.1.8",
          "form": "애 당황",
          "original_form": "애 당황",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005643.1.9",
          "form": "울뻔ㅋㅋ",
          "original_form": "울뻔ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005643.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005643.1.11",
          "form": "그래 눈물 흘릴뻔했도",
          "original_form": "그래 눈물 흘릴뻔했도",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005643.1.12",
          "form": "내가 맞아도 아픈데 ㅠ",
          "original_form": "내가 맞아도 아픈데 ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005643.1.13",
          "form": "그래 눈피했다.",
          "original_form": "그래 눈피했다.",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005643.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005643.1.15",
          "form": "보고있음 울까싶어서ㅋ",
          "original_form": "보고있음 울까싶어서ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005643.1.16",
          "form": "그래 잘했다\r\nㅋㅋㅋㅋㄱㄱ",
          "original_form": "그래 잘했다\r\nㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005643.1.17",
          "form": "결국 미끄러져서 울었지만",
          "original_form": "결국 미끄러져서 울었지만",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005643.1.18",
          "form": "나름 참았음",
          "original_form": "나름 참았음",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005643.1.19",
          "form": "ㅋㅋ맞아. ㅋㅋㅋ 참는거보니 큰것같더라",
          "original_form": "ㅋㅋ맞아. ㅋㅋㅋ 참는거보니 큰것같더라",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005643.1.20",
          "form": "언니야더라ㅋㅋㅋ",
          "original_form": "언니야더라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005643.1.21",
          "form": "요새 둘다 컸다 \r\n둘이 서로 맞춰주면서 놀고있음",
          "original_form": "요새 둘다 컸다 \r\n둘이 서로 맞춰주면서 놀고있음",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005643.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㄱ 5살 언니야지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㄱ 5살 언니야지",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005643.1.23",
          "form": "요새 좐이가 진짜 많이\r\n컸다고 느끼는게",
          "original_form": "요새 좐이가 진짜 많이\r\n컸다고 느끼는게",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.24",
          "form": "맞아.ㅋ응응",
          "original_form": "맞아.ㅋ응응",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.25",
          "form": "동생한테 배려를 많이함",
          "original_form": "동생한테 배려를 많이함",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.26",
          "form": "참는것도 보이고",
          "original_form": "참는것도 보이고",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.27",
          "form": "배려도 많이하고",
          "original_form": "배려도 많이하고",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.28",
          "form": "응 그런것같더라ㅋㅋ",
          "original_form": "응 그런것같더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.29",
          "form": "그냥 보기에도 많이컸어ㅋ",
          "original_form": "그냥 보기에도 많이컸어ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.30",
          "form": "맞춰주려고 하는게 보임",
          "original_form": "맞춰주려고 하는게 보임",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.31",
          "form": "엉 ㅋㅋㅋㅋㄱㅋㄱ",
          "original_form": "엉 ㅋㅋㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005643.1.32",
          "form": "응응ㅋㅋㅋ 문자쓰는것도 일기쓰는것도 잘하더라",
          "original_form": "응응ㅋㅋㅋ 문자쓰는것도 일기쓰는것도 잘하더라",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005643.1.33",
          "form": "둘이 같이 있어도 잘 안싸우니까 요새는 좀 살만하다",
          "original_form": "둘이 같이 있어도 잘 안싸우니까 요새는 좀 살만하다",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005643.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋ 일기는 같이 쓰고 있다 혼자가 아니고\r\nㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 일기는 같이 쓰고 있다 혼자가 아니고\r\nㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005643.1.35",
          "form": "그래도 글씨도 이쁘고ㅋ",
          "original_form": "그래도 글씨도 이쁘고ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005643.1.36",
          "form": "아까도 숙제땜에 또 한바탕하고",
          "original_form": "아까도 숙제땜에 또 한바탕하고",
          "speaker_id": "2",
          "time": "20210821 23:37"
        }
      ]
    }
