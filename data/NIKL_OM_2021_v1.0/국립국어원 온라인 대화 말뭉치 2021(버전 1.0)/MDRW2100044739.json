        },
        {
          "id": "MDRW2100044739.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044739.1.3",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044739.1.4",
          "form": "아뇨 졸업했어요 name1님은 학생이세요?",
          "original_form": "아뇨 졸업했어요 &name1&님은 학생이세요?",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044739.1.5",
          "form": "저도 졸업하고 일하고있습니당",
          "original_form": "저도 졸업하고 일하고있습니당",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044739.1.6",
          "form": "ㅎㅎ 학생때가 좋았죠..",
          "original_form": "ㅎㅎ 학생때가 좋았죠..",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044739.1.7",
          "form": "대학생때 생활어떠셨어요?",
          "original_form": "대학생때 생활어떠셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044739.1.8",
          "form": "전 별로였어요 ㅠ",
          "original_form": "전 별로였어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044739.1.9",
          "form": "흠 그랬나요? 저는 좀 심심하게 보냈어요",
          "original_form": "흠 그랬나요? 저는 좀 심심하게 보냈어요",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044739.1.10",
          "form": "그럼 어느 학창시절이 젤 좋으셨어요?",
          "original_form": "그럼 어느 학창시절이 젤 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044739.1.11",
          "form": "고등학교요! 님은요?",
          "original_form": "고등학교요! 님은요?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044739.1.12",
          "form": "저도 굳이 고르면 고등학교같아요",
          "original_form": "저도 굳이 고르면 고등학교같아요",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044739.1.13",
          "form": "큰 근심걱정이 없었던거같아요",
          "original_form": "큰 근심걱정이 없었던거같아요",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044739.1.14",
          "form": "지금도 고딩친구들만 남은거같아요 주변에 ㅎㅎ",
          "original_form": "지금도 고딩친구들만 남은거같아요 주변에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044739.1.15",
          "form": "오 고등학교는 어떠셨어요?",
          "original_form": "오 고등학교는 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044739.1.16",
          "form": "너무 재밌었어요 ㅎㅎ",
          "original_form": "너무 재밌었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044739.1.17",
          "form": "고딩때 넘 재밌었어요",
          "original_form": "고딩때 넘 재밌었어요",
