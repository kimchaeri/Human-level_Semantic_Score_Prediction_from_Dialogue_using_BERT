      "utterance": [
        {
          "id": "MDRW2100003161.1.1",
          "form": "요즘 날씨가 많이 선선해진것 같아요~",
          "original_form": "요즘 날씨가 많이 선선해진것 같아요~",
          "speaker_id": "1",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003161.1.2",
          "form": "맞아요 오늘 아침에는 비왔더라구요",
          "original_form": "맞아요 오늘 아침에는 비왔더라구요",
          "speaker_id": "2",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003161.1.3",
          "form": "이번 여름은 짧고 굵네요 ㅎㅎ",
          "original_form": "이번 여름은 짧고 굵네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003161.1.4",
          "form": "엇... 비가 왔었나요??",
          "original_form": "엇... 비가 왔었나요??",
          "speaker_id": "1",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003161.1.5",
          "form": "네 그리고 이번주는 2차 장마래요",
          "original_form": "네 그리고 이번주는 2차 장마래요",
          "speaker_id": "2",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003161.1.6",
          "form": "우산 갖고 다녀야겠네요~",
          "original_form": "우산 갖고 다녀야겠네요~",
          "speaker_id": "1",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003161.1.7",
          "form": "주말에 다 비오고 월요일만 비 안오고 화수 비오더라구요",
          "original_form": "주말에 다 비오고 월요일만 비 안오고 화수 비오더라구요",
          "speaker_id": "2",
          "time": "20210820 19:56"
        },
        {
          "id": "MDRW2100003161.1.8",
          "form": "아.. 몰랐어요 알려주셔서 감사합니당",
          "original_form": "아.. 몰랐어요 알려주셔서 감사합니당",
          "speaker_id": "1",
          "time": "20210820 19:56"
        },
        {
          "id": "MDRW2100003161.1.9",
          "form": "주말인데 비오다니.. 아쉽네요 ㅠ",
          "original_form": "주말인데 비오다니.. 아쉽네요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003161.1.10",
          "form": "네 근데 기상청 아시잖아요\r\n비가 와야 오는거...",
          "original_form": "네 근데 기상청 아시잖아요\r\n비가 와야 오는거...",
          "speaker_id": "2",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003161.1.11",
          "form": "아 그렇죠 ㅎㅎ",
          "original_form": "아 그렇죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003161.1.12",
          "form": "오전만 잠깐 오고 오후엔 맑았으면 좋겠네용",
          "original_form": "오전만 잠깐 오고 오후엔 맑았으면 좋겠네용",
          "speaker_id": "1",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003161.1.13",
          "form": "여름에 비가 너무 안와서 농작물 가격이 많이 올랐더라구요",
          "original_form": "여름에 비가 너무 안와서 농작물 가격이 많이 올랐더라구요",
          "speaker_id": "2",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003161.1.14",
          "form": "맞아요....채소값이 후덜덜..!!",
          "original_form": "맞아요....채소값이 후덜덜..!!",
          "speaker_id": "1",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100003161.1.15",
          "form": "수박값 보셨어요?",
          "original_form": "수박값 보셨어요?",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003161.1.16",
          "form": "1통에 3만원 주고 사왔어요",
          "original_form": "1통에 3만원 주고 사왔어요",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003161.1.17",
          "form": "아뇨...?",
          "original_form": "아뇨...?",
          "speaker_id": "1",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003161.1.18",
          "form": "헉.......",
          "original_form": "헉.......",
          "speaker_id": "1",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003161.1.19",
          "form": "와 ㅎㅎㅎㅎ 대받이다",
          "original_form": "와 ㅎㅎㅎㅎ 대받이다",
          "speaker_id": "1",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003161.1.20",
          "form": "3만원이라니!!",
          "original_form": "3만원이라니!!",
          "speaker_id": "1",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003161.1.21",
          "form": "그것도 날이 너무 더워서 수박이 상해서 그런거래요",
          "original_form": "그것도 날이 너무 더워서 수박이 상해서 그런거래요",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003161.1.22",
          "form": "올해는 미국도 너무 더워서 체리값도 올랐더라구요",
          "original_form": "올해는 미국도 너무 더워서 체리값도 올랐더라구요",
          "speaker_id": "2",
          "time": "20210820 20:01"
        },
        {
          "id": "MDRW2100003161.1.23",
          "form": "ㅠㅠ그래도 더위가 빨리 가서 다행이네요...",
          "original_form": "ㅠㅠ그래도 더위가 빨리 가서 다행이네요...",
          "speaker_id": "1",
          "time": "20210820 20:01"
        },
        {
          "id": "MDRW2100003161.1.24",
          "form": "이젠 집에서 키울 수 있는 채소는 키우는게 나을거 같기도 하구요",
          "original_form": "이젠 집에서 키울 수 있는 채소는 키우는게 나을거 같기도 하구요",
          "speaker_id": "1",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003161.1.25",
          "form": "저희집은 키우는데... 콩이 너무 비가 안와서 말라가요",
          "original_form": "저희집은 키우는데... 콩이 너무 비가 안와서 말라가요",
          "speaker_id": "2",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003161.1.26",
          "form": "아....진짜 심각하네요 지구온난화..",
          "original_form": "아....진짜 심각하네요 지구온난화..",
          "speaker_id": "1",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003161.1.27",
          "form": "비가 빨리 와야돼요 ㅜ ㅜ",
          "original_form": "비가 빨리 와야돼요 ㅜ ㅜ",
          "speaker_id": "2",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003161.1.28",
          "form": "ㅠㅠ네 2차 장마 온다니까... 좀 숨통이 트이겠네요",
          "original_form": "ㅠㅠ네 2차 장마 온다니까... 좀 숨통이 트이겠네요",
          "speaker_id": "1",
          "time": "20210820 20:04"
        },
        {
          "id": "MDRW2100003161.1.29",
          "form": "네 좀 와야돼요 ㅜ ㅜ 많이..\r\n계란값도 날이 너무 더워서 그렇다고 하기는 하잖아요",
          "original_form": "네 좀 와야돼요 ㅜ ㅜ 많이..\r\n계란값도 날이 너무 더워서 그렇다고 하기는 하잖아요",
          "speaker_id": "2",
          "time": "20210820 20:05"
        }
      ]
