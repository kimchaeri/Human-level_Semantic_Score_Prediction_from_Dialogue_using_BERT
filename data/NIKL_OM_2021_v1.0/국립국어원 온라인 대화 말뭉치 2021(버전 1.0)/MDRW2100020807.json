      "utterance": [
        {
          "id": "MDRW2100020807.1.1",
          "form": "크록스 살꺼면 말해",
          "original_form": "크록스 살꺼면 말해",
          "speaker_id": "2",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020807.1.2",
          "form": "한국에 8만얼만데 3만9천으로 삼",
          "original_form": "한국에 8만얼만데 3만9천으로 삼",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020807.1.3",
          "form": "ㄷㄷㄷ",
          "original_form": "ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020807.1.4",
          "form": "일과 사랑..사랑과 일..",
          "original_form": "일과 사랑..사랑과 일..",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100020807.1.5",
          "form": "일만 하자나 ㅋㅋㅋㅋㅋ",
          "original_form": "일만 하자나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020807.1.6",
          "form": "으앙너무잤어",
          "original_form": "으앙너무잤어",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100020807.1.7",
          "form": "ㅋㅋㅋㅋㅋ그래야 피로가 풀려",
          "original_form": "ㅋㅋㅋㅋㅋ그래야 피로가 풀려",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100020807.1.8",
          "form": "손톱 할려다 나도스루하고 영상이나 봄",
          "original_form": "손톱 할려다 나도스루하고 영상이나 봄",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100020807.1.9",
          "form": "내일출근하기시러!",
          "original_form": "내일출근하기시러!",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100020807.1.10",
          "form": "근데너무멀쩡해서병가도못쓰겟어",
          "original_form": "근데너무멀쩡해서병가도못쓰겟어",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100020807.1.11",
          "form": "안아파??",
          "original_form": "안아파??",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100020807.1.12",
          "form": "왜 나도 열나는거 같아 가니 멀쩡이라곸ㅋㅋㅋㅋㅋ",
          "original_form": "왜 나도 열나는거 같아 가니 멀쩡이라곸ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100020807.1.13",
          "form": "진료서써줬자나",
          "original_form": "진료서써줬자나",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100020807.1.14",
          "form": "나두 일 가기시르다",
          "original_form": "나두 일 가기시르다",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100020807.1.15",
          "form": "열도안나고",
          "original_form": "열도안나고",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100020807.1.16",
          "form": "근육통도없음ㅋㅋㅋ",
          "original_form": "근육통도없음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100020807.1.17",
          "form": "일가기시러잉",
          "original_form": "일가기시러잉",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100020807.1.18",
          "form": "후훞",
          "original_form": "후훞",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100020807.1.19",
          "form": "건강해",
          "original_form": "건강해",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100020807.1.20",
          "form": "2차때를 위해 애껴둬 연차",
          "original_form": "2차때를 위해 애껴둬 연차",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100020807.1.21",
          "form": "나 2차때 대체공휴라서",
          "original_form": "나 2차때 대체공휴라서",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100020807.1.22",
          "form": "일주일땡겨도 대체공흌ㅋㅋㅋ",
          "original_form": "일주일땡겨도 대체공흌ㅋㅋㅋ",
