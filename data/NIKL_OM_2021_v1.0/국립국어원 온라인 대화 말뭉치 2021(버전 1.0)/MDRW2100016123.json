      "utterance": [
        {
          "id": "MDRW2100016123.1.1",
          "form": "그럼 오늘은 다른거 먹어야겠네",
          "original_form": "그럼 오늘은 다른거 먹어야겠네",
          "speaker_id": "2",
          "time": "20210826 09:54"
        },
        {
          "id": "MDRW2100016123.1.2",
          "form": "냉장고에 삼겹살 있으니 그거랑 된장찌개 먹을까해",
          "original_form": "냉장고에 삼겹살 있으니 그거랑 된장찌개 먹을까해",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016123.1.3",
          "form": "ㅎㅎ 맛있겠네",
          "original_form": "ㅎㅎ 맛있겠네",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016123.1.4",
          "form": "삼겹살이랑 명이나물 촵촵",
          "original_form": "삼겹살이랑 명이나물 촵촵",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016123.1.5",
          "form": "후라이팬에 굽게?",
          "original_form": "후라이팬에 굽게?",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016123.1.6",
          "form": "아니 에어프라이어",
          "original_form": "아니 에어프라이어",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016123.1.7",
          "form": "ㅎㅎ 에프 잘 사용하네",
          "original_form": "ㅎㅎ 에프 잘 사용하네",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016123.1.8",
          "form": "아 그러고보니 에어프라이어 씻어놔야겠다",
          "original_form": "아 그러고보니 에어프라이어 씻어놔야겠다",
          "speaker_id": "1",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100016123.1.9",
          "form": "왜? 뭐 해먹었었나?",
          "original_form": "왜? 뭐 해먹었었나?",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100016123.1.10",
          "form": "고구마 돌려먹었지 ㅎㅎ",
          "original_form": "고구마 돌려먹었지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100016123.1.11",
          "form": "아 ㅋㅋ 그냥 종이호일 깔고 돌려도 될듯",
          "original_form": "아 ㅋㅋ 그냥 종이호일 깔고 돌려도 될듯",
          "speaker_id": "2",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016123.1.12",
          "form": "10시 반 넘으면 준비해야겠네",
          "original_form": "10시 반 넘으면 준비해야겠네",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016123.1.13",
          "form": "ㅇㅇ 색시도 오전엔 좀 쉬어야지",
          "original_form": "ㅇㅇ 색시도 오전엔 좀 쉬어야지",
          "speaker_id": "2",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016123.1.14",
          "form": "쉬긴 ㅎㅎ 설거지가 산더미",
          "original_form": "쉬긴 ㅎㅎ 설거지가 산더미",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016123.1.15",
          "form": "방금 된장찌개 다 끓여씸",
          "original_form": "방금 된장찌개 다 끓여씸",
