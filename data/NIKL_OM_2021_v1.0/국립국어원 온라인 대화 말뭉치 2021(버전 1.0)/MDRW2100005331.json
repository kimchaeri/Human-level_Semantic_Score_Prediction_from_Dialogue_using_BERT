      "utterance": [
        {
          "id": "MDRW2100005331.1.1",
          "form": "싸이월드 하셨어요?ㅎ",
          "original_form": "싸이월드 하셨어요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005331.1.2",
          "form": "싸이월드세대신가요?",
          "original_form": "싸이월드세대신가요?",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005331.1.3",
          "form": "전 완전이요ㅎㅎ",
          "original_form": "전 완전이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005331.1.4",
          "form": "대학교때까지했엇어요ㅎ",
          "original_form": "대학교때까지했엇어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005331.1.5",
          "form": "저도요ㅎ",
          "original_form": "저도요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005331.1.6",
          "form": "7월에 부활하다더니 늦어지나봐요",
          "original_form": "7월에 부활하다더니 늦어지나봐요",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005331.1.7",
          "form": "지금부활했어요ㅎ",
          "original_form": "지금부활했어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005331.1.8",
          "form": "어제 확인했는데 15년 이후만 복원됬다고 그 이전은 나중에 하라고 하던데요ㅎ",
          "original_form": "어제 확인했는데 15년 이후만 복원됬다고 그 이전은 나중에 하라고 하던데요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005331.1.9",
          "form": "아 이제들어가져요?",
          "original_form": "아 이제들어가져요?",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005331.1.10",
          "form": "네네ㅎ 어제들어가서 복원하려고 하니 안되더라구요ㅎ",
          "original_form": "네네ㅎ 어제들어가서 복원하려고 하니 안되더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005331.1.11",
          "form": "사진도 다복구된건가요?",
          "original_form": "사진도 다복구된건가요?",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005331.1.12",
          "form": "근데 기억이 가물가물해요ㅎ",
          "original_form": "근데 기억이 가물가물해요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005331.1.13",
          "form": "아뇨 제껀 안됬어요ㅎ",
          "original_form": "아뇨 제껀 안됬어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005331.1.14",
          "form": "전 비번도 기억이 안나는데",
          "original_form": "전 비번도 기억이 안나는데",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005331.1.15",
          "form": "비번은 저도ㅎㅎ",
          "original_form": "비번은 저도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005331.1.16",
          "form": "그걸알아야 들어가자나요ㅎ",
          "original_form": "그걸알아야 들어가자나요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005331.1.17",
          "form": "거기 비번은 안되어있고 전화번호랑 이름만 되어이ㅛ아요",
          "original_form": "거기 비번은 안되어있고 전화번호랑 이름만 되어이ㅛ아요",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005331.1.18",
          "form": "도토리도 남은건 보상해준다한거같던데ㅋ",
          "original_form": "도토리도 남은건 보상해준다한거같던데ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005331.1.19",
          "form": "남아있던 도토리가 있는지도 기억이 안나요",
          "original_form": "남아있던 도토리가 있는지도 기억이 안나요",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005331.1.20",
          "form": "있으면 좋으련만ㅎㅎ",
          "original_form": "있으면 좋으련만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005331.1.21",
          "form": "돈주고 산적은 없는거같아요ㅎ",
          "original_form": "돈주고 산적은 없는거같아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005331.1.22",
          "form": "지금 하는 이벤트는 50일해서 5만원인데ㅎ",
          "original_form": "지금 하는 이벤트는 50일해서 5만원인데ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005331.1.23",
          "form": "선물받으신거네요ㅎ",
          "original_form": "선물받으신거네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005331.1.24",
          "form": "헐 비싸네요",
          "original_form": "헐 비싸네요",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005331.1.25",
          "form": "무한정 꾸밀수있는거예요?",
          "original_form": "무한정 꾸밀수있는거예요?",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005331.1.26",
          "form": "아뇨아뇨ㅎㅎ 지금 심심이하는거요ㅎㅎ",
          "original_form": "아뇨아뇨ㅎㅎ 지금 심심이하는거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005331.1.27",
          "form": "네페 5만원ㅎ",
          "original_form": "네페 5만원ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005331.1.28",
          "form": "싸이월드 도토리 남은게 5만원 이상이면 좋겟다구요ㅎ",
          "original_form": "싸이월드 도토리 남은게 5만원 이상이면 좋겟다구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005331.1.29",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005331.1.30",
          "form": "전 배경음악만 바꿨던거 같아요",
          "original_form": "전 배경음악만 바꿨던거 같아요",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005331.1.31",
          "form": "다시 옛날 사진보면 손발  오그라들듯요",
          "original_form": "다시 옛날 사진보면 손발  오그라들듯요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005331.1.32",
          "form": "배경음악 뭐뭐있었는지 기억나요?",
          "original_form": "배경음악 뭐뭐있었는지 기억나요?",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005331.1.33",
          "form": "에이 그것뿐이겟어요?",
          "original_form": "에이 그것뿐이겟어요?",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005331.1.34",
          "form": "프리스타일 y",
          "original_form": "프리스타일 y",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005331.1.35",
          "form": "김동률 출발ㅎ",
          "original_form": "김동률 출발ㅎ",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005331.1.36",
          "form": "피아노곡도 깔기도하고ㅋ",
          "original_form": "피아노곡도 깔기도하고ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005331.1.37",
          "form": "팝송도",
          "original_form": "팝송도",
          "speaker_id": "1",
          "time": "20210821 22:05"
        }
      ]
