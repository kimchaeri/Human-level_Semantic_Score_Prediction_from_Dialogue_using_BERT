      "utterance": [
        {
          "id": "MDRW2100041925.1.1",
          "form": "가장 최근 여행이 어디야?-?",
          "original_form": "가장 최근 여행이 어디야?-?",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041925.1.2",
          "form": "최근에..? 어디지 대만???",
          "original_form": "최근에..? 어디지 대만???",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041925.1.3",
          "form": "코로나 전인가보넹!",
          "original_form": "코로나 전인가보넹!",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041925.1.4",
          "form": "대만은 가본적이 없는데 여행지 소개좀 해종",
          "original_form": "대만은 가본적이 없는데 여행지 소개좀 해종",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041925.1.5",
          "form": "난 대만 가서.. 야시장이 신기했어",
          "original_form": "난 대만 가서.. 야시장이 신기했어",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041925.1.6",
          "form": "음식. 매우 이국적",
          "original_form": "음식. 매우 이국적",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041925.1.7",
          "form": "야시장 좋지좋지",
          "original_form": "야시장 좋지좋지",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041925.1.8",
          "form": "취두부가 문제였어요. ㅡㅡ",
          "original_form": "취두부가 문제였어요. ㅡㅡ",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041925.1.9",
          "form": "으악 취두부",
          "original_form": "으악 취두부",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041925.1.10",
          "form": "나는 해외여행 마지막은 베트남!",
          "original_form": "나는 해외여행 마지막은 베트남!",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041925.1.11",
          "form": "오 베트남 부러운디",
          "original_form": "오 베트남 부러운디",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041925.1.12",
          "form": "거기 추천하시나엽?",
          "original_form": "거기 추천하시나엽?",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041925.1.13",
          "form": "조아용 완전 호화여행",
          "original_form": "조아용 완전 호화여행",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041925.1.14",
          "form": "나트랑 갔는데",
          "original_form": "나트랑 갔는데",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041925.1.15",
          "form": "맨날 마사지 받으러 가고 맨날 망고 사먹고 최고최고!",
          "original_form": "맨날 마사지 받으러 가고 맨날 망고 사먹고 최고최고!",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041925.1.16",
          "form": "와... 나 망고좋아하는디",
          "original_form": "와... 나 망고좋아하는디",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041925.1.17",
          "form": "재밌는 여행이겠다",
          "original_form": "재밌는 여행이겠다",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041925.1.18",
          "form": "담엔 꼬옥.",
          "original_form": "담엔 꼬옥.",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041925.1.19",
          "form": "꼬옥. . . ....",
          "original_form": "꼬옥. . . ....",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041925.1.20",
          "form": "야시장도 좋았고 아무래도 동남아라 저렴한게",
          "original_form": "야시장도 좋았고 아무래도 동남아라 저렴한게",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041925.1.21",
          "form": "진짜 좋았어!",
          "original_form": "진짜 좋았어!",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041925.1.22",
          "form": "돈 펑펑쓰고 우리나라에서 비싼데 필요한것들 이것저것 사왔징",
          "original_form": "돈 펑펑쓰고 우리나라에서 비싼데 필요한것들 이것저것 사왔징",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041925.1.23",
          "form": "하놔..... .... 베트남 은근 많이 가넵",
          "original_form": "하놔..... .... 베트남 은근 많이 가넵",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041925.1.24",
          "form": "역시 동남아라 그런지 물가가..",
          "original_form": "역시 동남아라 그런지 물가가..",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041925.1.25",
          "form": "마자마자,,, 짱이야",
          "original_form": "마자마자,,, 짱이야",
          "speaker_id": "2",
          "time": "20210906 17:54"
        }
      ]
    }
  ]
