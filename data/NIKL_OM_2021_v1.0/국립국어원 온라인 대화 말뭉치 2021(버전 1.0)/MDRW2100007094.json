      "utterance": [
        {
          "id": "MDRW2100007094.1.1",
          "form": "당근마켓이나 중고나라 자주 이용하시나요?",
          "original_form": "당근마켓이나 중고나라 자주 이용하시나요?",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007094.1.2",
          "form": "네 종종 이용해요!",
          "original_form": "네 종종 이용해요!",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007094.1.3",
          "form": "최근에 거래했던 건 뭔가요?",
          "original_form": "최근에 거래했던 건 뭔가요?",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007094.1.4",
          "form": "핸드폰 케이스요!",
          "original_form": "핸드폰 케이스요!",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007094.1.5",
          "form": "상태는 괜찮았나요?",
          "original_form": "상태는 괜찮았나요?",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007094.1.6",
          "form": "앗 제가 판매했던거라 ㅋㅋㅋ 만족하신 것 같았어요!",
          "original_form": "앗 제가 판매했던거라 ㅋㅋㅋ 만족하신 것 같았어요!",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007094.1.7",
          "form": "아하 그렇군요 ㅋㅋㅋ 저도 최근에 안 쓰는 기프티콘을 판매했는데 되게 쏠쏠하고 좋은 것 같아요",
          "original_form": "아하 그렇군요 ㅋㅋㅋ 저도 최근에 안 쓰는 기프티콘을 판매했는데 되게 쏠쏠하고 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007094.1.8",
          "form": "요새 사고싶은 물건 있으신가요?",
          "original_form": "요새 사고싶은 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007094.1.9",
          "form": "아 저도 기프티콘 종종 판매하고 해요ㅋㅋㅋ",
          "original_form": "아 저도 기프티콘 종종 판매하고 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007094.1.10",
          "form": "음..핸드폰이요!",
          "original_form": "음..핸드폰이요!",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007094.1.11",
          "form": "저도 핸드폰... 요새 젵플립 사고싶어요!",
          "original_form": "저도 핸드폰... 요새 젵플립 사고싶어요!",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007094.1.12",
          "form": "오 새로 나왔잖아요",
          "original_form": "오 새로 나왔잖아요",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007094.1.13",
          "form": "저도 갖고 싶더라구요ㅠㅠ",
          "original_form": "저도 갖고 싶더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007094.1.14",
          "form": "색이 너무 예쁘게 잘 나왔어요... 어떤 색이 제일 맘에 드세요?",
          "original_form": "색이 너무 예쁘게 잘 나왔어요... 어떤 색이 제일 맘에 드세요?",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007094.1.15",
          "form": "저는 크림색이 예쁜 것 같아요!",
          "original_form": "저는 크림색이 예쁜 것 같아요!",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007094.1.16",
          "form": "전 다 맘에 들어서 못고르겠더라구요 ㅋㅋㅋ",
          "original_form": "전 다 맘에 들어서 못고르겠더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007094.1.17",
          "form": "진짜 이번에 너무 예쁘게 잘 나오긴 했어요ㅠㅠ",
          "original_form": "진짜 이번에 너무 예쁘게 잘 나오긴 했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        }
      ]
