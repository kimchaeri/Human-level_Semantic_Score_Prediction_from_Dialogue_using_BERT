      "utterance": [
        {
          "id": "MDRW2100021894.1.1",
          "form": "이상형 똑똑하고 잘생기고 모든주제 이야기 다 가능한 사람? 대신 키는 안봅니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이상형 똑똑하고 잘생기고 모든주제 이야기 다 가능한 사람? 대신 키는 안봅니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100021894.1.2",
          "form": "이상형 어케되세요",
          "original_form": "이상형 어케되세요",
          "speaker_id": "2",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100021894.1.3",
          "form": "키봐요\nㄱㅋㄱㅈㅋㅈㄱㅈㄱㅈㅋㅈㄱㅈㄱㅈㅋㅈㄱ",
          "original_form": "키봐요\nㄱㅋㄱㅈㅋㅈㄱㅈㄱㅈㅋㅈㄱㅈㄱㅈㅋㅈㄱ",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100021894.1.4",
          "form": "유모코드가맞는사람이면좋겄어요",
          "original_form": "유모코드가맞는사람이면좋겄어요",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100021894.1.5",
          "form": "유머코드 중요하죠 너무",
          "original_form": "유머코드 중요하죠 너무",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100021894.1.6",
          "form": "유모라고 적었네요 제가",
          "original_form": "유모라고 적었네요 제가",
          "speaker_id": "1",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100021894.1.7",
          "form": "ㅋㅋㄱㄲㅋ",
          "original_form": "ㅋㅋㄱㄲㅋ",
          "speaker_id": "1",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100021894.1.8",
          "form": "유모코드뭐임ㅋㅋㅋ",
          "original_form": "유모코드뭐임ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100021894.1.9",
          "form": "유몰 유멀 같앙아요 ㅋㅋㅋ",
          "original_form": "유몰 유멀 같앙아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100021894.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ연예인중에 이상형을 말씀해보시오.",
          "original_form": "ㅋㅋㅋㅋㅋㅋ연예인중에 이상형을 말씀해보시오.",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100021894.1.11",
          "form": "차은우랑 송강 좋아해요 !",
          "original_form": "차은우랑 송강 좋아해요 !",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100021894.1.12",
          "form": "누구좋아하세요",
          "original_form": "누구좋아하세요",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100021894.1.13",
          "form": "저방탄진좋아해요",
          "original_form": "저방탄진좋아해요",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100021894.1.14",
          "form": "ㄱㅋㅋㅋㅋ",
          "original_form": "ㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100021894.1.15",
          "form": "앗 방탄 진 좋아하시는구나 제친구도 그래요",
          "original_form": "앗 방탄 진 좋아하시는구나 제친구도 그래요",
          "speaker_id": "2",
          "time": "20210830 07:45"
        },
        {
          "id": "MDRW2100021894.1.16",
          "form": "그렇군요ㅋㅋㅋ어깨넓어요 키커요 잘생겻어요",
          "original_form": "그렇군요ㅋㅋㅋ어깨넓어요 키커요 잘생겻어요",
          "speaker_id": "1",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100021894.1.17",
          "form": "정말 이상형이네요",
          "original_form": "정말 이상형이네요",
          "speaker_id": "2",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100021894.1.18",
          "form": "그렇습니다 그런남자없어요",
          "original_form": "그렇습니다 그런남자없어요",
          "speaker_id": "1",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100021894.1.19",
          "form": "아니에요 있을수있어요",
          "original_form": "아니에요 있을수있어요",
          "speaker_id": "2",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100021894.1.20",
          "form": "사랑과전쟁을써봐요ㅇㅈㄹ",
