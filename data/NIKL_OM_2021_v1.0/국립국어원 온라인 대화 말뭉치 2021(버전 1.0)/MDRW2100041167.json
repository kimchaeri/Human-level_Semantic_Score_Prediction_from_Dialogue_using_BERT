      "utterance": [
        {
          "id": "MDRW2100041167.1.1",
          "form": "안녕하세요 맥도날드 자주 드세요?",
          "original_form": "안녕하세요 맥도날드 자주 드세요?",
          "speaker_id": "1",
          "time": "20210905 19:16"
        },
        {
          "id": "MDRW2100041167.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210905 19:16"
        },
        {
          "id": "MDRW2100041167.1.3",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100041167.1.4",
          "form": "예전엔 많이 갔었는데 이사오면서 못 가고 있네요",
          "original_form": "예전엔 많이 갔었는데 이사오면서 못 가고 있네요",
          "speaker_id": "2",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100041167.1.5",
          "form": "아 매장이 멀어지셨나봐요 ㅠㅠ",
          "original_form": "아 매장이 멀어지셨나봐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100041167.1.6",
          "form": "그럼 다른 햄버거 가게 이용하시나요?",
          "original_form": "그럼 다른 햄버거 가게 이용하시나요?",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100041167.1.7",
          "form": "네 버거킹이나 KFC가 가까워서 이용하고 있어요",
          "original_form": "네 버거킹이나 KFC가 가까워서 이용하고 있어요",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100041167.1.8",
          "form": "맥날 좋아하세요?",
          "original_form": "맥날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100041167.1.9",
          "form": "네 젤 자주 갔던 곳 같어요",
          "original_form": "네 젤 자주 갔던 곳 같어요",
          "speaker_id": "1",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100041167.1.10",
          "form": "저도 전엔 젤 자주갔는데 행사도 많이 하고 메뉴가 다양해서요",
          "original_form": "저도 전엔 젤 자주갔는데 행사도 많이 하고 메뉴가 다양해서요",
          "speaker_id": "2",
          "time": "20210905 19:20"
        },
        {
          "id": "MDRW2100041167.1.11",
          "form": "네 그럭저럭 무난한 이미지에요",
          "original_form": "네 그럭저럭 무난한 이미지에요",
          "speaker_id": "1",
          "time": "20210905 19:20"
        },
        {
          "id": "MDRW2100041167.1.12",
          "form": "네 맞아요 뭘먹어도 그냥 보장되는 느낌이라 ㅋㅋ",
          "original_form": "네 맞아요 뭘먹어도 그냥 보장되는 느낌이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100041167.1.13",
          "form": "뭘 젤 잘 드시나요 메뉴",
          "original_form": "뭘 젤 잘 드시나요 메뉴",
          "speaker_id": "2",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100041167.1.14",
          "form": "요즘엔 슈비버거 자주먹어요",
          "original_form": "요즘엔 슈비버거 자주먹어요",
          "speaker_id": "1",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100041167.1.15",
          "form": "새우랑 소고기 한장씩 있어서 든든하고 맛있더라구요",
          "original_form": "새우랑 소고기 한장씩 있어서 든든하고 맛있더라구요",
          "speaker_id": "1",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100041167.1.16",
          "form": "맞아요 그거 든든해요 저는 슈슈 젤 좋아해요",
          "original_form": "맞아요 그거 든든해요 저는 슈슈 젤 좋아해요",
          "speaker_id": "2",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100041167.1.17",
          "form": "1955도 맛있고",
          "original_form": "1955도 맛있고",
          "speaker_id": "2",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100041167.1.18",
          "form": "아 네 슈슈버거를 안먹어봤더라구요",
          "original_form": "아 네 슈슈버거를 안먹어봤더라구요",
          "speaker_id": "1",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100041167.1.19",
          "form": "1955는 맛있었어요",
          "original_form": "1955는 맛있었어요",
          "speaker_id": "1",
          "time": "20210905 19:23"
        }
