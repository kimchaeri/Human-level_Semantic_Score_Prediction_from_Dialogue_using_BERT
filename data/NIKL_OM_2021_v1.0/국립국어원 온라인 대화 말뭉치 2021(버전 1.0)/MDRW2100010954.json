      "utterance": [
        {
          "id": "MDRW2100010954.1.1",
          "form": "제대하셨냐요?",
          "original_form": "제대하셨냐요?",
          "speaker_id": "1",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010954.1.2",
          "form": "넵 군대에 관심이 많으신가바요ㅎㅎ",
          "original_form": "넵 군대에 관심이 많으신가바요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010954.1.3",
          "form": "앗 그렇다기보다는 눌렀는데 군대라고 되어있길래요 ㅎㅎ",
          "original_form": "앗 그렇다기보다는 눌렀는데 군대라고 되어있길래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010954.1.4",
          "form": "주변 사람들한테 종종 얘기듣는 정도 같아요 ㅋㅋㅋ",
          "original_form": "주변 사람들한테 종종 얘기듣는 정도 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010954.1.5",
          "form": "군생활 어디서 하셨나요?",
          "original_form": "군생활 어디서 하셨나요?",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010954.1.6",
          "form": "음 저는 군생활을 하진 않았어요",
          "original_form": "음 저는 군생활을 하진 않았어요",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010954.1.7",
          "form": "성별이.. 전 여자예요",
          "original_form": "성별이.. 전 여자예요",
          "speaker_id": "1",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010954.1.8",
          "form": "아 그러시구나 대부분 군대얘기 싫어하시지 않나요ㅎㅎ",
          "original_form": "아 그러시구나 대부분 군대얘기 싫어하시지 않나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010954.1.9",
          "form": "음 그런가요??",
          "original_form": "음 그런가요??",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010954.1.10",
          "form": "요즘 강철부대 이런것도 해서그런가",
          "original_form": "요즘 강철부대 이런것도 해서그런가",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010954.1.11",
          "form": "별로 그런생각은 없어요 ㅋㅋㅋ 강철부대 재밌더라구오",
          "original_form": "별로 그런생각은 없어요 ㅋㅋㅋ 강철부대 재밌더라구오",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010954.1.12",
          "form": "오 강철부대 보셨나요?",
          "original_form": "오 강철부대 보셨나요?",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010954.1.13",
          "form": "네 저 진짜 팬이었어요 ㅋㅋㅋ",
          "original_form": "네 저 진짜 팬이었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010954.1.14",
          "form": "주변에는 특수부대는 없고 해병대 나온 친구들은 종종 있었는데",
          "original_form": "주변에는 특수부대는 없고 해병대 나온 친구들은 종종 있었는데",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010954.1.15",
          "form": "신기하고 재밌더라구요",
          "original_form": "신기하고 재밌더라구요",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010954.1.16",
          "form": "와 강철부대는 거의 남초방송 아닌가요",
          "original_form": "와 강철부대는 거의 남초방송 아닌가요",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010954.1.17",
          "form": "신기하네요ㅎㅎㅎ",
          "original_form": "신기하네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010954.1.18",
          "form": "어 그런가요? 제 체감은 여자팬들이 더 많은것같아서ㅋㅋㅋ",
          "original_form": "어 그런가요? 제 체감은 여자팬들이 더 많은것같아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010954.1.19",
          "form": "제대하신지 오래되셨어요?",
          "original_form": "제대하신지 오래되셨어요?",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010954.1.20",
          "form": "넵 오래됐는데 아직도 가끔 꿈에나와요..",
          "original_form": "넵 오래됐는데 아직도 가끔 꿈에나와요..",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010954.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010954.1.22",
          "form": "꿈에서 뭐하구 계세요??",
          "original_form": "꿈에서 뭐하구 계세요??",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010954.1.23",
          "form": "꿈에서 전역일자 계산해보니 몇백일 남아 좌절했는데",
          "original_form": "꿈에서 전역일자 계산해보니 몇백일 남아 좌절했는데",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010954.1.24",
          "form": "잠에서 깨고 안심하고 막 이래요ㅎㅎ",
          "original_form": "잠에서 깨고 안심하고 막 이래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010954.1.25",
          "form": "ㅋㅋㅋㅋㅋ꿈이라서 다행이네요....",
          "original_form": "ㅋㅋㅋㅋㅋ꿈이라서 다행이네요....",
          "speaker_id": "1",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010954.1.26",
          "form": "육군이었어요?",
          "original_form": "육군이었어요?",
          "speaker_id": "1",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010954.1.27",
          "form": "네 강원도 철원이었네요.. ㅎㅎ",
          "original_form": "네 강원도 철원이었네요.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010954.1.28",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010954.1.29",
          "form": "눈 담느라 고생 좀 하셨겠네요..",
          "original_form": "눈 담느라 고생 좀 하셨겠네요..",
