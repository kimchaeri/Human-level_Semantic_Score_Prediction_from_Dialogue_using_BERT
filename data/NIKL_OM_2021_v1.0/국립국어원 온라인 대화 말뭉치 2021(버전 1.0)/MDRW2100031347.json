      "utterance": [
        {
          "id": "MDRW2100031347.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031347.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031347.1.3",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031347.1.4",
          "form": "답이 좀 늦었네요 죄송해요",
          "original_form": "답이 좀 늦었네요 죄송해요",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031347.1.5",
          "form": "네 꽤 좋아해요",
          "original_form": "네 꽤 좋아해요",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031347.1.6",
          "form": "저는 스팀게임위주로 하는 편이예요",
          "original_form": "저는 스팀게임위주로 하는 편이예요",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100031347.1.7",
          "form": "다람님은 어떤게임 좋아하세요?",
          "original_form": "다람님은 어떤게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100031347.1.8",
          "form": "괜찮습니다!! 저는 사실 게임을 잘 몰라요",
          "original_form": "괜찮습니다!! 저는 사실 게임을 잘 몰라요",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031347.1.9",
          "form": "그나마 동물의 숲 같은 것만 좀 친구들 꺼 보고ㅋㅋㅋㅋ",
          "original_form": "그나마 동물의 숲 같은 것만 좀 친구들 꺼 보고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031347.1.10",
          "form": "헉 주제가 게임이셔서 게임 좋아하실줄 알았어요!",
          "original_form": "헉 주제가 게임이셔서 게임 좋아하실줄 알았어요!",
          "speaker_id": "2",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031347.1.11",
          "form": "동숲재미있죠",
          "original_form": "동숲재미있죠",
          "speaker_id": "2",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031347.1.12",
          "form": "동물들이 귀엽잖아요 ㅎㄹ",
          "original_form": "동물들이 귀엽잖아요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031347.1.13",
          "form": "친구들꺼 뺏어서 한적은 없으세요? ㅋㅋ",
          "original_form": "친구들꺼 뺏어서 한적은 없으세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031347.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저는 주로 보는 걸 좋아해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저는 주로 보는 걸 좋아해서",
          "speaker_id": "1",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100031347.1.15",
          "form": "그냥 친구들이 이웃 주민들이랑 생일 파티하는 거 구경하고ㅋㅋㅋㅋ",
          "original_form": "그냥 친구들이 이웃 주민들이랑 생일 파티하는 거 구경하고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100031347.1.16",
          "form": "구경하는것도 재미있죠",
          "original_form": "구경하는것도 재미있죠",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100031347.1.17",
          "form": "제 친구들도 동숲 많이 하더라구요",
          "original_form": "제 친구들도 동숲 많이 하더라구요",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100031347.1.18",
          "form": "제 친구는 엄청 빡겜해요 ㅋㅋ",
          "original_form": "제 친구는 엄청 빡겜해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031347.1.19",
          "form": "그쵸 스케일도 커지고 이웃 동물을이 귀여워서 꾸준히 수요가 있는 것 같아요",
          "original_form": "그쵸 스케일도 커지고 이웃 동물을이 귀여워서 꾸준히 수요가 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031347.1.20",
          "form": "아ㅋㅋㅋㅋㅋ어떤 식으로요?",
          "original_form": "아ㅋㅋㅋㅋㅋ어떤 식으로요?",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031347.1.21",
          "form": "빚값고 무주식하고?",
          "original_form": "빚값고 무주식하고?",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031347.1.22",
          "form": "꽃교배같은것도 하더라구요",
          "original_form": "꽃교배같은것도 하더라구요",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031347.1.23",
          "form": "집도 열심히 꾸미고",
          "original_form": "집도 열심히 꾸미고",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031347.1.24",
          "form": "집 꾸미기는 재미있을거같아요",
          "original_form": "집 꾸미기는 재미있을거같아요",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031347.1.25",
          "form": "아ㅋㅋㅋ무주식 그거 진짜 사람들 많이 신경 쓰더라구요",
          "original_form": "아ㅋㅋㅋ무주식 그거 진짜 사람들 많이 신경 쓰더라구요",
          "speaker_id": "1",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031347.1.26",
          "form": "와 꽃도 키울 수 있구나 그건 몰랐어요",
          "original_form": "와 꽃도 키울 수 있구나 그건 몰랐어요",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031347.1.27",
          "form": "진짜 빡겜하시네요ㅋㅋㅋ",
          "original_form": "진짜 빡겜하시네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031347.1.28",
          "form": "막 빨간꽃이랑 노랑꽃이랑 두면",
          "original_form": "막 빨간꽃이랑 노랑꽃이랑 두면",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031347.1.29",
          "form": "옆에 주황색꽃이 핀대요",
          "original_form": "옆에 주황색꽃이 핀대요",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031347.1.30",
          "form": "게임 신경많이 써서 만들었나봐요!",
          "original_form": "게임 신경많이 써서 만들었나봐요!",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031347.1.31",
          "form": "와 너무 신기하다 진짜 디테일하네요",
          "original_form": "와 너무 신기하다 진짜 디테일하네요",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100031347.1.32",
          "form": "그러니까요 저는 동숲 처음봤을때",
          "original_form": "그러니까요 저는 동숲 처음봤을때",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100031347.1.33",
          "form": "벌레를 너무 신경써서 만들었다 그런 생각만했는데",
          "original_form": "벌레를 너무 신경써서 만들었다 그런 생각만했는데",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100031347.1.34",
          "form": "벌레만 신경쓴게 아니었네요 ㅋㅋ",
          "original_form": "벌레만 신경쓴게 아니었네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100031347.1.35",
          "form": "거기 바퀴벌레가 자주 나오던데",
          "original_form": "거기 바퀴벌레가 자주 나오던데",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031347.1.36",
          "form": "놀랐어요ㅋㅋㅋ",
          "original_form": "놀랐어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031347.1.37",
          "form": "맞아요 넘 징그러워요 ㅜㅜ",
          "original_form": "맞아요 넘 징그러워요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031347.1.38",
          "form": "밟으면 콰직 소리나잖아요 ㅜㅜ",
          "original_form": "밟으면 콰직 소리나잖아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031347.1.39",
          "form": "으악 그건 몰랐어요 징그럽네요",
          "original_form": "으악 그건 몰랐어요 징그럽네요",
          "speaker_id": "1",
          "time": "20210904 21:18"
        }
      ]
    }
