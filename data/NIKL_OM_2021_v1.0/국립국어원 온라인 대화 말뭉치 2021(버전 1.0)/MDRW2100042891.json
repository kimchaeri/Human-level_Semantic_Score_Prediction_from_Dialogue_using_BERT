      "utterance": [
        {
          "id": "MDRW2100042891.1.1",
          "form": "난 요즘",
          "original_form": "난 요즘",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042891.1.2",
          "form": "심규선이라고",
          "original_form": "심규선이라고",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042891.1.3",
          "form": "예전 활동명은 루시아야",
          "original_form": "예전 활동명은 루시아야",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042891.1.4",
          "form": "이 사람 노래에 빠져있어",
          "original_form": "이 사람 노래에 빠져있어",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042891.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042891.1.6",
          "form": "인디?",
          "original_form": "인디?",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042891.1.7",
          "form": "응 그런것같아",
          "original_form": "응 그런것같아",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042891.1.8",
          "form": "난 밴드음악 정말 좋아하거든",
          "original_form": "난 밴드음악 정말 좋아하거든",
          "speaker_id": "1",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042891.1.9",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "2",
          "time": "20210907 10:57"
        },
        {
          "id": "MDRW2100042891.1.10",
          "form": "그럼 홍대 버스킹같은것도 구경하러가?",
          "original_form": "그럼 홍대 버스킹같은것도 구경하러가?",
          "speaker_id": "2",
          "time": "20210907 10:57"
        },
        {
          "id": "MDRW2100042891.1.11",
          "form": "내가 밴드음악을 좋아한다고 자각했을 땐 이미 홍대 버스킹이 많이 줄어들었다고 하더라고...ㅠㅠ",
          "original_form": "내가 밴드음악을 좋아한다고 자각했을 땐 이미 홍대 버스킹이 많이 줄어들었다고 하더라고...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042891.1.12",
          "form": "그래서 가보진 않았는데 만약 다시 홍대 버스킹이 많아지면 한번은 꼭 가보고 싶어",
          "original_form": "그래서 가보진 않았는데 만약 다시 홍대 버스킹이 많아지면 한번은 꼭 가보고 싶어",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042891.1.13",
          "form": "그냥 생각해보니까 난 모든 음악 장르를 다 좋아하는 것 같아ㅋㅋㅋㅋ",
          "original_form": "그냥 생각해보니까 난 모든 음악 장르를 다 좋아하는 것 같아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042891.1.14",
          "form": "스테이씨라고 내가 좋아하는 아이돌이 있는데",
          "original_form": "스테이씨라고 내가 좋아하는 아이돌이 있는데",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042891.1.15",
          "form": "걔네들이 어제 컴백했거든",
          "original_form": "걔네들이 어제 컴백했거든",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042891.1.16",
          "form": "뮤비보고 울었어..ㅠ",
          "original_form": "뮤비보고 울었어..ㅠ",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042891.1.17",
          "form": "아 색안경!",
          "original_form": "아 색안경!",
          "speaker_id": "2",
          "time": "20210907 11:12"
        },
        {
          "id": "MDRW2100042891.1.18",
          "form": "나 문특보고 알았어",
          "original_form": "나 문특보고 알았어",
          "speaker_id": "2",
          "time": "20210907 11:12"
        },
        {
          "id": "MDRW2100042891.1.19",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "1",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100042891.1.20",
          "form": "어쩔수없이 케이팝에 빠지게되나봐",
          "original_form": "어쩔수없이 케이팝에 빠지게되나봐",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042891.1.21",
          "form": "고등학교 때는 관심이 식었었는데",
          "original_form": "고등학교 때는 관심이 식었었는데",
          "speaker_id": "2",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042891.1.22",
          "form": "대학생되니까 다시 불타는중",
          "original_form": "대학생되니까 다시 불타는중",
          "speaker_id": "2",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042891.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042891.1.24",
          "form": "문특 나도 재밌게 봤어",
          "original_form": "문특 나도 재밌게 봤어",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042891.1.25",
          "form": "자본력이 받혀주니까 더 많이 쓰는거 같어",
          "original_form": "자본력이 받혀주니까 더 많이 쓰는거 같어",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042891.1.26",
          "form": "더 잘됐으면 좋겠다 문특ㅠㅠ",
          "original_form": "더 잘됐으면 좋겠다 문특ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042891.1.27",
          "form": "맞어맞어!!!!!",
          "original_form": "맞어맞어!!!!!",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042891.1.28",
          "form": "유튜브 많이 봐??",
          "original_form": "유튜브 많이 봐??",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042891.1.29",
          "form": "브랜드 상도 두번이나 받았어 우리 문특ㅜㅜㅜ",
          "original_form": "브랜드 상도 두번이나 받았어 우리 문특ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042891.1.30",
          "form": "맞아 !! 대단해",
          "original_form": "맞아 !! 대단해",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042891.1.31",
          "form": "보는것만 좀?",
          "original_form": "보는것만 좀?",
          "speaker_id": "2",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042891.1.32",
          "form": "나는 마마무 공식채널이랑 솔라시도 구독해놨고 나머지는 팻채널들? 많이 봐",
          "original_form": "나는 마마무 공식채널이랑 솔라시도 구독해놨고 나머지는 팻채널들? 많이 봐",
          "speaker_id": "2",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100042891.1.33",
          "form": "오 팻채널",
          "original_form": "오 팻채널",
          "speaker_id": "1",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042891.1.34",
          "form": "나는 예랑가랑이라고 요즘 빠져있는 유튜버있어ㅋㅋㅋㅋ",
          "original_form": "나는 예랑가랑이라고 요즘 빠져있는 유튜버있어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042891.1.35",
          "form": "너무 재밌음",
          "original_form": "너무 재밌음",
          "speaker_id": "1",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042891.1.36",
          "form": "한번 시간 나면 꼭 봐바",
          "original_form": "한번 시간 나면 꼭 봐바",
          "speaker_id": "1",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042891.1.37",
          "form": "예랑가랑 너도 보는구나!!!!",
          "original_form": "예랑가랑 너도 보는구나!!!!",
