      "utterance": [
        {
          "id": "MDRW2100041636.1.1",
          "form": "요즘 거기는 날씨어때요?",
          "original_form": "요즘 거기는 날씨어때요?",
          "speaker_id": "1",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100041636.1.2",
          "form": "흐려요 ㅠ 비가 오려나봐요",
          "original_form": "흐려요 ㅠ 비가 오려나봐요",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041636.1.3",
          "form": "name1님 쪽은 어때요?",
          "original_form": "&name1&님 쪽은 어때요?",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041636.1.4",
          "form": "저희도요 우산안챙겼는데 클났네요ㅠ",
          "original_form": "저희도요 우산안챙겼는데 클났네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041636.1.5",
          "form": "아이고 ㅠㅠ 진짜 집에 들어가기 전엔 비가 오지 말아야 할텐데요 ㅠ",
          "original_form": "아이고 ㅠㅠ 진짜 집에 들어가기 전엔 비가 오지 말아야 할텐데요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041636.1.6",
          "form": "ㅋㅋㅋ... 결국 저 비맞으면서 집왔어요",
          "original_form": "ㅋㅋㅋ... 결국 저 비맞으면서 집왔어요",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041636.1.7",
          "form": "으악... ㅠ",
          "original_form": "으악... ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041636.1.8",
          "form": "이쪽은 아직까지도 흐리기만해요",
          "original_form": "이쪽은 아직까지도 흐리기만해요",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041636.1.9",
          "form": "대신 바람이 상당히 불고 있는 것 같아요",
          "original_form": "대신 바람이 상당히 불고 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041636.1.10",
          "form": "여긴 바람은 안불어요",
          "original_form": "여긴 바람은 안불어요",
          "speaker_id": "1",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041636.1.11",
          "form": "주말내내 맑더니 하필 월요일날 이런 뒷통수를...",
          "original_form": "주말내내 맑더니 하필 월요일날 이런 뒷통수를...",
          "speaker_id": "1",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041636.1.12",
          "form": "바람이 창문이 울리도록 불고 있어요",
          "original_form": "바람이 창문이 울리도록 불고 있어요",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041636.1.13",
          "form": "진짜 왜 하필 월요일에 비가 오고 그랬을까요",
          "original_form": "진짜 왜 하필 월요일에 비가 오고 그랬을까요",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041636.1.14",
          "form": "주말에 왔음 기분좋게 넘어갔을 것 같은데 말이에요 ㅠ",
          "original_form": "주말에 왔음 기분좋게 넘어갔을 것 같은데 말이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100041636.1.15",
          "form": "비오는거 나름 좋긴한데... 제가 돌아다닐일 있을땐 안왔으면 좋겠어요..ㅋㅋㅋ",
          "original_form": "비오는거 나름 좋긴한데... 제가 돌아다닐일 있을땐 안왔으면 좋겠어요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041636.1.16",
          "form": "여기도 비와요 ㅠ",
          "original_form": "여기도 비와요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041636.1.17",
          "form": "집에 갈때까지만 안 오길 바랬는데",
          "original_form": "집에 갈때까지만 안 오길 바랬는데",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041636.1.18",
          "form": "우산은 왜 항상 챙기지 못한 날에만 비가 오는지 몰라요",
          "original_form": "우산은 왜 항상 챙기지 못한 날에만 비가 오는지 몰라요",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041636.1.19",
          "form": "헉 name2님도 우산없으세요?",
          "original_form": "헉 &name2&님도 우산없으세요?",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041636.1.20",
          "form": "다행히 조금 내릴때 뛰어들어 왔어요!!",
          "original_form": "다행히 조금 내릴때 뛰어들어 왔어요!!",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100041636.1.21",
          "form": "name1님은 비 많이 맞지 않으신거지요?",
          "original_form": "&name1&님은 비 많이 맞지 않으신거지요?",
