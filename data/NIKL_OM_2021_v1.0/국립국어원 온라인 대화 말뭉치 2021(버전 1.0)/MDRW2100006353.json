      "utterance": [
        {
          "id": "MDRW2100006353.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006353.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006353.1.3",
          "form": "저도 아직이요",
          "original_form": "저도 아직이요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006353.1.4",
          "form": "9월30일 예약인데 무섭네요",
          "original_form": "9월30일 예약인데 무섭네요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006353.1.5",
          "form": "언제맞으세요?",
          "original_form": "언제맞으세요?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006353.1.6",
          "form": "전 27일날 예약이요",
          "original_form": "전 27일날 예약이요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006353.1.7",
          "form": "모더나 순차공급이면 이걸로 놔줄려나요?",
          "original_form": "모더나 순차공급이면 이걸로 놔줄려나요?",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006353.1.8",
          "form": "큰딸은 이번주 금요일인데 벌써 떨리데요",
          "original_form": "큰딸은 이번주 금요일인데 벌써 떨리데요",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006353.1.9",
          "form": "화이자가 좋을지 모더나가 좋을지 모르겠어서",
          "original_form": "화이자가 좋을지 모더나가 좋을지 모르겠어서",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006353.1.10",
          "form": "어떤거 맞는지 말씀드릴게요",
          "original_form": "어떤거 맞는지 말씀드릴게요",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006353.1.11",
          "form": "어떤거맞는지 아직모르는거죠?",
          "original_form": "어떤거맞는지 아직모르는거죠?",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006353.1.12",
          "form": "그날 어떤건지 가봐야알꺼같아요",
          "original_form": "그날 어떤건지 가봐야알꺼같아요",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006353.1.13",
          "form": "아 당일 가야알수있군요",
          "original_form": "아 당일 가야알수있군요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006353.1.14",
          "form": "둘중하나라고만 예약때 한거같아요",
          "original_form": "둘중하나라고만 예약때 한거같아요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006353.1.15",
          "form": "지방은 모더나 서울은 화이자라는 얘기도 있던데",
          "original_form": "지방은 모더나 서울은 화이자라는 얘기도 있던데",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006353.1.16",
          "form": "솔직히 어떤게 더 좋은건지 몰라서...",
          "original_form": "솔직히 어떤게 더 좋은건지 몰라서...",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006353.1.17",
          "form": "그냥 잘지나가길 바래야죠",
          "original_form": "그냥 잘지나가길 바래야죠",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006353.1.18",
          "form": "그니까요 나한테 부작용생김 그게 안좋은거겠죠ㅎㅎ",
          "original_form": "그니까요 나한테 부작용생김 그게 안좋은거겠죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006353.1.19",
          "form": "별이상없이 2차까지 맞음 좋겠어요",
          "original_form": "별이상없이 2차까지 맞음 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        }
