      "utterance": [
        {
          "id": "MDRW2100006379.1.1",
          "form": "윙잇에서 갈비 드셔보셨어요?",
          "original_form": "윙잇에서 갈비 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006379.1.2",
          "form": "엄청 맛있나봐요ㅎ",
          "original_form": "엄청 맛있나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006379.1.3",
          "form": "갈비는 안먹어봤어요",
          "original_form": "갈비는 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006379.1.4",
          "form": "뭐드셔보셨나요",
          "original_form": "뭐드셔보셨나요",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006379.1.5",
          "form": "떡이랑 떡볶이요",
          "original_form": "떡이랑 떡볶이요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006379.1.6",
          "form": "창억떡이요?",
          "original_form": "창억떡이요?",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006379.1.7",
          "form": "떡 맛있더라구요. 개별포장이라좋구요",
          "original_form": "떡 맛있더라구요. 개별포장이라좋구요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006379.1.8",
          "form": "호박이 맛나다던데",
          "original_form": "호박이 맛나다던데",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006379.1.9",
          "form": "이번에  호박인절미시켰는데 아직 안먹어봤어요",
          "original_form": "이번에  호박인절미시켰는데 아직 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006379.1.10",
          "form": "전 백순대랑 제육 먹어봤는데 백순대 추천이요",
          "original_form": "전 백순대랑 제육 먹어봤는데 백순대 추천이요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006379.1.11",
          "form": "담에 쿠폰주면 시켜야겠어요",
          "original_form": "담에 쿠폰주면 시켜야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006379.1.12",
          "form": "순대도좋아하는데",
          "original_form": "순대도좋아하는데",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006379.1.13",
          "form": "차이 며칠전에 5천떠서",
          "original_form": "차이 며칠전에 5천떠서",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006379.1.14",
          "form": "백순대 또샀어요ㅎ",
          "original_form": "백순대 또샀어요ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006379.1.15",
          "form": "차이에도 할인있었나보네요",
          "original_form": "차이에도 할인있었나보네요",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006379.1.16",
          "form": "10개써야돼서 타겟인거같았는데",
          "original_form": "10개써야돼서 타겟인거같았는데",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006379.1.17",
          "form": "왠지 10개안되실듯해서 말씀안드렸어요",
          "original_form": "왠지 10개안되실듯해서 말씀안드렸어요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006379.1.18",
          "form": "전 요즘 차이 안써서 번개가 없어요",
          "original_form": "전 요즘 차이 안써서 번개가 없어요",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006379.1.19",
          "form": "저도 저번에 모은거쓰고있어서 오늘 서울우유 떴더라구요",
          "original_form": "저도 저번에 모은거쓰고있어서 오늘 서울우유 떴더라구요",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006379.1.20",
          "form": "두유 많이사시던데요",
          "original_form": "두유 많이사시던데요",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006379.1.21",
          "form": "멸균우유 많아서 그거먹저 먹고 나중에 또 뜨면 사야겠어요",
          "original_form": "멸균우유 많아서 그거먹저 먹고 나중에 또 뜨면 사야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006379.1.22",
          "form": "멸균우유는 맛이 싱겁지않아요?",
          "original_form": "멸균우유는 맛이 싱겁지않아요?",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006379.1.23",
          "form": "뭔가 좀 다른거같던데",
          "original_form": "뭔가 좀 다른거같던데",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006379.1.24",
          "form": "전 안먹는데 작은애가 맛있다고 잘먹네요",
          "original_form": "전 안먹는데 작은애가 맛있다고 잘먹네요",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006379.1.25",
          "form": "원래 흰우유 잘안먹는데",
          "original_form": "원래 흰우유 잘안먹는데",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006379.1.26",
          "form": "김치볶음밥만 드셨으니 디저트라도ㅎ",
          "original_form": "김치볶음밥만 드셨으니 디저트라도ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:35"
        }
      ]
    }
