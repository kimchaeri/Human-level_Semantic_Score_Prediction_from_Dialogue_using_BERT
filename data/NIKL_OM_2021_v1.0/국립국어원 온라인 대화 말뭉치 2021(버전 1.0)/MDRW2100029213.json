      "utterance": [
        {
          "id": "MDRW2100029213.1.1",
          "form": "가을 제일 좋아해요!",
          "original_form": "가을 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029213.1.2",
          "form": "저는 겨울이 좋아요",
          "original_form": "저는 겨울이 좋아요",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029213.1.3",
          "form": "눈을 좋아해서요.",
          "original_form": "눈을 좋아해서요.",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029213.1.4",
          "form": "오 겨울 좋죠",
          "original_form": "오 겨울 좋죠",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029213.1.5",
          "form": "눈 오면 밖에 나가서 뛰어노시나요?",
          "original_form": "눈 오면 밖에 나가서 뛰어노시나요?",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029213.1.6",
          "form": "크리스마스 분위기도 따뜻하구요",
          "original_form": "크리스마스 분위기도 따뜻하구요",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029213.1.7",
          "form": "눈사람 만든다던가 눈구경한다던가",
          "original_form": "눈사람 만든다던가 눈구경한다던가",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029213.1.8",
          "form": "눈오면 눈오리를 만들어요",
          "original_form": "눈오면 눈오리를 만들어요",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029213.1.9",
          "form": "아 눈오리 귀엽더라구요ㅋㅋㅋ",
          "original_form": "아 눈오리 귀엽더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029213.1.10",
          "form": "첫눈은 무조건 나가야지요",
          "original_form": "첫눈은 무조건 나가야지요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029213.1.11",
          "form": "저희 동네에도 눈 올때 누가 많이 만들어놓고 가더라구요",
          "original_form": "저희 동네에도 눈 올때 누가 많이 만들어놓고 가더라구요",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029213.1.12",
          "form": "겨울은 건조한게 힘들어요",
          "original_form": "겨울은 건조한게 힘들어요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029213.1.13",
          "form": "히터를 트니까요",
          "original_form": "히터를 트니까요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029213.1.14",
          "form": "맞아요 눈오리 유행했지요",
          "original_form": "맞아요 눈오리 유행했지요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029213.1.15",
          "form": "올겨울도 눈이 한번씩 내리면 좋겠어요",
          "original_form": "올겨울도 눈이 한번씩 내리면 좋겠어요",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029213.1.16",
          "form": "건조해서 가습기 물 채워놔야하는게 조금 번거롭더라구요",
          "original_form": "건조해서 가습기 물 채워놔야하는게 조금 번거롭더라구요",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029213.1.17",
          "form": "가습기 틀어놔도 건조하기도하고",
          "original_form": "가습기 틀어놔도 건조하기도하고",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029213.1.18",
          "form": "네 겨울엔 가습기가 필수에요",
          "original_form": "네 겨울엔 가습기가 필수에요",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029213.1.19",
          "form": "습도가 딱 여름과 겨울의 중간 지점이었으면 좋겠어요",
          "original_form": "습도가 딱 여름과 겨울의 중간 지점이었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029213.1.20",
          "form": "여름엔 제습기가  필요하구요",
          "original_form": "여름엔 제습기가  필요하구요",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029213.1.21",
          "form": "가을이 딱 좋네요",
          "original_form": "가을이 딱 좋네요",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029213.1.22",
          "form": "여름은 너무 습하고 겨울은 너무 건조해서",
          "original_form": "여름은 너무 습하고 겨울은 너무 건조해서",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029213.1.23",
          "form": "걷기 좋은 계절이에요",
          "original_form": "걷기 좋은 계절이에요",
          "speaker_id": "1",
          "time": "20210903 00:25"
        }
      ]
    }
