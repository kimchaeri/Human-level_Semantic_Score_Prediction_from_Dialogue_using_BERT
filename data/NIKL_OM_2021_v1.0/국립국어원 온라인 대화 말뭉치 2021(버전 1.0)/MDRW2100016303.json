      "utterance": [
        {
          "id": "MDRW2100016303.1.1",
          "form": "장마가 끝난 줄 알았는데 다시 시작된 거 같아",
          "original_form": "장마가 끝난 줄 알았는데 다시 시작된 거 같아",
          "speaker_id": "1",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016303.1.2",
          "form": "가을에 장마가 원래 한번 오지?",
          "original_form": "가을에 장마가 원래 한번 오지?",
          "speaker_id": "2",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016303.1.3",
          "form": "그래도 비오지만 선선해져서 기분은 좋더라",
          "original_form": "그래도 비오지만 선선해져서 기분은 좋더라",
          "speaker_id": "2",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016303.1.4",
          "form": "가을장마 오긴 할 거야ㅋㅋ",
          "original_form": "가을장마 오긴 할 거야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016303.1.5",
          "form": "맞아 습한 건 싫은데 선선한건 좋아",
          "original_form": "맞아 습한 건 싫은데 선선한건 좋아",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016303.1.6",
          "form": "헐 그럼 지금은 그냥 여름장마?",
          "original_form": "헐 그럼 지금은 그냥 여름장마?",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016303.1.7",
          "form": "습하면서 덥다서 생각해봐..",
          "original_form": "습하면서 덥다서 생각해봐..",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016303.1.8",
          "form": "으 최악이야",
          "original_form": "으 최악이야",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016303.1.9",
          "form": "이번에 기상예보 봣는데",
          "original_form": "이번에 기상예보 봣는데",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016303.1.10",
          "form": "여름 장마가 이례적으로 다시 생겼다더라고..",
          "original_form": "여름 장마가 이례적으로 다시 생겼다더라고..",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016303.1.11",
          "form": "기상이변..^^..",
          "original_form": "기상이변..^^..",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016303.1.12",
          "form": "습하면서 더운거 지옥이지ㅜ 사우나야",
          "original_form": "습하면서 더운거 지옥이지ㅜ 사우나야",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016303.1.13",
          "form": "헐!!!..진짜? 요새 이상기후 진짜 심각하구나..",
          "original_form": "헐!!!..진짜? 요새 이상기후 진짜 심각하구나..",
          "speaker_id": "2",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016303.1.14",
          "form": "이번엔그냥 태풍와서 그런 줄 알았어ㅠㅠ",
          "original_form": "이번엔그냥 태풍와서 그런 줄 알았어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016303.1.15",
          "form": "한국의 여름..너무 습하죠...ㅎㅎ",
          "original_form": "한국의 여름..너무 습하죠...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016303.1.16",
          "form": "진짜 기후공포 보도 보는건 너무 무섭더라ㅜㅜ 현실이 되고 있지",
          "original_form": "진짜 기후공포 보도 보는건 너무 무섭더라ㅜㅜ 현실이 되고 있지",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016303.1.17",
          "form": "이렇게 습할거면 뜨겁기라도 덜하지ㅎㅎ",
          "original_form": "이렇게 습할거면 뜨겁기라도 덜하지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016303.1.18",
          "form": "맞아.. 한여름에도 온도 진짜 올라갔었잖아 ㅠㅠ",
          "original_form": "맞아.. 한여름에도 온도 진짜 올라갔었잖아 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016303.1.19",
          "form": "다들 경각심 가지고 준비해야되는데..",
          "original_form": "다들 경각심 가지고 준비해야되는데..",
          "speaker_id": "2",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016303.1.20",
          "form": "그래도..곧있음 가을이네",
          "original_form": "그래도..곧있음 가을이네",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016303.1.21",
          "form": "정말 계절 빨리지나간다~",
          "original_form": "정말 계절 빨리지나간다~",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016303.1.22",
          "form": "맞아 이번 여름은 진짜 불지옥이었어",
          "original_form": "맞아 이번 여름은 진짜 불지옥이었어",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016303.1.23",
          "form": "가을이 오긴한다는게 신기해 그래도ㅋㅋㅋ",
          "original_form": "가을이 오긴한다는게 신기해 그래도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016303.1.24",
          "form": "그러고보니 너는 어떤 계절 좋아해?",
          "original_form": "그러고보니 너는 어떤 계절 좋아해?",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016303.1.25",
          "form": "나는 원래 여름 좋아했었는데",
          "original_form": "나는 원래 여름 좋아했었는데",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016303.1.26",
          "form": "요새 너~뮤 더워서 싫어죴어",
          "original_form": "요새 너~뮤 더워서 싫어죴어",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016303.1.27",
          "form": "요새는 가을 겨울 ㅎ 너눈?",
          "original_form": "요새는 가을 겨울 ㅎ 너눈?",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016303.1.28",
          "form": "오 나랑 똑같다ㅋㅋ",
          "original_form": "오 나랑 똑같다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016303.1.29",
          "form": "나도 가을 겨울",
          "original_form": "나도 가을 겨울",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016303.1.30",
          "form": "봄은 차차 더워지는게 좀 그렇더라구",
          "original_form": "봄은 차차 더워지는게 좀 그렇더라구",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016303.1.31",
          "form": "맞아 나도 ㅋㅋㅋ 그리고 이제 봄은 좀 부담스러워..",
          "original_form": "맞아 나도 ㅋㅋㅋ 그리고 이제 봄은 좀 부담스러워..",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016303.1.32",
          "form": "활기~ 새로움~ 이런 느낌이라",
          "original_form": "활기~ 새로움~ 이런 느낌이라",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016303.1.33",
          "form": "가을이 천천히 정리해가는 느낌이라 좋아 선선하니",
          "original_form": "가을이 천천히 정리해가는 느낌이라 좋아 선선하니",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016303.1.34",
          "form": "ㅋㅋ우리가 철들었나봐",
          "original_form": "ㅋㅋ우리가 철들었나봐",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016303.1.35",
          "form": "이제 가을겨울이 더 좋아진거뷰면ㅋㅋ",
          "original_form": "이제 가을겨울이 더 좋아진거뷰면ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016303.1.36",
          "form": "ㅋㅋㅋㅋ 나이들어서..ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 나이들어서..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016303.1.37",
          "form": "철들었다니 포장을 잘해주는군..^^ㅎ",
          "original_form": "철들었다니 포장을 잘해주는군..^^ㅎ",
