      "utterance": [
        {
          "id": "MDRW2100030119.1.1",
          "form": "이번 올림픽 보셨어요?",
          "original_form": "이번 올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100030119.1.2",
          "form": "네!! 양궁, 배구, 기계체조, 펜싱 봤어요!",
          "original_form": "네!! 양궁, 배구, 기계체조, 펜싱 봤어요!",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100030119.1.3",
          "form": "우와 저도요!",
          "original_form": "우와 저도요!",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100030119.1.4",
          "form": "저는 진짜 꼬박꼬박 챙겨봤어요",
          "original_form": "저는 진짜 꼬박꼬박 챙겨봤어요",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100030119.1.5",
          "form": "안산 선수 진짜 멋지지 않나요??ㅠㅠ",
          "original_form": "안산 선수 진짜 멋지지 않나요??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100030119.1.6",
          "form": "맞아요!! 여자 선수 4강 준결승 결승 쭈욱 봤는데...",
          "original_form": "맞아요!! 여자 선수 4강 준결승 결승 쭈욱 봤는데...",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030119.1.7",
          "form": "손에 너무 땀이 축축한거예요ㅠ",
          "original_form": "손에 너무 땀이 축축한거예요ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030119.1.8",
          "form": "결승 슛오프에서 엄청 빌었던거같아요ㅠㅠ",
          "original_form": "결승 슛오프에서 엄청 빌었던거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030119.1.9",
          "form": "ㅠㅠㅠㅠㅠ진짜요 ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ진짜요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100030119.1.10",
          "form": "제가 더 난리쳤어요 ㅋㅋㅋㅋ",
          "original_form": "제가 더 난리쳤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100030119.1.11",
          "form": "안산님은 담담하시던데..ㅋㅋㅋ",
          "original_form": "안산님은 담담하시던데..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100030119.1.12",
          "form": "심박수 안오른다면서요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "심박수 안오른다면서요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100030119.1.13",
          "form": "저는 혼자 긴장해서 화면에 대고 입김 불었는데ㅠ",
          "original_form": "저는 혼자 긴장해서 화면에 대고 입김 불었는데ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100030119.1.14",
          "form": "ㅋㅋㅋㅋㅋ엌ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ엌ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100030119.1.15",
          "form": "진짜 금메달 따고 소리질렀어요",
          "original_form": "진짜 금메달 따고 소리질렀어요",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100030119.1.16",
          "form": "너무 짜릿해서,,",
          "original_form": "너무 짜릿해서,,",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100030119.1.17",
          "form": "저도 친구들  있는 카톡방에 엄청 난리쳤어요ㅠ",
          "original_form": "저도 친구들  있는 카톡방에 엄청 난리쳤어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030119.1.18",
          "form": "금메달!!!!!!!!! 금메달.금메달!!! 하면서 막 ㅠ",
          "original_form": "금메달!!!!!!!!! 금메달.금메달!!! 하면서 막 ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030119.1.19",
          "form": "ㅠㅠㅠㅠㅠㅠ그쵸 ㅠㅠㅠㅠ으으",
          "original_form": "ㅠㅠㅠㅠㅠㅠ그쵸 ㅠㅠㅠㅠ으으",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030119.1.20",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030119.1.21",
          "form": "3관왕 말도 안돼요 진짜",
          "original_form": "3관왕 말도 안돼요 진짜",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030119.1.22",
          "form": "너무 귀엽고,,",
          "original_form": "너무 귀엽고,,",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.23",
          "form": "이번에 맞아요ㅠㅠ",
          "original_form": "이번에 맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.24",
          "form": "단일종목 3관왕 세계 최초 타이틀...",
          "original_form": "단일종목 3관왕 세계 최초 타이틀...",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.25",
          "form": "이제 20살인데 너무 멋있어요ㅠㅠ",
          "original_form": "이제 20살인데 너무 멋있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.26",
          "form": "그니까요ㅠㅠㅠ",
          "original_form": "그니까요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.27",
          "form": "진짜 너무 멋있어서 기절",
          "original_form": "진짜 너무 멋있어서 기절",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.28",
          "form": "안산선수 멋있어서...",
          "original_form": "안산선수 멋있어서...",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.29",
          "form": "제 몸에도 주몽의 피가 흐를거라고",
          "original_form": "제 몸에도 주몽의 피가 흐를거라고",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030119.1.30",
          "form": "ㅋㅋㅋㄴㅋㅋㄴㄴㄴ",
          "original_form": "ㅋㅋㅋㄴㅋㅋㄴㄴㄴ",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.31",
          "form": "양궁카페 갔다가",
          "original_form": "양궁카페 갔다가",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.33",
          "form": "실패하고 말았죠..",
          "original_form": "실패하고 말았죠..",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제 친구도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제 친구도",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.35",
          "form": "오진혁 선수분?",
          "original_form": "오진혁 선수분?",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.36",
          "form": "40살 이시라고 자기도 도전한다고",
          "original_form": "40살 이시라고 자기도 도전한다고",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋ그러더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러더라구요",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030119.1.38",
          "form": "희망이 있다고 ㅋㅋㅋ",
          "original_form": "희망이 있다고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100030119.1.39",
          "form": "아 ㅋㅋㅋㅋ ㅠㅠㅠㅠ 저도 스포츠 도전하기에 늦은 나이가 아닐 것이라고",
          "original_form": "아 ㅋㅋㅋㅋ ㅠㅠㅠㅠ 저도 스포츠 도전하기에 늦은 나이가 아닐 것이라고",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030119.1.40",
          "form": "완전 젊은 피라고... 그렇게 설쳤는데..",
          "original_form": "완전 젊은 피라고... 그렇게 설쳤는데..",
          "speaker_id": "2",
