      "utterance": [
        {
          "id": "MDRW2100019109.1.1",
          "form": "저는 고양이를 키워요",
          "original_form": "저는 고양이를 키워요",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019109.1.2",
          "form": "뭐 키우시나요",
          "original_form": "뭐 키우시나요",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019109.1.3",
          "form": "오...저는 동물을 안 좋아해서",
          "original_form": "오...저는 동물을 안 좋아해서",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019109.1.4",
          "form": "안키워요",
          "original_form": "안키워요",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019109.1.5",
          "form": "고양이가 낯선 사람처럼 대해서 늘 새로워요",
          "original_form": "고양이가 낯선 사람처럼 대해서 늘 새로워요",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019109.1.6",
          "form": "강아지는 산책을 가야되니까 힘들더라구요",
          "original_form": "강아지는 산책을 가야되니까 힘들더라구요",
          "speaker_id": "2",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019109.1.7",
          "form": "고양이는 사람한테 정 안 주는 걸로 알아용",
          "original_form": "고양이는 사람한테 정 안 주는 걸로 알아용",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019109.1.8",
          "form": "넹 그래서 강아지가 좋가한데 혼자 두기 미안해서요",
          "original_form": "넹 그래서 강아지가 좋가한데 혼자 두기 미안해서요",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019109.1.9",
          "form": "날씨가 넘더워서 털갈이 해줘야겠어요",
          "original_form": "날씨가 넘더워서 털갈이 해줘야겠어요",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019109.1.10",
          "form": "음 고양이도 털갈이 해주나유",
          "original_form": "음 고양이도 털갈이 해주나유",
          "speaker_id": "1",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019109.1.11",
          "form": "넹 조금 다듬어줘요",
          "original_form": "넹 조금 다듬어줘요",
          "speaker_id": "2",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019109.1.12",
          "form": "헉 첨 알았습니다ㅋ",
          "original_form": "헉 첨 알았습니다ㅋ",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019109.1.13",
          "form": "주택 가면 개 키우려구요",
          "original_form": "주택 가면 개 키우려구요",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019109.1.14",
          "form": "오우 .. 고양이 이름은 뭔가용",
