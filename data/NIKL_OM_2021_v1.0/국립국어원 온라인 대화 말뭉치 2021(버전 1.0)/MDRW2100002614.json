      "utterance": [
        {
          "id": "MDRW2100002614.1.1",
          "form": "요새 날씨가 좀 풀려서 덜 덥네요",
          "original_form": "요새 날씨가 좀 풀려서 덜 덥네요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002614.1.2",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002614.1.3",
          "form": "갑자기 이번주에 확 시원해졌어요",
          "original_form": "갑자기 이번주에 확 시원해졌어요",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002614.1.4",
          "form": "네 비올것 같은 날씨네요",
          "original_form": "네 비올것 같은 날씨네요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002614.1.5",
          "form": "주말인데 비오는건 싫지만 안더워서 좋아요 ㅎㅎ",
          "original_form": "주말인데 비오는건 싫지만 안더워서 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002614.1.6",
          "form": "안그래도 다음주 내내 장마래요ㅠㅠ",
          "original_form": "안그래도 다음주 내내 장마래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002614.1.7",
          "form": "이런 ㅜㅜ 유독 올해는",
          "original_form": "이런 ㅜㅜ 유독 올해는",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002614.1.8",
          "form": "장마가 갑자기 많은 느낌이네요",
          "original_form": "장마가 갑자기 많은 느낌이네요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002614.1.9",
          "form": "그래도 이번장마는 끝나면 가을이니까",
          "original_form": "그래도 이번장마는 끝나면 가을이니까",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002614.1.10",
          "form": "전처럼 엄청 싫지는 않네요ㅎㅎ",
          "original_form": "전처럼 엄청 싫지는 않네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002614.1.11",
          "form": "빨리 가을되면 좋겠어요",
          "original_form": "빨리 가을되면 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002614.1.12",
          "form": "마자요 ㅠㅠ",
          "original_form": "마자요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002614.1.13",
          "form": "슬슬 옷장정리도 해야겠어요",
          "original_form": "슬슬 옷장정리도 해야겠어요",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002614.1.14",
          "form": "가을은 좋은데 옷장정린 너무 귀찮네여...ㅠ",
          "original_form": "가을은 좋은데 옷장정린 너무 귀찮네여...ㅠ",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002614.1.15",
          "form": "요즘은 낮에도 그렇게 덥진않아서 금방 긴팔입을것같아요",
          "original_form": "요즘은 낮에도 그렇게 덥진않아서 금방 긴팔입을것같아요",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002614.1.16",
          "form": "그러게요 잘때도 춥더라구요",
          "original_form": "그러게요 잘때도 춥더라구요",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002614.1.17",
          "form": "가을 금방 끝나지 않음좋겠어요 ㅠㅎ",
          "original_form": "가을 금방 끝나지 않음좋겠어요 ㅠㅎ",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002614.1.18",
          "form": "사계절이 봄가을봄가을이면 좋을텐데",
          "original_form": "사계절이 봄가을봄가을이면 좋을텐데",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002614.1.19",
          "form": "봄가을이 너무 짧아서 아쉬워요",
          "original_form": "봄가을이 너무 짧아서 아쉬워요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002614.1.20",
          "form": "맞아요 넘 아쉽 ㅠㅠ",
          "original_form": "맞아요 넘 아쉽 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002614.1.21",
          "form": "넘 덥지도 춥지도 않은게좋은데~",
          "original_form": "넘 덥지도 춥지도 않은게좋은데~",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002614.1.22",
          "form": "저두요~ 더위도 추위도 많이타서",
          "original_form": "저두요~ 더위도 추위도 많이타서",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002614.1.23",
          "form": "여름겨울 극혐",
          "original_form": "여름겨울 극혐",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002614.1.24",
          "form": "마자요 넘 싫어요 ㅠㅠ 여름~ 겨울~~ 넘 극단적인 날씨",
          "original_form": "마자요 넘 싫어요 ㅠㅠ 여름~ 겨울~~ 넘 극단적인 날씨",
