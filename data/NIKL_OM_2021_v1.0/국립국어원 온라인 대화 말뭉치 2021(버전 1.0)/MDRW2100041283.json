      "utterance": [
        {
          "id": "MDRW2100041283.1.1",
          "form": "아모레 꿀 좋다했거든요 ㅎㅎ",
          "original_form": "아모레 꿀 좋다했거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041283.1.2",
          "form": "사실 팩은 다 거기서거기 같아요",
          "original_form": "사실 팩은 다 거기서거기 같아요",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041283.1.3",
          "form": "마자요ㅋㅋㅋㅋ 성분이 되게 좋은지 잘 모르겠어요",
          "original_form": "마자요ㅋㅋㅋㅋ 성분이 되게 좋은지 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041283.1.4",
          "form": "아이크림도 영양크림과 이름만 다르다고하더라고요",
          "original_form": "아이크림도 영양크림과 이름만 다르다고하더라고요",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041283.1.5",
          "form": "마자요ㅋㅋㅋㅋ그래서 화장품도 거의 안 바르고",
          "original_form": "마자요ㅋㅋㅋㅋ그래서 화장품도 거의 안 바르고",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041283.1.6",
          "form": "그냥 피부과에서 사온 재생크림 하나만 발라여 ㅠ",
          "original_form": "그냥 피부과에서 사온 재생크림 하나만 발라여 ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041283.1.7",
          "form": "피부과 크림은 뭔가 더 좋다곤 하더라구요",
          "original_form": "피부과 크림은 뭔가 더 좋다곤 하더라구요",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041283.1.8",
          "form": "저도 여름엔 오일섞인 토너만 바르기도 해요",
          "original_form": "저도 여름엔 오일섞인 토너만 바르기도 해요",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041283.1.9",
          "form": "왕 피부 미인이실거같아용",
          "original_form": "왕 피부 미인이실거같아용",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041283.1.10",
          "form": "아... 거무죽죽ㅜㅠ",
          "original_form": "아... 거무죽죽ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041283.1.11",
          "form": "미인되고싶네요!",
          "original_form": "미인되고싶네요!",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041283.1.12",
          "form": "ㅋㅋㅋ저는 피부가 안좋게 태어난 타입이라 ㅠㅠ",
          "original_form": "ㅋㅋㅋ저는 피부가 안좋게 태어난 타입이라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041283.1.13",
          "form": "피부과 이제서야 왔더니 돈 왕창 깨지고",
          "original_form": "피부과 이제서야 왔더니 돈 왕창 깨지고",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041283.1.14",
          "form": "ㅜㅜ 어렸을때부터 관리해야한대요ㅜ 훅 가기전에",
          "original_form": "ㅜㅜ 어렸을때부터 관리해야한대요ㅜ 훅 가기전에",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100041283.1.15",
          "form": "마자요...어릴땐 그걸 몰랐어요..",
          "original_form": "마자요...어릴땐 그걸 몰랐어요..",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041283.1.16",
          "form": "다들 시술도 받고 그러고 살고있더라고요ㅋㅋ",
          "original_form": "다들 시술도 받고 그러고 살고있더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 07:55"
        },
        {
          "id": "MDRW2100041283.1.17",
          "form": "그니까요 좀만 일찍 맘먹을걸",
          "original_form": "그니까요 좀만 일찍 맘먹을걸",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100041283.1.18",
          "form": "아직 늦지 않았습니다!! ㅋㅋ 이제부터 같이해요!",
          "original_form": "아직 늦지 않았습니다!! ㅋㅋ 이제부터 같이해요!",
