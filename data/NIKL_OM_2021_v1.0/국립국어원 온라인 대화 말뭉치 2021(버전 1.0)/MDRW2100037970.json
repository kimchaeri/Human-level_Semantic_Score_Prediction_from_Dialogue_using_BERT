      "utterance": [
        {
          "id": "MDRW2100037970.1.1",
          "form": "오늘 날씨 괜찮으셨어요?",
          "original_form": "오늘 날씨 괜찮으셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100037970.1.2",
          "form": "저는 좀 춥더라구요",
          "original_form": "저는 좀 춥더라구요",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100037970.1.3",
          "form": "저도 그랫어요",
          "original_form": "저도 그랫어요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100037970.1.4",
          "form": "흐리고 쌀쌀하더라구요",
          "original_form": "흐리고 쌀쌀하더라구요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100037970.1.5",
          "form": "맞아요 긴팔입어야하는 날씨네요 ..",
          "original_form": "맞아요 긴팔입어야하는 날씨네요 ..",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037970.1.6",
          "form": "이제 진짜 가을이 오나봐요",
          "original_form": "이제 진짜 가을이 오나봐요",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100037970.1.7",
          "form": "그러게요 ..! 젤 좋아하는 계절이 언제신가요?",
          "original_form": "그러게요 ..! 젤 좋아하는 계절이 언제신가요?",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037970.1.8",
          "form": "저는 가을 제일 좋아해서 요즘 넘 좋아요..",
          "original_form": "저는 가을 제일 좋아해서 요즘 넘 좋아요..",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100037970.1.9",
          "form": "저는 봄 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 봄 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100037970.1.10",
          "form": "맞아요 봄도 따뜻해서 넘 좋죠 ㅎㅎ!",
          "original_form": "맞아요 봄도 따뜻해서 넘 좋죠 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100037970.1.11",
          "form": "봄가을이 좋은데 갈수록 봄가을이 짧아지는것같아 슬퍼요 ㅋㅋㅋㅋ",
          "original_form": "봄가을이 좋은데 갈수록 봄가을이 짧아지는것같아 슬퍼요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100037970.1.12",
          "form": "그쵸 ㅠㅠ 이번 가을은 조금 길었으면 좋겠어요",
          "original_form": "그쵸 ㅠㅠ 이번 가을은 조금 길었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100037970.1.13",
          "form": "금방 겨울이 되버리니 슬퍼요..",
          "original_form": "금방 겨울이 되버리니 슬퍼요..",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100037970.1.14",
          "form": "맞아요 ㅠㅠ 전 겨울싫어해서 더더욱 가을 짧은게 슬퍼요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 전 겨울싫어해서 더더욱 가을 짧은게 슬퍼요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100037970.1.15",
          "form": "그니까요.. 전 겨울 눈오는걸 너무 싫어해요...",
          "original_form": "그니까요.. 전 겨울 눈오는걸 너무 싫어해요...",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100037970.1.16",
          "form": "워낙 잘 넘어져서 .. 진짜 조심조심걷고 .. 까맣게되서 질퍽한 것도 넘 싫러요...ㅠ",
          "original_form": "워낙 잘 넘어져서 .. 진짜 조심조심걷고 .. 까맣게되서 질퍽한 것도 넘 싫러요...ㅠ",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100037970.1.17",
          "form": "전 추위를 많이타요...ㅠㅠ",
          "original_form": "전 추위를 많이타요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100037970.1.18",
          "form": "조금만 추워도 턱이 덜덜 떨림 ㅠㅠ",
          "original_form": "조금만 추워도 턱이 덜덜 떨림 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100037970.1.19",
          "form": "춥죠 ㅠㅠ저저번 겨울때 엄청 춥지않았나요 막 다 얼고..",
          "original_form": "춥죠 ㅠㅠ저저번 겨울때 엄청 춥지않았나요 막 다 얼고..",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100037970.1.20",
          "form": "러시아보다 춥던 그시절ㅜㅠ",
          "original_form": "러시아보다 춥던 그시절ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100037970.1.21",
          "form": "진짜 그랬었죠..",
          "original_form": "진짜 그랬었죠..",
          "speaker_id": "1",
