      "utterance": [
        {
          "id": "MDRW2100019766.1.1",
          "form": "알바 많이해보셧나요?",
          "original_form": "알바 많이해보셧나요?",
          "speaker_id": "1",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019766.1.2",
          "form": "아니요 많이는 안해봤어요~",
          "original_form": "아니요 많이는 안해봤어요~",
          "speaker_id": "2",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019766.1.3",
          "form": "알바 해보셨냐요?",
          "original_form": "알바 해보셨냐요?",
          "speaker_id": "2",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019766.1.4",
          "form": "어릴때 많이해봣죠.. 몸쓰는거도 하고.. 서빙도 하고..",
          "original_form": "어릴때 많이해봣죠.. 몸쓰는거도 하고.. 서빙도 하고..",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019766.1.5",
          "form": "뭐 해보엉요?",
          "original_form": "뭐 해보엉요?",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019766.1.6",
          "form": "저는 박람회 알바 해봤어요! 그리고 파리바게트요ㅋㅋㅋ",
          "original_form": "저는 박람회 알바 해봤어요! 그리고 파리바게트요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019766.1.7",
          "form": "박람회 알바요? 진행스텝 같은거여?",
          "original_form": "박람회 알바요? 진행스텝 같은거여?",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019766.1.8",
          "form": "아니요 판매요!!ㅋㅋㅋ",
          "original_form": "아니요 판매요!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019766.1.9",
          "form": "어떤 박람회엿어여?",
          "original_form": "어떤 박람회엿어여?",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019766.1.10",
          "form": "베이비페어요ㅋㅋㅋㅋ",
          "original_form": "베이비페어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019766.1.11",
          "form": "애기들 용품 팔았어요",
          "original_form": "애기들 용품 팔았어요",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019766.1.12",
          "form": "오... 그런데 의외로 엄청 팔리던데..",
          "original_form": "오... 그런데 의외로 엄청 팔리던데..",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019766.1.13",
          "form": "할만하셧나여?",
          "original_form": "할만하셧나여?",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019766.1.14",
          "form": "네ㅋㅋㅋ 근데 품목에 따라 파리 날리는 곳도 있어요ㅋㅋ 그런곳 걸리면 좀 지겹긴해요",
          "original_form": "네ㅋㅋㅋ 근데 품목에 따라 파리 날리는 곳도 있어요ㅋㅋ 그런곳 걸리면 좀 지겹긴해요",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019766.1.15",
          "form": "바쁜게 좋다는 말씀?",
          "original_form": "바쁜게 좋다는 말씀?",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019766.1.16",
          "form": "바쁘면 힘들어도 시간은 금방 가니까요ㅋㅋ",
          "original_form": "바쁘면 힘들어도 시간은 금방 가니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019766.1.17",
          "form": "그게 알바하긴 편하죠.. ㅎㅎㅎ",
          "original_form": "그게 알바하긴 편하죠.. ㅎㅎㅎ",
