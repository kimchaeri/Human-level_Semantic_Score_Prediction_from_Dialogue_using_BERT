      "utterance": [
        {
          "id": "MDRW2100033042.1.1",
          "form": "집놀이족은 몬가요??",
          "original_form": "집놀이족은 몬가요??",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033042.1.2",
          "form": "집에서 먹고 자고 놀고 이러는거 아닐까요",
          "original_form": "집에서 먹고 자고 놀고 이러는거 아닐까요",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033042.1.3",
          "form": "반식욕 보석십자수 패브릭포스터",
          "original_form": "반식욕 보석십자수 패브릭포스터",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033042.1.4",
          "form": "이런느낌",
          "original_form": "이런느낌",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033042.1.5",
          "form": "아 저는 뒹굴족 입니다 ㅋㅋ",
          "original_form": "아 저는 뒹굴족 입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033042.1.6",
          "form": "집꾸미기도 있구요",
          "original_form": "집꾸미기도 있구요",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033042.1.7",
          "form": "전 꾸미는거에는 소질이 없더라구요 똥..손,..ㅋㅋㅋ",
          "original_form": "전 꾸미는거에는 소질이 없더라구요 똥..손,..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033042.1.8",
          "form": "저도인데 ㅎㅎ",
          "original_form": "저도인데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033042.1.9",
          "form": "그럼 넷플릭스 같은거 보시나요",
          "original_form": "그럼 넷플릭스 같은거 보시나요",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033042.1.10",
          "form": "아니요 ㅎㅎ 저는 드라마파입니다,",
          "original_form": "아니요 ㅎㅎ 저는 드라마파입니다,",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033042.1.11",
          "form": "어 한국드라마 좋아요",
          "original_form": "어 한국드라마 좋아요",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033042.1.12",
          "form": "자막 안읽어도 돼구",
          "original_form": "자막 안읽어도 돼구",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033042.1.13",
          "form": "맞아요 ㅎㅎ 집에서 주로 머하고 보내세요?",
          "original_form": "맞아요 ㅎㅎ 집에서 주로 머하고 보내세요?",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033042.1.14",
          "form": "저 외향적이라 힘든데",
          "original_form": "저 외향적이라 힘든데",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033042.1.15",
          "form": "제주가 날씨 안좋으면 다니기 그래서",
          "original_form": "제주가 날씨 안좋으면 다니기 그래서",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033042.1.16",
          "form": "청귤 사서 청담고요",
          "original_form": "청귤 사서 청담고요",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033042.1.17",
          "form": "아 ㅋㅋㅋ 지금 별로에요??",
          "original_form": "아 ㅋㅋㅋ 지금 별로에요??",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033042.1.18",
          "form": "날씨가?",
          "original_form": "날씨가?",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033042.1.19",
          "form": "네 왔다갔다해요",
          "original_form": "네 왔다갔다해요",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033042.1.20",
          "form": "오 청귤청.ㅎㅎ 집콕하니",
          "original_form": "오 청귤청.ㅎㅎ 집콕하니",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033042.1.21",
          "form": "그런재미도",
          "original_form": "그런재미도",
          "speaker_id": "2",
          "time": "20210906 14:46"
        }
