      "utterance": [
        {
          "id": "MDRW2100045892.1.1",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045892.1.2",
          "form": "만보 걸으신적 있나요?",
          "original_form": "만보 걸으신적 있나요?",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045892.1.3",
          "form": "평일엔 거의요",
          "original_form": "평일엔 거의요",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045892.1.4",
          "form": "회사에서도 자주 돌아다니려고 하고 잇어요",
          "original_form": "회사에서도 자주 돌아다니려고 하고 잇어요",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045892.1.5",
          "form": "진짜 대단하세요",
          "original_form": "진짜 대단하세요",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045892.1.6",
          "form": "주말엔 바닥에만 붙어잇어요ㅋ",
          "original_form": "주말엔 바닥에만 붙어잇어요ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045892.1.7",
          "form": "전 퇴근후 3정거장 앞에 내려서 걷는게 다인데ㅋㅋㅋㅋ",
          "original_form": "전 퇴근후 3정거장 앞에 내려서 걷는게 다인데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045892.1.8",
          "form": "주말은 쉬어야죠ㅋㅋㅋㅋ",
          "original_form": "주말은 쉬어야죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045892.1.9",
          "form": "세정거장되 꽤되지않나요",
          "original_form": "세정거장되 꽤되지않나요",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045892.1.10",
          "form": "네 그렇긴 한데 워크온으로 보면",
          "original_form": "네 그렇긴 한데 워크온으로 보면",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045892.1.11",
          "form": "하루 5,6천 정도라",
          "original_form": "하루 5,6천 정도라",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045892.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045892.1.13",
          "form": "그것도 한시간정도는 되죠",
          "original_form": "그것도 한시간정도는 되죠",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045892.1.14",
          "form": "천보가 십분정도더라고요",
          "original_form": "천보가 십분정도더라고요",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045892.1.15",
          "form": "한시간 좀안되요..",
          "original_form": "한시간 좀안되요..",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045892.1.16",
          "form": "루트를 바꿔볼까요?",
          "original_form": "루트를 바꿔볼까요?",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045892.1.17",
          "form": "같은길은 지루하긴하죠",
          "original_form": "같은길은 지루하긴하죠",
          "speaker_id": "1",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045892.1.18",
          "form": "새로운길을 개척해야겠어요ㅋㅋㅋㅋ",
          "original_form": "새로운길을 개척해야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045892.1.19",
          "form": "좋아하는 스타일 있으신가요?",
          "original_form": "좋아하는 스타일 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045892.1.20",
          "form": "어떤?",
          "original_form": "어떤?",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045892.1.21",
          "form": "패션이옷",
          "original_form": "패션이옷",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045892.1.22",
          "form": "전 하늘하늘 원피스요ㅋ",
          "original_form": "전 하늘하늘 원피스요ㅋ",
          "speaker_id": "1",
