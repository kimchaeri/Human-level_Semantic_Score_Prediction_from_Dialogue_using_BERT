      "utterance": [
        {
          "id": "MDRW2100028192.1.1",
          "form": "피프티피플이 딱 그래",
          "original_form": "피프티피플이 딱 그래",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028192.1.2",
          "form": "진짜 주인공이 50명이거든",
          "original_form": "진짜 주인공이 50명이거든",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028192.1.3",
          "form": "옴니버스같은데? 또 다른 내용에도 얽혀서 나오고",
          "original_form": "옴니버스같은데? 또 다른 내용에도 얽혀서 나오고",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028192.1.4",
          "form": "누구누구 헤어졌다며? 그 남자친구가 누구라던데에 나온 남자친구가 또 주인공으로 나중에 나와",
          "original_form": "누구누구 헤어졌다며? 그 남자친구가 누구라던데에 나온 남자친구가 또 주인공으로 나중에 나와",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028192.1.5",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028192.1.6",
          "form": "약간 여기서 이런 말 해도 되나",
          "original_form": "약간 여기서 이런 말 해도 되나",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028192.1.7",
          "form": "name3언니 아는 친구 중에서",
          "original_form": "&name3&언니 아는 친구 중에서",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028192.1.8",
          "form": "다코야키 좋아하시는 분 있잖아",
          "original_form": "다코야키 좋아하시는 분 있잖아",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028192.1.9",
          "form": "그분이 커뮤니티 만든거 주제가 그거랑 비슷했어",
          "original_form": "그분이 커뮤니티 만든거 주제가 그거랑 비슷했어",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028192.1.10",
          "form": "소설 주인공들이 마지막권에서 총집합한다는",
          "original_form": "소설 주인공들이 마지막권에서 총집합한다는",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028192.1.11",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028192.1.12",
          "form": "이것도 마지막에 보면",
          "original_form": "이것도 마지막에 보면",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028192.1.13",
          "form": "결국 사실은 다들 모이는 연계점이 생겨",
          "original_form": "결국 사실은 다들 모이는 연계점이 생겨",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028192.1.14",
          "form": "그럼 뭔가 이야기가 좀더 생생한 것 같아",
          "original_form": "그럼 뭔가 이야기가 좀더 생생한 것 같아",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028192.1.15",
          "form": "오... 본인 소설에서 그런 기믹을 적용한단 말이야?",
          "original_form": "오... 본인 소설에서 그런 기믹을 적용한단 말이야?",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028192.1.16",
          "form": "진짜 대단한 작가님인데",
          "original_form": "진짜 대단한 작가님인데",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028192.1.17",
          "form": "대단하지",
          "original_form": "대단하지",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028192.1.18",
          "form": "이야기 마다 몇몇씩 자꾸 등장하는 것도 좋아",
          "original_form": "이야기 마다 몇몇씩 자꾸 등장하는 것도 좋아",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028192.1.19",
          "form": "진짜 똑똑한듯",
          "original_form": "진짜 똑똑한듯",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028192.1.20",
          "form": "이건 팬들 좋아할만하다",
          "original_form": "이건 팬들 좋아할만하다",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028192.1.21",
          "form": "나도 까메오출연 이런거 좋아하는데",
          "original_form": "나도 까메오출연 이런거 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028192.1.22",
          "form": "소설 인물이 다른 곳에서 나오는건 매력적이야",
          "original_form": "소설 인물이 다른 곳에서 나오는건 매력적이야",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028192.1.23",
          "form": "한 소설 속에서긴 하지만",
          "original_form": "한 소설 속에서긴 하지만",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028192.1.24",
          "form": "어쨌든 50편의 단편이니까!",
          "original_form": "어쨌든 50편의 단편이니까!",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028192.1.25",
          "form": "그래도 좋아",
          "original_form": "그래도 좋아",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028192.1.26",
          "form": "나도 난중에 책읽고 싶어지면 봐야지",
          "original_form": "나도 난중에 책읽고 싶어지면 봐야지",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028192.1.27",
          "form": "피프티피플이었지",
          "original_form": "피프티피플이었지",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028192.1.28",
          "form": "인천에 있었으면 빌려주는건데",
          "original_form": "인천에 있었으면 빌려주는건데",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028192.1.29",
          "form": "흡 아냐 난 책 빌려 읽는거보다 내가 직접 사서 읽는걸 좋아해",
          "original_form": "흡 아냐 난 책 빌려 읽는거보다 내가 직접 사서 읽는걸 좋아해",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028192.1.30",
          "form": "나는 둘 다 좋아",
          "original_form": "나는 둘 다 좋아",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028192.1.31",
          "form": "일단 그냥 궁금만하면 먼저 읽어보고 사기도 하고",
          "original_form": "일단 그냥 궁금만하면 먼저 읽어보고 사기도 하고",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028192.1.32",
          "form": "난 내 소유인 뭔가가 좋은가바",
          "original_form": "난 내 소유인 뭔가가 좋은가바",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028192.1.33",
          "form": "그건 나도 그래...그래서 이제 슬슬 책장이 버거워해",
          "original_form": "그건 나도 그래...그래서 이제 슬슬 책장이 버거워해",
          "speaker_id": "2",
          "time": "20210902 18:32"
        }
      ]
