      "utterance": [
        {
          "id": "MDRW2100026824.1.1",
          "form": "커피좋아하세요?",
          "original_form": "커피좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026824.1.2",
          "form": "배달음식 주문하시나요",
          "original_form": "배달음식 주문하시나요",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.4",
          "form": "커피는 아메리카노 마십니다",
          "original_form": "커피는 아메리카노 마십니다",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.5",
          "form": "제일 쌉니다",
          "original_form": "제일 쌉니다",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.6",
          "form": "요리를 못해서 배달을 많이해요",
          "original_form": "요리를 못해서 배달을 많이해요",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.7",
          "form": "아메리카노가 깔끔하죠",
          "original_form": "아메리카노가 깔끔하죠",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.8",
          "form": "네 싸고요",
          "original_form": "네 싸고요",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.9",
          "form": "남이해주는 요리가 젤 맛나요",
          "original_form": "남이해주는 요리가 젤 맛나요",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.10",
          "form": "가성비가 좋습니다",
          "original_form": "가성비가 좋습니다",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.11",
          "form": "진리입니다",
          "original_form": "진리입니다",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026824.1.12",
          "form": "전 뜨아보다 아아가 좋아요",
          "original_form": "전 뜨아보다 아아가 좋아요",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026824.1.13",
          "form": "얼음까지 깨먹으면 스트레스 빵",
          "original_form": "얼음까지 깨먹으면 스트레스 빵",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026824.1.14",
          "form": "얼죽아입니나",
          "original_form": "얼죽아입니나",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026824.1.15",
          "form": "완벽음료에요",
          "original_form": "완벽음료에요",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026824.1.16",
          "form": "커피는 얼마하나요?",
          "original_form": "커피는 얼마하나요?",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026824.1.17",
          "form": "저는 1900원짜리 마십니다",
          "original_form": "저는 1900원짜리 마십니다",
          "speaker_id": "1",
