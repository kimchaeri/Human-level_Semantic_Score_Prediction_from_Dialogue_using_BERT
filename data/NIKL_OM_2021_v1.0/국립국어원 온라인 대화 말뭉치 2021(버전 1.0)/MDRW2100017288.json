      "utterance": [
        {
          "id": "MDRW2100017288.1.1",
          "form": "지금 퇴근",
          "original_form": "지금 퇴근",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017288.1.2",
          "form": "무료봉사 근무 1시간",
          "original_form": "무료봉사 근무 1시간",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017288.1.3",
          "form": "저녁은??",
          "original_form": "저녁은??",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017288.1.4",
          "form": "지금 먹을려고\r\n순대국밥",
          "original_form": "지금 먹을려고\r\n순대국밥",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017288.1.5",
          "form": "너는 저녁 뭐먹어?\r\n정했어?",
          "original_form": "너는 저녁 뭐먹어?\r\n정했어?",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017288.1.6",
          "form": "오 순대국밥? 식당메뉴에 나왔어?",
          "original_form": "오 순대국밥? 식당메뉴에 나왔어?",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017288.1.7",
          "form": "응ㅎ",
          "original_form": "응ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017288.1.8",
          "form": "난 그냥 밥에 고추참치 김 이렇게 먹었엉",
          "original_form": "난 그냥 밥에 고추참치 김 이렇게 먹었엉",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017288.1.9",
          "form": "배가 불러서 저녁 고민하다 그냥 먹어ㅋ",
          "original_form": "배가 불러서 저녁 고민하다 그냥 먹어ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017288.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017288.1.11",
          "form": "맛있는거 주네 ㅋㅋ 너 순대국밥 좋아하지않았어?",
          "original_form": "맛있는거 주네 ㅋㅋ 너 순대국밥 좋아하지않았어?",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017288.1.12",
          "form": "참치 김 맛나",
          "original_form": "참치 김 맛나",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017288.1.13",
          "form": "맞어 내장 좋아해",
          "original_form": "맞어 내장 좋아해",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017288.1.14",
          "form": "근데 여긴 너장 쪼금줘 ㅎ",
          "original_form": "근데 여긴 너장 쪼금줘 ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017288.1.15",
          "form": "내장",
          "original_form": "내장",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017288.1.16",
          "form": "아 그렇구만 아까비 ㅎㅎ 그래도 맛있겠엉 ㅋㅋ",
          "original_form": "아 그렇구만 아까비 ㅎㅎ 그래도 맛있겠엉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017288.1.17",
          "form": "응 식당밥 가성비는 아니라도 맛은 괜찮은편이야ㅎ",
          "original_form": "응 식당밥 가성비는 아니라도 맛은 괜찮은편이야ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017288.1.18",
          "form": "ㅇㅇ 메뉴가 다양하게 나와서 좋을것 같아",
          "original_form": "ㅇㅇ 메뉴가 다양하게 나와서 좋을것 같아",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017288.1.19",
          "form": "집에선 매일 먹는거 먹으니까",
          "original_form": "집에선 매일 먹는거 먹으니까",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017288.1.20",
          "form": "내말 안갔어ㅜㅠ\r\n\r\n식당밥도 좋지만 먹다봄 질리니까\r\n식단은 다양해야 하는거같아ㅎ",
          "original_form": "내말 안갔어ㅜㅠ\r\n\r\n식당밥도 좋지만 먹다봄 질리니까\r\n식단은 다양해야 하는거같아ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017288.1.21",
          "form": "ㅇㅇ 다양한게 좋아 식단 기대하는 즐거움도 있고",
          "original_form": "ㅇㅇ 다양한게 좋아 식단 기대하는 즐거움도 있고",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017288.1.22",
          "form": "마져 골라먹는 재미",
          "original_form": "마져 골라먹는 재미",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017288.1.23",
          "form": "저녁 다먹었어ㅋ",
          "original_form": "저녁 다먹었어ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:47"
        }
