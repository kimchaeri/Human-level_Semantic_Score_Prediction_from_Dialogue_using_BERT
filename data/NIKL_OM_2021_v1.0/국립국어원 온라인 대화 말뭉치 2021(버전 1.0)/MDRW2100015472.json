      "utterance": [
        {
          "id": "MDRW2100015472.1.1",
          "form": "메추리알 맛있긴 한데",
          "original_form": "메추리알 맛있긴 한데",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015472.1.2",
          "form": "우리집은 반찬 할 때 한번에 많이 하는 편이라",
          "original_form": "우리집은 반찬 할 때 한번에 많이 하는 편이라",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015472.1.3",
          "form": "껍찔제거가 넘 오래걸려",
          "original_form": "껍찔제거가 넘 오래걸려",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015472.1.4",
          "form": "안그래두 그거땜에",
          "original_form": "안그래두 그거땜에",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015472.1.5",
          "form": "엄마가 잘안해줘요",
          "original_form": "엄마가 잘안해줘요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015472.1.6",
          "form": "메추리알장조림은 사먹어야댐...ㅎㅎ...",
          "original_form": "메추리알장조림은 사먹어야댐...ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015472.1.7",
          "form": "ㅇㅈㅇㅈ 손 많이 가",
          "original_form": "ㅇㅈㅇㅈ 손 많이 가",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015472.1.8",
          "form": "반찬가게 짱",
          "original_form": "반찬가게 짱",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015472.1.9",
          "form": "흑흑 메추리알 장조림",
          "original_form": "흑흑 메추리알 장조림",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015472.1.10",
          "form": "잔뜩 쌓아두고",
          "original_form": "잔뜩 쌓아두고",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015472.1.11",
          "form": "밥보다ㅜ장조림을 더 많이 집어먹고 싶어요",
          "original_form": "밥보다ㅜ장조림을 더 많이 집어먹고 싶어요",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015472.1.12",
          "form": "그 예전에 요리왕 비룡에 나오는 아저씨처럼",
          "original_form": "그 예전에 요리왕 비룡에 나오는 아저씨처럼",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015472.1.13",
          "form": "숨 들이쉬면서",
          "original_form": "숨 들이쉬면서",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015472.1.14",
          "form": "어떤 아저씨욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어떤 아저씨욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015472.1.15",
          "form": "그 볶음밥 마시는 아저씨요",
          "original_form": "그 볶음밥 마시는 아저씨요",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015472.1.16",
          "form": "어릴때 보면서 와 쩐다",
          "original_form": "어릴때 보면서 와 쩐다",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015472.1.17",
          "form": "아 그분",
          "original_form": "아 그분",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015472.1.18",
          "form": "나도 하고싶다 이랬는데",
          "original_form": "나도 하고싶다 이랬는데",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015472.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015472.1.20",
          "form": "밥 맛난거 드셨나여",
          "original_form": "밥 맛난거 드셨나여",
          "speaker_id": "2",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100015472.1.21",
          "form": "오늘도 김볶밥데이야",
          "original_form": "오늘도 김볶밥데이야",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100015472.1.22",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100015472.1.23",
          "form": "전 오늘도 오징어젓갈과",
          "original_form": "전 오늘도 오징어젓갈과",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100015472.1.24",
          "form": "김자반데이",
          "original_form": "김자반데이",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100015472.1.25",
          "form": "ㅎㅎ...",
          "original_form": "ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100015472.1.26",
          "form": "앗 네버엔딩 루프인가",
          "original_form": "앗 네버엔딩 루프인가",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100015472.1.27",
          "form": "둘 다 기시감 있는 메뉴인데",
          "original_form": "둘 다 기시감 있는 메뉴인데",
