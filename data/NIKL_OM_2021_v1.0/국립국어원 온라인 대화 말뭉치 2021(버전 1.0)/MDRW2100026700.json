      "utterance": [
        {
          "id": "MDRW2100026700.1.1",
          "form": "1955  검색하고 왔어요",
          "original_form": "1955  검색하고 왔어요",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026700.1.2",
          "form": "아 전 그 포실한 감자 좋아해요",
          "original_form": "아 전 그 포실한 감자 좋아해요",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026700.1.3",
          "form": "아보시란 감사",
          "original_form": "아보시란 감사",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026700.1.4",
          "form": "복고풍인가 했는데 고기가 실한 버거군요ㅎ",
          "original_form": "복고풍인가 했는데 고기가 실한 버거군요ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026700.1.5",
          "form": "포시란 감자 너무 좋아 좋아",
          "original_form": "포시란 감자 너무 좋아 좋아",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026700.1.6",
          "form": "고기가 어마어마합니다",
          "original_form": "고기가 어마어마합니다",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026700.1.7",
          "form": "버거킹은 사달라",
          "original_form": "버거킹은 사달라",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026700.1.8",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026700.1.9",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026700.1.10",
          "form": "김영철이 환율을 예측했다는 사딸라ㅎ",
          "original_form": "김영철이 환율을 예측했다는 사딸라ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026700.1.11",
          "form": "런치 사딸라 넘 좋아요ㅎㅎ",
          "original_form": "런치 사딸라 넘 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026700.1.12",
          "form": "ㅋㅋㅋㅋㅋ팩트인가요?",
          "original_form": "ㅋㅋㅋㅋㅋ팩트인가요?",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026700.1.13",
          "form": "여기는 고기는 그리 안 두꺼워도 po 소고기 wer 이느낌이라ㅎ",
          "original_form": "여기는 고기는 그리 안 두꺼워도 po 소고기 wer 이느낌이라ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026700.1.14",
          "form": "팩트였는데ㅎ 이제 다시 팩트가 아니게 됐네요ㅎㅎ",
          "original_form": "팩트였는데ㅎ 이제 다시 팩트가 아니게 됐네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026700.1.15",
          "form": "파워 소고기",
          "original_form": "파워 소고기",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026700.1.16",
          "form": "버거킹은 소스가 조금 강한 느낌이 나지 않나요",
          "original_form": "버거킹은 소스가 조금 강한 느낌이 나지 않나요",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026700.1.17",
          "form": "미국 냄새가 흠뻑 나더라구요",
          "original_form": "미국 냄새가 흠뻑 나더라구요",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026700.1.18",
          "form": "칠번가 냄새",
          "original_form": "칠번가 냄새",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026700.1.19",
          "form": "저 가는곳은 소스 적게 주던데ㅜ",
          "original_form": "저 가는곳은 소스 적게 주던데ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026700.1.20",
          "form": "사람 차별하나요",
          "original_form": "사람 차별하나요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026700.1.21",
          "form": "100프로 차별 입니다",
          "original_form": "100프로 차별 입니다",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026700.1.22",
          "form": "ㅂㄷㅂㄷ 버거킹과 님  고소",
          "original_form": "ㅂㄷㅂㄷ 버거킹과 님  고소",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026700.1.23",
          "form": "님 건강 생각해서 소스 적게 준 거임 ㅎㅎ",
          "original_form": "님 건강 생각해서 소스 적게 준 거임 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026700.1.24",
          "form": "ㅎㅎ 그래도 박탈감을 안겨줬으니 님 고소할 거임ㅎㅅㅎ",
          "original_form": "ㅎㅎ 그래도 박탈감을 안겨줬으니 님 고소할 거임ㅎㅅㅎ",
          "speaker_id": "2",
          "time": "20210901 23:49"
        }
      ]
    }
