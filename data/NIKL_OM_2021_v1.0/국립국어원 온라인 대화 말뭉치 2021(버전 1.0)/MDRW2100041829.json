      "utterance": [
        {
          "id": "MDRW2100041829.1.1",
          "form": "이제 거의 가을 날씨네요!",
          "original_form": "이제 거의 가을 날씨네요!",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041829.1.2",
          "form": "사시는 곳은 비오나요?",
          "original_form": "사시는 곳은 비오나요?",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041829.1.3",
          "form": "앗 아뇽! 여긴 아침부터 점심무렵까지 빗방울 떨어지더니 이젠 안오네요 ㅎㅎ",
          "original_form": "앗 아뇽! 여긴 아침부터 점심무렵까지 빗방울 떨어지더니 이젠 안오네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041829.1.4",
          "form": "name1님 계시는 곳은 비오고 있나요?",
          "original_form": "&name1&님 계시는 곳은 비오고 있나요?",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041829.1.5",
          "form": "저 사는곳도 잠깐 오더니 이제 그쳤어요",
          "original_form": "저 사는곳도 잠깐 오더니 이제 그쳤어요",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041829.1.6",
          "form": "집갈땐 안왛으면 좋겠네요 ㅠㅜ",
          "original_form": "집갈땐 안왛으면 좋겠네요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041829.1.7",
          "form": "다행히 비는 안왔는데 바람이 엄청 부네요..",
          "original_form": "다행히 비는 안왔는데 바람이 엄청 부네요..",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100041829.1.8",
          "form": "태풍 온것 같아요..",
          "original_form": "태풍 온것 같아요..",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100041829.1.9",
          "form": "아니 그러게요!",
          "original_form": "아니 그러게요!",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100041829.1.10",
          "form": "여긴 비 갑자기 엄청 많이 어ㅏ요 ㅜㅜ",
          "original_form": "여긴 비 갑자기 엄청 많이 어ㅏ요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100041829.1.11",
          "form": "이게 무슨 일이래요ㅜㅜ",
          "original_form": "이게 무슨 일이래요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100041829.1.12",
          "form": "차가 바람에 막 흔들릴 정도에요;;",
          "original_form": "차가 바람에 막 흔들릴 정도에요;;",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100041829.1.13",
          "form": "아고 그래도 집 가실땐 비 안맞으셨나요?ㅜㅜ",
          "original_form": "아고 그래도 집 가실땐 비 안맞으셨나요?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100041829.1.14",
          "form": "저 지금 갑니다!!",
          "original_form": "저 지금 갑니다!!",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100041829.1.15",
          "form": "엄마차 타고 가려고요",
          "original_form": "엄마차 타고 가려고요",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100041829.1.16",
          "form": "흑흗",
          "original_form": "흑흗",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100041829.1.17",
          "form": "비 짇짜 많이 오져",
          "original_form": "비 짇짜 많이 오져",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100041829.1.18",
          "form": "아고 귀가가 늦으셨군요ㅜㅜ 조심히 들어가셔요!!",
          "original_form": "아고 귀가가 늦으셨군요ㅜㅜ 조심히 들어가셔요!!",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100041829.1.19",
          "form": "빗길 운전 조심하시길..!",
          "original_form": "빗길 운전 조심하시길..!",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100041829.1.20",
          "form": "맞아요 갑자기 비 무슨일이에요 진짜",
          "original_form": "맞아요 갑자기 비 무슨일이에요 진짜",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041829.1.21",
          "form": "얼른 집에 듈아가고파요",
          "original_form": "얼른 집에 듈아가고파요",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041829.1.22",
          "form": "천천히 빠르게 귀가하시길 바라겠습니다..!",
          "original_form": "천천히 빠르게 귀가하시길 바라겠습니다..!",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100041829.1.23",
          "form": "내일도 비가 올까요?",
          "original_form": "내일도 비가 올까요?",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100041829.1.24",
          "form": "저희 지역은 내일 오전에 또 비가 온다곤 하는데 안왔으면 좋겠어요...",
          "original_form": "저희 지역은 내일 오전에 또 비가 온다곤 하는데 안왔으면 좋겠어요...",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100041829.1.25",
          "form": "저두요.. 다행히 내일 저 재택근뮤입이다~~",
          "original_form": "저두요.. 다행히 내일 저 재택근뮤입이다~~",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100041829.1.26",
          "form": "내일은 외출할때 긴팔입어야할거같아요",
          "original_form": "내일은 외출할때 긴팔입어야할거같아요",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100041829.1.27",
