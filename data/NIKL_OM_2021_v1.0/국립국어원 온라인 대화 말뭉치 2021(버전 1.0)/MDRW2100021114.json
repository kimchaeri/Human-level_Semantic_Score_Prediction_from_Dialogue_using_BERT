      "utterance": [
        {
          "id": "MDRW2100021114.1.1",
          "form": "커피 수혈했니?",
          "original_form": "커피 수혈했니?",
          "speaker_id": "2",
          "time": "20210830 12:13"
        },
        {
          "id": "MDRW2100021114.1.2",
          "form": "아늬ㅠ 물만 홀짝",
          "original_form": "아늬ㅠ 물만 홀짝",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100021114.1.3",
          "form": "따흑 티마실거 없어?",
          "original_form": "따흑 티마실거 없어?",
          "speaker_id": "2",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100021114.1.4",
          "form": "응ㅠ아직분위기 파악 안됨ㅋㅋ",
          "original_form": "응ㅠ아직분위기 파악 안됨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100021114.1.5",
          "form": "간식같은거 어케하는지 모르겄네",
          "original_form": "간식같은거 어케하는지 모르겄네",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100021114.1.6",
          "form": "적응기를 거쳐야겠군 ㅎ",
          "original_form": "적응기를 거쳐야겠군 ㅎ",
          "speaker_id": "2",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100021114.1.7",
          "form": "응 커피가 엄청 땡겨ㅋ",
          "original_form": "응 커피가 엄청 땡겨ㅋ",
          "speaker_id": "1",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100021114.1.8",
          "form": "보통 맥심먹어?",
          "original_form": "보통 맥심먹어?",
          "speaker_id": "2",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100021114.1.9",
          "form": "단거는맥심먹구",
          "original_form": "단거는맥심먹구",
          "speaker_id": "1",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100021114.1.10",
          "form": "아아는 수프리모 먹어~",
          "original_form": "아아는 수프리모 먹어~",
          "speaker_id": "1",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100021114.1.11",
          "form": "나는 카누보다 그게 맞더라구",
          "original_form": "나는 카누보다 그게 맞더라구",
          "speaker_id": "1",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100021114.1.12",
          "form": "수프리모~~ 들어봤음!",
          "original_form": "수프리모~~ 들어봤음!",
          "speaker_id": "2",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100021114.1.13",
          "form": "응ㅋ그러나 디카페인이라는 반전",
          "original_form": "응ㅋ그러나 디카페인이라는 반전",
          "speaker_id": "1",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100021114.1.14",
          "form": "엥?ㅋ 수프리모중에 디카페인?ㅎ",
          "original_form": "엥?ㅋ 수프리모중에 디카페인?ㅎ",
          "speaker_id": "2",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100021114.1.15",
          "form": "응ㅋㅋ",
          "original_form": "응ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100021114.1.16",
          "form": "오호ㅋㅋ 근데 커피빨받는건가ㅋ",
          "original_form": "오호ㅋㅋ 근데 커피빨받는건가ㅋ",
          "speaker_id": "2",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100021114.1.17",
          "form": "쓴맛때문인지 잠좀 깨는거같아",
          "original_form": "쓴맛때문인지 잠좀 깨는거같아",
          "speaker_id": "1",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100021114.1.18",
          "form": "위약효과인가",
          "original_form": "위약효과인가",
          "speaker_id": "1",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100021114.1.19",
          "form": "뭐든 배에 들어가믄 힘이나지ㅎ",
          "original_form": "뭐든 배에 들어가믄 힘이나지ㅎ",
