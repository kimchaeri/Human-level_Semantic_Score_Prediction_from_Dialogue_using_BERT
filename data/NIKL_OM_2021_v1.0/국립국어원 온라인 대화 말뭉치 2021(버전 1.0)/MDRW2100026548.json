      "utterance": [
        {
          "id": "MDRW2100026548.1.1",
          "form": "아 ! 저도 거기 한번 가봣던거같은데",
          "original_form": "아 ! 저도 거기 한번 가봣던거같은데",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026548.1.2",
          "form": "풍차 ㅠㅜ 조앗어요 아 정말 어딜가든 조은듯",
          "original_form": "풍차 ㅠㅜ 조앗어요 아 정말 어딜가든 조은듯",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026548.1.3",
          "form": "잘보면 아주 돌고래가 떼로 지어다니더라구요",
          "original_form": "잘보면 아주 돌고래가 떼로 지어다니더라구요",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026548.1.4",
          "form": "아 !! 신기하다 ㅋㅋㅋ",
          "original_form": "아 !! 신기하다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026548.1.5",
          "form": "제주에서는 뭐드셨어요?",
          "original_form": "제주에서는 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026548.1.6",
          "form": "꼭 기회되시면 보시길 추천드려요",
          "original_form": "꼭 기회되시면 보시길 추천드려요",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026548.1.7",
          "form": "넹 돌고래 기다려바야겟네요z",
          "original_form": "넹 돌고래 기다려바야겟네요z",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026548.1.8",
          "form": "조식먹고 민트래스토랑? 갔다가",
          "original_form": "조식먹고 민트래스토랑? 갔다가",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026548.1.9",
          "form": "복자씨연탄구이",
          "original_form": "복자씨연탄구이",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026548.1.10",
          "form": "zzzz 민트레스토랑 핫한덴가",
          "original_form": "zzzz 민트레스토랑 핫한덴가",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026548.1.11",
          "form": "거기뷰조은데죠",
          "original_form": "거기뷰조은데죠",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.12",
          "form": "마자여ㅋㅋ",
          "original_form": "마자여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.13",
          "form": "제친구 두명이나 거기 갓던데ㅋㅋㅋㅋ 모르는사이 ㅋㅋㅋ",
          "original_form": "제친구 두명이나 거기 갓던데ㅋㅋㅋㅋ 모르는사이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.14",
          "form": "사진봣는데 뷰 좋긴하더라구요",
          "original_form": "사진봣는데 뷰 좋긴하더라구요",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.15",
          "form": "이쁘고 안비싸여ㅋㅋ",
          "original_form": "이쁘고 안비싸여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.16",
          "form": "섭지코지 있는데라",
          "original_form": "섭지코지 있는데라",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.17",
          "form": "이쁘긴 하더라구요",
          "original_form": "이쁘긴 하더라구요",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.18",
          "form": "그네 꼭 타야해요ㅋ",
          "original_form": "그네 꼭 타야해요ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.19",
          "form": "ㅋㅋㅋㅋ 저도담에 가야겟네욬ㅋㅋㅋㅋ복자씨연탄구이는 어땟어요",
          "original_form": "ㅋㅋㅋㅋ 저도담에 가야겟네욬ㅋㅋㅋㅋ복자씨연탄구이는 어땟어요",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026548.1.20",
          "form": "웨이팅이 두시간인가ㅋㅋ",
          "original_form": "웨이팅이 두시간인가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026548.1.21",
          "form": "너무너무 화딱지가 났어요ㅋ",
          "original_form": "너무너무 화딱지가 났어요ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026548.1.22",
          "form": "에엥 거기도 핫플인가보네요",
          "original_form": "에엥 거기도 핫플인가보네요",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026548.1.23",
          "form": "맛은 기다리다 지쳐서 기억이 잘 안나여",
          "original_form": "맛은 기다리다 지쳐서 기억이 잘 안나여",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026548.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026548.1.25",
          "form": "재가 추천하는 맛집은 상춘잰가",
          "original_form": "재가 추천하는 맛집은 상춘잰가",
          "speaker_id": "1",
