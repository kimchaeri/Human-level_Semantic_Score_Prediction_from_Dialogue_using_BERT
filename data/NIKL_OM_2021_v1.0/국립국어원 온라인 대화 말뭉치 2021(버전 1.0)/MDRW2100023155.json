      "utterance": [
        {
          "id": "MDRW2100023155.1.1",
          "form": "마자요 진짜 결심히 젤 힘든거자나요",
          "original_form": "마자요 진짜 결심히 젤 힘든거자나요",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.2",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.3",
          "form": "혹시 그",
          "original_form": "혹시 그",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.4",
          "form": "결심을 했다는게 대견한거",
          "original_form": "결심을 했다는게 대견한거",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.6",
          "form": "피부관리는 어떻게 하고 계세요?",
          "original_form": "피부관리는 어떻게 하고 계세요?",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.7",
          "form": "피부과 가여 그냥",
          "original_form": "피부과 가여 그냥",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.8",
          "form": "어머 역시 돈이 최고야",
          "original_form": "어머 역시 돈이 최고야",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.9",
          "form": "다 부질없다는 생각이 들엇어요",
          "original_form": "다 부질없다는 생각이 들엇어요",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.10",
          "form": "저는 그냥 모든것을 돈으로 조지는 중입니다",
          "original_form": "저는 그냥 모든것을 돈으로 조지는 중입니다",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.11",
          "form": "피부과 얼마나 들어요?",
          "original_form": "피부과 얼마나 들어요?",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023155.1.12",
          "form": "저는 주근깨랑 여드름이 고민이었기 때문에",
          "original_form": "저는 주근깨랑 여드름이 고민이었기 때문에",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.13",
          "form": "여드름 케어랑 토닝을 받는 중인데",
          "original_form": "여드름 케어랑 토닝을 받는 중인데",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.14",
          "form": "피부과 막 한 번에 3만~5만 할 거 같아서 못가겠어요",
          "original_form": "피부과 막 한 번에 3만~5만 할 거 같아서 못가겠어요",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.15",
          "form": "첨에는 예산 200정도 잡고 갓으나 의외의 지출로 벌써 500은 쏟은거 같아요",
          "original_form": "첨에는 예산 200정도 잡고 갓으나 의외의 지출로 벌써 500은 쏟은거 같아요",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.16",
          "form": "오우야 ㅋㅋ",
          "original_form": "오우야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.17",
          "form": "효과 엄청나겠네요...",
          "original_form": "효과 엄청나겠네요...",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.18",
          "form": "휴....어릴때 피부관리 했어야 했는데 너무 늦었어요",
          "original_form": "휴....어릴때 피부관리 했어야 했는데 너무 늦었어요",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.20",
          "form": "혹시 그 뭐라고 하지 여드름시체?",
          "original_form": "혹시 그 뭐라고 하지 여드름시체?",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023155.1.21",
          "form": "이런것도 피부과에서 도와주시나여...",
          "original_form": "이런것도 피부과에서 도와주시나여...",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023155.1.22",
          "form": "네네 저 여드름 흉터 치료 받거든요",
          "original_form": "네네 저 여드름 흉터 치료 받거든요",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023155.1.23",
          "form": "그거 레이저 이름 뭐더라?",
          "original_form": "그거 레이저 이름 뭐더라?",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023155.1.24",
          "form": "피부재생 해주는 그 레이저 있던데",
          "original_form": "피부재생 해주는 그 레이저 있던데",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023155.1.25",
          "form": "레이저 이름은 모르겟어요 그냥 저는 의사쌤이 이런이런 시술해야한다 하고 알려주면 해주세요 하는",
          "original_form": "레이저 이름은 모르겟어요 그냥 저는 의사쌤이 이런이런 시술해야한다 하고 알려주면 해주세요 하는",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023155.1.26",
          "form": "아... 그럼 그 착색된 것도 살려주시나요?",
          "original_form": "아... 그럼 그 착색된 것도 살려주시나요?",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023155.1.27",
          "form": "토닝 몇번 받으면 깨끗해지더라규요",
          "original_form": "토닝 몇번 받으면 깨끗해지더라규요",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023155.1.28",
          "form": "아 착색된건 토닝으로 해결하는거에요?",
          "original_form": "아 착색된건 토닝으로 해결하는거에요?",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023155.1.29",
          "form": "네네 토닝으로 해결중 아님 레이저인데",
          "original_form": "네네 토닝으로 해결중 아님 레이저인데",
