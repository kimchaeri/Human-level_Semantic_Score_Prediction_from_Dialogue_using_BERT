      "utterance": [
        {
          "id": "MDRW2100026186.1.1",
          "form": "와 나 반려동물하니까 생각난 건데",
          "original_form": "와 나 반려동물하니까 생각난 건데",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026186.1.2",
          "form": "초딩때 병아리 키웠어",
          "original_form": "초딩때 병아리 키웠어",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026186.1.3",
          "form": "맞아 어릴 때 병아리 많이 키우더라",
          "original_form": "맞아 어릴 때 병아리 많이 키우더라",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026186.1.4",
          "form": "헐 자세히 얘기해줘~",
          "original_form": "헐 자세히 얘기해줘~",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026186.1.5",
          "form": "걍... 학교 앞에서 파는 500원짜리 병아리",
          "original_form": "걍... 학교 앞에서 파는 500원짜리 병아리",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026186.1.6",
          "form": "데려와서 엄마한테 냅다 보여줬던 거 같애",
          "original_form": "데려와서 엄마한테 냅다 보여줬던 거 같애",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026186.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026186.1.8",
          "form": "싫어하면서 병아리 키울 박스나 이런 건 다 줬는듯",
          "original_form": "싫어하면서 병아리 키울 박스나 이런 건 다 줬는듯",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026186.1.9",
          "form": "근데 다 죽었어",
          "original_form": "근데 다 죽었어",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.10",
          "form": "맞아 학교 주변에 그렇게 많이 팔았었지",
          "original_form": "맞아 학교 주변에 그렇게 많이 팔았었지",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.11",
          "form": "아니야 하나는 도망갔어",
          "original_form": "아니야 하나는 도망갔어",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.12",
          "form": "도망갔어???",
          "original_form": "도망갔어???",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.13",
          "form": "걔가 좀 오래 산 애였는데",
          "original_form": "걔가 좀 오래 산 애였는데",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.14",
          "form": "도망가서 좀 찾아댕겼는데",
          "original_form": "도망가서 좀 찾아댕겼는데",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.15",
          "form": "할아버지가",
          "original_form": "할아버지가",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.16",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.17",
          "form": "그거 고양이가 물어갔을 거라고",
          "original_form": "그거 고양이가 물어갔을 거라고",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026186.1.18",
          "form": "헐 너무 비참한 결말인데",
          "original_form": "헐 너무 비참한 결말인데",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026186.1.19",
          "form": "그랬어",
          "original_form": "그랬어",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026186.1.20",
          "form": "나 예전에는 독립하면 고양이 키우겠단",
          "original_form": "나 예전에는 독립하면 고양이 키우겠단",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.21",
          "form": "굳은 의지가 있었는데",
          "original_form": "굳은 의지가 있었는데",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.22",
          "form": "그 일 이후로",
          "original_form": "그 일 이후로",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.23",
          "form": "지금은 걍 나 자신만 키우기로 마음 먹었어",
          "original_form": "지금은 걍 나 자신만 키우기로 마음 먹었어",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.24",
          "form": "그 의지가 사라졌니",
          "original_form": "그 의지가 사라졌니",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.25",
          "form": "딱히 그런 건 아니고",
          "original_form": "딱히 그런 건 아니고",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.26",
          "form": "ㅋㅋㅋㅋㅈㅋㅋ좋은 생각",
          "original_form": "ㅋㅋㅋㅋㅈㅋㅋ좋은 생각",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.27",
          "form": "걍... 현실에 순응해버렸음",
          "original_form": "걍... 현실에 순응해버렸음",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.28",
          "form": "나 하나 먹여 살리기도",
          "original_form": "나 하나 먹여 살리기도",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026186.1.29",
          "form": "벅차다",
          "original_form": "벅차다",
          "speaker_id": "1",
          "time": "20210901 20:50"
        }
      ]
    }
