      "utterance": [
        {
          "id": "MDRW2100000607.1.1",
          "form": "휴가는 다녀오셨어요?",
          "original_form": "휴가는 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000607.1.2",
          "form": "아뇨\n그냥 가까운 해변만 다녀왔어요",
          "original_form": "아뇨\n그냥 가까운 해변만 다녀왔어요",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000607.1.3",
          "form": "요즘 코로나땜에 어디가기도 글킨하죠\n저는 성주다녀왔어요",
          "original_form": "요즘 코로나땜에 어디가기도 글킨하죠\n저는 성주다녀왔어요",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000607.1.4",
          "form": "오오 곶감이 유명한 곳이죠",
          "original_form": "오오 곶감이 유명한 곳이죠",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000607.1.5",
          "form": "근데 사람들이 코로나땜에 안다니는지 자연휴양림 물놀이장에 저희랑 1팀 더있었어요 ㅎㅎ",
          "original_form": "근데 사람들이 코로나땜에 안다니는지 자연휴양림 물놀이장에 저희랑 1팀 더있었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000607.1.6",
          "form": "그래요? 여기해변은 폐쇄했음에도\n사람들이 엄청 많아서 놀랐어용",
          "original_form": "그래요? 여기해변은 폐쇄했음에도\n사람들이 엄청 많아서 놀랐어용",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000607.1.7",
          "form": "성주에 자연휴양림도 있나요?",
          "original_form": "성주에 자연휴양림도 있나요?",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000607.1.8",
          "form": "네 독용산성자연휴양림이라고 있더라구요\n산에 ㅎㅎ",
          "original_form": "네 독용산성자연휴양림이라고 있더라구요\n산에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000607.1.9",
          "form": "아~ 저도 막 검색해봤어요\n성주호 근처라 그런지 \n산과 물 둘 다 가볐네요^^",
          "original_form": "아~ 저도 막 검색해봤어요\n성주호 근처라 그런지 \n산과 물 둘 다 가볐네요^^",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000607.1.10",
          "form": "가졌네요",
          "original_form": "가졌네요",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000607.1.11",
          "form": "산언덕에 있어서 길이 좀 가파른 단점은 있지만 뷰도 공기도 좋긴해요",
          "original_form": "산언덕에 있어서 길이 좀 가파른 단점은 있지만 뷰도 공기도 좋긴해요",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000607.1.12",
          "form": "힐링하기 좋아보이네요\n가족과 같이 가신거예요?",
          "original_form": "힐링하기 좋아보이네요\n가족과 같이 가신거예요?",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000607.1.13",
          "form": "네 가족들같이 갔어요\n전자렌지가 없는게 또다른단점 ㅎㅎ",
          "original_form": "네 가족들같이 갔어요\n전자렌지가 없는게 또다른단점 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000607.1.14",
          "form": "해변어디로다녀오셨어요?",
          "original_form": "해변어디로다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000607.1.15",
          "form": "헉 렌지가 없다니\n여행지에서 간단하게 음식 데워먹기엔\n렌지가 편하고 좋은데",
          "original_form": "헉 렌지가 없다니\n여행지에서 간단하게 음식 데워먹기엔\n렌지가 편하고 좋은데",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000607.1.16",
          "form": "인천 을왕리요",
          "original_form": "인천 을왕리요",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000607.1.17",
          "form": "그러니까요 깜짝 놀랬어요 그게 좀 불편했어요",
          "original_form": "그러니까요 깜짝 놀랬어요 그게 좀 불편했어요",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000607.1.18",
          "form": "그럼 식사는 나가서 드신거예요?\n아님 가스로 요리를",
          "original_form": "그럼 식사는 나가서 드신거예요?\n아님 가스로 요리를",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000607.1.19",
          "form": "아 인덕션이구나",
          "original_form": "아 인덕션이구나",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000607.1.20",
          "form": "아뇨 고기 구워서 먹고 밥은 햇반가져갔는데 렌지가 없어서 뜨거운물에 넣어두니 1시간걸리더라구요 ㅎ",
          "original_form": "아뇨 고기 구워서 먹고 밥은 햇반가져갔는데 렌지가 없어서 뜨거운물에 넣어두니 1시간걸리더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000607.1.21",
          "form": "아~ 햇반\n렌지없으면 곤란하지요",
          "original_form": "아~ 햇반\n렌지없으면 곤란하지요",
          "speaker_id": "2",
          "time": "20210819 17:26"
        }
      ]
