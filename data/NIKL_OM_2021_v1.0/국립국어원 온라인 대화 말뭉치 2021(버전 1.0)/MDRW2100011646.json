      "utterance": [
        {
          "id": "MDRW2100011646.1.1",
          "form": "기억에 남는 여행지는요?",
          "original_form": "기억에 남는 여행지는요?",
          "speaker_id": "2",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011646.1.2",
          "form": "송도요",
          "original_form": "송도요",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011646.1.3",
          "form": "야경이 예뻤어요",
          "original_form": "야경이 예뻤어요",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011646.1.4",
          "form": "오 저는 송도 안가봤어요 궁금하네요",
          "original_form": "오 저는 송도 안가봤어요 궁금하네요",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011646.1.5",
          "form": "도심 하늘에 갈매기가 날아다니는데",
          "original_form": "도심 하늘에 갈매기가 날아다니는데",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011646.1.6",
          "form": "너무 웃겨요",
          "original_form": "너무 웃겨요",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011646.1.7",
          "form": "보트같은것도 있던데 타보셨나요",
          "original_form": "보트같은것도 있던데 타보셨나요",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011646.1.8",
          "form": "아니요 탈시간이 없어서",
          "original_form": "아니요 탈시간이 없어서",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011646.1.9",
          "form": "송도가 인프라가 좋더라고요",
          "original_form": "송도가 인프라가 좋더라고요",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011646.1.10",
          "form": "자전거타면서 돌아다녀도 재미있었어요",
          "original_form": "자전거타면서 돌아다녀도 재미있었어요",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011646.1.11",
          "form": "그럴것같아요~ 티비에서 보면 좋아보이더라구요",
          "original_form": "그럴것같아요~ 티비에서 보면 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011646.1.12",
          "form": "돈만 많으면 살고싶네요",
          "original_form": "돈만 많으면 살고싶네요",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011646.1.13",
          "form": "그러게요 집값도 엄청 비쌀테죠..",
          "original_form": "그러게요 집값도 엄청 비쌀테죠..",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011646.1.14",
          "form": "님은 좋았던 여행지가 있나요?",
          "original_form": "님은 좋았던 여행지가 있나요?",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011646.1.15",
          "form": "저는 제주도가 참 좋더라구요",
          "original_form": "저는 제주도가 참 좋더라구요",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011646.1.16",
          "form": "제주도 좋죠~",
          "original_form": "제주도 좋죠~",
          "speaker_id": "1",
          "time": "20210824 15:14"
        }
      ]
