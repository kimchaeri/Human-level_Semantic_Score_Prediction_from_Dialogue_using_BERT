        {
          "id": "MDRW2100020798.1.1",
          "form": "야구좋아하세요?",
          "original_form": "야구좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020798.1.2",
          "form": "야구 ㅋㅋㅋ",
          "original_form": "야구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020798.1.3",
          "form": "원래 안좋아했었는데 규칙 알고 나니까 좀 재밌더라구요",
          "original_form": "원래 안좋아했었는데 규칙 알고 나니까 좀 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020798.1.4",
          "form": "그런데 올림픽 때 야구는 좀 답답했어요",
          "original_form": "그런데 올림픽 때 야구는 좀 답답했어요",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020798.1.5",
          "form": "문제가 많았죠",
          "original_form": "문제가 많았죠",
          "speaker_id": "2",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020798.1.6",
          "form": "선수들 상태가 왜그래요?",
          "original_form": "선수들 상태가 왜그래요?",
          "speaker_id": "1",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020798.1.7",
          "form": "국가대표 맞아요?",
          "original_form": "국가대표 맞아요?",
          "speaker_id": "1",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020798.1.8",
          "form": "ㅋㅋㅋㅋ 아주 빠진거죠",
          "original_form": "ㅋㅋㅋㅋ 아주 빠진거죠",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020798.1.9",
          "form": "메달만 따면 그냥 군면제니까",
          "original_form": "메달만 따면 그냥 군면제니까",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020798.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ확률 거의 50퍼 아니였어요?",
