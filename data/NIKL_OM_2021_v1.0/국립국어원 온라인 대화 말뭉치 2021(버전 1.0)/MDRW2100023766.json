      "utterance": [
        {
          "id": "MDRW2100023766.1.1",
          "form": "좋아하는연예인은없으신가용?\r\n",
          "original_form": "좋아하는연예인은없으신가용?\r\n",
          "speaker_id": "1",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023766.1.2",
          "form": "좋아하는 연예인은...",
          "original_form": "좋아하는 연예인은...",
          "speaker_id": "2",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100023766.1.3",
          "form": "예쁜 연예인은 다좋아요..ㅋㅋㅋㅋ",
          "original_form": "예쁜 연예인은 다좋아요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023766.1.4",
          "form": "누구 팬이신가요?",
          "original_form": "누구 팬이신가요?",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023766.1.5",
          "form": "오 솔직함ㅋㅋㅋㄱㅋㅋ",
          "original_form": "오 솔직함ㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023766.1.6",
          "form": "저 김재환 좋아합니다ㅋㅋㄱㅋㅋ",
          "original_form": "저 김재환 좋아합니다ㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023766.1.7",
          "form": "아시려나요..",
          "original_form": "아시려나요..",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023766.1.8",
          "form": "어? 그 가수?",
          "original_form": "어? 그 가수?",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023766.1.9",
          "form": "박명수? 아닌가.. 배우인가요?",
          "original_form": "박명수? 아닌가.. 배우인가요?",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023766.1.10",
          "form": "아 박명수ㅋㅋㅋㅋ 그분 유재환씨일거예요ㅋㄲ",
          "original_form": "아 박명수ㅋㅋㅋㅋ 그분 유재환씨일거예요ㅋㄲ",
          "speaker_id": "1",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023766.1.11",
          "form": "워너원 출신 가수입니당 김재환은ㅋㅋㄲ",
          "original_form": "워너원 출신 가수입니당 김재환은ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023766.1.12",
          "form": "김재환... 프로듀서 때부터 픽이엇어요?",
          "original_form": "김재환... 프로듀서 때부터 픽이엇어요?",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023766.1.13",
          "form": "앗 네!  프로듀스아시네요..ㅋㅋㅋㄱ",
          "original_form": "앗 네!  프로듀스아시네요..ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100023766.1.14",
          "form": "워낙 유명했잖아요..",
          "original_form": "워낙 유명했잖아요..",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023766.1.15",
          "form": "대한민국을 떠들석하게 했으니.. ㅋㅋㅋ",
          "original_form": "대한민국을 떠들석하게 했으니.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023766.1.16",
          "form": "막 엄마아빠폰 총 동원해서 투표하고 그러셧나여?",
          "original_form": "막 엄마아빠폰 총 동원해서 투표하고 그러셧나여?",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023766.1.17",
          "form": "네...( ..)\r\nㅋㅋㅋㄱㅋㅋㄱ약간 미쳤던 시절이라구 하면 될 것 같아요...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네...( ..)\r\nㅋㅋㅋㄱㅋㅋㄱ약간 미쳤던 시절이라구 하면 될 것 같아요...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023766.1.18",
          "form": "돌아보면 흑역사에여? ㅋㅋㅋ",
