      "utterance": [
        {
          "id": "MDRW2100023841.1.1",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210830 18:52"
        },
        {
          "id": "MDRW2100023841.1.2",
          "form": "사료는 어디서 구매하세요?",
          "original_form": "사료는 어디서 구매하세요?",
          "speaker_id": "1",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100023841.1.3",
          "form": "저는 직접 병원가서 사고있습니다.",
          "original_form": "저는 직접 병원가서 사고있습니다.",
          "speaker_id": "2",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100023841.1.4",
          "form": "나이에따라서 달라져서",
          "original_form": "나이에따라서 달라져서",
          "speaker_id": "2",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100023841.1.5",
          "form": "직접 가서 사시는균요",
          "original_form": "직접 가서 사시는균요",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023841.1.6",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023841.1.7",
          "form": "부모님이 사다주시죠...ㅋㅋㅋ",
          "original_form": "부모님이 사다주시죠...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023841.1.8",
          "form": "앗ㅋㅋ 부모님 찬스는 감사함이죠ㅋㅋ",
          "original_form": "앗ㅋㅋ 부모님 찬스는 감사함이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023841.1.9",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:23"
        },
        {
          "id": "MDRW2100023841.1.10",
          "form": "함부로 못쓰긴하지만ㅠ",
          "original_form": "함부로 못쓰긴하지만ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100023841.1.11",
          "form": "ㅋㅋㅋ그래도 해주시니 감사하죠ㅋㅋ",
          "original_form": "ㅋㅋㅋ그래도 해주시니 감사하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100023841.1.12",
          "form": "저는 인터넷 주문하거든요",
          "original_form": "저는 인터넷 주문하거든요",
          "speaker_id": "1",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100023841.1.13",
          "form": "아하 인터넷이 편하긴하죵",
          "original_form": "아하 인터넷이 편하긴하죵",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023841.1.14",
          "form": "넹ㅋㅋㅋ 종류도 많고 세일도 있구요",
          "original_form": "넹ㅋㅋㅋ 종류도 많고 세일도 있구요",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023841.1.15",
          "form": "인터넷이 싸고 종류도 많죠!",
          "original_form": "인터넷이 싸고 종류도 많죠!",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023841.1.16",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023841.1.17",
          "form": "간식도 오프에서 사세요?",
          "original_form": "간식도 오프에서 사세요?",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023841.1.18",
          "form": "간식은 항상 인터넷이요!",
          "original_form": "간식은 항상 인터넷이요!",
          "speaker_id": "2",
          "time": "20210830 21:00"
        }
      ]
