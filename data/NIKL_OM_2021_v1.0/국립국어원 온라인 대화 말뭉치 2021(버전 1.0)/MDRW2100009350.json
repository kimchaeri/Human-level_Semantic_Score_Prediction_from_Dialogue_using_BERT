      "utterance": [
        {
          "id": "MDRW2100009350.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009350.1.2",
          "form": "당연한거 아닌가요......",
          "original_form": "당연한거 아닌가요......",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009350.1.3",
          "form": "안하면 어떻게 사나요 ㅋㅋ",
          "original_form": "안하면 어떻게 사나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009350.1.4",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009350.1.5",
          "form": "분담하더라도 하는건 하는거니까요",
          "original_form": "분담하더라도 하는건 하는거니까요",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009350.1.6",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210823 07:17"
        },
        {
          "id": "MDRW2100009350.1.7",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100009350.1.8",
          "form": "화장실전담이요",
          "original_form": "화장실전담이요",
          "speaker_id": "1",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100009350.1.9",
          "form": "오 전 청소기랑 빨래요",
          "original_form": "오 전 청소기랑 빨래요",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009350.1.10",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009350.1.11",
          "form": "전 락스쓰면 확 깨끗해진 느낌들어서 좋던데요",
          "original_form": "전 락스쓰면 확 깨끗해진 느낌들어서 좋던데요",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009350.1.12",
          "form": "머리도 아프고",
          "original_form": "머리도 아프고",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009350.1.13",
          "form": "락스 뿌려뒀다가 화장실 문닫고 환풍기 켜뒀다가",
          "original_form": "락스 뿌려뒀다가 화장실 문닫고 환풍기 켜뒀다가",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009350.1.14",
          "form": "나중에 솔질하면서 찬물로 씻어내면 괜찮은데..",
          "original_form": "나중에 솔질하면서 찬물로 씻어내면 괜찮은데..",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009350.1.15",
          "form": "따뜻한물은 쓰지마시구요",
          "original_form": "따뜻한물은 쓰지마시구요",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
