      "utterance": [
        {
          "id": "MDRW2100026357.1.1",
          "form": "백신은 맞으셨나요?",
          "original_form": "백신은 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026357.1.2",
          "form": "네 화이자 1차 맞았어요!",
          "original_form": "네 화이자 1차 맞았어요!",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026357.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026357.1.4",
          "form": "와 축하드려요",
          "original_form": "와 축하드려요",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026357.1.5",
          "form": "저는 25일쯤 예약이요",
          "original_form": "저는 25일쯤 예약이요",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026357.1.6",
          "form": "무섭네요ㅠㅠ",
          "original_form": "무섭네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026357.1.7",
          "form": "타이레놀 꼭꼭 챙기셔요ㅠㅠ",
          "original_form": "타이레놀 꼭꼭 챙기셔요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026357.1.8",
          "form": "화이자는 1차는 안아프대요!",
          "original_form": "화이자는 1차는 안아프대요!",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026357.1.9",
          "form": "저도 미열만 쭉 있다 말았어요!",
          "original_form": "저도 미열만 쭉 있다 말았어요!",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026357.1.10",
          "form": "네 꼭 챙길게요",
          "original_form": "네 꼭 챙길게요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026357.1.11",
          "form": "건강이 최고죠",
          "original_form": "건강이 최고죠",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026357.1.12",
          "form": "맞아요 진짜 건강이 최고에요 딴거 다 있어도 건강하지 않으면 무슨 소용이에요ㅠㅠ",
          "original_form": "맞아요 진짜 건강이 최고에요 딴거 다 있어도 건강하지 않으면 무슨 소용이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026357.1.13",
          "form": "그렇더라구요 다음주에 건강검진을해서 검진후 주사맞는게 좋겠다생각이들었어요",
          "original_form": "그렇더라구요 다음주에 건강검진을해서 검진후 주사맞는게 좋겠다생각이들었어요",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026357.1.14",
          "form": "아 건강검진 하시는군요!",
          "original_form": "아 건강검진 하시는군요!",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026357.1.15",
          "form": "네 이번주금요일에 해요",
          "original_form": "네 이번주금요일에 해요",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100026357.1.16",
          "form": "건강검진도 떨리긴 마찬가지네요ㅋㅋㅋ",
          "original_form": "건강검진도 떨리긴 마찬가지네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100026357.1.17",
          "form": "좋은 결과 나오실거에요!",
          "original_form": "좋은 결과 나오실거에요!",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100026357.1.18",
          "form": "저도 올해 해야하는데 전 백신 2차까지 맞고 가려고요ㅎㅎ",
          "original_form": "저도 올해 해야하는데 전 백신 2차까지 맞고 가려고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100026357.1.19",
          "form": "주기적인 건강검진이 꼭 필요한거같아요",
          "original_form": "주기적인 건강검진이 꼭 필요한거같아요",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100026357.1.20",
          "form": "건강하게 오래살려면ㅋㅋ",
          "original_form": "건강하게 오래살려면ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100026357.1.21",
          "form": "맞아요 조기에 발견하면 치료되는 병들이 많잖아요 이제",
          "original_form": "맞아요 조기에 발견하면 치료되는 병들이 많잖아요 이제",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100026357.1.22",
          "form": "그쵸^^",
          "original_form": "그쵸^^",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100026357.1.23",
          "form": "부디건강한몸이길요",
          "original_form": "부디건강한몸이길요",
          "speaker_id": "1",
          "time": "20210905 22:35"
        }
      ]
