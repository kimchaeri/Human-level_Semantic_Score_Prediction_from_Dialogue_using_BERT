        {
          "id": "MDRW2100034974.1.1",
          "form": "우비다시살생각없나요",
          "original_form": "우비다시살생각없나요",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034974.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034974.1.3",
          "form": "항상 제 마음 속 1위 우비가 있어요",
          "original_form": "항상 제 마음 속 1위 우비가 있어요",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034974.1.4",
          "form": "타이페이 다시 가야되고둔요",
          "original_form": "타이페이 다시 가야되고둔요",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034974.1.5",
          "form": "타이페이역 앞 우비가게 가서",
          "original_form": "타이페이역 앞 우비가게 가서",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034974.1.6",
          "form": "사오는거버킷리스트",
          "original_form": "사오는거버킷리스트",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034974.1.7",
          "form": "거기이뻐요?!",
          "original_form": "거기이뻐요?!",
          "speaker_id": "2",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034974.1.8",
          "form": "가격은어때요??!",
          "original_form": "가격은어때요??!",
          "speaker_id": "2",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034974.1.9",
          "form": "가격 몰라여",
          "original_form": "가격 몰라여",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034974.1.10",
          "form": "그치만 대만 생필품이니까 마니 안 비싸지 않을까요",
          "original_form": "그치만 대만 생필품이니까 마니 안 비싸지 않을까요",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034974.1.11",
          "form": "아하 디자인이궁금해지네요",
          "original_form": "아하 디자인이궁금해지네요",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034974.1.12",
          "form": "검은 우비였고",
          "original_form": "검은 우비였고",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034974.1.13",
          "form": "체크무늬에 가까웟던걸로 기억합니다",
          "original_form": "체크무늬에 가까웟던걸로 기억합니다",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034974.1.14",
          "form": "아쉬워아쉬워",
          "original_form": "아쉬워아쉬워",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034974.1.15",
          "form": "오 깔끔하겠는데요",
          "original_form": "오 깔끔하겠는데요",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034974.1.16",
          "form": "대만가서 쇼핑하고싶어요",
          "original_form": "대만가서 쇼핑하고싶어요",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034974.1.17",
          "form": "저도 어딜가서든 돈 쓰고 다니면 기분 좋을득요",
          "original_form": "저도 어딜가서든 돈 쓰고 다니면 기분 좋을득요",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034974.1.18",
          "form": "기분진짜좋을듯요ㅋㅋㅋ많이벌어놔야겠어요",
          "original_form": "기분진짜좋을듯요ㅋㅋㅋ많이벌어놔야겠어요",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034974.1.19",
          "form": "여기서부터저기까지주세요는 언제해볼수있을까요",
          "original_form": "여기서부터저기까지주세요는 언제해볼수있을까요",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034974.1.20",
          "form": "주식부자 되시길",
          "original_form": "주식부자 되시길",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034974.1.21",
          "form": "ㅋㅋㅋㅋㄱㅋ하 마이너스나복구햇으면.ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ하 마이너스나복구햇으면.ㅋㅋㅋㄱㅋ",
