      "utterance": [
        {
          "id": "MDRW2100015684.1.1",
          "form": "저도 독립하면 따라하고 싶네여 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "저도 독립하면 따라하고 싶네여 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015684.1.2",
          "form": "이케아에 있지않을까요 아님 주문제작해야할듯요",
          "original_form": "이케아에 있지않을까요 아님 주문제작해야할듯요",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015684.1.3",
          "form": "잃어버린 것들의 책 <- 이라는 책 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잃어버린 것들의 책 <- 이라는 책 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015684.1.4",
          "form": "오 첨들었어요",
          "original_form": "오 첨들었어요",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015684.1.5",
          "form": "찾아봐야지",
          "original_form": "찾아봐야지",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015684.1.6",
          "form": "쫌 옛날책이라...",
          "original_form": "쫌 옛날책이라...",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015684.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015684.1.8",
          "form": "좀 어두운 분위기인데 동화풍이예요 어른들을 위한 동화 느낌",
          "original_form": "좀 어두운 분위기인데 동화풍이예요 어른들을 위한 동화 느낌",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015684.1.9",
          "form": "..페이지가 600장이라는군요 ^^;",
          "original_form": "..페이지가 600장이라는군요 ^^;",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015684.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋ좀 두껍긴하죠 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ좀 두껍긴하죠 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015684.1.11",
          "form": "호신용",
          "original_form": "호신용",
          "speaker_id": "1",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015684.1.12",
          "form": "아님 팔근육용으로도 좋겠어요",
          "original_form": "아님 팔근육용으로도 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015684.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015684.1.14",
          "form": "진짜 호신용책은 나니아 연대기 합본이 아닐까요...",
          "original_form": "진짜 호신용책은 나니아 연대기 합본이 아닐까요...",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015684.1.15",
          "form": "아 그러네요",
          "original_form": "아 그러네요",
          "speaker_id": "1",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015684.1.16",
          "form": "그건 넣을공간 계산이 안나와서 사지도 못했어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 넣을공간 계산이 안나와서 사지도 못했어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015684.1.17",
          "form": "권수도 많아",
          "original_form": "권수도 많아",
          "speaker_id": "1",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015684.1.18",
          "form": "저는 모방범 재밌게 읽었어요",
          "original_form": "저는 모방범 재밌게 읽었어요",
          "speaker_id": "1",
          "time": "20210826 00:20"
        },
        {
