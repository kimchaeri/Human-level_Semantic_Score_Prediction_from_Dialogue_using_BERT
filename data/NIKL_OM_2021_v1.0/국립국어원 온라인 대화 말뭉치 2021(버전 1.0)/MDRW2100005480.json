      "utterance": [
        {
          "id": "MDRW2100005480.1.1",
          "form": "나도 그래.. 오늘도 피났어ㅠ",
          "original_form": "나도 그래.. 오늘도 피났어ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005480.1.2",
          "form": "아들도 오늘 입술 건조하다고 그러던데",
          "original_form": "아들도 오늘 입술 건조하다고 그러던데",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005480.1.3",
          "form": "입술 보호제 하나 사",
          "original_form": "입술 보호제 하나 사",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005480.1.4",
          "form": "약국 가서 니베아꺼 하나 사와야겠다",
          "original_form": "약국 가서 니베아꺼 하나 사와야겠다",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005480.1.5",
          "form": "집에 있는 건 오래된거지?",
          "original_form": "집에 있는 건 오래된거지?",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005480.1.6",
          "form": "ㅇㅇ 작년에 산거 ㅎㅎ",
          "original_form": "ㅇㅇ 작년에 산거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005480.1.7",
          "form": "다시 사야겠다",
          "original_form": "다시 사야겠다",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005480.1.8",
          "form": "무향으로 사야겠지?",
          "original_form": "무향으로 사야겠지?",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005480.1.9",
          "form": "글치 딸기향 발라줄려고? ㅋㅋㅋㅋ",
          "original_form": "글치 딸기향 발라줄려고? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005480.1.10",
          "form": "그러네 ㅋㅋㅋ 딸기향은 색깔도 있는데",
          "original_form": "그러네 ㅋㅋㅋ 딸기향은 색깔도 있는데",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005480.1.11",
          "form": "오~ 노 참아줘 ㅎㅎ",
          "original_form": "오~ 노 참아줘 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005480.1.12",
          "form": "왜~ 요즘 마스크 써서 회사에서도 안보일텐데 ㅋㅋ",
          "original_form": "왜~ 요즘 마스크 써서 회사에서도 안보일텐데 ㅋㅋ",
