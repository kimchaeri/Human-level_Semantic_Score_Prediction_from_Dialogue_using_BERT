          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013748.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013748.1.6",
          "form": "뭘주로 파시나요?",
          "original_form": "뭘주로 파시나요?",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013748.1.7",
          "form": "화장품이요",
          "original_form": "화장품이요",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013748.1.8",
          "form": "뭐 파셨어요?",
          "original_form": "뭐 파셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013748.1.9",
          "form": "음 전",
          "original_form": "음 전",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013748.1.10",
          "form": "토익책이런거팔아봤네요ㅋㅋㅋ",
          "original_form": "토익책이런거팔아봤네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013748.1.11",
          "form": "아아 ㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013748.1.12",
          "form": "이제 필요없는거예요?",
          "original_form": "이제 필요없는거예요?",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013748.1.13",
          "form": "네네 거의5년도",
          "original_form": "네네 거의5년도",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013748.1.14",
          "form": "더된책들이네요ㅋㅋㅋ",
          "original_form": "더된책들이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013748.1.15",
          "form": "ㅋㅋㅋㅋ 팔려서 다행이네요",
          "original_form": "ㅋㅋㅋㅋ 팔려서 다행이네요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013748.1.16",
          "form": "다른것들도 팔아보세요 안쓰는거",
          "original_form": "다른것들도 팔아보세요 안쓰는거",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100013748.1.17",
          "form": "ㅋㅋㅋ 더이상팔게없네요",
          "original_form": "ㅋㅋㅋ 더이상팔게없네요",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100013748.1.18",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
