      "utterance": [
        {
          "id": "MDRW2100016105.1.1",
          "form": "저녁은 뭘 드실 예정이신가요",
          "original_form": "저녁은 뭘 드실 예정이신가요",
          "speaker_id": "1",
          "time": "20210826 09:41"
        },
        {
          "id": "MDRW2100016105.1.2",
          "form": "치킨먹고싶은데 아직 모르겠어요",
          "original_form": "치킨먹고싶은데 아직 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 09:41"
        },
        {
          "id": "MDRW2100016105.1.3",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20210826 09:42"
        },
        {
          "id": "MDRW2100016105.1.4",
          "form": "전 수제버거 먹고 들어가려구요 ㅎ",
          "original_form": "전 수제버거 먹고 들어가려구요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 09:50"
        },
        {
          "id": "MDRW2100016105.1.5",
          "form": "아 햄버거 맛있겠네요ㅎㅎ",
          "original_form": "아 햄버거 맛있겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:51"
        },
        {
          "id": "MDRW2100016105.1.6",
          "form": "혼밥이라 만만한 게 햄버거네요 ㅎ",
          "original_form": "혼밥이라 만만한 게 햄버거네요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:04"
        },
        {
          "id": "MDRW2100016105.1.7",
          "form": "맛있기도 하구요ㅎㅎ 감튀먹고싶네요ㅎㅎ",
          "original_form": "맛있기도 하구요ㅎㅎ 감튀먹고싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:05"
        },
        {
          "id": "MDRW2100016105.1.8",
          "form": "감튀는 필수죠 ㅎㅎ",
          "original_form": "감튀는 필수죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016105.1.9",
          "form": "저도 급끌리네요ㅎㅎ맥날",
          "original_form": "저도 급끌리네요ㅎㅎ맥날",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016105.1.10",
          "form": "맥날 최애가 뭔가요",
          "original_form": "맥날 최애가 뭔가요",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016105.1.11",
          "form": "전 핫크리스피좋아해요ㅋㅋ",
          "original_form": "전 핫크리스피좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016105.1.12",
          "form": "맥날은 치킨이죠 ㅎㅎ",
          "original_form": "맥날은 치킨이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016105.1.13",
          "form": "맞아요 은근잘해요ㅋㅋ",
          "original_form": "맞아요 은근잘해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016105.1.14",
          "form": "가볍게 먹기 좋아요 ㅋㅋ",
          "original_form": "가볍게 먹기 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016105.1.15",
          "form": "네 바로 포장해와도 좋구...",
          "original_form": "네 바로 포장해와도 좋구...",
