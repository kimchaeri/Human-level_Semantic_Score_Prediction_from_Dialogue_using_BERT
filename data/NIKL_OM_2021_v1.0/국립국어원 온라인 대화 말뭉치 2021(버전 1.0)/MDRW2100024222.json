      "utterance": [
        {
          "id": "MDRW2100024222.1.1",
          "form": "그냥 마딛는 논란이요",
          "original_form": "그냥 마딛는 논란이요",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024222.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024222.1.3",
          "form": "아 ㅋㅋ ㅋ진짜 웃겨",
          "original_form": "아 ㅋㅋ ㅋ진짜 웃겨",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024222.1.4",
          "form": "그래서 최강창민이 뭐라구 했어요?",
          "original_form": "그래서 최강창민이 뭐라구 했어요?",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024222.1.5",
          "form": "기억은 안나는데",
          "original_form": "기억은 안나는데",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024222.1.6",
          "form": "물의를 일으켜서 죄송하다 그랫나",
          "original_form": "물의를 일으켜서 죄송하다 그랫나",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024222.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024222.1.8",
          "form": "제금 유튜브에 찾아서 재생중 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제금 유튜브에 찾아서 재생중 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024222.1.9",
          "form": "이 때 샤기컷 유행했던 거 새삼 떠오르네요",
          "original_form": "이 때 샤기컷 유행했던 거 새삼 떠오르네요",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024222.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024222.1.11",
          "form": "아니 근데 마딛는이 맞나봐여",
          "original_form": "아니 근데 마딛는이 맞나봐여",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024222.1.12",
          "form": "후 임용고시 책 또 다시 꺼내고 싶지 않은데 ㅠ",
          "original_form": "후 임용고시 책 또 다시 꺼내고 싶지 않은데 ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024222.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024222.1.14",
          "form": "아 진짜 마딛는이라고 하네 진짜 웃곀ㅋㅋ",
          "original_form": "아 진짜 마딛는이라고 하네 진짜 웃곀ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024222.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024222.1.16",
          "form": "근데 되게 옛날인데도 하나도 안변했네요 동방신기..",
          "original_form": "근데 되게 옛날인데도 하나도 안변했네요 동방신기..",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024222.1.17",
          "form": "워낙 잘생겻던 사람들이라 그런가..",
          "original_form": "워낙 잘생겻던 사람들이라 그런가..",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024222.1.18",
          "form": "지금 봐도 진짜 잘생겼네용",
          "original_form": "지금 봐도 진짜 잘생겼네용",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024222.1.19",
          "form": "그치만 서강준 남주혁 승",
          "original_form": "그치만 서강준 남주혁 승",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024222.1.20",
          "form": "진짜 외모가 센세이션했었어요",
          "original_form": "진짜 외모가 센세이션했었어요",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024222.1.21",
          "form": "하지만 아이돌보단 배우가 잘생겻어요",
          "original_form": "하지만 아이돌보단 배우가 잘생겻어요",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024222.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024222.1.23",
          "form": "되게 추억 얘기하니까 좋네요 ㅎㅎ",
          "original_form": "되게 추억 얘기하니까 좋네요 ㅎㅎ",
