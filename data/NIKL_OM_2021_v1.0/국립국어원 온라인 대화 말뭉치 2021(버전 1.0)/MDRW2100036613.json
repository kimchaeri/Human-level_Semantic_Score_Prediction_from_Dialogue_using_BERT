      "utterance": [
        {
          "id": "MDRW2100036613.1.1",
          "form": "중고로 팔고 사고 다해봤어요 ㅎㅎ",
          "original_form": "중고로 팔고 사고 다해봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 02:12"
        },
        {
          "id": "MDRW2100036613.1.2",
          "form": "오호 ㅋㅋㅋ저는 사보긴했어요",
          "original_form": "오호 ㅋㅋㅋ저는 사보긴했어요",
          "speaker_id": "2",
          "time": "20210831 02:16"
        },
        {
          "id": "MDRW2100036613.1.3",
          "form": "얼마전에도 옷 하나 구매했어여 ㅋㅋㅋ",
          "original_form": "얼마전에도 옷 하나 구매했어여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:16"
        },
        {
          "id": "MDRW2100036613.1.4",
          "form": "어디서요? 당근에서요?",
          "original_form": "어디서요? 당근에서요?",
          "speaker_id": "1",
          "time": "20210831 02:17"
        },
        {
          "id": "MDRW2100036613.1.5",
          "form": "저는 번개장터에서!!!",
          "original_form": "저는 번개장터에서!!!",
          "speaker_id": "2",
          "time": "20210831 02:17"
        },
        {
          "id": "MDRW2100036613.1.6",
          "form": "주로 당근이용하시나여?!?!",
          "original_form": "주로 당근이용하시나여?!?!",
          "speaker_id": "2",
          "time": "20210831 02:17"
        },
        {
          "id": "MDRW2100036613.1.7",
          "form": "아뇨 요즘은 중고거래안해요",
          "original_form": "아뇨 요즘은 중고거래안해요",
          "speaker_id": "1",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100036613.1.8",
          "form": "아 기프티콘은 중고로 팔아봤어요",
          "original_form": "아 기프티콘은 중고로 팔아봤어요",
          "speaker_id": "1",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100036613.1.9",
          "form": "아항 ㅋㅋㅋㅋ 저는 사고싶은거 품절되면 검색해봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "아항 ㅋㅋㅋㅋ 저는 사고싶은거 품절되면 검색해봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100036613.1.10",
          "form": "그러다 있으면 럭키.. ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러다 있으면 럭키.. ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100036613.1.11",
          "form": "그러면 중고로 파는 사람 나오나요??",
          "original_form": "그러면 중고로 파는 사람 나오나요??",
          "speaker_id": "1",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100036613.1.12",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "1",
          "time": "20210831 02:18"
        },
        {
          "id": "MDRW2100036613.1.13",
          "form": "네 가끔 중고매물있을때가 있어서 그럼 후다닥사죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 가끔 중고매물있을때가 있어서 그럼 후다닥사죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100036613.1.14",
          "form": "온라인 쇼핑 오프라인쇼핑 어떤게 더좋으세여??",
          "original_form": "온라인 쇼핑 오프라인쇼핑 어떤게 더좋으세여??",
          "speaker_id": "2",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100036613.1.15",
          "form": "좋네요..ㅎㅎㅎ",
          "original_form": "좋네요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100036613.1.16",
          "form": "온라인이 편한데",
          "original_form": "온라인이 편한데",
          "speaker_id": "1",
          "time": "20210831 02:19"
        },
        {
          "id": "MDRW2100036613.1.17",
          "form": "요즘은좀비싸도",
          "original_form": "요즘은좀비싸도",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100036613.1.18",
          "form": "오프라인에서 보고 사고 싶더라구요",
          "original_form": "오프라인에서 보고 사고 싶더라구요",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100036613.1.19",
          "form": "온라인은 충동구매하기 쉬운거같아요",
          "original_form": "온라인은 충동구매하기 쉬운거같아요",
          "speaker_id": "1",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100036613.1.20",
          "form": "오 맞아요 충동구매!!",
          "original_form": "오 맞아요 충동구매!!",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100036613.1.21",
          "form": "오프라인에선 10마넌이면 넘 비싸가 싶은데",
          "original_form": "오프라인에선 10마넌이면 넘 비싸가 싶은데",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100036613.1.22",
          "form": "왜 온라인에서는 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "왜 온라인에서는 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100036613.1.23",
          "form": "10만원짜라도 턱턱사게되는지 ㅋㅋㅋㅋㅋ",
          "original_form": "10만원짜라도 턱턱사게되는지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:20"
        },
        {
          "id": "MDRW2100036613.1.24",
          "form": "결제 다 막아주면",
          "original_form": "결제 다 막아주면",
          "speaker_id": "1",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100036613.1.25",
          "form": "두면",
          "original_form": "두면",
          "speaker_id": "1",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100036613.1.26",
          "form": "안사게되긴 했어요",
          "original_form": "안사게되긴 했어요",
          "speaker_id": "1",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100036613.1.27",
          "form": "그러니 충동구매였던거죠",
          "original_form": "그러니 충동구매였던거죠",
          "speaker_id": "1",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100036613.1.28",
          "form": "그쵸.. 진짜 ㅋㅋㅋㅋ 저는 먹는거빼곤 뭐 거의 다 충동구매같네여 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸.. 진짜 ㅋㅋㅋㅋ 저는 먹는거빼곤 뭐 거의 다 충동구매같네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:21"
        },
        {
          "id": "MDRW2100036613.1.29",
          "form": "온라인이",
          "original_form": "온라인이",
          "speaker_id": "1",
