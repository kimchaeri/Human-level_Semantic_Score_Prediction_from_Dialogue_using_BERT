        },
        {
          "id": "MDRW2100009412.1.2",
          "form": "북한분들 나오는 지금만나러갑니다 많이보세요",
          "original_form": "북한분들 나오는 지금만나러갑니다 많이보세요",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009412.1.3",
          "form": "저도 그래서 유투브로 탈북민유투브 많이봐요",
          "original_form": "저도 그래서 유투브로 탈북민유투브 많이봐요",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009412.1.4",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009412.1.5",
          "form": "전 자주는 안봐요",
          "original_form": "전 자주는 안봐요",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009412.1.6",
          "form": "이것저것 많이 하시더라구요",
          "original_form": "이것저것 많이 하시더라구요",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009412.1.7",
          "form": "아 유투브는 많이 안보시나봐요",
          "original_form": "아 유투브는 많이 안보시나봐요",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009412.1.8",
          "form": "아뇨 딴걸봐요 ㅋㅋ",
          "original_form": "아뇨 딴걸봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009412.1.9",
          "form": "ㅋㅋㅋ어떤거보시나요",
          "original_form": "ㅋㅋㅋ어떤거보시나요",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009412.1.10",
          "form": "주식요 ㅎㅎ",
          "original_form": "주식요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009412.1.11",
          "form": "음악이나요",
          "original_form": "음악이나요",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009412.1.12",
          "form": "크 실용적이시네요 저는 영화에나오는 북한말투가",
          "original_form": "크 실용적이시네요 저는 영화에나오는 북한말투가",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009412.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009412.1.14",
          "form": "정말 쓰는말인지 북한유투버가",
          "original_form": "정말 쓰는말인지 북한유투버가",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009412.1.15",
          "form": "평가해주는거 봤었는데",
          "original_form": "평가해주는거 봤었는데",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009412.1.16",
          "form": "재밌더라구요ㅋㅋ",
          "original_form": "재밌더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009412.1.17",
          "form": "북한을 많이 접해보지 않았는데 덕분에 공부가 많이 됐어요",
          "original_form": "북한을 많이 접해보지 않았는데 덕분에 공부가 많이 됐어요",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009412.1.18",
          "form": "어머 그런게 있어요",
          "original_form": "어머 그런게 있어요",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009412.1.19",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009412.1.20",
          "form": "네 놀새나라라는 유투버에요",
          "original_form": "네 놀새나라라는 유투버에요",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009412.1.21",
          "form": "방송이 진짜 다양하네요",
          "original_form": "방송이 진짜 다양하네요",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009412.1.22",
          "form": "네 한번 봐볼께요 ㅎㅎ",
          "original_form": "네 한번 봐볼께요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009412.1.23",
          "form": "열살땐가 엄마따라 탈북했대요",
          "original_form": "열살땐가 엄마따라 탈북했대요",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009412.1.24",
          "form": "ㅋㅋ 탈북민들 탈북스토리 유투브올려주시거든요ㅋㅋ",
          "original_form": "ㅋㅋ 탈북민들 탈북스토리 유투브올려주시거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009412.1.25",
          "form": "어린나이에도 정말 탈북 많이 하더라고요",
          "original_form": "어린나이에도 정말 탈북 많이 하더라고요",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009412.1.26",
          "form": "진짜 아이가 대단해요",
          "original_form": "진짜 아이가 대단해요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009412.1.27",
          "form": "넘 고단해서..남한에 사는 고생이 별거아니게 느껴질정도에요",
          "original_form": "넘 고단해서..남한에 사는 고생이 별거아니게 느껴질정도에요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009412.1.28",
          "form": "어머니가 무용수시더라구요",
          "original_form": "어머니가 무용수시더라구요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009412.1.29",
          "form": "무용수가 많으세요",
          "original_form": "무용수가 많으세요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009412.1.30",
          "form": "예전에 공연 봤어요",
          "original_form": "예전에 공연 봤어요",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009412.1.31",
          "form": "유투브가 참 유용해요 ㄴ북한 주민들 생활이나 생각을 많이 알수있게해줘요",
          "original_form": "유투브가 참 유용해요 ㄴ북한 주민들 생활이나 생각을 많이 알수있게해줘요",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009412.1.32",
          "form": "아 무슨공연이요?",
          "original_form": "아 무슨공연이요?",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009412.1.33",
          "form": "임진각 공연요",
          "original_form": "임진각 공연요",
          "speaker_id": "2",
          "time": "20210822 23:24"
        }
      ]
    }
