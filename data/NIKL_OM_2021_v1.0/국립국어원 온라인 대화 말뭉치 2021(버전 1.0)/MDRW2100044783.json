      "utterance": [
        {
          "id": "MDRW2100044783.1.1",
          "form": "저 사라다빵도 좋아하는데",
          "original_form": "저 사라다빵도 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044783.1.2",
          "form": "그건 잘 파는곳이 없더라고요",
          "original_form": "그건 잘 파는곳이 없더라고요",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044783.1.3",
          "form": "파라바게뜨나 뚜쥬가면 팔아영",
          "original_form": "파라바게뜨나 뚜쥬가면 팔아영",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044783.1.4",
          "form": "비싸서 그렇지ㅠ",
          "original_form": "비싸서 그렇지ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044783.1.5",
          "form": "아 그렇긴 한데 그 시장표",
          "original_form": "아 그렇긴 한데 그 시장표",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044783.1.6",
          "form": ".시장표는 저도 못본듯해여",
          "original_form": ".시장표는 저도 못본듯해여",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044783.1.7",
          "form": "그맛이 먹고 싶은데 뚜쥬는 세련됐더라고요",
          "original_form": "그맛이 먹고 싶은데 뚜쥬는 세련됐더라고요",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044783.1.8",
          "form": "새련 ㅋㅋㅋ 아니 그것보다도 양이 작아요",
          "original_form": "새련 ㅋㅋㅋ 아니 그것보다도 양이 작아요",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044783.1.9",
          "form": "그런건 커야맛있는데",
          "original_form": "그런건 커야맛있는데",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044783.1.10",
          "form": "ㅋㅋㅋㅋㅋ 햄이 고급지면 안돼요",
          "original_form": "ㅋㅋㅋㅋㅋ 햄이 고급지면 안돼요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044783.1.11",
          "form": "근데 햄이 그맛이 아니었어요",
          "original_form": "근데 햄이 그맛이 아니었어요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044783.1.12",
          "form": "햄은  싸구려 햄",
          "original_form": "햄은  싸구려 햄",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044783.1.13",
          "form": "그걸 넣어야",
          "original_form": "그걸 넣어야",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044783.1.14",
          "form": "그 시장표맛이!!",
          "original_form": "그 시장표맛이!!",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044783.1.15",
          "form": "크하하하ㅠㅠㅠ",
          "original_form": "크하하하ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044783.1.16",
          "form": "시장가면 맛있는거 많아서 너무 좋아요",
          "original_form": "시장가면 맛있는거 많아서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044783.1.17",
          "form": "순대,.튀김..오뎅...",
          "original_form": "순대,.튀김..오뎅...",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044783.1.18",
          "form": "ㅋㅋㅋㅋㅋ 저 순대 먹은지 좀 오래돼서 먹고 싶네요",
          "original_form": "ㅋㅋㅋㅋㅋ 저 순대 먹은지 좀 오래돼서 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044783.1.19",
          "form": "간이랑 순대만!!",
          "original_form": "간이랑 순대만!!",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044783.1.20",
          "form": "순대 염통 안드셔요??",
          "original_form": "순대 염통 안드셔요??",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044783.1.21",
          "form": "염통 그쫄깃함??",
          "original_form": "염통 그쫄깃함??",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044783.1.22",
          "form": "못드시나요??",
          "original_form": "못드시나요??",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044783.1.23",
          "form": "넹 약간 질긴 그느낌이 싫어요",
          "original_form": "넹 약간 질긴 그느낌이 싫어요",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044783.1.24",
          "form": "저 그래서 곱창도 별로 안좋아해요",
          "original_form": "저 그래서 곱창도 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:49"
        }
      ]
