      "utterance": [
        {
          "id": "MDRW2100032800.1.1",
          "form": "네... 2년이었어요..^^ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네... 2년이었어요..^^ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.2",
          "form": "진짜 저정도면 참 징글징글하네요",
          "original_form": "진짜 저정도면 참 징글징글하네요",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.3",
          "form": "우와 진짜여?",
          "original_form": "우와 진짜여?",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.4",
          "form": "근데 이해가네용",
          "original_form": "근데 이해가네용",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.5",
          "form": "저래놓고 신곡 나오면 뮤비 다 보고 ㅠ 무대도 다 챙겨봐요",
          "original_form": "저래놓고 신곡 나오면 뮤비 다 보고 ㅠ 무대도 다 챙겨봐요",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.6",
          "form": "본진은 쉽게 정하면 안대요",
          "original_form": "본진은 쉽게 정하면 안대요",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.7",
          "form": "좀...알져",
          "original_form": "좀...알져",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.8",
          "form": "약간 밀당하고",
          "original_form": "약간 밀당하고",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.9",
          "form": "약간...아 내가 여기서 더 보면 위험하다!!!!",
          "original_form": "약간...아 내가 여기서 더 보면 위험하다!!!!",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.10",
          "form": "차도녀처럼 굴어주다가",
          "original_form": "차도녀처럼 굴어주다가",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032800.1.11",
          "form": "이러고 의도적으로 피하는..",
          "original_form": "이러고 의도적으로 피하는..",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032800.1.12",
          "form": "아 알아욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 알아욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032800.1.13",
          "form": "그래도 아.. 너를 사랑하는 것만 가타... 아냐...",
          "original_form": "그래도 아.. 너를 사랑하는 것만 가타... 아냐...",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032800.1.14",
          "form": "아 공감돼욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 공감돼욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032800.1.15",
          "form": "하면서 바쁘게 내적 고뇌를 해야",
          "original_form": "하면서 바쁘게 내적 고뇌를 해야",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032800.1.16",
          "form": "본진 자리에 등극할 수 있죠",
          "original_form": "본진 자리에 등극할 수 있죠",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032800.1.17",
          "form": "그러고 쟁취한 본진자리..",
          "original_form": "그러고 쟁취한 본진자리..",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032800.1.18",
          "form": "진짜 수록곡까지 다듣고 아 난 노래만 좋아하는거야 okok~이랬다니까요..",
          "original_form": "진짜 수록곡까지 다듣고 아 난 노래만 좋아하는거야 okok~이랬다니까요..",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032800.1.19",
          "form": "본진이 생기면 일단 외도하면 안되기에 신중해질 수밖에 없어요",
          "original_form": "본진이 생기면 일단 외도하면 안되기에 신중해질 수밖에 없어요",
          "speaker_id": "1",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032800.1.20",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032800.1.21",
          "form": "라잌 혼인신고",
          "original_form": "라잌 혼인신고",
          "speaker_id": "1",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032800.1.22",
          "form": "물론 걔는 모르지만",
          "original_form": "물론 걔는 모르지만",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032800.1.23",
          "form": "저는 진심이라구요",
          "original_form": "저는 진심이라구요",
          "speaker_id": "2",
          "time": "20210905 23:54"
        }
      ]
