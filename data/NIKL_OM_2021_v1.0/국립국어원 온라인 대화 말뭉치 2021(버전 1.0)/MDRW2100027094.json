      "utterance": [
        {
          "id": "MDRW2100027094.1.1",
          "form": "커뮤니티에서 많이 얻을수 있어요",
          "original_form": "커뮤니티에서 많이 얻을수 있어요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027094.1.2",
          "form": "밀가루도 매우 좋아해요",
          "original_form": "밀가루도 매우 좋아해요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027094.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027094.1.4",
          "form": "빵 좋아하고",
          "original_form": "빵 좋아하고",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027094.1.5",
          "form": "면종류도 좋아합니다",
          "original_form": "면종류도 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027094.1.6",
          "form": "튀겨도 맛있습니다",
          "original_form": "튀겨도 맛있습니다",
          "speaker_id": "1",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.7",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.8",
          "form": "전도 좋아해요",
          "original_form": "전도 좋아해요",
          "speaker_id": "1",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.9",
          "form": "먹는 이야기는 끝이 없습니다",
          "original_form": "먹는 이야기는 끝이 없습니다",
          "speaker_id": "1",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.10",
          "form": "김치전",
          "original_form": "김치전",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.11",
          "form": "치즈빈대떡",
          "original_form": "치즈빈대떡",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.12",
          "form": "부추전",
          "original_form": "부추전",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.13",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.14",
          "form": "점심은 무얼드실거에요",
          "original_form": "점심은 무얼드실거에요",
          "speaker_id": "1",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.15",
          "form": "전 남은 볶음밥을 또 먹을겁니다",
          "original_form": "전 남은 볶음밥을 또 먹을겁니다",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.16",
          "form": "저는 카페에서 샐러드 먹을거에요",
          "original_form": "저는 카페에서 샐러드 먹을거에요",
          "speaker_id": "1",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027094.1.17",
          "form": "카레.남은거 데워서올려먹으려구요",
          "original_form": "카레.남은거 데워서올려먹으려구요",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027094.1.18",
          "form": "대량 생산하셨군요",
          "original_form": "대량 생산하셨군요",
          "speaker_id": "1",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027094.1.19",
          "form": "샐러드 맛있어요",
          "original_form": "샐러드 맛있어요",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027094.1.20",
          "form": "좋군요. 대부분 재탕하시나봐요",
          "original_form": "좋군요. 대부분 재탕하시나봐요",
          "speaker_id": "1",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027094.1.21",
          "form": "무슨샐러드죠?",
          "original_form": "무슨샐러드죠?",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027094.1.22",
          "form": "알뜰한 식습관이군요",
          "original_form": "알뜰한 식습관이군요",
