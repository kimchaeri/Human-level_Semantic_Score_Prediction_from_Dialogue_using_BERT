          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039609.1.3",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039609.1.4",
          "form": "아뇨 퇴사요 실업급여타고있네요",
          "original_form": "아뇨 퇴사요 실업급여타고있네요",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039609.1.5",
          "form": "다시 취업준비하시겠네요.",
          "original_form": "다시 취업준비하시겠네요.",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039609.1.6",
          "form": "애키우느라 안할거같아요",
          "original_form": "애키우느라 안할거같아요",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039609.1.7",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039609.1.8",
          "form": "프리랜서요",
          "original_form": "프리랜서요",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039609.1.9",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039609.1.10",
          "form": "코로나 영향있나요?",
          "original_form": "코로나 영향있나요?",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039609.1.11",
          "form": "코로나라 좀 덜해요",
          "original_form": "코로나라 좀 덜해요",
          "speaker_id": "1",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039609.1.12",
          "form": "덜하다는게 없다는뜻?",
          "original_form": "덜하다는게 없다는뜻?",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039609.1.13",
          "form": "일이 좀 줄었어요",
          "original_form": "일이 좀 줄었어요",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039609.1.14",
          "form": "아 다 타격이 있군요",
          "original_form": "아 다 타격이 있군요",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039609.1.15",
          "form": "공무원이나 대기업외에는 다들타격있는것같아요",
          "original_form": "공무원이나 대기업외에는 다들타격있는것같아요",
          "speaker_id": "1",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039609.1.16",
          "form": "월급 꼬박꼬박나오기가 힘들더라고요",
          "original_form": "월급 꼬박꼬박나오기가 힘들더라고요",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039609.1.17",
          "form": "저도 일한만큼받아요",
          "original_form": "저도 일한만큼받아요",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039609.1.18",
          "form": "저는 밀려서요",
          "original_form": "저는 밀려서요",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039609.1.19",
          "form": "언제 받을련지",
          "original_form": "언제 받을련지",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039609.1.20",
          "form": "아.밀려서 퇴사하신거군요",
          "original_form": "아.밀려서 퇴사하신거군요",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039609.1.21",
          "form": "넹 오래 일해서 정으로 다녔죠 ㅜ",
