      "utterance": [
        {
          "id": "MDRW2100007001.1.1",
          "form": "절에 못 간지도 오래됐네",
          "original_form": "절에 못 간지도 오래됐네",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007001.1.2",
          "form": "절에 못 간지도 오래됐네",
          "original_form": "절에 못 간지도 오래됐네",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007001.1.3",
          "form": "절에 간 지도",
          "original_form": "절에 간 지도",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007001.1.4",
          "form": "오래됐다 그지?",
          "original_form": "오래됐다 그지?",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007001.1.5",
          "form": "그러게 코로나 때문에",
          "original_form": "그러게 코로나 때문에",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007001.1.6",
          "form": "원래는 부처님오신 날에 매년 갔었는데",
          "original_form": "원래는 부처님오신 날에 매년 갔었는데",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007001.1.7",
          "form": "그니까 가면 은근히 재미있어",
          "original_form": "그니까 가면 은근히 재미있어",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007001.1.8",
          "form": "맞아 주변에 맛있는 것들도 있고",
          "original_form": "맞아 주변에 맛있는 것들도 있고",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007001.1.9",
          "form": "절에서 행사도 진행하니까",
          "original_form": "절에서 행사도 진행하니까",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007001.1.10",
          "form": "염주 같은 기념품도 사고 그랬는데",
          "original_form": "염주 같은 기념품도 사고 그랬는데",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007001.1.11",
          "form": "나는 공연봤던 거 생각나",
          "original_form": "나는 공연봤던 거 생각나",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007001.1.12",
          "form": "마지막으로 갔을 때 누구 왔었지?",
          "original_form": "마지막으로 갔을 때 누구 왔었지?",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007001.1.13",
          "form": "장사익 왔었던 거 같은데",
          "original_form": "장사익 왔었던 거 같은데",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007001.1.14",
          "form": "맞아 그랬던 거 같아",
          "original_form": "맞아 그랬던 거 같아",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007001.1.15",
          "form": "실제로 들은 건 처음이었는데 좋았어",
          "original_form": "실제로 들은 건 처음이었는데 좋았어",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007001.1.16",
          "form": "되게 여러 곡 부르고 갔는데",
          "original_form": "되게 여러 곡 부르고 갔는데",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007001.1.17",
          "form": "그래서 예정보다 늦게 집에 갔었지",
          "original_form": "그래서 예정보다 늦게 집에 갔었지",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007001.1.18",
          "form": "다른 종교에서도 행사할 때 그렇게 초대가수 부르고 그러겠지?",
          "original_form": "다른 종교에서도 행사할 때 그렇게 초대가수 부르고 그러겠지?",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007001.1.19",
          "form": "크리스마스 행사같은 것도 가보면 재미있을텐데",
          "original_form": "크리스마스 행사같은 것도 가보면 재미있을텐데",
          "speaker_id": "2",
          "time": "20210822 00:34"
        }
      ]
