      "utterance": [
        {
          "id": "MDRW2100023791.1.1",
          "form": "나 그때 파운드케이크산거 성공적",
          "original_form": "나 그때 파운드케이크산거 성공적",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023791.1.2",
          "form": "파운드케이크 15개중 6개남움ㅋㅋㅋㅋㅋㅋ",
          "original_form": "파운드케이크 15개중 6개남움ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100023791.1.3",
          "form": "헐벌써-?ㅋㅋㅋ",
          "original_form": "헐벌써-?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100023791.1.4",
          "form": "맛있어?",
          "original_form": "맛있어?",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100023791.1.5",
          "form": "어어 ㅋㅋㅋ 엄마아빠도 잘드심 ㅋㅋㅋㅋ",
          "original_form": "어어 ㅋㅋㅋ 엄마아빠도 잘드심 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100023791.1.6",
          "form": "엄청싸게샀는데",
          "original_form": "엄청싸게샀는데",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100023791.1.7",
          "form": "어디서산거야?",
          "original_form": "어디서산거야?",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100023791.1.8",
          "form": "작은거라고했징?ㅋㅋ",
          "original_form": "작은거라고했징?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100023791.1.9",
          "form": "어어 십센치파운드!",
          "original_form": "어어 십센치파운드!",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100023791.1.10",
          "form": "얼마씩해?",
          "original_form": "얼마씩해?",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100023791.1.11",
          "form": "나 파운드 다섯가지맛 세개씩해서 7천원줌!",
          "original_form": "나 파운드 다섯가지맛 세개씩해서 7천원줌!",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100023791.1.12",
          "form": "우와! 다른 파운드 케익이랑은 어떻게 달라?",
          "original_form": "우와! 다른 파운드 케익이랑은 어떻게 달라?",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100023791.1.13",
          "form": "그냥 파운드케익이긴함ㅋㅋㅋ",
          "original_form": "그냥 파운드케익이긴함ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100023791.1.14",
          "form": "근데 소포장되어있어서 좋아",
          "original_form": "근데 소포장되어있어서 좋아",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100023791.1.15",
          "form": "소포장되어있는게 메리트네!",
          "original_form": "소포장되어있는게 메리트네!",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100023791.1.16",
          "form": "냉장보관안해도되고",
          "original_form": "냉장보관안해도되고",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100023791.1.17",
          "form": "진짜 ㄹㅇ ㅋㅋ",
          "original_form": "진짜 ㄹㅇ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100023791.1.18",
          "form": "가방에쏙! 임 ㅋㅋ근데 생각보다는커",
          "original_form": "가방에쏙! 임 ㅋㅋ근데 생각보다는커",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100023791.1.19",
          "form": "오ㅋㅋ 너무 퍽퍽하지는 않을까?",
          "original_form": "오ㅋㅋ 너무 퍽퍽하지는 않을까?",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100023791.1.20",
          "form": "엄청 퍽퍽! 이런느낌은 또 아님 ㅋㅋㅋㅋ",
          "original_form": "엄청 퍽퍽! 이런느낌은 또 아님 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100023791.1.21",
          "form": "포슬포슬해",
          "original_form": "포슬포슬해",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100023791.1.22",
          "form": "나도 사봐야지!어떤맛이 제일 맛있었어?",
          "original_form": "나도 사봐야지!어떤맛이 제일 맛있었어?",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100023791.1.23",
          "form": "신세계에서 뭐삼?",
          "original_form": "신세계에서 뭐삼?",
          "speaker_id": "1",
          "time": "20210902 22:52"
        }
      ]
