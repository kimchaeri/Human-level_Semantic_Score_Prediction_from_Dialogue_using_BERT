          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034799.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034799.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
