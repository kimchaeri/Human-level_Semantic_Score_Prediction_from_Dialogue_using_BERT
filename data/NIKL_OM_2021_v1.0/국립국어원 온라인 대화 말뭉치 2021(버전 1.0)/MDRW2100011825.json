      "utterance": [
        {
          "id": "MDRW2100011825.1.1",
          "form": "다들 연애에 세뇌된거같지않아요?",
          "original_form": "다들 연애에 세뇌된거같지않아요?",
          "speaker_id": "1",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011825.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011825.1.3",
          "form": "좀 그런데?",
          "original_form": "좀 그런데?",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011825.1.4",
          "form": "이래도 사랑하니까",
          "original_form": "이래도 사랑하니까",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011825.1.5",
          "form": "이런말이면 다 해결되는",
          "original_form": "이런말이면 다 해결되는",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011825.1.6",
          "form": "사랑이 뭐길래~~~~~",
          "original_form": "사랑이 뭐길래~~~~~",
          "speaker_id": "1",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011825.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011825.1.8",
          "form": "그리고 연애 안하면 하자 있는 것처럼",
          "original_form": "그리고 연애 안하면 하자 있는 것처럼",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.9",
          "form": "아 맞아요 ㄹㅇ 공감",
          "original_form": "아 맞아요 ㄹㅇ 공감",
          "speaker_id": "1",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.10",
          "form": "보는 시선들도 한 몫 했다고 생각해요",
          "original_form": "보는 시선들도 한 몫 했다고 생각해요",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.11",
          "form": "이게 유머로 통하고",
          "original_form": "이게 유머로 통하고",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.12",
          "form": "진짜 전 어렸을 때 연애 못한게 부끄럽다고 생각했거든요",
          "original_form": "진짜 전 어렸을 때 연애 못한게 부끄럽다고 생각했거든요",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.13",
          "form": "n살때까지 결혼못하면 영원히못한다고 독촉하고",
          "original_form": "n살때까지 결혼못하면 영원히못한다고 독촉하고",
          "speaker_id": "1",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.15",
          "form": "아 그러셨구나 근데 ㄹㅇ 분위기가 그랫어요",
          "original_form": "아 그러셨구나 근데 ㄹㅇ 분위기가 그랫어요",
          "speaker_id": "1",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.16",
          "form": "20살 넘기전에 해봐야한다",
          "original_form": "20살 넘기전에 해봐야한다",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011825.1.17",
          "form": "고딩때 교복입고 데이트",
          "original_form": "고딩때 교복입고 데이트",
