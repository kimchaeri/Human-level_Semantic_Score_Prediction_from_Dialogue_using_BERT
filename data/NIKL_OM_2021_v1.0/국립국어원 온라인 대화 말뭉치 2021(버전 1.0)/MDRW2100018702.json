      "utterance": [
        {
          "id": "MDRW2100018702.1.1",
          "form": "똑똑",
          "original_form": "똑똑",
          "speaker_id": "1",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018702.1.2",
          "form": "찾았쥬ㅋㅋ",
          "original_form": "찾았쥬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018702.1.3",
          "form": "안녕하세요 name1.",
          "original_form": "안녕하세요 &name1&.",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018702.1.4",
          "form": "혹시 결혼하셨나요?",
          "original_form": "혹시 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018702.1.5",
          "form": "아직이요ㅋ",
          "original_form": "아직이요ㅋ",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018702.1.6",
          "form": "name2은 결혼 하셨나요?",
          "original_form": "&name2&은 결혼 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018702.1.7",
          "form": "아니요. 저 비혼주의에요.",
          "original_form": "아니요. 저 비혼주의에요.",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018702.1.8",
          "form": "그냥 연애만 하고 싶엉ㆍ",
          "original_form": "그냥 연애만 하고 싶엉ㆍ",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018702.1.9",
          "form": "연애만 하고 싶어요",
          "original_form": "연애만 하고 싶어요",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018702.1.10",
          "form": "name1은 결혼하실 예정이세요?",
          "original_form": "&name1&은 결혼하실 예정이세요?",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018702.1.11",
          "form": "와우ㅋㅋ하고 싶은데 아직은 쫌 두려워용ㅜ",
          "original_form": "와우ㅋㅋ하고 싶은데 아직은 쫌 두려워용ㅜ",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018702.1.12",
          "form": "하고 싶을 때 하심 되죠ㅎㅎ",
          "original_form": "하고 싶을 때 하심 되죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018702.1.13",
          "form": "이상형이 있으신가요?",
          "original_form": "이상형이 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018702.1.14",
          "form": "ㅋㅋ지금 남친이요?ㅋㅋ",
          "original_form": "ㅋㅋ지금 남친이요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018702.1.15",
          "form": "애처럼 까불까불하지만 그래도 근접해요ㅋㅋ",
          "original_form": "애처럼 까불까불하지만 그래도 근접해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018702.1.16",
          "form": "앗 연애 중이시구나 ㅎㅎ",
          "original_form": "앗 연애 중이시구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018702.1.17",
          "form": "예쁜 사랑 기원합니당",
          "original_form": "예쁜 사랑 기원합니당",
          "speaker_id": "2",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018702.1.18",
          "form": "데이트는 자주 하세요?",
          "original_form": "데이트는 자주 하세요?",
          "speaker_id": "2",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018702.1.19",
          "form": "감사합니당ㅋㅋ",
          "original_form": "감사합니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018702.1.20",
          "form": "일요일 빼고 매일매일이요ㅋㅋ",
          "original_form": "일요일 빼고 매일매일이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018702.1.21",
          "form": "일요일에는 농활을 떠나야해서 쉬는 날이요ㅋ",
          "original_form": "일요일에는 농활을 떠나야해서 쉬는 날이요ㅋ",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018702.1.22",
          "form": "와ㅋㅋㅋ 거의 회사출근인데요",
          "original_form": "와ㅋㅋㅋ 거의 회사출근인데요",
          "speaker_id": "2",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018702.1.23",
          "form": "혹시 사내커플은 아니시죠?",
          "original_form": "혹시 사내커플은 아니시죠?",
          "speaker_id": "2",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018702.1.24",
          "form": "ㅋㅋㅋ아니에용ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아니에용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018702.1.25",
          "form": "사내연애는 지양하는 편이라서요ㅋㅋ",
          "original_form": "사내연애는 지양하는 편이라서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018702.1.26",
          "form": "아 사내연애 힘들죠.. 최고와 최악  둘 중 하나라ㅋㅋ",
          "original_form": "아 사내연애 힘들죠.. 최고와 최악  둘 중 하나라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018702.1.27",
          "form": "저는 연애중이긴 한데 2주에 한번 만나요",
          "original_form": "저는 연애중이긴 한데 2주에 한번 만나요",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018702.1.28",
          "form": "1주는 체력회복..",
          "original_form": "1주는 체력회복..",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018702.1.29",
          "form": "코로나 터지고나선 반년도 안 봤어요ㅜㅜ",
          "original_form": "코로나 터지고나선 반년도 안 봤어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018702.1.30",
          "form": "보통 데이트는 어디서 하시는 편이세요?",
          "original_form": "보통 데이트는 어디서 하시는 편이세요?",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018702.1.31",
          "form": "매일매일이면 데이트비용도 만만치 않겠어요ㅜ",
          "original_form": "매일매일이면 데이트비용도 만만치 않겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018702.1.32",
          "form": "토닥토닥~",
          "original_form": "토닥토닥~",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018702.1.33",
          "form": "대부분 남친집에서 저녁 같이 만들어 먹는데 요새는 외식이 잦아지고 있어요ㅜ",
          "original_form": "대부분 남친집에서 저녁 같이 만들어 먹는데 요새는 외식이 잦아지고 있어요ㅜ",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018702.1.34",
          "form": "둘다 술을 좋아해서요ㅜ",
          "original_form": "둘다 술을 좋아해서요ㅜ",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018702.1.35",
          "form": "공통점이 많으셔서 매일 만나셔도 좋으신 듯ㅎㅎ",
          "original_form": "공통점이 많으셔서 매일 만나셔도 좋으신 듯ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:07"
        }
