      "utterance": [
        {
          "id": "MDRW2100009672.1.1",
          "form": "지금은 미역국이 떠오르네요 ㅎㅎ",
          "original_form": "지금은 미역국이 떠오르네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009672.1.2",
          "form": "오 미역국 맛있죠 ㅠㅠ 자취하고 많이 못먹는 것 같아요ㅠㅠㅠ",
          "original_form": "오 미역국 맛있죠 ㅠㅠ 자취하고 많이 못먹는 것 같아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:59"
        },
        {
          "id": "MDRW2100009672.1.3",
          "form": "바게트님은 어떤 거 좋아하세요?",
          "original_form": "바게트님은 어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 03:02"
        },
        {
          "id": "MDRW2100009672.1.4",
          "form": "저는 다 좋아하는데 요즘에는 뼈해장국 좋아해요",
          "original_form": "저는 다 좋아하는데 요즘에는 뼈해장국 좋아해요",
          "speaker_id": "1",
          "time": "20210823 03:04"
        },
        {
          "id": "MDRW2100009672.1.5",
          "form": "맛있어요!",
          "original_form": "맛있어요!",
          "speaker_id": "1",
          "time": "20210823 03:04"
        },
        {
          "id": "MDRW2100009672.1.6",
          "form": "근처에 맛집이 있나봐요",
          "original_form": "근처에 맛집이 있나봐요",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009672.1.7",
          "form": "헐 어떻게 아셨어요?",
          "original_form": "헐 어떻게 아셨어요?",
          "speaker_id": "1",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009672.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009672.1.9",
          "form": "ㅋㅋ 근처에 맛집 있으면 자주 가게 되죠\r\n뼈해장국 먹어본지도 꽤 됐네요",
          "original_form": "ㅋㅋ 근처에 맛집 있으면 자주 가게 되죠\r\n뼈해장국 먹어본지도 꽤 됐네요",
          "speaker_id": "2",
          "time": "20210823 03:11"
        },
        {
          "id": "MDRW2100009672.1.10",
          "form": "잡내 안내고 한느게 중요한 것 ㅏㅌ아요",
          "original_form": "잡내 안내고 한느게 중요한 것 ㅏㅌ아요",
          "speaker_id": "1",
          "time": "20210823 03:14"
        },
        {
          "id": "MDRW2100009672.1.11",
          "form": "고기요리는 잡내 잡는 게 진짜 중요하죠",
          "original_form": "고기요리는 잡내 잡는 게 진짜 중요하죠",
          "speaker_id": "2",
          "time": "20210823 03:18"
        },
        {
          "id": "MDRW2100009672.1.12",
          "form": "맞아요 근데 그런 가게가 많지 않아요ㅠㅠ",
          "original_form": "맞아요 근데 그런 가게가 많지 않아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 01:06"
        },
        {
          "id": "MDRW2100009672.1.13",
          "form": "name2 님은 무슨 음식 좋아해요?",
          "original_form": "&name2& 님은 무슨 음식 좋아해요?",
          "speaker_id": "1",
          "time": "20210824 01:07"
        },
        {
          "id": "MDRW2100009672.1.14",
          "form": "순두부찌개도 좋아해요",
          "original_form": "순두부찌개도 좋아해요",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100009672.1.15",
          "form": "오 순찌 저 가끔 만들어 먹어요!",
          "original_form": "오 순찌 저 가끔 만들어 먹어요!",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100009672.1.16",
          "form": "흰밥에 먹으면 진짜 맛있죠 ㅎㅎㅎ",
          "original_form": "흰밥에 먹으면 진짜 맛있죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100009672.1.17",
          "form": "얼마 전에도 먹었는데 또 생각나네요 ㅎㅎ",
          "original_form": "얼마 전에도 먹었는데 또 생각나네요 ㅎㅎ",
