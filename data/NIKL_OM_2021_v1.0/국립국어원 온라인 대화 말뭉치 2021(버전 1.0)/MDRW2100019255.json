      "utterance": [
        {
          "id": "MDRW2100019255.1.1",
          "form": "또..?",
          "original_form": "또..?",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019255.1.2",
          "form": "아 알바가는데",
          "original_form": "아 알바가는데",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019255.1.3",
          "form": "우산 챙겨 가",
          "original_form": "우산 챙겨 가",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019255.1.4",
          "form": "넹..",
          "original_form": "넹..",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019255.1.5",
          "form": "알바 끝날 때까지 안 왔으면 좋겠는데... 날이 흐리네",
          "original_form": "알바 끝날 때까지 안 왔으면 좋겠는데... 날이 흐리네",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019255.1.6",
          "form": "어짜피 차 타고 오겠지만... 그래도 챙긴다",
          "original_form": "어짜피 차 타고 오겠지만... 그래도 챙긴다",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019255.1.7",
          "form": "그려그려",
          "original_form": "그려그려",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019255.1.8",
          "form": "옷도 따숩게 입고 가",
          "original_form": "옷도 따숩게 입고 가",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019255.1.9",
          "form": "..반팔에 긴바지.",
          "original_form": "..반팔에 긴바지.",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019255.1.10",
          "form": "반팔 이따 올 때 춥지 않을까?",
          "original_form": "반팔 이따 올 때 춥지 않을까?",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019255.1.11",
          "form": "괜춘",
          "original_form": "괜춘",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019255.1.12",
          "form": "차타고 와서",
          "original_form": "차타고 와서",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019255.1.13",
          "form": "바로 오는 버스 있어?",
          "original_form": "바로 오는 버스 있어?",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019255.1.14",
          "form": "고모 차 타고 오지",
          "original_form": "고모 차 타고 오지",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019255.1.15",
          "form": "아 원래 같이 퇴근하는 교?",
          "original_form": "아 원래 같이 퇴근하는 교?",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019255.1.16",
          "form": "대박 좋다",
          "original_form": "대박 좋다",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019255.1.17",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019255.1.18",
          "form": "그래서 마감도 하는겨",
          "original_form": "그래서 마감도 하는겨",
          "speaker_id": "2",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019255.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019255.1.20",
