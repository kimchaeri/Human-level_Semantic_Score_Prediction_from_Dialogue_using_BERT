      "utterance": [
        {
          "id": "MDRW2100040869.1.1",
          "form": "저두 야채 김치 안좋아했어가꼬",
          "original_form": "저두 야채 김치 안좋아했어가꼬",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100040869.1.2",
          "form": "첫째한테 강요도 못하쥬 ㅋ",
          "original_form": "첫째한테 강요도 못하쥬 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100040869.1.3",
          "form": "어렸을땐 다 그런가봐요",
          "original_form": "어렸을땐 다 그런가봐요",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100040869.1.4",
          "form": "그냥 김치도 3조각주고 하나만 먹으라고해요 ㅎ",
          "original_form": "그냥 김치도 3조각주고 하나만 먹으라고해요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100040869.1.5",
          "form": "저도 특히 파랑 콩 싫어했거든요",
          "original_form": "저도 특히 파랑 콩 싫어했거든요",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100040869.1.6",
          "form": "지금도 콩은 집밥엔 없어요~",
          "original_form": "지금도 콩은 집밥엔 없어요~",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040869.1.7",
          "form": "그래도 하나는 먹더라구요",
          "original_form": "그래도 하나는 먹더라구요",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040869.1.8",
          "form": "저도 콩 싫어해요 ㅎ",
          "original_form": "저도 콩 싫어해요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040869.1.9",
          "form": "콩밥 한적이 없어요 ㅎㅎ",
          "original_form": "콩밥 한적이 없어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040869.1.10",
          "form": "콩반찬은 할줄도 모르고 ㅋ",
          "original_form": "콩반찬은 할줄도 모르고 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040869.1.11",
          "form": "ㅎㅎ 저도요",
          "original_form": "ㅎㅎ 저도요",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040869.1.12",
          "form": "근데 시댁가니 콩반찬 해주시던데",
          "original_form": "근데 시댁가니 콩반찬 해주시던데",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040869.1.13",
          "form": "둘째가 나 맛있다 하면서",
          "original_form": "둘째가 나 맛있다 하면서",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040869.1.14",
          "form": "으찌나 먹는지",
          "original_form": "으찌나 먹는지",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040869.1.15",
          "form": "가끔 콩장 먹고싶은데 사먹는건 예전 그맛이 아니더라구요",
          "original_form": "가끔 콩장 먹고싶은데 사먹는건 예전 그맛이 아니더라구요",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040869.1.16",
          "form": "오~~~ 어렸을땐 콩먹음 다 먹는건데",
          "original_form": "오~~~ 어렸을땐 콩먹음 다 먹는건데",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100040869.1.17",
          "form": "어머니가 보시면 굶긴줄알것같더라구요",
          "original_form": "어머니가 보시면 굶긴줄알것같더라구요",
          "speaker_id": "1",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100040869.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100040869.1.19",
          "form": "애들 식습관은 엄마 따라가는거 같아요",
          "original_form": "애들 식습관은 엄마 따라가는거 같아요",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100040869.1.20",
          "form": "맞아요 ㅎ",
          "original_form": "맞아요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100040869.1.21",
          "form": "제가 싫어하는거를 안만드니~~",
          "original_form": "제가 싫어하는거를 안만드니~~",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100040869.1.22",
