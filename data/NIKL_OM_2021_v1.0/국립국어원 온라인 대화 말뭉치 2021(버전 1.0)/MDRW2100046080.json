      "utterance": [
        {
          "id": "MDRW2100046080.1.1",
          "form": "식사하셧나용",
          "original_form": "식사하셧나용",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046080.1.2",
          "form": "아뇨 아침 안먹었어요",
          "original_form": "아뇨 아침 안먹었어요",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046080.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046080.1.4",
          "form": "저도 아직..!",
          "original_form": "저도 아직..!",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046080.1.5",
          "form": "점심 어떤거 드실건아용",
          "original_form": "점심 어떤거 드실건아용",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046080.1.6",
          "form": "저 다른 분이랑 이걸로 스무고개했는데 대화도 빨리빨리 진행되고 좋더라구요",
          "original_form": "저 다른 분이랑 이걸로 스무고개했는데 대화도 빨리빨리 진행되고 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046080.1.7",
          "form": "한번 해보실래용,,,? ㅋㅋㅋ",
          "original_form": "한번 해보실래용,,,? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046080.1.8",
          "form": "좋아하는 음식 물어보는건가용??",
          "original_form": "좋아하는 음식 물어보는건가용??",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046080.1.9",
          "form": "아뇨 스무고개로 식사메뉴 맞추는거였어요 ㅋㅋ",
          "original_form": "아뇨 스무고개로 식사메뉴 맞추는거였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046080.1.10",
          "form": "오 점심메뉴 맞춰 볼게요",
          "original_form": "오 점심메뉴 맞춰 볼게요",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046080.1.11",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046080.1.12",
          "form": "앗 좋아여!",
          "original_form": "앗 좋아여!",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046080.1.13",
          "form": "으음 ㅠ 이건 한식...이라고해여랄까요..",
          "original_form": "으음 ㅠ 이건 한식...이라고해여랄까요..",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046080.1.14",
          "form": "이건 딱히 무ㅓ 그런거없이",
          "original_form": "이건 딱히 무ㅓ 그런거없이",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046080.1.15",
          "form": "어느국가든 다먹어요!",
          "original_form": "어느국가든 다먹어요!",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046080.1.16",
          "form": "오오 우리나라사람도 좋아하나요",
          "original_form": "오오 우리나라사람도 좋아하나요",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046080.1.17",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046080.1.18",
          "form": "빨간 음식인가요",
          "original_form": "빨간 음식인가요",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046080.1.19",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046080.1.20",
          "form": "오조미료가 많이 들어가나요.?",
          "original_form": "오조미료가 많이 들어가나요.?",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046080.1.21",
          "form": "ㅇㅏ뇨 안들어가요!",
          "original_form": "ㅇㅏ뇨 안들어가요!",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046080.1.22",
          "form": "건강식이에요 ㅎ",
          "original_form": "건강식이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 10:14"
        }
      ]
