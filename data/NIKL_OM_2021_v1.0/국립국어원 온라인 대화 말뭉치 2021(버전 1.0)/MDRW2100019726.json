      "utterance": [
        {
          "id": "MDRW2100019726.1.1",
          "form": "어우 여행가고싶네요",
          "original_form": "어우 여행가고싶네요",
          "speaker_id": "2",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019726.1.2",
          "form": "그러니까요. 여행간지 한참 됐어요",
          "original_form": "그러니까요. 여행간지 한참 됐어요",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019726.1.3",
          "form": "가장 최근에는 어디 다녀오셨어요,",
          "original_form": "가장 최근에는 어디 다녀오셨어요,",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019726.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019726.1.5",
          "form": "제가 사는 도시 말고는 어딜 못가겠더라구요ㅋㅋㅋㅋ",
          "original_form": "제가 사는 도시 말고는 어딜 못가겠더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019726.1.6",
          "form": "마지막 해외여행이 2019년 12월이었는데 그 때 기억으로 살아요ㅜㅜ",
          "original_form": "마지막 해외여행이 2019년 12월이었는데 그 때 기억으로 살아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019726.1.7",
          "form": "그땐 어디 가셨어요?",
          "original_form": "그땐 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019726.1.8",
          "form": "타이페이 다녀왔어요",
          "original_form": "타이페이 다녀왔어요",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019726.1.9",
          "form": "전 거긴 아직 못 가봤어요. 가보고 싶었는데",
          "original_form": "전 거긴 아직 못 가봤어요. 가보고 싶었는데",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019726.1.10",
          "form": "음식이 너무 취향에 맞더라구요ㅋㅋㅋ 좀 분위기도 중국이랑 일본 합친 것 같고",
          "original_form": "음식이 너무 취향에 맞더라구요ㅋㅋㅋ 좀 분위기도 중국이랑 일본 합친 것 같고",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019726.1.11",
          "form": "전 싱가포르 갔었는데 거기 음식도 괜찮더라구요",
          "original_form": "전 싱가포르 갔었는데 거기 음식도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019726.1.12",
          "form": "아 싱가포르 진짜 가보고싶어요!",
          "original_form": "아 싱가포르 진짜 가보고싶어요!",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019726.1.13",
          "form": "그 무슨 나무같은거 엄청 커다란 공원 뭐였지",
          "original_form": "그 무슨 나무같은거 엄청 커다란 공원 뭐였지",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019726.1.14",
          "form": "거기 진짜 좋더라구요 ㅋㅋ",
          "original_form": "거기 진짜 좋더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019726.1.15",
          "form": "가든스 바이 더 베이",
          "original_form": "가든스 바이 더 베이",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019726.1.16",
          "form": "아 맞아요 거기 가야하는데 ㅠㅠ",
          "original_form": "아 맞아요 거기 가야하는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019726.1.17",
          "form": "며칠이나 다녀오셨어요",
          "original_form": "며칠이나 다녀오셨어요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019726.1.18",
          "form": "3박 4일 갔었어요",
          "original_form": "3박 4일 갔었어요",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019726.1.19",
          "form": "아 역시 적당히 즐기고 적당히 집이 그리워지는 기간이죠ㅋㅋㅋ",
          "original_form": "아 역시 적당히 즐기고 적당히 집이 그리워지는 기간이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019726.1.20",
          "form": "ㅋㅋ 맞아요. 한참 돌아다니다보면 막바지엔 좀 지치더라구요",
          "original_form": "ㅋㅋ 맞아요. 한참 돌아다니다보면 막바지엔 좀 지치더라구요",
          "speaker_id": "1",
          "time": "20210827 21:57"
        }
      ]
