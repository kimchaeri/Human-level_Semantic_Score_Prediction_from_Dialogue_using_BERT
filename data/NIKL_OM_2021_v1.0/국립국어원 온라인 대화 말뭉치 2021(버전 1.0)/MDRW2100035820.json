      "utterance": [
        {
          "id": "MDRW2100035820.1.1",
          "form": "나 고민이 생겼어",
          "original_form": "나 고민이 생겼어",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.2",
          "form": "뭔데 그래",
          "original_form": "뭔데 그래",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.3",
          "form": "얼굴에 여드름이 잔뜩 났어",
          "original_form": "얼굴에 여드름이 잔뜩 났어",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.4",
          "form": "아이고 어쩌다가",
          "original_form": "아이고 어쩌다가",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.5",
          "form": "그저께 너무 피곤해서 씻지도 않고 잤더니",
          "original_form": "그저께 너무 피곤해서 씻지도 않고 잤더니",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.6",
          "form": "그럼 나도 바로 여드름 나더라",
          "original_form": "그럼 나도 바로 여드름 나더라",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.7",
          "form": "여드름 나니까 얼굴에 자신감이 없어지네",
          "original_form": "여드름 나니까 얼굴에 자신감이 없어지네",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.8",
          "form": "피부과를 가보는 건 어때",
          "original_form": "피부과를 가보는 건 어때",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035820.1.9",
          "form": "피부과에서 여드름도 짜주나",
          "original_form": "피부과에서 여드름도 짜주나",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035820.1.10",
          "form": "당연하지 그리고 흉도 안 지게 해줘",
          "original_form": "당연하지 그리고 흉도 안 지게 해줘",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035820.1.11",
          "form": "가격이 비싸잖아",
          "original_form": "가격이 비싸잖아",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035820.1.12",
          "form": "얼굴은 피부과 가는 게 좋더라",
          "original_form": "얼굴은 피부과 가는 게 좋더라",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035820.1.13",
          "form": "여드름에 좋다는 팩도 해야겠어",
          "original_form": "여드름에 좋다는 팩도 해야겠어",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035820.1.14",
          "form": "그리고 여드름 자리를 많이 안 만드는 게 좋아",
          "original_form": "그리고 여드름 자리를 많이 안 만드는 게 좋아",
          "speaker_id": "2",
          "time": "20210830 03:31"
        }
