      "utterance": [
        {
          "id": "MDRW2100025031.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그쵸.... 근데 별난 학생들은 꼭",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그쵸.... 근데 별난 학생들은 꼭",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025031.1.2",
          "form": "신청할거같아요...",
          "original_form": "신청할거같아요...",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025031.1.3",
          "form": "근데 의외로",
          "original_form": "근데 의외로",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025031.1.4",
          "form": "그게 영업이 돼서",
          "original_form": "그게 영업이 돼서",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025031.1.5",
          "form": "우리 연구실에 관심을 가질지도 몰라요!!!",
          "original_form": "우리 연구실에 관심을 가질지도 몰라요!!!",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025031.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025031.1.7",
          "form": "하하하 그러려면 실험은 저희 랩에서...",
          "original_form": "하하하 그러려면 실험은 저희 랩에서...",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025031.1.8",
          "form": "앗 너무 나쁜가요 저 !!!! ㅋㅋㅋㅋㅋ",
          "original_form": "앗 너무 나쁜가요 저 !!!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025031.1.9",
          "form": "그러게요ㅠ",
          "original_form": "그러게요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025031.1.10",
          "form": "그리고 언니가 막 실험하시다가 한번씩",
          "original_form": "그리고 언니가 막 실험하시다가 한번씩",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025031.1.11",
          "form": "웃어줏시고 그러면 되게 우와 분위기 좋다 이렇게 생각할거같아요",
          "original_form": "웃어줏시고 그러면 되게 우와 분위기 좋다 이렇게 생각할거같아요",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025031.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 저도... 하하하(행복) 이러고있으면...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저도... 하하하(행복) 이러고있으면...",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025031.1.13",
          "form": "너무 연극 시나리오같은가요",
          "original_form": "너무 연극 시나리오같은가요",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025031.1.14",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ 아니에요 ㅋㅋㅋㅋㅋㅋ 그정도 웃음은",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ 아니에요 ㅋㅋㅋㅋㅋㅋ 그정도 웃음은",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025031.1.15",
          "form": "평소에도 잘 웃으니까",
          "original_form": "평소에도 잘 웃으니까",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025031.1.16",
          "form": "괜찮을거같아요 충분히!!!!!!",
          "original_form": "괜찮을거같아요 충분히!!!!!!",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025031.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025031.1.18",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025031.1.19",
          "form": "하 그래도 그냥 신청 안하고 인턴으로 오면 좋겠다...",
          "original_form": "하 그래도 그냥 신청 안하고 인턴으로 오면 좋겠다...",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100025031.1.20",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100025031.1.21",
          "form": "그리고 제 친구의 지인이 이번에 저희 연구실",
          "original_form": "그리고 제 친구의 지인이 이번에 저희 연구실",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100025031.1.22",
          "form": "학기중 인턴으로 들어오고 싶다고 교수님한테 메일 보냈는데",
          "original_form": "학기중 인턴으로 들어오고 싶다고 교수님한테 메일 보냈는데",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100025031.1.23",
          "form": "예에에?",
          "original_form": "예에에?",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100025031.1.24",
          "form": "내일이잖아요!",
          "original_form": "내일이잖아요!",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100025031.1.25",
          "form": "오시는거에요??",
          "original_form": "오시는거에요??",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100025031.1.26",
          "form": "아아 제가 말을 끊었네요ㅠ 그랬는데",
          "original_form": "아아 제가 말을 끊었네요ㅠ 그랬는데",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025031.1.27",
          "form": "교수님이 학기중 인턴은 안된다고 하셨대요",
          "original_form": "교수님이 학기중 인턴은 안된다고 하셨대요",
          "speaker_id": "2",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025031.1.28",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025031.1.29",
          "form": "네ㅔㅇ??",
          "original_form": "네ㅔㅇ??",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025031.1.30",
          "form": "왜지ㅠㅜ 그 지인분 몇학년이신가요...",
          "original_form": "왜지ㅠㅜ 그 지인분 몇학년이신가요...",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025031.1.31",
          "form": "몇학년인지는 모르겠는데 지금 24살이구",
          "original_form": "몇학년인지는 모르겠는데 지금 24살이구",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.32",
          "form": "연구비때문인가ㅠㅠ",
          "original_form": "연구비때문인가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.33",
          "form": "피트를 4수 했어요",
          "original_form": "피트를 4수 했어요",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.34",
          "form": "근데 이번에 들어오는 것도",
          "original_form": "근데 이번에 들어오는 것도",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.35",
          "form": "스펙 때문에 들어오려고 하는 거여서...",
          "original_form": "스펙 때문에 들어오려고 하는 거여서...",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.36",
          "form": "대학원은 플랜B고 ㅠ",
          "original_form": "대학원은 플랜B고 ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.37",
          "form": "아하...? 그럼 저학년일수도 있네요..??",
          "original_form": "아하...? 그럼 저학년일수도 있네요..??",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.38",
          "form": "아 진학목적은 아니군요!",
          "original_form": "아 진학목적은 아니군요!",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025031.1.39",
          "form": "저처럼 생각이 바뀔 수도 있으니...희망을...",
          "original_form": "저처럼 생각이 바뀔 수도 있으니...희망을...",
          "speaker_id": "1",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100025031.1.40",
          "form": "네네 ㅋㅋㅋㅋ 사실 저도 그렇게 생각해요...",
          "original_form": "네네 ㅋㅋㅋㅋ 사실 저도 그렇게 생각해요...",
          "speaker_id": "2",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100025031.1.41",
          "form": "아니 핏 점수가",
          "original_form": "아니 핏 점수가",
          "speaker_id": "2",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100025031.1.42",
          "form": "247점 막 이런데도 떨어졌대요ㅠ",
          "original_form": "247점 막 이런데도 떨어졌대요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025031.1.43",
          "form": "높은데만 쓰신건가ㅠㅠ",
          "original_form": "높은데만 쓰신건가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025031.1.44",
          "form": "제가 너무 오래전이라 기억이 잘 안나는데 많이 높은거죠??",
          "original_form": "제가 너무 오래전이라 기억이 잘 안나는데 많이 높은거죠??",
          "speaker_id": "1",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025031.1.45",
          "form": "원서는 안썼었어서 ㅋㅋㅋㅋ 햇",
          "original_form": "원서는 안썼었어서 ㅋㅋㅋㅋ 햇",
          "speaker_id": "1",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025031.1.46",
          "form": "네네 저 정도면 그래도",
          "original_form": "네네 저 정도면 그래도",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025031.1.47",
          "form": "정성대는 뚫을 수 있어요!!!",
          "original_form": "정성대는 뚫을 수 있어요!!!",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025031.1.48",
          "form": "근데 억울하게 떨어져서... 의기소침해 있대요",
          "original_form": "근데 억울하게 떨어져서... 의기소침해 있대요",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025031.1.49",
          "form": "안타깝네요... 그런 얘기들으면 참 몰입이... 너무 잘돼요",
          "original_form": "안타깝네요... 그런 얘기들으면 참 몰입이... 너무 잘돼요",
          "speaker_id": "1",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025031.1.50",
          "form": "어휴 그러면 근데 기본은 탄탄하게ㅛ네요 화학...",
          "original_form": "어휴 그러면 근데 기본은 탄탄하게ㅛ네요 화학...",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025031.1.51",
          "form": "방학때 올까요..?",
          "original_form": "방학때 올까요..?",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025031.1.52",
          "form": "맞아요 공부 정말 잘한다고 하더라구요!",
          "original_form": "맞아요 공부 정말 잘한다고 하더라구요!",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025031.1.53",
          "form": "아마... 이번에 불합해야 들어오지 않을까요?!",
          "original_form": "아마... 이번에 불합해야 들어오지 않을까요?!",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025031.1.54",
          "form": "아 이번에도 약대지원하신거에요??",
          "original_form": "아 이번에도 약대지원하신거에요??",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025031.1.55",
          "form": "사실 친구가",
          "original_form": "사실 친구가",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025031.1.56",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025031.1.57",
          "form": "친구가 저 있는 연구실이라고 보내준거라서",
          "original_form": "친구가 저 있는 연구실이라고 보내준거라서",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025031.1.58",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025031.1.59",
          "form": "핰ㅋㅋㅋㅋㅋ 제가 교수님 몰래 무슨 짓을 하는 기분이에욬ㅋㅋㅋㅋㅋ",
          "original_form": "핰ㅋㅋㅋㅋㅋ 제가 교수님 몰래 무슨 짓을 하는 기분이에욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
