      "utterance": [
        {
          "id": "MDRW2100036990.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036990.1.2",
          "form": "거기도 비오고 있나요?",
          "original_form": "거기도 비오고 있나요?",
          "speaker_id": "1",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036990.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100036990.1.4",
          "form": "아뇨 여긴비가 아직 안와요",
          "original_form": "아뇨 여긴비가 아직 안와요",
          "speaker_id": "2",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100036990.1.5",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "2",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100036990.1.6",
          "form": "여긴 하루종일 오고 있답니다",
          "original_form": "여긴 하루종일 오고 있답니다",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100036990.1.7",
          "form": "비 좋아하시나요?",
          "original_form": "비 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100036990.1.8",
          "form": "집에 있는날에는 좋은데",
          "original_form": "집에 있는날에는 좋은데",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036990.1.9",
          "form": "그런날이 별로 없어서 ㅎㅎ",
          "original_form": "그런날이 별로 없어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036990.1.10",
          "form": "비 좋아하세요?",
          "original_form": "비 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036990.1.11",
          "form": "저는 점점 비와 눈이 별로네요..",
          "original_form": "저는 점점 비와 눈이 별로네요..",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036990.1.12",
          "form": "해가 환하게 뜬 날이 좋아요",
          "original_form": "해가 환하게 뜬 날이 좋아요",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036990.1.13",
          "form": "요즘은 너무 많이 와서",
          "original_form": "요즘은 너무 많이 와서",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036990.1.14",
          "form": "좀 지겨운감도 있죠",
          "original_form": "좀 지겨운감도 있죠",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036990.1.15",
          "form": "습하기도해져서...",
          "original_form": "습하기도해져서...",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036990.1.16",
          "form": "그러게요.. 언제부터 가을 장마가 생겼는지",
          "original_form": "그러게요.. 언제부터 가을 장마가 생겼는지",
          "speaker_id": "1",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036990.1.17",
          "form": "어느 계절을 가장 좋아하나요?",
          "original_form": "어느 계절을 가장 좋아하나요?",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036990.1.18",
          "form": "글쵸 장마는 여름이였는데",
          "original_form": "글쵸 장마는 여름이였는데",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036990.1.19",
          "form": "요즘날씨는 동남아같아져서",
          "original_form": "요즘날씨는 동남아같아져서",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036990.1.20",
          "form": "스콜같은것도 잦구요",
          "original_form": "스콜같은것도 잦구요",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036990.1.21",
          "form": "저는 가을 좋아해요",
          "original_form": "저는 가을 좋아해요",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036990.1.22",
          "form": "선선한 날",
          "original_form": "선선한 날",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036990.1.23",
          "form": "어떤계절좋아하세요?",
          "original_form": "어떤계절좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100036990.1.24",
          "form": "저랑 같네요.. 저도 가을 좋아합니다",
          "original_form": "저랑 같네요.. 저도 가을 좋아합니다",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100036990.1.25",
          "form": "단풍 좋아해요..",
          "original_form": "단풍 좋아해요..",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100036990.1.26",
          "form": "봄은 황사땜에 여름은 덥고 겨울은 춥고",
          "original_form": "봄은 황사땜에 여름은 덥고 겨울은 춥고",
          "speaker_id": "2",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036990.1.27",
          "form": "적당한 가을이 좋은데",
          "original_form": "적당한 가을이 좋은데",
          "speaker_id": "2",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036990.1.28",
          "form": "점점짧아지는듯한 기분이예요",
          "original_form": "점점짧아지는듯한 기분이예요",
          "speaker_id": "2",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036990.1.29",
          "form": "맞아요.. 봄가을은 너무 짧고 여름 겨울이 기네요",
          "original_form": "맞아요.. 봄가을은 너무 짧고 여름 겨울이 기네요",
          "speaker_id": "1",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100036990.1.30",
          "form": "겨울부터 봄까지는 미세먼지가 심하구..",
          "original_form": "겨울부터 봄까지는 미세먼지가 심하구..",
          "speaker_id": "1",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100036990.1.31",
          "form": "가을은 좋은데.. 요새 왜 가을장마가 생긴건지..",
          "original_form": "가을은 좋은데.. 요새 왜 가을장마가 생긴건지..",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036990.1.32",
          "form": "비오니 미세먼지 없어서 좋긴해요 ㅎ",
          "original_form": "비오니 미세먼지 없어서 좋긴해요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036990.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036990.1.34",
          "form": "그러게요..",
          "original_form": "그러게요..",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036990.1.35",
          "form": "장마지나믄 바로 겨울 올듯해요 ㅎ",
          "original_form": "장마지나믄 바로 겨울 올듯해요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036990.1.36",
          "form": "이제 슬슬 겨울이 다가오면 중국발 미세먼지가 심해지겠죠",
          "original_form": "이제 슬슬 겨울이 다가오면 중국발 미세먼지가 심해지겠죠",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036990.1.37",
          "form": "가을을 좀더 느끼고 싶은데요",
          "original_form": "가을을 좀더 느끼고 싶은데요",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036990.1.38",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036990.1.39",
          "form": "오늘 날씨가 2주만에 화창해요..",
          "original_form": "오늘 날씨가 2주만에 화창해요..",
          "speaker_id": "1",
          "time": "20210903 13:29"
        }
      ]
