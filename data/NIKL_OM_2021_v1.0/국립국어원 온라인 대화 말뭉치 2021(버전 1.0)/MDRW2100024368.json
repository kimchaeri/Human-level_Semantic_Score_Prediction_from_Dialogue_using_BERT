      "utterance": [
        {
          "id": "MDRW2100024368.1.1",
          "form": "길냥이들 인식이 좋게 바뀐 거 같아서",
          "original_form": "길냥이들 인식이 좋게 바뀐 거 같아서",
          "speaker_id": "2",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100024368.1.2",
          "form": "좋은것같아여",
          "original_form": "좋은것같아여",
          "speaker_id": "2",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100024368.1.3",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100024368.1.4",
          "form": "가끔 막 자기 데려가달라고 안달하능애도잇더라고요",
          "original_form": "가끔 막 자기 데려가달라고 안달하능애도잇더라고요",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100024368.1.5",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100024368.1.6",
          "form": "맞아여  ㅠㅠㅠ",
          "original_form": "맞아여  ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.7",
          "form": "특히",
          "original_form": "특히",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.8",
          "form": "걔는 누군가가 주워간것같아요",
          "original_form": "걔는 누군가가 주워간것같아요",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.9",
          "form": "겨울에 그런 애들 보면 진짜..ㅜㅠ",
          "original_form": "겨울에 그런 애들 보면 진짜..ㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.11",
          "form": "맴찢..",
          "original_form": "맴찢..",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.12",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.13",
          "form": "ㅠㅠ 그쵸... 아니 우리동네에 먹을것도없는데",
          "original_form": "ㅠㅠ 그쵸... 아니 우리동네에 먹을것도없는데",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.15",
          "form": "ㅠㅠ 저는 엄마가 고양이를 안좋아하셔서",
          "original_form": "ㅠㅠ 저는 엄마가 고양이를 안좋아하셔서",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.16",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠ따흑",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠ따흑",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.17",
          "form": "미래로 미뤄두고잇어요 ㅜㅜ",
          "original_form": "미래로 미뤄두고잇어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.18",
          "form": "ㅠㅠㅠㅠ 어머님이",
          "original_form": "ㅠㅠㅠㅠ 어머님이",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100024368.1.19",
          "form": "아예 동물을 안 좋아하세요?",
          "original_form": "아예 동물을 안 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 02:36"
        },
        {
          "id": "MDRW2100024368.1.20",
          "form": "그래서 유투브영상으로 만족중이에여",
          "original_form": "그래서 유투브영상으로 만족중이에여",
          "speaker_id": "1",
          "time": "20210831 02:36"
        },
        {
          "id": "MDRW2100024368.1.21",
          "form": "보는것만 좋아하시고 그러더라고요",
          "original_form": "보는것만 좋아하시고 그러더라고요",
