      "utterance": [
        {
          "id": "MDRW2100040132.1.1",
          "form": "진짜 잠들기전에",
          "original_form": "진짜 잠들기전에",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100040132.1.2",
          "form": "위험해요",
          "original_form": "위험해요",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100040132.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100040132.1.4",
          "form": "개미지옥",
          "original_form": "개미지옥",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100040132.1.5",
          "form": "구쵸",
          "original_form": "구쵸",
          "speaker_id": "1",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.6",
          "form": "하루종일 시간 버료요ㅠ",
          "original_form": "하루종일 시간 버료요ㅠ",
          "speaker_id": "1",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.7",
          "form": "사지도않고",
          "original_form": "사지도않고",
          "speaker_id": "1",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.8",
          "form": "모델분들",
          "original_form": "모델분들",
          "speaker_id": "2",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.9",
          "form": "넘나마르고",
          "original_form": "넘나마르고",
          "speaker_id": "2",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.10",
          "form": "이쁘심",
          "original_form": "이쁘심",
          "speaker_id": "2",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.11",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.12",
          "form": "맞죠 ㅠㅠ 뷰러운 인생들",
          "original_form": "맞죠 ㅠㅠ 뷰러운 인생들",
          "speaker_id": "1",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040132.1.13",
          "form": "삭스부츠도",
          "original_form": "삭스부츠도",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100040132.1.14",
          "form": "잇는데",
          "original_form": "잇는데",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100040132.1.15",
          "form": "하나더 살려고",
          "original_form": "하나더 살려고",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100040132.1.16",
          "form": "장바구니담앗어욤",
          "original_form": "장바구니담앗어욤",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100040132.1.17",
          "form": "저도요ㅠㅠ 삭스부츠샀어요",
          "original_form": "저도요ㅠㅠ 삭스부츠샀어요",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040132.1.18",
          "form": "근데개시못했어요 ㅋㅋ",
          "original_form": "근데개시못했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040132.1.19",
          "form": "추석쯤 개시하면될꺼같아요 ㅎㅎㅎ",
          "original_form": "추석쯤 개시하면될꺼같아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040132.1.20",
          "form": "맞아요 옷을 잘 챙겨입어야해서 잘 안신어져요",
          "original_form": "맞아요 옷을 잘 챙겨입어야해서 잘 안신어져요",
          "speaker_id": "1",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100040132.1.21",
          "form": "주말 잘보내시고계신가쇼",
          "original_form": "주말 잘보내시고계신가쇼",
          "speaker_id": "2",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100040132.1.22",
          "form": "날이 조으네요",
          "original_form": "날이 조으네요",
          "speaker_id": "2",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100040132.1.23",
          "form": "네네 오늘 오랜만에 해떴어용",
          "original_form": "네네 오늘 오랜만에 해떴어용",
          "speaker_id": "1",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100040132.1.24",
          "form": "어제 먼가 사구싶어서 돌아댕겻는데",
          "original_form": "어제 먼가 사구싶어서 돌아댕겻는데",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100040132.1.25",
          "form": "빈손으로왓어요",
          "original_form": "빈손으로왓어요",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100040132.1.26",
          "form": "대신",
          "original_form": "대신",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100040132.1.27",
          "form": "배는 꽉채웟쥬",
          "original_form": "배는 꽉채웟쥬",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100040132.1.28",
          "form": "뭐로 채우셨나요ㅋㅋ",
          "original_form": "뭐로 채우셨나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:39"
        },
        {
          "id": "MDRW2100040132.1.29",
          "form": "화덕피자 리조또 먹엇어요",
          "original_form": "화덕피자 리조또 먹엇어요",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100040132.1.30",
          "form": "우와 맛나겠다ㅠ",
          "original_form": "우와 맛나겠다ㅠ",
          "speaker_id": "1",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100040132.1.31",
          "form": "오늘벌써 주말다간거같아서 슬퍼요 흑",
          "original_form": "오늘벌써 주말다간거같아서 슬퍼요 흑",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100040132.1.32",
          "form": "저는 아침부터 간장게장 먹었어요",
          "original_form": "저는 아침부터 간장게장 먹었어요",
