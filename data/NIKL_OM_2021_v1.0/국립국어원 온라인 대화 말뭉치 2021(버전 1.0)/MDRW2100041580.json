      "utterance": [
        {
          "id": "MDRW2100041580.1.1",
          "form": "오늘점심은 뭘로 드시나요?",
          "original_form": "오늘점심은 뭘로 드시나요?",
          "speaker_id": "2",
          "time": "20210906 11:42"
        },
        {
          "id": "MDRW2100041580.1.2",
          "form": "저는 회가 먹고싶네요ㅠ",
          "original_form": "저는 회가 먹고싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 11:42"
        },
        {
          "id": "MDRW2100041580.1.3",
          "form": "회요? 생선회..?",
          "original_form": "회요? 생선회..?",
          "speaker_id": "2",
          "time": "20210906 11:42"
        },
        {
          "id": "MDRW2100041580.1.4",
          "form": "네 우럭이랑 광어 쫄깃쫄깃한 회요!",
          "original_form": "네 우럭이랑 광어 쫄깃쫄깃한 회요!",
          "speaker_id": "1",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100041580.1.5",
          "form": "아 맛있겠네요 저는 매운탕이 땡겨요!",
          "original_form": "아 맛있겠네요 저는 매운탕이 땡겨요!",
          "speaker_id": "2",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100041580.1.6",
          "form": "회먹고 나서 먹는 매운탕 맛있죠ㅠㅠ",
          "original_form": "회먹고 나서 먹는 매운탕 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100041580.1.7",
          "form": "네 산초가루 팍팍쳐서",
          "original_form": "네 산초가루 팍팍쳐서",
          "speaker_id": "2",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100041580.1.8",
          "form": "맞아요 매운탕 원래는 별로 안좋아하는데",
          "original_form": "맞아요 매운탕 원래는 별로 안좋아하는데",
          "speaker_id": "1",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100041580.1.9",
          "form": "회먹고나서는 꼭 먹어줘야하는 ㅎㅎ",
          "original_form": "회먹고나서는 꼭 먹어줘야하는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100041580.1.10",
          "form": "그러니까요!ㅋㅋ 그거먹으러 횟집가요",
          "original_form": "그러니까요!ㅋㅋ 그거먹으러 횟집가요",
          "speaker_id": "1",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100041580.1.11",
          "form": "횟집에서 주는 반찬들 은근 맛있지요",
          "original_form": "횟집에서 주는 반찬들 은근 맛있지요",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100041580.1.12",
          "form": "네 콘치즈부터 ㅋㅋㅋ",
          "original_form": "네 콘치즈부터 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100041580.1.13",
          "form": "조만간 회 한번 먹어야겠어요..",
          "original_form": "조만간 회 한번 먹어야겠어요..",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100041580.1.14",
          "form": "요즘 요거트를 즐겨먹고있어요",
          "original_form": "요즘 요거트를 즐겨먹고있어요",
