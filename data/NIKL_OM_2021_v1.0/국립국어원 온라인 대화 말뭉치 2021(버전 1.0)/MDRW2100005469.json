      "utterance": [
        {
          "id": "MDRW2100005469.1.1",
          "form": "님 해보고싶은 운동 있삼?",
          "original_form": "님 해보고싶은 운동 있삼?",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005469.1.2",
          "form": "나 필라테스 궁금해",
          "original_form": "나 필라테스 궁금해",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005469.1.3",
          "form": "그리고 폴댄스도 궁금해",
          "original_form": "그리고 폴댄스도 궁금해",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005469.1.4",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005469.1.5",
          "form": "난 발레 배워보고싶고",
          "original_form": "난 발레 배워보고싶고",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005469.1.6",
          "form": "배웠던거중에선 수영이랑 태권도",
          "original_form": "배웠던거중에선 수영이랑 태권도",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005469.1.7",
          "form": "나 수영도 배우고 싶고 복싱도 배우고 싶삼",
          "original_form": "나 수영도 배우고 싶고 복싱도 배우고 싶삼",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005469.1.8",
          "form": "발레도 재밌을 것 같아 왜 발레 배우고 싶어?",
          "original_form": "발레도 재밌을 것 같아 왜 발레 배우고 싶어?",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005469.1.9",
          "form": "그냥 뭔가 재밌을 것 같아 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 뭔가 재밌을 것 같아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005469.1.10",
          "form": "집근처 수영장 있었으면 당장 배웠을텐데 너무 아쉽다",
          "original_form": "집근처 수영장 있었으면 당장 배웠을텐데 너무 아쉽다",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005469.1.11",
          "form": "좀 멀더라도 슬렁슬렁 다녀봐봐",
          "original_form": "좀 멀더라도 슬렁슬렁 다녀봐봐",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005469.1.12",
          "form": "근처에 아예 없?",
          "original_form": "근처에 아예 없?",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005469.1.13",
          "form": "무족권 버스 갈아타야해",
          "original_form": "무족권 버스 갈아타야해",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005469.1.14",
          "form": "한번에 가는게 없어",
          "original_form": "한번에 가는게 없어",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005469.1.15",
          "form": "너무 귀찮...",
          "original_form": "너무 귀찮...",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005469.1.16",
          "form": "아 한번에 가는거면 몰라 갈아타는 건 좀 에바네...",
          "original_form": "아 한번에 가는거면 몰라 갈아타는 건 좀 에바네...",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005469.1.17",
          "form": "그래도 나름 번화가 쪽 아닌가",
          "original_form": "그래도 나름 번화가 쪽 아닌가",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005469.1.18",
          "form": "ㄴㄴ..",
          "original_form": "ㄴㄴ..",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005469.1.19",
          "form": "한번에 가는 버스 없는 게 의외네...",
          "original_form": "한번에 가는 버스 없는 게 의외네...",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005469.1.20",
          "form": "address가 살기 훨씬좋아",
          "original_form": "&address&가 살기 훨씬좋아",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005469.1.21",
          "form": "address 도태됨",
          "original_form": "&address& 도태됨",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005469.1.22",
          "form": "암튼..",
          "original_form": "암튼..",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005469.1.23",
          "form": "근데 님 전에",
          "original_form": "근데 님 전에",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005469.1.24",
          "form": "복싱 배웠었지 않았나",
          "original_form": "복싱 배웠었지 않았나",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005469.1.25",
          "form": "다이어트 복싱이여 별 도움 안됨 걍 복싱 배우고 싶어",
          "original_form": "다이어트 복싱이여 별 도움 안됨 걍 복싱 배우고 싶어",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005469.1.26",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005469.1.27",
          "form": "다이어트복싱은 뭐했어?",
          "original_form": "다이어트복싱은 뭐했어?",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005469.1.28",
          "form": "아 나 그것도 해보고싶다 점핑?",
          "original_form": "아 나 그것도 해보고싶다 점핑?",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005469.1.29",
          "form": "걍 주먹 휙휙이 다야 별로임",
          "original_form": "걍 주먹 휙휙이 다야 별로임",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005469.1.30",
          "form": "점핑 집에서 했는데ㅜ별로엿여 난",
          "original_form": "점핑 집에서 했는데ㅜ별로엿여 난",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005469.1.31",
          "form": "오.. 집에서도 가능하구나",
          "original_form": "오.. 집에서도 가능하구나",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005469.1.32",
          "form": "트램펄린 삿는데...",
          "original_form": "트램펄린 삿는데...",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005469.1.33",
          "form": "그닥이여...",
          "original_form": "그닥이여...",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005469.1.34",
          "form": "비추",
          "original_form": "비추",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005469.1.35",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005469.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005469.1.37",
          "form": "태권도 다시 다녀보면 어때",
          "original_form": "태권도 다시 다녀보면 어때",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005469.1.38",
          "form": "태권도도 다시 다녀보고 싶긴 해",
          "original_form": "태권도도 다시 다녀보고 싶긴 해",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005469.1.39",
          "form": "집 근처에 태권도 도장 없어?",
          "original_form": "집 근처에 태권도 도장 없어?",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005469.1.40",
          "form": "도장은 있긴 한데",
          "original_form": "도장은 있긴 한데",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005469.1.41",
          "form": "성인반이 있나 모르겠다",
          "original_form": "성인반이 있나 모르겠다",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005469.1.42",
          "form": "한번 전화 해봐봐",
          "original_form": "한번 전화 해봐봐",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100005469.1.43",
          "form": "성인반 있음 다닐겨?",
          "original_form": "성인반 있음 다닐겨?",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100005469.1.44",
          "form": "생각해보고..",
          "original_form": "생각해보고..",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100005469.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
