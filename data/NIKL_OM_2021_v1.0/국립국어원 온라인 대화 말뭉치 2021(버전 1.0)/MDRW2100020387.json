        },
        {
          "id": "MDRW2100020387.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020387.1.3",
          "form": "어릴때 하던 게임이잇나요. 백구말고요",
          "original_form": "어릴때 하던 게임이잇나요. 백구말고요",
          "speaker_id": "1",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020387.1.4",
          "form": "아니여",
          "original_form": "아니여",
          "speaker_id": "2",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020387.1.5",
          "form": "저는 게임기 했엇어요",
          "original_form": "저는 게임기 했엇어요",
          "speaker_id": "1",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020387.1.6",
          "form": "오천원의 행벅",
          "original_form": "오천원의 행벅",
          "speaker_id": "1",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020387.1.7",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "2",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020387.1.8",
          "form": "테트리스도하고 벽돌깨기도 했엇어요",
          "original_form": "테트리스도하고 벽돌깨기도 했엇어요",
          "speaker_id": "1",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020387.1.9",
          "form": "잘했나요?",
          "original_form": "잘했나요?",
          "speaker_id": "2",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020387.1.10",
          "form": "동생이더잘햇어요",
          "original_form": "동생이더잘햇어요",
          "speaker_id": "1",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020387.1.11",
          "form": "와 동생 대단해요!",
          "original_form": "와 동생 대단해요!",
          "speaker_id": "2",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020387.1.12",
          "form": "프로개이머였다니깐요!",
          "original_form": "프로개이머였다니깐요!",
          "speaker_id": "1",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020387.1.13",
          "form": "올 멋져요",
          "original_form": "올 멋져요",
          "speaker_id": "2",
          "time": "20210828 02:52"
