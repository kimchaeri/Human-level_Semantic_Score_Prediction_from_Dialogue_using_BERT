      "utterance": [
        {
          "id": "MDRW2100027613.1.1",
          "form": "크으 어묵탕에 소주 한잔하면 끝이죠",
          "original_form": "크으 어묵탕에 소주 한잔하면 끝이죠",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027613.1.2",
          "form": "와.. 맛잘알 ㅠㅠ",
          "original_form": "와.. 맛잘알 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027613.1.3",
          "form": "진짜 최고죠",
          "original_form": "진짜 최고죠",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027613.1.4",
          "form": "ㅠㅠ 너무 좋아요",
          "original_form": "ㅠㅠ 너무 좋아요",
          "speaker_id": "2",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027613.1.5",
          "form": "붕어빵 어떤 거 좋아하세요?",
          "original_form": "붕어빵 어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027613.1.6",
          "form": "팥 아님 슈크림?",
          "original_form": "팥 아님 슈크림?",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027613.1.7",
          "form": "저 팥이요!",
          "original_form": "저 팥이요!",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027613.1.8",
          "form": "슈크림도 좋은데 팥이 역시 ㅎㅎ",
          "original_form": "슈크림도 좋은데 팥이 역시 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027613.1.9",
          "form": "와 저두요!!",
          "original_form": "와 저두요!!",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027613.1.10",
          "form": "슈크림도 맛있지만 뭔가 부족한 느낌???",
          "original_form": "슈크림도 맛있지만 뭔가 부족한 느낌???",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027613.1.11",
          "form": "슈크림이 달기는 한데",
          "original_form": "슈크림이 달기는 한데",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027613.1.12",
          "form": "그것만 메리트가 있는거같아요 큐ㅠㅠ",
          "original_form": "그것만 메리트가 있는거같아요 큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027613.1.13",
          "form": "팥은 뭔가 맛의 조화도 적절하고 무난해서 좋은거같아요",
          "original_form": "팥은 뭔가 맛의 조화도 적절하고 무난해서 좋은거같아요",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027613.1.14",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027613.1.15",
          "form": "붕어빵은 역시 팥이 최고예요...",
          "original_form": "붕어빵은 역시 팥이 최고예요...",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027613.1.16",
          "form": "호떡도 좋아하세요??",
          "original_form": "호떡도 좋아하세요??",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027613.1.17",
          "form": "네 호떡도 엄청 좋아해요..ㅜㅜ",
          "original_form": "네 호떡도 엄청 좋아해요..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027613.1.18",
          "form": "씨호떡 엄청 좋아해요 특히",
          "original_form": "씨호떡 엄청 좋아해요 특히",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027613.1.19",
          "form": "씨앗호떡 말하는 거죠??",
          "original_form": "씨앗호떡 말하는 거죠??",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100027613.1.20",
          "form": "견과류 좋아하시나봐요!",
          "original_form": "견과류 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100027613.1.21",
          "form": "네! 엄청 좋아해요 ㅠㅠ",
          "original_form": "네! 엄청 좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027613.1.22",
          "form": "아 저는 별로 안 좋아해서 기본 호떡이 제일 좋아요...",
          "original_form": "아 저는 별로 안 좋아해서 기본 호떡이 제일 좋아요...",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100027613.1.23",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100027613.1.24",
          "form": "넹 호빵도 먹고 싶네요",
          "original_form": "넹 호빵도 먹고 싶네요",
