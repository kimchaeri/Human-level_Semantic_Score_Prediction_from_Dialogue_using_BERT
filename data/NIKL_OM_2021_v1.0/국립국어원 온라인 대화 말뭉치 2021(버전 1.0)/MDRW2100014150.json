      "utterance": [
        {
          "id": "MDRW2100014150.1.1",
          "form": "올림픽 보셨어요?",
          "original_form": "올림픽 보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014150.1.2",
          "form": "네 올림픽 드문드문 챙겨봤어요 name2님은 보셨어요?",
          "original_form": "네 올림픽 드문드문 챙겨봤어요 &name2&님은 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014150.1.3",
          "form": "네 저도 드문드문ㅎㅎ",
          "original_form": "네 저도 드문드문ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014150.1.4",
          "form": "어떤 경기 보셨어요?",
          "original_form": "어떤 경기 보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014150.1.5",
          "form": "양궁 보셨어요??",
          "original_form": "양궁 보셨어요??",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014150.1.6",
          "form": "전 여자 결승 밖에 못봤는데 엄청 긴장되더라구요ㅋㅋㅋ",
          "original_form": "전 여자 결승 밖에 못봤는데 엄청 긴장되더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014150.1.7",
          "form": "네 저 양궁 봤는데 ㅋㅋ진짜 잘하더라고요ㅎㅎ",
          "original_form": "네 저 양궁 봤는데 ㅋㅋ진짜 잘하더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014150.1.8",
          "form": "맞아요 ㅋㅋㅋㅋ 긴장하신 거 같은데도 엄청 잘 쏴서 놀랐어요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 긴장하신 거 같은데도 엄청 잘 쏴서 놀랐어요",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014150.1.9",
          "form": "ㅋㅋ맞아요 안산 선수는 게다가 3관왕했잖아요ㅎㅎ",
          "original_form": "ㅋㅋ맞아요 안산 선수는 게다가 3관왕했잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014150.1.10",
          "form": "대단하신듯ㅎㅎ",
          "original_form": "대단하신듯ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014150.1.11",
          "form": "맞아요 정말 멋지시더라구요...!",
          "original_form": "맞아요 정말 멋지시더라구요...!",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014150.1.12",
          "form": "또 다른 거는 뭐 보셨나요?",
          "original_form": "또 다른 거는 뭐 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014150.1.13",
          "form": "전 수영도 봤어요ㅋㅋ",
          "original_form": "전 수영도 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014150.1.14",
          "form": "오오 수영 좋았죠 이번에 수영이 메달을 땄던가요??!",
          "original_form": "오오 수영 좋았죠 이번에 수영이 메달을 땄던가요??!",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014150.1.15",
          "form": "메달은 못 땄는데 잘하더라고요ㅎㅎ",
          "original_form": "메달은 못 땄는데 잘하더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014150.1.16",
          "form": "제 2의 박태환같은 선수였어요ㅋㅋ",
          "original_form": "제 2의 박태환같은 선수였어요ㅋㅋ",
