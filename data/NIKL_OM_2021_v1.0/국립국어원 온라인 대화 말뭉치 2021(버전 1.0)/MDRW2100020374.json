        {
          "id": "MDRW2100020374.1.1",
          "form": "ㅎㅇ",
          "original_form": "ㅎㅇ",
          "speaker_id": "2",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020374.1.2",
          "form": "요즘 스위치가 대세야",
          "original_form": "요즘 스위치가 대세야",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020374.1.3",
          "form": "닌텐도 말하는거지?",
          "original_form": "닌텐도 말하는거지?",
          "speaker_id": "2",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020374.1.4",
          "form": "맞아 나도 하나 있어",
          "original_form": "맞아 나도 하나 있어",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020374.1.5",
          "form": "우리 저번에 오버 쿡 해봤잖아",
          "original_form": "우리 저번에 오버 쿡 해봤잖아",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.6",
          "form": "맞아 우리 같이 한 거 꽤 어려웠지",
          "original_form": "맞아 우리 같이 한 거 꽤 어려웠지",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.7",
          "form": "원래는 그게 싸움 유발 게임 이래",
          "original_form": "원래는 그게 싸움 유발 게임 이래",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.8",
          "form": "우리는 그래도 웃기면서 했던 거 같죠",
          "original_form": "우리는 그래도 웃기면서 했던 거 같죠",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.9",
          "form": "뭐 완전 재미있게 했는데",
          "original_form": "뭐 완전 재미있게 했는데",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.10",
          "form": "다른 버전 있으면 또 하고 싶어",
          "original_form": "다른 버전 있으면 또 하고 싶어",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.11",
          "form": "그게 새로운 시즌 꺼 나오면 좋겠다",
          "original_form": "그게 새로운 시즌 꺼 나오면 좋겠다",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.12",
          "form": "진짜 잘 맞는 게임 같아",
          "original_form": "진짜 잘 맞는 게임 같아",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.13",
          "form": "딴 사람이랑도 한번 해보고 싶다",
          "original_form": "딴 사람이랑도 한번 해보고 싶다",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.14",
          "form": "말이 안통해서 답답할 거 같아",
          "original_form": "말이 안통해서 답답할 거 같아",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020374.1.15",
          "form": "소리가 안 들려서 그건 그러네",
          "original_form": "소리가 안 들려서 그건 그러네",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
