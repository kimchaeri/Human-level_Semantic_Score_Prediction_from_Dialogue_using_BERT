      "utterance": [
        {
          "id": "MDRW2100039786.1.1",
          "form": "안녕하세요 저녁드셨나요?",
          "original_form": "안녕하세요 저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039786.1.2",
          "form": "안녕하세요! 저녁 먹었습니당",
          "original_form": "안녕하세요! 저녁 먹었습니당",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039786.1.3",
          "form": "전 찜닭먹었는데 name1님은 뭐드셨나요?",
          "original_form": "전 찜닭먹었는데 &name1&님은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100039786.1.4",
          "form": "저는 오늘 돼지갈비요ㅎㅎ",
          "original_form": "저는 오늘 돼지갈비요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039786.1.5",
          "form": "돼지갈비ㅜㅜ 맛있을거같아요ㅋㅋㅋㅋ",
          "original_form": "돼지갈비ㅜㅜ 맛있을거같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039786.1.6",
          "form": "돼지갈비 양념에 밥도 비벼먹고싶네용...",
          "original_form": "돼지갈비 양념에 밥도 비벼먹고싶네용...",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039786.1.7",
          "form": "그죠 맛있더라구요",
          "original_form": "그죠 맛있더라구요",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039786.1.8",
          "form": "오랜만에 먹으니까",
          "original_form": "오랜만에 먹으니까",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039786.1.9",
          "form": "갈비는 그날 다 안 먹구 담날까지 계속 데우면서 푹 익혀먹어도",
          "original_form": "갈비는 그날 다 안 먹구 담날까지 계속 데우면서 푹 익혀먹어도",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039786.1.10",
          "form": "맛잇엇던거같아요",
          "original_form": "맛잇엇던거같아요",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039786.1.11",
          "form": "마치 며칠끓인 카레가 더 맛난것처럼...ㅋㅋㅋ",
          "original_form": "마치 며칠끓인 카레가 더 맛난것처럼...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039786.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039786.1.13",
          "form": "고기는 옳죠",
          "original_form": "고기는 옳죠",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039786.1.14",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039786.1.15",
          "form": "카레도 맛나요",
          "original_form": "카레도 맛나요",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039786.1.16",
          "form": "좋아해요?",
          "original_form": "좋아해요?",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039786.1.17",
          "form": "네ㅋㅋㅋㅋ 고기도 카레도 좋아해요",
          "original_form": "네ㅋㅋㅋㅋ 고기도 카레도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039786.1.18",
          "form": "카레에 고기 듬뿍 넣은 고기카레도 좋습니당",
          "original_form": "카레에 고기 듬뿍 넣은 고기카레도 좋습니당",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039786.1.19",
          "form": "맛있으면 다 좋아요 사실ㅋㅋㅋ",
          "original_form": "맛있으면 다 좋아요 사실ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039786.1.20",
          "form": "아 저도욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 저도욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039786.1.21",
          "form": "맛있으면 땡ㅎㅎ",
          "original_form": "맛있으면 땡ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039786.1.22",
          "form": "그리고 기왕이면... 고기 들어가면 씹는맛 있는게 좋은거같구요..ㅋㅋㅋㅋ",
          "original_form": "그리고 기왕이면... 고기 들어가면 씹는맛 있는게 좋은거같구요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039786.1.23",
          "form": "전에 마블링 디게 많은 고기 먹었는데",
          "original_form": "전에 마블링 디게 많은 고기 먹었는데",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039786.1.24",
          "form": "그건또 생각보다 그렇게까지 맛잇지는않더라구요",
          "original_form": "그건또 생각보다 그렇게까지 맛잇지는않더라구요",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039786.1.25",
          "form": "약간 느끼하죠?",
          "original_form": "약간 느끼하죠?",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039786.1.26",
          "form": "네ㅠ ㅋㅋㅋ많이는 못먹을거같았어요",
          "original_form": "네ㅠ ㅋㅋㅋ많이는 못먹을거같았어요",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100039786.1.27",
          "form": "저두요ㅎㅎ소고기 많이는 못먹어요",
          "original_form": "저두요ㅎㅎ소고기 많이는 못먹어요",
          "speaker_id": "1",
          "time": "20210904 08:32"
        },
        {
          "id": "MDRW2100039786.1.28",
          "form": "그래서 소도 맛있지만 또 돼지나 닭이 땡길때도 있나 싶어요",
          "original_form": "그래서 소도 맛있지만 또 돼지나 닭이 땡길때도 있나 싶어요",
          "speaker_id": "2",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100039786.1.29",
          "form": "그날그날 입맛이 그런걸수도있지만..",
          "original_form": "그날그날 입맛이 그런걸수도있지만..",
          "speaker_id": "2",
          "time": "20210904 13:06"
        }
      ]
