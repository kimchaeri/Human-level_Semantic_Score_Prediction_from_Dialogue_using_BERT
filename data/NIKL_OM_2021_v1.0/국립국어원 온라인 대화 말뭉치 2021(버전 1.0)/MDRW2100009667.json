      "utterance": [
        {
          "id": "MDRW2100009667.1.1",
          "form": "나 치킨 사랑해",
          "original_form": "나 치킨 사랑해",
          "speaker_id": "2",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009667.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009667.1.3",
          "form": "앜ㅋㅋㅋ 무슨 치킨 제일 좋아해?",
          "original_form": "앜ㅋㅋㅋ 무슨 치킨 제일 좋아해?",
          "speaker_id": "1",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009667.1.4",
          "form": "나는 뿌링클 사랑해!",
          "original_form": "나는 뿌링클 사랑해!",
          "speaker_id": "1",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009667.1.5",
          "form": "나 뿌링클",
          "original_form": "나 뿌링클",
          "speaker_id": "2",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009667.1.6",
          "form": "헐!!!!!",
          "original_form": "헐!!!!!",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.8",
          "form": "와 역시~",
          "original_form": "와 역시~",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.9",
          "form": "와 통했네~~~",
          "original_form": "와 통했네~~~",
          "speaker_id": "1",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.10",
          "form": "두번 째 사랑하는거는 허니콤보랑 노랑통닭이야~",
          "original_form": "두번 째 사랑하는거는 허니콤보랑 노랑통닭이야~",
          "speaker_id": "1",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.12",
          "form": "ㄴㅏ 노랑통닭 진짜 좋아해",
          "original_form": "ㄴㅏ 노랑통닭 진짜 좋아해",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.13",
          "form": "근데 요즘",
          "original_form": "근데 요즘",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.14",
          "form": "금동이 통닭인가",
          "original_form": "금동이 통닭인가",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.15",
          "form": "그거에 꽂혔어",
          "original_form": "그거에 꽂혔어",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009667.1.16",
          "form": "친구가 그거에 꽂혀가지고",
          "original_form": "친구가 그거에 꽂혀가지고",
          "speaker_id": "2",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009667.1.17",
          "form": "영업당했스",
          "original_form": "영업당했스",
          "speaker_id": "2",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009667.1.18",
          "form": "엥 그거는 프랜차이즈야?",
          "original_form": "엥 그거는 프랜차이즈야?",
          "speaker_id": "1",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009667.1.19",
          "form": "처음들어봐 진짜",
          "original_form": "처음들어봐 진짜",
          "speaker_id": "1",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009667.1.20",
          "form": "씁... 프랜차이즈일걸?",
          "original_form": "씁... 프랜차이즈일걸?",
          "speaker_id": "2",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009667.1.21",
          "form": "아마도?",
          "original_form": "아마도?",
          "speaker_id": "2",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009667.1.22",
          "form": "근데 한국에 치킨 브랜드 워낙많아서",
          "original_form": "근데 한국에 치킨 브랜드 워낙많아서",
          "speaker_id": "2",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009667.1.23",
          "form": "정말? 꼭 시켜먹어볼게",
          "original_form": "정말? 꼭 시켜먹어볼게",
          "speaker_id": "1",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009667.1.24",
          "form": "맛있겠다 이름 약간 옛날 감성같아",
          "original_form": "맛있겠다 이름 약간 옛날 감성같아",
          "speaker_id": "1",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009667.1.25",
          "form": "근데 진짜 옛날감성 맛이야",
          "original_form": "근데 진짜 옛날감성 맛이야",
          "speaker_id": "2",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009667.1.26",
          "form": "진짜 진짜 맛있어 정석 치킨!",
          "original_form": "진짜 진짜 맛있어 정석 치킨!",
          "speaker_id": "2",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009667.1.27",
          "form": "오 그렇구나 기대된다~",
          "original_form": "오 그렇구나 기대된다~",
          "speaker_id": "1",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009667.1.28",
          "form": "허니콤보는 어때?",
          "original_form": "허니콤보는 어때?",
          "speaker_id": "1",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009667.1.29",
          "form": "허니콤보 감히 내가",
          "original_form": "허니콤보 감히 내가",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009667.1.30",
          "form": "평가할 수 없지",
          "original_form": "평가할 수 없지",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009667.1.31",
          "form": "근ㄷㅔ 어디서 먹어본 맛이라서",
          "original_form": "근ㄷㅔ 어디서 먹어본 맛이라서",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009667.1.32",
          "form": "잘 안찾는듯",
          "original_form": "잘 안찾는듯",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009667.1.33",
          "form": "ㅋㅋㅋㅋㅋ 그거 맞아 근데 허니콤보",
          "original_form": "ㅋㅋㅋㅋㅋ 그거 맞아 근데 허니콤보",
          "speaker_id": "1",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009667.1.34",
          "form": "요즘 너무 흔한 맛임",
          "original_form": "요즘 너무 흔한 맛임",
          "speaker_id": "1",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009667.1.35",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009667.1.36",
          "form": "약간 간장치킨?",
          "original_form": "약간 간장치킨?",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009667.1.37",
          "form": "요즘에 김치치킨도 나온대",
          "original_form": "요즘에 김치치킨도 나온대",
          "speaker_id": "1",
          "time": "20210823 03:06"
        }
      ]
