        {
          "id": "MDRW2100002671.1.1",
          "form": "날씨좋아요?",
          "original_form": "날씨좋아요?",
          "speaker_id": "1",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002671.1.2",
          "form": "날씨어때요?",
          "original_form": "날씨어때요?",
          "speaker_id": "1",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002671.1.3",
          "form": "날씨 맑아요",
          "original_form": "날씨 맑아요",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002671.1.4",
          "form": "그쪽은 어떤가요",
          "original_form": "그쪽은 어떤가요",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002671.1.5",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002671.1.6",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100002671.1.7",
          "form": "네 오늘은 오네요",
          "original_form": "네 오늘은 오네요",
          "speaker_id": "2",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100002671.1.8",
          "form": "비가 오나요?",
          "original_form": "비가 오나요?",
          "speaker_id": "2",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100002671.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100002671.1.10",
          "form": "이번주 다음주 장마래요",
          "original_form": "이번주 다음주 장마래요",
          "speaker_id": "2",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100002671.1.11",
          "form": "가을장마",
          "original_form": "가을장마",
          "speaker_id": "2",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100002671.1.12",
          "form": "심하네요",
          "original_form": "심하네요",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100002671.1.13",
          "form": "하늘에 구멍이 뚫린 것 마냥 내리는데 좀 무섭네요",
          "original_form": "하늘에 구멍이 뚫린 것 마냥 내리는데 좀 무섭네요",
          "speaker_id": "2",
          "time": "20210821 12:58"
        },
        {
          "id": "MDRW2100002671.1.14",
          "form": "네 앞이 안보이더라구요",
          "original_form": "네 앞이 안보이더라구요",
          "speaker_id": "1",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100002671.1.15",
          "form": "비오는 날 어떠세요?",
          "original_form": "비오는 날 어떠세요?",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100002671.1.16",
          "form": "전 싫어요",
          "original_form": "전 싫어요",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100002671.1.17",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100002671.1.18",
          "form": "저도 싫어하는 편이에요",
          "original_form": "저도 싫어하는 편이에요",
          "speaker_id": "2",
          "time": "20210821 13:11"
        },
        {
          "id": "MDRW2100002671.1.19",
          "form": "비오는거 왜 싫어하시나요",
          "original_form": "비오는거 왜 싫어하시나요",
          "speaker_id": "2",
          "time": "20210821 13:11"
        },
        {
          "id": "MDRW2100002671.1.20",
          "form": "습해서요 ㅎㅎ",
          "original_form": "습해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:13"
        }
      ]
