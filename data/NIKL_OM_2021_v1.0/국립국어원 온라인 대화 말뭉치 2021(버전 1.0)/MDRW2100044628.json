      "utterance": [
        {
          "id": "MDRW2100044628.1.1",
          "form": "저 바람과 함께 사라지다랑 구마님 하나씩 말해요 저희 ㅋㅋㅋㅋ",
          "original_form": "저 바람과 함께 사라지다랑 구마님 하나씩 말해요 저희 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044628.1.2",
          "form": "저는 사빠딸이요!",
          "original_form": "저는 사빠딸이요!",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044628.1.3",
          "form": "악 저 그거 말하려했는데ㅋㅋㅋ 그럼 전 그린티!!",
          "original_form": "악 저 그거 말하려했는데ㅋㅋㅋ 그럼 전 그린티!!",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044628.1.4",
          "form": "저는 엄마는 외계인도 좋아해요!",
          "original_form": "저는 엄마는 외계인도 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044628.1.5",
          "form": "저는 또 뉴치케!",
          "original_form": "저는 또 뉴치케!",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044628.1.6",
          "form": "오 저는 애플민트도 좋아해요",
          "original_form": "오 저는 애플민트도 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044628.1.7",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044628.1.8",
          "form": "애플민트 별미죠! 가끔 먹어요",
          "original_form": "애플민트 별미죠! 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044628.1.9",
          "form": "맞아요 맛있어요",
          "original_form": "맞아요 맛있어요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044628.1.10",
          "form": "레샤 드시나요? 호불호 많이 갈리던데",
          "original_form": "레샤 드시나요? 호불호 많이 갈리던데",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044628.1.11",
          "form": "아 좋아합니다",
          "original_form": "아 좋아합니다",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044628.1.12",
          "form": "드시나요?",
          "original_form": "드시나요?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044628.1.13",
          "form": "저 완전 사랑",
          "original_form": "저 완전 사랑",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044628.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044628.1.15",
          "form": "맛있죠?",
          "original_form": "맛있죠?",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044628.1.16",
          "form": "네 ㅋㅋㅋㅋㅋ 레샤만 있으면 입이 싹 정리되는 기분?",
