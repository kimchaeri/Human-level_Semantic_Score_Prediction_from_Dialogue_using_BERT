      "utterance": [
        {
          "id": "MDRW2100020730.1.1",
          "form": "양많고 맛좋은게 짱이져",
          "original_form": "양많고 맛좋은게 짱이져",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020730.1.2",
          "form": "저는 그렇담",
          "original_form": "저는 그렇담",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020730.1.3",
          "form": "곱창 좋아하세요",
          "original_form": "곱창 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020730.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020730.1.5",
          "form": "네 말씀하세요",
          "original_form": "네 말씀하세요",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020730.1.6",
          "form": "완전 좋아해묘",
          "original_form": "완전 좋아해묘",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020730.1.7",
          "form": "마이셰프 회원가입을 하겠다고..",
          "original_form": "마이셰프 회원가입을 하겠다고..",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020730.1.8",
          "form": "앜ㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.9",
          "form": "곱창 구이Vs전골",
          "original_form": "곱창 구이Vs전골",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.10",
          "form": "하 어렵...",
          "original_form": "하 어렵...",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.11",
          "form": "구이요",
          "original_form": "구이요",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.12",
          "form": "구이가 좀더 감칠맛이있지",
          "original_form": "구이가 좀더 감칠맛이있지",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.13",
          "form": "박빙인데 오늘은 해가 떴으니까",
          "original_form": "박빙인데 오늘은 해가 떴으니까",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.14",
          "form": "구이",
          "original_form": "구이",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.15",
          "form": "그 십으면 십을수록",
          "original_form": "그 십으면 십을수록",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.16",
          "form": "고소하잖아요",
          "original_form": "고소하잖아요",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.17",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020730.1.18",
          "form": "뭐찍어드세요",
          "original_form": "뭐찍어드세요",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020730.1.19",
          "form": "돼지곱창vs소곱창",
          "original_form": "돼지곱창vs소곱창",
          "speaker_id": "2",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020730.1.20",
          "form": "소곱창",
          "original_form": "소곱창",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020730.1.21",
          "form": "구이는 소곱창이죠",
          "original_form": "구이는 소곱창이죠",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020730.1.22",
          "form": "역시 그렇담 콩가루죠",
          "original_form": "역시 그렇담 콩가루죠",
          "speaker_id": "2",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020730.1.23",
          "form": "혹시.. 돼지곱창",
          "original_form": "혹시.. 돼지곱창",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020730.1.24",
          "form": "좋아하시나요",
          "original_form": "좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020730.1.25",
          "form": "전 소막창도 사랑해요",
          "original_form": "전 소막창도 사랑해요",
          "speaker_id": "2",
          "time": "20210828 11:48"
        },
        {
