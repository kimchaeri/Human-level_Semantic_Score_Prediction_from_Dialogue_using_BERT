      "utterance": [
        {
          "id": "MDRW2100032849.1.1",
          "form": "아니 나한테 사이트 좀 보내줘",
          "original_form": "아니 나한테 사이트 좀 보내줘",
          "speaker_id": "2",
          "time": "20210906 01:47"
        },
        {
          "id": "MDRW2100032849.1.2",
          "form": "카톡으로 보냈어",
          "original_form": "카톡으로 보냈어",
          "speaker_id": "1",
          "time": "20210906 01:49"
        },
        {
          "id": "MDRW2100032849.1.3",
          "form": "이게 이번에 새르 나온거야???",
          "original_form": "이게 이번에 새르 나온거야???",
          "speaker_id": "2",
          "time": "20210906 01:49"
        },
        {
          "id": "MDRW2100032849.1.4",
          "form": "이쁘네",
          "original_form": "이쁘네",
          "speaker_id": "2",
          "time": "20210906 01:50"
        },
        {
          "id": "MDRW2100032849.1.5",
          "form": "이쁘지 기능도 좋더라구",
          "original_form": "이쁘지 기능도 좋더라구",
          "speaker_id": "1",
          "time": "20210906 01:51"
        },
        {
          "id": "MDRW2100032849.1.6",
          "form": "저것도 사려고???",
          "original_form": "저것도 사려고???",
          "speaker_id": "2",
          "time": "20210906 01:51"
        },
        {
          "id": "MDRW2100032849.1.7",
          "form": "시계를 안차고 다녀서 필요없긴한데",
          "original_form": "시계를 안차고 다녀서 필요없긴한데",
          "speaker_id": "1",
          "time": "20210906 01:53"
        },
        {
          "id": "MDRW2100032849.1.8",
          "form": "교통카드로 쓸 수 있더라고",
          "original_form": "교통카드로 쓸 수 있더라고",
          "speaker_id": "1",
          "time": "20210906 01:53"
        },
        {
          "id": "MDRW2100032849.1.9",
          "form": "시계 안차고 다니는데 뭐하러사",
          "original_form": "시계 안차고 다니는데 뭐하러사",
          "speaker_id": "2",
          "time": "20210906 01:54"
        },
        {
          "id": "MDRW2100032849.1.10",
          "form": "집에 사놓고 안쓰는거 많잖아",
          "original_form": "집에 사놓고 안쓰는거 많잖아",
          "speaker_id": "2",
          "time": "20210906 01:54"
        },
        {
          "id": "MDRW2100032849.1.11",
          "form": "나에 대해서 너무 잘아는군",
          "original_form": "나에 대해서 너무 잘아는군",
          "speaker_id": "1",
          "time": "20210906 01:56"
        },
        {
          "id": "MDRW2100032849.1.12",
          "form": "그냥 핸드폰으로 시간봐",
          "original_form": "그냥 핸드폰으로 시간봐",
          "speaker_id": "2",
          "time": "20210906 01:57"
        },
        {
          "id": "MDRW2100032849.1.13",
          "form": "흥 그럴거야",
          "original_form": "흥 그럴거야",
          "speaker_id": "1",
          "time": "20210906 01:58"
        },
        {
          "id": "MDRW2100032849.1.14",
          "form": "시계 가격은 얼마야",
          "original_form": "시계 가격은 얼마야",
