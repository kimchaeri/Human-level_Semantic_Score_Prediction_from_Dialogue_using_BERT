      "utterance": [
        {
          "id": "MDRW2100016254.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016254.1.2",
          "form": "스포츠좋아하는종목 있으신가요?",
          "original_form": "스포츠좋아하는종목 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016254.1.3",
          "form": "배구요!",
          "original_form": "배구요!",
          "speaker_id": "1",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100016254.1.4",
          "form": "이번에 올림픽 재밌었어요",
          "original_form": "이번에 올림픽 재밌었어요",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016254.1.5",
          "form": "맞아요  터키 일본전 박진감 넘쳤죠",
          "original_form": "맞아요  터키 일본전 박진감 넘쳤죠",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016254.1.6",
          "form": "대단했어요ㅋㅋ 보는 제가 더 긴장감이..",
          "original_form": "대단했어요ㅋㅋ 보는 제가 더 긴장감이..",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016254.1.7",
          "form": "양효진 선수 멋있죠",
          "original_form": "양효진 선수 멋있죠",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016254.1.8",
          "form": "전사실 김연경선수밖에 몰랐는데 다멋있졌어요ㅋ",
          "original_form": "전사실 김연경선수밖에 몰랐는데 다멋있졌어요ㅋ",
          "speaker_id": "2",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016254.1.9",
          "form": "직관 해보셨나요?",
          "original_form": "직관 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016254.1.10",
          "form": "직관은안해봤어요",
          "original_form": "직관은안해봤어요",
          "speaker_id": "2",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016254.1.11",
          "form": "응원하는 팀있으신가요?",
          "original_form": "응원하는 팀있으신가요?",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016254.1.12",
          "form": "딱히 응원하는팀은없어요.",
          "original_form": "딱히 응원하는팀은없어요.",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016254.1.13",
          "form": "응원하는 팀 있으세요?",
          "original_form": "응원하는 팀 있으세요?",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016254.1.14",
          "form": "고민중이에요",
          "original_form": "고민중이에요",
          "speaker_id": "1",
