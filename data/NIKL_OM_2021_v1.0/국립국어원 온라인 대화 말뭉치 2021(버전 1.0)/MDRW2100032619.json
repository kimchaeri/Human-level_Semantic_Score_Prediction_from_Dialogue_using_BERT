      "utterance": [
        {
          "id": "MDRW2100032619.1.1",
          "form": "만화책 읽어보셨나요?",
          "original_form": "만화책 읽어보셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100032619.1.2",
          "form": "웹툰 말고 만화책으로 본지는 한참 됐네요",
          "original_form": "웹툰 말고 만화책으로 본지는 한참 됐네요",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032619.1.3",
          "form": "그쵸 웹툰보죠 요즘은",
          "original_form": "그쵸 웹툰보죠 요즘은",
          "speaker_id": "2",
          "time": "20210906 08:31"
        },
        {
          "id": "MDRW2100032619.1.4",
          "form": "재밌게 본 만화책 있으세요~?",
          "original_form": "재밌게 본 만화책 있으세요~?",
          "speaker_id": "2",
          "time": "20210906 08:31"
        },
        {
          "id": "MDRW2100032619.1.5",
          "form": "예전에 강철의 연금술사 봤었는데 지금 생각하면 내용이 쫌 그래요",
          "original_form": "예전에 강철의 연금술사 봤었는데 지금 생각하면 내용이 쫌 그래요",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032619.1.6",
          "form": "저는 항상 도전은 하는데 강철연술사는 보질못하겠어요",
          "original_form": "저는 항상 도전은 하는데 강철연술사는 보질못하겠어요",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100032619.1.7",
          "form": "제 타입이 아닌",
          "original_form": "제 타입이 아닌",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100032619.1.8",
          "form": "블리치 다시 볼까해요 ㅎ 애니로는 다시 보고있는데",
          "original_form": "블리치 다시 볼까해요 ㅎ 애니로는 다시 보고있는데",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100032619.1.9",
          "form": "블리치도 한참 보다 말았는데 완결 났나요?",
          "original_form": "블리치도 한참 보다 말았는데 완결 났나요?",
          "speaker_id": "1",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100032619.1.10",
          "form": "네 그럴걸요!?",
          "original_form": "네 그럴걸요!?",
          "speaker_id": "2",
          "time": "20210906 16:29"
        },
        {
          "id": "MDRW2100032619.1.11",
          "form": "저도 한참 안보다가 최근에 다시보고있어용",
          "original_form": "저도 한참 안보다가 최근에 다시보고있어용",
          "speaker_id": "2",
          "time": "20210906 16:29"
        },
        {
          "id": "MDRW2100032619.1.12",
          "form": "어디서 어떻게 보세요?",
          "original_form": "어디서 어떻게 보세요?",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100032619.1.13",
          "form": "저~ 라프텔로 보고있어요! 원래 유튜브로 간략정리한거 보다가 ㅎ 그 이후 내용 궁금해져서 이어서 보고있어요",
          "original_form": "저~ 라프텔로 보고있어요! 원래 유튜브로 간략정리한거 보다가 ㅎ 그 이후 내용 궁금해져서 이어서 보고있어요",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100032619.1.14",
          "form": "라프텔 처음 들어봤는데 찾아보니 신기하네요 ㅎㅎ",
          "original_form": "라프텔 처음 들어봤는데 찾아보니 신기하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100032619.1.15",
          "form": "애니 보는 곳이에용",
          "original_form": "애니 보는 곳이에용",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100032619.1.16",
          "form": "괜찮더라고용",
          "original_form": "괜찮더라고용",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100032619.1.17",
          "form": "크롬캐스트로도 볼수있어서 보고있어여",
          "original_form": "크롬캐스트로도 볼수있어서 보고있어여",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100032619.1.18",
          "form": "만화책방 가서 한참 쭉 읽고싶네요ㅎㅎ",
          "original_form": "만화책방 가서 한참 쭉 읽고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100032619.1.19",
          "form": "그쵸ㅠ 코로나땜에 못가본지 1년이 넘네요",
          "original_form": "그쵸ㅠ 코로나땜에 못가본지 1년이 넘네요",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100032619.1.20",
          "form": "진짜 밤새 있었는데ㅋㅋㅋ",
          "original_form": "진짜 밤새 있었는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100032619.1.21",
          "form": "만화책보고싶네요",
          "original_form": "만화책보고싶네요",
          "speaker_id": "2",
          "time": "20210907 08:50"
        }
      ]
