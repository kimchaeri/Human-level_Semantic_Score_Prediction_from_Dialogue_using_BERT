      "utterance": [
        {
          "id": "MDRW2100015661.1.1",
          "form": "넷플릭스 결제해서 보시나요?",
          "original_form": "넷플릭스 결제해서 보시나요?",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015661.1.2",
          "form": "이번에 해지했어요",
          "original_form": "이번에 해지했어요",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015661.1.3",
          "form": "헉 왜 해지하셨어요?",
          "original_form": "헉 왜 해지하셨어요?",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015661.1.4",
          "form": "보고싶은 컨텐츠들을 다보고",
          "original_form": "보고싶은 컨텐츠들을 다보고",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015661.1.5",
          "form": "생각보다 볼시간이 적어져서요",
          "original_form": "생각보다 볼시간이 적어져서요",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015661.1.6",
          "form": "추천해주실만한 컨텐츠 있으신가요?",
          "original_form": "추천해주실만한 컨텐츠 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015661.1.7",
          "form": "종이의 집? 제목이 헷갈리는데 킬링타임용으로 추천이요",
          "original_form": "종이의 집? 제목이 헷갈리는데 킬링타임용으로 추천이요",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015661.1.8",
          "form": "왓챠나 티빙도 결제 안하세요?",
          "original_form": "왓챠나 티빙도 결제 안하세요?",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015661.1.9",
          "form": "네 안해요",
          "original_form": "네 안해요",
          "speaker_id": "2",
          "time": "20210826 07:52"
        },
        {
          "id": "MDRW2100015661.1.10",
          "form": "음악들을 때 어디 사이트 애용하세요??",
          "original_form": "음악들을 때 어디 사이트 애용하세요??",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100015661.1.11",
          "form": "저는 유튜브 레드 썼었는데 이젠 멜론이요",
          "original_form": "저는 유튜브 레드 썼었는데 이젠 멜론이요",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100015661.1.12",
          "form": "뭐 사용하시나요",
          "original_form": "뭐 사용하시나요",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100015661.1.13",
          "form": "저는 벅스요! 요즘 무슨 노래 들으세요?",
          "original_form": "저는 벅스요! 요즘 무슨 노래 들으세요?",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100015661.1.14",
          "form": "악뮤 낙화들어요",
          "original_form": "악뮤 낙화들어요",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100015661.1.15",
          "form": "요즘 노래 뭐 들으세요?",
          "original_form": "요즘 노래 뭐 들으세요?",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100015661.1.16",
          "form": "낙하네요 오타예요",
          "original_form": "낙하네요 오타예요",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100015661.1.17",
          "form": "전소미 신곡 들어요!",
          "original_form": "전소미 신곡 들어요!",
          "speaker_id": "1",
          "time": "20210826 11:25"
        }
      ]
