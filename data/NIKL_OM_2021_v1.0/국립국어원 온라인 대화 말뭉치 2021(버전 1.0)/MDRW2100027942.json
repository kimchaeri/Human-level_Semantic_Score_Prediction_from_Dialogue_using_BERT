      "utterance": [
        {
          "id": "MDRW2100027942.1.1",
          "form": "지금 먹고싶은거 뭐야",
          "original_form": "지금 먹고싶은거 뭐야",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027942.1.2",
          "form": "치킨 먹고 싶어",
          "original_form": "치킨 먹고 싶어",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027942.1.3",
          "form": "어제 양념 치킨 시켰는데 후라이드 옴",
          "original_form": "어제 양념 치킨 시켰는데 후라이드 옴",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027942.1.4",
          "form": "하지만 항의하지 않았다",
          "original_form": "하지만 항의하지 않았다",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027942.1.5",
          "form": "치킨 맛있겠다. 어디꺼로 주문했어",
          "original_form": "치킨 맛있겠다. 어디꺼로 주문했어",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027942.1.6",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100027942.1.7",
          "form": "왜 답장 안해",
          "original_form": "왜 답장 안해",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027942.1.8",
          "form": "나 했어",
          "original_form": "나 했어",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100027942.1.9",
          "form": "케이크 먹고 싶다",
          "original_form": "케이크 먹고 싶다",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100027942.1.10",
          "form": "근데 왜 안왔지 케이크 무슨종류",
          "original_form": "근데 왜 안왔지 케이크 무슨종류",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100027942.1.11",
          "form": "오늘 저녁 초밥이다",
          "original_form": "오늘 저녁 초밥이다",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100027942.1.12",
          "form": "나는 돈가스 먹었다 점심에",
          "original_form": "나는 돈가스 먹었다 점심에",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100027942.1.13",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100027942.1.14",
          "form": "본부장이랑 먹어서 체할뻔",
          "original_form": "본부장이랑 먹어서 체할뻔",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100027942.1.15",
          "form": "다남김",
          "original_form": "다남김",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100027942.1.16",
          "form": "내아까운우동",
          "original_form": "내아까운우동",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100027942.1.17",
          "form": "맞다 참",
          "original_form": "맞다 참",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100027942.1.18",
          "form": "넌 저녁 뭐 먹어?",
          "original_form": "넌 저녁 뭐 먹어?",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100027942.1.19",
          "form": "나는 육포",
          "original_form": "나는 육포",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100027942.1.20",
          "form": "그게 다야?",
          "original_form": "그게 다야?",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100027942.1.21",
          "form": "육포랑 삶은계란",
          "original_form": "육포랑 삶은계란",
