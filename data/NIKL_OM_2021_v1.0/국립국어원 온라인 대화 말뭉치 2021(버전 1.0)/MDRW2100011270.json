      "utterance": [
        {
          "id": "MDRW2100011270.1.1",
          "form": "이번에 올림픽 보셨나요?",
          "original_form": "이번에 올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 10:30"
        },
        {
          "id": "MDRW2100011270.1.2",
          "form": "네 올림픽 봤습니다 배구가 난리였죠",
          "original_form": "네 올림픽 봤습니다 배구가 난리였죠",
          "speaker_id": "2",
          "time": "20210824 10:32"
        },
        {
          "id": "MDRW2100011270.1.3",
          "form": "맞아요 뉴스 엄청 떴었죠",
          "original_form": "맞아요 뉴스 엄청 떴었죠",
          "speaker_id": "1",
          "time": "20210824 10:33"
        },
        {
          "id": "MDRW2100011270.1.4",
          "form": "혹시 기억에 남는 경기 있으세요?",
          "original_form": "혹시 기억에 남는 경기 있으세요?",
          "speaker_id": "1",
          "time": "20210824 10:33"
        },
        {
          "id": "MDRW2100011270.1.5",
          "form": "전 김제덕선수랑 안산선수 양궁이 기억에 남네요",
          "original_form": "전 김제덕선수랑 안산선수 양궁이 기억에 남네요",
          "speaker_id": "2",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011270.1.6",
          "form": "첫 금메달이라 그런지는 몰라도 재밌었어요",
          "original_form": "첫 금메달이라 그런지는 몰라도 재밌었어요",
          "speaker_id": "2",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011270.1.7",
          "form": "어떤 경기 좋아하셨나요?",
          "original_form": "어떤 경기 좋아하셨나요?",
          "speaker_id": "2",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011270.1.8",
          "form": "우리나라 경기는 배구 한일전이 제일 좋았고",
          "original_form": "우리나라 경기는 배구 한일전이 제일 좋았고",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011270.1.9",
          "form": "다른 나라 경기까지 합하면 카누가 기억이 나네요",
          "original_form": "다른 나라 경기까지 합하면 카누가 기억이 나네요",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011270.1.10",
          "form": "오 많이 챙겨보셨나봐요",
          "original_form": "오 많이 챙겨보셨나봐요",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100011270.1.11",
          "form": "카누는 못 봤습니다",
          "original_form": "카누는 못 봤습니다",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100011270.1.12",
          "form": "높이뛰기도 인상 깊어썽요",
          "original_form": "높이뛰기도 인상 깊어썽요",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100011270.1.13",
          "form": "높이뛰기도 봤어요",
          "original_form": "높이뛰기도 봤어요",
          "speaker_id": "1",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100011270.1.14",
          "form": "우리나라 기록이 그렇게 높을줄은 몰랐어요",
          "original_form": "우리나라 기록이 그렇게 높을줄은 몰랐어요",
          "speaker_id": "1",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100011270.1.15",
          "form": "그러게요 4등할줄이야 멋있었습니다",
          "original_form": "그러게요 4등할줄이야 멋있었습니다",
          "speaker_id": "2",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100011270.1.16",
          "form": "시도 좀 더 해봤더라면 메달 충분히 딸 것 같았는데 아쉬웠구요",
          "original_form": "시도 좀 더 해봤더라면 메달 충분히 딸 것 같았는데 아쉬웠구요",
          "speaker_id": "2",
          "time": "20210824 11:48"
        },
        {
          "id": "MDRW2100011270.1.17",
          "form": "아쉽긴 하더라구요 이번에 따면 첫 메달이었던가요",
          "original_form": "아쉽긴 하더라구요 이번에 따면 첫 메달이었던가요",
          "speaker_id": "1",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100011270.1.18",
          "form": "네네 이미 한국 신기록은 깼어요",
          "original_form": "네네 이미 한국 신기록은 깼어요",
          "speaker_id": "2",
          "time": "20210824 12:00"
        },
        {
          "id": "MDRW2100011270.1.19",
          "form": "벌써 다음 올림픽이 기대되는군요 진짜 메달 따실수 있을거 같아요",
          "original_form": "벌써 다음 올림픽이 기대되는군요 진짜 메달 따실수 있을거 같아요",
          "speaker_id": "1",
          "time": "20210824 12:04"
        },
        {
          "id": "MDRW2100011270.1.20",
          "form": "다른 스포츠경기 좋아하는건 있으세요?",
          "original_form": "다른 스포츠경기 좋아하는건 있으세요?",
          "speaker_id": "2",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011270.1.21",
          "form": "농구도 봐요 한국리그는 안보고 NBA만요",
          "original_form": "농구도 봐요 한국리그는 안보고 NBA만요",
          "speaker_id": "1",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100011270.1.22",
          "form": "와 농구를 보시다니",
          "original_form": "와 농구를 보시다니",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011270.1.23",
          "form": "전 옛날에 농구겜 할때 선수들 조금 알고는 끝이네요",
          "original_form": "전 옛날에 농구겜 할때 선수들 조금 알고는 끝이네요",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011270.1.24",
          "form": "저는 딱 2시즌 보고 작년 시즌은 쉬었어요",
          "original_form": "저는 딱 2시즌 보고 작년 시즌은 쉬었어요",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011270.1.25",
          "form": "응원팀이 예전만큼 하질 못해서...",
          "original_form": "응원팀이 예전만큼 하질 못해서...",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011270.1.26",
          "form": "어디 팬이세요?",
          "original_form": "어디 팬이세요?",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011270.1.27",
          "form": "골든스테이트 응원했었어요",
          "original_form": "골든스테이트 응원했었어요",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011270.1.28",
          "form": "아? 모르겠네요ㅋㅋㅋㅋ",
          "original_form": "아? 모르겠네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011270.1.29",
          "form": "완전 유명한 팀만 몇개 알아서",
          "original_form": "완전 유명한 팀만 몇개 알아서",
