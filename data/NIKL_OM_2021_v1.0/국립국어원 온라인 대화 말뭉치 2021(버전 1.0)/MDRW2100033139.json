      "utterance": [
        {
          "id": "MDRW2100033139.1.1",
          "form": "언니 공연 보러 자주 가?",
          "original_form": "언니 공연 보러 자주 가?",
          "speaker_id": "1",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100033139.1.2",
          "form": "나는 고등학교때가 마지막이었던거 같아",
          "original_form": "나는 고등학교때가 마지막이었던거 같아",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100033139.1.3",
          "form": "아 헐 고등학교 때 뭐 보러 갔는디",
          "original_form": "아 헐 고등학교 때 뭐 보러 갔는디",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100033139.1.4",
          "form": "아 아닌가 중학교때인가 수학여행때",
          "original_form": "아 아닌가 중학교때인가 수학여행때",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033139.1.5",
          "form": "뮤지컬 보러갔었어",
          "original_form": "뮤지컬 보러갔었어",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033139.1.6",
          "form": "와 수학여행 때? 대박",
          "original_form": "와 수학여행 때? 대박",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033139.1.7",
          "form": "ㅜㅜ 부럽당",
          "original_form": "ㅜㅜ 부럽당",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033139.1.8",
          "form": "너는?-?",
          "original_form": "너는?-?",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033139.1.9",
          "form": "난 누가 학교 와서 공연 한 적은 있는 거 같아",
          "original_form": "난 누가 학교 와서 공연 한 적은 있는 거 같아",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033139.1.10",
          "form": "뮤지컬이었나??",
          "original_form": "뮤지컬이었나??",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033139.1.11",
          "form": "오오 좋지좋지",
          "original_form": "오오 좋지좋지",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033139.1.12",
          "form": "콘서트는 가본 적 있어?",
          "original_form": "콘서트는 가본 적 있어?",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033139.1.13",
          "form": "그건 엄마 따라..",
          "original_form": "그건 엄마 따라..",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033139.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033139.1.15",
          "form": "무슨 콘서트??",
          "original_form": "무슨 콘서트??",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100033139.1.16",
          "form": "몰라 무슨 가수였나",
          "original_form": "몰라 무슨 가수였나",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100033139.1.17",
          "form": "그때 신기했음.",
          "original_form": "그때 신기했음.",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100033139.1.18",
          "form": "나는 티켓팅은 몇번해봤어 친구꺼 대신!",
          "original_form": "나는 티켓팅은 몇번해봤어 친구꺼 대신!",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100033139.1.19",
          "form": "티켓팅 잘해?????",
          "original_form": "티켓팅 잘해?????",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100033139.1.20",
          "form": "멋있당",
          "original_form": "멋있당",
          "speaker_id": "1",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033139.1.21",
          "form": "모르겠어.. 티켓팅 된거였는데 안된줄 알고",
          "original_form": "모르겠어.. 티켓팅 된거였는데 안된줄 알고",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033139.1.22",
          "form": "뒤로가서",
          "original_form": "뒤로가서",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033139.1.23",
          "form": "실패했었어",
          "original_form": "실패했었어",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033139.1.24",
          "form": "안돼ㅠㅠ",
          "original_form": "안돼ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033139.1.25",
          "form": "그래도 티켓팅 많이 하면",
          "original_form": "그래도 티켓팅 많이 하면",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033139.1.26",
          "form": "수강신청도.",
          "original_form": "수강신청도.",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033139.1.27",
          "form": "잘하겠군",
          "original_form": "잘하겠군",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033139.1.28",
          "form": "나는 시간표 짜서 나와서.....",
          "original_form": "나는 시간표 짜서 나와서.....",
