      "utterance": [
        {
          "id": "MDRW2100036665.1.1",
          "form": "아직이요ㅠ",
          "original_form": "아직이요ㅠ",
          "speaker_id": "2",
          "time": "20210831 06:39"
        },
        {
          "id": "MDRW2100036665.1.2",
          "form": "name1님은요.?",
          "original_form": "&name1&님은요.?",
          "speaker_id": "2",
          "time": "20210831 06:39"
        },
        {
          "id": "MDRW2100036665.1.3",
          "form": "저도 담달에 예약 돼있어요",
          "original_form": "저도 담달에 예약 돼있어요",
          "speaker_id": "1",
          "time": "20210831 06:43"
        },
        {
          "id": "MDRW2100036665.1.4",
          "form": "가족들은 다 맞아서 저만 늦는거 같아요",
          "original_form": "가족들은 다 맞아서 저만 늦는거 같아요",
          "speaker_id": "1",
          "time": "20210831 06:43"
        },
        {
          "id": "MDRW2100036665.1.5",
          "form": "가족분들은 일찍 맞으셨네요!",
          "original_form": "가족분들은 일찍 맞으셨네요!",
          "speaker_id": "2",
          "time": "20210831 06:49"
        },
        {
          "id": "MDRW2100036665.1.6",
          "form": "저도 늦네요ㅠ",
          "original_form": "저도 늦네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 06:49"
        },
        {
          "id": "MDRW2100036665.1.7",
          "form": "가족들이 맞고 큰 부작용 없이 넘어가서 다행인데",
          "original_form": "가족들이 맞고 큰 부작용 없이 넘어가서 다행인데",
          "speaker_id": "1",
          "time": "20210831 07:10"
        },
        {
          "id": "MDRW2100036665.1.8",
          "form": "저는 어떨지 걱정이 좀 되네요",
          "original_form": "저는 어떨지 걱정이 좀 되네요",
          "speaker_id": "1",
          "time": "20210831 07:10"
        },
        {
          "id": "MDRW2100036665.1.9",
          "form": "다행이네요!",
          "original_form": "다행이네요!",
          "speaker_id": "2",
          "time": "20210831 07:12"
        },
        {
          "id": "MDRW2100036665.1.10",
          "form": "name1님도 그러실거에요!",
          "original_form": "&name1&님도 그러실거에요!",
          "speaker_id": "2",
          "time": "20210831 07:12"
        },
        {
          "id": "MDRW2100036665.1.11",
          "form": "네 전 아직 백신이 안정해졌는데 name2님도 그렇죠?",
          "original_form": "네 전 아직 백신이 안정해졌는데 &name2&님도 그렇죠?",
          "speaker_id": "1",
          "time": "20210831 07:16"
        },
        {
          "id": "MDRW2100036665.1.12",
          "form": "네ㅠ 둘 중 하나로 되어있네요",
          "original_form": "네ㅠ 둘 중 하나로 되어있네요",
          "speaker_id": "2",
          "time": "20210831 07:18"
        },
        {
          "id": "MDRW2100036665.1.13",
          "form": "은근 떨려요",
          "original_form": "은근 떨려요",
          "speaker_id": "2",
          "time": "20210831 07:18"
        },
        {
          "id": "MDRW2100036665.1.14",
          "form": "저도 화이자 아니먼 모더나래요",
          "original_form": "저도 화이자 아니먼 모더나래요",
          "speaker_id": "1",
          "time": "20210831 07:22"
        },
        {
          "id": "MDRW2100036665.1.15",
          "form": "선택권은 없겠지만 있다면 뭘 선호하세요?",
          "original_form": "선택권은 없겠지만 있다면 뭘 선호하세요?",
          "speaker_id": "1",
          "time": "20210831 07:23"
        },
        {
          "id": "MDRW2100036665.1.16",
          "form": "모더나요!",
          "original_form": "모더나요!",
          "speaker_id": "2",
          "time": "20210831 07:23"
        },
        {
          "id": "MDRW2100036665.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 07:24"
        },
        {
          "id": "MDRW2100036665.1.18",
          "form": "전 화이자 생각했는데 요새 모더나가 인기더라구요",
          "original_form": "전 화이자 생각했는데 요새 모더나가 인기더라구요",
          "speaker_id": "1",
          "time": "20210831 07:25"
        },
        {
          "id": "MDRW2100036665.1.19",
          "form": "모더나가 더 좋아요?",
          "original_form": "모더나가 더 좋아요?",
          "speaker_id": "1",
          "time": "20210831 07:25"
        },
        {
          "id": "MDRW2100036665.1.20",
          "form": "20대는 화이자 부작용이 있기도하다해서오ㅜㅜ",
          "original_form": "20대는 화이자 부작용이 있기도하다해서오ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 07:30"
        },
        {
          "id": "MDRW2100036665.1.21",
          "form": "아항 모더나가 젤 부작용이 적나봐요",
          "original_form": "아항 모더나가 젤 부작용이 적나봐요",
          "speaker_id": "1",
          "time": "20210831 07:33"
        },
        {
          "id": "MDRW2100036665.1.22",
          "form": "그렇다더라구요!",
          "original_form": "그렇다더라구요!",
          "speaker_id": "2",
          "time": "20210831 08:24"
        },
        {
          "id": "MDRW2100036665.1.23",
          "form": "아예 없을 수는 없으니 불안하기는 하지만요ㅠ",
          "original_form": "아예 없을 수는 없으니 불안하기는 하지만요ㅠ",
          "speaker_id": "2",
          "time": "20210831 08:24"
        },
        {
          "id": "MDRW2100036665.1.24",
          "form": "뭘 맞든 부작용없이 지나가요 우리~",
          "original_form": "뭘 맞든 부작용없이 지나가요 우리~",
          "speaker_id": "1",
          "time": "20210831 08:33"
        }
      ]
