      "utterance": [
        {
          "id": "MDRW2100002211.1.1",
          "form": "운동 좋아해요?",
          "original_form": "운동 좋아해요?",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002211.1.2",
          "form": "저는 수영을 좋아해서",
          "original_form": "저는 수영을 좋아해서",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002211.1.3",
          "form": "수영경기 관람 좋아해요",
          "original_form": "수영경기 관람 좋아해요",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002211.1.4",
          "form": "오 저는 물을 무서워해서",
          "original_form": "오 저는 물을 무서워해서",
          "speaker_id": "2",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002211.1.5",
          "form": "수영은 어릴때 해본게 다인거같아요",
          "original_form": "수영은 어릴때 해본게 다인거같아요",
          "speaker_id": "2",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002211.1.6",
          "form": "아무래도 배워두면 좋을 것 같긴해요",
          "original_form": "아무래도 배워두면 좋을 것 같긴해요",
          "speaker_id": "2",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002211.1.7",
          "form": "수영 재미있는데",
          "original_form": "수영 재미있는데",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002211.1.8",
          "form": "저도 어릴때 수영하다가",
          "original_form": "저도 어릴때 수영하다가",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002211.1.9",
          "form": "귀에 물들어가서 중이염와서 쉬게됐어요",
          "original_form": "귀에 물들어가서 중이염와서 쉬게됐어요",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002211.1.10",
          "form": "아 저도 귀에 물이 잘 들어가는 편이래요",
          "original_form": "아 저도 귀에 물이 잘 들어가는 편이래요",
          "speaker_id": "2",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002211.1.11",
          "form": "근데 수영이 칼로리 소모도 좋고",
          "original_form": "근데 수영이 칼로리 소모도 좋고",
          "speaker_id": "2",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002211.1.12",
          "form": "땀나는 운동이 아니니까",
          "original_form": "땀나는 운동이 아니니까",
          "speaker_id": "2",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002211.1.13",
          "form": "나중에 여유로워지면 꼭 해보고싶어요",
          "original_form": "나중에 여유로워지면 꼭 해보고싶어요",
          "speaker_id": "2",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002211.1.14",
          "form": "저도 땀나는 운동이 아니라",
          "original_form": "저도 땀나는 운동이 아니라",
          "speaker_id": "1",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002211.1.15",
          "form": "수영 경기는 직관해보셨어요?",
          "original_form": "수영 경기는 직관해보셨어요?",
          "speaker_id": "2",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002211.1.16",
          "form": "수영이 좋았어요",
          "original_form": "수영이 좋았어요",
          "speaker_id": "1",
          "time": "20210820 01:09"
        },
        {
          "id": "MDRW2100002211.1.17",
          "form": "직관이라고 할만한 경기는 아닌데",
          "original_form": "직관이라고 할만한 경기는 아닌데",
          "speaker_id": "1",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002211.1.18",
          "form": "지역대회 출전하고 관람한적 있어요",
          "original_form": "지역대회 출전하고 관람한적 있어요",
          "speaker_id": "1",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002211.1.19",
          "form": "우와 출전까지요?",
          "original_form": "우와 출전까지요?",
          "speaker_id": "2",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002211.1.20",
          "form": "너무 멋있다",
          "original_form": "너무 멋있다",
          "speaker_id": "2",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002211.1.21",
          "form": "전 직접 본적은 한번도 없는거같아요",
          "original_form": "전 직접 본적은 한번도 없는거같아요",
          "speaker_id": "2",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002211.1.22",
          "form": "지역대회여서 취미 수영인들끼리 한거였어요!",
          "original_form": "지역대회여서 취미 수영인들끼리 한거였어요!",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002211.1.23",
          "form": "수영대회 직관은 조금..",
          "original_form": "수영대회 직관은 조금..",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002211.1.24",
          "form": "좋아하는? 관심가는 선수가 없으면",
          "original_form": "좋아하는? 관심가는 선수가 없으면",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002211.1.25",
          "form": "재미없는거 같아요",
          "original_form": "재미없는거 같아요",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002211.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 보면 시원할 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 보면 시원할 것 같아요",
          "speaker_id": "2",
          "time": "20210820 01:15"
        },
        {
          "id": "MDRW2100002211.1.27",
          "form": "그리거 야구처럼 어려운게 아니니까",
          "original_form": "그리거 야구처럼 어려운게 아니니까",
          "speaker_id": "2",
          "time": "20210820 01:15"
        },
        {
          "id": "MDRW2100002211.1.28",
          "form": "하긴 그래요",
          "original_form": "하긴 그래요",
          "speaker_id": "1",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002211.1.29",
          "form": "이해는 쉬워서",
          "original_form": "이해는 쉬워서",
          "speaker_id": "1",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002211.1.30",
          "form": "나중에 기회되면 직관 가보고싶네요!!",
          "original_form": "나중에 기회되면 직관 가보고싶네요!!",
          "speaker_id": "2",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002211.1.31",
          "form": "코로나가 빨리 사라져야 경기들을 직관할텐데ㅠㅠ",
          "original_form": "코로나가 빨리 사라져야 경기들을 직관할텐데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002211.1.32",
          "form": "그러게요ㅜㅜ",
          "original_form": "그러게요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002211.1.33",
          "form": "올림픽 무관중",
          "original_form": "올림픽 무관중",
          "speaker_id": "1",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002211.1.34",
          "form": "충격적이었어요",
          "original_form": "충격적이었어요",
          "speaker_id": "1",
          "time": "20210820 01:17"
        }
      ]
