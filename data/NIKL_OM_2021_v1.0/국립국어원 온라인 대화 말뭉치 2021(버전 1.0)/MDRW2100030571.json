      "utterance": [
        {
          "id": "MDRW2100030571.1.1",
          "form": "name2님 저는 지금 일어났어요,,",
          "original_form": "&name2&님 저는 지금 일어났어요,,",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030571.1.2",
          "form": "어제 일 좀 열심히 하고 퇴근 후에 필테까지 해서",
          "original_form": "어제 일 좀 열심히 하고 퇴근 후에 필테까지 해서",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030571.1.3",
          "form": "뻗었나봐욬ㅋㅋㅋㅋ",
          "original_form": "뻗었나봐욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030571.1.4",
          "form": "ㅋㅋㅋㅋㅋ와 진짜 너무 행복한 토욜이죠",
          "original_form": "ㅋㅋㅋㅋㅋ와 진짜 너무 행복한 토욜이죠",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100030571.1.5",
          "form": "알람없이 늦잠자는 주말",
          "original_form": "알람없이 늦잠자는 주말",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030571.1.6",
          "form": "제일 행복해욬ㅋㅋㅋㅋ",
          "original_form": "제일 행복해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030571.1.7",
          "form": "맞아요...아침에 잠깐 일어나긴 했는데",
          "original_form": "맞아요...아침에 잠깐 일어나긴 했는데",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030571.1.8",
          "form": "오늘도 출근하는 날인 줄 알고",
          "original_form": "오늘도 출근하는 날인 줄 알고",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030571.1.9",
          "form": "저절로 깼다가 토요일인거 알고 다시 잤답니다",
          "original_form": "저절로 깼다가 토요일인거 알고 다시 잤답니다",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030571.1.10",
          "form": "name2님은 오늘 뭐하세요?!",
          "original_form": "&name2&님은 오늘 뭐하세요?!",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030571.1.11",
          "form": "저 오늘 완전 집순이~~~~",
          "original_form": "저 오늘 완전 집순이~~~~",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030571.1.12",
          "form": "지금 아이돌받쓰대회 보고잇어욬ㅋㅋㅋㅋ",
          "original_form": "지금 아이돌받쓰대회 보고잇어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030571.1.13",
          "form": "오 아주 좋아요~~!!! 저도 오늘은 집에만 있을거예요!!!",
          "original_form": "오 아주 좋아요~~!!! 저도 오늘은 집에만 있을거예요!!!",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030571.1.14",
          "form": "휴일에는 주로 집에서 쉬시나요?!",
          "original_form": "휴일에는 주로 집에서 쉬시나요?!",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030571.1.15",
          "form": "주말은 주로 아인이랑 친구들만나서 놀아요 ㅋㅋㅋㅋ",
          "original_form": "주말은 주로 아인이랑 친구들만나서 놀아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030571.1.16",
          "form": "가끔 이렇게 쉬어주는 주도 있구 ㅋㅋㅋㅋ",
          "original_form": "가끔 이렇게 쉬어주는 주도 있구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030571.1.17",
          "form": "name1님은 주말에 뭐하시나요",
          "original_form": "&name1&님은 주말에 뭐하시나요",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030571.1.18",
          "form": "name3님이 어제 제 블로그에 놀러오셨어요!!!",
          "original_form": "&name3&님이 어제 제 블로그에 놀러오셨어요!!!",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100030571.1.19",
          "form": "저도 주말에 원래 친구들이랑 만나서 노는데",
          "original_form": "저도 주말에 원래 친구들이랑 만나서 노는데",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030571.1.20",
          "form": "오늘은 쉬면서 블로그 좀 하고 내일은 친구 생일이라 이태원 가요!!",
          "original_form": "오늘은 쉬면서 블로그 좀 하고 내일은 친구 생일이라 이태원 가요!!",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030571.1.21",
          "form": "오 이태원 핫플이죠!!",
          "original_form": "오 이태원 핫플이죠!!",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030571.1.22",
          "form": "맛집 서치하셧나요 ㅎㅎ",
          "original_form": "맛집 서치하셧나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030571.1.23",
          "form": "네네!! 근데 어디를 갈지 아직 결정을 못했어요ㅠ",
          "original_form": "네네!! 근데 어디를 갈지 아직 결정을 못했어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030571.1.24",
          "form": "다음주에는 name4 생일이라",
          "original_form": "다음주에는 &name4& 생일이라",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030571.1.25",
          "form": "그그 도치요!",
          "original_form": "그그 도치요!",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
