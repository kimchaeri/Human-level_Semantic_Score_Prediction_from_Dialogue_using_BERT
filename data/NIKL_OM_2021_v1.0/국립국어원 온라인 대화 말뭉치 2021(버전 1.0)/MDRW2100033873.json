      "utterance": [
        {
          "id": "MDRW2100033873.1.1",
          "form": "삼겹살vs목살은 어떠세요?!",
          "original_form": "삼겹살vs목살은 어떠세요?!",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100033873.1.2",
          "form": "전 삼겹살요",
          "original_form": "전 삼겹살요",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100033873.1.3",
          "form": "근데 반반사요",
          "original_form": "근데 반반사요",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100033873.1.4",
          "form": "ㅋㅋㅋ저도 삼겹이요",
          "original_form": "ㅋㅋㅋ저도 삼겹이요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033873.1.5",
          "form": "목살은 안먹은지 꽤된것같아요",
          "original_form": "목살은 안먹은지 꽤된것같아요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033873.1.6",
          "form": "한국인은 삼겹이죠",
          "original_form": "한국인은 삼겹이죠",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033873.1.7",
          "form": "흑돼지는 어떠세요?",
          "original_form": "흑돼지는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100033873.1.8",
          "form": "오 샤브샤브 맛나요",
          "original_form": "오 샤브샤브 맛나요",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033873.1.9",
          "form": "제주도 흑돼지 샤브",
          "original_form": "제주도 흑돼지 샤브",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033873.1.10",
          "form": "반했었어요",
          "original_form": "반했었어요",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033873.1.11",
          "form": "ㅋㅋㅋ 흑돼지샤브오오",
          "original_form": "ㅋㅋㅋ 흑돼지샤브오오",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100033873.1.12",
          "form": "저는 흑돼지구이만먹어봤고",
          "original_form": "저는 흑돼지구이만먹어봤고",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100033873.1.13",
          "form": "샤브는 안먹어봤네요",
          "original_form": "샤브는 안먹어봤네요",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100033873.1.14",
          "form": "소고기 질긴데 돼지 완전 부드러워요",
          "original_form": "소고기 질긴데 돼지 완전 부드러워요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033873.1.15",
          "form": "제주도라 신선 냄새안나요",
          "original_form": "제주도라 신선 냄새안나요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033873.1.16",
          "form": "어우 제주도가고싶네요",
          "original_form": "어우 제주도가고싶네요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100033873.1.17",
          "form": "흑돼지 짱",
          "original_form": "흑돼지 짱",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100033873.1.18",
          "form": "네 흑돼지는 제주도죠",
          "original_form": "네 흑돼지는 제주도죠",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100033873.1.19",
          "form": "점심도 여태 못먹소 이제 먹네요",
          "original_form": "점심도 여태 못먹소 이제 먹네요",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100033873.1.20",
          "form": "뭐드셨죠?",
          "original_form": "뭐드셨죠?",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100033873.1.21",
          "form": "저 정신이 혼미해요 배고파서 ㅋㅋ",
          "original_form": "저 정신이 혼미해요 배고파서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100033873.1.22",
          "form": "ㅋㅋㅋㅋ 저 치킨먹었어요ㅠ",
          "original_form": "ㅋㅋㅋㅋ 저 치킨먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100033873.1.23",
          "form": "오 치킨 앙 부러워요",
          "original_form": "오 치킨 앙 부러워요",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100033873.1.24",
          "form": "저 남은거 어제 다 먹었어요 ㅋㅋ",
          "original_form": "저 남은거 어제 다 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100033873.1.25",
          "form": "앙 아쉽다",
          "original_form": "앙 아쉽다",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100033873.1.26",
          "form": "전 찌개에 대충 김으로 때웠어요",
          "original_form": "전 찌개에 대충 김으로 때웠어요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033873.1.27",
          "form": "아들오면 떡볶이 사오라 하려고요",
          "original_form": "아들오면 떡볶이 사오라 하려고요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033873.1.28",
          "form": "저녁 뭐 드실거예요",
          "original_form": "저녁 뭐 드실거예요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033873.1.29",
          "form": "오 떡볶이마싰겠어요ㅋㅋㅋㅋ",
          "original_form": "오 떡볶이마싰겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100033873.1.30",
          "form": "저는 저녁은아직",
          "original_form": "저는 저녁은아직",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100033873.1.31",
          "form": "저는 저녁은 아직 고민중이랍니다ㅠ",
          "original_form": "저는 저녁은 아직 고민중이랍니다ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100033873.1.32",
          "form": "오떡볶이 맛있겠어요!!!!!!",
          "original_form": "오떡볶이 맛있겠어요!!!!!!",
