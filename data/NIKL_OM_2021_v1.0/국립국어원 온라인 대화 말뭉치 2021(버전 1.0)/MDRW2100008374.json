      "utterance": [
        {
          "id": "MDRW2100008374.1.1",
          "form": "진짜 큰일날뻔 하셨네요..",
          "original_form": "진짜 큰일날뻔 하셨네요..",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008374.1.2",
          "form": "그거때문에 관절이 안좋으신거예요?",
          "original_form": "그거때문에 관절이 안좋으신거예요?",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008374.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008374.1.4",
          "form": "수술후 재활이 안됐나봐요",
          "original_form": "수술후 재활이 안됐나봐요",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008374.1.5",
          "form": "아고 ㅠㅠㅠ",
          "original_form": "아고 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008374.1.6",
          "form": "이어 붙였는데 짧아서",
          "original_form": "이어 붙였는데 짧아서",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008374.1.7",
          "form": "지금이라도 어떻게 안되나요?ㅠㅠ",
          "original_form": "지금이라도 어떻게 안되나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008374.1.8",
          "form": "걷기해요",
          "original_form": "걷기해요",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008374.1.9",
          "form": "저는 체중때문에 관절이 안좋은건지",
          "original_form": "저는 체중때문에 관절이 안좋은건지",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008374.1.10",
          "form": "심한건 무릎이 무리가서요",
          "original_form": "심한건 무릎이 무리가서요",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008374.1.11",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008374.1.12",
          "form": "그래서 다이어트를 하는데... ㅎㅎㅎ",
          "original_form": "그래서 다이어트를 하는데... ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008374.1.13",
          "form": "도저희 안되서 정형외과가서 벌써 무릎주사도 여러번 맞았어요 ㅠㅠ",
          "original_form": "도저희 안되서 정형외과가서 벌써 무릎주사도 여러번 맞았어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008374.1.14",
          "form": "엥 저보다 더 심하신가봐요",
          "original_form": "엥 저보다 더 심하신가봐요",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008374.1.15",
          "form": "계단 올라갈때 아이구야라는 말이 저절로 나와요 ㅎㅎㅎㅎㅎㅎ",
          "original_form": "계단 올라갈때 아이구야라는 말이 저절로 나와요 ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008374.1.16",
          "form": "어릴때부터 허리도 안좋았구.. 뼈가 원래 좀 그런가봐요 ㅠㅠ",
          "original_form": "어릴때부터 허리도 안좋았구.. 뼈가 원래 좀 그런가봐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008374.1.17",
          "form": "세상에 원래 약한분이 있어요 ㅎㅎ",
          "original_form": "세상에 원래 약한분이 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008374.1.18",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008374.1.19",
          "form": "원래 그런듯해요 ㅜㅜ",
          "original_form": "원래 그런듯해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008374.1.20",
          "form": "진짜 관리 잘하셔야 해요",
          "original_form": "진짜 관리 잘하셔야 해요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008374.1.21",
          "form": "체중이 한쪽으로 쏠리면 무릎이 빠지면서 안펴지거든요..",
          "original_form": "체중이 한쪽으로 쏠리면 무릎이 빠지면서 안펴지거든요..",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008374.1.22",
          "form": "전 그때 물리치료안해서 후회중요",
          "original_form": "전 그때 물리치료안해서 후회중요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        }
      ]
