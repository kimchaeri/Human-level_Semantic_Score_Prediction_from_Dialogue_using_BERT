      "utterance": [
        {
          "id": "MDRW2100044056.1.1",
          "form": "name2님 공연 많이 가보셨나요?",
          "original_form": "&name2&님 공연 많이 가보셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044056.1.2",
          "form": "저~ 음... 콘서트?",
          "original_form": "저~ 음... 콘서트?",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044056.1.3",
          "form": "갔었죠",
          "original_form": "갔었죠",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044056.1.4",
          "form": "덕질을 해서 ㅎ",
          "original_form": "덕질을 해서 ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044056.1.5",
          "form": "누구 콘서트 가셨나요?",
          "original_form": "누구 콘서트 가셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044056.1.6",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044056.1.7",
          "form": "누굴 말해야할까 고민돼요ㅋ",
          "original_form": "누굴 말해야할까 고민돼요ㅋ",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044056.1.8",
          "form": "콘서트를 정말 많이 가보셨군요?",
          "original_form": "콘서트를 정말 많이 가보셨군요?",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044056.1.9",
          "form": "예ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "예ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044056.1.10",
          "form": "그정도면",
          "original_form": "그정도면",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044056.1.11",
          "form": "많이 간편이죠",
          "original_form": "많이 간편이죠",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044056.1.12",
          "form": "해외도 가봤으니깐",
          "original_form": "해외도 가봤으니깐",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044056.1.13",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044056.1.14",
          "form": "해외콘서트도 가신건가요?",
          "original_form": "해외콘서트도 가신건가요?",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044056.1.15",
          "form": "네네 ㅎ 가봤어요 ㅎ 2-3번?",
          "original_form": "네네 ㅎ 가봤어요 ㅎ 2-3번?",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044056.1.16",
          "form": "해외는 입장료부터 비싸지 않나요?",
          "original_form": "해외는 입장료부터 비싸지 않나요?",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044056.1.17",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044056.1.18",
          "form": "일본은 심지어 랜덤이에요",
          "original_form": "일본은 심지어 랜덤이에요",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044056.1.19",
          "form": "좌석이 ㅎ",
          "original_form": "좌석이 ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044056.1.20",
          "form": "와 게다가 해외는 콘서트장이 엄청 크지 않나요?",
          "original_form": "와 게다가 해외는 콘서트장이 엄청 크지 않나요?",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044056.1.21",
          "form": "아뇨 ㅎ",
          "original_form": "아뇨 ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044056.1.22",
          "form": "어떤가수냐에 따라 달라요 ㅎ",
          "original_form": "어떤가수냐에 따라 달라요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044056.1.23",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210907 22:10"
        }
      ]
    }
