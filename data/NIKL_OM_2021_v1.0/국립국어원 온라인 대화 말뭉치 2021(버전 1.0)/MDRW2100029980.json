      "utterance": [
        {
          "id": "MDRW2100029980.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029980.1.2",
          "form": "무교이긴한데 엄마따라 교회 오래다녔어요!",
          "original_form": "무교이긴한데 엄마따라 교회 오래다녔어요!",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029980.1.3",
          "form": "ㅋㄴ님은요?",
          "original_form": "ㅋㄴ님은요?",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029980.1.4",
          "form": "기독교 모태였다가",
          "original_form": "기독교 모태였다가",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029980.1.5",
          "form": "탈출해서 무교에요",
          "original_form": "탈출해서 무교에요",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029980.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ... 저도 따지고보면 탈출해서 무교된거같애요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ... 저도 따지고보면 탈출해서 무교된거같애요",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029980.1.7",
          "form": "그런가요ㅋㅋ",
          "original_form": "그런가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100029980.1.8",
          "form": "무교를 택하신 이유가 있나요",
          "original_form": "무교를 택하신 이유가 있나요",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100029980.1.9",
          "form": "그냥 좀...꽉막힌거같고",
          "original_form": "그냥 좀...꽉막힌거같고",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029980.1.10",
          "form": "성격읽다보면 왜 이걸 이렇게 받아들이고 칭찬하는지 이해안되는것도있고....",
          "original_form": "성격읽다보면 왜 이걸 이렇게 받아들이고 칭찬하는지 이해안되는것도있고....",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029980.1.11",
          "form": "가치관같은게 달라서...??",
          "original_form": "가치관같은게 달라서...??",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029980.1.12",
          "form": "저두요... 요즘 시대랑 맞지도 않죠",
          "original_form": "저두요... 요즘 시대랑 맞지도 않죠",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029980.1.13",
          "form": "ㅜㅜ맞아요",
          "original_form": "ㅜㅜ맞아요",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029980.1.14",
          "form": "근데 저 요즘도 교회나가요...목사님한테 전화오는데 거절하기가 너무 ㅋㅋㅋ",
          "original_form": "근데 저 요즘도 교회나가요...목사님한테 전화오는데 거절하기가 너무 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029980.1.15",
          "form": "저는 코로나 시작됐을때",
          "original_form": "저는 코로나 시작됐을때",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029980.1.16",
          "form": "이때다 싶어서 다 차단했어요",
          "original_form": "이때다 싶어서 다 차단했어요",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029980.1.17",
          "form": "헐 그거 괜찮은거같애요",
          "original_form": "헐 그거 괜찮은거같애요",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029980.1.18",
          "form": "코로나걱정돼서ㅎㅎ안가겠습니다 핑계대기",
          "original_form": "코로나걱정돼서ㅎㅎ안가겠습니다 핑계대기",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029980.1.19",
          "form": "맞아요 종교모임 위험해요",
          "original_form": "맞아요 종교모임 위험해요",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100029980.1.20",
          "form": "꼭 써먹어보겠습니다",
          "original_form": "꼭 써먹어보겠습니다",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029980.1.21",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
