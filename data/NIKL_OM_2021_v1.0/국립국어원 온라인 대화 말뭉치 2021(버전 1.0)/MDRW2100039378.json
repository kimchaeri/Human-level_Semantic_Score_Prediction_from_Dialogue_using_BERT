      "utterance": [
        {
          "id": "MDRW2100039378.1.1",
          "form": "안녕하세요?\r\n오늘 점심은 뭐 드셨나요?",
          "original_form": "안녕하세요?\r\n오늘 점심은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039378.1.2",
          "form": "닭볶음탕이요",
          "original_form": "닭볶음탕이요",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039378.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039378.1.4",
          "form": "저는 김치찌개요",
          "original_form": "저는 김치찌개요",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039378.1.5",
          "form": "저녁은 뭐 드셨어요?",
          "original_form": "저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039378.1.6",
          "form": "저는 오랜만에 햄버거를 먹었습니다",
          "original_form": "저는 오랜만에 햄버거를 먹었습니다",
          "speaker_id": "1",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039378.1.7",
          "form": "저는 아마 볶음밥이요 ㅋㅋ",
          "original_form": "저는 아마 볶음밥이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039378.1.8",
          "form": "무슨 햄버거 좋아하세요?",
          "original_form": "무슨 햄버거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039378.1.9",
          "form": "저는 새우가 들어간 햄버거를 좋아하지만",
          "original_form": "저는 새우가 들어간 햄버거를 좋아하지만",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039378.1.10",
          "form": "오늘은 불고기를 먹었어요ㅋㅋ",
          "original_form": "오늘은 불고기를 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039378.1.11",
          "form": "아직 저녁 식사 전이시군요!",
          "original_form": "아직 저녁 식사 전이시군요!",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039378.1.12",
          "form": "볶음밥에 어떤 재료들이 들어갈 예정일까요ㅋㅋ",
          "original_form": "볶음밥에 어떤 재료들이 들어갈 예정일까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039378.1.13",
          "form": "새우랑 뭐.. 남는 채소들이요 ㅋㅋㅋ",
          "original_form": "새우랑 뭐.. 남는 채소들이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039378.1.14",
          "form": "새우볶음밥이네요!",
          "original_form": "새우볶음밥이네요!",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039378.1.15",
          "form": "볶음밥은 따로 좋아하는 볶음밥이 있으신가요?",
          "original_form": "볶음밥은 따로 좋아하는 볶음밥이 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039378.1.16",
          "form": "새우볶음밥이요 ㅋㅋㅋ",
          "original_form": "새우볶음밥이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039378.1.17",
          "form": "새우를 좋아합니다",
          "original_form": "새우를 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039378.1.18",
          "form": "계란볶음밥도 좋아요",
          "original_form": "계란볶음밥도 좋아요",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039378.1.19",
          "form": "새우가 어딜 넣어도 참 맛있는 거 같아요ㅎㅎ",
          "original_form": "새우가 어딜 넣어도 참 맛있는 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039378.1.20",
          "form": "진짜 그래여",
          "original_form": "진짜 그래여",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100039378.1.21",
          "form": "걍 구워도 맛있고",
          "original_form": "걍 구워도 맛있고",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100039378.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100039378.1.23",
          "form": "새벽인데 배고프네요",
          "original_form": "새벽인데 배고프네요",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100039378.1.24",
          "form": "아침 뭐드실거예요?",
          "original_form": "아침 뭐드실거예요?",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100039378.1.25",
          "form": "와 늦게까지 안 주무셨군요!",
          "original_form": "와 늦게까지 안 주무셨군요!",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100039378.1.26",
          "form": "저는 아침으로 야채죽을 먹었어요ㅎㅎ",
          "original_form": "저는 아침으로 야채죽을 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100039378.1.27",
          "form": "같이 사는 사람이 아파서ㅎㅎ",
          "original_form": "같이 사는 사람이 아파서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100039378.1.28",
          "form": "아침 뭐 드셨나요!",
          "original_form": "아침 뭐 드셨나요!",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100039378.1.29",
          "form": "저는 빵을 먹었네요ㅎㅎ",
          "original_form": "저는 빵을 먹었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100039378.1.30",
          "form": "식빵에 아몬드스프레드 조합 좋아해요~",
          "original_form": "식빵에 아몬드스프레드 조합 좋아해요~",
          "speaker_id": "2",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100039378.1.31",
          "form": "우와 아몬드 스프레드는 처음 들어보는데",
          "original_form": "우와 아몬드 스프레드는 처음 들어보는데",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100039378.1.32",
          "form": "맛있을 것 같아요!",
          "original_form": "맛있을 것 같아요!",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100039378.1.33",
          "form": "저는 오늘 점심으로는 카레를 만들어먹었어요",
          "original_form": "저는 오늘 점심으로는 카레를 만들어먹었어요",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100039378.1.34",
          "form": "맛있어오 ㅋㅋ",
          "original_form": "맛있어오 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100039378.1.35",
          "form": "오 카레 맛있었겠네요",
          "original_form": "오 카레 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100039378.1.36",
          "form": "저는 백세카레 약간 매콤한 맛을 젤 좋아해요",
          "original_form": "저는 백세카레 약간 매콤한 맛을 젤 좋아해요",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100039378.1.37",
          "form": "저도 순한맛 보다는 매콤한 맛이 좋더라구요!",
          "original_form": "저도 순한맛 보다는 매콤한 맛이 좋더라구요!",
