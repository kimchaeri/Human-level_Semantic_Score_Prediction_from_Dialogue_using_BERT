      "utterance": [
        {
          "id": "MDRW2100020737.1.1",
          "form": "이마트트레이더스 뭐사러가세여",
          "original_form": "이마트트레이더스 뭐사러가세여",
          "speaker_id": "1",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020737.1.2",
          "form": "일단 먹고 한바퀴 할려구여",
          "original_form": "일단 먹고 한바퀴 할려구여",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020737.1.3",
          "form": "오 뭐드실건가요",
          "original_form": "오 뭐드실건가요",
          "speaker_id": "1",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020737.1.4",
          "form": "일단 피자먹을려구요",
          "original_form": "일단 피자먹을려구요",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100020737.1.5",
          "form": "요즘 위시템 잇으신가요",
          "original_form": "요즘 위시템 잇으신가요",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100020737.1.6",
          "form": "레인부츠용ㅋㅋ",
          "original_form": "레인부츠용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020737.1.7",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100020737.1.8",
          "form": "장마다지난거아닌가욥 ㅎㅎ",
          "original_form": "장마다지난거아닌가욥 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100020737.1.9",
          "form": "혹시나해서요 ㅋㅋ",
          "original_form": "혹시나해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100020737.1.10",
          "form": "다움주도 내내 비던데ㅠ",
          "original_form": "다움주도 내내 비던데ㅠ",
          "speaker_id": "1",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100020737.1.11",
          "form": "아 가을장마가 남앗긴하죠ㅎㅎ",
          "original_form": "아 가을장마가 남앗긴하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100020737.1.12",
          "form": "태풍도 남은거같구",
          "original_form": "태풍도 남은거같구",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100020737.1.13",
          "form": "저는 넘나 사고싶지만 호빗족이라서",
          "original_form": "저는 넘나 사고싶지만 호빗족이라서",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100020737.1.14",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100020737.1.15",
          "form": "레인부츠가 허벅지덮을꺼같아욤",
          "original_form": "레인부츠가 허벅지덮을꺼같아욤",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100020737.1.16",
          "form": "저도 안벗겨질거같아서 미디움 사려구용",
          "original_form": "저도 안벗겨질거같아서 미디움 사려구용",
          "speaker_id": "1",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100020737.1.17",
          "form": "헌터부츠 이쁘더라구욤",
          "original_form": "헌터부츠 이쁘더라구욤",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100020737.1.18",
          "form": "고민하는 시간이 아까우니 언능 지르세욥",
          "original_form": "고민하는 시간이 아까우니 언능 지르세욥",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100020737.1.19",
          "form": "마자욥 보고 싸게싸게 주문",
          "original_form": "마자욥 보고 싸게싸게 주문",
          "speaker_id": "1",
          "time": "20210828 22:16"
        }
      ]
