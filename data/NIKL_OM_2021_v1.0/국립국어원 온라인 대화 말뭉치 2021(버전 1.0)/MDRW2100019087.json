      "utterance": [
        {
          "id": "MDRW2100019087.1.1",
          "form": "오늘 국제수사봤어요",
          "original_form": "오늘 국제수사봤어요",
          "speaker_id": "2",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019087.1.2",
          "form": "어땠나요",
          "original_form": "어땠나요",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019087.1.3",
          "form": "곽도원배우님 영화는 다 재밌다고 생각해요",
          "original_form": "곽도원배우님 영화는 다 재밌다고 생각해요",
          "speaker_id": "2",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019087.1.4",
          "form": "곽배우님 좋아하시구나",
          "original_form": "곽배우님 좋아하시구나",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019087.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019087.1.6",
          "form": "전 펜트하우스 좋아해서 열시가 되길 기다리는 중이에요",
          "original_form": "전 펜트하우스 좋아해서 열시가 되길 기다리는 중이에요",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019087.1.7",
          "form": "펜트하우스 재밌죠ㅜㅜ",
          "original_form": "펜트하우스 재밌죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019087.1.8",
          "form": "오늘은 또 어떤 전개일지 너무 궁금해요",
          "original_form": "오늘은 또 어떤 전개일지 너무 궁금해요",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019087.1.9",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019087.1.10",
          "form": "곧 끝나니까 허전하겠어요",
          "original_form": "곧 끝나니까 허전하겠어요",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019087.1.11",
          "form": "또 찾아나서야죠ㅋㅋ",
          "original_form": "또 찾아나서야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019087.1.12",
          "form": "김새론 배우나오는 드라마나 김유정님 나오는 것두 재밌어보이던데요",
          "original_form": "김새론 배우나오는 드라마나 김유정님 나오는 것두 재밌어보이던데요",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019087.1.13",
          "form": "저도 예고봤어요",
          "original_form": "저도 예고봤어요",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019087.1.14",
          "form": "김유정님 눈은 진짜... 사극이랑너무 잘 어울려요",
          "original_form": "김유정님 눈은 진짜... 사극이랑너무 잘 어울려요",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019087.1.15",
          "form": "현대극보다 퓨전사극에서 빛발하는 느낌이 강하죠",
          "original_form": "현대극보다 퓨전사극에서 빛발하는 느낌이 강하죠",
          "speaker_id": "2",
          "time": "20210827 15:15"
        }
