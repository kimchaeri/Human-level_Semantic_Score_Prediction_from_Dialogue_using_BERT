      "utterance": [
        {
          "id": "MDRW2100001473.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.2",
          "form": "하세요",
          "original_form": "하세요",
          "speaker_id": "1",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.4",
          "form": "혹시 민초단이신가요!",
          "original_form": "혹시 민초단이신가요!",
          "speaker_id": "2",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.5",
          "form": "싫어합니다",
          "original_form": "싫어합니다",
          "speaker_id": "1",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.6",
          "form": "헉 반민초단이시구나..",
          "original_form": "헉 반민초단이시구나..",
          "speaker_id": "2",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.7",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.8",
          "form": "민초단으로써 궁금한건데",
          "original_form": "민초단으로써 궁금한건데",
          "speaker_id": "2",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.9",
          "form": "정말..",
          "original_form": "정말..",
          "speaker_id": "2",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001473.1.10",
          "form": "치약맛이라고 느껴지시나요",
          "original_form": "치약맛이라고 느껴지시나요",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.11",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.12",
          "form": "그정도까지는 아닙니다",
          "original_form": "그정도까지는 아닙니다",
          "speaker_id": "1",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.13",
          "form": "단지 맛이없어서 안먹을뿐입니다",
          "original_form": "단지 맛이없어서 안먹을뿐입니다",
          "speaker_id": "1",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.14",
          "form": "뭔가 더 상처다..",
          "original_form": "뭔가 더 상처다..",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.15",
          "form": "그렇다면",
          "original_form": "그렇다면",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.16",
          "form": "파인애플피자..",
          "original_form": "파인애플피자..",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.17",
          "form": "용납 가능하신가요",
          "original_form": "용납 가능하신가요",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.18",
          "form": "전 정말 불가능",
          "original_form": "전 정말 불가능",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.19",
          "form": "따뜻한 파인애플이 말이 됩니까 증말",
          "original_form": "따뜻한 파인애플이 말이 됩니까 증말",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.20",
          "form": "치즈들어간 음식 자체를 안먹습니다",
          "original_form": "치즈들어간 음식 자체를 안먹습니다",
          "speaker_id": "1",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.21",
          "form": "헐.......",
          "original_form": "헐.......",
          "speaker_id": "2",
          "time": "20211015 11:05"
        },
        {
          "id": "MDRW2100001473.1.22",
          "form": "그러시구나...!",
          "original_form": "그러시구나...!",
          "speaker_id": "2",
          "time": "20211015 11:06"
        },
        {
          "id": "MDRW2100001473.1.23",
          "form": "알러지나서요",
          "original_form": "알러지나서요",
          "speaker_id": "1",
          "time": "20211015 11:06"
        },
        {
          "id": "MDRW2100001473.1.24",
          "form": "어어 ㅠㅠㅠ 알러지.... 힘드시겠어요 ㅠㅠ",
          "original_form": "어어 ㅠㅠㅠ 알러지.... 힘드시겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211015 11:06"
        },
        {
          "id": "MDRW2100001473.1.25",
          "form": "심한건 아닌데",
          "original_form": "심한건 아닌데",
          "speaker_id": "1",
          "time": "20211015 11:07"
        },
        {
          "id": "MDRW2100001473.1.26",
          "form": "일단 치즈향을 아주 싫어해서요",
          "original_form": "일단 치즈향을 아주 싫어해서요",
          "speaker_id": "1",
          "time": "20211015 11:07"
        },
        {
          "id": "MDRW2100001473.1.27",
          "form": "아하 약간 역하게 느껴질 순 있을 거 같아요..!",
          "original_form": "아하 약간 역하게 느껴질 순 있을 거 같아요..!",
          "speaker_id": "2",
          "time": "20211015 11:07"
        }
      ]
    }
