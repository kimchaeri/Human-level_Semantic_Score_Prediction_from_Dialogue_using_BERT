      "utterance": [
        {
          "id": "MDRW2100003241.1.1",
          "form": "오늘 재밌는거 모하나요?",
          "original_form": "오늘 재밌는거 모하나요?",
          "speaker_id": "1",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003241.1.2",
          "form": "지금 식스센스보고있어요",
          "original_form": "지금 식스센스보고있어요",
          "speaker_id": "2",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003241.1.3",
          "form": "볼께 없네요",
          "original_form": "볼께 없네요",
          "speaker_id": "2",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003241.1.4",
          "form": "그거 제작비엄청들어 가겠던데요",
          "original_form": "그거 제작비엄청들어 가겠던데요",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003241.1.5",
          "form": "가게하나를 만드는거니까 제작비는 엄청들어갈꺼같아요",
          "original_form": "가게하나를 만드는거니까 제작비는 엄청들어갈꺼같아요",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003241.1.6",
          "form": "대탈출도 어마어마할듯해요",
          "original_form": "대탈출도 어마어마할듯해요",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003241.1.7",
          "form": "티비엔이죠? 둘다",
          "original_form": "티비엔이죠? 둘다",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003241.1.8",
          "form": "그럴꺼에요.  돈이 많은가봐요",
          "original_form": "그럴꺼에요.  돈이 많은가봐요",
          "speaker_id": "2",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003241.1.9",
          "form": "티비엔이 돈많은듯요 나영석 피디 하는것만 봐도 지원많이해주나봐요",
          "original_form": "티비엔이 돈많은듯요 나영석 피디 하는것만 봐도 지원많이해주나봐요",
          "speaker_id": "1",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003241.1.10",
          "form": "그러게요.",
          "original_form": "그러게요.",
          "speaker_id": "2",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003241.1.11",
          "form": "지금 내가 키운다보고있는데",
          "original_form": "지금 내가 키운다보고있는데",
          "speaker_id": "1",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003241.1.12",
          "form": "김현숙도 이혼했더라구요",
          "original_form": "김현숙도 이혼했더라구요",
          "speaker_id": "1",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003241.1.13",
          "form": "잘사는줄 알았는데 작년인가 한듯해요",
          "original_form": "잘사는줄 알았는데 작년인가 한듯해요",
          "speaker_id": "2",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003241.1.14",
          "form": "이동건이랑 조윤희는 급하게 결혼한느낌이라 아~~그랬구요",
          "original_form": "이동건이랑 조윤희는 급하게 결혼한느낌이라 아~~그랬구요",
          "speaker_id": "2",
          "time": "20210820 21:16"
        },
        {
          "id": "MDRW2100003241.1.15",
          "form": "넘 빨리 이혼하긴했죠 애기가 이국적으로 생겼던데",
          "original_form": "넘 빨리 이혼하긴했죠 애기가 이국적으로 생겼던데",
          "speaker_id": "1",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003241.1.16",
          "form": "둘다이쁘니 커갈수록 더 이뻐질듯해요",
          "original_form": "둘다이쁘니 커갈수록 더 이뻐질듯해요",
          "speaker_id": "2",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003241.1.17",
          "form": "조윤희닮은거같은데 다시보면 이동건얼굴도 보이고ㅎ",
          "original_form": "조윤희닮은거같은데 다시보면 이동건얼굴도 보이고ㅎ",
          "speaker_id": "1",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003241.1.18",
          "form": "ㅎㅎ 부모니까 둘다 닮았겠죠모^^",
          "original_form": "ㅎㅎ 부모니까 둘다 닮았겠죠모^^",
