      "utterance": [
        {
          "id": "MDRW2100031696.1.1",
          "form": "오늘은 어디계시나요",
          "original_form": "오늘은 어디계시나요",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031696.1.2",
          "form": "날이 선선하네요",
          "original_form": "날이 선선하네요",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031696.1.3",
          "form": "원룸에요",
          "original_form": "원룸에요",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031696.1.4",
          "form": "시원하네요",
          "original_form": "시원하네요",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.5",
          "form": "거기는 날씨가 어떤가요",
          "original_form": "거기는 날씨가 어떤가요",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.6",
          "form": "시원하고 좋습니다",
          "original_form": "시원하고 좋습니다",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.7",
          "form": "에어컨틀고있었는데 괜히 틀었네요",
          "original_form": "에어컨틀고있었는데 괜히 틀었네요",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.10",
          "form": "밖이 더 추웠어요 ㅋㅋ",
          "original_form": "밖이 더 추웠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.11",
          "form": "괜히 전기세 나갔네요",
          "original_form": "괜히 전기세 나갔네요",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.12",
          "form": "약간 춥기까지 한데 애어컨이라뇨",
          "original_form": "약간 춥기까지 한데 애어컨이라뇨",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.13",
          "form": "ㅋㅋㅋ저흰 에어컨 매일 틀거든요",
          "original_form": "ㅋㅋㅋ저흰 에어컨 매일 틀거든요",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.14",
          "form": "문 안열고 살아서",
          "original_form": "문 안열고 살아서",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.15",
          "form": "창문 열면 이제 가을이에요",
          "original_form": "창문 열면 이제 가을이에요",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031696.1.16",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031696.1.17",
          "form": "여긴 낮에 더웠는데",
          "original_form": "여긴 낮에 더웠는데",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031696.1.18",
          "form": "환기 자주 시키세요",
          "original_form": "환기 자주 시키세요",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031696.1.19",
          "form": "공기청정기 켰거든욬ㅋㅋㅋ",
          "original_form": "공기청정기 켰거든욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031696.1.20",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031696.1.21",
          "form": "전자제품에서 환경호르몬 나온대요",
          "original_form": "전자제품에서 환경호르몬 나온대요",
          "speaker_id": "2",
          "time": "20210904 23:37"
