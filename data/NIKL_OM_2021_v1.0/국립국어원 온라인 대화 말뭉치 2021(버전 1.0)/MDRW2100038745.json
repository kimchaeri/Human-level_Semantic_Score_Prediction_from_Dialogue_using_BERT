      "utterance": [
        {
          "id": "MDRW2100038745.1.1",
          "form": "책말고 유투브 쪽은 안보시나요?",
          "original_form": "책말고 유투브 쪽은 안보시나요?",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038745.1.2",
          "form": "유튜브 뭐보세요",
          "original_form": "유튜브 뭐보세요",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038745.1.3",
          "form": "많이보죠",
          "original_form": "많이보죠",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038745.1.4",
          "form": "저는 고양이 게임 여행 먹방 브이로그 등등이요",
          "original_form": "저는 고양이 게임 여행 먹방 브이로그 등등이요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038745.1.5",
          "form": "그쪽은요?!",
          "original_form": "그쪽은요?!",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038745.1.6",
          "form": "오오 많이보시네요",
          "original_form": "오오 많이보시네요",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038745.1.7",
          "form": "저는 아이돌 무대만 주로 봐요 뮤비랑",
          "original_form": "저는 아이돌 무대만 주로 봐요 뮤비랑",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038745.1.8",
          "form": "오 춤을 좋아하시는건가요?",
          "original_form": "오 춤을 좋아하시는건가요?",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038745.1.9",
          "form": "네네 춤 따라추는거 좋아해요",
          "original_form": "네네 춤 따라추는거 좋아해요",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038745.1.10",
          "form": "저는 저스트댄스라고 춤 게임 아니면 안춰봣어요 ㅋㅋ",
          "original_form": "저는 저스트댄스라고 춤 게임 아니면 안춰봣어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038745.1.11",
          "form": "아 그거 한참했어요",
          "original_form": "아 그거 한참했어요",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038745.1.12",
          "form": "오 해보셨군요 스위치로 하셨을까요?",
          "original_form": "오 해보셨군요 스위치로 하셨을까요?",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038745.1.13",
          "form": "아 친구거로 해보고 저는 폰으류",
          "original_form": "아 친구거로 해보고 저는 폰으류",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038745.1.14",
          "form": "은근 운동돼요 ㅎㅎ",
          "original_form": "은근 운동돼요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038745.1.15",
          "form": "진짜 땀 많이나요",
          "original_form": "진짜 땀 많이나요",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038745.1.16",
          "form": "저는 티비에 축구하길래 보고있어요",
          "original_form": "저는 티비에 축구하길래 보고있어요",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038745.1.17",
          "form": "저도 축구봐용 ㅋㅋ",
          "original_form": "저도 축구봐용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100038745.1.18",
          "form": "손흥민 맞나요 보니까 반갑네요 ㅎㅎ 간만인",
          "original_form": "손흥민 맞나요 보니까 반갑네요 ㅎㅎ 간만인",
