      "utterance": [
        {
          "id": "MDRW2100021485.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021485.1.2",
          "form": "아니용ㅠㅠ 하지만 늘 키우고싶어서 유튜브로 랜선 이모 자주해요ㅠㅠ",
          "original_form": "아니용ㅠㅠ 하지만 늘 키우고싶어서 유튜브로 랜선 이모 자주해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021485.1.3",
          "form": "ㅋㅋㅋㅋㅋ요즘은 인스타나 유튜브로 반려동물 계정 많더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ요즘은 인스타나 유튜브로 반려동물 계정 많더라구요",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021485.1.4",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021485.1.5",
          "form": "오복이 아시나요?",
          "original_form": "오복이 아시나요?",
          "speaker_id": "2",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021485.1.6",
          "form": "말티즈인데",
          "original_form": "말티즈인데",
          "speaker_id": "2",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021485.1.7",
          "form": "영상도 짧아서 쉽게쉽게 넘길수있고",
          "original_form": "영상도 짧아서 쉽게쉽게 넘길수있고",
          "speaker_id": "2",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021485.1.8",
          "form": "무엇보다 승질이 대박이에요ㅋㅋㅋ",
          "original_form": "무엇보다 승질이 대박이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021485.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아녀! 제가 보는 건 아닌데 친구가 봐서 몇 번 얘기 들은 적은 있어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아녀! 제가 보는 건 아닌데 친구가 봐서 몇 번 얘기 들은 적은 있어요",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100021485.1.10",
          "form": "저는 고양이 채널 가끔 봐요",
          "original_form": "저는 고양이 채널 가끔 봐요",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100021485.1.11",
          "form": "메탈남님 영상도 보시나용?",
          "original_form": "메탈남님 영상도 보시나용?",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100021485.1.12",
          "form": "헐 네넹",
          "original_form": "헐 네넹",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021485.1.13",
          "form": "메탈남님꺼랑 haha.ha 봐요 ㅋㅋ",
          "original_form": "메탈남님꺼랑 haha.ha 봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021485.1.14",
          "form": "유명하신분들 다 보시는군요!ㅋㅋ",
          "original_form": "유명하신분들 다 보시는군요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021485.1.15",
          "form": "오리도 좋아하시면",
          "original_form": "오리도 좋아하시면",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021485.1.16",
          "form": "도시오리 이분도 추천드려요",
          "original_form": "도시오리 이분도 추천드려요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021485.1.17",
          "form": "어 알아여",
          "original_form": "어 알아여",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021485.1.18",
          "form": "ㅋㅋㅋㅋ가끔 추천에 떠서 봐요",
          "original_form": "ㅋㅋㅋㅋ가끔 추천에 떠서 봐요",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.19",
          "form": "아 보시는구나!",
          "original_form": "아 보시는구나!",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.20",
          "form": "전 오리를 너무너무 좋아해서",
          "original_form": "전 오리를 너무너무 좋아해서",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.21",
          "form": "오리 잘 몰랐는데 넘 보드라와 보이고 귀욥더라구요",
          "original_form": "오리 잘 몰랐는데 넘 보드라와 보이고 귀욥더라구요",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.22",
          "form": "이분 영상 매일봐요ㅋㅋㅋ",
          "original_form": "이분 영상 매일봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.23",
          "form": "맞아요ㅠㅠㅠ 너무 귀여워요...",
          "original_form": "맞아요ㅠㅠㅠ 너무 귀여워요...",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.24",
          "form": "오리 귀엽죠",
          "original_form": "오리 귀엽죠",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.25",
          "form": "이 오리 종 더 보고싶으시면 유튜브에",
          "original_form": "이 오리 종 더 보고싶으시면 유튜브에",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.26",
          "form": "걔네도 주인? 알아보는거 신기하더라고요",
          "original_form": "걔네도 주인? 알아보는거 신기하더라고요",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021485.1.27",
          "form": "call duck 이라고 검색하시면 더 많이 나와요",
          "original_form": "call duck 이라고 검색하시면 더 많이 나와요",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100021485.1.28",
          "form": "네네 강아지랑 똑같아요ㅋㅋㅋ",
          "original_form": "네네 강아지랑 똑같아요ㅋㅋㅋ",
