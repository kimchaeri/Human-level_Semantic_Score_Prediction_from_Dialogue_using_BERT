      "utterance": [
        {
          "id": "MDRW2100003540.1.1",
          "form": "요즘 날씨가 참 변화가 심해",
          "original_form": "요즘 날씨가 참 변화가 심해",
          "speaker_id": "2",
          "time": "20210821 00:57"
        },
        {
          "id": "MDRW2100003540.1.2",
          "form": "맞아 더운듯 추운듯",
          "original_form": "맞아 더운듯 추운듯",
          "speaker_id": "1",
          "time": "20210821 00:58"
        },
        {
          "id": "MDRW2100003540.1.3",
          "form": "별로야 정말루",
          "original_form": "별로야 정말루",
          "speaker_id": "1",
          "time": "20210821 00:58"
        },
        {
          "id": "MDRW2100003540.1.4",
          "form": "옷을 어떻게 입어야 할 지 모르겠어",
          "original_form": "옷을 어떻게 입어야 할 지 모르겠어",
          "speaker_id": "2",
          "time": "20210821 01:01"
        },
        {
          "id": "MDRW2100003540.1.5",
          "form": "반팔에 가디건이 적당할듯",
          "original_form": "반팔에 가디건이 적당할듯",
          "speaker_id": "1",
          "time": "20210821 01:01"
        },
        {
          "id": "MDRW2100003540.1.6",
          "form": "나도 요새 반팔에 뭘 걸쳐 ㅎㅎ",
          "original_form": "나도 요새 반팔에 뭘 걸쳐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 01:01"
        },
        {
          "id": "MDRW2100003540.1.7",
          "form": "추위 많이 타?",
          "original_form": "추위 많이 타?",
          "speaker_id": "1",
          "time": "20210821 01:02"
        },
        {
          "id": "MDRW2100003540.1.8",
          "form": "추위 꽤 타는 편이야",
          "original_form": "추위 꽤 타는 편이야",
          "speaker_id": "2",
          "time": "20210821 01:02"
        },
        {
          "id": "MDRW2100003540.1.9",
          "form": "겨울에 롱패딩 필수 ㅠㅠ",
          "original_form": "겨울에 롱패딩 필수 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 01:02"
        },
        {
          "id": "MDRW2100003540.1.10",
          "form": "그러면 긴팔 가디건 입자",
          "original_form": "그러면 긴팔 가디건 입자",
          "speaker_id": "1",
          "time": "20210821 01:02"
        },
        {
          "id": "MDRW2100003540.1.11",
          "form": "평창 갔는데 거기는 가을이더라 ㅠㅠㅠ",
          "original_form": "평창 갔는데 거기는 가을이더라 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 01:02"
        },
        {
          "id": "MDRW2100003540.1.12",
          "form": "우와 날씨차이 심하네",
          "original_form": "우와 날씨차이 심하네",
          "speaker_id": "2",
          "time": "20210821 01:02"
        },
        {
          "id": "MDRW2100003540.1.13",
          "form": "부산은 여름이었다 가을이었다 하는거 같아",
          "original_form": "부산은 여름이었다 가을이었다 하는거 같아",
          "speaker_id": "2",
          "time": "20210821 01:03"
        },
        {
          "id": "MDRW2100003540.1.14",
          "form": "맞지 나도 신기했어 ㅎㅎ",
          "original_form": "맞지 나도 신기했어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 01:03"
        },
        {
          "id": "MDRW2100003540.1.15",
          "form": "부산은 좀 덥더라",
          "original_form": "부산은 좀 덥더라",
          "speaker_id": "1",
          "time": "20210821 01:03"
        },
        {
          "id": "MDRW2100003540.1.16",
          "form": "부산 날씨는 오락가락하네",
          "original_form": "부산 날씨는 오락가락하네",
          "speaker_id": "2",
          "time": "20210821 01:03"
        },
        {
          "id": "MDRW2100003540.1.17",
          "form": "아열대 기후는 다 그런가봥",
          "original_form": "아열대 기후는 다 그런가봥",
          "speaker_id": "1",
          "time": "20210821 01:04"
        },
        {
          "id": "MDRW2100003540.1.18",
          "form": "이것도 온난화때문인가? ㅋㅋ",
          "original_form": "이것도 온난화때문인가? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 01:04"
        },
        {
          "id": "MDRW2100003540.1.19",
          "form": "그렇구만 똑똑해!",
          "original_form": "그렇구만 똑똑해!",
          "speaker_id": "2",
          "time": "20210821 01:04"
        },
        {
          "id": "MDRW2100003540.1.20",
          "form": "평창은 고산지대라서  ㅋㅋㅋ",
          "original_form": "평창은 고산지대라서  ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 01:04"
        },
        {
          "id": "MDRW2100003540.1.21",
          "form": "우앙 고마웡",
          "original_form": "우앙 고마웡",
          "speaker_id": "1",
          "time": "20210821 01:04"
        },
        {
          "id": "MDRW2100003540.1.22",
          "form": "아니 진짜 똑똑하네",
          "original_form": "아니 진짜 똑똑하네",
          "speaker_id": "2",
          "time": "20210821 01:04"
        },
        {
          "id": "MDRW2100003540.1.23",
          "form": "비가 쏟아져",
          "original_form": "비가 쏟아져",
          "speaker_id": "2",
          "time": "20210824 01:20"
        }
      ]
