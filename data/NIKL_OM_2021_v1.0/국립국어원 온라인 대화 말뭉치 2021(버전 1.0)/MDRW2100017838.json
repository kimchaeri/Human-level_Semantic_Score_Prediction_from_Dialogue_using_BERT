      "utterance": [
        {
          "id": "MDRW2100017838.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100017838.1.2",
          "form": "네 정말 좋아합니다 최근엔 못갔어요ㅠ",
          "original_form": "네 정말 좋아합니다 최근엔 못갔어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100017838.1.3",
          "form": "멀리멀리 해외가고싶어요",
          "original_form": "멀리멀리 해외가고싶어요",
          "speaker_id": "2",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100017838.1.4",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100017838.1.5",
          "form": "가고싶은곳 잇으세요?",
          "original_form": "가고싶은곳 잇으세요?",
          "speaker_id": "2",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100017838.1.6",
          "form": "저는 남자친구가 해외유학중이라",
          "original_form": "저는 남자친구가 해외유학중이라",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100017838.1.7",
          "form": "거기로 가고싶네요ㅠㅠ 북유럽이요",
          "original_form": "거기로 가고싶네요ㅠㅠ 북유럽이요",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100017838.1.8",
          "form": "남친 해외유학중이라하셧엇는데",
          "original_form": "남친 해외유학중이라하셧엇는데",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100017838.1.9",
          "form": "북유럽이엿다니!!!!!!",
          "original_form": "북유럽이엿다니!!!!!!",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100017838.1.10",
          "form": "아ㅜㅜ부러워요 저도 여행 꼭 가보고싶어요ㅠㅠ",
          "original_form": "아ㅜㅜ부러워요 저도 여행 꼭 가보고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100017838.1.11",
          "form": "북유럽ㅠㅠ",
          "original_form": "북유럽ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100017838.1.12",
          "form": "너무멀죠",
          "original_form": "너무멀죠",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100017838.1.13",
          "form": "북유럽 어디에 잇나용",
          "original_form": "북유럽 어디에 잇나용",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100017838.1.14",
          "form": "노르웨이?",
          "original_form": "노르웨이?",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100017838.1.15",
          "form": "네 맞아요 바로 맞히셨네요",
          "original_form": "네 맞아요 바로 맞히셨네요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017838.1.16",
          "form": "아 가보고싶네요ㅜ",
          "original_form": "아 가보고싶네요ㅜ",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100017838.1.17",
          "form": "정말가고시 어요",
          "original_form": "정말가고시 어요",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100017838.1.18",
          "form": "노르웨이 좋다던가용?",
          "original_form": "노르웨이 좋다던가용?",
          "speaker_id": "2",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100017838.1.19",
          "form": "네 좋아보여요 사람도 많이없고",
          "original_form": "네 좋아보여요 사람도 많이없고",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100017838.1.20",
          "form": "부럽네요",
          "original_form": "부럽네요",
