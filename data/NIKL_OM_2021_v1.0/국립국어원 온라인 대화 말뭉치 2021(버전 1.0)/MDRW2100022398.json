      "utterance": [
        {
          "id": "MDRW2100022398.1.1",
          "form": "아니요 9우러에 맞을 예정이에요",
          "original_form": "아니요 9우러에 맞을 예정이에요",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022398.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022398.1.3",
          "form": "저도 사전예약해놨어요 9월!",
          "original_form": "저도 사전예약해놨어요 9월!",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022398.1.4",
          "form": "추석 전주에 맞아요 님은 언제쯤인가요?",
          "original_form": "추석 전주에 맞아요 님은 언제쯤인가요?",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022398.1.5",
          "form": "저는 9,9일이요!",
          "original_form": "저는 9,9일이요!",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022398.1.6",
          "form": "아하 전 13일이요 비슷한 날자네요",
          "original_form": "아하 전 13일이요 비슷한 날자네요",
          "speaker_id": "2",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022398.1.7",
          "form": "추석전에 맞아야 안심하고 고향에 갈 수 있을 것 같아서 전으로 신청했어요",
          "original_form": "추석전에 맞아야 안심하고 고향에 갈 수 있을 것 같아서 전으로 신청했어요",
          "speaker_id": "2",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022398.1.8",
          "form": "화이자? 모더나? 아직 안정해졌죠",
          "original_form": "화이자? 모더나? 아직 안정해졌죠",
          "speaker_id": "1",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100022398.1.9",
          "form": "네 화이자 또는 모더나라고만 떠요",
          "original_form": "네 화이자 또는 모더나라고만 떠요",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100022398.1.10",
          "form": "님도 그렇죠? 물량이 아직 확보가 안됐나봐요",
          "original_form": "님도 그렇죠? 물량이 아직 확보가 안됐나봐요",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100022398.1.11",
          "form": "조금걱정도 되네요ㅜ",
          "original_form": "조금걱정도 되네요ㅜ",
          "speaker_id": "1",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100022398.1.12",
          "form": "뭐가 되었건 추석전에 맞고 부작용이 없음 좋겠어요",
          "original_form": "뭐가 되었건 추석전에 맞고 부작용이 없음 좋겠어요",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100022398.1.13",
          "form": "마자요 부작용무서뭐요ㅜ",
          "original_form": "마자요 부작용무서뭐요ㅜ",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022398.1.14",
          "form": "부작용도 무섭고 그걸 증명하기가 또 너무 힘드니까요",
          "original_form": "부작용도 무섭고 그걸 증명하기가 또 너무 힘드니까요",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022398.1.15",
          "form": "마자요 보상받기도 엄청 힘들죠",
          "original_form": "마자요 보상받기도 엄청 힘들죠",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022398.1.16",
          "form": "만약 죽으면 가족들이 몇달 투쟁해야 한두건 인정받고",
          "original_form": "만약 죽으면 가족들이 몇달 투쟁해야 한두건 인정받고",
          "speaker_id": "2",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022398.1.17",
          "form": "살아있어도 부작용으로 아프면 본인은 나라랑은 못 싸우죠 결국 가족 몫이네요",
          "original_form": "살아있어도 부작용으로 아프면 본인은 나라랑은 못 싸우죠 결국 가족 몫이네요",
          "speaker_id": "2",
          "time": "20210829 13:53"
        }
      ]
    }
