      "utterance": [
        {
          "id": "MDRW2100023828.1.1",
          "form": "저녁은 뭐드시나요~?",
          "original_form": "저녁은 뭐드시나요~?",
          "speaker_id": "2",
          "time": "20210830 18:23"
        },
        {
          "id": "MDRW2100023828.1.2",
          "form": "운동하고 닭고기랑 콘칩이랑 콜라랑.. 카라멜콘과땅콩 먹을 예정이에요..ㅋㅋㅋ",
          "original_form": "운동하고 닭고기랑 콘칩이랑 콜라랑.. 카라멜콘과땅콩 먹을 예정이에요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:24"
        },
        {
          "id": "MDRW2100023828.1.3",
          "form": "운동하고 ㅎㅎㅎ 전 운동안하고 먹네요ㅠㅠ",
          "original_form": "운동하고 ㅎㅎㅎ 전 운동안하고 먹네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023828.1.4",
          "form": "그러다가 죽어요..ㅋㅋㅋㅋ",
          "original_form": "그러다가 죽어요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023828.1.5",
          "form": "조금이라도 움직이세요.. ㅋㅋㅋ",
          "original_form": "조금이라도 움직이세요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023828.1.6",
          "form": "ㅠㅠ 너무싫으네요 ㅠ 알면서도 ㅋㅋ",
          "original_form": "ㅠㅠ 너무싫으네요 ㅠ 알면서도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023828.1.7",
          "form": "저녁은 뭐 드실렵니까?",
          "original_form": "저녁은 뭐 드실렵니까?",
          "speaker_id": "1",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100023828.1.8",
          "form": "닭탕포장해가려구 주문했어요 ㅋㅋ",
          "original_form": "닭탕포장해가려구 주문했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100023828.1.9",
          "form": "오.. 고단백질이네요..",
          "original_form": "오.. 고단백질이네요..",
          "speaker_id": "1",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023828.1.10",
          "form": "반주도 하시나요?",
          "original_form": "반주도 하시나요?",
          "speaker_id": "1",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023828.1.11",
          "form": "얼큰한게 땡기네요",
          "original_form": "얼큰한게 땡기네요",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100023828.1.12",
          "form": "마시면 청하 반병이나 아님 맥주 한캔? 정도요!",
          "original_form": "마시면 청하 반병이나 아님 맥주 한캔? 정도요!",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023828.1.13",
          "form": "평일엔 안마시려 자제해요ㅠㅠㅋㅋㅋ",
          "original_form": "평일엔 안마시려 자제해요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023828.1.14",
          "form": "비오는날 왠지 칼칼한거 땡기죠...",
          "original_form": "비오는날 왠지 칼칼한거 땡기죠...",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023828.1.15",
          "form": "처음시켜봤는데 맛있길 ㅠㅠ",
          "original_form": "처음시켜봤는데 맛있길 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023828.1.16",
          "form": "1인분씩도 팔아요?",
          "original_form": "1인분씩도 팔아요?",
          "speaker_id": "1",
          "time": "20210830 19:02"
        },
        {
          "id": "MDRW2100023828.1.17",
          "form": "소자로시켯어요!",
          "original_form": "소자로시켯어요!",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023828.1.18",
          "form": "왜 제가 다 기대될까여? ㅋㅋㅋㅋ",
          "original_form": "왜 제가 다 기대될까여? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100023828.1.19",
          "form": "배고파서 긍가..",
          "original_form": "배고파서 긍가..",
