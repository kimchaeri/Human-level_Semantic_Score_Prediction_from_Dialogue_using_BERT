      "utterance": [
        {
          "id": "MDRW2100036260.1.1",
          "form": "아침드셨나용?",
          "original_form": "아침드셨나용?",
          "speaker_id": "1",
          "time": "20210831 06:51"
        },
        {
          "id": "MDRW2100036260.1.2",
          "form": "계란 두알을 아침으러 매일 먹어용!!",
          "original_form": "계란 두알을 아침으러 매일 먹어용!!",
          "speaker_id": "2",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100036260.1.3",
          "form": "오오 건강식이네용",
          "original_form": "오오 건강식이네용",
          "speaker_id": "1",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100036260.1.4",
          "form": "삶아서 드시나요?",
          "original_form": "삶아서 드시나요?",
          "speaker_id": "1",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036260.1.5",
          "form": "네넹! 뭔가 밥은 안 넘어가더라구요 배아프고 ㅋㅋㅋ",
          "original_form": "네넹! 뭔가 밥은 안 넘어가더라구요 배아프고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036260.1.6",
          "form": "맞아요ㅋㅋㅋ 저도 그래서 쥬스나 단백질바 먹어요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ 저도 그래서 쥬스나 단백질바 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036260.1.7",
          "form": "달걀은 좀 덜익음 좋겠는데ㅈ늘 완숙이라 안먹게되네요",
          "original_form": "달걀은 좀 덜익음 좋겠는데ㅈ늘 완숙이라 안먹게되네요",
          "speaker_id": "1",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036260.1.8",
          "form": "ㅋㅋㅋㅋ삶아두고 까먹으시나요",
          "original_form": "ㅋㅋㅋㅋ삶아두고 까먹으시나요",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100036260.1.9",
          "form": "넼ㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100036260.1.10",
          "form": "매일매일우 힘들더라구요",
          "original_form": "매일매일우 힘들더라구요",
          "speaker_id": "1",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100036260.1.11",
          "form": "ㅋㅋㅋㅋ맞아요..저도 엄마가 삶아줘요...불효녀",
          "original_form": "ㅋㅋㅋㅋ맞아요..저도 엄마가 삶아줘요...불효녀",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036260.1.12",
          "form": "저두.. 불효녀는 웁니다..광광",
          "original_form": "저두.. 불효녀는 웁니다..광광",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100036260.1.13",
          "form": "덕분에 아침을 잘 먹고있어요ㅋㅋ",
          "original_form": "덕분에 아침을 잘 먹고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100036260.1.14",
          "form": "네ㅋㅋㅋㅋ아니었으면 영양실조 걸렸을거같아요",
          "original_form": "네ㅋㅋㅋㅋ아니었으면 영양실조 걸렸을거같아요",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036260.1.15",
          "form": "맞아오ㅋㅋ 이렇게 사랑도 깨닫고..",
          "original_form": "맞아오ㅋㅋ 이렇게 사랑도 깨닫고..",
          "speaker_id": "1",
          "time": "20210831 12:37"
        },
        {
          "id": "MDRW2100036260.1.16",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 12:37"
        },
        {
          "id": "MDRW2100036260.1.17",
          "form": "어우 오늘 엄청 바빴네요 저녁 맛있게드세용ㅎㅎㅎ",
          "original_form": "어우 오늘 엄청 바빴네요 저녁 맛있게드세용ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100036260.1.18",
          "form": "넹! 저녁 잘챙겨드세욥!",
          "original_form": "넹! 저녁 잘챙겨드세욥!",
          "speaker_id": "1",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100036260.1.19",
          "form": "뭔가 호르몬의 영향으러 음식 많이 땡기는 때가 됐어요",
          "original_form": "뭔가 호르몬의 영향으러 음식 많이 땡기는 때가 됐어요",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100036260.1.20",
          "form": "이럴때 많이 먹으면 진짜 다 살로 가던데",
          "original_form": "이럴때 많이 먹으면 진짜 다 살로 가던데",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100036260.1.21",
          "form": "아ㅠ 진짜 싫죠",
          "original_form": "아ㅠ 진짜 싫죠",
          "speaker_id": "1",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100036260.1.22",
          "form": "화수분이 되버린다구요ㅠ",
          "original_form": "화수분이 되버린다구요ㅠ",
          "speaker_id": "1",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100036260.1.23",
          "form": "안먹던 과자를 계속 먹고있네욬ㅋㅋ",
          "original_form": "안먹던 과자를 계속 먹고있네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100036260.1.24",
          "form": "앗..ㅋㅋㅋ 과자는 진짜 입맛을 사로잡죠",
          "original_form": "앗..ㅋㅋㅋ 과자는 진짜 입맛을 사로잡죠",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100036260.1.25",
          "form": "넹 단짠의 조화를 위해",
          "original_form": "넹 단짠의 조화를 위해",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100036260.1.26",
          "form": "어제는 짠맛",
          "original_form": "어제는 짠맛",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100036260.1.27",
          "form": "오늘은 단맛",
          "original_form": "오늘은 단맛",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100036260.1.28",
          "form": "크으 최고네용",
          "original_form": "크으 최고네용",
          "speaker_id": "1",
