      "utterance": [
        {
          "id": "MDRW2100031874.1.1",
          "form": "따님과 즐거운 시간이 되셨습니까",
          "original_form": "따님과 즐거운 시간이 되셨습니까",
          "speaker_id": "2",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100031874.1.2",
          "form": "저도     처가 댁 가서 밥 두 그릇 먹었습니다",
          "original_form": "저도     처가 댁 가서 밥 두 그릇 먹었습니다",
          "speaker_id": "2",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100031874.1.3",
          "form": "간만에 좋은 시간이었습니다",
          "original_form": "간만에 좋은 시간이었습니다",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100031874.1.4",
          "form": "저희 부부도 지난주에 2차까지 백신 맞고",
          "original_form": "저희 부부도 지난주에 2차까지 백신 맞고",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100031874.1.5",
          "form": "딸도 이번주에 백신 2차 맞아서",
          "original_form": "딸도 이번주에 백신 2차 맞아서",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100031874.1.6",
          "form": "좀 더 편한 마음으로 오붓하게 대화도 나누고 식사도 했습니다",
          "original_form": "좀 더 편한 마음으로 오붓하게 대화도 나누고 식사도 했습니다",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100031874.1.7",
          "form": "혹시 딸 두분이신가요?",
          "original_form": "혹시 딸 두분이신가요?",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100031874.1.8",
          "form": "오늘 딸 다녀가신분과 배우자 사이신가보네요",
          "original_form": "오늘 딸 다녀가신분과 배우자 사이신가보네요",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100031874.1.9",
          "form": "아 제 아내도 이거 합니다",
          "original_form": "아 제 아내도 이거 합니다",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100031874.1.10",
          "form": "딸이 알려줬어요",
          "original_form": "딸이 알려줬어요",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100031874.1.11",
          "form": "아  스마트폰 잘 사용하시더라고요",
          "original_form": "아  스마트폰 잘 사용하시더라고요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100031874.1.12",
          "form": "닉네임이 비슷해서 알아봤습니다",
          "original_form": "닉네임이 비슷해서 알아봤습니다",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100031874.1.13",
          "form": "저보다 잘합니다",
          "original_form": "저보다 잘합니다",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100031874.1.14",
          "form": "저는 겨우겨우 하고 있지요",
          "original_form": "저는 겨우겨우 하고 있지요",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100031874.1.15",
          "form": "잘 하십니다",
          "original_form": "잘 하십니다",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100031874.1.16",
          "form": "가족 중 가장 늦게 스마트폰에 입문했습니다",
          "original_form": "가족 중 가장 늦게 스마트폰에 입문했습니다",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100031874.1.17",
          "form": "시작이 반입니다",
          "original_form": "시작이 반입니다",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100031874.1.18",
          "form": "더 좋아 지실겁니다",
          "original_form": "더 좋아 지실겁니다",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100031874.1.19",
          "form": "축하드려요",
          "original_form": "축하드려요",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100031874.1.20",
          "form": "이게 알려줘도 잊어버리고 또 들어도 모르겠고",
          "original_form": "이게 알려줘도 잊어버리고 또 들어도 모르겠고",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100031874.1.21",
          "form": "계속 해봐서 익숙해져야  하는데 쉽지가 않습니다",
          "original_form": "계속 해봐서 익숙해져야  하는데 쉽지가 않습니다",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100031874.1.22",
          "form": "요새는 큐알코드 연습중입니다",
          "original_form": "요새는 큐알코드 연습중입니다",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100031874.1.23",
          "form": "반복 학습이 답입니다",
          "original_form": "반복 학습이 답입니다",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100031874.1.24",
          "form": "요행은 없어유",
          "original_form": "요행은 없어유",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100031874.1.25",
          "form": "많이해보셔요",
          "original_form": "많이해보셔요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100031874.1.26",
          "form": "그러게 말입니다",
          "original_form": "그러게 말입니다",
          "speaker_id": "1",
          "time": "20210906 06:07"
        },
        {
          "id": "MDRW2100031874.1.27",
          "form": "완전 암기가 될때까지 해야할 듯 합니다",
          "original_form": "완전 암기가 될때까지 해야할 듯 합니다",
          "speaker_id": "1",
          "time": "20210906 06:07"
        },
        {
          "id": "MDRW2100031874.1.28",
          "form": "그래도 아내와 딸이 있어서 도움을 많이 받고 있어요",
          "original_form": "그래도 아내와 딸이 있어서 도움을 많이 받고 있어요",
          "speaker_id": "1",
          "time": "20210906 06:07"
        },
        {
          "id": "MDRW2100031874.1.29",
          "form": "네 좋으시겠습니다",
          "original_form": "네 좋으시겠습니다",
          "speaker_id": "2",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100031874.1.30",
          "form": "두 딸 다 결혼했나요?",
          "original_form": "두 딸 다 결혼했나요?",
          "speaker_id": "2",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100031874.1.31",
          "form": "한딸만 결혼해서 아들이 있습니다",
          "original_form": "한딸만 결혼해서 아들이 있습니다",
          "speaker_id": "1",
          "time": "20210906 14:05"
        }
      ]
    }
