      "utterance": [
        {
          "id": "MDRW2100004510.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004510.1.2",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004510.1.3",
          "form": "설거지 해요",
          "original_form": "설거지 해요",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004510.1.4",
          "form": "본가에서는 설거지 하고",
          "original_form": "본가에서는 설거지 하고",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004510.1.5",
          "form": "가끔 빨래도 하고 분리수거도 돕고",
          "original_form": "가끔 빨래도 하고 분리수거도 돕고",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004510.1.6",
          "form": "자취방에서는",
          "original_form": "자취방에서는",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004510.1.7",
          "form": "혼자 다 해요",
          "original_form": "혼자 다 해요",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004510.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004510.1.9",
          "form": "혼자 살면 이게 문제죠",
          "original_form": "혼자 살면 이게 문제죠",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004510.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004510.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004510.1.12",
          "form": "귀찮아요",
          "original_form": "귀찮아요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004510.1.13",
          "form": "혼자 사세요?",
          "original_form": "혼자 사세요?",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004510.1.14",
          "form": "아니면 다른 사람이랑 같이 사세요?",
          "original_form": "아니면 다른 사람이랑 같이 사세요?",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004510.1.15",
          "form": "결혼했어요",
          "original_form": "결혼했어요",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004510.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004510.1.17",
          "form": "그럼 집안일 하시겠네요",
          "original_form": "그럼 집안일 하시겠네요",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004510.1.18",
          "form": "귀찮으시겠어요",
          "original_form": "귀찮으시겠어요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004510.1.19",
          "form": "그래도 해야죠",
          "original_form": "그래도 해야죠",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004510.1.20",
          "form": "그렇기는 해요",
          "original_form": "그렇기는 해요",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004510.1.21",
          "form": "ㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004510.1.22",
          "form": "뭐가 가장 귀찮으세요?",
          "original_form": "뭐가 가장 귀찮으세요?",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004510.1.23",
          "form": "청소죠",
          "original_form": "청소죠",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004510.1.24",
          "form": "분담이지만",
          "original_form": "분담이지만",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004510.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004510.1.26",
          "form": "청소 진짜",
          "original_form": "청소 진짜",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004510.1.27",
          "form": "전 화장실 청소랑",
          "original_form": "전 화장실 청소랑",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004510.1.28",
          "form": "싱크대 청소가 제일 싫더라고요",
          "original_form": "싱크대 청소가 제일 싫더라고요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004510.1.29",
          "form": "특히 여름이 힘들어요",
          "original_form": "특히 여름이 힘들어요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004510.1.30",
          "form": "화장실이 대박이죠 락스",
          "original_form": "화장실이 대박이죠 락스",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004510.1.31",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004510.1.32",
          "form": "여름에 더운데",
          "original_form": "여름에 더운데",
          "speaker_id": "2",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004510.1.33",
          "form": "에어컨 꺼야 하고",
          "original_form": "에어컨 꺼야 하고",
          "speaker_id": "2",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004510.1.34",
          "form": "창문은 열어야하고",
          "original_form": "창문은 열어야하고",
          "speaker_id": "2",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004510.1.35",
          "form": "네 고통이 2배",
          "original_form": "네 고통이 2배",
          "speaker_id": "1",
          "time": "20210821 17:49"
        }
      ]
