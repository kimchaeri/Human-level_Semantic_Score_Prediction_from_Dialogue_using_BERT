      "utterance": [
        {
          "id": "MDRW2100026390.1.1",
          "form": "운동 중이죠",
          "original_form": "운동 중이죠",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026390.1.2",
          "form": "다른 사람들은 모르겠네요",
          "original_form": "다른 사람들은 모르겠네요",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026390.1.3",
          "form": "지금쯤 집에 들어오지 않았을까요",
          "original_form": "지금쯤 집에 들어오지 않았을까요",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026390.1.4",
          "form": "비마니온다",
          "original_form": "비마니온다",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026390.1.5",
          "form": "집에안갈끼가",
          "original_form": "집에안갈끼가",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026390.1.6",
          "form": "우산이 없는데",
          "original_form": "우산이 없는데",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026390.1.7",
          "form": "비는 많이 오고",
          "original_form": "비는 많이 오고",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026390.1.8",
          "form": "그칠 때까진 못 갈거 같아요ㅋㅋㅋ",
          "original_form": "그칠 때까진 못 갈거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026390.1.9",
          "form": "엄마한테 가져오라해라 ㅋ",
          "original_form": "엄마한테 가져오라해라 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026390.1.10",
          "form": "아님 맞고가던지",
          "original_form": "아님 맞고가던지",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026390.1.11",
          "form": "작은우산을 들고다니라 ㅋ 동생이 가져다 줄것도아니고",
          "original_form": "작은우산을 들고다니라 ㅋ 동생이 가져다 줄것도아니고",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026390.1.12",
          "form": "다행히 지금은 그쳤어요",
          "original_form": "다행히 지금은 그쳤어요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026390.1.13",
          "form": "스트레칭하고 집 가려구요",
          "original_form": "스트레칭하고 집 가려구요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026390.1.14",
          "form": "집에선 가족들이 걱정도안하제",
          "original_form": "집에선 가족들이 걱정도안하제",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026390.1.15",
          "form": "다컸다고 ㅋ",
          "original_form": "다컸다고 ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026390.1.16",
          "form": "늦으면 아빠가 걱정은 하는데",
          "original_form": "늦으면 아빠가 걱정은 하는데",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026390.1.17",
          "form": "헬스장이 가까워서 그런가",
          "original_form": "헬스장이 가까워서 그런가",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026390.1.18",
          "form": "걱정은 좀 덜한거 같아요",
          "original_form": "걱정은 좀 덜한거 같아요",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026390.1.19",
          "form": "아 가까워서",
          "original_form": "아 가까워서",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026390.1.20",
          "form": "가족들은 안주무시고?",
          "original_form": "가족들은 안주무시고?",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026390.1.21",
          "form": "난 육퇴했고 잘라고~",
          "original_form": "난 육퇴했고 잘라고~",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026390.1.22",
          "form": "엄마는 잘거고 아빠는 핸드폰 할걸요ㅋㅋㅋ",
          "original_form": "엄마는 잘거고 아빠는 핸드폰 할걸요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026390.1.23",
          "form": "보일러 키면 또 엄마가 깨서 한소리 하긴 하는데...ㅎ",
          "original_form": "보일러 키면 또 엄마가 깨서 한소리 하긴 하는데...ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026390.1.24",
          "form": "그래도씻으려면 켜야쥐~~",
          "original_form": "그래도씻으려면 켜야쥐~~",
          "speaker_id": "1",
          "time": "20210902 07:25"
        },
        {
          "id": "MDRW2100026390.1.25",
          "form": "맞쥬",
          "original_form": "맞쥬",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100026390.1.26",
          "form": "근데 보일러가 안방에 있어서",
          "original_form": "근데 보일러가 안방에 있어서",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100026390.1.27",
          "form": "들어가면 깨웠다고 혼나요 7ㅁ8",
          "original_form": "들어가면 깨웠다고 혼나요 7ㅁ8",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100026390.1.28",
          "form": "아~~그러후나",
          "original_form": "아~~그러후나",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100026390.1.29",
          "form": "그 집 보일러는 거실에 있어요?",
          "original_form": "그 집 보일러는 거실에 있어요?",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100026390.1.30",
          "form": "보일러를 밖에 빼주면 좋은데",
          "original_form": "보일러를 밖에 빼주면 좋은데",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100026390.1.31",
          "form": "왜 같이 쓰는 집에 보일러를 안방에 뒀는지",
          "original_form": "왜 같이 쓰는 집에 보일러를 안방에 뒀는지",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100026390.1.32",
          "form": "이해할 수 없음...",
          "original_form": "이해할 수 없음...",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100026390.1.33",
          "form": "아니 안방에있는데 늘 켜져있어서 온수는",
          "original_form": "아니 안방에있는데 늘 켜져있어서 온수는",
          "speaker_id": "1",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100026390.1.34",
          "form": "좋겠네요...",
          "original_form": "좋겠네요...",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100026390.1.35",
          "form": "우리집은 꺼진게 기본인데...",
          "original_form": "우리집은 꺼진게 기본인데...",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100026390.1.36",
          "form": "집에 사람이 거의 없기도 한데",
          "original_form": "집에 사람이 거의 없기도 한데",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100026390.1.37",
          "form": "쓸 때만큼은 편하게 쓰고 싶다구요 8ㅁ8",
          "original_form": "쓸 때만큼은 편하게 쓰고 싶다구요 8ㅁ8",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100026390.1.38",
          "form": "ㅋㅋ 그래좀심하다 오마니",
          "original_form": "ㅋㅋ 그래좀심하다 오마니",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100026390.1.39",
          "form": "일마치고 운동다녀온 딸이 씻어야지ㅜ",
          "original_form": "일마치고 운동다녀온 딸이 씻어야지ㅜ",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100026390.1.40",
          "form": "그것보단 잠이 더 소중하지 않을까요",
          "original_form": "그것보단 잠이 더 소중하지 않을까요",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100026390.1.41",
          "form": "저라도 자는거 깨우면 화낼거 같긴함...",
          "original_form": "저라도 자는거 깨우면 화낼거 같긴함...",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100026390.1.42",
          "form": "하지만 한 두번 있는 일이 아니라 화내고",
          "original_form": "하지만 한 두번 있는 일이 아니라 화내고",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100026390.1.43",
          "form": "당당하게 들어가쥬ㅋㅋㅋㅋ",
          "original_form": "당당하게 들어가쥬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100026390.1.44",
          "form": "잘한다 엄마한테 욕한번들어무모되지",
          "original_form": "잘한다 엄마한테 욕한번들어무모되지",
          "speaker_id": "1",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100026390.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100026390.1.46",
          "form": "어제도 춥다고 문닫아놓은거 열어놓고 들어갔다왔쥬",
          "original_form": "어제도 춥다고 문닫아놓은거 열어놓고 들어갔다왔쥬",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100026390.1.47",
          "form": "ㅋㅋ 잘했다..크게인사하고들어가뿌라",
          "original_form": "ㅋㅋ 잘했다..크게인사하고들어가뿌라",
          "speaker_id": "1",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100026390.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그러면 엄마가 뭐 하나 던지지 않을까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그러면 엄마가 뭐 하나 던지지 않을까요",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100026390.1.49",
          "form": "으악 엄마!",
          "original_form": "으악 엄마!",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100026390.1.50",
          "form": "할 듯",
          "original_form": "할 듯",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100026390.1.51",
          "form": "친딸맞제?",
          "original_form": "친딸맞제?",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100026390.1.52",
          "form": "유전자해봐봐",
          "original_form": "유전자해봐봐",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100026390.1.53",
          "form": "얼굴이 검사 안해도",
          "original_form": "얼굴이 검사 안해도",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100026390.1.54",
          "form": "판박이라는데",
          "original_form": "판박이라는데",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100026390.1.55",
          "form": "...ㅎ",
          "original_form": "...ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100026390.1.56",
          "form": "ㅍㅎㅎㅎ 음 그렇단말이지",
          "original_form": "ㅍㅎㅎㅎ 음 그렇단말이지",
          "speaker_id": "1",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100026390.1.57",
          "form": "얼른 돈 벌어서 집을 나가던가 해야지...",
