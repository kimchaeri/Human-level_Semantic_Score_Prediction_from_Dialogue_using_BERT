        {
          "id": "MDRW2100016174.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100016174.1.3",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016174.1.4",
          "form": "운동 좋아하진 않는데..",
          "original_form": "운동 좋아하진 않는데..",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016174.1.5",
          "form": "하긴 해요",
          "original_form": "하긴 해요",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016174.1.6",
          "form": "ㅎㅎ..",
          "original_form": "ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016174.1.7",
          "form": "운동 좋아하세요??",
          "original_form": "운동 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016174.1.8",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016174.1.9",
          "form": "무슨 운동하세요??",
          "original_form": "무슨 운동하세요??",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016174.1.10",
          "form": "홈트 해보려구요",
          "original_form": "홈트 해보려구요",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016174.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016174.1.12",
          "form": "필라테스 해보셨어요??",
          "original_form": "필라테스 해보셨어요??",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016174.1.13",
          "form": "아뇨 필라테스는 안해봤어요",
          "original_form": "아뇨 필라테스는 안해봤어요",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016174.1.14",
          "form": "와 전 얼마 전에 해봤는데",
          "original_form": "와 전 얼마 전에 해봤는데",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016174.1.15",
          "form": "죽는 줄 알았어요",
          "original_form": "죽는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016174.1.16",
          "form": "많이 힘든가요",
          "original_form": "많이 힘든가요",
          "speaker_id": "1",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016174.1.17",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100016174.1.18",
          "form": "대체 저 자세를 어떻게 할까 생각이 드는 운동",
          "original_form": "대체 저 자세를 어떻게 할까 생각이 드는 운동",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100016174.1.19",
          "form": "약간 요가 비슷한가요",
          "original_form": "약간 요가 비슷한가요",
          "speaker_id": "1",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016174.1.20",
          "form": "뭔가 더 힘든 느낌..",
          "original_form": "뭔가 더 힘든 느낌..",
