      "utterance": [
        {
          "id": "MDRW2100010274.1.1",
          "form": "저녁메뉴 뭐드시나요?!",
          "original_form": "저녁메뉴 뭐드시나요?!",
          "speaker_id": "1",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010274.1.2",
          "form": "ㅋㅋㅋㅋ글쎄요",
          "original_form": "ㅋㅋㅋㅋ글쎄요",
          "speaker_id": "2",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010274.1.3",
          "form": "어제는 떡볶이 먹었는데",
          "original_form": "어제는 떡볶이 먹었는데",
          "speaker_id": "2",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010274.1.4",
          "form": "낙곱새 땡깁니다",
          "original_form": "낙곱새 땡깁니다",
          "speaker_id": "2",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010274.1.5",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010274.1.6",
          "form": "아 저 어제 낙곱새먹었는데ㅋㅋㅋ",
          "original_form": "아 저 어제 낙곱새먹었는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010274.1.7",
          "form": "오늘은 쫄면돈까스먹었답니다",
          "original_form": "오늘은 쫄면돈까스먹었답니다",
          "speaker_id": "1",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010274.1.8",
          "form": "차돌짬뽕 먹으려다가",
          "original_form": "차돌짬뽕 먹으려다가",
          "speaker_id": "1",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010274.1.9",
          "form": "오우 제가 좋아하는 것들이네요",
          "original_form": "오우 제가 좋아하는 것들이네요",
          "speaker_id": "2",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010274.1.10",
          "form": "돈까스 비냉이랑 먹어도 맛있어요",
          "original_form": "돈까스 비냉이랑 먹어도 맛있어요",
          "speaker_id": "2",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010274.1.11",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010274.1.12",
          "form": "날거는 좋아하시나요?",
          "original_form": "날거는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010274.1.13",
          "form": "싫어하는건 아닌데 즐기진 않아요",
          "original_form": "싫어하는건 아닌데 즐기진 않아요",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010274.1.14",
          "form": "있으면 먹고?",
          "original_form": "있으면 먹고?",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010274.1.15",
          "form": "다만 초밥은 단골집 생긴 뒤로 종종 갑니다ㅋㅋ",
          "original_form": "다만 초밥은 단골집 생긴 뒤로 종종 갑니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010274.1.16",
          "form": "싫어하거나 못 드시는 음식 있으세요?",
          "original_form": "싫어하거나 못 드시는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010274.1.17",
          "form": "헐 어딘가요 초밥단골집\r\n전 회랑 초밥 진짜 환장해요",
          "original_form": "헐 어딘가요 초밥단골집\r\n전 회랑 초밥 진짜 환장해요",
          "speaker_id": "1",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010274.1.18",
          "form": "싫어하는건 닭발? 순대? 정도요ㅋㅋㅋ",
          "original_form": "싫어하는건 닭발? 순대? 정도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010274.1.19",
          "form": "으 저도 닭발 싫어해요",
          "original_form": "으 저도 닭발 싫어해요",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010274.1.20",
          "form": "ㅋㅋㅋ지수사<< 라고 검색하면 나올거에요",
          "original_form": "ㅋㅋㅋ지수사<< 라고 검색하면 나올거에요",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010274.1.21",
          "form": "어디 거주하시는진 모르겠지만 지나가다 근처면 드셔보세요",
          "original_form": "어디 거주하시는진 모르겠지만 지나가다 근처면 드셔보세요",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010274.1.22",
          "form": "아 회 저 숭어회는 좋아하는데",
          "original_form": "아 회 저 숭어회는 좋아하는데",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010274.1.23",
          "form": "참치회는 싫어요 푸석푸석해서",
          "original_form": "참치회는 싫어요 푸석푸석해서",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010274.1.24",
          "form": "와우 맛있어보이네요 역시 초밥짱",
          "original_form": "와우 맛있어보이네요 역시 초밥짱",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010274.1.25",
          "form": "참치왜요ㅠㅠ참치도 너무맛있는데",
          "original_form": "참치왜요ㅠㅠ참치도 너무맛있는데",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010274.1.26",
          "form": "맛 없어요... 뭐랄까 식감이 얼었으면 푸석푸석하고 녹은건 물컹푸석?ㅋㅋㅋ 아무 맛도 안나구요ㅋㅋ",
          "original_form": "맛 없어요... 뭐랄까 식감이 얼었으면 푸석푸석하고 녹은건 물컹푸석?ㅋㅋㅋ 아무 맛도 안나구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100010274.1.27",
          "form": "왜 비싼지 모르겠어요",
          "original_form": "왜 비싼지 모르겠어요",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100010274.1.28",
          "form": "ㅋㅋㅋ 제가 다 먹고싶네요",
          "original_form": "ㅋㅋㅋ 제가 다 먹고싶네요",
          "speaker_id": "1",
          "time": "20210823 16:43"
        },
        {
          "id": "MDRW2100010274.1.29",
          "form": "입에서 녹는데ㅠ",
          "original_form": "입에서 녹는데ㅠ",
          "speaker_id": "1",
          "time": "20210823 16:43"
        },
        {
          "id": "MDRW2100010274.1.30",
          "form": "제 몫까지 드세요ㅋㅋㅋ",
          "original_form": "제 몫까지 드세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010274.1.31",
          "form": "녹는건 우니죠",
          "original_form": "녹는건 우니죠",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010274.1.32",
          "form": "우니도 좋죠 저는 진짜 이런종류는 다좋아해요ㅋㅋㅋ",
          "original_form": "우니도 좋죠 저는 진짜 이런종류는 다좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010274.1.33",
          "form": "육회 육사시미 우니 광어 등등",
          "original_form": "육회 육사시미 우니 광어 등등",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010274.1.34",
          "form": "낙지는요?",
          "original_form": "낙지는요?",
          "speaker_id": "2",
          "time": "20210823 16:53"
        },
        {
          "id": "MDRW2100010274.1.35",
          "form": "낙지좋죠ㅋㅋㅋ",
          "original_form": "낙지좋죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100010274.1.36",
          "form": "근데 산낙지는 안돼요",
          "original_form": "근데 산낙지는 안돼요",
          "speaker_id": "1",
          "time": "20210823 16:59"
        }
      ]
    }
