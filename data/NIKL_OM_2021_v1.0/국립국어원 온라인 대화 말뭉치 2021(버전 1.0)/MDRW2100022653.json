      "utterance": [
        {
          "id": "MDRW2100022653.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022653.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022653.1.3",
          "form": "스포츠 좋아하시나요?",
          "original_form": "스포츠 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022653.1.4",
          "form": "올림픽 열심히 봤어요",
          "original_form": "올림픽 열심히 봤어요",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022653.1.5",
          "form": "저도 이번 올림픽은 더 챙겨보고 응원하게 되더라구요!",
          "original_form": "저도 이번 올림픽은 더 챙겨보고 응원하게 되더라구요!",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022653.1.6",
          "form": "특히 배구랑 양궁이요 ㅎㅎㅎ",
          "original_form": "특히 배구랑 양궁이요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022653.1.7",
          "form": "저도요 김연경 선수 너무 멋있어요",
          "original_form": "저도요 김연경 선수 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022653.1.8",
          "form": "맞아요! 진짜 터키랑 했던 경기는 정말 드라마같았어요...",
          "original_form": "맞아요! 진짜 터키랑 했던 경기는 정말 드라마같았어요...",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022653.1.9",
          "form": "멋있어서 유튜브로 계속 찾아보다가 옛날 다큐까지 흘러갔어요 ㅋㅋㅋ",
          "original_form": "멋있어서 유튜브로 계속 찾아보다가 옛날 다큐까지 흘러갔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022653.1.10",
          "form": "옛날 인터뷰 보면 다 잘생기셨어요 ㅋㅋ ㅜ",
          "original_form": "옛날 인터뷰 보면 다 잘생기셨어요 ㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022653.1.11",
          "form": "맞아요... 김희진 선수도! 그리고 그 자신감 넘치는 태도랑 그걸 뒷받침하는 실력이랑...",
          "original_form": "맞아요... 김희진 선수도! 그리고 그 자신감 넘치는 태도랑 그걸 뒷받침하는 실력이랑...",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022653.1.12",
          "form": "그냥 다 멋있어요ㅜㅜㅠ",
          "original_form": "그냥 다 멋있어요ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022653.1.13",
          "form": "방송에 자주 나오셨으면 좋겠어요!",
          "original_form": "방송에 자주 나오셨으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022653.1.14",
          "form": "맞아요... 선수들 연봉도 오르면 좋을텐데",
          "original_form": "맞아요... 선수들 연봉도 오르면 좋을텐데",
          "speaker_id": "1",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100022653.1.15",
          "form": "그러게요...ㅜㅜ 여자배구는 지원이 엄청 적다고 들었어요...",
          "original_form": "그러게요...ㅜㅜ 여자배구는 지원이 엄청 적다고 들었어요...",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022653.1.16",
          "form": "김연경 선수는 정말 세계 최고인데ㅜㅜ",
          "original_form": "김연경 선수는 정말 세계 최고인데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022653.1.17",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022653.1.18",
          "form": "국내 리그에서 뛰는 것도 보고 싶은데 ㅜㅜ",
          "original_form": "국내 리그에서 뛰는 것도 보고 싶은데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022653.1.19",
          "form": "저도요... 아마 이번에 여자배구 관심갖은 분들이 많을텐데",
          "original_form": "저도요... 아마 이번에 여자배구 관심갖은 분들이 많을텐데",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022653.1.20",
          "form": "저도 그렇구요...",
          "original_form": "저도 그렇구요...",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022653.1.21",
          "form": "너무 늦게 알게돼서 안타까워요ㅜㅜ",
          "original_form": "너무 늦게 알게돼서 안타까워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022653.1.22",
          "form": "이제 은퇴하신다니...",
          "original_form": "이제 은퇴하신다니...",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022653.1.23",
          "form": "이제라도 지원이 더 많이 이루어졋으면 좋겠네요",
          "original_form": "이제라도 지원이 더 많이 이루어졋으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210829 17:19"
        }
      ]
    }
