      "utterance": [
        {
          "id": "MDRW2100025598.1.1",
          "form": "최근 영화 관람 뭐 하셨나요?",
          "original_form": "최근 영화 관람 뭐 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025598.1.2",
          "form": "최근에는 영화관을 못 갔어요",
          "original_form": "최근에는 영화관을 못 갔어요",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025598.1.3",
          "form": "요새 싱크홀 보고 싶긴 하더라구요",
          "original_form": "요새 싱크홀 보고 싶긴 하더라구요",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025598.1.4",
          "form": "오 저 싱크홀 봤어요",
          "original_form": "오 저 싱크홀 봤어요",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025598.1.5",
          "form": "그래요? 어떤가요?",
          "original_form": "그래요? 어떤가요?",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025598.1.6",
          "form": "요즘 영화관 천원에 선착으로 풀더라구요",
          "original_form": "요즘 영화관 천원에 선착으로 풀더라구요",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025598.1.7",
          "form": "제법 재밌더라구요 웃긴걸 중간중간 넣어서",
          "original_form": "제법 재밌더라구요 웃긴걸 중간중간 넣어서",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025598.1.8",
          "form": "ㅋㅋ예고편 웃기더라구요",
          "original_form": "ㅋㅋ예고편 웃기더라구요",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025598.1.9",
          "form": "이광수님 나와서 보고싶어요",
          "original_form": "이광수님 나와서 보고싶어요",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025598.1.10",
          "form": "맞아요 광수님 겁나 재밌었어요 ㅋㅋ",
          "original_form": "맞아요 광수님 겁나 재밌었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025598.1.11",
          "form": "저는 쫄본데 공포영화 보는거 좋아해요",
          "original_form": "저는 쫄본데 공포영화 보는거 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025598.1.12",
          "form": "그럼 이번에 랑종 보셨나요?",
          "original_form": "그럼 이번에 랑종 보셨나요?",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025598.1.13",
          "form": "랑종 대박 시사회로 봤어요",
          "original_form": "랑종 대박 시사회로 봤어요",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025598.1.14",
          "form": "첨에 다큐같았는게 뒤에는 쫌 무섭더라구요",
          "original_form": "첨에 다큐같았는게 뒤에는 쫌 무섭더라구요",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025598.1.15",
          "form": "평가가 갈리더라구요",
          "original_form": "평가가 갈리더라구요",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025598.1.16",
          "form": "잔인한게 많이 나온다고 해서 안봤어요",
          "original_form": "잔인한게 많이 나온다고 해서 안봤어요",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025598.1.17",
          "form": "아 맞아요 저는 또 쫄본데 잔인한거는 강해서 ㅜㅜㅋㅋ",
          "original_form": "아 맞아요 저는 또 쫄본데 잔인한거는 강해서 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100025598.1.18",
          "form": "부산 사는데 부국제도 곧 해서 기대하고 있어요",
          "original_form": "부산 사는데 부국제도 곧 해서 기대하고 있어요",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025598.1.19",
          "form": "저 가보고 싶어요... 코로나 땜시ㅠㅠ",
          "original_form": "저 가보고 싶어요... 코로나 땜시ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025598.1.20",
          "form": "그러게요 ㅠㅠ 일단 백신은 2차까지 맞았는데",
          "original_form": "그러게요 ㅠㅠ 일단 백신은 2차까지 맞았는데",
          "speaker_id": "1",
          "time": "20210901 14:24"
        },
        {
          "id": "MDRW2100025598.1.21",
          "form": "코로나 없던시절 유일하게 연예인 보는 곳이였는데 ㅜㅜ",
          "original_form": "코로나 없던시절 유일하게 연예인 보는 곳이였는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025598.1.22",
          "form": "전 2차는 아직이에요... 아프다던데ㅎ",
          "original_form": "전 2차는 아직이에요... 아프다던데ㅎ",
          "speaker_id": "2",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025598.1.23",
          "form": "저는 교차접종 걸려서 했는데 1차가 죽을뻔하고 2차는 근육통만 ㅎㅎ",
          "original_form": "저는 교차접종 걸려서 했는데 1차가 죽을뻔하고 2차는 근육통만 ㅎㅎ",
          "speaker_id": "1",
