      "utterance": [
        {
          "id": "MDRW2100022450.1.1",
          "form": "오늘의 첫끼는 무엇이죠?!",
          "original_form": "오늘의 첫끼는 무엇이죠?!",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022450.1.2",
          "form": "먼저씻구 ㅋㅋㅋ 카스테라에 우유?",
          "original_form": "먼저씻구 ㅋㅋㅋ 카스테라에 우유?",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022450.1.3",
          "form": "헉!ㅋㅋ실컷 칼국수 얘기하다가 ㅋㅋ",
          "original_form": "헉!ㅋㅋ실컷 칼국수 얘기하다가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022450.1.4",
          "form": "나가야하니깐 ㅠㅠㅋㅋ",
          "original_form": "나가야하니깐 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022450.1.5",
          "form": "ㅋㅋ맞아 배달이 편하긴 하지",
          "original_form": "ㅋㅋ맞아 배달이 편하긴 하지",
          "speaker_id": "2",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100022450.1.6",
          "form": "오빠는 수다좀떨다 잠듬 ㅋㅋ",
          "original_form": "오빠는 수다좀떨다 잠듬 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100022450.1.7",
          "form": "ㅋㅋ난 점심에 또 떡볶이 해먹음",
          "original_form": "ㅋㅋ난 점심에 또 떡볶이 해먹음",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100022450.1.8",
          "form": "떡볶이완전좋아라는구만 ㅋㅋㅋ!!!",
          "original_form": "떡볶이완전좋아라는구만 ㅋㅋㅋ!!!",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100022450.1.9",
          "form": "난라뽀끼가더조항",
          "original_form": "난라뽀끼가더조항",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100022450.1.10",
          "form": "밀떡이 있어서 아주 기쁜 마음으로! ㅋㅋ",
          "original_form": "밀떡이 있어서 아주 기쁜 마음으로! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100022450.1.11",
          "form": "김말이튀김도 에어프라이어에 구워서 먹음",
          "original_form": "김말이튀김도 에어프라이어에 구워서 먹음",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100022450.1.12",
          "form": "어제떡보낑 밀떡이라 써있엇는데",
          "original_form": "어제떡보낑 밀떡이라 써있엇는데",
          "speaker_id": "1",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100022450.1.13",
          "form": "맛잇엇엉 !!탱글탱글?",
          "original_form": "맛잇엇엉 !!탱글탱글?",
          "speaker_id": "1",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100022450.1.14",
          "form": "응 진짜 밀가루가 넘 좋아서 우짠댜",
          "original_form": "응 진짜 밀가루가 넘 좋아서 우짠댜",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100022450.1.15",
          "form": "ㅋㅋㅋㅋ아바보처럼 떡이오ㅐ 이럴뻔 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아바보처럼 떡이오ㅐ 이럴뻔 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100022450.1.16",
          "form": "ㅋㅋ카스테라는 먹었나요",
          "original_form": "ㅋㅋ카스테라는 먹었나요",
