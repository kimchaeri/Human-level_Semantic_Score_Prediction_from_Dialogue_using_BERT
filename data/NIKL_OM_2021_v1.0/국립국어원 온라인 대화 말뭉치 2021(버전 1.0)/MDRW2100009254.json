      "utterance": [
        {
          "id": "MDRW2100009254.1.1",
          "form": "게임 하세요?",
          "original_form": "게임 하세요?",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009254.1.2",
          "form": "나이먹고는 게임 안해요..",
          "original_form": "나이먹고는 게임 안해요..",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009254.1.3",
          "form": "겜 모하세요?",
          "original_form": "겜 모하세요?",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009254.1.4",
          "form": "ㅋㅋ저도 잘안하는데",
          "original_form": "ㅋㅋ저도 잘안하는데",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009254.1.5",
          "form": "예전에 하던 메이플 가끔씩 켜서해요 ㅋㅋ",
          "original_form": "예전에 하던 메이플 가끔씩 켜서해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009254.1.6",
          "form": "ㅋㅋㅋ 메이플은 초딩겜?",
          "original_form": "ㅋㅋㅋ 메이플은 초딩겜?",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009254.1.7",
          "form": "그쵸 ㅋㅋ가끔 생각나서 해요",
          "original_form": "그쵸 ㅋㅋ가끔 생각나서 해요",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009254.1.8",
          "form": "그거  얼마전에 가챠 땜에 시끄러웠던 건가요?",
          "original_form": "그거  얼마전에 가챠 땜에 시끄러웠던 건가요?",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009254.1.9",
          "form": "현질안함 힘들죠?",
          "original_form": "현질안함 힘들죠?",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009254.1.10",
          "form": "가챠가 뭐예요?",
          "original_form": "가챠가 뭐예요?",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009254.1.11",
          "form": "확률형 아이템 뽑기요..",
          "original_form": "확률형 아이템 뽑기요..",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009254.1.12",
          "form": "애니팡 같은거도 안하세요?",
          "original_form": "애니팡 같은거도 안하세요?",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009254.1.13",
          "form": "아아 잘모르겠어여 ㅠ",
          "original_form": "아아 잘모르겠어여 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009254.1.14",
          "form": "애니팡은 전에 붐할때",
          "original_form": "애니팡은 전에 붐할때",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009254.1.15",
          "form": "한창 하긴했었둉",
          "original_form": "한창 하긴했었둉",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009254.1.16",
          "form": "저도 딱 초창기에.",
          "original_form": "저도 딱 초창기에.",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009254.1.17",
          "form": "막 하트 구걸하던거 생각나네요",
          "original_form": "막 하트 구걸하던거 생각나네요",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009254.1.18",
          "form": "맞아요 ㅋㅋㅋ 그거하느라",
          "original_form": "맞아요 ㅋㅋㅋ 그거하느라",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009254.1.19",
          "form": "잠도 늦게자고했는데 ㅠ",
          "original_form": "잠도 늦게자고했는데 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:29"
        }
      ]
