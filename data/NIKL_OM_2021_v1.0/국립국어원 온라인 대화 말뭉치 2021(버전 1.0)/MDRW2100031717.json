      "utterance": [
        {
          "id": "MDRW2100031717.1.1",
          "form": "홈트 해?",
          "original_form": "홈트 해?",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100031717.1.2",
          "form": "아니 전혀 ㅋㅋㅋ",
          "original_form": "아니 전혀 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031717.1.3",
          "form": "말로만 하고 있음ㅋㅋ",
          "original_form": "말로만 하고 있음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031717.1.4",
          "form": "왴ㅋㅋㅋㅋ 입은 운동을 하고 있구나",
          "original_form": "왴ㅋㅋㅋㅋ 입은 운동을 하고 있구나",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031717.1.5",
          "form": "그런 셈이지 ㅋㅋㅋㅋ",
          "original_form": "그런 셈이지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031717.1.6",
          "form": "운동... 그치.. 이지은 운동 해봐라..",
          "original_form": "운동... 그치.. 이지은 운동 해봐라..",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031717.1.7",
          "form": "한방에 지옥을 맛볼것이다",
          "original_form": "한방에 지옥을 맛볼것이다",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031717.1.8",
          "form": "그건 또 뭐람..",
          "original_form": "그건 또 뭐람..",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031717.1.9",
          "form": "유튭에 있는 운동인데",
          "original_form": "유튭에 있는 운동인데",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031717.1.10",
          "form": "이지은이 아이유 인것만 알겟다",
          "original_form": "이지은이 아이유 인것만 알겟다",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031717.1.11",
          "form": "이게 되게 유명하거든",
          "original_form": "이게 되게 유명하거든",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031717.1.12",
          "form": "ㅋㅋㅋ 그치만 아니야 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 그치만 아니야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031717.1.13",
