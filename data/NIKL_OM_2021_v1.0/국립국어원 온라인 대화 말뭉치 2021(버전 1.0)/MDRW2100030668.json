      "utterance": [
        {
          "id": "MDRW2100030668.1.1",
          "form": "여행은 가을이죠",
          "original_form": "여행은 가을이죠",
          "speaker_id": "2",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.2",
          "form": "오늘저는해남여행왔어요",
          "original_form": "오늘저는해남여행왔어요",
          "speaker_id": "1",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.3",
          "form": "오 해남 어디가 좋나요",
          "original_form": "오 해남 어디가 좋나요",
          "speaker_id": "2",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.4",
          "form": "음 땅끝이요",
          "original_form": "음 땅끝이요",
          "speaker_id": "1",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.5",
          "form": "가을 여행지 좀 추천해주세요",
          "original_form": "가을 여행지 좀 추천해주세요",
          "speaker_id": "2",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.6",
          "form": "해남은 고구마죠",
          "original_form": "해남은 고구마죠",
          "speaker_id": "1",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.7",
          "form": "전 대흥사가봤어요",
          "original_form": "전 대흥사가봤어요",
          "speaker_id": "2",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.8",
          "form": "가을에는 드라이브죠 해안도로",
          "original_form": "가을에는 드라이브죠 해안도로",
          "speaker_id": "1",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030668.1.9",
          "form": "고구마빵도 맛나요",
          "original_form": "고구마빵도 맛나요",
          "speaker_id": "2",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030668.1.10",
          "form": "해남 와보셨나보네요",
          "original_form": "해남 와보셨나보네요",
          "speaker_id": "1",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030668.1.11",
          "form": "해안도로는 강원도 나 남해?",
          "original_form": "해안도로는 강원도 나 남해?",
          "speaker_id": "2",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030668.1.12",
          "form": "피낭시에. 저는 개인적으로 그다지...",
          "original_form": "피낭시에. 저는 개인적으로 그다지...",
          "speaker_id": "1",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030668.1.13",
          "form": "넹 좀 가봤어요",
          "original_form": "넹 좀 가봤어요",
          "speaker_id": "2",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030668.1.14",
          "form": "남해도 좋죠. 영광에잇어요",
          "original_form": "남해도 좋죠. 영광에잇어요",
          "speaker_id": "1",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030668.1.15",
          "form": "거기 진도대교도요",
          "original_form": "거기 진도대교도요",
          "speaker_id": "2",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030668.1.16",
          "form": "백수해안도로입니다.",
          "original_form": "백수해안도로입니다.",
          "speaker_id": "1",
          "time": "20210904 11:12"
        }
      ]
    }
