      "utterance": [
        {
          "id": "MDRW2100039638.1.1",
          "form": "무슨 과자 드시고 있으세요?",
          "original_form": "무슨 과자 드시고 있으세요?",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039638.1.2",
          "form": "고구마깡이랑 팝콘이요",
          "original_form": "고구마깡이랑 팝콘이요",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039638.1.3",
          "form": "단짠단짠으로 끊임없이 먹고 있어요ㅋㅋ",
          "original_form": "단짠단짠으로 끊임없이 먹고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039638.1.4",
          "form": "맛있겠어요 팝콘!!",
          "original_form": "맛있겠어요 팝콘!!",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039638.1.5",
          "form": "피코크 인절미 팝콘도 맛있더라고요",
          "original_form": "피코크 인절미 팝콘도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039638.1.6",
          "form": "아 그래요? 이마트 가서 사야겠네요",
          "original_form": "아 그래요? 이마트 가서 사야겠네요",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039638.1.7",
          "form": "얼마전에 처음 먹어봤는데 단짠단짠 맛있더라고요",
          "original_form": "얼마전에 처음 먹어봤는데 단짠단짠 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039638.1.8",
          "form": "이마트 팝콘 생각보다 다 맛있어요",
          "original_form": "이마트 팝콘 생각보다 다 맛있어요",
          "speaker_id": "1",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039638.1.9",
          "form": "이마트 과자들 꽤 괜찮아요ㅋㅋ",
          "original_form": "이마트 과자들 꽤 괜찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039638.1.10",
          "form": "마트 한번 털어야겠네요",
          "original_form": "마트 한번 털어야겠네요",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039638.1.11",
          "form": "요즘 신제품 드셔보신거 있으세요?",
          "original_form": "요즘 신제품 드셔보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039638.1.12",
          "form": "지금 생각이 안나는데 브이콘같은거 이마트노브랜드꺼있는데 맛있어요",
          "original_form": "지금 생각이 안나는데 브이콘같은거 이마트노브랜드꺼있는데 맛있어요",
          "speaker_id": "2",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039638.1.13",
          "form": "저도 먹어보진 않았는대 돼지바 쿠키였나 그런거도 나왔더라고요",
          "original_form": "저도 먹어보진 않았는대 돼지바 쿠키였나 그런거도 나왔더라고요",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039638.1.14",
          "form": "않았는뎈ㅋㅋ",
          "original_form": "않았는뎈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039638.1.15",
          "form": "맛있나보네요!!",
          "original_form": "맛있나보네요!!",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039638.1.16",
          "form": "민트초코 좋아하세요?",
          "original_form": "민트초코 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039638.1.17",
          "form": "아 진짜요? 전 젤리좋아해서 젤리류도 많이 사먹어요",
          "original_form": "아 진짜요? 전 젤리좋아해서 젤리류도 많이 사먹어요",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039638.1.18",
          "form": "민트초코 싫어해요ㅋㅋ",
          "original_form": "민트초코 싫어해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039638.1.19",
          "form": "민초파세요?",
          "original_form": "민초파세요?",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039638.1.20",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039638.1.21",
          "form": "젤리 뭐가 맛있어요?(",
          "original_form": "젤리 뭐가 맛있어요?(",
          "speaker_id": "1",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039638.1.22",
          "form": "저는 윌치스 젤리 좋아해요ㅋㅋ",
          "original_form": "저는 윌치스 젤리 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039638.1.23",
          "form": "한박스 사놓고 회사 집 여기저기 놓고 먹어요",
          "original_form": "한박스 사놓고 회사 집 여기저기 놓고 먹어요",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039638.1.24",
          "form": "그러시구나 저는 하리보가 쫄깃해서 맛있더라고요",
          "original_form": "그러시구나 저는 하리보가 쫄깃해서 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039638.1.25",
          "form": "턱이 끊어질거 같긴 한데요",
          "original_form": "턱이 끊어질거 같긴 한데요",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039638.1.26",
          "form": "하리보도 좋아해요",
          "original_form": "하리보도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039638.1.27",
          "form": "하리보에서 나온 것 중 지렁이모양인거 맛있어요!",
          "original_form": "하리보에서 나온 것 중 지렁이모양인거 맛있어요!",
          "speaker_id": "2",
          "time": "20210903 20:17"
        }
      ]
