      "utterance": [
        {
          "id": "MDRW2100013991.1.1",
          "form": "온라인 커뮤니티 하시나요",
          "original_form": "온라인 커뮤니티 하시나요",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013991.1.2",
          "form": "잘은 안해요",
          "original_form": "잘은 안해요",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013991.1.3",
          "form": "제가 집순이라서요",
          "original_form": "제가 집순이라서요",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013991.1.4",
          "form": "그렇군요 온라인커뮤니티도 안하시나요?",
          "original_form": "그렇군요 온라인커뮤니티도 안하시나요?",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013991.1.5",
          "form": "하긴하는데  막 올리고하진 않아요",
          "original_form": "하긴하는데  막 올리고하진 않아요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013991.1.6",
          "form": "친구들 아가야들  구경해요",
          "original_form": "친구들 아가야들  구경해요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013991.1.7",
          "form": "아 저는 네이버.다음 카페 게시글도 잘 보거든요^^",
          "original_form": "아 저는 네이버.다음 카페 게시글도 잘 보거든요^^",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013991.1.8",
          "form": "대단하시다",
          "original_form": "대단하시다",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013991.1.9",
          "form": "그래서  이것도  하시는군요",
          "original_form": "그래서  이것도  하시는군요",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013991.1.10",
          "form": "전 네이버는 메일만 ㅋㅋㅋ",
          "original_form": "전 네이버는 메일만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013991.1.11",
          "form": "네 온라인커뮤니티에 인스타도 속하는걸까요?",
          "original_form": "네 온라인커뮤니티에 인스타도 속하는걸까요?",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100013991.1.12",
          "form": "인스타도 맞을걸요",
          "original_form": "인스타도 맞을걸요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100013991.1.13",
          "form": "인스타 해요?",
          "original_form": "인스타 해요?",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100013991.1.14",
          "form": "네 최근에 친구들 근황 보려고 만들었어요",
          "original_form": "네 최근에 친구들 근황 보려고 만들었어요",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100013991.1.15",
          "form": "어렵다아니에요 ㅋ",
          "original_form": "어렵다아니에요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100013991.1.16",
          "form": "전 테그다는데  아우 ....",
          "original_form": "전 테그다는데  아우 ....",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100013991.1.17",
          "form": "맞아요 글 한번 올리려면 30분이 훅 흘러요 ㅋㅋ 어려워요",
          "original_form": "맞아요 글 한번 올리려면 30분이 훅 흘러요 ㅋㅋ 어려워요",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100013991.1.18",
          "form": "저눈그래서  눈팅만 했어오",
          "original_form": "저눈그래서  눈팅만 했어오",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100013991.1.19",
          "form": "저는 인스타보단 카페글 보는게 재밌더라구요~^^",
          "original_form": "저는 인스타보단 카페글 보는게 재밌더라구요~^^",
