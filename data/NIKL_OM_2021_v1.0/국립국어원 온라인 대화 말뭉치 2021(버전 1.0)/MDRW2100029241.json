      "utterance": [
        {
          "id": "MDRW2100029241.1.1",
          "form": "왜 싫으세요",
          "original_form": "왜 싫으세요",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029241.1.2",
          "form": "냉면이 조아요!",
          "original_form": "냉면이 조아요!",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029241.1.3",
          "form": "냉면 쵝오!",
          "original_form": "냉면 쵝오!",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029241.1.4",
          "form": "밀면이 싫은거랑",
          "original_form": "밀면이 싫은거랑",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029241.1.5",
          "form": "냉면이좋은거랑은",
          "original_form": "냉면이좋은거랑은",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029241.1.6",
          "form": "전혀상관없어요",
          "original_form": "전혀상관없어요",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029241.1.7",
          "form": "면이잖아요..!!!",
          "original_form": "면이잖아요..!!!",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029241.1.8",
          "form": "냉면 먹을데가 없어요",
          "original_form": "냉면 먹을데가 없어요",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029241.1.9",
          "form": "아니에요...있어요...!!",
          "original_form": "아니에요...있어요...!!",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029241.1.10",
          "form": "어디요?",
          "original_form": "어디요?",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029241.1.11",
          "form": "덕천요!!",
          "original_form": "덕천요!!",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029241.1.12",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029241.1.13",
          "form": "냉면이 왜좋으세요",
          "original_form": "냉면이 왜좋으세요",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029241.1.14",
          "form": "그냥 면이 얇구 식감 좋구 맛도 좋구 ㅎㅎㅎㅎ",
          "original_form": "그냥 면이 얇구 식감 좋구 맛도 좋구 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029241.1.15",
          "form": "면이 얇고 식감이 좋은게 핵심인거같아여",
          "original_form": "면이 얇고 식감이 좋은게 핵심인거같아여",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029241.1.16",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029241.1.17",
          "form": "또어떤음식좋아하세요",
          "original_form": "또어떤음식좋아하세요",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029241.1.18",
          "form": "어제먹은 육개장이요!",
          "original_form": "어제먹은 육개장이요!",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029241.1.19",
          "form": "너무좋아해서",
          "original_form": "너무좋아해서",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029241.1.20",
          "form": "만들어볼라고 시작했다가",
          "original_form": "만들어볼라고 시작했다가",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029241.1.21",
          "form": "숙주담근탕만들었어요^^",
          "original_form": "숙주담근탕만들었어요^^",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029241.1.22",
          "form": "아하 근데 맛있었다는건가요?",
          "original_form": "아하 근데 맛있었다는건가요?",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029241.1.23",
          "form": "아뇨 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029241.1.24",
          "form": "숙주담근물이...안맛있죠",
          "original_form": "숙주담근물이...안맛있죠",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029241.1.25",
          "form": "그럼 육개장은 어디서났어요?",
          "original_form": "그럼 육개장은 어디서났어요?",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029241.1.26",
          "form": "엄마가 사왔어요~",
          "original_form": "엄마가 사왔어요~",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029241.1.27",
          "form": "아하...자본주의의맛..",
          "original_form": "아하...자본주의의맛..",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029241.1.28",
          "form": "어디서샀나요",
          "original_form": "어디서샀나요",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029241.1.29",
          "form": "해운대 반찬집이요!@",
          "original_form": "해운대 반찬집이요!@",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029241.1.30",
          "form": "거기 정말 개존맛!!",
          "original_form": "거기 정말 개존맛!!",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029241.1.31",
          "form": "시레국도 넘 존맛!!",
          "original_form": "시레국도 넘 존맛!!",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029241.1.32",
          "form": "거기안살아서 아쉬워여...",
          "original_form": "거기안살아서 아쉬워여...",
          "speaker_id": "1",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100029241.1.33",
          "form": "먹는것만...",
