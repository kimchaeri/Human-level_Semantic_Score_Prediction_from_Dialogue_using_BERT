      "utterance": [
        {
          "id": "MDRW2100028740.1.1",
          "form": "저는 티비에 축구를 보고있어요 ㅎㅎ",
          "original_form": "저는 티비에 축구를 보고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028740.1.2",
          "form": "오 요즘 무슨 경기가 있나요?",
          "original_form": "오 요즘 무슨 경기가 있나요?",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028740.1.3",
          "form": "월드컵 최종예선이래요",
          "original_form": "월드컵 최종예선이래요",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028740.1.4",
          "form": "내년에 월드컵 하나바요",
          "original_form": "내년에 월드컵 하나바요",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028740.1.5",
          "form": "아하 축구쪽 세상은 그렇게 돌아가고 있군요",
          "original_form": "아하 축구쪽 세상은 그렇게 돌아가고 있군요",
          "speaker_id": "2",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028740.1.6",
          "form": "요즘 티비를 안본지 오래돼서..",
          "original_form": "요즘 티비를 안본지 오래돼서..",
          "speaker_id": "2",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028740.1.7",
          "form": "유투브는 머머 보시나요?",
          "original_form": "유투브는 머머 보시나요?",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028740.1.8",
          "form": "고양이 채널?",
          "original_form": "고양이 채널?",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028740.1.9",
          "form": "아니면 음악 플리만 들어요ㅋㅋ",
          "original_form": "아니면 음악 플리만 들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028740.1.10",
          "form": "넷플도 결제만 하고ㅋㅋ",
          "original_form": "넷플도 결제만 하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028740.1.11",
          "form": "오오 저도 고양이 여행 먹방 게임 많이봐요",
          "original_form": "오오 저도 고양이 여행 먹방 게임 많이봐요",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028740.1.12",
          "form": "고양이 어디 제일 좋아하시나요",
          "original_form": "고양이 어디 제일 좋아하시나요",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028740.1.13",
          "form": "크림히어로즈랑 외국계정 하나 있는데",
          "original_form": "크림히어로즈랑 외국계정 하나 있는데",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028740.1.14",
          "form": "여기 화려한 편집은 없지만 고화질 고양이라서 힐링하기 좋아요ㅋㅋ",
          "original_form": "여기 화려한 편집은 없지만 고화질 고양이라서 힐링하기 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028740.1.15",
          "form": "루루기여워요 ㅠㅠㅎㅎ",
          "original_form": "루루기여워요 ㅠㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028740.1.16",
          "form": "저는 코숏 채널 많이봐요",
          "original_form": "저는 코숏 채널 많이봐요",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028740.1.17",
          "form": "코숏 좋죠! 추천하실 데 있어요?",
          "original_form": "코숏 좋죠! 추천하실 데 있어요?",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028740.1.18",
          "form": "매탈남님꺼 보셨나요?",
          "original_form": "매탈남님꺼 보셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028740.1.19",
          "form": "검색해보고 왔는데 내가 사랑하는 치즈가 잔뜩! 좋네요ㅋㅋ",
          "original_form": "검색해보고 왔는데 내가 사랑하는 치즈가 잔뜩! 좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028740.1.20",
          "form": "그 파이프 구조영상이 최고에요 ㅋㅋㅋ 치즈 가득이 된 시작입니당",
          "original_form": "그 파이프 구조영상이 최고에요 ㅋㅋㅋ 치즈 가득이 된 시작입니당",
          "speaker_id": "1",
          "time": "20210902 21:58"
        }
      ]
