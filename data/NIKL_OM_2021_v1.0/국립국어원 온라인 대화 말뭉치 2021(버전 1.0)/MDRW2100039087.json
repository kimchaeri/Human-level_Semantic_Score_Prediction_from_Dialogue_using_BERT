      "utterance": [
        {
          "id": "MDRW2100039087.1.1",
          "form": "ㅋㅋ 다이어트같은건 안하시나요",
          "original_form": "ㅋㅋ 다이어트같은건 안하시나요",
          "speaker_id": "1",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100039087.1.2",
          "form": "홈트 꾸준히 하고 있어요",
          "original_form": "홈트 꾸준히 하고 있어요",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100039087.1.3",
          "form": "오 어떤거보고요",
          "original_form": "오 어떤거보고요",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039087.1.4",
          "form": "유튜브 빅씨쓰 보고 있어요",
          "original_form": "유튜브 빅씨쓰 보고 있어요",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100039087.1.5",
          "form": "빅씨쓰 들어봣ㅅ어요",
          "original_form": "빅씨쓰 들어봣ㅅ어요",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039087.1.6",
          "form": "배경이좋다던대",
          "original_form": "배경이좋다던대",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039087.1.7",
          "form": "맞아요 배경도 좋고 노래도 잘 깔아요",
          "original_form": "맞아요 배경도 좋고 노래도 잘 깔아요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100039087.1.8",
          "form": "빡세요?",
          "original_form": "빡세요?",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100039087.1.9",
          "form": "저는 적응돼서 딱히 빡세다고 느끼진 못했어요",
          "original_form": "저는 적응돼서 딱히 빡세다고 느끼진 못했어요",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039087.1.10",
          "form": "ㅋㅋㅋㅋ 오 ㄱ홈트경력 어케되세요",
          "original_form": "ㅋㅋㅋㅋ 오 ㄱ홈트경력 어케되세요",
          "speaker_id": "1",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039087.1.11",
          "form": "두달 조금 넘었어요",
          "original_form": "두달 조금 넘었어요",
          "speaker_id": "2",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100039087.1.12",
          "form": "일주 몇번이나하시나용",
          "original_form": "일주 몇번이나하시나용",
          "speaker_id": "1",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100039087.1.13",
          "form": "하루에 한번씩 30분이요ㅎㅎ",
          "original_form": "하루에 한번씩 30분이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100039087.1.14",
          "form": "진짜꾸준히하시네용!",
          "original_form": "진짜꾸준히하시네용!",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100039087.1.15",
          "form": "건강해지신거느껴요?",
          "original_form": "건강해지신거느껴요?",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100039087.1.16",
          "form": "전보다는 좀 가벼워진거 같아요",
          "original_form": "전보다는 좀 가벼워진거 같아요",
