      "utterance": [
        {
          "id": "MDRW2100028767.1.1",
          "form": "name1님은 책 좋아하세요?",
          "original_form": "&name1&님은 책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028767.1.2",
          "form": "네! 저는 책 엄청 좋아해요",
          "original_form": "네! 저는 책 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028767.1.3",
          "form": "주로 어떤 책을 읽으시나요?",
          "original_form": "주로 어떤 책을 읽으시나요?",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028767.1.4",
          "form": "저는 추리소설이요!",
          "original_form": "저는 추리소설이요!",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028767.1.5",
          "form": "헉 저도요!",
          "original_form": "헉 저도요!",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028767.1.6",
          "form": "좋아하시는 작가님이 있으신가요??",
          "original_form": "좋아하시는 작가님이 있으신가요??",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028767.1.7",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028767.1.8",
          "form": "저는 히가시노 게이고 넬레 노이하우스 기시 유스케..",
          "original_form": "저는 히가시노 게이고 넬레 노이하우스 기시 유스케..",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028767.1.9",
          "form": "이런 작가님들 책을 좋아해요",
          "original_form": "이런 작가님들 책을 좋아해요",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028767.1.10",
          "form": "와 저도 히가시노 게이고랑 넬레 노이하우수 작가님 좋아해요!",
          "original_form": "와 저도 히가시노 게이고랑 넬레 노이하우수 작가님 좋아해요!",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028767.1.11",
          "form": "헉 혹시 최근에 읽으신게 있을까요?",
          "original_form": "헉 혹시 최근에 읽으신게 있을까요?",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028767.1.12",
          "form": "저는 최근에는.. 게이고 작가님의 연애의 행방을 읽었어요",
          "original_form": "저는 최근에는.. 게이고 작가님의 연애의 행방을 읽었어요",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028767.1.13",
          "form": "앗 저는 안 읽어본 책이네요",
          "original_form": "앗 저는 안 읽어본 책이네요",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028767.1.14",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028767.1.15",
          "form": "네! 히가시노 게이고 작가님의 책은 재미없는게 없는 것 같아요",
          "original_form": "네! 히가시노 게이고 작가님의 책은 재미없는게 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028767.1.16",
          "form": "저도 다음에 읽어봐야겠네요~",
          "original_form": "저도 다음에 읽어봐야겠네요~",
          "speaker_id": "2",
          "time": "20210902 22:00"
        }
      ]
