      "utterance": [
        {
          "id": "MDRW2100044174.1.1",
          "form": "뭐 진짜?",
          "original_form": "뭐 진짜?",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044174.1.2",
          "form": "그게 뭐야 괴담이야?",
          "original_form": "그게 뭐야 괴담이야?",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044174.1.3",
          "form": "아니 아니냐고 믇는거였어",
          "original_form": "아니 아니냐고 믇는거였어",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044174.1.4",
          "form": "ㅋㅋㄱㄱㄱㄱㄱㄱㄱㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㄱㄱㄱㄱㄱㄱㄱㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044174.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044174.1.6",
          "form": "난 또 유니콘처럼",
          "original_form": "난 또 유니콘처럼",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044174.1.7",
          "form": "전설의 무지개 마카로니가 있는데",
          "original_form": "전설의 무지개 마카로니가 있는데",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044174.1.8",
          "form": "그걸 먹으면 죽는다는 괴담인 줄",
          "original_form": "그걸 먹으면 죽는다는 괴담인 줄",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044174.1.9",
          "form": "알았어",
          "original_form": "알았어",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044174.1.10",
          "form": "무슨 세상 독소있을것같아",
          "original_form": "무슨 세상 독소있을것같아",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044174.1.11",
          "form": "ㅋㄱㅋㄱㄱㄱㅋㄱㄱㅣㅣㄱㄱ",
          "original_form": "ㅋㄱㅋㄱㄱㄱㅋㄱㄱㅣㅣㄱㄱ",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044174.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044174.1.13",
          "form": "아 생각났다",
          "original_form": "아 생각났다",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044174.1.14",
          "form": "내가 먹은 거 라면볶이가 아니라 스파게티면이었어",
          "original_form": "내가 먹은 거 라면볶이가 아니라 스파게티면이었어",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044174.1.15",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044174.1.16",
          "form": "스파게티면도 싼티나는 스파게티맛이라",
          "original_form": "스파게티면도 싼티나는 스파게티맛이라",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044174.1.17",
          "form": "좋아해",
          "original_form": "좋아해",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044174.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044174.1.19",
          "form": "그래서 케찹맛으로 기억하고 있었납하",
          "original_form": "그래서 케찹맛으로 기억하고 있었납하",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044174.1.20",
          "form": "나봐",
          "original_form": "나봐",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044174.1.21",
          "form": "라면볶이도 먹은 기억 있다",
          "original_form": "라면볶이도 먹은 기억 있다",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044174.1.22",
          "form": "맞아 매콤한 고추장맛이었어",
          "original_form": "맞아 매콤한 고추장맛이었어",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044174.1.23",
          "form": "그건 가격이 안비싸서 좋아",
          "original_form": "그건 가격이 안비싸서 좋아",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044174.1.24",
          "form": "치즈볶이랑 라볶이 섞으면 만있다",
          "original_form": "치즈볶이랑 라볶이 섞으면 만있다",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044174.1.25",
          "form": "헉 그렇게는 안 먹어봤는데",
          "original_form": "헉 그렇게는 안 먹어봤는데",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044174.1.26",
          "form": "안 느끼해?",
          "original_form": "안 느끼해?",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044174.1.27",
          "form": "응 약간매콤해서",
          "original_form": "응 약간매콤해서",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044174.1.28",
          "form": "괜찮아",
          "original_form": "괜찮아",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044174.1.29",
          "form": "싼맛이 먹고싶을때",
          "original_form": "싼맛이 먹고싶을때",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044174.1.30",
          "form": "자주먹어",
          "original_form": "자주먹어",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044174.1.31",
          "form": "까르보불닭이랑은 또 달라?",
          "original_form": "까르보불닭이랑은 또 달라?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044174.1.32",
          "form": "응 진짜 싼맛",
          "original_form": "응 진짜 싼맛",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044174.1.33",
          "form": "ㅌㅋㅋㅌㅋㅋㅋㅋㅋ싼 맛이래",
          "original_form": "ㅌㅋㅋㅌㅋㅋㅋㅋㅋ싼 맛이래",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044174.1.34",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "2",
          "time": "20210907 21:54"
        }
