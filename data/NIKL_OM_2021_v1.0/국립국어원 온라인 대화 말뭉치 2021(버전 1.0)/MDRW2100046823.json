      "utterance": [
        {
          "id": "MDRW2100046823.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 21:06"
        },
        {
          "id": "MDRW2100046823.1.2",
          "form": "안녕하세용 ㅎㅎ",
          "original_form": "안녕하세용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046823.1.3",
          "form": "요즘 오징어게임이",
          "original_form": "요즘 오징어게임이",
          "speaker_id": "2",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046823.1.4",
          "form": "아주 핫한데",
          "original_form": "아주 핫한데",
          "speaker_id": "2",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046823.1.5",
          "form": "보셨나유 ㅋㅋ",
          "original_form": "보셨나유 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046823.1.6",
          "form": "그럼요ㅎㅎ",
          "original_form": "그럼요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046823.1.7",
          "form": "재밌게봤어요",
          "original_form": "재밌게봤어요",
          "speaker_id": "1",
          "time": "20211104 21:08"
        },
        {
          "id": "MDRW2100046823.1.8",
          "form": "가장 맘에 드는 캐릭터는?!",
          "original_form": "가장 맘에 드는 캐릭터는?!",
          "speaker_id": "2",
          "time": "20211104 21:08"
        },
        {
          "id": "MDRW2100046823.1.9",
          "form": "음 전 이병헌?",
          "original_form": "음 전 이병헌?",
          "speaker_id": "1",
          "time": "20211104 21:08"
        },
        {
          "id": "MDRW2100046823.1.10",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 21:08"
        },
        {
          "id": "MDRW2100046823.1.11",
          "form": "잠깐 출현이지만 인상깊어서",
          "original_form": "잠깐 출현이지만 인상깊어서",
          "speaker_id": "1",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046823.1.12",
          "form": "아 ㅎㅎㅎ 이병헌의 존재감은 무시할 수 없죠 ㅎㅎㅎ",
          "original_form": "아 ㅎㅎㅎ 이병헌의 존재감은 무시할 수 없죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046823.1.13",
          "form": "저는 새벽이....",
          "original_form": "저는 새벽이....",
          "speaker_id": "2",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046823.1.14",
          "form": "얼굴이 넘 매력적",
          "original_form": "얼굴이 넘 매력적",
          "speaker_id": "2",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046823.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046823.1.16",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "1",
          "time": "20211104 21:09"
        },
        {
          "id": "MDRW2100046823.1.17",
          "form": "신인이죠?",
          "original_form": "신인이죠?",
          "speaker_id": "1",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046823.1.18",
          "form": "배우로써는 그렇죠 ㅎㅎ",
          "original_form": "배우로써는 그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046823.1.19",
          "form": "모델계에서는 엄청 잘나가는 경력 좀 있는 분일거에요!",
          "original_form": "모델계에서는 엄청 잘나가는 경력 좀 있는 분일거에요!",
          "speaker_id": "2",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046823.1.20",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046823.1.21",
          "form": "그 배우분 얼굴이 외국인들이 참 좋아하는 얼굴이라 하더라고요",
          "original_form": "그 배우분 얼굴이 외국인들이 참 좋아하는 얼굴이라 하더라고요",
          "speaker_id": "2",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046823.1.22",
          "form": "거기선 안꾸미고나오니",
          "original_form": "거기선 안꾸미고나오니",
          "speaker_id": "1",
          "time": "20211104 21:10"
        },
        {
          "id": "MDRW2100046823.1.23",
          "form": "잘모르겠더라고요",
          "original_form": "잘모르겠더라고요",
          "speaker_id": "1",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046823.1.24",
          "form": "다들 어두워서",
          "original_form": "다들 어두워서",
          "speaker_id": "1",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046823.1.25",
          "form": "그쵸 완전 생얼 느낌 ㅎㅎ",
          "original_form": "그쵸 완전 생얼 느낌 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046823.1.26",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046823.1.27",
          "form": "그래도 화보찍고 그런거 보면 진짜 매력있게 생기신 ㅎㅎ",
          "original_form": "그래도 화보찍고 그런거 보면 진짜 매력있게 생기신 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046823.1.28",
          "form": "몬가 다르긴하겠죠ㅎ",
          "original_form": "몬가 다르긴하겠죠ㅎ",
          "speaker_id": "1",
          "time": "20211104 21:11"
        },
        {
          "id": "MDRW2100046823.1.29",
          "form": "ㅎㅎㅎㅎ 그쵸 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅎㅎㅎㅎ 그쵸 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046823.1.30",
          "form": "하여튼 원래",
          "original_form": "하여튼 원래",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046823.1.31",
          "form": "왕좌의게임이",
          "original_form": "왕좌의게임이",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046823.1.32",
          "form": "역대 드라마 중 1위였다는데",
          "original_form": "역대 드라마 중 1위였다는데",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046823.1.33",
          "form": "오징어게임이 눌렀다하더라고요 ㅋㅋ 정말 대단",
          "original_form": "오징어게임이 눌렀다하더라고요 ㅋㅋ 정말 대단",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046823.1.34",
          "form": "그건 안봤어요",
          "original_form": "그건 안봤어요",
          "speaker_id": "1",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046823.1.35",
          "form": "헉 그거 안보셨나여.. 진짜 진짜",
          "original_form": "헉 그거 안보셨나여.. 진짜 진짜",
          "speaker_id": "2",
          "time": "20211104 21:12"
        },
        {
          "id": "MDRW2100046823.1.36",
          "form": "재밌어요 왕좌의게임 ㅠㅠ",
          "original_form": "재밌어요 왕좌의게임 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.37",
          "form": "꼭 보세요 제발!!!!",
          "original_form": "꼭 보세요 제발!!!!",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.38",
          "form": "첨들어봐요",
          "original_form": "첨들어봐요",
          "speaker_id": "1",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.39",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.40",
          "form": "반지의제왕같은거",
          "original_form": "반지의제왕같은거",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.41",
          "form": "그것도 게임하는건가요?",
          "original_form": "그것도 게임하는건가요?",
          "speaker_id": "1",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.42",
          "form": "재밌게 보셨으면",
          "original_form": "재밌게 보셨으면",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.43",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.44",
          "form": "좋아하실거같은 느낌?",
          "original_form": "좋아하실거같은 느낌?",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.45",
          "form": "세계관 되게 크고",
          "original_form": "세계관 되게 크고",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.46",
          "form": "중세시대 배경은 아닌데",
          "original_form": "중세시대 배경은 아닌데",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.47",
          "form": "그런 느낌이에요",
          "original_form": "그런 느낌이에요",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.48",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.49",
          "form": "엄청 재밌어요......진짜 강추강추",
          "original_form": "엄청 재밌어요......진짜 강추강추",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.50",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.51",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.52",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20211104 21:13"
        },
        {
          "id": "MDRW2100046823.1.53",
          "form": "철왕좌를 놓고 가문들이 싸우는 내용인데 ㅎㅎ 간단하게 말하면 ",
          "original_form": "철왕좌를 놓고 가문들이 싸우는 내용인데 ㅎㅎ 간단하게 말하면 ",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046823.1.54",
          "form": "되게 재밌습니다...",
          "original_form": "되게 재밌습니다...",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046823.1.55",
          "form": "말이 많아지네요",
          "original_form": "말이 많아지네요",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046823.1.56",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046823.1.57",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046823.1.58",
          "form": "꼭한번볼께요",
          "original_form": "꼭한번볼께요",
          "speaker_id": "1",
          "time": "20211104 21:14"
        },
        {
          "id": "MDRW2100046823.1.59",
          "form": "네네 진짜 보세요.. 후회안하십니다!",
          "original_form": "네네 진짜 보세요.. 후회안하십니다!",
          "speaker_id": "2",
