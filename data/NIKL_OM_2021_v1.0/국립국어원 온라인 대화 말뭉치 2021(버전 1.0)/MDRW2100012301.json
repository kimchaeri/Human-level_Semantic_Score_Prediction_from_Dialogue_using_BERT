      "utterance": [
        {
          "id": "MDRW2100012301.1.1",
          "form": "지인 중에 군대 간 사람 있나요?",
          "original_form": "지인 중에 군대 간 사람 있나요?",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012301.1.2",
          "form": "다들 재대했어요",
          "original_form": "다들 재대했어요",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012301.1.3",
          "form": "ㅋㅋㅋ 군인 친구들한테",
          "original_form": "ㅋㅋㅋ 군인 친구들한테",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012301.1.4",
          "form": "뭐 사다달라고 한 거 있나요?",
          "original_form": "뭐 사다달라고 한 거 있나요?",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012301.1.5",
          "form": "아니요 그런적은 없네요 흥미가 없어서ㅋㅋㅋㅋ",
          "original_form": "아니요 그런적은 없네요 흥미가 없어서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100012301.1.6",
          "form": "사다달라고 하신적 있나요?",
          "original_form": "사다달라고 하신적 있나요?",
          "speaker_id": "1",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100012301.1.7",
          "form": "ㅋㅋㅋㅋ 아뇨 전 가족이라",
          "original_form": "ㅋㅋㅋㅋ 아뇨 전 가족이라",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012301.1.8",
          "form": "그냥 포카리 가루 사달라고 했어요",
          "original_form": "그냥 포카리 가루 사달라고 했어요",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012301.1.9",
          "form": "포카리 가루 는 뭐예요? 물에 타면 포카리가 되나요?",
          "original_form": "포카리 가루 는 뭐예요? 물에 타면 포카리가 되나요?",
          "speaker_id": "1",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100012301.1.10",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012301.1.11",
          "form": "물에 타먹는 포카리요",
          "original_form": "물에 타먹는 포카리요",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012301.1.12",
          "form": "그런게 있구나 군대에서만 파는건가요?",
          "original_form": "그런게 있구나 군대에서만 파는건가요?",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100012301.1.13",
          "form": "아뇨 인터넷도 파는데",
          "original_form": "아뇨 인터넷도 파는데",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100012301.1.14",
          "form": "군대가 저렴하죠!!",
          "original_form": "군대가 저렴하죠!!",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100012301.1.15",
          "form": "그렇구나 가족분한테 편지는 쓰셨나요?",
          "original_form": "그렇구나 가족분한테 편지는 쓰셨나요?",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012301.1.16",
          "form": "네 ㅋㅋ ㅋㅋ",
          "original_form": "네 ㅋㅋ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:44"
        },
        {
          "id": "MDRW2100012301.1.17",
          "form": "인터넷편지때",
          "original_form": "인터넷편지때",
          "speaker_id": "2",
          "time": "20210825 07:44"
        },
        {
          "id": "MDRW2100012301.1.18",
          "form": "썼어요",
          "original_form": "썼어요",
          "speaker_id": "2",
          "time": "20210825 07:44"
        },
        {
          "id": "MDRW2100012301.1.19",
          "form": "와 저는 한편도 안썼는데 부지런하시다",
          "original_form": "와 저는 한편도 안썼는데 부지런하시다",
          "speaker_id": "1",
          "time": "20210825 07:54"
        },
        {
          "id": "MDRW2100012301.1.20",
          "form": "ㅋㅋㅋㅋ그냥 한번씩",
          "original_form": "ㅋㅋㅋㅋ그냥 한번씩",
          "speaker_id": "2",
          "time": "20210825 08:21"
        },
        {
          "id": "MDRW2100012301.1.21",
          "form": "써줬어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "써줬어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:21"
        },
        {
          "id": "MDRW2100012301.1.22",
          "form": "시간 날때요",
          "original_form": "시간 날때요",
