      "utterance": [
        {
          "id": "MDRW2100030482.1.1",
          "form": "그래도채우곳사고싶어",
          "original_form": "그래도채우곳사고싶어",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030482.1.2",
          "form": "내일강남가는데 가을옷좀살까봐",
          "original_form": "내일강남가는데 가을옷좀살까봐",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030482.1.3",
          "form": "넘추어",
          "original_form": "넘추어",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030482.1.4",
          "form": "마자 따순옷피료햐",
          "original_form": "마자 따순옷피료햐",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030482.1.5",
          "form": "나 너무 여름옷만샀나봐",
          "original_form": "나 너무 여름옷만샀나봐",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030482.1.6",
          "form": "막상추워지니입을게없어",
          "original_form": "막상추워지니입을게없어",
          "speaker_id": "1",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030482.1.7",
          "form": "난 약간 여름은 포기하는 계절이러",
          "original_form": "난 약간 여름은 포기하는 계절이러",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030482.1.8",
          "form": "옷에 크게 신경못쓰게써",
          "original_form": "옷에 크게 신경못쓰게써",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030482.1.9",
          "form": "ㅋㅋㅋㅋㅋ가을이중요하지",
          "original_form": "ㅋㅋㅋㅋㅋ가을이중요하지",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030482.1.10",
          "form": "티셔츠 거의 대부분이고",
          "original_form": "티셔츠 거의 대부분이고",
          "speaker_id": "2",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030482.1.11",
          "form": "많이 힘쓰면 반팔셔츠정두...?",
          "original_form": "많이 힘쓰면 반팔셔츠정두...?",
          "speaker_id": "2",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030482.1.12",
          "form": "반바지 넘시람",
          "original_form": "반바지 넘시람",
          "speaker_id": "2",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030482.1.13",
          "form": "름",
          "original_form": "름",
          "speaker_id": "2",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030482.1.14",
          "form": "ㅋㅋㅋㄱ난 짧은 옷",
          "original_form": "ㅋㅋㅋㄱ난 짧은 옷",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030482.1.15",
          "form": "왜 웃도리를 다 크롭을해놨지",
          "original_form": "왜 웃도리를 다 크롭을해놨지",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030482.1.16",
          "form": "배를 소중히하고싶은사람도있는데",
          "original_form": "배를 소중히하고싶은사람도있는데",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030482.1.17",
          "form": "왜자꾸 배차갑게까고그래",
          "original_form": "왜자꾸 배차갑게까고그래",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030482.1.18",
          "form": "배아퍼그러다가",
          "original_form": "배아퍼그러다가",
          "speaker_id": "2",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030482.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030482.1.20",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030482.1.21",
          "form": "내배는 소중하다구",
          "original_form": "내배는 소중하다구",
          "speaker_id": "1",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030482.1.22",
          "form": "난 크게입는 유행좋어",
          "original_form": "난 크게입는 유행좋어",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030482.1.23",
          "form": "넘편해",
          "original_form": "넘편해",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030482.1.24",
          "form": "안바꼈음좋겠어",
          "original_form": "안바꼈음좋겠어",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030482.1.25",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030482.1.26",
          "form": "빅사이즈좋다예요",
          "original_form": "빅사이즈좋다예요",
          "speaker_id": "1",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030482.1.27",
          "form": "바지는 크게입는게 편하고",
          "original_form": "바지는 크게입는게 편하고",
          "speaker_id": "2",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030482.1.28",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030482.1.29",
          "form": "스키니시러",
          "original_form": "스키니시러",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030482.1.30",
          "form": "위에도 사실 큰게 편한데",
          "original_form": "위에도 사실 큰게 편한데",
          "speaker_id": "2",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030482.1.31",
          "form": "위는 그냥...적당한게 좋은듯",
          "original_form": "위는 그냥...적당한게 좋은듯",
          "speaker_id": "2",
          "time": "20210904 02:05"
        }
      ]
