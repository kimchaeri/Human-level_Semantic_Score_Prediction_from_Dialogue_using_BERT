        },
        {
          "id": "MDRW2100006161.1.2",
          "form": "남편갱생을 잔인하게하는...내용?",
          "original_form": "남편갱생을 잔인하게하는...내용?",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006161.1.3",
          "form": "오노우",
          "original_form": "오노우",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006161.1.4",
          "form": "잼나겠네요 ㅎㅎ",
          "original_form": "잼나겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006161.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006161.1.6",
          "form": "근데 내 남편과 결혼해줘 도",
          "original_form": "근데 내 남편과 결혼해줘 도",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006161.1.7",
          "form": "죽고나서 과거로 돌아오는건데 이정도명 그녕 로맨스로",
          "original_form": "죽고나서 과거로 돌아오는건데 이정도명 그녕 로맨스로",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006161.1.8",
          "form": "분류되나요?",
          "original_form": "분류되나요?",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006161.1.9",
          "form": "로판은 어느정도가 로판인가요?",
          "original_form": "로판은 어느정도가 로판인가요?",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006161.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006161.1.11",
          "form": "현대물은 다 로맨스",
          "original_form": "현대물은 다 로맨스",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006161.1.12",
          "form": "로판은 서양물..?",
          "original_form": "로판은 서양물..?",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006161.1.13",
          "form": "아항~~~",
          "original_form": "아항~~~",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006161.1.14",
          "form": "현대물을 그냥 다 로맨스로 분류하는군요!",
          "original_form": "현대물을 그냥 다 로맨스로 분류하는군요!",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006161.1.15",
          "form": "새로운 사실을 알고가네요!",
          "original_form": "새로운 사실을 알고가네요!",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006161.1.16",
          "form": "당신의 이해를 돕기 위해서는 언젠가 꼭 봐야게ㅛ어요",
          "original_form": "당신의 이해를 돕기 위해서는 언젠가 꼭 봐야게ㅛ어요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006161.1.17",
          "form": "추천 하는 사람이 많네요!",
          "original_form": "추천 하는 사람이 많네요!",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006161.1.18",
          "form": "ㅋㅋㅌㅋㅋㅋㅋ꼭!",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋ꼭!",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006161.1.19",
          "form": "다른 재미난것도 많지만",
          "original_form": "다른 재미난것도 많지만",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006161.1.20",
          "form": "정말 너무 존잼이에요",
          "original_form": "정말 너무 존잼이에요",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006161.1.21",
          "form": "넵 또 시리즈에서는 어떤거 제일 재밌게 보셯어요",
          "original_form": "넵 또 시리즈에서는 어떤거 제일 재밌게 보셯어요",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006161.1.22",
          "form": "시리즈",
          "original_form": "시리즈",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006161.1.23",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006161.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006161.1.25",
          "form": "시리즈 안깔려고했다가",
          "original_form": "시리즈 안깔려고했다가",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006161.1.26",
          "form": "깔게된",
          "original_form": "깔게된",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006161.1.27",
          "form": "소설이있어요",
          "original_form": "소설이있어요",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006161.1.28",
          "form": "파멸 예정 연하 남주가 내게 빠졌다",
          "original_form": "파멸 예정 연하 남주가 내게 빠졌다",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006161.1.29",
          "form": "원래 네이버 연재작이었ㄴ느데",
          "original_form": "원래 네이버 연재작이었ㄴ느데",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006161.1.30",
          "form": "그니까 베스트소설 연재작",
          "original_form": "그니까 베스트소설 연재작",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006161.1.31",
          "form": "홀라당 네이버가 델고가는바람에",
          "original_form": "홀라당 네이버가 델고가는바람에",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006161.1.32",
          "form": "따라왔어요",
          "original_form": "따라왔어요",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006161.1.33",
          "form": "넘 내용알고싶은데 ㅠㅠ",
          "original_form": "넘 내용알고싶은데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006161.1.34",
          "form": "이러면서",
          "original_form": "이러면서",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006161.1.35",
          "form": "헐 뭔가 제목만 봤을때 완전 제취향이에여",
          "original_form": "헐 뭔가 제목만 봤을때 완전 제취향이에여",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006161.1.36",
          "form": "웹소설 맛집 잘알아시군요 name1",
          "original_form": "웹소설 맛집 잘알아시군요 &name1&",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006161.1.37",
          "form": "ㅋㅋㅋ많이봐서..?",
          "original_form": "ㅋㅋㅋ많이봐서..?",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006161.1.38",
          "form": "저도 맛집을 먹어봐여겠어요",
          "original_form": "저도 맛집을 먹어봐여겠어요",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006161.1.39",
          "form": "파멸예정 연하남주 넘 궁금해요 저",
          "original_form": "파멸예정 연하남주 넘 궁금해요 저",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006161.1.40",
          "form": "ㅋㅋㅋㅋㅋ 꼭 보셔요 정말 재미나오 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 꼭 보셔요 정말 재미나오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006161.1.41",
          "form": "앟겠습니다!!",
          "original_form": "앟겠습니다!!",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006161.1.42",
          "form": "웹소설 추천 너무 좋네여",
          "original_form": "웹소설 추천 너무 좋네여",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006161.1.43",
          "form": "ㅎㅎㄹ 좋아요!",
          "original_form": "ㅎㅎㄹ 좋아요!",
