        },
        {
          "id": "MDRW2100029196.1.2",
          "form": "그 퀴즈 일정 정한거보면",
          "original_form": "그 퀴즈 일정 정한거보면",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029196.1.3",
          "form": "대면일때 조별과제하면",
          "original_form": "대면일때 조별과제하면",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029196.1.4",
          "form": "일정 어케맞출까 걱정됨",
          "original_form": "일정 어케맞출까 걱정됨",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029196.1.5",
          "form": "어 맞음...",
          "original_form": "어 맞음...",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029196.1.6",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029196.1.7",
          "form": "그런거 시간 안맞아도",
          "original_form": "그런거 시간 안맞아도",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029196.1.8",
          "form": "ㅎ...",
          "original_form": "ㅎ...",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029196.1.9",
          "form": "각자 맡은 일 열심히 하면 되는데",
          "original_form": "각자 맡은 일 열심히 하면 되는데",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029196.1.10",
          "form": "하질 않아요 인간들이",
          "original_form": "하질 않아요 인간들이",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029196.1.11",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029196.1.12",
          "form": "1인분이라도 해라",
          "original_form": "1인분이라도 해라",
          "speaker_id": "1",
          "time": "20210903 00:14"
