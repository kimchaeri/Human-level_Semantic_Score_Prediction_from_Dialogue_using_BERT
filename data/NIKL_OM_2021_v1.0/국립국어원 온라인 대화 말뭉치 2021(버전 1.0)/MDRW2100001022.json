      "utterance": [
        {
          "id": "MDRW2100001022.1.1",
          "form": "결혼은 하셨나요?ㅎ",
          "original_form": "결혼은 하셨나요?ㅎ",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001022.1.2",
          "form": "네ㅎ했어요",
          "original_form": "네ㅎ했어요",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001022.1.3",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001022.1.4",
          "form": "네~ 저는 애둘있어용",
          "original_form": "네~ 저는 애둘있어용",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100001022.1.5",
          "form": "애키우기가 일하는것보다 정말 힘들어요ㅜ",
          "original_form": "애키우기가 일하는것보다 정말 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100001022.1.6",
          "form": "진짜요ㅠ애보는것보다 일하는게 나은것같아여ㅠ",
          "original_form": "진짜요ㅠ애보는것보다 일하는게 나은것같아여ㅠ",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100001022.1.7",
          "form": "맞아요. 집안일까지 하려니 힘들어요ㅜ",
          "original_form": "맞아요. 집안일까지 하려니 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100001022.1.8",
          "form": "ㅠㅠ 해도티도 안나고ㅠㅠ",
          "original_form": "ㅠㅠ 해도티도 안나고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100001022.1.9",
          "form": "해도 진짜 티가 안나고 힘들죠ㅠ",
          "original_form": "해도 진짜 티가 안나고 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100001022.1.10",
          "form": "네ㅠ그래서 저는 기본만ㅋㅋ해요",
          "original_form": "네ㅠ그래서 저는 기본만ㅋㅋ해요",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100001022.1.11",
          "form": "저두요ㅎㅎ눈에보이는것만요ㅎ",
          "original_form": "저두요ㅎㅎ눈에보이는것만요ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100001022.1.12",
          "form": "유지만 잘하면 되죠뭐ㅋ",
          "original_form": "유지만 잘하면 되죠뭐ㅋ",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100001022.1.13",
          "form": "유지가 제일 어려워요ㅎ",
          "original_form": "유지가 제일 어려워요ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100001022.1.14",
          "form": "맞아요~  ㅋㅋㅋ 집밥. 다 차려먹고 그정도만 해도 ㅋㅋ",
          "original_form": "맞아요~  ㅋㅋㅋ 집밥. 다 차려먹고 그정도만 해도 ㅋㅋ",
          "speaker_id": "1",
