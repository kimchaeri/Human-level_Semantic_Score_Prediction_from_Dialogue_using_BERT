        },
        {
          "id": "MDRW2100022171.1.2",
          "form": "자주 하시는 게임이 있나요?",
          "original_form": "자주 하시는 게임이 있나요?",
          "speaker_id": "1",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100022171.1.3",
          "form": "무한의 계단",
          "original_form": "무한의 계단",
          "speaker_id": "2",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100022171.1.4",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100022171.1.5",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "1",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100022171.1.6",
          "form": "name3 아버님",
          "original_form": "&name3& 아버님",
          "speaker_id": "2",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.7",
          "form": "네 ㅋㅋㅋname3 아빠입니다",
          "original_form": "네 ㅋㅋㅋ&name3& 아빠입니다",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.8",
          "form": "게임 안에서 할 말이 없는데요",
          "original_form": "게임 안에서 할 말이 없는데요",
          "speaker_id": "2",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.9",
          "form": "name3도 크면 저거 하겠죠",
          "original_form": "&name3&도 크면 저거 하겠죠",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.10",
          "form": "조금 이야기하다가",
          "original_form": "조금 이야기하다가",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.11",
          "form": "저것만 할까요",
          "original_form": "저것만 할까요",
          "speaker_id": "2",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.12",
          "form": "혹시 피카추 배구아세요?",
          "original_form": "혹시 피카추 배구아세요?",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.13",
          "form": "옛날 게임인데",
          "original_form": "옛날 게임인데",
          "speaker_id": "1",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022171.1.14",
          "form": "아니요 그런 거 모릅니다만",
          "original_form": "아니요 그런 거 모릅니다만",
          "speaker_id": "2",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022171.1.15",
          "form": "꿈이 프로 게이머세요?",
          "original_form": "꿈이 프로 게이머세요?",
          "speaker_id": "2",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022171.1.16",
          "form": "무한계단은 모바일로 하는거죠?",
          "original_form": "무한계단은 모바일로 하는거죠?",
          "speaker_id": "1",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022171.1.17",
          "form": "내 네 폰으로 한답니다",
          "original_form": "내 네 폰으로 한답니다",
          "speaker_id": "2",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022171.1.18",
          "form": "아뇨 돈 많이 버는게 꿈입니다",
          "original_form": "아뇨 돈 많이 버는게 꿈입니다",
          "speaker_id": "1",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022171.1.19",
          "form": "손가락이 튼튼해질 거 같은 게임이에요",
          "original_form": "손가락이 튼튼해질 거 같은 게임이에요",
