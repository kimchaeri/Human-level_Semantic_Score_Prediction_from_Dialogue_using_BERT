        },
        {
          "id": "MDRW2100026815.1.2",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026815.1.3",
          "form": "돼지고기보다 닭이 비싼게",
          "original_form": "돼지고기보다 닭이 비싼게",
          "speaker_id": "1",
          "time": "20210902 00:20"
