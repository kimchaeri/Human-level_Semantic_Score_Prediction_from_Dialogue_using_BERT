        {
          "id": "MDRW2100007841.1.1",
          "form": "동생에 휴가 일정이 넘 늦게 정해지는구나",
          "original_form": "동생에 휴가 일정이 넘 늦게 정해지는구나",
          "speaker_id": "1",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007841.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007841.1.3",
          "form": "내일 휴가잖아",
          "original_form": "내일 휴가잖아",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007841.1.4",
          "form": "넘 늧어서 이제 드디어 첫휴가 나오네",
          "original_form": "넘 늧어서 이제 드디어 첫휴가 나오네",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007841.1.5",
          "form": "그니까 .. ㅋㅋㅋ",
          "original_form": "그니까 .. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007841.1.6",
          "form": "길게 나오니깐 좋으네",
          "original_form": "길게 나오니깐 좋으네",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007841.1.7",
          "form": "뭐 먹고싶어 하려나?",
          "original_form": "뭐 먹고싶어 하려나?",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007841.1.8",
          "form": "엄마가해준 닭 갈비 아닐까 ?",
          "original_form": "엄마가해준 닭 갈비 아닐까 ?",
          "speaker_id": "1",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007841.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ내일 닭갈비 먹어?우리",
          "original_form": "ㅋㅋㅋㅋㅋㅋ내일 닭갈비 먹어?우리",
          "speaker_id": "2",
          "time": "20210822 12:45"
        },
        {
          "id": "MDRW2100007841.1.10",
          "form": "그러게 name3이가 별로라고 할까 ?",
          "original_form": "그러게 &name3&이가 별로라고 할까 ?",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100007841.1.11",
          "form": "아니 좋아하겠지",
          "original_form": "아니 좋아하겠지",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100007841.1.12",
          "form": "그냥 먹고싶은거 다른거 사주깔 ?",
          "original_form": "그냥 먹고싶은거 다른거 사주깔 ?",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100007841.1.13",
          "form": "그것도 괜찮긴 하다!",
          "original_form": "그것도 괜찮긴 하다!",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100007841.1.14",
          "form": "치킨 같은 것도 좋구",
