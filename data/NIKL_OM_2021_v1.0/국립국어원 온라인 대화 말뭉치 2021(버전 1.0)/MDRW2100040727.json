      "utterance": [
        {
          "id": "MDRW2100040727.1.1",
          "form": "창업 되게 커보이는 단언데요",
          "original_form": "창업 되게 커보이는 단언데요",
          "speaker_id": "2",
          "time": "20210905 00:56"
        },
        {
          "id": "MDRW2100040727.1.2",
          "form": "친구가 창업 안 해보고 창업 가르치는 친구 있거든요",
          "original_form": "친구가 창업 안 해보고 창업 가르치는 친구 있거든요",
          "speaker_id": "1",
          "time": "20210905 00:56"
        },
        {
          "id": "MDRW2100040727.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 네??ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:57"
        },
        {
          "id": "MDRW2100040727.1.4",
          "form": "선생님이면 그럴수도 있긴 하죠 뭐",
          "original_form": "선생님이면 그럴수도 있긴 하죠 뭐",
          "speaker_id": "2",
          "time": "20210905 00:57"
        },
        {
          "id": "MDRW2100040727.1.5",
          "form": "그럼 컨설턴트 쪽으로 그런 사짜 들 많은 거 같아요",
          "original_form": "그럼 컨설턴트 쪽으로 그런 사짜 들 많은 거 같아요",
          "speaker_id": "1",
          "time": "20210905 00:57"
        },
        {
          "id": "MDRW2100040727.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:57"
        },
        {
          "id": "MDRW2100040727.1.7",
          "form": "그게 참",
          "original_form": "그게 참",
          "speaker_id": "2",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100040727.1.8",
          "form": "그래도 듣는 분 많으신가보다",
          "original_form": "그래도 듣는 분 많으신가보다",
          "speaker_id": "2",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100040727.1.9",
          "form": "약간 간절한 분들도 있을거 같아요",
          "original_form": "약간 간절한 분들도 있을거 같아요",
          "speaker_id": "2",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100040727.1.10",
          "form": "그냥 창업 대학원 가서 학위따고 그걸로 천문대 같은데 강의 하더라고",
          "original_form": "그냥 창업 대학원 가서 학위따고 그걸로 천문대 같은데 강의 하더라고",
          "speaker_id": "1",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100040727.1.11",
          "form": "전문대",
          "original_form": "전문대",
          "speaker_id": "1",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100040727.1.12",
          "form": "그러고 나라에서 그런 컨설팅 비용을 지원해 줘서요",
          "original_form": "그러고 나라에서 그런 컨설팅 비용을 지원해 줘서요",
          "speaker_id": "1",
          "time": "20210905 00:59"
        },
        {
          "id": "MDRW2100040727.1.13",
          "form": "뭐 교수도 아닌데",
          "original_form": "뭐 교수도 아닌데",
          "speaker_id": "2",
          "time": "20210905 00:59"
        },
        {
          "id": "MDRW2100040727.1.14",
          "form": "괜찮은가 싶기도 하네욬ㅋㅋㅋ",
          "original_form": "괜찮은가 싶기도 하네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:59"
        },
        {
          "id": "MDRW2100040727.1.15",
          "form": "그런게 그런 업자들이 다 로비에서 세금 털어 먹는 거 같더라고요",
          "original_form": "그런게 그런 업자들이 다 로비에서 세금 털어 먹는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 00:59"
        },
        {
          "id": "MDRW2100040727.1.16",
          "form": "진짜 그걸로 창업하는 분이 있을까 하는 생각은 드는데",
          "original_form": "진짜 그걸로 창업하는 분이 있을까 하는 생각은 드는데",
          "speaker_id": "2",
          "time": "20210905 00:59"
        },
        {
          "id": "MDRW2100040727.1.17",
          "form": "아 새롭네요",
          "original_form": "아 새롭네요",
          "speaker_id": "2",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100040727.1.18",
          "form": "나라에서 하는 교육 같은게 진짜 시간 때워 먹기식으로 하는 거 많아요",
          "original_form": "나라에서 하는 교육 같은게 진짜 시간 때워 먹기식으로 하는 거 많아요",
          "speaker_id": "1",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100040727.1.19",
          "form": "이제 세금이 그런곳에서 새네요",
          "original_form": "이제 세금이 그런곳에서 새네요",
          "speaker_id": "2",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100040727.1.20",
          "form": "어차피 그거 듣는 사람 그걸로 창업 안 하고 강사만 배불러요",
          "original_form": "어차피 그거 듣는 사람 그걸로 창업 안 하고 강사만 배불러요",
          "speaker_id": "1",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100040727.1.21",
          "form": "성교육 그런거 진짜 심하잖아요",
          "original_form": "성교육 그런거 진짜 심하잖아요",
          "speaker_id": "2",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100040727.1.22",
          "form": "컨텐츠 만드는 회사 그런걸로 돈 엄청 벌더라고요",
          "original_form": "컨텐츠 만드는 회사 그런걸로 돈 엄청 벌더라고요",
          "speaker_id": "2",
          "time": "20210905 01:00"
        }
      ]
    }
