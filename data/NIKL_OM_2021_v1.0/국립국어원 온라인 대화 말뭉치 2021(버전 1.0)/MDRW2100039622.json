      "utterance": [
        {
          "id": "MDRW2100039622.1.1",
          "form": "아직 근무중이신거예요????",
          "original_form": "아직 근무중이신거예요????",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039622.1.2",
          "form": "전 오리주물럭해먹었어요ㅎㅎ",
          "original_form": "전 오리주물럭해먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039622.1.3",
          "form": "미용실에서요ㅜㅜ",
          "original_form": "미용실에서요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039622.1.4",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039622.1.5",
          "form": "아직멀엇어욤",
          "original_form": "아직멀엇어욤",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039622.1.6",
          "form": "파마끝나구 언능 맛잇는거 먹고시퍼여",
          "original_form": "파마끝나구 언능 맛잇는거 먹고시퍼여",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039622.1.7",
          "form": "아 맞다!",
          "original_form": "아 맞다!",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039622.1.8",
          "form": "ㅋㅋ 세상에 고새!!! ㅋㅋ 파마 세시간쯤 걸\r\n리죠??",
          "original_form": "ㅋㅋ 세상에 고새!!! ㅋㅋ 파마 세시간쯤 걸\r\n리죠??",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039622.1.9",
          "form": "뭐드실거예요???",
          "original_form": "뭐드실거예요???",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039622.1.10",
          "form": "ㅜㅜ네 세시간ㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜ네 세시간ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039622.1.11",
          "form": "집에가서 아무거나 닥치는데로 먹을거에유",
          "original_form": "집에가서 아무거나 닥치는데로 먹을거에유",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039622.1.12",
          "form": "넘나배고픈것",
          "original_form": "넘나배고픈것",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039622.1.13",
          "form": "요즘은 미용실에서 먹을거 안줘서 속상ㅜ",
          "original_form": "요즘은 미용실에서 먹을거 안줘서 속상ㅜ",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039622.1.14",
          "form": "아ㅜㅜ",
          "original_form": "아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039622.1.15",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039622.1.16",
          "form": "오리주물럭",
          "original_form": "오리주물럭",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039622.1.17",
          "form": "장금이시네요 ㅜㅜ",
          "original_form": "장금이시네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039622.1.18",
          "form": "직접 만들었어요!!! ㅋㅋ",
          "original_form": "직접 만들었어요!!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039622.1.19",
          "form": "이야~~~",
          "original_form": "이야~~~",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039622.1.20",
          "form": "요즘은 오리고기사면 손질해줘요",
          "original_form": "요즘은 오리고기사면 손질해줘요",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
