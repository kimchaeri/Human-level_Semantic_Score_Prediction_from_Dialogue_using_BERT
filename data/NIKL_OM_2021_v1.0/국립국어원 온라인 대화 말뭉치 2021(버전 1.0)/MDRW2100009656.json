      "utterance": [
        {
          "id": "MDRW2100009656.1.1",
          "form": "넷플릭스 가입했어?",
          "original_form": "넷플릭스 가입했어?",
          "speaker_id": "1",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009656.1.2",
          "form": "응 가입했어",
          "original_form": "응 가입했어",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009656.1.3",
          "form": "너는 가입했어?",
          "original_form": "너는 가입했어?",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009656.1.4",
          "form": "나도 최근에 가입했어~",
          "original_form": "나도 최근에 가입했어~",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009656.1.5",
          "form": "나는 애니메이션 많이봐!",
          "original_form": "나는 애니메이션 많이봐!",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009656.1.6",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009656.1.7",
          "form": "나는 다큐멘터리 자주 봐",
          "original_form": "나는 다큐멘터리 자주 봐",
          "speaker_id": "2",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009656.1.8",
          "form": "어떤 애니메이션 봐?",
          "original_form": "어떤 애니메이션 봐?",
          "speaker_id": "2",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009656.1.9",
          "form": "최근엔 목소리의 형태라는 애니를 봤어",
          "original_form": "최근엔 목소리의 형태라는 애니를 봤어",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009656.1.10",
          "form": "리틀 포레스트도!",
          "original_form": "리틀 포레스트도!",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009656.1.11",
          "form": "목소리의 형태는 봤어",
          "original_form": "목소리의 형태는 봤어",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009656.1.12",
          "form": "어땠어? 감상!",
          "original_form": "어땠어? 감상!",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009656.1.13",
          "form": "재밌지 않아?",
          "original_form": "재밌지 않아?",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009656.1.14",
          "form": "색채미가 좋더라고",
          "original_form": "색채미가 좋더라고",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009656.1.15",
          "form": "응 재밌더라",
          "original_form": "응 재밌더라",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009656.1.16",
          "form": "맞아 색이 예뻤어",
          "original_form": "맞아 색이 예뻤어",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009656.1.17",
          "form": "학교폭력은 정말 없어져야할 것 같아",
          "original_form": "학교폭력은 정말 없어져야할 것 같아",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009656.1.18",
          "form": "응 학교폭력은 끔찍해",
          "original_form": "응 학교폭력은 끔찍해",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009656.1.19",
          "form": "맞아 ㅠㅠㅠ 정말 사라졌으면 좋겠어",
          "original_form": "맞아 ㅠㅠㅠ 정말 사라졌으면 좋겠어",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009656.1.20",
          "form": "넷플 주로 뭐봐?",
          "original_form": "넷플 주로 뭐봐?",
          "speaker_id": "1",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009656.1.21",
          "form": "나는 최근에 테일러 스위프트 공연 영상 봤어",
          "original_form": "나는 최근에 테일러 스위프트 공연 영상 봤어",
          "speaker_id": "2",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009656.1.22",
          "form": "오 나도 그거 예전에 봣어",
          "original_form": "오 나도 그거 예전에 봣어",
          "speaker_id": "1",
          "time": "20210823 02:41"
        }
      ]
