      "utterance": [
        {
          "id": "MDRW2100039688.1.1",
          "form": "찹쌀탕수육좋아하세요?!!",
          "original_form": "찹쌀탕수육좋아하세요?!!",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039688.1.2",
          "form": "꿔바로우랑 비슷해요!",
          "original_form": "꿔바로우랑 비슷해요!",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039688.1.3",
          "form": "네! 오 맛있을거 같아요",
          "original_form": "네! 오 맛있을거 같아요",
          "speaker_id": "2",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039688.1.4",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039688.1.5",
          "form": "ㅎㅎname2님은 뭐시키셨어요?!",
          "original_form": "ㅎㅎ&name2&님은 뭐시키셨어요?!",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039688.1.6",
          "form": "한 번 먹어봐야겠어요ㅎㅎ",
          "original_form": "한 번 먹어봐야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039688.1.7",
          "form": "저는 오랜만에 돈까스 주문했어요!",
          "original_form": "저는 오랜만에 돈까스 주문했어요!",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039688.1.8",
          "form": "저오늘 돈까스먹을까 엄청고민했는데!!",
          "original_form": "저오늘 돈까스먹을까 엄청고민했는데!!",
          "speaker_id": "1",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039688.1.9",
          "form": "엇 같은 메뉴였을수도 있었네요ㅋㅋㅋ",
          "original_form": "엇 같은 메뉴였을수도 있었네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039688.1.10",
          "form": "저 진짜 오랜만에 돈까스 먹었어요ㅎㅎ",
          "original_form": "저 진짜 오랜만에 돈까스 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100039688.1.11",
          "form": "부먹이세요?찍먹이세요?",
          "original_form": "부먹이세요?찍먹이세요?",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100039688.1.12",
          "form": "전 찍먹이요",
          "original_form": "전 찍먹이요",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100039688.1.13",
          "form": "저는 경양식돈까스 부먹 환장해요",
          "original_form": "저는 경양식돈까스 부먹 환장해요",
          "speaker_id": "1",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039688.1.14",
          "form": "오 부먹파시군요! 그럼 탕수육 드실때도 부먹인가요?",
          "original_form": "오 부먹파시군요! 그럼 탕수육 드실때도 부먹인가요?",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039688.1.15",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039688.1.16",
          "form": "촉촉한게",
          "original_form": "촉촉한게",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039688.1.17",
          "form": "좋더라고요ㅎㅎ",
          "original_form": "좋더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039688.1.18",
          "form": "오 촉촉한거 좋아하시면 부먹이죠!\r\n저는ㅈ바삭바삭함에 소스 살짝 찍어먹는거 좋아해요ㅎㅎ",
          "original_form": "오 촉촉한거 좋아하시면 부먹이죠!\r\n저는ㅈ바삭바삭함에 소스 살짝 찍어먹는거 좋아해요ㅎㅎ",
