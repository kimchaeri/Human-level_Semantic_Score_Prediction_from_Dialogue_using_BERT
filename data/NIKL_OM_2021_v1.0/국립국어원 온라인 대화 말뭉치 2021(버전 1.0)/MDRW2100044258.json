      "utterance": [
        {
          "id": "MDRW2100044258.1.1",
          "form": "아 괜찮아요!",
          "original_form": "아 괜찮아요!",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044258.1.2",
          "form": "ㅎㅎ 오늘은 가을 치마를 입었는데요",
          "original_form": "ㅎㅎ 오늘은 가을 치마를 입었는데요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044258.1.3",
          "form": "출근하니 아직 다들 여름이셔서 조금 쑥쓰러웠어요",
          "original_form": "출근하니 아직 다들 여름이셔서 조금 쑥쓰러웠어요",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044258.1.4",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044258.1.5",
          "form": "전 오늘 긴팔 셔츠 입고 갔는데",
          "original_form": "전 오늘 긴팔 셔츠 입고 갔는데",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044258.1.6",
          "form": "덥지 않더라고요",
          "original_form": "덥지 않더라고요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044258.1.7",
          "form": "다들 여름 슬랙스에 조리에 반팔이셨어요",
          "original_form": "다들 여름 슬랙스에 조리에 반팔이셨어요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044258.1.8",
          "form": "와 다들 여름이시네요",
          "original_form": "와 다들 여름이시네요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044258.1.9",
          "form": "네 다들 청춘이셔...저는 추웠는데요ㅋㅋ",
          "original_form": "네 다들 청춘이셔...저는 추웠는데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044258.1.10",
          "form": "저두 쌀쌀하던데",
          "original_form": "저두 쌀쌀하던데",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044258.1.11",
          "form": "저도 내일은 긴셔츠입으려고요",
          "original_form": "저도 내일은 긴셔츠입으려고요",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044258.1.12",
          "form": "네 따뜻하게 입어야해요",
          "original_form": "네 따뜻하게 입어야해요",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044258.1.13",
          "form": "name2님도요 감기걸림 큰일이니까요",
          "original_form": "&name2&님도요 감기걸림 큰일이니까요",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044258.1.14",
          "form": "겨울 오기전에 겨울옷을 꺼내야하는데",
          "original_form": "겨울 오기전에 겨울옷을 꺼내야하는데",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044258.1.15",
          "form": "엄두가안나요ㅋㅋ",
          "original_form": "엄두가안나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044258.1.16",
          "form": "아아 저두 거기까진 엄두가...",
          "original_form": "아아 저두 거기까진 엄두가...",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044258.1.17",
          "form": "계속 다음 주말로 미루고 있어요ㅋㅋ",
          "original_form": "계속 다음 주말로 미루고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044258.1.18",
          "form": "ㅋㅋㅋ여름옷 넣을 마음의 준비도 안됐어요",
          "original_form": "ㅋㅋㅋ여름옷 넣을 마음의 준비도 안됐어요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044258.1.19",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
