      "utterance": [
        {
          "id": "MDRW2100015565.1.1",
          "form": "name1님은 name1 음료 조아하시나용",
          "original_form": "&name1&님은 &name1& 음료 조아하시나용",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015565.1.2",
          "form": "네ㅎㅎ name1 음료 좋아해서 아메리카도 얼죽아에요ㅋㅋ",
          "original_form": "네ㅎㅎ &name1& 음료 좋아해서 아메리카도 얼죽아에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015565.1.3",
          "form": "저두요! 얼음 너무 좋아요",
          "original_form": "저두요! 얼음 너무 좋아요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015565.1.4",
          "form": "전 무조건 아아메",
          "original_form": "전 무조건 아아메",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015565.1.5",
          "form": "오호ㅎㅎ 막 아아메 먹다가 그냥 커피먹으면",
          "original_form": "오호ㅎㅎ 막 아아메 먹다가 그냥 커피먹으면",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015565.1.6",
          "form": "더 달고 맛있긴해도 갈증이 안풀리는 그론게 있어욬ㅋㅋ",
          "original_form": "더 달고 맛있긴해도 갈증이 안풀리는 그론게 있어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015565.1.7",
          "form": "맞아요 시켜놓고 아 아아메 한잔 더 마실까 생각하게 되는..",
          "original_form": "맞아요 시켜놓고 아 아아메 한잔 더 마실까 생각하게 되는..",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015565.1.8",
          "form": "입에 뭔가 남는 느낌이라 깔끔하지가 않아요ㅠ",
          "original_form": "입에 뭔가 남는 느낌이라 깔끔하지가 않아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015565.1.9",
          "form": "마자요ㅋㅋ",
          "original_form": "마자요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015565.1.10",
          "form": "특히 점심먹고 졸릴때",
          "original_form": "특히 점심먹고 졸릴때",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015565.1.11",
          "form": "아아메 딱 한모금마시면",
          "original_form": "아아메 딱 한모금마시면",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015565.1.12",
          "form": "약간 반쯤잠들어있다가 깨어나는 그 느낌 ㅋㅋ",
          "original_form": "약간 반쯤잠들어있다가 깨어나는 그 느낌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015565.1.13",
          "form": "name1님 얘기대로 입도 막 개운해지고",
          "original_form": "&name1&님 얘기대로 입도 막 개운해지고",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015565.1.14",
          "form": "맞아요맞아요!",
          "original_form": "맞아요맞아요!",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015565.1.15",
          "form": "특히 수요일 목요일엔",
          "original_form": "특히 수요일 목요일엔",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015565.1.16",
          "form": "커피필수ㅠㅠ",
          "original_form": "커피필수ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015565.1.17",
          "form": "헐 진짜인정이요ㅜㅜ 딱 수목에 아아메가 거의 뭐 생명수ㅋㅋ",
          "original_form": "헐 진짜인정이요ㅜㅜ 딱 수목에 아아메가 거의 뭐 생명수ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015565.1.18",
          "form": "근데 가아끔 이제",
          "original_form": "근데 가아끔 이제",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015565.1.19",
          "form": "바닐라 라떼 이런거 먹어요 먹고 후회ㅋㅋㅋ",
          "original_form": "바닐라 라떼 이런거 먹어요 먹고 후회ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015565.1.20",
          "form": "저 오늘 그랬어요",
          "original_form": "저 오늘 그랬어요",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015565.1.21",
          "form": "아아메 말고는 어떤 음료 좋아하세요??",
          "original_form": "아아메 말고는 어떤 음료 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015565.1.22",
          "form": "스벅 바크콜 아세요? 그거 먹고",
          "original_form": "스벅 바크콜 아세요? 그거 먹고",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015565.1.23",
          "form": "아아메 마실걸 했어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아메 마실걸 했어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015565.1.24",
          "form": "저 그냥 탄산수도 좋아하고.. 너무 단 음료나 우유 들어간건 잘 안마셔요",
          "original_form": "저 그냥 탄산수도 좋아하고.. 너무 단 음료나 우유 들어간건 잘 안마셔요",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015565.1.25",
          "form": "바크콜이 커피종류인가용?",
          "original_form": "바크콜이 커피종류인가용?",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100015565.1.26",
          "form": "마자요 우유 들어간건 저도 별로더라구요",
          "original_form": "마자요 우유 들어간건 저도 별로더라구요",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100015565.1.27",
          "form": "name1님 얘기처럼 너무 단것고 그렇고",
          "original_form": "&name1&님 얘기처럼 너무 단것고 그렇고",
          "speaker_id": "2",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100015565.1.28",
          "form": "막 인스타 카페같은데 보면 마시멜로우 올릭ㅎ",
          "original_form": "막 인스타 카페같은데 보면 마시멜로우 올릭ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100015565.1.29",
          "form": "올리고",
          "original_form": "올리고",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100015565.1.30",
          "form": "보기 예쁜애들 있잖아요 그런것도 예쁜거에 혹해서 주문했다가",
          "original_form": "보기 예쁜애들 있잖아요 그런것도 예쁜거에 혹해서 주문했다가",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100015565.1.31",
          "form": "후회하고 나서는 아아메로 돌아와요 ㅋㅋ",
          "original_form": "후회하고 나서는 아아메로 돌아와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100015565.1.32",
          "form": "혹시 음식음 어떤거 좋아하세요?",
          "original_form": "혹시 음식음 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100015565.1.33",
          "form": "네네 바닐라 크림 콜드브루 이거예요!",
          "original_form": "네네 바닐라 크림 콜드브루 이거예요!",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100015565.1.34",
          "form": "바닐라라떼보단 덜 단",
          "original_form": "바닐라라떼보단 덜 단",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100015565.1.35",
          "form": "전 밀가루 좋아해요! 모든 밀가루",
          "original_form": "전 밀가루 좋아해요! 모든 밀가루{emoji:♡}",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100015565.1.36",
          "form": "아 콜이 콜드브루였구나ㅋㅋ 전 막 콜라 이런건가?이랬어요ㅋㅋ",
          "original_form": "아 콜이 콜드브루였구나ㅋㅋ 전 막 콜라 이런건가?이랬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100015565.1.37",
          "form": "그럼 빵 좋아하세요??",
          "original_form": "그럼 빵 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100015565.1.38",
          "form": "전 요즘 크루아쌍이랑 이런빵종류에 빠져있어욬ㅋ",
          "original_form": "전 요즘 크루아쌍이랑 이런빵종류에 빠져있어욬ㅋ",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100015565.1.39",
          "form": "네넵 당연하져ㅠㅠ빵완전조아용",
          "original_form": "네넵 당연하져ㅠㅠ빵완전조아용",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100015565.1.40",
          "form": "저도 크루아상좋아해요!",
          "original_form": "저도 크루아상좋아해요!",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100015565.1.41",
          "form": "오! 크루아상에도 근데 종류가 디게 많지않나요?",
          "original_form": "오! 크루아상에도 근데 종류가 디게 많지않나요?",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100015565.1.42",
          "form": "전 막연히 아무것도 안들고 기본 크루아상만 먹어보다가",
          "original_form": "전 막연히 아무것도 안들고 기본 크루아상만 먹어보다가",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100015565.1.43",
          "form": "전문점있길래 가봤더니 위에 뿌리는것도 엄청 다양하고",
          "original_form": "전문점있길래 가봤더니 위에 뿌리는것도 엄청 다양하고",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100015565.1.44",
          "form": "안에 크림도 들어있고 그래서 신세계였어요ㅋㅋ",
          "original_form": "안에 크림도 들어있고 그래서 신세계였어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:59"
        }
      ]
