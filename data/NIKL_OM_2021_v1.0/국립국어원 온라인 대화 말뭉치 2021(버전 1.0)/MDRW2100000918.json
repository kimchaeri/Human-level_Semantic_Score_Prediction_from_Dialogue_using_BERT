      "utterance": [
        {
          "id": "MDRW2100000918.1.1",
          "form": "오늘 폰케이스 산 거 벌써 왔다",
          "original_form": "오늘 폰케이스 산 거 벌써 왔다",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000918.1.2",
          "form": "아 요즘 당근마켓",
          "original_form": "아 요즘 당근마켓",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000918.1.3",
          "form": "당근마켓에 살 거 있나",
          "original_form": "당근마켓에 살 거 있나",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000918.1.4",
          "form": "폰케이스?",
          "original_form": "폰케이스?",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000918.1.5",
          "form": "폰은 없고 폰케이스만 있겠네",
          "original_form": "폰은 없고 폰케이스만 있겠네",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000918.1.6",
          "form": "어ㅠ 폰폰이다 완전.. 빨리 왓으면 폰!",
          "original_form": "어ㅠ 폰폰이다 완전.. 빨리 왓으면 폰!",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000918.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞네 니 폰도 최근에 샀구아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞네 니 폰도 최근에 샀구아",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.8",
          "form": "플립 어때 실물봤나?",
          "original_form": "플립 어때 실물봤나?",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.9",
          "form": "맞아맞아 배송 올 게 많다",
          "original_form": "맞아맞아 배송 올 게 많다",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.10",
          "form": "아직 실물 못보고 배송도 화요일에 온다",
          "original_form": "아직 실물 못보고 배송도 화요일에 온다",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.11",
          "form": "사람들 스티커 쇼핑 많이 하던데",
          "original_form": "사람들 스티커 쇼핑 많이 하던데",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.12",
          "form": "나는 별로 관심 없었고든?",
          "original_form": "나는 별로 관심 없었고든?",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.13",
          "form": "그래도 화요일이면 빠르네",
          "original_form": "그래도 화요일이면 빠르네",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.14",
          "form": "하지만 진쩌 영겁의 시간 ㅠ",
          "original_form": "하지만 진쩌 영겁의 시간 ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.15",
          "form": "폰에 붙이는 스티커?",
          "original_form": "폰에 붙이는 스티커?",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.16",
          "form": "어어 근데 폰전용 스티커도 있더라",
          "original_form": "어어 근데 폰전용 스티커도 있더라",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000918.1.17",
          "form": "플립전용스티커",
          "original_form": "플립전용스티커",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.18",
          "form": "신기하제",
          "original_form": "신기하제",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.19",
          "form": "그럼 잘 떨어지는겅가?",
          "original_form": "그럼 잘 떨어지는겅가?",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.20",
          "form": "그렇지않을까",
          "original_form": "그렇지않을까",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.21",
          "form": "그거 아니더라도 그냥 잘라서 폰케이스 사이에 끼워도 되고",
          "original_form": "그거 아니더라도 그냥 잘라서 폰케이스 사이에 끼워도 되고",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.22",
          "form": "궁금하니까 한번 사봐",
          "original_form": "궁금하니까 한번 사봐",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.23",
          "form": "돈 줄래?",
          "original_form": "돈 줄래?",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.24",
          "form": "아무래도 내폰은 아니니까",
          "original_form": "아무래도 내폰은 아니니까",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000918.1.25",
          "form": "버즈2 사은품으로 오잖아",
          "original_form": "버즈2 사은품으로 오잖아",
          "speaker_id": "2",
          "time": "20210819 20:38"
        }
      ]
    }
