      "utterance": [
        {
          "id": "MDRW2100008030.1.1",
          "form": "오늘뭐먹노?",
          "original_form": "오늘뭐먹노?",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100008030.1.2",
          "form": "여보셔ㅋㅋ",
          "original_form": "여보셔ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100008030.1.3",
          "form": "아따마 ㅋㅋㅋㅋㄱ",
          "original_form": "아따마 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008030.1.4",
          "form": "반갑구먼 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "반갑구먼 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008030.1.5",
          "form": "언냐 커피마신다 밥은 안먹고 ㅋㅋㅋㅋㄱ",
          "original_form": "언냐 커피마신다 밥은 안먹고 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008030.1.6",
          "form": "점심 먹었슈??",
          "original_form": "점심 먹었슈??",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008030.1.7",
          "form": "우린 석경간다",
          "original_form": "우린 석경간다",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008030.1.8",
          "form": "테크노 최애 맛집",
          "original_form": "테크노 최애 맛집",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008030.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱ 한번도 안가봤다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱ 한번도 안가봤다",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008030.1.10",
          "form": "잘먹는다 이언니 ㅋㅋㅋㅋㅋ",
          "original_form": "잘먹는다 이언니 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008030.1.11",
          "form": "맛집투어하자ㅋ",
          "original_form": "맛집투어하자ㅋ",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008030.1.12",
          "form": "코로나 끝나고",
          "original_form": "코로나 끝나고",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008030.1.13",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008030.1.14",
          "form": "맛있는데 좀 알아놓으슈",
          "original_form": "맛있는데 좀 알아놓으슈",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008030.1.15",
          "form": "다먹고뭐할껀데??",
          "original_form": "다먹고뭐할껀데??",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008030.1.16",
          "form": "커피마셔야지ㅋ",
          "original_form": "커피마셔야지ㅋ",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008030.1.17",
          "form": "눈깜술하러 반도 갈라는데 \r\n110동 첫째만나러 ㅋㅋㅋㅋㅋ",
          "original_form": "눈깜술하러 반도 갈라는데 \r\n110동 첫째만나러 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008030.1.18",
          "form": "그게뭐고?",
          "original_form": "그게뭐고?",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008030.1.19",
          "form": "눈 감고 술래잡기 ㅋㅋㅋㅋ",
          "original_form": "눈 감고 술래잡기 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008030.1.20",
          "form": "이언니 아무것도 모르노",
          "original_form": "이언니 아무것도 모르노",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008030.1.21",
          "form": "먹는것만 알고",
          "original_form": "먹는것만 알고",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008030.1.22",
          "form": "ㅋㅋ 먹고살자고 하는건듸ㅣㅋ",
          "original_form": "ㅋㅋ 먹고살자고 하는건듸ㅣㅋ",
          "speaker_id": "1",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008030.1.23",
          "form": "신나게 놀이터고고",
          "original_form": "신나게 놀이터고고",
          "speaker_id": "1",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008030.1.24",
          "form": "다먹고 빨리 오세이",
          "original_form": "다먹고 빨리 오세이",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008030.1.25",
          "form": "난 이제 나갑니다",
          "original_form": "난 이제 나갑니다",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008030.1.26",
          "form": "놀이터 화이팅",
          "original_form": "놀이터 화이팅",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008030.1.27",
          "form": "저녁메뉴 여기적고가라ㅋㅋ",
          "original_form": "저녁메뉴 여기적고가라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008030.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 하아...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 하아...",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008030.1.29",
          "form": "아직\r\n미정 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "아직\r\n미정 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008030.1.30",
          "form": "먹기 귀찮.....",
          "original_form": "먹기 귀찮.....",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008030.1.31",
          "form": "아무꺼나 시켜무야겠슈 메뉴 추천해주슈",
          "original_form": "아무꺼나 시켜무야겠슈 메뉴 추천해주슈",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008030.1.32",
          "form": "우린 미역국이다ㅋㅋ",
          "original_form": "우린 미역국이다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008030.1.33",
          "form": "니도 국해라ㅂㄱ",
          "original_form": "니도 국해라ㅂㄱ",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008030.1.34",
          "form": "오늘 몬스터커피사러갔는데 넘 많이기다려서ㅡㅡ",
          "original_form": "오늘 몬스터커피사러갔는데 넘 많이기다려서ㅡㅡ",
