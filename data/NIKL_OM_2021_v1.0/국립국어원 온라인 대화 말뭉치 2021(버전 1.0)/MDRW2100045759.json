        {
          "id": "MDRW2100045759.1.2",
          "form": "파스타 뭐 좋아해",
          "original_form": "파스타 뭐 좋아해",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045759.1.3",
          "form": "나는 로제 파스타",
          "original_form": "나는 로제 파스타",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045759.1.4",
          "form": "나는 크림이 제일 좋더라",
          "original_form": "나는 크림이 제일 좋더라",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045759.1.5",
          "form": "둘다 생크림 들어간거네",
          "original_form": "둘다 생크림 들어간거네",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045759.1.6",
          "form": "ㅋㅋㅋㅋ그러네 토마토는?",
          "original_form": "ㅋㅋㅋㅋ그러네 토마토는?",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045759.1.7",
          "form": "난 토마토 소스는 별로야",
          "original_form": "난 토마토 소스는 별로야",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045759.1.8",
          "form": "왜 맛있는데ㅜㅜ",
          "original_form": "왜 맛있는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045759.1.9",
          "form": "보통 특이한 향도 같이 넣어서ㅜ",
          "original_form": "보통 특이한 향도 같이 넣어서ㅜ",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045759.1.10",
          "form": "아 나도 그건 싫더라",
          "original_form": "아 나도 그건 싫더라",
          "speaker_id": "1",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045759.1.11",
          "form": "생토마토는 나으려나",
          "original_form": "생토마토는 나으려나",
          "speaker_id": "2",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045759.1.12",
          "form": "응 그렇게 한건 더 맛있어",
          "original_form": "응 그렇게 한건 더 맛있어",
          "speaker_id": "1",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045759.1.13",
          "form": "오일파스타도 맛있어",
          "original_form": "오일파스타도 맛있어",
