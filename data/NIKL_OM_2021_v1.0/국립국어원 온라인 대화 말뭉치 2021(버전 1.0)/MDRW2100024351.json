        },
        {
          "id": "MDRW2100024351.1.2",
          "form": "name2님~죄송하지만",
          "original_form": "&name2&님~죄송하지만",
          "speaker_id": "1",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.3",
          "form": "저먼저잘께요^^",
          "original_form": "저먼저잘께요^^",
          "speaker_id": "1",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.4",
          "form": "대화너무즐거웠어요~~",
          "original_form": "대화너무즐거웠어요~~",
          "speaker_id": "1",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.5",
          "form": "낼출근하시죠",
          "original_form": "낼출근하시죠",
          "speaker_id": "2",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.6",
          "form": "또 이야기나눠요",
          "original_form": "또 이야기나눠요",
          "speaker_id": "2",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.7",
          "form": "넹ㅠㅠㅋ",
          "original_form": "넹ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.8",
          "form": "내일또이야기나눠요ㅋㅋ",
          "original_form": "내일또이야기나눠요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.9",
          "form": "심심하실때톡주세요^^",
          "original_form": "심심하실때톡주세요^^",
          "speaker_id": "2",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.10",
          "form": "마음이잘맞는친구같아요",
          "original_form": "마음이잘맞는친구같아요",
          "speaker_id": "2",
          "time": "20210831 01:24"
        },
        {
          "id": "MDRW2100024351.1.11",
          "form": "넵넵ㅋㅋ",
          "original_form": "넵넵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:25"
        },
        {
          "id": "MDRW2100024351.1.12",
          "form": "퇴근하셨나요?",
          "original_form": "퇴근하셨나요?",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100024351.1.13",
          "form": "오늘은 둘째접종을하고왔어요",
          "original_form": "오늘은 둘째접종을하고왔어요",
          "speaker_id": "2",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024351.1.14",
          "form": "첫째가정보육까지 힘든하루였답니다",
          "original_form": "첫째가정보육까지 힘든하루였답니다",
          "speaker_id": "2",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024351.1.15",
          "form": "힘드시겠어요ㅠㅠ",
          "original_form": "힘드시겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100024351.1.16",
          "form": "저는 집에가서 또 육아해야해요",
          "original_form": "저는 집에가서 또 육아해야해요",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024351.1.17",
          "form": "이제 퇴근해야지여",
          "original_form": "이제 퇴근해야지여",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024351.1.18",
          "form": "비오는데 퇴근길이더 고되실듯요",
          "original_form": "비오는데 퇴근길이더 고되실듯요",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100024351.1.19",
          "form": "폰보지마지고 안전귀가하세요",
          "original_form": "폰보지마지고 안전귀가하세요",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100024351.1.20",
          "form": "아이들 접종열은 없기를 바래여",
          "original_form": "아이들 접종열은 없기를 바래여",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024351.1.21",
          "form": "살짝미열은 있는듯요",
          "original_form": "살짝미열은 있는듯요",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024351.1.22",
          "form": "길게못자서 육아가힘이듭니다",
          "original_form": "길게못자서 육아가힘이듭니다",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024351.1.23",
          "form": "육아 힘들어요ㅠㅠ",
          "original_form": "육아 힘들어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024351.1.24",
          "form": "저도 퇴근하는 순간 육아시작이네요",
          "original_form": "저도 퇴근하는 순간 육아시작이네요",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024351.1.25",
          "form": "아이둘이니 더 힘드시겠네요ㅠ",
          "original_form": "아이둘이니 더 힘드시겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024351.1.26",
          "form": "워킹맘이더힘드시죠ㅠ",
          "original_form": "워킹맘이더힘드시죠ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024351.1.27",
          "form": "진정한워킹맘의길로ㅋㅋ",
          "original_form": "진정한워킹맘의길로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024351.1.28",
          "form": "오늘 비 너무 오네요",
          "original_form": "오늘 비 너무 오네요",
          "speaker_id": "1",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024351.1.29",
          "form": "이런날은 더 힘들어요ㅋ",
          "original_form": "이런날은 더 힘들어요ㅋ",
          "speaker_id": "1",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024351.1.30",
          "form": "밖에 못나가기 힘듬이 2배ㅋ",
          "original_form": "밖에 못나가기 힘듬이 2배ㅋ",
          "speaker_id": "1",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024351.1.31",
          "form": "오늘 날씨가제법쌀쌀하네요",
          "original_form": "오늘 날씨가제법쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024351.1.32",
          "form": "추워요ㅋㅋ감기걸릴듯",
          "original_form": "추워요ㅋㅋ감기걸릴듯",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024351.1.33",
          "form": "아이들 감기 안걸리게 옷입혀야겠어여",
          "original_form": "아이들 감기 안걸리게 옷입혀야겠어여",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024351.1.34",
          "form": "이제긴팔입어야하나 ㅋㅋ",
          "original_form": "이제긴팔입어야하나 ㅋㅋ",
