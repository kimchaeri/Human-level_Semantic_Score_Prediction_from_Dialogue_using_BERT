      "utterance": [
        {
          "id": "MDRW2100018147.1.1",
          "form": "요거트에 복숭아 추천드려요!",
          "original_form": "요거트에 복숭아 추천드려요!",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018147.1.2",
          "form": "네 아무래도 요거트를 먹어야될거 같아요 ㅋㅋㅋ",
          "original_form": "네 아무래도 요거트를 먹어야될거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.3",
          "form": "저는 그릭요거트 좋아하거든요",
          "original_form": "저는 그릭요거트 좋아하거든요",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.4",
          "form": "으 저는 못먹겠어여",
          "original_form": "으 저는 못먹겠어여",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.5",
          "form": "크림치즈 먹자마자 아 아니구나 했어욬ㅋㅋ",
          "original_form": "크림치즈 먹자마자 아 아니구나 했어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.6",
          "form": "그 꾸덕함",
          "original_form": "그 꾸덕함",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.7",
          "form": "ㅎㅎㅎㅎㅎㅎ 비주얼은 비슷해요",
          "original_form": "ㅎㅎㅎㅎㅎㅎ 비주얼은 비슷해요",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.8",
          "form": "그 꾸덕함이 매력인데 뭘 모르시네여ㅋ",
          "original_form": "그 꾸덕함이 매력인데 뭘 모르시네여ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.10",
          "form": "맞아요 비주얼을 믿고 실패ㅠㅠ",
          "original_form": "맞아요 비주얼을 믿고 실패ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018147.1.11",
          "form": "더 배워야겠어여ㅋㅋ",
          "original_form": "더 배워야겠어여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018147.1.12",
          "form": "벌써 11신데 이시간에 드시면 안붓나요?",
          "original_form": "벌써 11신데 이시간에 드시면 안붓나요?",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018147.1.13",
          "form": "그러게요 그건 내일의 내가...",
          "original_form": "그러게요 그건 내일의 내가...",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018147.1.14",
          "form": "하아 저 위산이 분비되는 느낌이 나네여..",
          "original_form": "하아 저 위산이 분비되는 느낌이 나네여..",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018147.1.15",
          "form": "배고픈거 같아여",
          "original_form": "배고픈거 같아여",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018147.1.16",
          "form": "생각해보니 건강식이라 괜찮을거 같아요",
          "original_form": "생각해보니 건강식이라 괜찮을거 같아요",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018147.1.17",
          "form": "맞네요",
          "original_form": "맞네요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018147.1.18",
          "form": "몸에 좋지 않을 요소가 없어여!!",
          "original_form": "몸에 좋지 않을 요소가 없어여!!",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018147.1.19",
          "form": "치즈넘너 좋져",
          "original_form": "치즈넘너 좋져",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018147.1.20",
          "form": "라면이 아니니까!!",
          "original_form": "라면이 아니니까!!",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018147.1.21",
          "form": "맞아여!!!!",
          "original_form": "맞아여!!!!",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018147.1.22",
          "form": "치즈는 다 맛있어용",
          "original_form": "치즈는 다 맛있어용",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018147.1.23",
          "form": "저는 스트링치즈 전자렌지에 돌려먹는거 너무 좋아요",
          "original_form": "저는 스트링치즈 전자렌지에 돌려먹는거 너무 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018147.1.24",
          "form": "오늘 스트링치즈 살려다가 멈췄어요",
          "original_form": "오늘 스트링치즈 살려다가 멈췄어요",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018147.1.25",
          "form": "제어를 잘하시네요ㅎ",
          "original_form": "제어를 잘하시네요ㅎ",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018147.1.26",
          "form": "홈플러스에서 원플원하는데 못멈출까봐",
          "original_form": "홈플러스에서 원플원하는데 못멈출까봐",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018147.1.27",
          "form": "그거 진짜 한꺼번에 사면 훅먹어서 아예 사질 말아야돼요",
          "original_form": "그거 진짜 한꺼번에 사면 훅먹어서 아예 사질 말아야돼요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018147.1.28",
          "form": "원플원은 못참져ㅋㅋ",
          "original_form": "원플원은 못참져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018147.1.29",
          "form": "그래놓고 편의점에서 사먹고 ㅋㅋㅋㅋㅋ",
          "original_form": "그래놓고 편의점에서 사먹고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018147.1.30",
          "form": "괜히 더 비싸게 사먹기 대장 ㅠㅠ",
          "original_form": "괜히 더 비싸게 사먹기 대장 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018147.1.31",
          "form": "이게 비싸게찐 살인데",
          "original_form": "이게 비싸게찐 살인데",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018147.1.32",
          "form": "어케 빼여ㅋㅋ",
          "original_form": "어케 빼여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018147.1.33",
          "form": "ㅋㅋㅋㅋ 그러니까여 뺄살이 없어",
          "original_form": "ㅋㅋㅋㅋ 그러니까여 뺄살이 없어",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018147.1.34",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018147.1.35",
          "form": "찬거 같길래 ㅎㅎ 오늘 저녁 뭐 드셨어요?",
