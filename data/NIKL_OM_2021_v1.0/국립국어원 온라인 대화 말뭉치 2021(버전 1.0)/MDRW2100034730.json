      "utterance": [
        {
          "id": "MDRW2100034730.1.1",
          "form": "노래 자주 들으세요?",
          "original_form": "노래 자주 들으세요?",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034730.1.2",
          "form": "네 노래 매일 듣죠!\n출퇴근하면서 많이 들어요!",
          "original_form": "네 노래 매일 듣죠!\n출퇴근하면서 많이 들어요!",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034730.1.3",
          "form": "노래 뭐들으세요 요새?",
          "original_form": "노래 뭐들으세요 요새?",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034730.1.4",
          "form": "저는 찾아서 듣는 편은 아니지만",
          "original_form": "저는 찾아서 듣는 편은 아니지만",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034730.1.5",
          "form": "요새는 퀸덤 괜찮더라구요",
          "original_form": "요새는 퀸덤 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034730.1.6",
          "form": "맞다 퀸덤 듣는다고 하셨죠?",
          "original_form": "맞다 퀸덤 듣는다고 하셨죠?",
          "speaker_id": "2",
          "time": "20210828 18:21"
        },
        {
          "id": "MDRW2100034730.1.7",
          "form": "아넵 ㅋㅋㅋㅋ",
          "original_form": "아넵 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034730.1.8",
          "form": "또 그...헤일리..? 이름이 기억안나네요 여자랩퍼인데..",
          "original_form": "또 그...헤일리..? 이름이 기억안나네요 여자랩퍼인데..",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034730.1.9",
          "form": "헤이즐!!",
          "original_form": "헤이즐!!",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034730.1.10",
          "form": "아 헤이즈인거같네욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 헤이즈인거같네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034730.1.11",
          "form": "ㅋㅋㅋㅋ맞아요 헤이즈ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아요 헤이즈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034730.1.12",
          "form": "헤이즈 노래도 좋죠!",
          "original_form": "헤이즈 노래도 좋죠!",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034730.1.13",
          "form": "헤이즈노래는 다 좋더라구요 ㅋㅋ",
          "original_form": "헤이즈노래는 다 좋더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034730.1.14",
          "form": "마자요 진짜 이번에 헤픈우연도 좋고 비도오고그래서도 진짜 좋더라구요",
          "original_form": "마자요 진짜 이번에 헤픈우연도 좋고 비도오고그래서도 진짜 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 18:24"
        },
        {
          "id": "MDRW2100034730.1.15",
          "form": "맞아요 헤픈우연! 그거 진짜 좋던데요",
          "original_form": "맞아요 헤픈우연! 그거 진짜 좋던데요",
          "speaker_id": "1",
          "time": "20210828 18:24"
        },
        {
          "id": "MDRW2100034730.1.16",
          "form": "특유의 감성이 좋아요",
          "original_form": "특유의 감성이 좋아요",
          "speaker_id": "1",
          "time": "20210828 18:24"
        },
        {
          "id": "MDRW2100034730.1.17",
          "form": "마자요! 저는 아이돌음악만 주로 듣는데 헤이즈노래는 참 좋더라구요",
          "original_form": "마자요! 저는 아이돌음악만 주로 듣는데 헤이즈노래는 참 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034730.1.18",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034730.1.19",
          "form": "근데뭔가 노래가 다 비슷비슷하긴 해요 ㅌㅋㅋ",
          "original_form": "근데뭔가 노래가 다 비슷비슷하긴 해요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034730.1.20",
          "form": "아 맞아요! 저도 그런거 느꼈어요ㅋㅋ",
          "original_form": "아 맞아요! 저도 그런거 느꼈어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034730.1.21",
          "form": "그쳐 ㅋㅋㅋ",
          "original_form": "그쳐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034730.1.22",
          "form": "목소리가 특색있어서 그런건지",
          "original_form": "목소리가 특색있어서 그런건지",
          "speaker_id": "2",
          "time": "20210828 18:26"
        }
      ]
