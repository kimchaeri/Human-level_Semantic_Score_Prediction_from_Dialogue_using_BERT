      "utterance": [
        {
          "id": "MDRW2100039769.1.1",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039769.1.2",
          "form": "저는 샤브샤브요",
          "original_form": "저는 샤브샤브요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039769.1.3",
          "form": "아 샤브샤브도 너무 맛있죠!!",
          "original_form": "아 샤브샤브도 너무 맛있죠!!",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039769.1.4",
          "form": "샤브샤브 먹은지 오래되서 먹으러 가고 싶네요 ㅠㅠ",
          "original_form": "샤브샤브 먹은지 오래되서 먹으러 가고 싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039769.1.5",
          "form": "저도요ㅜㅜㅜ",
          "original_form": "저도요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039769.1.6",
          "form": "식당에서 오래먹어야되서",
          "original_form": "식당에서 오래먹어야되서",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039769.1.7",
          "form": "코로나땜에 가기 꺼려져요",
          "original_form": "코로나땜에 가기 꺼려져요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039769.1.8",
          "form": "맞아요ㅜㅜㅜ 코로나때문에 쉽게 갈 수가 없네요",
          "original_form": "맞아요ㅜㅜㅜ 코로나때문에 쉽게 갈 수가 없네요",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039769.1.9",
          "form": "샤브샤브  해먹으려면",
          "original_form": "샤브샤브  해먹으려면",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039769.1.10",
          "form": "재료비가 너무 많이들더라구요",
          "original_form": "재료비가 너무 많이들더라구요",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039769.1.11",
          "form": "아 맞아요 ㅠㅠㅠ 재료비도 재료비고 손도 많이 가서",
          "original_form": "아 맞아요 ㅠㅠㅠ 재료비도 재료비고 손도 많이 가서",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039769.1.12",
          "form": "집에서 해먹기 쉽지 않죠",
          "original_form": "집에서 해먹기 쉽지 않죠",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039769.1.13",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039769.1.14",
          "form": "끓이면서 먹어야하는데",
          "original_form": "끓이면서 먹어야하는데",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039769.1.15",
          "form": "쉽지않아요",
          "original_form": "쉽지않아요",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039769.1.16",
          "form": "ㅠㅠㅠㅜ 어서 코로나가 없어져서 편히 먹으러 가고 싶네요",
          "original_form": "ㅠㅠㅠㅜ 어서 코로나가 없어져서 편히 먹으러 가고 싶네요",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039769.1.17",
          "form": "진짜요 ᆢᆢ친구도 좀 편하게 만나고싶어요ㅜ",
          "original_form": "진짜요 ᆢᆢ친구도 좀 편하게 만나고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039769.1.18",
          "form": "배달음식은 어떤 거 주로 드시나요?",
          "original_form": "배달음식은 어떤 거 주로 드시나요?",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039769.1.19",
          "form": "name2님은 어떤음식 좋아하세요?",
          "original_form": "&name2&님은 어떤음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039769.1.20",
          "form": "역시 치킨이요",
          "original_form": "역시 치킨이요",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039769.1.21",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039769.1.22",
          "form": "저는 떡볶이나 양식류 좋아해요!",
          "original_form": "저는 떡볶이나 양식류 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039769.1.23",
          "form": "오 떡볶이 어디거 좋아하세요?",
          "original_form": "오 떡볶이 어디거 좋아하세요?",
