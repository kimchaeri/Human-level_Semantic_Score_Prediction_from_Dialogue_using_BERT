      "utterance": [
        {
          "id": "MDRW2100025573.1.1",
          "form": "최근 구매 물건 있으신가요?",
          "original_form": "최근 구매 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025573.1.2",
          "form": "우양산 하나 샀어요",
          "original_form": "우양산 하나 샀어요",
          "speaker_id": "2",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025573.1.3",
          "form": "오 햇빛도 아직 강하고 비도 와서 좋네요",
          "original_form": "오 햇빛도 아직 강하고 비도 와서 좋네요",
          "speaker_id": "1",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100025573.1.4",
          "form": "저는 홈플러스에서 주문했어요",
          "original_form": "저는 홈플러스에서 주문했어요",
          "speaker_id": "1",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100025573.1.5",
          "form": "네 양산으로 괜찮더라구요",
          "original_form": "네 양산으로 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025573.1.6",
          "form": "비 너무 많이올때는 작아서 좀 그렇구요ㅋㅋ",
          "original_form": "비 너무 많이올때는 작아서 좀 그렇구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025573.1.7",
          "form": "요즘 꽃게철이라 꽃게 샀어요",
          "original_form": "요즘 꽃게철이라 꽃게 샀어요",
          "speaker_id": "1",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025573.1.8",
          "form": "꽃게 맛있죠",
          "original_form": "꽃게 맛있죠",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025573.1.9",
          "form": "쇼핑 자주 하시나요?",
          "original_form": "쇼핑 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025573.1.10",
          "form": "고양이 키워서 고양이 용품 자주 사욤",
          "original_form": "고양이 키워서 고양이 용품 자주 사욤",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025573.1.11",
          "form": "구매 하고 싶은 물건 있으신가요?",
          "original_form": "구매 하고 싶은 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025573.1.12",
          "form": "저는 실내자전거 사고싶어요 ㅠ",
          "original_form": "저는 실내자전거 사고싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025573.1.13",
          "form": "저 플립3이요ㅋㅋㅋ",
          "original_form": "저 플립3이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025573.1.14",
          "form": "폰 바꾸고 싶네요",
          "original_form": "폰 바꾸고 싶네요",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025573.1.15",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025573.1.16",
          "form": "그거 너무 이쁘죠",
          "original_form": "그거 너무 이쁘죠",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025573.1.17",
          "form": "지금도 안드로이드 쓰시나요?",
          "original_form": "지금도 안드로이드 쓰시나요?",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025573.1.18",
          "form": "노트 10이 너무 쌩쌩해서 못바꾸겠어요",
          "original_form": "노트 10이 너무 쌩쌩해서 못바꾸겠어요",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025573.1.19",
          "form": "네 계속 안드로이드 썼어요",
          "original_form": "네 계속 안드로이드 썼어요",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025573.1.20",
          "form": "아이패드 한번 사봤는데 전 불편하더라구여",
          "original_form": "아이패드 한번 사봤는데 전 불편하더라구여",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025573.1.21",
          "form": "저도 안드파  패드 저도 6세대인가 쳐박혀있어요 ㅎㅎ",
          "original_form": "저도 안드파  패드 저도 6세대인가 쳐박혀있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:58"
        },
        {
          "id": "MDRW2100025573.1.22",
          "form": "리듬게임 같은거 하기엔 좋더라구요",
          "original_form": "리듬게임 같은거 하기엔 좋더라구요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025573.1.23",
          "form": "그냥 문서뷰어겸 게임기 된거 같아요",
          "original_form": "그냥 문서뷰어겸 게임기 된거 같아요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025573.1.24",
          "form": "맞아요ㅜㅋㅋ 충전이 귀찮아서 잘 켜지도 않는",
          "original_form": "맞아요ㅜㅋㅋ 충전이 귀찮아서 잘 켜지도 않는",
          "speaker_id": "1",
          "time": "20210901 14:04"
        }
      ]
    }
