        {
          "id": "MDRW2100019078.1.1",
          "form": "아 근데 확실히",
          "original_form": "아 근데 확실히",
          "speaker_id": "2",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100019078.1.2",
          "form": "움짤띄우는건 좀",
          "original_form": "움짤띄우는건 좀",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019078.1.3",
          "form": "혹하긴 하더라",
          "original_form": "혹하긴 하더라",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019078.1.4",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019078.1.5",
          "form": "그 기능이 제일 탐나",
          "original_form": "그 기능이 제일 탐나",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019078.1.6",
          "form": "외부액정에서 이것저것 할 수 있는게",
          "original_form": "외부액정에서 이것저것 할 수 있는게",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019078.1.7",
          "form": "사람들이 사는 이유일듯 ㅋㅋㅋ",
          "original_form": "사람들이 사는 이유일듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019078.1.8",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ별걸 다하더라 진짜",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ별걸 다하더라 진짜",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019078.1.9",
          "form": "근데 폰 자체만 봐서는 기능적으로 크게 끌리진 않아서",
          "original_form": "근데 폰 자체만 봐서는 기능적으로 크게 끌리진 않아서",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019078.1.10",
          "form": "ㅇㅇ 기능적으로는 잘 모르겠어",
          "original_form": "ㅇㅇ 기능적으로는 잘 모르겠어",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019078.1.11",
          "form": "디자인이 귀엽더라",
          "original_form": "디자인이 귀엽더라",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019078.1.12",
          "form": "요즘 휴대폰은 기능은 사실상 엇비슷해서",
          "original_form": "요즘 휴대폰은 기능은 사실상 엇비슷해서",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019078.1.13",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019078.1.14",
          "form": "디자인 컨셉을 어떻게 잡느냐가",
          "original_form": "디자인 컨셉을 어떻게 잡느냐가",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019078.1.15",
          "form": "일반인들은 잘모를",
          "original_form": "일반인들은 잘모를",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019078.1.16",
          "form": "미세 기능이나 추가되지",
          "original_form": "미세 기능이나 추가되지",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019078.1.17",
          "form": "판매를 좌우하는듯",
          "original_form": "판매를 좌우하는듯",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019078.1.18",
          "form": "z플립 디자인은 진짜",
          "original_form": "z플립 디자인은 진짜",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019078.1.19",
          "form": "예쁘면서 약간 그시절 감성도 있는게",
          "original_form": "예쁘면서 약간 그시절 감성도 있는게",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019078.1.20",
          "form": "ㄹㅇ잘뽑긴 했어",
          "original_form": "ㄹㅇ잘뽑긴 했어",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019078.1.21",
          "form": "이번엔 색깔도 구매욕 자극하게 뽑았어",
          "original_form": "이번엔 색깔도 구매욕 자극하게 뽑았어",
          "speaker_id": "1",
          "time": "20210827 15:09"
