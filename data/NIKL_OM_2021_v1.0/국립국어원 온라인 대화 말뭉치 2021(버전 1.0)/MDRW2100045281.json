      "utterance": [
        {
          "id": "MDRW2100045281.1.1",
          "form": "전 사실 편식없이 이것저것 잘먹어요!",
          "original_form": "전 사실 편식없이 이것저것 잘먹어요!",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045281.1.2",
          "form": "오오 매운것도 잘드시나용",
          "original_form": "오오 매운것도 잘드시나용",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045281.1.3",
          "form": "잘 먹진 못하는데 좋아해서 울면서 먹어요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "잘 먹진 못하는데 좋아해서 울면서 먹어요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045281.1.4",
          "form": "우왕 ㅠㅜ 울면서 ㅋㅋㅋㅋㅋ",
          "original_form": "우왕 ㅠㅜ 울면서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045281.1.5",
          "form": "닭발 좋아하세요?",
          "original_form": "닭발 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045281.1.6",
          "form": "오! 엄청 좋아해요",
          "original_form": "오! 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045281.1.7",
          "form": "닭발에 쏘주 국률이죠ㅠㅠㅋㅋㅋ",
          "original_form": "닭발에 쏘주 국률이죠ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045281.1.8",
          "form": "와아 너무 좋아요 ㅋㅋㅋㅅ",
          "original_form": "와아 너무 좋아요 ㅋㅋㅋㅅ",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045281.1.9",
          "form": "술 잘드새요?",
          "original_form": "술 잘드새요?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045281.1.10",
          "form": "잘 마시진 않고 소주 2병정도? 마시는거같아요 ㅎㅎㅎ",
          "original_form": "잘 마시진 않고 소주 2병정도? 마시는거같아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045281.1.11",
          "form": "와우 엄청 잘마시는거 아닌가요?? ㅎㅎ",
          "original_form": "와우 엄청 잘마시는거 아닌가요?? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045281.1.12",
          "form": "술안주 뭐 좋아하세요",
          "original_form": "술안주 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045281.1.13",
          "form": "저는 탕 종류 좋아해요~ 해물탕 이런거!",
          "original_form": "저는 탕 종류 좋아해요~ 해물탕 이런거!",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045281.1.14",
          "form": "오오 해뭉탕!!! 저도 조개탕 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "오오 해뭉탕!!! 저도 조개탕 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045281.1.15",
          "form": "맥주도 드새요?",
          "original_form": "맥주도 드새요?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
