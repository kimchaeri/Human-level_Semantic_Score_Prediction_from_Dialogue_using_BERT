      "utterance": [
        {
          "id": "MDRW2100024685.1.1",
          "form": "최근 구매하신 물건 있으세요~? 저는 씨제이꺼 탕수육이요!",
          "original_form": "최근 구매하신 물건 있으세요~? 저는 씨제이꺼 탕수육이요!",
          "speaker_id": "1",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024685.1.2",
          "form": "저는 쌀 샀어요ㅋㅋ",
          "original_form": "저는 쌀 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024685.1.3",
          "form": "철원오대미ㅋㅋ찰지더라구요~~",
          "original_form": "철원오대미ㅋㅋ찰지더라구요~~",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024685.1.4",
          "form": "오~ 다음에 살때 참고할게요ㅋㅋ",
          "original_form": "오~ 다음에 살때 참고할게요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024685.1.5",
          "form": "오늘 당근했는데 소소하지만 물건 팔리니깐 기분이 좋더라고요",
          "original_form": "오늘 당근했는데 소소하지만 물건 팔리니깐 기분이 좋더라고요",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024685.1.6",
          "form": "탕수육은 어때요?",
          "original_form": "탕수육은 어때요?",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024685.1.7",
          "form": "오늘 배달왔을거에요!ㅋㅋ저도 처음 구매한거라죠",
          "original_form": "오늘 배달왔을거에요!ㅋㅋ저도 처음 구매한거라죠",
          "speaker_id": "1",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024685.1.8",
          "form": "오ㅋㅋㅋ택배가 오늘 오는군요ㅋㅋ택배 오는거 넘 좋아요ㅋㅋ",
          "original_form": "오ㅋㅋㅋ택배가 오늘 오는군요ㅋㅋ택배 오는거 넘 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024685.1.9",
          "form": "저도 오늘 쌀 도착했어요ㅋㅋ찰져서 재구매했답니다",
          "original_form": "저도 오늘 쌀 도착했어요ㅋㅋ찰져서 재구매했답니다",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024685.1.10",
          "form": "맞아여 맞아여 ㅋㅋ 오 ~ 찰진 밥 좋아하시나요",
          "original_form": "맞아여 맞아여 ㅋㅋ 오 ~ 찰진 밥 좋아하시나요",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024685.1.11",
          "form": "네~~좋아해요ㅋㅋㅋ",
          "original_form": "네~~좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024685.1.12",
          "form": "혹시 에어후라이어 있으세요?",
          "original_form": "혹시 에어후라이어 있으세요?",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024685.1.13",
          "form": "네네 있어요!",
          "original_form": "네네 있어요!",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024685.1.14",
          "form": "고구마 구울때나 쓰고있지만 ㅎ 요번에 만두 산김에 써보려고요",
          "original_form": "고구마 구울때나 쓰고있지만 ㅎ 요번에 만두 산김에 써보려고요",
          "speaker_id": "1",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024685.1.15",
          "form": "저는 구매하고 싶은데, 어떤게 좋을까요?",
          "original_form": "저는 구매하고 싶은데, 어떤게 좋을까요?",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024685.1.16",
          "form": "큰거살지 작은거 살지 고민입니다",
          "original_form": "큰거살지 작은거 살지 고민입니다",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024685.1.17",
          "form": "저...는 필립스꺼 쓰고있어여",
          "original_form": "저...는 필립스꺼 쓰고있어여",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024685.1.18",
          "form": "자주 해먹으실거면 큰게 좋지않을까요!",
          "original_form": "자주 해먹으실거면 큰게 좋지않을까요!",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024685.1.19",
          "form": "큰거를 추천하시는군요ㅋㅋ",
          "original_form": "큰거를 추천하시는군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024685.1.20",
          "form": "할인하면 구매해야겠어요ㅎㅎㅎ",
          "original_form": "할인하면 구매해야겠어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024685.1.21",
          "form": "필립스..구매시 참고하겠습니다",
          "original_form": "필립스..구매시 참고하겠습니다",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024685.1.22",
          "form": "네네 큰거요!",
          "original_form": "네네 큰거요!",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024685.1.23",
          "form": "지금까지 고장은 안났어요",
          "original_form": "지금까지 고장은 안났어요",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024685.1.24",
          "form": "그리고 그...뚜껑있는게 좋아요",
          "original_form": "그리고 그...뚜껑있는게 좋아요",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024685.1.25",
          "form": "뚜껑있는 큰거ㅋㅋ구매할때 참고할께여ㅋ",
          "original_form": "뚜껑있는 큰거ㅋㅋ구매할때 참고할께여ㅋ",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024685.1.26",
          "form": "좋은 정보 감사해요ㅎㅎ",
          "original_form": "좋은 정보 감사해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:33"
        }
      ]
    }
