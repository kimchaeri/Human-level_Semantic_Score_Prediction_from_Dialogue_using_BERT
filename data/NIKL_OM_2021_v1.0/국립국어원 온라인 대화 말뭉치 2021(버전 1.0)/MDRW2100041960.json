      "utterance": [
        {
          "id": "MDRW2100041960.1.1",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041960.1.2",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041960.1.3",
          "form": "저는 파워에이드를 마셨어요",
          "original_form": "저는 파워에이드를 마셨어요",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041960.1.4",
          "form": "학원이 끝나고 밥을먹을예정이에요",
          "original_form": "학원이 끝나고 밥을먹을예정이에요",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041960.1.5",
          "form": "근데 ㄹㅇ요즘도넛먹고싶음요",
          "original_form": "근데 ㄹㅇ요즘도넛먹고싶음요",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041960.1.6",
          "form": "노티드도넛인가 들어봤니",
          "original_form": "노티드도넛인가 들어봤니",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041960.1.7",
          "form": "아뇨.... 어떤 도넛인가요",
          "original_form": "아뇨.... 어떤 도넛인가요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041960.1.8",
          "form": "그게 되게 엄청 달지 않으면서",
          "original_form": "그게 되게 엄청 달지 않으면서",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041960.1.9",
          "form": "빵이 쫀득쫀득하니 맛있대",
          "original_form": "빵이 쫀득쫀득하니 맛있대",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041960.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 맛있겠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041960.1.11",
          "form": "저녁 뭐 먹을 예정??",
          "original_form": "저녁 뭐 먹을 예정??",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041960.1.12",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041960.1.13",
          "form": "라면 끓여먹어야하나",
          "original_form": "라면 끓여먹어야하나",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041960.1.14",
          "form": "무슨 라면 좋아해",
          "original_form": "무슨 라면 좋아해",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041960.1.15",
          "form": "나는 참깨라면",
          "original_form": "나는 참깨라면",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041960.1.16",
          "form": "매운거 잘먹어?",
          "original_form": "매운거 잘먹어?",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041960.1.17",
          "form": "그렇게 막 잘먹진 않아",
          "original_form": "그렇게 막 잘먹진 않아",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041960.1.18",
          "form": "불닭 먹을 수 있어?",
          "original_form": "불닭 먹을 수 있어?",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041960.1.19",
          "form": "불닭 까르보도 잘 못먹음,,",
          "original_form": "불닭 까르보도 잘 못먹음,,",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041960.1.20",
          "form": "한국인들은 고통을 즐기는것이 분명",
          "original_form": "한국인들은 고통을 즐기는것이 분명",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041960.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041960.1.22",
          "form": "맞아... 핵불닭은 대체 왜 존재하는건지 이해할 수 없음",
          "original_form": "맞아... 핵불닭은 대체 왜 존재하는건지 이해할 수 없음",
          "speaker_id": "1",
          "time": "20210906 18:04"
        }
      ]
