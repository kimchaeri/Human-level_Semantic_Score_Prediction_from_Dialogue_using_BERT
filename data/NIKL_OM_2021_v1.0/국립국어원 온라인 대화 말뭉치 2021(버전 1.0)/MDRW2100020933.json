      "utterance": [
        {
          "id": "MDRW2100020933.1.1",
          "form": "강아지 같은 거 키워요?",
          "original_form": "강아지 같은 거 키워요?",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020933.1.2",
          "form": "강아지 키워요?",
          "original_form": "강아지 키워요?",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020933.1.3",
          "form": "반려동물 없네요ㅜ",
          "original_form": "반려동물 없네요ㅜ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020933.1.4",
          "form": "동물 키우시나요?",
          "original_form": "동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020933.1.5",
          "form": "지금은 키우지 않은데",
          "original_form": "지금은 키우지 않은데",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020933.1.6",
          "form": "요즘 집에 있는 시간이 많다보니 키워보고 싶어요",
          "original_form": "요즘 집에 있는 시간이 많다보니 키워보고 싶어요",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020933.1.7",
          "form": "강아지 고양이 다 너무 좋죠",
          "original_form": "강아지 고양이 다 너무 좋죠",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020933.1.8",
          "form": "저도 여유만 있음 키우고싶네요!",
          "original_form": "저도 여유만 있음 키우고싶네요!",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020933.1.9",
          "form": "파충류 같은 건 어때요?",
          "original_form": "파충류 같은 건 어때요?",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020933.1.10",
          "form": "파충류는 조금 거리감이 있네요 ㅎㅎ 싫어하진않는데 강아지를 더 좋아해요",
          "original_form": "파충류는 조금 거리감이 있네요 ㅎㅎ 싫어하진않는데 강아지를 더 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100020933.1.11",
          "form": "ㅋㅋㅋ그래도 도마뱀이랑 카멜레온은 귀엽던데요",
          "original_form": "ㅋㅋㅋ그래도 도마뱀이랑 카멜레온은 귀엽던데요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020933.1.12",
          "form": "뱀은 좀 무서워요 ㅜㅜ",
          "original_form": "뱀은 좀 무서워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020933.1.13",
          "form": "도마뱀 새 ㅇ키 유튜브에서 봤는데 귀엽더라구요",
          "original_form": "도마뱀 새 ㅇ키 유튜브에서 봤는데 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100020933.1.14",
          "form": "ㅋㅋㅋ저 예전에 캄보디아에서 잘 때 숙소에 도마뱀 있었어요",
          "original_form": "ㅋㅋㅋ저 예전에 캄보디아에서 잘 때 숙소에 도마뱀 있었어요",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100020933.1.15",
          "form": "그때 소리 꺅꺅 하고 질렀는데 ㅜㅜ",
          "original_form": "그때 소리 꺅꺅 하고 질렀는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100020933.1.16",
          "form": "아 예전에 방송에서 어떤 연예인 동남아 갔었는데 숙소에서 도마뱀 나온거 기억나네요",
          "original_form": "아 예전에 방송에서 어떤 연예인 동남아 갔었는데 숙소에서 도마뱀 나온거 기억나네요",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020933.1.17",
          "form": "동남아는 자주 나오나봐요",
          "original_form": "동남아는 자주 나오나봐요",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020933.1.18",
          "form": "어우야 ㅠ 그렇다고 하더라구요",
          "original_form": "어우야 ㅠ 그렇다고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100020933.1.19",
          "form": "숙소에 도마뱀 있는게 흔하대요 ㅋㅋㅋㅋ",
          "original_form": "숙소에 도마뱀 있는게 흔하대요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100020933.1.20",
          "form": "반려동물 요즘 새 키우시는 분도 좀 있더라구요",
          "original_form": "반려동물 요즘 새 키우시는 분도 좀 있더라구요",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100020933.1.21",
          "form": "유튜브로 가끔 봐요 ㅋㅋ",
          "original_form": "유튜브로 가끔 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100020933.1.22",
          "form": "저 아는 사람 새 키우는데 ㅋㅋㅋㅋ",
          "original_form": "저 아는 사람 새 키우는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100020933.1.23",
          "form": "앵무새는 반려동물로도 그래도 귀엽더라구요?",
          "original_form": "앵무새는 반려동물로도 그래도 귀엽더라구요?",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100020933.1.24",
          "form": "네네 생각보다 귀여워요! 사람을 잘 따르더라구요",
          "original_form": "네네 생각보다 귀여워요! 사람을 잘 따르더라구요",
          "speaker_id": "2",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100020933.1.25",
          "form": "앵무새 말 따라하는 거 본 적 있어요?",
          "original_form": "앵무새 말 따라하는 거 본 적 있어요?",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100020933.1.26",
          "form": "네 유튜브에서요 자기가 상황에 맞게 말도하고 노래도 부르더라고요",
          "original_form": "네 유튜브에서요 자기가 상황에 맞게 말도하고 노래도 부르더라고요",
          "speaker_id": "2",
          "time": "20210828 15:17"
        }
      ]
    }
  ]
