      "utterance": [
        {
          "id": "MDRW2100023070.1.1",
          "form": "요즘에는 거의 구매하지 않았어요",
          "original_form": "요즘에는 거의 구매하지 않았어요",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023070.1.2",
          "form": "코로나로 외출이 줄어드니 옷 구매도 자연스레 줄어들더라구요",
          "original_form": "코로나로 외출이 줄어드니 옷 구매도 자연스레 줄어들더라구요",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023070.1.3",
          "form": "아하 저는 요즘 옷에빠져서 사는중인데 날씨가 애매해서ㅠㅠ",
          "original_form": "아하 저는 요즘 옷에빠져서 사는중인데 날씨가 애매해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023070.1.4",
          "form": "요즘 날씨가 애매한 날씨죠ㅜㅜ",
          "original_form": "요즘 날씨가 애매한 날씨죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023070.1.5",
          "form": "맞아요 애매한게 제일싫더라궁ᆢ",
          "original_form": "맞아요 애매한게 제일싫더라궁ᆢ",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023070.1.6",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023070.1.7",
          "form": "선택하기가 난감하니까요",
          "original_form": "선택하기가 난감하니까요",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023070.1.8",
          "form": "그래서 가을 옷차림은 눈치 싸움이에요",
          "original_form": "그래서 가을 옷차림은 눈치 싸움이에요",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023070.1.9",
          "form": "아하? 그렇네요ㅋㅋㅋ",
          "original_form": "아하? 그렇네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023070.1.10",
          "form": "항상가방에 겉옷가지고다니는데ㅋㅋㅋ",
          "original_form": "항상가방에 겉옷가지고다니는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023070.1.11",
          "form": "겉옷 요즘 날씨에는 필요하죠",
          "original_form": "겉옷 요즘 날씨에는 필요하죠",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023070.1.12",
          "form": "우산도 필수구요",
          "original_form": "우산도 필수구요",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023070.1.13",
          "form": "우산은 항상회사에!",
          "original_form": "우산은 항상회사에!",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023070.1.14",
          "form": "잘 준비해두셨네요!",
          "original_form": "잘 준비해두셨네요!",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023070.1.15",
          "form": "일기예보를 못믿어서요ㅋㅋㅋ",
          "original_form": "일기예보를 못믿어서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023070.1.16",
          "form": "언제나 대기중이죠",
          "original_form": "언제나 대기중이죠",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023070.1.17",
          "form": "ㅋㅋㅋㅋ 맞아요\n일기예보를 보면서 100%믿지는 않아요\n참고만 해야죠",
          "original_form": "ㅋㅋㅋㅋ 맞아요\n일기예보를 보면서 100%믿지는 않아요\n참고만 해야죠",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023070.1.18",
          "form": "저돈데ㅋㅋㅋ",
          "original_form": "저돈데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023070.1.19",
          "form": "가끔 보는정도?",
          "original_form": "가끔 보는정도?",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023070.1.20",
          "form": "요즘 날씨가 이상하다 싶을 때, 아니면 주변에서 태풍 장마 이런 얘기 나오면 가끔 찾아봐요ㅋㅋㅋ",
          "original_form": "요즘 날씨가 이상하다 싶을 때, 아니면 주변에서 태풍 장마 이런 얘기 나오면 가끔 찾아봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023070.1.21",
          "form": "저는 지인들이 아침에 애기를 하도해줘서ㅋㅋㅋ",
          "original_form": "저는 지인들이 아침에 애기를 하도해줘서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023070.1.22",
          "form": "그때챙겨요",
          "original_form": "그때챙겨요",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023070.1.23",
          "form": "주변에서 잘 챙겨주시니 좋은데요",
          "original_form": "주변에서 잘 챙겨주시니 좋은데요",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023070.1.24",
          "form": "ㅋㅋㅋ생각해보니좋네요?",
          "original_form": "ㅋㅋㅋ생각해보니좋네요?",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023070.1.25",
          "form": "좋은거죠ㅎㅎ",
          "original_form": "좋은거죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:40"
        }
      ]
    }
