      "utterance": [
        {
          "id": "MDRW2100018747.1.1",
          "form": "지금 에타에 다 구걸 글밖에 없음",
          "original_form": "지금 에타에 다 구걸 글밖에 없음",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018747.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.3",
          "form": "항상수강신청기간에는",
          "original_form": "항상수강신청기간에는",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.4",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.6",
          "form": "너네학교는버리면바로떠?",
          "original_form": "너네학교는버리면바로떠?",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.7",
          "form": "빈자리",
          "original_form": "빈자리",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.8",
          "form": "아마 그러지 앟을까?",
          "original_form": "아마 그러지 앟을까?",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.9",
          "form": "원래 안그래?",
          "original_form": "원래 안그래?",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018747.1.10",
          "form": "안그런학교도있음",
          "original_form": "안그런학교도있음",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018747.1.11",
          "form": "전학교는하루지나고랜덤으로떴음",
          "original_form": "전학교는하루지나고랜덤으로떴음",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018747.1.12",
          "form": "그나마다행이네",
          "original_form": "그나마다행이네",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018747.1.13",
          "form": "그러면지금",
          "original_form": "그러면지금",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018747.1.14",
          "form": "존버중임?",
          "original_form": "존버중임?",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018747.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018747.1.16",
          "form": "지금 계속 새로고침중임",
          "original_form": "지금 계속 새로고침중임",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018747.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ근데전공그렇게못잡으면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ근데전공그렇게못잡으면",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018747.1.18",
          "form": "내년에몰아들어야하는거아님?",
          "original_form": "내년에몰아들어야하는거아님?",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018747.1.19",
          "form": "내년에 졸전때문에 죽는다는데",
          "original_form": "내년에 졸전때문에 죽는다는데",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018747.1.20",
          "form": "어떻게든 잡아야 겠다",
          "original_form": "어떻게든 잡아야 겠다",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018747.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018747.1.22",
          "form": "너네는",
          "original_form": "너네는",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018747.1.23",
          "form": "졸전도",
          "original_form": "졸전도",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018747.1.24",
          "form": "있구나",
          "original_form": "있구나",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018747.1.25",
          "form": "와 그러면",
          "original_form": "와 그러면",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018747.1.26",
          "form": "내년에는20학점들으면서",
          "original_form": "내년에는20학점들으면서",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018747.1.27",
          "form": "졸전도함,",
          "original_form": "졸전도함,",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018747.1.28",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018747.1.29",
          "form": "헐 그러게",
          "original_form": "헐 그러게",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018747.1.30",
          "form": "나 어떻게 하냐",
          "original_form": "나 어떻게 하냐",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018747.1.31",
          "form": "명복이나 빌어주셈...",
          "original_form": "명복이나 빌어주셈...",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018747.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ그냥차라리1년기절하는게",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그냥차라리1년기절하는게",
          "speaker_id": "1",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018747.1.33",
          "form": "나을듯",
          "original_form": "나을듯",
          "speaker_id": "1",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018747.1.34",
          "form": "ㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018747.1.35",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018747.1.36",
          "form": ".... 그게 내 현실이라는게",
          "original_form": ".... 그게 내 현실이라는게",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018747.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ졸업한선배들은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ졸업한선배들은",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018747.1.38",
          "form": "그러면다한거임?",
          "original_form": "그러면다한거임?",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018747.1.39",
          "form": "졸전까지",
          "original_form": "졸전까지",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018747.1.40",
          "form": "아마 그럴 걸",
          "original_form": "아마 그럴 걸",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018747.1.41",
          "form": "강의 들으면서 디자인도 하면서 졸전",
          "original_form": "강의 들으면서 디자인도 하면서 졸전",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018747.1.42",
          "form": "졸전그냥시나리오쓰는거아니야?",
          "original_form": "졸전그냥시나리오쓰는거아니야?",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018747.1.43",
          "form": "디자인도해?",
          "original_form": "디자인도해?",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018747.1.44",
          "form": "무대디자인하는건가?",
          "original_form": "무대디자인하는건가?",
