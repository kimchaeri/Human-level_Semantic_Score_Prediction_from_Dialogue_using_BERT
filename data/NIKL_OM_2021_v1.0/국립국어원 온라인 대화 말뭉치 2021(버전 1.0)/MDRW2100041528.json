      "utterance": [
        {
          "id": "MDRW2100041528.1.1",
          "form": "점심은 머먹",
          "original_form": "점심은 머먹",
          "speaker_id": "2",
          "time": "20210906 09:09"
        },
        {
          "id": "MDRW2100041528.1.2",
          "form": "오므라이스",
          "original_form": "오므라이스",
          "speaker_id": "1",
          "time": "20210906 09:14"
        },
        {
          "id": "MDRW2100041528.1.3",
          "form": "니는",
          "original_form": "니는",
          "speaker_id": "1",
          "time": "20210906 09:14"
        },
        {
          "id": "MDRW2100041528.1.4",
          "form": "난 아짇...지금 오렌지먹고있어",
          "original_form": "난 아짇...지금 오렌지먹고있어",
          "speaker_id": "2",
          "time": "20210906 09:26"
        },
        {
          "id": "MDRW2100041528.1.5",
          "form": "집가서먹냐",
          "original_form": "집가서먹냐",
          "speaker_id": "1",
          "time": "20210906 09:48"
        },
        {
          "id": "MDRW2100041528.1.6",
          "form": "웅 그럴듯",
          "original_form": "웅 그럴듯",
          "speaker_id": "2",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041528.1.7",
          "form": "외식잘안햌ㅋㅋㅋ",
          "original_form": "외식잘안햌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041528.1.8",
          "form": "시켜먹거나 포장해서 집에서 먹을듯",
          "original_form": "시켜먹거나 포장해서 집에서 먹을듯",
          "speaker_id": "2",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041528.1.9",
          "form": "하긴 코로나땜에",
          "original_form": "하긴 코로나땜에",
          "speaker_id": "1",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041528.1.10",
          "form": "수제비먹어라",
          "original_form": "수제비먹어라",
          "speaker_id": "1",
          "time": "20210906 09:54"
        },
        {
          "id": "MDRW2100041528.1.11",
          "form": "ㅋㅋㅋㅋㅋ재료없어",
          "original_form": "ㅋㅋㅋㅋㅋ재료없어",
          "speaker_id": "2",
          "time": "20210906 09:54"
        },
        {
          "id": "MDRW2100041528.1.12",
          "form": "칼국수면은 있는데",
          "original_form": "칼국수면은 있는데",
          "speaker_id": "2",
          "time": "20210906 09:54"
        },
        {
          "id": "MDRW2100041528.1.13",
          "form": "1인분..",
          "original_form": "1인분..",
          "speaker_id": "2",
          "time": "20210906 09:54"
        },
        {
          "id": "MDRW2100041528.1.14",
          "form": "저녁머먹을거냐",
          "original_form": "저녁머먹을거냐",
          "speaker_id": "2",
          "time": "20210906 09:54"
        },
        {
          "id": "MDRW2100041528.1.15",
          "form": "고민중 굶을까..",
          "original_form": "고민중 굶을까..",
          "speaker_id": "1",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100041528.1.16",
          "form": "왱 본가가서 살마니쪄왔어???",
          "original_form": "왱 본가가서 살마니쪄왔어???",
          "speaker_id": "2",
          "time": "20210906 10:19"
        },
        {
          "id": "MDRW2100041528.1.17",
          "form": "밥안먹고 54 기본이더라 ^^",
          "original_form": "밥안먹고 54 기본이더라 ^^",
          "speaker_id": "1",
          "time": "20210906 10:30"
        },
        {
          "id": "MDRW2100041528.1.18",
          "form": "ㅋㅋㅋㅋ그러면 바로쪼여야 ㄱㄱ",
          "original_form": "ㅋㅋㅋㅋ그러면 바로쪼여야 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041528.1.19",
          "form": "너는 뭐먹?",
          "original_form": "너는 뭐먹?",
          "speaker_id": "1",
          "time": "20210906 11:16"
        },
        {
          "id": "MDRW2100041528.1.20",
          "form": "굶을수있을까..",
          "original_form": "굶을수있을까..",
          "speaker_id": "1",
          "time": "20210906 11:16"
        },
        {
          "id": "MDRW2100041528.1.21",
          "form": "점심 글쎄...집에모있는지바야대...만두먹을수도",
          "original_form": "점심 글쎄...집에모있는지바야대...만두먹을수도",
          "speaker_id": "2",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100041528.1.22",
          "form": "차라리 점심 굶는게쉽지 저녁 어려버..",
          "original_form": "차라리 점심 굶는게쉽지 저녁 어려버..",
          "speaker_id": "2",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100041528.1.23",
          "form": "쫄면먹고싶다",
          "original_form": "쫄면먹고싶다",
          "speaker_id": "2",
          "time": "20210906 11:19"
        },
        {
          "id": "MDRW2100041528.1.24",
          "form": "점심도어려워",
          "original_form": "점심도어려워",
          "speaker_id": "1",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100041528.1.25",
          "form": "요즘 그냥 식욕 도라이라",
          "original_form": "요즘 그냥 식욕 도라이라",
          "speaker_id": "1",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100041528.1.26",
          "form": "ㅋㅋㅋㅋㅋ근데그럴때있어...",
          "original_form": "ㅋㅋㅋㅋㅋ근데그럴때있어...",
          "speaker_id": "2",
          "time": "20210906 11:29"
        },
        {
          "id": "MDRW2100041528.1.27",
          "form": "약갠 대체해야지...밥대신 두부..",
          "original_form": "약갠 대체해야지...밥대신 두부..",
