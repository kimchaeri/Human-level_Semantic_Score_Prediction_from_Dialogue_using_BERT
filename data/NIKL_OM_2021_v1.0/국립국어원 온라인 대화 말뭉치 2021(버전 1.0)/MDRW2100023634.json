      "utterance": [
        {
          "id": "MDRW2100023634.1.1",
          "form": "화자오가 마라탕에 들어가는건가?",
          "original_form": "화자오가 마라탕에 들어가는건가?",
          "speaker_id": "2",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023634.1.2",
          "form": "응 마라탕에 들어가는거 맞아! 통으로 말고 갈려있더라구",
          "original_form": "응 마라탕에 들어가는거 맞아! 통으로 말고 갈려있더라구",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023634.1.3",
          "form": "1시에 바로 일안해?",
          "original_form": "1시에 바로 일안해?",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023634.1.4",
          "form": "점심메뉴 되게 다양하게 먹네",
          "original_form": "점심메뉴 되게 다양하게 먹네",
          "speaker_id": "2",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100023634.1.5",
          "form": "식당이 중식당인가?",
          "original_form": "식당이 중식당인가?",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100023634.1.6",
          "form": "응 짬뽕집 유명한덴데 마파두부가 특이해",
          "original_form": "응 짬뽕집 유명한덴데 마파두부가 특이해",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100023634.1.7",
          "form": "화자오 넣을정도면",
          "original_form": "화자오 넣을정도면",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100023634.1.8",
          "form": "짜장없거 짬뽕 볶음밥 마파두부밥 있어",
          "original_form": "짜장없거 짬뽕 볶음밥 마파두부밥 있어",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100023634.1.9",
          "form": "저녁엔 족발도 판다? 족발 유명해",
          "original_form": "저녁엔 족발도 판다? 족발 유명해",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100023634.1.10",
          "form": "와 족발도 팔아?",
          "original_form": "와 족발도 팔아?",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023634.1.11",
          "form": "완전 맛집이네",
          "original_form": "완전 맛집이네",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023634.1.12",
          "form": "담에 한번 포장해서 먹어볼까 싶기도 해ㅋㅋ\n거기랑 우리 회사 밑에가 식당인데 거기도 맛집이래 아구찜ㅋㅋㅋ 점심때마다 웨이팅있음",
          "original_form": "담에 한번 포장해서 먹어볼까 싶기도 해ㅋㅋ\n거기랑 우리 회사 밑에가 식당인데 거기도 맛집이래 아구찜ㅋㅋㅋ 점심때마다 웨이팅있음",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023634.1.13",
          "form": "오 너네 회사 근처에 맛집 많네",
          "original_form": "오 너네 회사 근처에 맛집 많네",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023634.1.14",
          "form": "근데 정작 유명한 메뉴는 못먹어보고 걍 점심메뉴만^^ㅋㅋ",
          "original_form": "근데 정작 유명한 메뉴는 못먹어보고 걍 점심메뉴만^^ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023634.1.15",
          "form": "카레가 제일 맛있어 아비꼬ㅋㅋ",
          "original_form": "카레가 제일 맛있어 아비꼬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023634.1.16",
          "form": "앜ㅋㅋㅋ\n카레도 맛있지",
          "original_form": "앜ㅋㅋㅋ\n카레도 맛있지",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023634.1.17",
          "form": "족발은 점심에 안파는건가?",
          "original_form": "족발은 점심에 안파는건가?",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023634.1.18",
          "form": "응 족발은 저녁에만 팔아",
          "original_form": "응 족발은 저녁에만 팔아",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023634.1.19",
          "form": "그럼 나 놀러갈 때 포장해오면 되겠다ㅋㅋㅋㅋㅋ",
          "original_form": "그럼 나 놀러갈 때 포장해오면 되겠다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023634.1.20",
          "form": "ㅋㅋㅋ먹고싶어? 아구찜은? 좋아해?",
          "original_form": "ㅋㅋㅋ먹고싶어? 아구찜은? 좋아해?",
          "speaker_id": "1",
          "time": "20210830 13:33"
        }
      ]
