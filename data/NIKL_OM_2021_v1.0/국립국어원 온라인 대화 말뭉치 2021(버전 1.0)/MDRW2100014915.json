        {
          "id": "MDRW2100014915.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100014915.1.2",
          "form": "네 못간지 한참됐어요ㅠ",
          "original_form": "네 못간지 한참됐어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100014915.1.3",
          "form": "어디 가시고 싶으세요~",
          "original_form": "어디 가시고 싶으세요~",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014915.1.4",
          "form": "전 유럽이여",
          "original_form": "전 유럽이여",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100014915.1.5",
          "form": "저도 한번도 안가봐서",
          "original_form": "저도 한번도 안가봐서",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100014915.1.6",
          "form": "이탈리아피자 맛나대요ㅋ",
          "original_form": "이탈리아피자 맛나대요ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100014915.1.7",
          "form": "크 본토 피자",
          "original_form": "크 본토 피자",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100014915.1.8",
          "form": "맛있을듯요",
          "original_form": "맛있을듯요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100014915.1.9",
          "form": "분위기도요ㅎ",
          "original_form": "분위기도요ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100014915.1.10",
          "form": "ㅎㅎ 가고싶네요ㅠㅠ",
          "original_form": "ㅎㅎ 가고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100014915.1.11",
          "form": "언제쯤 갈수있을지ㅠ",
          "original_form": "언제쯤 갈수있을지ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100014915.1.12",
