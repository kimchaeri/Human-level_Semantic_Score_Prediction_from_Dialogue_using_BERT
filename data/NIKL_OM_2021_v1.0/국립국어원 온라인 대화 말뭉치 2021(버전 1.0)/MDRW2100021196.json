      "utterance": [
        {
          "id": "MDRW2100021196.1.1",
          "form": "전 청포도그린티에 먹었어요~",
          "original_form": "전 청포도그린티에 먹었어요~",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021196.1.2",
          "form": "오 그거 한번도 안먹어봤는데",
          "original_form": "오 그거 한번도 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021196.1.3",
          "form": "당도 몇으로 드세요? 다음에 한번 먹어볼게요!",
          "original_form": "당도 몇으로 드세요? 다음에 한번 먹어볼게요!",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021196.1.4",
          "form": "당도는 모든메뉴 다 중간으로 하는거같아요ㅋㅋㅋ",
          "original_form": "당도는 모든메뉴 다 중간으로 하는거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021196.1.5",
          "form": "너무 안단거는 안좋아해서요",
          "original_form": "너무 안단거는 안좋아해서요",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021196.1.6",
          "form": "저는 단걸 조금 피하는 편이라",
          "original_form": "저는 단걸 조금 피하는 편이라",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021196.1.7",
          "form": "당도 30으로 자주먹는데 담에 한번 50으로 청포도그린티 먹어볼게요 ㅎㅎ",
          "original_form": "당도 30으로 자주먹는데 담에 한번 50으로 청포도그린티 먹어볼게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021196.1.8",
          "form": "아하! 간거피하시는거면 30하셔도 충분할거같아요!ㅋㅋㅋ",
          "original_form": "아하! 간거피하시는거면 30하셔도 충분할거같아요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021196.1.9",
          "form": "ㅎㅎ 참고할게용",
          "original_form": "ㅎㅎ 참고할게용",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021196.1.10",
          "form": "베이커리류는 혹시 좋아하세요?",
          "original_form": "베이커리류는 혹시 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021196.1.11",
          "form": "저 완전 빵순이에용ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 완전 빵순이에용ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021196.1.12",
          "form": "오오!!",
          "original_form": "오오!!",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021196.1.13",
          "form": "무슨 빵 제일 좋아하세요?",
          "original_form": "무슨 빵 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021196.1.14",
          "form": "저는 치아바타같은 사워도우 종류의 빵들 좋아해욮ㅎㅎ",
          "original_form": "저는 치아바타같은 사워도우 종류의 빵들 좋아해욮ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021196.1.15",
          "form": "jmt....",
          "original_form": "jmt....",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021196.1.16",
          "form": "저도 그런 것 좋아하구 또 구움과자류 좋아해요",
          "original_form": "저도 그런 것 좋아하구 또 구움과자류 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021196.1.17",
          "form": "오 구움과자 너무좋죠 ㅜㅜ 저 휘낭시에 좋아해여",
          "original_form": "오 구움과자 너무좋죠 ㅜㅜ 저 휘낭시에 좋아해여",
