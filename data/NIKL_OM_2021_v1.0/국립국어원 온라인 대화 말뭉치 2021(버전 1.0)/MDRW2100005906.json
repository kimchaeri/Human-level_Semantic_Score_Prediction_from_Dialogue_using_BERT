        {
          "id": "MDRW2100005906.1.1",
          "form": "그때 아브라함이랑 아내 세라가 99살이었나? 그쯤이었어",
          "original_form": "그때 아브라함이랑 아내 세라가 99살이었나? 그쯤이었어",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100005906.1.2",
          "form": "자식은 없고",
          "original_form": "자식은 없고",
          "speaker_id": "2",
