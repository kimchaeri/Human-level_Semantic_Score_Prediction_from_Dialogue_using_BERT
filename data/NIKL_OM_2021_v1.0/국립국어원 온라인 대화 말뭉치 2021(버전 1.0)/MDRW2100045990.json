      "utterance": [
        {
          "id": "MDRW2100045990.1.1",
          "form": "맞아요 ㅋㅋ 깁콘생기면",
          "original_form": "맞아요 ㅋㅋ 깁콘생기면",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045990.1.2",
          "form": "거기에 보태서 먹어요",
          "original_form": "거기에 보태서 먹어요",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045990.1.3",
          "form": "제값주고 먹지는 못하겠더라구요",
          "original_form": "제값주고 먹지는 못하겠더라구요",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045990.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045990.1.5",
          "form": "가격이 ㅠㅠ",
          "original_form": "가격이 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045990.1.6",
          "form": "넘비싸죠",
          "original_form": "넘비싸죠",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045990.1.7",
          "form": "밥값 저리가라할정도니",
          "original_form": "밥값 저리가라할정도니",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045990.1.8",
          "form": "맞아여 신메뉴보면",
          "original_form": "맞아여 신메뉴보면",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045990.1.9",
          "form": "가격이 후덜덜해요",
          "original_form": "가격이 후덜덜해요",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045990.1.10",
          "form": "전 아메만 거의즐겨서",
          "original_form": "전 아메만 거의즐겨서",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045990.1.11",
          "form": "다행일지도ㅠㅠ",
          "original_form": "다행일지도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045990.1.12",
          "form": "그러네요 ㅎㅎ",
          "original_form": "그러네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045990.1.13",
          "form": "아메가 쓰지않으신가요?",
          "original_form": "아메가 쓰지않으신가요?",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045990.1.14",
          "form": "아뇨ㅠㅠ!! 전넘좋어요",
          "original_form": "아뇨ㅠㅠ!! 전넘좋어요",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045990.1.15",
          "form": "사실 브랜드안가리고 ㅋㅋㅋ",
          "original_form": "사실 브랜드안가리고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045990.1.16",
          "form": "아...저는 어디는 쓴맛이 있어서",
          "original_form": "아...저는 어디는 쓴맛이 있어서",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045990.1.17",
          "form": "전 좀 진한걸좋아햐요! ㅋㅋㅋ",
          "original_form": "전 좀 진한걸좋아햐요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100045990.1.18",
          "form": "다크 한걸 즐기시는군요",
          "original_form": "다크 한걸 즐기시는군요",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100045990.1.19",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100045990.1.20",
          "form": "커피랑 빵먹고싶네요ㅠ",
          "original_form": "커피랑 빵먹고싶네요ㅠ",
