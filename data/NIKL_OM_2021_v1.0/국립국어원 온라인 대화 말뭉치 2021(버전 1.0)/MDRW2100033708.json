        {
          "id": "MDRW2100033708.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아맞아요 예랑가랑 전설의 크리스피도넛처럼",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아맞아요 예랑가랑 전설의 크리스피도넛처럼",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033708.1.2",
          "form": "그래서 업 어디까지 보셧어요",
          "original_form": "그래서 업 어디까지 보셧어요",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033708.1.3",
          "form": "갑자기;",
          "original_form": "갑자기;",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033708.1.4",
          "form": "저 6화!! 딱 6화보고",
          "original_form": "저 6화!! 딱 6화보고",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033708.1.5",
          "form": "하 씁 2화얘기해야겟다...",
          "original_form": "하 씁 2화얘기해야겟다...",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033708.1.6",
          "form": "하고 왔어요 저 진짜",
          "original_form": "하고 왔어요 저 진짜",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033708.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.8",
          "form": "아 진짜 너무 좋더라고요",
          "original_form": "아 진짜 너무 좋더라고요",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.9",
          "form": "2화가 키스신이.",
          "original_form": "2화가 키스신이.",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.10",
          "form": "2화가ㅡ제일 좋죠",
          "original_form": "2화가ㅡ제일 좋죠",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.11",
          "form": "하 씁...얘네가....",
          "original_form": "하 씁...얘네가....",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.12",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.13",
          "form": "아니 씁",
          "original_form": "아니 씁",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.14",
          "form": "잘알이다...이거 만든사람이 연출할줄안다.....",
          "original_form": "잘알이다...이거 만든사람이 연출할줄안다.....",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.15",
          "form": "그 업 위주로 비추는게",
          "original_form": "그 업 위주로 비추는게",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.16",
          "form": "사람 돌아버려요",
          "original_form": "사람 돌아버려요",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.17",
          "form": "그리고 끝난줄 알았는데",
          "original_form": "그리고 끝난줄 알았는데",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.18",
          "form": "아 그러니까요 와 저",
          "original_form": "아 그러니까요 와 저",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.19",
          "form": "글쓰면서 회상한다?????",
          "original_form": "글쓰면서 회상한다?????",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033708.1.20",
          "form": "아 이건...",
          "original_form": "아 이건...",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
