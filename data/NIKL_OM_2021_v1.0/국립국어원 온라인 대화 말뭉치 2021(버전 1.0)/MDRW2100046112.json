      "utterance": [
        {
          "id": "MDRW2100046112.1.1",
          "form": "네 잘 먹어요",
          "original_form": "네 잘 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046112.1.2",
          "form": "매운 음식 좋아하는 편입니다",
          "original_form": "매운 음식 좋아하는 편입니다",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046112.1.3",
          "form": "우아 어떤거 좋아하세요",
          "original_form": "우아 어떤거 좋아하세요",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046112.1.4",
          "form": "저는 닭발.. 불족발..",
          "original_form": "저는 닭발.. 불족발..",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046112.1.5",
          "form": "아불족발 너무 좋죠",
          "original_form": "아불족발 너무 좋죠",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046112.1.6",
          "form": "저도 불족발 정말 좋아해요",
          "original_form": "저도 불족발 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.7",
          "form": "네 ㅠㅜ 너무 좋아요",
          "original_form": "네 ㅠㅜ 너무 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.8",
          "form": "불맛 좋아하세요",
          "original_form": "불맛 좋아하세요",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.10",
          "form": "네 그럼요",
          "original_form": "네 그럼요",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.11",
          "form": "불맛 캬 ㅋㅋㅋㅋ",
          "original_form": "불맛 캬 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.12",
          "form": "닭발도 좋아요 ㅋㅋㅋ",
          "original_form": "닭발도 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.13",
          "form": "네 닭발도 사랑하죠 ㅎㅎ",
          "original_form": "네 닭발도 사랑하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.14",
          "form": "국물..닭발 이요 ㅋㅋ",
          "original_form": "국물..닭발 이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.15",
          "form": "뼈있는걸로 드세요?",
          "original_form": "뼈있는걸로 드세요?",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.16",
          "form": "매운 오뎅이 좋아하세요??",
          "original_form": "매운 오뎅이 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.17",
          "form": "너무 매운건 못먹어요 ㅋㅋㅋ",
          "original_form": "너무 매운건 못먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.18",
          "form": "저는 뼈 있는 것도 먹고 뼈 없는 것도 먹어요",
          "original_form": "저는 뼈 있는 것도 먹고 뼈 없는 것도 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046112.1.19",
          "form": "뼈는 중요한 게 아니니깐요",
          "original_form": "뼈는 중요한 게 아니니깐요",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046112.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046112.1.21",
          "form": "맞아욬ㅋㅋㅋ 둘다 좋죠",
          "original_form": "맞아욬ㅋㅋㅋ 둘다 좋죠",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046112.1.22",
          "form": "계란찜도 맛이ㅅ어요 ㅋㅋ",
          "original_form": "계란찜도 맛이ㅅ어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046112.1.23",
          "form": "네 맞습니다",
          "original_form": "네 맞습니다",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046112.1.24",
          "form": "어제 계란찜을 먹었어요",
