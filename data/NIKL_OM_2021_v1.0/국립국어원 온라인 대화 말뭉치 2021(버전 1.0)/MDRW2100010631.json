      "utterance": [
        {
          "id": "MDRW2100010631.1.1",
          "form": "VR게임이 저한테 약간 그러더라고요",
          "original_form": "VR게임이 저한테 약간 그러더라고요",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010631.1.2",
          "form": "오 3디 게임 하니까 생각났는데 이번에 언리얼엔진이 혁신이라고 하더라고요",
          "original_form": "오 3디 게임 하니까 생각났는데 이번에 언리얼엔진이 혁신이라고 하더라고요",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010631.1.3",
          "form": "언리얼엔진은 그래픽 안끊기는 기술을 개발했다고 하더라고요",
          "original_form": "언리얼엔진은 그래픽 안끊기는 기술을 개발했다고 하더라고요",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010631.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010631.1.5",
          "form": "점점 그래픽 기술이 높아지네요",
          "original_form": "점점 그래픽 기술이 높아지네요",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010631.1.6",
          "form": "맞아요 이제",
          "original_form": "맞아요 이제",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010631.1.7",
          "form": "그래픽 느낌 없이 배경 만들 수 있을 것 같아요",
          "original_form": "그래픽 느낌 없이 배경 만들 수 있을 것 같아요",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010631.1.8",
          "form": "VR 겜방 혹시 가보셨나요?",
          "original_form": "VR 겜방 혹시 가보셨나요?",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010631.1.9",
          "form": "1번 가봤어요",
          "original_form": "1번 가봤어요",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010631.1.10",
          "form": "짧게 하다가 나오긴 했지만요",
          "original_form": "짧게 하다가 나오긴 했지만요",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010631.1.11",
          "form": "저도 그 벽돌쌓기 함 해보고",
          "original_form": "저도 그 벽돌쌓기 함 해보고",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010631.1.12",
          "form": "게임이구나싶어서 나왔어요",
          "original_form": "게임이구나싶어서 나왔어요",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010631.1.13",
          "form": "근데 진짜같은 롤러코스터겜 나오면 매일 갈 것 같아요",
          "original_form": "근데 진짜같은 롤러코스터겜 나오면 매일 갈 것 같아요",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010631.1.14",
          "form": "취향이 확고하시군요!",
          "original_form": "취향이 확고하시군요!",
          "speaker_id": "1",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010631.1.15",
          "form": "좀비겜이라던가",
          "original_form": "좀비겜이라던가",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010631.1.16",
          "form": "무서운 스토리의 게임도 하시나요?",
          "original_form": "무서운 스토리의 게임도 하시나요?",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010631.1.17",
          "form": "예전에 대도서관 씨였나 그분이 무서운 겜 스트리밍 하는 걸 봤었어요",
          "original_form": "예전에 대도서관 씨였나 그분이 무서운 겜 스트리밍 하는 걸 봤었어요",
          "speaker_id": "2",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010631.1.18",
          "form": "공포 게임 스트리머가 하는 걸 보는 건 좋아하는데",
          "original_form": "공포 게임 스트리머가 하는 걸 보는 건 좋아하는데",
          "speaker_id": "1",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010631.1.19",
          "form": "제가 직접 플레이 해본 적은 없는 거 같아요",
          "original_form": "제가 직접 플레이 해본 적은 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010631.1.20",
          "form": "ㅡ저도",
          "original_form": "ㅡ저도",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010631.1.21",
          "form": "아 그 추리겜도 있는데",
          "original_form": "아 그 추리겜도 있는데",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010631.1.22",
          "form": "그 방탈출겜같은거",
          "original_form": "그 방탈출겜같은거",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010631.1.23",
          "form": "해보셨나요?!",
          "original_form": "해보셨나요?!",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010631.1.24",
          "form": "저 그거 하는데 1도 못맞췄어요",
          "original_form": "저 그거 하는데 1도 못맞췄어요",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010631.1.25",
          "form": "방탈출 핸드폰 게임은 몇 번 해봤어요",
          "original_form": "방탈출 핸드폰 게임은 몇 번 해봤어요",
          "speaker_id": "1",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100010631.1.26",
          "form": "헉 모발겜도 있나요?!!!!!",
          "original_form": "헉 모발겜도 있나요?!!!!!",
          "speaker_id": "2",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100010631.1.27",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100010631.1.28",
          "form": "퍼즐 푸는 걸 좋아해서 재밌게 했어요",
          "original_form": "퍼즐 푸는 걸 좋아해서 재밌게 했어요",
          "speaker_id": "1",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100010631.1.29",
          "form": "꽤 여러 종류 있을 걸요!",
          "original_form": "꽤 여러 종류 있을 걸요!",
          "speaker_id": "1",
          "time": "20210823 20:29"
        },
        {
          "id": "MDRW2100010631.1.30",
          "form": "헐 신기하다 저도 해볼게요!!!!!",
          "original_form": "헐 신기하다 저도 해볼게요!!!!!",
          "speaker_id": "2",
          "time": "20210823 20:29"
        },
        {
          "id": "MDRW2100010631.1.31",
          "form": "아 퍼즐 언제나 즐겁죠",
          "original_form": "아 퍼즐 언제나 즐겁죠",
          "speaker_id": "2",
          "time": "20210823 20:29"
        },
        {
          "id": "MDRW2100010631.1.32",
          "form": "그 모여서 하는 퍼즐겜 요즘 열심히한답니다",
          "original_form": "그 모여서 하는 퍼즐겜 요즘 열심히한답니다",
          "speaker_id": "2",
          "time": "20210823 20:30"
        }
      ]
    }
