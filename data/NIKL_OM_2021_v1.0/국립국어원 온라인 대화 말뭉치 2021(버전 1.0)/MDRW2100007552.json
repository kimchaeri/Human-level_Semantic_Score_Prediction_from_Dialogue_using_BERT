      "utterance": [
        {
          "id": "MDRW2100007552.1.1",
          "form": "ㅋㅋㅋㅋㅋ좋아",
          "original_form": "ㅋㅋㅋㅋㅋ좋아",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007552.1.2",
          "form": "언제결혼할거야",
          "original_form": "언제결혼할거야",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007552.1.3",
          "form": "나 못하지",
          "original_form": "나 못하지",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007552.1.4",
          "form": "능력이 없지",
          "original_form": "능력이 없지",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007552.1.5",
          "form": "아니 그럼 왜 결혼을 하자구해",
          "original_form": "아니 그럼 왜 결혼을 하자구해",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007552.1.6",
          "form": "그럼...",
          "original_form": "그럼...",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007552.1.7",
          "form": "비혼에 대해 얘기ㅏ나누자",
          "original_form": "비혼에 대해 얘기ㅏ나누자",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007552.1.8",
          "form": "아니 나 결혼을 하고싶은데",
          "original_form": "아니 나 결혼을 하고싶은데",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007552.1.9",
          "form": "능력이 없는거지",
          "original_form": "능력이 없는거지",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007552.1.10",
          "form": "집 차 얼굴 사람 매력 직장",
          "original_form": "집 차 얼굴 사람 매력 직장",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007552.1.11",
          "form": "흠... 보통 집은 남자가 해오는게 국룰인걸!",
          "original_form": "흠... 보통 집은 남자가 해오는게 국룰인걸!",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007552.1.12",
          "form": "남자가 집 해오면 차정도 해가면 되지않을까?",
          "original_form": "남자가 집 해오면 차정도 해가면 되지않을까?",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007552.1.13",
          "form": "중요한 점 그런 남자는 나를 만나지 않는다",
          "original_form": "중요한 점 그런 남자는 나를 만나지 않는다",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007552.1.14",
          "form": "그리고 그건 성차별인거같애",
          "original_form": "그리고 그건 성차별인거같애",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007552.1.15",
          "form": "집은 기준이 넉넉한걸...",
          "original_form": "집은 기준이 넉넉한걸...",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007552.1.16",
          "form": "5천만원 집이 있는가하면",
          "original_form": "5천만원 집이 있는가하면",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007552.1.17",
          "form": "5억짜리 집도 있지...",
          "original_form": "5억짜리 집도 있지...",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007552.1.18",
          "form": "엥 아니야",
          "original_form": "엥 아니야",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007552.1.19",
          "form": "여자가 결혼생활하면서 희생하는게 더 많은게 당연히 예정된 수순이니까",
          "original_form": "여자가 결혼생활하면서 희생하는게 더 많은게 당연히 예정된 수순이니까",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007552.1.20",
          "form": "그건 당연히 받아야하능거고 그게 사회적 약속이야!",
          "original_form": "그건 당연히 받아야하능거고 그게 사회적 약속이야!",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007552.1.21",
          "form": "친구야...",
          "original_form": "친구야...",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007552.1.22",
          "form": "일단 나한테 집을 줄 남자는 없어...",
          "original_form": "일단 나한테 집을 줄 남자는 없어...",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007552.1.23",
          "form": "친구야 내가 아까도 말했지만",
          "original_form": "친구야 내가 아까도 말했지만",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007552.1.24",
          "form": "집은 5천만원짜리도 있도",
          "original_form": "집은 5천만원짜리도 있도",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007552.1.25",
          "form": "5억짜리도 있어",
          "original_form": "5억짜리도 있어",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.26",
          "form": "남자도 마찬가지로 1등급이있고",
          "original_form": "남자도 마찬가지로 1등급이있고",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.27",
          "form": "9등급이 있겠지",
          "original_form": "9등급이 있겠지",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.28",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "1",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.29",
          "form": "결혼을 (스펙짱짱5억집해오는남자랑) 못한다",
          "original_form": "결혼을 (스펙짱짱5억집해오는남자랑) 못한다",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.30",
          "form": "이지",
          "original_form": "이지",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.31",
          "form": "내가 몇등급인지 알면 결혼이 힘든거지",
          "original_form": "내가 몇등급인지 알면 결혼이 힘든거지",
          "speaker_id": "1",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.32",
          "form": "결혼을 (노스펙500만원짜리반지하집해오는남자와) 못한다",
          "original_form": "결혼을 (노스펙500만원짜리반지하집해오는남자와) 못한다",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007552.1.33",
          "form": "이게 아니잖아!!",
          "original_form": "이게 아니잖아!!",
          "speaker_id": "2",
          "time": "20210822 03:20"
        }
      ]
    }
