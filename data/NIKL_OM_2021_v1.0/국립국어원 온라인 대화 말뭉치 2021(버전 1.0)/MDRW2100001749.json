      "utterance": [
        {
          "id": "MDRW2100001749.1.1",
          "form": "name2이님은 학생이세요?",
          "original_form": "&name2&이님은 학생이세요?",
          "speaker_id": "1",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001749.1.2",
          "form": "네 학생이에요",
          "original_form": "네 학생이에요",
          "speaker_id": "2",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001749.1.3",
          "form": "지금 중간고사가 일주일정도 남아서",
          "original_form": "지금 중간고사가 일주일정도 남아서",
          "speaker_id": "2",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001749.1.4",
          "form": "공부하느라바쁘죠",
          "original_form": "공부하느라바쁘죠",
          "speaker_id": "2",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001749.1.5",
          "form": "헐 얼마안남았네요!!",
          "original_form": "헐 얼마안남았네요!!",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001749.1.6",
          "form": "요즘에도 학교 가요? 정규수업 해요?",
          "original_form": "요즘에도 학교 가요? 정규수업 해요?",
          "speaker_id": "1",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001749.1.7",
          "form": "네 저희는 정상등교해요!",
          "original_form": "네 저희는 정상등교해요!",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001749.1.8",
          "form": "name1님은 학생이신가요? 학교 다니세요?",
          "original_form": "&name1&님은 학생이신가요? 학교 다니세요?",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001749.1.9",
          "form": "저는 대학교까지 다 졸업해서... 다른 시험 준비해요!",
          "original_form": "저는 대학교까지 다 졸업해서... 다른 시험 준비해요!",
          "speaker_id": "1",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001749.1.10",
          "form": "대학교까지 다 나와도 시험은 끝이 없네요....",
          "original_form": "대학교까지 다 나와도 시험은 끝이 없네요....",
          "speaker_id": "1",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001749.1.11",
          "form": "학교에서는 어떤 동아리 해요??",
          "original_form": "학교에서는 어떤 동아리 해요??",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001749.1.12",
          "form": "와.. 대학교까지 졸업해도 시험이 여전하다니..끔찍하네요",
          "original_form": "와.. 대학교까지 졸업해도 시험이 여전하다니..끔찍하네요",
          "speaker_id": "2",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100001749.1.13",
          "form": "동아리는 교육동아리 하는데 활동은 잘 안해요",
          "original_form": "동아리는 교육동아리 하는데 활동은 잘 안해요",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001749.1.14",
          "form": "교육동아리면 생활기록부에 적히기에 좋겠네요!!",
          "original_form": "교육동아리면 생활기록부에 적히기에 좋겠네요!!",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100001749.1.15",
          "form": "시험은 3일동안 봐요?",
          "original_form": "시험은 3일동안 봐요?",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100001749.1.16",
          "form": "네! 3일동안 해요",
          "original_form": "네! 3일동안 해요",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100001749.1.17",
          "form": "시험 다 끝나면 짧게 방학하나요...? 여름방학이 늦네요...",
          "original_form": "시험 다 끝나면 짧게 방학하나요...? 여름방학이 늦네요...",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001749.1.18",
          "form": "여름방학 끝났어요ㅜ 개학한지 얼마안됐어요",
          "original_form": "여름방학 끝났어요ㅜ 개학한지 얼마안됐어요",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001749.1.19",
          "form": "고삼이라서 중간시험을 개학거의 하자마자",
          "original_form": "고삼이라서 중간시험을 개학거의 하자마자",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001749.1.20",
          "form": "치거든요ㅠㅠ",
          "original_form": "치거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001749.1.21",
          "form": "허얼 그렇구나",
          "original_form": "허얼 그렇구나",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001749.1.22",
          "form": "아 왠지 ㅜㅜ 친구가 초등학교선생님인데 초등학생들 곧 개학한다고 그러더라고요.... 그래서 고등학생이나 중학생은 개학을 벌써한건지.. 아직 방학도 안한건지..궁금했는데",
          "original_form": "아 왠지 ㅜㅜ 친구가 초등학교선생님인데 초등학생들 곧 개학한다고 그러더라고요.... 그래서 고등학생이나 중학생은 개학을 벌써한건지.. 아직 방학도 안한건지..궁금했는데",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001749.1.23",
          "form": "그러면 정상등교하는거니까 완전 아침에 가야겠네요ㅜ",
          "original_form": "그러면 정상등교하는거니까 완전 아침에 가야겠네요ㅜ",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001749.1.24",
          "form": "9시 등교예요?",
          "original_form": "9시 등교예요?",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001749.1.25",
          "form": "아니요 저희는 7시 50분까지가요ㅜ",
          "original_form": "아니요 저희는 7시 50분까지가요ㅜ",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001749.1.26",
          "form": "일어나기힘들죠",
          "original_form": "일어나기힘들죠",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001749.1.27",
          "form": "허얼 아직도....",
          "original_form": "허얼 아직도....",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001749.1.28",
          "form": "저는 늦게자는건 되는데 일찍 일어나는 건 못해서 학교다닐 때 매일 아슬아슬하게 도착하고 그랬어요ㅠ",
          "original_form": "저는 늦게자는건 되는데 일찍 일어나는 건 못해서 학교다닐 때 매일 아슬아슬하게 도착하고 그랬어요ㅠ",
          "speaker_id": "1",
