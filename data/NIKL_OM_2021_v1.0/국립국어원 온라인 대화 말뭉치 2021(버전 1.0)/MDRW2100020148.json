      "utterance": [
        {
          "id": "MDRW2100020148.1.1",
          "form": "후쿠오카랑.. 도쿄에서 1년살고요..",
          "original_form": "후쿠오카랑.. 도쿄에서 1년살고요..",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020148.1.2",
          "form": "1년이나 살아봤어요?",
          "original_form": "1년이나 살아봤어요?",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020148.1.3",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020148.1.4",
          "form": "저는 후쿠오카 다녀왔었어요",
          "original_form": "저는 후쿠오카 다녀왔었어요",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020148.1.5",
          "form": "어학연수로요.. ㅎㅎㅎ",
          "original_form": "어학연수로요.. ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020148.1.6",
          "form": "하카타역?",
          "original_form": "하카타역?",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020148.1.7",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020148.1.8",
          "form": "네ㅔ 하카타역!!!",
          "original_form": "네ㅔ 하카타역!!!",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020148.1.9",
          "form": "완전 오랜만에 듣는 이름이네요",
          "original_form": "완전 오랜만에 듣는 이름이네요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020148.1.10",
          "form": "그 무슨 해변? 캐널시티? ㅋㅋㅋ",
          "original_form": "그 무슨 해변? 캐널시티? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020148.1.11",
          "form": "어떻게 가셧어요? 전 20살때 배타고..",
          "original_form": "어떻게 가셧어요? 전 20살때 배타고..",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020148.1.12",
          "form": "전 2년전쯤? 비행기타고 다녀왔어요",
          "original_form": "전 2년전쯤? 비행기타고 다녀왔어요",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020148.1.13",
          "form": "넘 재밌더라구요!!!!",
          "original_form": "넘 재밌더라구요!!!!",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020148.1.14",
          "form": "아.. 후쿠오카만 가셧어요? 나가사키나? 아소나?",
          "original_form": "아.. 후쿠오카만 가셧어요? 나가사키나? 아소나?",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020148.1.15",
          "form": "후쿠오카랑 교토 묶어서 갔었어요!",
          "original_form": "후쿠오카랑 교토 묶어서 갔었어요!",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020148.1.16",
          "form": "또 어디더라 온천있는 동네인데",
          "original_form": "또 어디더라 온천있는 동네인데",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020148.1.17",
          "form": "이름이 기억이 안나네요",
          "original_form": "이름이 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020148.1.18",
          "form": "벳뿌?",
          "original_form": "벳뿌?",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020148.1.19",
          "form": "후쿠오카랑 쿄토 먼데... \n기차로 이동햇어요?",
          "original_form": "후쿠오카랑 쿄토 먼데... \n기차로 이동햇어요?",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020148.1.20",
          "form": "네네 기차타고 갔던 것 같아요",
          "original_form": "네네 기차타고 갔던 것 같아요",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020148.1.21",
          "form": "재밌었는데~",
          "original_form": "재밌었는데~",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020148.1.22",
          "form": "또 가고싶네용",
          "original_form": "또 가고싶네용",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020148.1.23",
          "form": "일본 뭔가 깔끔해서 좋아요...",
          "original_form": "일본 뭔가 깔끔해서 좋아요...",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020148.1.24",
          "form": "저도 일본 다시 가고싶네요",
          "original_form": "저도 일본 다시 가고싶네요",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020148.1.25",
          "form": "맞아요 길냥이도 많고",
          "original_form": "맞아요 길냥이도 많고",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020148.1.26",
          "form": "언제쯤 갈 수 있을까요~",
          "original_form": "언제쯤 갈 수 있을까요~",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020148.1.27",
          "form": "내년말은 되야할꺼 같아요.. \n짐 일본은 코로나 개판이라..",
          "original_form": "내년말은 되야할꺼 같아요.. \n짐 일본은 코로나 개판이라..",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020148.1.28",
          "form": "ㅜㅠ슬프네요",
          "original_form": "ㅜㅠ슬프네요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        }
      ]
    }
