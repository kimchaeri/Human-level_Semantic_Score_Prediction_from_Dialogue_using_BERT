      "utterance": [
        {
          "id": "MDRW2100004166.1.1",
          "form": "너 이번 올림픽 봤니?",
          "original_form": "너 이번 올림픽 봤니?",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004166.1.2",
          "form": "이번 올림픽 대박이였지",
          "original_form": "이번 올림픽 대박이였지",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004166.1.3",
          "form": "나는 이번에 정말 김연경 선수랑 김희진 선수랑 안산 선수랑",
          "original_form": "나는 이번에 정말 김연경 선수랑 김희진 선수랑 안산 선수랑",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004166.1.4",
          "form": "아니 사실 다 짱이셨어",
          "original_form": "아니 사실 다 짱이셨어",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004166.1.5",
          "form": "맞아 다 짱이지",
          "original_form": "맞아 다 짱이지",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004166.1.6",
          "form": "김연경 선수 목걸이 봤니?",
          "original_form": "김연경 선수 목걸이 봤니?",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004166.1.7",
          "form": "무슨 목걸이?",
          "original_form": "무슨 목걸이?",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.9",
          "form": "그 팬들이 선물해준",
          "original_form": "그 팬들이 선물해준",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.10",
          "form": "목걸이?",
          "original_form": "목걸이?",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.11",
          "form": "봤지 봤지ㅠㅠ",
          "original_form": "봤지 봤지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.13",
          "form": "그거 금메달",
          "original_form": "그거 금메달",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.14",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.15",
          "form": "나 방금 티비에 김연경 선수 나왔어",
          "original_form": "나 방금 티비에 김연경 선수 나왔어",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004166.1.16",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004166.1.17",
          "form": "어디에?",
          "original_form": "어디에?",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004166.1.18",
          "form": "링티 광고 찍었나봐",
          "original_form": "링티 광고 찍었나봐",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004166.1.19",
          "form": "김연경 선수 잘되는거 너무 좋아",
          "original_form": "김연경 선수 잘되는거 너무 좋아",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004166.1.20",
          "form": "아아 그거 올림픽 전부터 모델이셨어",
          "original_form": "아아 그거 올림픽 전부터 모델이셨어",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004166.1.21",
          "form": "진짜 더 잘되셨으면 좋겠어ㅠㅠ",
          "original_form": "진짜 더 잘되셨으면 좋겠어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004166.1.22",
          "form": "이번에 중국리그로 떠나신다더라",
          "original_form": "이번에 중국리그로 떠나신다더라",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004166.1.23",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.24",
          "form": "너는 그러면",
          "original_form": "너는 그러면",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.25",
          "form": "너는 여자배구 팀 중에 응원하는 팀 있어?",
          "original_form": "너는 여자배구 팀 중에 응원하는 팀 있어?",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.26",
          "form": "코로나 끝나고",
          "original_form": "코로나 끝나고",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.27",
          "form": "직관",
          "original_form": "직관",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.28",
          "form": "가고싶지ㅜㅜㅜ",
          "original_form": "가고싶지ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.29",
          "form": "나는 기업!! 김희진 선수!!",
          "original_form": "나는 기업!! 김희진 선수!!",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.30",
          "form": "그 속도감을 느껴보고싶어",
          "original_form": "그 속도감을 느껴보고싶어",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004166.1.31",
          "form": "김희진 선수 진짜 멋져",
          "original_form": "김희진 선수 진짜 멋져",
          "speaker_id": "1",
          "time": "20210821 15:31"
        }
      ]
