      "utterance": [
        {
          "id": "MDRW2100044347.1.1",
          "form": "육퇴 하셨어요?",
          "original_form": "육퇴 하셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044347.1.2",
          "form": "네 육퇴했어요^^",
          "original_form": "네 육퇴했어요^^",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044347.1.3",
          "form": "아이들은 일찍 자나요??",
          "original_form": "아이들은 일찍 자나요??",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044347.1.4",
          "form": "아뇨..저희 애들은 11시 넘어서 자요 ㅠ",
          "original_form": "아뇨..저희 애들은 11시 넘어서 자요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044347.1.5",
          "form": "어이쿠 힘드시겠어요 ㅠ",
          "original_form": "어이쿠 힘드시겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044347.1.6",
          "form": "어린이집에서 다 낮잠을 자고 오나봐요",
          "original_form": "어린이집에서 다 낮잠을 자고 오나봐요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044347.1.7",
          "form": "아뇨 첫째는 안 자고 오는데도 그 때까지 버티더라구요 ㅠ",
          "original_form": "아뇨 첫째는 안 자고 오는데도 그 때까지 버티더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044347.1.8",
          "form": "둘째는 자고 와요~",
          "original_form": "둘째는 자고 와요~",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044347.1.9",
          "form": "잠이 없는 아이인가봐요 \n아이들따라 잠시간이 있나봐요",
          "original_form": "잠이 없는 아이인가봐요 \n아이들따라 잠시간이 있나봐요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044347.1.10",
          "form": "잠이 없는 아이는\n엄마가 힘들죠",
          "original_form": "잠이 없는 아이는\n엄마가 힘들죠",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044347.1.11",
          "form": "저희 아이들은 밤잠은 일찍 자는데",
          "original_form": "저희 아이들은 밤잠은 일찍 자는데",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044347.1.12",
          "form": "낮잠 자는 걸 엄청 싫어해서",
          "original_form": "낮잠 자는 걸 엄청 싫어해서",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044347.1.13",
          "form": "둘 다 4살 때 어린이집 갈 때",
          "original_form": "둘 다 4살 때 어린이집 갈 때",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044347.1.14",
          "form": "자기 싫다고 매일 울면서 갔어요ㅠㅠ",
          "original_form": "자기 싫다고 매일 울면서 갔어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044347.1.15",
          "form": "아이고 ㅠㅠ 울 정도로 자는 게 싫었군요~",
          "original_form": "아이고 ㅠㅠ 울 정도로 자는 게 싫었군요~",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044347.1.16",
          "form": "밤잠 없으니 제 시간이 없어요 ㅠ",
          "original_form": "밤잠 없으니 제 시간이 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044347.1.17",
          "form": "복직하니 피곤해서 아이들 재우다 같이 잠들더라고요...",
          "original_form": "복직하니 피곤해서 아이들 재우다 같이 잠들더라고요...",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044347.1.18",
          "form": "그러시겠어요 ㅠ",
          "original_form": "그러시겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044347.1.19",
          "form": "원랜 일어나서 새벽까지 깨어있다가 잤는데",
          "original_form": "원랜 일어나서 새벽까지 깨어있다가 잤는데",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044347.1.20",
          "form": "애들이 일찍\n잠들어야 혼자 좀 쉴수있는데",
          "original_form": "애들이 일찍\n잠들어야 혼자 좀 쉴수있는데",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044347.1.21",
          "form": "일 하시니 새벽에도 깨어 있기 힘들겠어요",
          "original_form": "일 하시니 새벽에도 깨어 있기 힘들겠어요",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044347.1.22",
          "form": "맞아요 ㅠ 한번 적응안되서 늦게 잤는데 그 날 바로 늦잠자서 허둥지둥 나갔네요ㅋㅋㅋ",
          "original_form": "맞아요 ㅠ 한번 적응안되서 늦게 잤는데 그 날 바로 늦잠자서 허둥지둥 나갔네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044347.1.23",
          "form": "그렇죠 ㅠ 저도 일을 해서 일찍 자야되는데",
          "original_form": "그렇죠 ㅠ 저도 일을 해서 일찍 자야되는데",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044347.1.24",
          "form": "애들 9시에 잠들고 나면",
          "original_form": "애들 9시에 잠들고 나면",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044347.1.25",
          "form": "늘 놀고 싶어서 놀다가 다음날 피곤해서 힘들어요",
          "original_form": "늘 놀고 싶어서 놀다가 다음날 피곤해서 힘들어요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044347.1.26",
          "form": "ㅋㅋㅋ극공감.\n쉴 땐 그냥 보내놓고 자면 됐는데 이제 그런거 없으니 힘들어요",
          "original_form": "ㅋㅋㅋ극공감.\n쉴 땐 그냥 보내놓고 자면 됐는데 이제 그런거 없으니 힘들어요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        }
      ]
    }
