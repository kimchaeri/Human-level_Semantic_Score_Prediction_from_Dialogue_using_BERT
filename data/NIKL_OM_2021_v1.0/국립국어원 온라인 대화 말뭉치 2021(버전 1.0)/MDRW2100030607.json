      "utterance": [
        {
          "id": "MDRW2100030607.1.1",
          "form": "이케아에서 조명 샀는데 좋어요",
          "original_form": "이케아에서 조명 샀는데 좋어요",
          "speaker_id": "2",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100030607.1.2",
          "form": "조명 큰거요?아니면 단스탠드?",
          "original_form": "조명 큰거요?아니면 단스탠드?",
          "speaker_id": "1",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100030607.1.3",
          "form": "단스탠드 샀어요",
          "original_form": "단스탠드 샀어요",
          "speaker_id": "2",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100030607.1.4",
          "form": "5천원인데 만족도 5만원",
          "original_form": "5천원인데 만족도 5만원",
          "speaker_id": "2",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100030607.1.5",
          "form": "저두 조명이 없어서 사고싶었거든요",
          "original_form": "저두 조명이 없어서 사고싶었거든요",
          "speaker_id": "1",
          "time": "20210904 10:11"
        },
        {
          "id": "MDRW2100030607.1.6",
          "form": "고민이네여 근데",
          "original_form": "고민이네여 근데",
          "speaker_id": "1",
          "time": "20210904 10:11"
        },
        {
          "id": "MDRW2100030607.1.7",
          "form": "이케아 이쁜거많아요 저렴하고",
          "original_form": "이케아 이쁜거많아요 저렴하고",
          "speaker_id": "2",
          "time": "20210904 10:11"
        },
        {
          "id": "MDRW2100030607.1.8",
          "form": "아무거나사도 이뻐요 정말 ㅋㅋ",
          "original_form": "아무거나사도 이뻐요 정말 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:11"
        },
        {
          "id": "MDRW2100030607.1.9",
          "form": "한번 가보려구요ㅋㅋ",
          "original_form": "한번 가보려구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030607.1.10",
          "form": "제가 예전부터 사고싶었던 조명이있었는데",
          "original_form": "제가 예전부터 사고싶었던 조명이있었는데",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030607.1.11",
          "form": "너무 비싸서",
          "original_form": "너무 비싸서",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030607.1.12",
          "form": "약간 타협점을 찾고 있었어요...ㅠㅠ",
          "original_form": "약간 타협점을 찾고 있었어요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030607.1.13",
          "form": "오 얼마정도인가요",
          "original_form": "오 얼마정도인가요",
          "speaker_id": "2",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100030607.1.14",
          "form": "빈티지조명이라 세상에 단하나뿐인거거등요",
          "original_form": "빈티지조명이라 세상에 단하나뿐인거거등요",
          "speaker_id": "1",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030607.1.15",
          "form": "16만원이에여",
          "original_form": "16만원이에여",
          "speaker_id": "1",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030607.1.16",
          "form": "ㅠㅠㅠㅠ근데진짜 너무 이쁘게생겼어요",
          "original_form": "ㅠㅠㅠㅠ근데진짜 너무 이쁘게생겼어요",
          "speaker_id": "1",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030607.1.17",
          "form": "불빛이 퍼지는게 물가에 일렁이는 빛같이 퍼져서 너무 이쁩니다...",
          "original_form": "불빛이 퍼지는게 물가에 일렁이는 빛같이 퍼져서 너무 이쁩니다...",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030607.1.18",
          "form": "16만원정도묜 괜찮은데요?",
          "original_form": "16만원정도묜 괜찮은데요?",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030607.1.19",
          "form": "지르세요!!",
          "original_form": "지르세요!!",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030607.1.20",
          "form": "쫌쫌따리로 모으고있긴해요",
          "original_form": "쫌쫌따리로 모으고있긴해요",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030607.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030607.1.22",
          "form": "이쁠것같아요",
          "original_form": "이쁠것같아요",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030607.1.23",
          "form": "이뻐요!!",
          "original_form": "이뻐요!!",
          "speaker_id": "1",
          "time": "20210904 12:55"
        },
        {
          "id": "MDRW2100030607.1.24",
          "form": "약간 전등 갓같은게 돌아가는데",
          "original_form": "약간 전등 갓같은게 돌아가는데",
          "speaker_id": "1",
          "time": "20210904 12:55"
        },
        {
          "id": "MDRW2100030607.1.25",
          "form": "움직이면서 뭔가 찰랑~하는 느낌이있어요",
          "original_form": "움직이면서 뭔가 찰랑~하는 느낌이있어요",
          "speaker_id": "1",
          "time": "20210904 12:56"
        },
        {
          "id": "MDRW2100030607.1.26",
          "form": "저만 떠들었네요ㅋㅋㅋ\r\n사고싶은 물건 같은거 있으신가영...ㅎㅎ",
          "original_form": "저만 떠들었네요ㅋㅋㅋ\r\n사고싶은 물건 같은거 있으신가영...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:56"
        },
        {
          "id": "MDRW2100030607.1.27",
          "form": "오 뭠지 궁금해요 검색해봐도되나요?",
          "original_form": "오 뭠지 궁금해요 검색해봐도되나요?",
          "speaker_id": "2",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030607.1.28",
          "form": "갑자기 찾으려니까 안나와서",
          "original_form": "갑자기 찾으려니까 안나와서",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030607.1.29",
          "form": "인스타에 delight._.glass라고 치시면 비슷한 거 나오는데 벽등에 불비치는게 비슷한 느낌이에요!!",
          "original_form": "인스타에 delight._.glass라고 치시면 비슷한 거 나오는데 벽등에 불비치는게 비슷한 느낌이에요!!",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030607.1.30",
          "form": "물론 공예가 아니라 곡선 유리 갓이지만",
          "original_form": "물론 공예가 아니라 곡선 유리 갓이지만",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030607.1.31",
          "form": "오ㅏ 진짜이쁘네요",
          "original_form": "오ㅏ 진짜이쁘네요",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100030607.1.32",
          "form": "성당에도 이런 유리공예 많잖아요 창문에",
          "original_form": "성당에도 이런 유리공예 많잖아요 창문에",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100030607.1.33",
          "form": "네네!!저것도 이쁘죠ㅠㅠㅠ",
          "original_form": "네네!!저것도 이쁘죠ㅠㅠㅠ",
