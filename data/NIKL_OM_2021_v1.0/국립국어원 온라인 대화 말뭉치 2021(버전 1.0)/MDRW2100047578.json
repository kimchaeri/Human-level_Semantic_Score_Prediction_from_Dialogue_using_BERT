        {
          "id": "MDRW2100047578.1.1",
          "form": "잘하는 요리 있으신가요?",
          "original_form": "잘하는 요리 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047578.1.2",
          "form": "그나마 볶음밥이요 ㅎㅎ",
          "original_form": "그나마 볶음밥이요 ㅎㅎ",
          "speaker_id": "1",
