      "utterance": [
        {
          "id": "MDRW2100031302.1.1",
          "form": "곰 호주야?",
          "original_form": "곰 호주야?",
          "speaker_id": "2",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100031302.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100031302.1.3",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100031302.1.4",
          "form": "공모주요ㅋㅋ",
          "original_form": "공모주요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100031302.1.5",
          "form": "그게 뭔가요",
          "original_form": "그게 뭔가요",
          "speaker_id": "2",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100031302.1.6",
          "form": "아이피 오에 관련된 내용이에요",
          "original_form": "아이피 오에 관련된 내용이에요",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100031302.1.7",
          "form": "저는 처음 듣네요",
          "original_form": "저는 처음 듣네요",
          "speaker_id": "2",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100031302.1.8",
          "form": "저도 잘 몰라서 책으로 보고 있어요",
          "original_form": "저도 잘 몰라서 책으로 보고 있어요",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100031302.1.9",
          "form": "모르는 건데 배워 나 가면 좋은 거죠",
          "original_form": "모르는 건데 배워 나 가면 좋은 거죠",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100031302.1.10",
          "form": "맞아요 그래서 책도 읽고 유튜브 도 보고 있어요",
          "original_form": "맞아요 그래서 책도 읽고 유튜브 도 보고 있어요",
          "speaker_id": "1",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100031302.1.11",
          "form": "훌륭하시네요",
          "original_form": "훌륭하시네요",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100031302.1.12",
          "form": "요새 일곡 계신 책 있으세요",
          "original_form": "요새 일곡 계신 책 있으세요",
          "speaker_id": "1",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100031302.1.13",
          "form": "저는 요리 책을 보고 있어요",
          "original_form": "저는 요리 책을 보고 있어요",
          "speaker_id": "2",
