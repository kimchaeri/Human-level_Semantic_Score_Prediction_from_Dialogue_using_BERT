      "utterance": [
        {
          "id": "MDRW2100025767.1.1",
          "form": "우리학교 앞엔 분식집 말고 토스트집 있었어!",
          "original_form": "우리학교 앞엔 분식집 말고 토스트집 있었어!",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025767.1.2",
          "form": "이삭같은건가?",
          "original_form": "이삭같은건가?",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025767.1.3",
          "form": "응 그랬던거같아ㅋㅋ",
          "original_form": "응 그랬던거같아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025767.1.4",
          "form": "맛있겠다ㅠㅠ 이삭먹고싶네",
          "original_form": "맛있겠다ㅠㅠ 이삭먹고싶네",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025767.1.5",
          "form": "야자 쉬는시간에 몰래 나가서 토스트 사먹고 그랬는데ㅋㅋㅋㅋ",
          "original_form": "야자 쉬는시간에 몰래 나가서 토스트 사먹고 그랬는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025767.1.6",
          "form": "헉 듣기만 해도 맛있다",
          "original_form": "헉 듣기만 해도 맛있다",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025767.1.7",
          "form": "진짜 맛있었어.... 계란 속에서 터지는 옥수수알...",
          "original_form": "진짜 맛있었어.... 계란 속에서 터지는 옥수수알...",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025767.1.8",
          "form": "맞아 난 이삭에서 햄치즈 스폐셜 제일 좋아해 클래식ㅋㅋㅋ",
          "original_form": "맞아 난 이삭에서 햄치즈 스폐셜 제일 좋아해 클래식ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025767.1.9",
          "form": "아 또 먹고싶어ㅠㅠㅠ 근데 이삭 배달시키려면 기본 주문비가 너무 비싸ㅠㅠ",
          "original_form": "아 또 먹고싶어ㅠㅠㅠ 근데 이삭 배달시키려면 기본 주문비가 너무 비싸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025767.1.10",
          "form": "맞아 배달비를 이삭에서 하나도 부담 안해주더라",
          "original_form": "맞아 배달비를 이삭에서 하나도 부담 안해주더라",
          "speaker_id": "1",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025767.1.11",
          "form": "너무 비싸ㅠㅠ 진짜ㅠㅠ",
          "original_form": "너무 비싸ㅠㅠ 진짜ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025767.1.12",
          "form": "그리고 내친구가 추천하던데 이삭 피자토스트도 맛있대",
          "original_form": "그리고 내친구가 추천하던데 이삭 피자토스트도 맛있대",
          "speaker_id": "1",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025767.1.13",
          "form": "오 궁금하다 피자빵 같은 그런건가?",
          "original_form": "오 궁금하다 피자빵 같은 그런건가?",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025767.1.14",
          "form": "아 페퍼로니 피자 먹고싶다",
          "original_form": "아 페퍼로니 피자 먹고싶다",
          "speaker_id": "1",
          "time": "20210901 16:55"
        }
      ]
