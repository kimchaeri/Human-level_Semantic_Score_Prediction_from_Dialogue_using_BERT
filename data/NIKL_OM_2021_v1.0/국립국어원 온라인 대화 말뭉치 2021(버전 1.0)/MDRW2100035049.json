        },
        {
          "id": "MDRW2100035049.1.2",
          "form": "영화 좋아하니",
          "original_form": "영화 좋아하니",
          "speaker_id": "1",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100035049.1.3",
          "form": "좋아하지",
          "original_form": "좋아하지",
          "speaker_id": "2",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100035049.1.4",
          "form": "영화관 가는 것도 좋아하니",
          "original_form": "영화관 가는 것도 좋아하니",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100035049.1.5",
          "form": "최근엔 자주 못 갔어",
          "original_form": "최근엔 자주 못 갔어",
          "speaker_id": "2",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100035049.1.6",
          "form": "이번에 나온 영화 봤니",
          "original_form": "이번에 나온 영화 봤니",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100035049.1.7",
          "form": "모가디슈 말하지?",
          "original_form": "모가디슈 말하지?",
          "speaker_id": "2",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100035049.1.8",
          "form": "그거 재밌다던데",
          "original_form": "그거 재밌다던데",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100035049.1.9",
          "form": "실화 기반이라더라",
          "original_form": "실화 기반이라더라",
          "speaker_id": "2",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100035049.1.10",
          "form": "김윤석 조인성이 주인공이야",
          "original_form": "김윤석 조인성이 주인공이야",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100035049.1.11",
          "form": "연기력이 무척 좋겠네",
          "original_form": "연기력이 무척 좋겠네",
          "speaker_id": "2",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100035049.1.12",
          "form": "감동적인 요소도 있다더라",
          "original_form": "감동적인 요소도 있다더라",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100035049.1.13",
          "form": "극장서 언제 봐야겠다",
          "original_form": "극장서 언제 봐야겠다",
          "speaker_id": "2",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100035049.1.14",
          "form": "꼭 보길 바라",
          "original_form": "꼭 보길 바라",
