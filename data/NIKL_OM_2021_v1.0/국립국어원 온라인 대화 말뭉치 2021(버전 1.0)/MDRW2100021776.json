      "utterance": [
        {
          "id": "MDRW2100021776.1.1",
          "form": "다이소에 롤도 판다고요?!!",
          "original_form": "다이소에 롤도 판다고요?!!",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.2",
          "form": "다이소에 없는게 있나요",
          "original_form": "다이소에 없는게 있나요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.3",
          "form": "아니 진짜 다이소에 다있네요",
          "original_form": "아니 진짜 다이소에 다있네요",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.4",
          "form": "롤 한개에 천원이에요? 비싼데 흠",
          "original_form": "롤 한개에 천원이에요? 비싼데 흠",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.5",
          "form": "헤어롤이 뭐 되게 고급진 물건도 아니고",
          "original_form": "헤어롤이 뭐 되게 고급진 물건도 아니고",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.7",
          "form": "그 헤어롤 말고 그것도 있어요?",
          "original_form": "그 헤어롤 말고 그것도 있어요?",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.8",
          "form": "아 저 다이소말고 올리브영꺼 구입해서",
          "original_form": "아 저 다이소말고 올리브영꺼 구입해서",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.9",
          "form": "비싼거 말한거에요",
          "original_form": "비싼거 말한거에요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.10",
          "form": "그 삐쭉삐쭉 이상한 도깨비방망이 같이 생긴 빗이요",
          "original_form": "그 삐쭉삐쭉 이상한 도깨비방망이 같이 생긴 빗이요",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021776.1.11",
          "form": "그거 이름이 뭐에요?",
          "original_form": "그거 이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.12",
          "form": "아 탱글티저 같은거 말씀하시는건가",
          "original_form": "아 탱글티저 같은거 말씀하시는건가",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.13",
          "form": "탱글티저가 뭐에요?",
          "original_form": "탱글티저가 뭐에요?",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.14",
          "form": "그냥 아줌마들 그 빗에 머리 말아서 헤어드라이기 슈우우웅",
          "original_form": "그냥 아줌마들 그 빗에 머리 말아서 헤어드라이기 슈우우웅",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.15",
          "form": "쓰는거요",
          "original_form": "쓰는거요",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.16",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.17",
          "form": "이름이 웬지 롤빗 이런거 같은데",
          "original_form": "이름이 웬지 롤빗 이런거 같은데",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.18",
          "form": "그럴거 갘네요",
          "original_form": "그럴거 갘네요",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021776.1.20",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.21",
          "form": "이름을 생각해본적이 없는",
          "original_form": "이름을 생각해본적이 없는",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.22",
          "form": "when I go to hotel",
          "original_form": "when I go to hotel",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.23",
          "form": "봤어요 그거",
          "original_form": "봤어요 그거",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.25",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.26",
          "form": "아 그래서 제가 하고 싶은 말ㅇ느",
          "original_form": "아 그래서 제가 하고 싶은 말ㅇ느",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.27",
          "form": "다이소에 그런 빗도 팔아요?",
          "original_form": "다이소에 그런 빗도 팔아요?",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021776.1.28",
          "form": "있지않을까요?",
          "original_form": "있지않을까요?",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021776.1.29",
          "form": "심지어 고데기 기능 결합된 롤빗 판다는",
          "original_form": "심지어 고데기 기능 결합된 롤빗 판다는",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021776.1.30",
          "form": "기사가 있네여",
          "original_form": "기사가 있네여",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021776.1.31",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021776.1.32",
          "form": "옛날거라 지금도 파는진 몰라도",
          "original_form": "옛날거라 지금도 파는진 몰라도",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021776.1.33",
          "form": "어! 저 그거 아까 오후에 여기서 얘기 들었어요",
          "original_form": "어! 저 그거 아까 오후에 여기서 얘기 들었어요",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021776.1.34",
          "form": "고등학교 다ㅣ니는 분이 자랑하더라구요 ㅋㅋㅋ",
          "original_form": "고등학교 다ㅣ니는 분이 자랑하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021776.1.35",
          "form": "그게 님이었나..",
          "original_form": "그게 님이었나..",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021776.1.36",
          "form": "앜ㅋㅋㅋㅋ아니여",
          "original_form": "앜ㅋㅋㅋㅋ아니여",
          "speaker_id": "2",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021776.1.37",
          "form": "저도 저 제품의 존재를 몰라서 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 저 제품의 존재를 몰라서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021776.1.38",
          "form": "그 분이 엄청 발랄하게 자랑해대서",
          "original_form": "그 분이 엄청 발랄하게 자랑해대서",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021776.1.39",
          "form": "저도 사고싶어졌어요 그거",
          "original_form": "저도 사고싶어졌어요 그거",
          "speaker_id": "1",
          "time": "20210828 22:23"
        }
      ]
    }
