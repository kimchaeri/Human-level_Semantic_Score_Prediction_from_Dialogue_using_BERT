      "utterance": [
        {
          "id": "MDRW2100047483.1.1",
          "form": "저녁은 가족과 드시나요",
          "original_form": "저녁은 가족과 드시나요",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100047483.1.2",
          "form": "네^^",
          "original_form": "네^^",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100047483.1.3",
          "form": "전 이제야 저녁먹어요",
          "original_form": "전 이제야 저녁먹어요",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047483.1.4",
          "form": "저녁드셨나요",
          "original_form": "저녁드셨나요",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047483.1.5",
          "form": "네~  돈가스 먹었어요 수제로다가",
          "original_form": "네~  돈가스 먹었어요 수제로다가",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047483.1.6",
          "form": "맛있는거드셨네요",
          "original_form": "맛있는거드셨네요",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047483.1.7",
          "form": "네~ 제가 한장한장ㅋㅋ",
          "original_form": "네~ 제가 한장한장ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047483.1.8",
          "form": "와우~~",
          "original_form": "와우~~",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047483.1.9",
          "form": "대단하시당ㅜㅜㅜㅜ",
          "original_form": "대단하시당ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047483.1.10",
          "form": "어케만드셨나요",
          "original_form": "어케만드셨나요",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047483.1.11",
          "form": "정육점가서 고기 사구 밀가루 계란물 빵가루 묻혀서용",
          "original_form": "정육점가서 고기 사구 밀가루 계란물 빵가루 묻혀서용",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047483.1.12",
          "form": "와우~~~",
          "original_form": "와우~~~",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047483.1.13",
          "form": "솜씨가 대단하시네요 냉동도아니고",
          "original_form": "솜씨가 대단하시네요 냉동도아니고",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100047483.1.14",
          "form": "애기가 두꺼운걸 못먹어서 하다보니ㅋㅋ",
          "original_form": "애기가 두꺼운걸 못먹어서 하다보니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047483.1.15",
          "form": "엄청 맛났겠어요",
          "original_form": "엄청 맛났겠어요",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047483.1.16",
          "form": "가위로잘라줬겠네요",
          "original_form": "가위로잘라줬겠네요",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047483.1.17",
          "form": "잘게잘게~ 3살이라",
          "original_form": "잘게잘게~ 3살이라",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047483.1.18",
          "form": "ㅎㅎ 아유 맛있게 잘먹었겠네요 소스랑",
          "original_form": "ㅎㅎ 아유 맛있게 잘먹었겠네요 소스랑",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100047483.1.19",
          "form": "남자애들은 고기없이 잘안먹더라구요",
          "original_form": "남자애들은 고기없이 잘안먹더라구요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
