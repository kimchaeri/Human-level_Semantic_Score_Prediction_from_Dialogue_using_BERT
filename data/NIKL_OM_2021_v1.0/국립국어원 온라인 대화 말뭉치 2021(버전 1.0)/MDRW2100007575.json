        },
        {
          "id": "MDRW2100007575.1.2",
          "form": "귀찮아서 잘 안먹어ㅋㅋㅋㅋ 시켜먹거나..",
          "original_form": "귀찮아서 잘 안먹어ㅋㅋㅋㅋ 시켜먹거나..",
          "speaker_id": "2",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007575.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.4",
          "form": "뭐좋아해 먹는거?!",
          "original_form": "뭐좋아해 먹는거?!",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.5",
          "form": "난 다 좋아",
          "original_form": "난 다 좋아",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.6",
          "form": "빵이면 더 좋아",
          "original_form": "빵이면 더 좋아",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.7",
          "form": "나도ㅋㅋㅋㅋ 안가려",
          "original_form": "나도ㅋㅋㅋㅋ 안가려",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.8",
          "form": "팥앙금? 미쳐",
          "original_form": "팥앙금? 미쳐",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.9",
          "form": "헉 빵 무슨종류",
          "original_form": "헉 빵 무슨종류",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.10",
          "form": "앙버터ㅠ",
          "original_form": "앙버터ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.11",
          "form": "공갈이나 깨찰빵도.. 타르트도...",
          "original_form": "공갈이나 깨찰빵도.. 타르트도...",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.12",
          "form": "앙버터 너무 좋아ㅠㅠㅠ",
          "original_form": "앙버터 너무 좋아ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.13",
          "form": "겨울 붕어빵 그립다",
          "original_form": "겨울 붕어빵 그립다",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007575.1.14",
          "form": "아악 공갈빵 진짜좋아해",
          "original_form": "아악 공갈빵 진짜좋아해",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007575.1.15",
          "form": "요즘 잘 안팔자나ㅠㅠ",
          "original_form": "요즘 잘 안팔자나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007575.1.16",
          "form": "그니까잉",
          "original_form": "그니까잉",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007575.1.17",
          "form": "붕어빵ㅠㅠ 호떡도 먹구싶다",
          "original_form": "붕어빵ㅠㅠ 호떡도 먹구싶다",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007575.1.18",
          "form": "다들 마노이 사먹으란 말여",
          "original_form": "다들 마노이 사먹으란 말여",
          "speaker_id": "1",
