      "utterance": [
        {
          "id": "MDRW2100016136.1.1",
          "form": "하 게임 할말있어요",
          "original_form": "하 게임 할말있어요",
          "speaker_id": "2",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016136.1.2",
          "form": "농장 얘긴가요",
          "original_form": "농장 얘긴가요",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016136.1.3",
          "form": "레알팜 농작물 사러간 후 어케됐나요",
          "original_form": "레알팜 농작물 사러간 후 어케됐나요",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016136.1.4",
          "form": "그건잘탔는데",
          "original_form": "그건잘탔는데",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.5",
          "form": "요즘 미션때문에 하는 게임이있거든요",
          "original_form": "요즘 미션때문에 하는 게임이있거든요",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.6",
          "form": "마지막단계가 진짜 너무 고난이도라",
          "original_form": "마지막단계가 진짜 너무 고난이도라",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.7",
          "form": "오 게임을 시작했군요",
          "original_form": "오 게임을 시작했군요",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.8",
          "form": "안깨져서돌겠어요",
          "original_form": "안깨져서돌겠어요",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.9",
          "form": "승부욕의 신인데 또",
          "original_form": "승부욕의 신인데 또",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.10",
          "form": "미션때문이예요 완성하면 다신안볼",
          "original_form": "미션때문이예요 완성하면 다신안볼",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016136.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016136.1.13",
          "form": "퀴즈인가요",
          "original_form": "퀴즈인가요",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016136.1.14",
          "form": "아니요 무슨 왜 그 3개씩블록맞추고 터지는 그런거있잖아요",
          "original_form": "아니요 무슨 왜 그 3개씩블록맞추고 터지는 그런거있잖아요",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016136.1.15",
          "form": "그런건데",
          "original_form": "그런건데",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016136.1.16",
          "form": "그냥 50단계까지가라 이럼쉽거든요..?",
          "original_form": "그냥 50단계까지가라 이럼쉽거든요..?",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016136.1.17",
          "form": "근데 특별히 미션으로 따로 해둔게 있어요  그거깨라고.. .",
          "original_form": "근데 특별히 미션으로 따로 해둔게 있어요  그거깨라고.. .",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016136.1.18",
          "form": "근데 달라요?",
          "original_form": "근데 달라요?",
          "speaker_id": "1",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016136.1.19",
          "form": "뭐여 안보내져",
          "original_form": "뭐여 안보내져",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016136.1.20",
          "form": "나는 동물의 숲을 할로윈까지만 플레이하고 그 뒤로 방치했네요",
          "original_form": "나는 동물의 숲을 할로윈까지만 플레이하고 그 뒤로 방치했네요",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016136.1.21",
          "form": "ㅋㅋㅋㅋ동물의숲간만이에용",
          "original_form": "ㅋㅋㅋㅋ동물의숲간만이에용",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016136.1.22",
          "form": "우리는 왜 게임을 하며 고통받을까요",
          "original_form": "우리는 왜 게임을 하며 고통받을까요",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016136.1.23",
          "form": "맨날 게임안에서 농사짓고 청소하고",
          "original_form": "맨날 게임안에서 농사짓고 청소하고",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016136.1.24",
          "form": "겜재밌는거없지여",
          "original_form": "겜재밌는거없지여",
