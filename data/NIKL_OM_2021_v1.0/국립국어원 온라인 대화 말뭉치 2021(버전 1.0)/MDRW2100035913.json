      "utterance": [
        {
          "id": "MDRW2100035913.1.1",
          "form": "그래 학교 가자마자 숙제가 있네",
          "original_form": "그래 학교 가자마자 숙제가 있네",
          "speaker_id": "2",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100035913.1.2",
          "form": "영어 다닐거가",
          "original_form": "영어 다닐거가",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.3",
          "form": "영어선생님 지속적으로 관리를 해줘서 좋다",
          "original_form": "영어선생님 지속적으로 관리를 해줘서 좋다",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.4",
          "form": "이학년부턴 다녀야 안 되겠어요??",
          "original_form": "이학년부턴 다녀야 안 되겠어요??",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.5",
          "form": "근데 진짜 안할라함ㅠ",
          "original_form": "근데 진짜 안할라함ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.6",
          "form": "영어를 왜 이래 시로 하는지",
          "original_form": "영어를 왜 이래 시로 하는지",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.7",
          "form": "그래 혼자 하니 더 안하지",
          "original_form": "그래 혼자 하니 더 안하지",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.8",
          "form": "친구들이랑 해도 안 한대요ㅠ",
          "original_form": "친구들이랑 해도 안 한대요ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.9",
          "form": "친구들이랑 같이 수업듣고 하면 좀 나을수도 있음",
          "original_form": "친구들이랑 같이 수업듣고 하면 좀 나을수도 있음",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.10",
          "form": "선생님이 재밌게\r\n잘 가르쳐준다",
          "original_form": "선생님이 재밌게\r\n잘 가르쳐준다",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.11",
          "form": "동기유발",
          "original_form": "동기유발",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.12",
          "form": "웁스",
          "original_form": "웁스",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.13",
          "form": "name3이 도 같이 가자고 얘기 했는데",
          "original_form": "&name3&이 도 같이 가자고 얘기 했는데",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035913.1.14",
          "form": "그조차 싫다 해서 그냥 보낼라고요",
          "original_form": "그조차 싫다 해서 그냥 보낼라고요",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035913.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035913.1.16",
          "form": "해줄 수 있는 얘기는 다 해줌ㅋㅋㅋ",
          "original_form": "해줄 수 있는 얘기는 다 해줌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035913.1.17",
          "form": "그래 일단 학원으로 밀어 넣으면",
          "original_form": "그래 일단 학원으로 밀어 넣으면",
          "speaker_id": "2",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035913.1.18",
          "form": "어떻게든 되겠지",
          "original_form": "어떻게든 되겠지",
          "speaker_id": "2",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035913.1.19",
          "form": "집도 가까워서 딱 좋을 거 같은데",
          "original_form": "집도 가까워서 딱 좋을 거 같은데",
          "speaker_id": "2",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035913.1.20",
          "form": "읽을 수는 있나??",
          "original_form": "읽을 수는 있나??",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035913.1.21",
          "form": "그러니까요  영어를요??",
          "original_form": "그러니까요  영어를요??",
          "speaker_id": "1",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035913.1.22",
          "form": "영어는 누가 얘기하는 거 그냥 따라 하는 건 줄알아여",
          "original_form": "영어는 누가 얘기하는 거 그냥 따라 하는 건 줄알아여",
          "speaker_id": "1",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035913.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035913.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 따라하는거 ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 따라하는거 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100035913.1.25",
          "form": "1학년 때 같이 시작하지",
          "original_form": "1학년 때 같이 시작하지",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100035913.1.26",
          "form": "으이구",
          "original_form": "으이구",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100035913.1.27",
          "form": "처음엔 도 요새를 엄청 잘했어요",
          "original_form": "처음엔 도 요새를 엄청 잘했어요",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100035913.1.28",
          "form": "그래서 아 괜찮겠다 했는데 아니었어요",
          "original_form": "그래서 아 괜찮겠다 했는데 아니었어요",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100035913.1.29",
          "form": "그래 컴퓨터를 보고 하는 거는",
          "original_form": "그래 컴퓨터를 보고 하는 거는",
          "speaker_id": "2",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035913.1.30",
          "form": "의지가 강해야 된다",
          "original_form": "의지가 강해야 된다",
          "speaker_id": "2",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035913.1.31",
          "form": "우리 name4은 시작도 안 했을 듯",
          "original_form": "우리 &name4&은 시작도 안 했을 듯",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100035913.1.32",
          "form": "맞아요 점점 안 하게 돼요",
          "original_form": "맞아요 점점 안 하게 돼요",
          "speaker_id": "1",
          "time": "20210830 12:16"
        },
        {
          "id": "MDRW2100035913.1.33",
          "form": "이것도 돈 아깝네요ㅋㅋㅋ",
          "original_form": "이것도 돈 아깝네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:16"
        },
        {
          "id": "MDRW2100035913.1.34",
          "form": "도요새는 언제까진데??",
          "original_form": "도요새는 언제까진데??",
          "speaker_id": "2",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100035913.1.35",
          "form": "학원가서 열심히 굴려야됨",
          "original_form": "학원가서 열심히 굴려야됨",
