      "utterance": [
        {
          "id": "MDRW2100025440.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025440.1.2",
          "form": "아니요 name1님은 키우시나요?",
          "original_form": "아니요 &name1&님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025440.1.3",
          "form": "사실 저도 안키워요 ㅎ",
          "original_form": "사실 저도 안키워요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025440.1.4",
          "form": "키우고 싶은데 여건이 안되서 못키우고있어요",
          "original_form": "키우고 싶은데 여건이 안되서 못키우고있어요",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025440.1.5",
          "form": "그럼 키우고싶은 반려동물이 있으신가요?",
          "original_form": "그럼 키우고싶은 반려동물이 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100025440.1.6",
          "form": "네네 고양이도 키우고 싶고 개도 키우고 싶어요!",
          "original_form": "네네 고양이도 키우고 싶고 개도 키우고 싶어요!",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025440.1.7",
          "form": "근데 돈이 많이들어서 포기했어요ㅜ",
          "original_form": "근데 돈이 많이들어서 포기했어요ㅜ",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025440.1.8",
          "form": "유기견도 막 찾아보는데 안쓰럽고",
          "original_form": "유기견도 막 찾아보는데 안쓰럽고",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025440.1.9",
          "form": "맞아요 혹시 알러지나 질병있으면 또 마음아프니끼요ㅠㅠ",
          "original_form": "맞아요 혹시 알러지나 질병있으면 또 마음아프니끼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100025440.1.10",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025440.1.11",
          "form": "그래서 임보 하고 있는 아이 입양해도 괜찮다곤 하는데",
          "original_form": "그래서 임보 하고 있는 아이 입양해도 괜찮다곤 하는데",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025440.1.12",
          "form": "막상 그 책임감이 무겁더라고요",
          "original_form": "막상 그 책임감이 무겁더라고요",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025440.1.13",
          "form": "맞아요 그리고 간식이나 용품비용도 어마어마하니까요",
          "original_form": "맞아요 그리고 간식이나 용품비용도 어마어마하니까요",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025440.1.14",
          "form": "진짜 맞아요! 또 아프면 병원비가 장난이 아니라더라고요",
          "original_form": "진짜 맞아요! 또 아프면 병원비가 장난이 아니라더라고요",
          "speaker_id": "1",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100025440.1.15",
          "form": "사람들이 그거 안알아보고 무조건 귀엽다고 키워서 버리고ㅠ 나쁜",
          "original_form": "사람들이 그거 안알아보고 무조건 귀엽다고 키워서 버리고ㅠ 나쁜",
          "speaker_id": "1",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100025440.1.16",
          "form": "그니까요 입양을 해왔으면 비용도 다 생각해야지 단순히 귀엽다고 데려오는게 말이나 되나요?",
          "original_form": "그니까요 입양을 해왔으면 비용도 다 생각해야지 단순히 귀엽다고 데려오는게 말이나 되나요?",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025440.1.17",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025440.1.18",
          "form": "진짜 책임감 없는 인간들",
          "original_form": "진짜 책임감 없는 인간들",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025440.1.19",
          "form": "동물들이 무슨죄라고",
          "original_form": "동물들이 무슨죄라고",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025440.1.20",
          "form": "그러니까요 동물들은 이유도 없이 버려지는데 말이에요",
          "original_form": "그러니까요 동물들은 이유도 없이 버려지는데 말이에요",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025440.1.21",
          "form": "맞아요. 아무튼 반려동물 법도 강화되면 좋겠어요",
          "original_form": "맞아요. 아무튼 반려동물 법도 강화되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025440.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 13:15"
        }
      ]
