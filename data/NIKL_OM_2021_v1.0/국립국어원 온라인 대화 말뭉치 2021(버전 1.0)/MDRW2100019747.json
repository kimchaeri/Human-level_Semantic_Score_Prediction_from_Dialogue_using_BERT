      "utterance": [
        {
          "id": "MDRW2100019747.1.1",
          "form": "선물 많이 받으시나요?",
          "original_form": "선물 많이 받으시나요?",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019747.1.2",
          "form": "선물받을 일이 없어요 ㅜ",
          "original_form": "선물받을 일이 없어요 ㅜ",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019747.1.3",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019747.1.4",
          "form": "내가 나에게 선물을",
          "original_form": "내가 나에게 선물을",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019747.1.5",
          "form": "아 슬프네요  최근 뭐 선물하셨어요",
          "original_form": "아 슬프네요  최근 뭐 선물하셨어요",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019747.1.6",
          "form": "옷이랑 먹을꺼요ㅎ",
          "original_form": "옷이랑 먹을꺼요ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019747.1.7",
          "form": "아 ㅎㅎ 성공적이었나요 ㅎㅎ",
          "original_form": "아 ㅎㅎ 성공적이었나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100019747.1.8",
          "form": "살때만 좋았네요ㅋ",
          "original_form": "살때만 좋았네요ㅋ",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100019747.1.9",
          "form": "ㅎㅎㅎ 후회없는 기분좋은 소비를 해야하는데 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 후회없는 기분좋은 소비를 해야하는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100019747.1.10",
          "form": "맨날 후회하고 사고 반복",
          "original_form": "맨날 후회하고 사고 반복",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100019747.1.11",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100019747.1.12",
          "form": "그니까요 ㅎㅎㅎ 쇼핑은 쉬우면서 어려운 ㅋㅋ ㅜㅜ",
          "original_form": "그니까요 ㅎㅎㅎ 쇼핑은 쉬우면서 어려운 ㅋㅋ ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100019747.1.13",
          "form": "근데 기분은 좋고",
          "original_form": "근데 기분은 좋고",
          "speaker_id": "1",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100019747.1.14",
          "form": "집에는 쌓여가고",
          "original_form": "집에는 쌓여가고",
          "speaker_id": "1",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100019747.1.15",
          "form": "맞아요 ㅜ 냉장고그 비어서 오늘도 해야 ㅡㅡㅜㅜ",
          "original_form": "맞아요 ㅜ 냉장고그 비어서 오늘도 해야 ㅡㅡㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 15:50"
        },
        {
          "id": "MDRW2100019747.1.16",
          "form": "정말 무한반복",
          "original_form": "정말 무한반복",
