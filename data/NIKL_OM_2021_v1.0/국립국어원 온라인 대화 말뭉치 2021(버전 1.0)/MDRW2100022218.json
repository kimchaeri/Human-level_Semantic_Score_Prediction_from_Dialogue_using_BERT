      "utterance": [
        {
          "id": "MDRW2100022218.1.1",
          "form": "오 어떤 방법으로요?",
          "original_form": "오 어떤 방법으로요?",
          "speaker_id": "2",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022218.1.2",
          "form": "일단 식이조절중이고..",
          "original_form": "일단 식이조절중이고..",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022218.1.3",
          "form": "다이어트는 식이가 제일 중요한 거 같아요",
          "original_form": "다이어트는 식이가 제일 중요한 거 같아요",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.4",
          "form": "네네 탄수화물 제한하는 저탄고지 중이에요",
          "original_form": "네네 탄수화물 제한하는 저탄고지 중이에요",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.5",
          "form": "효과가 좀 있나요?",
          "original_form": "효과가 좀 있나요?",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.6",
          "form": "저녁때만 탄수화물 섭취를 안하는중인데",
          "original_form": "저녁때만 탄수화물 섭취를 안하는중인데",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.7",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.8",
          "form": "생각보다 체중감량 효과가 꽤 커요",
          "original_form": "생각보다 체중감량 효과가 꽤 커요",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.9",
          "form": "운동은 안하구요?",
          "original_form": "운동은 안하구요?",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.10",
          "form": "운동은 저녁때 가볍게 산책 한시간정도요!",
          "original_form": "운동은 저녁때 가볍게 산책 한시간정도요!",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022218.1.11",
          "form": "얼마나 빼셨어요?",
          "original_form": "얼마나 빼셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022218.1.12",
          "form": "한달에 3키로정도 빠지는 거 같아요",
          "original_form": "한달에 3키로정도 빠지는 거 같아요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022218.1.13",
          "form": "오 괜찮네요?",
          "original_form": "오 괜찮네요?",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022218.1.14",
          "form": "네 너무 부담가지도 않고 좋더라구요",
          "original_form": "네 너무 부담가지도 않고 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        }
