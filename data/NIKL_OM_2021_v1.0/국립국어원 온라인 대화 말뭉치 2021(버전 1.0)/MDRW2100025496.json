      "utterance": [
        {
          "id": "MDRW2100025496.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100025496.1.2",
          "form": "아뇨 지금은 그쳤어요",
          "original_form": "아뇨 지금은 그쳤어요",
          "speaker_id": "2",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025496.1.3",
          "form": "하지만 오후에 또 올 거처럼 날이 엄청 흐려요",
          "original_form": "하지만 오후에 또 올 거처럼 날이 엄청 흐려요",
          "speaker_id": "2",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025496.1.4",
          "form": "구름이 많네요",
          "original_form": "구름이 많네요",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025496.1.5",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025496.1.6",
          "form": "장마가 얼른 끝났으면 좋겠어요",
          "original_form": "장마가 얼른 끝났으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025496.1.7",
          "form": "근데 끝나면 바로 추워질까봐 두렵기도 하네요ㅋㅋㅋ",
          "original_form": "근데 끝나면 바로 추워질까봐 두렵기도 하네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025496.1.8",
          "form": "바로 겨율 ㅋㅋ",
          "original_form": "바로 겨율 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025496.1.9",
          "form": "좋아하는 계절이 있나요?",
          "original_form": "좋아하는 계절이 있나요?",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025496.1.10",
          "form": "봄이요",
          "original_form": "봄이요",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025496.1.11",
          "form": "봄 좋죠",
          "original_form": "봄 좋죠",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025496.1.12",
          "form": "날도 천천히 풀리고",
          "original_form": "날도 천천히 풀리고",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025496.1.13",
          "form": "두꺼운 패딩 벗을 날을 손 꼽아서 기다리게 되잖아요ㅋㅋㅋ",
          "original_form": "두꺼운 패딩 벗을 날을 손 꼽아서 기다리게 되잖아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025496.1.14",
          "form": "네 꽃도",
          "original_form": "네 꽃도",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025496.1.15",
          "form": "꽃도 좋죠^^",
          "original_form": "꽃도 좋죠^^",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100025496.1.16",
          "form": "아침에 나설 때 길가에 민들레 피면 기분이 좋아요",
          "original_form": "아침에 나설 때 길가에 민들레 피면 기분이 좋아요",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100025496.1.17",
          "form": "노랑노랑 풍경 좋잖아요",
          "original_form": "노랑노랑 풍경 좋잖아요",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100025496.1.18",
          "form": "이쁘죠",
          "original_form": "이쁘죠",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100025496.1.19",
          "form": "싫어하는 계절도 있나요?",
          "original_form": "싫어하는 계절도 있나요?",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100025496.1.20",
          "form": "저는 여름이요ㅋㅋㅋ 좋아하지만 싫어하는 계절이에요",
          "original_form": "저는 여름이요ㅋㅋㅋ 좋아하지만 싫어하는 계절이에요",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100025496.1.21",
          "form": "저도 여름",
          "original_form": "저도 여름",
          "speaker_id": "1",
