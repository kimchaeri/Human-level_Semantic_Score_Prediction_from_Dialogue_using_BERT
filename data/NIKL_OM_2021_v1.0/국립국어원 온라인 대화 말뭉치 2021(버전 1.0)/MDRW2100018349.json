      "utterance": [
        {
          "id": "MDRW2100018349.1.1",
          "form": "봉사 많이 하시나요?",
          "original_form": "봉사 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100018349.1.2",
          "form": "자주는 아니고 가끔 했어요 ㅋㅋㅋㅋ",
          "original_form": "자주는 아니고 가끔 했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018349.1.3",
          "form": "코로나 이후론 한적이 없네요 ㅠㅠ",
          "original_form": "코로나 이후론 한적이 없네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018349.1.4",
          "form": "봉사 많이 하세요?",
          "original_form": "봉사 많이 하세요?",
          "speaker_id": "2",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018349.1.5",
          "form": "저는 학생때 단체 말고는...",
          "original_form": "저는 학생때 단체 말고는...",
          "speaker_id": "1",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100018349.1.6",
          "form": "선뜻 발이...",
          "original_form": "선뜻 발이...",
          "speaker_id": "1",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100018349.1.7",
          "form": "어디 단체에서 하시는거에요?",
          "original_form": "어디 단체에서 하시는거에요?",
          "speaker_id": "1",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100018349.1.8",
          "form": "아니요 개인으로 신청해서 하곤 햇어요",
          "original_form": "아니요 개인으로 신청해서 하곤 햇어요",
          "speaker_id": "2",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018349.1.9",
          "form": "저도 학생때는 단체로 ㅋㅋㅋㅋ",
          "original_form": "저도 학생때는 단체로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018349.1.10",
          "form": "학생때 무슨 봉사하셧어요?",
          "original_form": "학생때 무슨 봉사하셧어요?",
          "speaker_id": "2",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018349.1.11",
          "form": "농활같은거 있죠...",
          "original_form": "농활같은거 있죠...",
          "speaker_id": "1",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100018349.1.12",
          "form": "쓰러진벼 세우러 가는거",
          "original_form": "쓰러진벼 세우러 가는거",
          "speaker_id": "1",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100018349.1.13",
          "form": "아 농활!!",
          "original_form": "아 농활!!",
          "speaker_id": "2",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100018349.1.14",
          "form": "전 농활은 한번도 안가봣는데 이제와선 약간 후회되네요",
          "original_form": "전 농활은 한번도 안가봣는데 이제와선 약간 후회되네요",
          "speaker_id": "2",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100018349.1.15",
          "form": "그래도 지금 다니시잖아요..",
          "original_form": "그래도 지금 다니시잖아요..",
          "speaker_id": "1",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100018349.1.16",
          "form": "코로나 터지고 저도 봉사간적 없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "코로나 터지고 저도 봉사간적 없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018349.1.17",
          "form": "농활은 학교 친구들이랑 추억이 됏을 것 같아서 ㅋㅋㅋㅋㅋ",
          "original_form": "농활은 학교 친구들이랑 추억이 됏을 것 같아서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018349.1.18",
          "form": "재미는 있죠... 젊기도하니깐 힘도 덜들고.. ㅋㅋ",
          "original_form": "재미는 있죠... 젊기도하니깐 힘도 덜들고.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100018349.1.19",
          "form": "전 반대로 어른되서 한번도 못해봐서..",
          "original_form": "전 반대로 어른되서 한번도 못해봐서..",
          "speaker_id": "1",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100018349.1.20",
          "form": "시간 여유 있으실 때 신청해보세요",
          "original_form": "시간 여유 있으실 때 신청해보세요",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018349.1.21",
          "form": "인터넷으로 신청 가능해요",
          "original_form": "인터넷으로 신청 가능해요",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018349.1.22",
          "form": "코로나 끝남 해봐야겟네요...",
          "original_form": "코로나 끝남 해봐야겟네요...",
          "speaker_id": "1",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018349.1.23",
          "form": "네 추천해요",
          "original_form": "네 추천해요",
          "speaker_id": "2",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018349.1.24",
          "form": "주로 어디 가보셧어요?",
          "original_form": "주로 어디 가보셧어요?",
          "speaker_id": "1",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100018349.1.25",
          "form": "연탄나르는 봉사랑",
          "original_form": "연탄나르는 봉사랑",
          "speaker_id": "2",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100018349.1.26",
          "form": "복지시설이랑",
          "original_form": "복지시설이랑",
          "speaker_id": "2",
          "time": "20210827 00:24"
        },
        {
          "id": "MDRW2100018349.1.27",
          "form": "예전엔 방과후학교도 ㅎㅎ",
          "original_form": "예전엔 방과후학교도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:24"
        },
        {
          "id": "MDRW2100018349.1.28",
          "form": "봉사활동이요?",
          "original_form": "봉사활동이요?",
