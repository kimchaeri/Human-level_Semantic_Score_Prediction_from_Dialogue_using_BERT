      "utterance": [
        {
          "id": "MDRW2100022419.1.1",
          "form": "ㅎㅎ 가야지",
          "original_form": "ㅎㅎ 가야지",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022419.1.2",
          "form": "알만 바꾸는 거라 그냥 맡기기만 하면 되는데 얼른 다녀와",
          "original_form": "알만 바꾸는 거라 그냥 맡기기만 하면 되는데 얼른 다녀와",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022419.1.3",
          "form": "혼자가기 싫은데 ㅠㅠ",
          "original_form": "혼자가기 싫은데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022419.1.4",
          "form": "맡기기만 하면 금방인데 혼자 가기가 싫어? ㅋㅋㅋ",
          "original_form": "맡기기만 하면 금방인데 혼자 가기가 싫어? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022419.1.5",
          "form": "ㅇㅇ 나중에 같이 가 ㅎㅎ",
          "original_form": "ㅇㅇ 나중에 같이 가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022419.1.6",
          "form": "훌 ㅋ 알 빨리 바꿔야 일할 때 쓰지",
          "original_form": "훌 ㅋ 알 빨리 바꿔야 일할 때 쓰지",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022419.1.7",
          "form": "지금 막 쓰는 안경 있으니 괜찮아",
          "original_form": "지금 막 쓰는 안경 있으니 괜찮아",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022419.1.8",
          "form": "그거 칠 다 망가져서ㅠ",
          "original_form": "그거 칠 다 망가져서ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022419.1.9",
          "form": "좋은 안경테 있는데도 쓰질 못하네",
          "original_form": "좋은 안경테 있는데도 쓰질 못하네",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022419.1.10",
          "form": "ㅎㅎ 아무 때나 가면 돼",
          "original_form": "ㅎㅎ 아무 때나 가면 돼",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022419.1.11",
          "form": "기왕할 거 빨리 하지 ㅎㅎ",
          "original_form": "기왕할 거 빨리 하지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022419.1.12",
          "form": "커피 마시고 같이 가자",
          "original_form": "커피 마시고 같이 가자",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022419.1.13",
          "form": "ㅋㅋㅋ 알았응",
          "original_form": "ㅋㅋㅋ 알았응",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022419.1.14",
          "form": "고마워~ 혼자가기 너무 싫어",
          "original_form": "고마워~ 혼자가기 너무 싫어",
