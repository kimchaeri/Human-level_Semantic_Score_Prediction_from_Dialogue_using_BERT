      "utterance": [
        {
          "id": "MDRW2100036715.1.1",
          "form": "모델 홍현희만 봤는데 간판을 화려하게 걸었네요",
          "original_form": "모델 홍현희만 봤는데 간판을 화려하게 걸었네요",
          "speaker_id": "1",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100036715.1.2",
          "form": "일단 제가 아침에 먹기전에 몸무게 쟀어요",
          "original_form": "일단 제가 아침에 먹기전에 몸무게 쟀어요",
          "speaker_id": "2",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100036715.1.3",
          "form": "모레까지 얼마나 빠지려나 ㅋㅋ",
          "original_form": "모레까지 얼마나 빠지려나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100036715.1.4",
          "form": "ㄷㄱㄷㄱ 그래도 효과 좋았음 좋겠어요",
          "original_form": "ㄷㄱㄷㄱ 그래도 효과 좋았음 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 10:20"
        },
        {
          "id": "MDRW2100036715.1.5",
          "form": "독소배출이 말대로 제대로 됐음",
          "original_form": "독소배출이 말대로 제대로 됐음",
          "speaker_id": "1",
          "time": "20210831 10:20"
        },
        {
          "id": "MDRW2100036715.1.6",
          "form": "그러게요 빠지고 또 바로 물마시면 찌는 그런게 아니길 ㅋㅋ",
          "original_form": "그러게요 빠지고 또 바로 물마시면 찌는 그런게 아니길 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100036715.1.7",
          "form": "그거 끝나고 나서 식단도 고민이겠네요",
          "original_form": "그거 끝나고 나서 식단도 고민이겠네요",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036715.1.8",
          "form": "음 일단 저녁은 다시 그 민초 쉐이크로 복귀요 ㅋㅋ",
          "original_form": "음 일단 저녁은 다시 그 민초 쉐이크로 복귀요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100036715.1.9",
          "form": "아...영고의 굴레네요",
          "original_form": "아...영고의 굴레네요",
          "speaker_id": "1",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100036715.1.10",
          "form": "거기는 지옥인가요",
          "original_form": "거기는 지옥인가요",
          "speaker_id": "1",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100036715.1.11",
          "form": "일단 추석전까지만ㅜㅜ",
          "original_form": "일단 추석전까지만ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100036715.1.12",
          "form": "하 최근 살이 많이찐 제탓이죠ㅜㅜㅜ",
          "original_form": "하 최근 살이 많이찐 제탓이죠ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100036715.1.13",
          "form": "ㅜㅜ 얼른 다요트 성공하시길!",
          "original_form": "ㅜㅜ 얼른 다요트 성공하시길!",
          "speaker_id": "1",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100036715.1.14",
          "form": "치팅데이도 없는건가요",
          "original_form": "치팅데이도 없는건가요",
          "speaker_id": "1",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100036715.1.15",
          "form": "일욜까지 정말 정말 잘먹었죠ㅜㅜ",
          "original_form": "일욜까지 정말 정말 잘먹었죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036715.1.16",
          "form": "저의일욜을 ㅋㅋ 회상하며 참고있어요",
          "original_form": "저의일욜을 ㅋㅋ 회상하며 참고있어요",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036715.1.17",
          "form": "그날 메뉴는 정말 아름다웠죠ㅎ",
          "original_form": "그날 메뉴는 정말 아름다웠죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100036715.1.18",
          "form": "제가 그 시간에 피자를 시키게 만든ㅎㅎ",
          "original_form": "제가 그 시간에 피자를 시키게 만든ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100036715.1.19",
          "form": "체중 몇키로 빼는게 목표인가요?",
          "original_form": "체중 몇키로 빼는게 목표인가요?",
          "speaker_id": "1",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100036715.1.20",
          "form": "제가 일단 두달만에 15키로가쪄서요",
          "original_form": "제가 일단 두달만에 15키로가쪄서요",
          "speaker_id": "2",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100036715.1.21",
          "form": "한 칠팔십일 만에?",
          "original_form": "한 칠팔십일 만에?",
          "speaker_id": "2",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100036715.1.22",
          "form": "허허ㅜㅜㅜ",
          "original_form": "허허ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100036715.1.23",
          "form": "아니;; 그 두달간 무슨일이ㄷㄷㄷ",
          "original_form": "아니;; 그 두달간 무슨일이ㄷㄷㄷ",
