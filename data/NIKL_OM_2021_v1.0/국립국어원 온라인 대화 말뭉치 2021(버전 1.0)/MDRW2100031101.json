      "utterance": [
        {
          "id": "MDRW2100031101.1.1",
          "form": "강강술래 고깃집 이름인가요?",
          "original_form": "강강술래 고깃집 이름인가요?",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031101.1.2",
          "form": "네 고기 체인점인데 깔끔하고 갈만해요",
          "original_form": "네 고기 체인점인데 깔끔하고 갈만해요",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031101.1.3",
          "form": "아 한번 가봐야겠네요 고기집 자주가시나요",
          "original_form": "아 한번 가봐야겠네요 고기집 자주가시나요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031101.1.4",
          "form": "평일은 관리한다고 거의안가고 주말에 한번쯤가요ㅋ",
          "original_form": "평일은 관리한다고 거의안가고 주말에 한번쯤가요ㅋ",
          "speaker_id": "2",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031101.1.5",
          "form": "육고기를 너무좋아해서ㅜ",
          "original_form": "육고기를 너무좋아해서ㅜ",
          "speaker_id": "2",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031101.1.6",
          "form": "와 자주가시네요",
          "original_form": "와 자주가시네요",
          "speaker_id": "1",
          "time": "20210906 13:44"
        },
        {
          "id": "MDRW2100031101.1.7",
          "form": "어디를 제일 추천하세요?",
          "original_form": "어디를 제일 추천하세요?",
          "speaker_id": "1",
          "time": "20210906 13:44"
        },
        {
          "id": "MDRW2100031101.1.8",
          "form": "전입이 저렴해서",
          "original_form": "전입이 저렴해서",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100031101.1.9",
          "form": "명륜진사. 갈비..",
          "original_form": "명륜진사. 갈비..",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100031101.1.10",
          "form": "아ㅋㅋ저도가봤는데",
          "original_form": "아ㅋㅋ저도가봤는데",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100031101.1.11",
          "form": "칠레산 돼지고기인가?쓰더라구요",
          "original_form": "칠레산 돼지고기인가?쓰더라구요",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100031101.1.12",
          "form": "전 그냥저냥 먹을만하더라구요",
          "original_form": "전 그냥저냥 먹을만하더라구요",
          "speaker_id": "2",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100031101.1.13",
          "form": "아직은 질보단 양이라..",
          "original_form": "아직은 질보단 양이라..",
          "speaker_id": "2",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100031101.1.14",
          "form": "네네 한번씩 먹긴 좋아요",
          "original_form": "네네 한번씩 먹긴 좋아요",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100031101.1.15",
          "form": "저는 비슷한컨셉의",
          "original_form": "저는 비슷한컨셉의",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100031101.1.16",
          "form": "홍대꿀갈비라는곳 갔는데",
          "original_form": "홍대꿀갈비라는곳 갔는데",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100031101.1.17",
          "form": "거기가 좀 더 나은거같더라구요",
          "original_form": "거기가 좀 더 나은거같더라구요",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100031101.1.18",
          "form": "거기도 괜찮아요",
          "original_form": "거기도 괜찮아요",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100031101.1.19",
          "form": "홍대라니..혹시 댁이 어디세요?",
          "original_form": "홍대라니..혹시 댁이 어디세요?",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100031101.1.20",
          "form": "아 저는지방사는데",
          "original_form": "아 저는지방사는데",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100031101.1.21",
          "form": "동네에 간판이름이",
          "original_form": "동네에 간판이름이",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100031101.1.22",
          "form": "홍대꿀갈비더라구요",
          "original_form": "홍대꿀갈비더라구요",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100031101.1.23",
          "form": "홍대랑 아주 멉니다",
          "original_form": "홍대랑 아주 멉니다",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100031101.1.24",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100031101.1.25",
          "form": "고기얘기하니 고기먹고싶네요",
          "original_form": "고기얘기하니 고기먹고싶네요",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100031101.1.26",
          "form": "오늘 저녁에 드셔요ㅋㅋ",
          "original_form": "오늘 저녁에 드셔요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:43"
        }
      ]
