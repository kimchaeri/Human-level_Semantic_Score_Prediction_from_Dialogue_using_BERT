      "utterance": [
        {
          "id": "MDRW2100002359.1.1",
          "form": "날씨가 제법 시원해짐",
          "original_form": "날씨가 제법 시원해짐",
          "speaker_id": "1",
          "time": "20210820 10:19"
        },
        {
          "id": "MDRW2100002359.1.2",
          "form": "이제 좀 선선하죠",
          "original_form": "이제 좀 선선하죠",
          "speaker_id": "2",
          "time": "20210820 10:22"
        },
        {
          "id": "MDRW2100002359.1.3",
          "form": "맞아 가을이야",
          "original_form": "맞아 가을이야",
          "speaker_id": "1",
          "time": "20210820 10:22"
        },
        {
          "id": "MDRW2100002359.1.4",
          "form": "놀기 딱 좋은 날씨임",
          "original_form": "놀기 딱 좋은 날씨임",
          "speaker_id": "2",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002359.1.5",
          "form": "무슨 계절 좋아함?",
          "original_form": "무슨 계절 좋아함?",
          "speaker_id": "1",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002359.1.6",
          "form": "봄거을",
          "original_form": "봄거을",
          "speaker_id": "2",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002359.1.7",
          "form": "역시 ㅋㅋ",
          "original_form": "역시 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002359.1.8",
          "form": "근데 난 겨울도 좋아함",
          "original_form": "근데 난 겨울도 좋아함",
          "speaker_id": "1",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002359.1.9",
          "form": "추워여",
          "original_form": "추워여",
          "speaker_id": "2",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002359.1.10",
          "form": "하지만 붕어빵과 호떡이 있지",
          "original_form": "하지만 붕어빵과 호떡이 있지",
          "speaker_id": "1",
          "time": "20210820 10:24"
        },
        {
          "id": "MDRW2100002359.1.11",
          "form": "온천은 좋아함",
          "original_form": "온천은 좋아함",
          "speaker_id": "2",
          "time": "20210820 10:24"
        },
        {
          "id": "MDRW2100002359.1.12",
          "form": "맞아 노천탕은 겨울에 해야 제맛",
          "original_form": "맞아 노천탕은 겨울에 해야 제맛",
          "speaker_id": "1",
          "time": "20210820 10:24"
        },
        {
          "id": "MDRW2100002359.1.13",
          "form": "붕어빵땡긱다",
          "original_form": "붕어빵땡긱다",
          "speaker_id": "2",
          "time": "20210820 10:25"
        },
        {
          "id": "MDRW2100002359.1.14",
          "form": "붕어빵은 겨울에 먹어야 제맛이지",
          "original_form": "붕어빵은 겨울에 먹어야 제맛이지",
          "speaker_id": "1",
          "time": "20210820 10:25"
        },
        {
          "id": "MDRW2100002359.1.15",
          "form": "담주는 내내 비온다던데",
          "original_form": "담주는 내내 비온다던데",
          "speaker_id": "1",
          "time": "20210820 10:25"
        },
        {
          "id": "MDRW2100002359.1.16",
          "form": "헐 이제 비오면 날씨 풀리겟다",
          "original_form": "헐 이제 비오면 날씨 풀리겟다",
          "speaker_id": "2",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002359.1.17",
          "form": "그치 내내온다했으니 ㅋㅋ",
          "original_form": "그치 내내온다했으니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002359.1.18",
          "form": "가디건의 계절이 오는거지",
          "original_form": "가디건의 계절이 오는거지",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002359.1.19",
          "form": "ㅋㅋ추워지면 좋겟다",
          "original_form": "ㅋㅋ추워지면 좋겟다",
          "speaker_id": "2",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002359.1.20",
          "form": "그래 더운것보단 추운게 좋지",
          "original_form": "그래 더운것보단 추운게 좋지",
          "speaker_id": "1",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002359.1.21",
          "form": "추워지면 또 감기들까봐 걱정이지만",
          "original_form": "추워지면 또 감기들까봐 걱정이지만",
          "speaker_id": "2",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002359.1.22",
          "form": "그래도 더운것보단 추운게 좋지",
          "original_form": "그래도 더운것보단 추운게 좋지",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002359.1.23",
          "form": "근데 꽤 시원해졌는지 오늘 삼실 에어컨 안킴",
          "original_form": "근데 꽤 시원해졌는지 오늘 삼실 에어컨 안킴",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002359.1.24",
          "form": "맞아 시원해여 바람도",
          "original_form": "맞아 시원해여 바람도",
          "speaker_id": "2",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002359.1.25",
          "form": "하지만 간간히 더워",
          "original_form": "하지만 간간히 더워",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002359.1.26",
          "form": "왜 아무도 키지않는가",
          "original_form": "왜 아무도 키지않는가",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002359.1.27",
          "form": "ㅋㅋㅋ선풍기라도",
          "original_form": "ㅋㅋㅋ선풍기라도",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002359.1.28",
          "form": "속상해 ㅋㅋㅋ",
          "original_form": "속상해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002359.1.29",
          "form": "난 조금 더운걸",
          "original_form": "난 조금 더운걸",
          "speaker_id": "1",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002359.1.30",
          "form": "오늘 가볍게 입고왔는데 점점 덥다",
          "original_form": "오늘 가볍게 입고왔는데 점점 덥다",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002359.1.31",
          "form": "ㅋㅋ전 평소대로 입엇더니 시원",
          "original_form": "ㅋㅋ전 평소대로 입엇더니 시원",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002359.1.32",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002359.1.33",
          "form": "난 계속 더운데 나만 더운거 같은 느낌 ㅋㅋ",
          "original_form": "난 계속 더운데 나만 더운거 같은 느낌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002359.1.34",
          "form": "수줍어서 말못하겠어",
          "original_form": "수줍어서 말못하겠어",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002359.1.35",
          "form": "ㅋㅋㅋㅋ에어컨좀..",
          "original_form": "ㅋㅋㅋㅋ에어컨좀..",
          "speaker_id": "2",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100002359.1.36",
          "form": "더워서 죽어감",
          "original_form": "더워서 죽어감",
          "speaker_id": "1",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100002359.1.37",
          "form": "ㄲㅋ살아나야져",
          "original_form": "ㄲㅋ살아나야져",
          "speaker_id": "2",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100002359.1.38",
          "form": "날씨보고 가지",
          "original_form": "날씨보고 가지",
          "speaker_id": "2",
          "time": "20210820 11:37"
        }
      ]
