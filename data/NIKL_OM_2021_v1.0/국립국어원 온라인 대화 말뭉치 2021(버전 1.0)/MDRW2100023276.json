      "utterance": [
        {
          "id": "MDRW2100023276.1.1",
          "form": "방학중 프로그램?",
          "original_form": "방학중 프로그램?",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023276.1.2",
          "form": "이런거 수요조사하고 운영하던데",
          "original_form": "이런거 수요조사하고 운영하던데",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023276.1.3",
          "form": "그럼 그것도 따로 수당이 없는거에요?",
          "original_form": "그럼 그것도 따로 수당이 없는거에요?",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.5",
          "form": "아 그건",
          "original_form": "아 그건",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.6",
          "form": "...정말요?",
          "original_form": "...정말요?",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.7",
          "form": "수당이 이ㅛ져",
          "original_form": "수당이 이ㅛ져",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.8",
          "form": "있죠",
          "original_form": "있죠",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.9",
          "form": "오 금액이 좀 되나요 그래도?",
          "original_form": "오 금액이 좀 되나요 그래도?",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.10",
          "form": "네 고등학교는 그래도 좀 부유해요",
          "original_form": "네 고등학교는 그래도 좀 부유해요",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.11",
          "form": "그거 수강인원수에 따라 금액이 달라져요 아니면 똑같아요?",
          "original_form": "그거 수강인원수에 따라 금액이 달라져요 아니면 똑같아요?",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.12",
          "form": "시간당이라",
          "original_form": "시간당이라",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023276.1.13",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023276.1.14",
          "form": "초근수당하고 비교하면 어느정도 차이 날까요",
          "original_form": "초근수당하고 비교하면 어느정도 차이 날까요",
          "speaker_id": "1",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023276.1.15",
          "form": "초근수당은 얼마 안되지 않나요?",
          "original_form": "초근수당은 얼마 안되지 않나요?",
          "speaker_id": "2",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023276.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023276.1.17",
          "form": "네 ㅋㅋ그쳐 대충 알고 싶어서..",
          "original_form": "네 ㅋㅋ그쳐 대충 알고 싶어서..",
          "speaker_id": "1",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023276.1.18",
          "form": "방과후는 그래도 40_50씩 들어왓던거 같은데",
          "original_form": "방과후는 그래도 40_50씩 들어왓던거 같은데",
          "speaker_id": "2",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023276.1.19",
          "form": "어.. 매일 1교시씩이요?",
          "original_form": "어.. 매일 1교시씩이요?",
          "speaker_id": "1",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023276.1.20",
          "form": "방학중엔 2교시 정도 연강해서 수업하거든요",
          "original_form": "방학중엔 2교시 정도 연강해서 수업하거든요",
          "speaker_id": "2",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023276.1.21",
          "form": "뭔가 초근 비슷한거 같기도 하고..",
          "original_form": "뭔가 초근 비슷한거 같기도 하고..",
          "speaker_id": "1",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023276.1.22",
          "form": "그래도 방학중에 주는거면 괜찮은 것 같기도 하네요",
          "original_form": "그래도 방학중에 주는거면 괜찮은 것 같기도 하네요",
          "speaker_id": "1",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023276.1.23",
          "form": "제가 초근을 사실 잘 안달아서 초근이 얼만지 잘 몰라요 ㅋㅋㅋㅋ",
          "original_form": "제가 초근을 사실 잘 안달아서 초근이 얼만지 잘 몰라요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023276.1.24",
          "form": "진짜 가끔가다 한번 단거라",
          "original_form": "진짜 가끔가다 한번 단거라",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023276.1.25",
          "form": "앗 그렇구나!! 저희끼리만 아는 이야기로 은밀히 알고 싶었는데 아쉽네용",
