        {
          "id": "MDRW2100044320.1.1",
          "form": "대단하다",
          "original_form": "대단하다",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044320.1.2",
          "form": "매번생각하지만 업무에 집중하는 너의 추진력은",
          "original_form": "매번생각하지만 업무에 집중하는 너의 추진력은",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044320.1.3",
          "form": "정말 대단해",
          "original_form": "정말 대단해",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044320.1.4",
          "form": "ㅋㅋㅋ과제집착력",
          "original_form": "ㅋㅋㅋ과제집착력",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044320.1.5",
          "form": "조금만 더 힘내자!!",
          "original_form": "조금만 더 힘내자!!",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044320.1.6",
          "form": "응 덕분에 나도 힘내서 하고있어",
          "original_form": "응 덕분에 나도 힘내서 하고있어",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044320.1.7",
          "form": "업무환경이 덥지는 않으신지",
          "original_form": "업무환경이 덥지는 않으신지",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044320.1.8",
          "form": "저는 괜찮습니다",
          "original_form": "저는 괜찮습니다",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.9",
          "form": "덥나요?",
          "original_form": "덥나요?",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.10",
          "form": "열일해서 열 나나봐",
          "original_form": "열일해서 열 나나봐",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.11",
          "form": "그런가벼",
          "original_form": "그런가벼",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.12",
          "form": "자세가 더운가? 자세도 바꿔보고",
          "original_form": "자세가 더운가? 자세도 바꿔보고",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.13",
          "form": "집에서 열일하는 일개미들",
          "original_form": "집에서 열일하는 일개미들",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.14",
          "form": "선풍기도 틀고 하면서 업무환경에 변화를 줘봤네",
          "original_form": "선풍기도 틀고 하면서 업무환경에 변화를 줘봤네",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.15",
          "form": "일개미 흑흑 사업개미 되고싶다",
          "original_form": "일개미 흑흑 사업개미 되고싶다",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044320.1.16",
          "form": "경제적 자유를 주십쇼",
          "original_form": "경제적 자유를 주십쇼",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044320.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044320.1.18",
          "form": "일개미탈출을 꿈꾸다",
          "original_form": "일개미탈출을 꿈꾸다",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044320.1.19",
          "form": "요새 그거 유행이잖아",
          "original_form": "요새 그거 유행이잖아",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044320.1.20",
          "form": "파이어족",
          "original_form": "파이어족",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044320.1.21",
          "form": "일찍 은퇴하는거",
          "original_form": "일찍 은퇴하는거",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044320.1.22",
          "form": "그것도 돈이 있어야쥐",
          "original_form": "그것도 돈이 있어야쥐",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044320.1.23",
          "form": "그치 그래서 다들 주식하고 뭐하고 부업하면서",
          "original_form": "그치 그래서 다들 주식하고 뭐하고 부업하면서",
