        },
        {
          "id": "MDRW2100046170.1.2",
          "form": "동물 좋아하시나요?",
          "original_form": "동물 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046170.1.3",
          "form": "네 좋아하는 편이죠",
          "original_form": "네 좋아하는 편이죠",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046170.1.4",
          "form": "아 그럼 동물 키우시나요?",
          "original_form": "아 그럼 동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046170.1.5",
          "form": "아니요 보는건 좋아하는데 키우는건 힘들거같아요",
          "original_form": "아니요 보는건 좋아하는데 키우는건 힘들거같아요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046170.1.6",
          "form": "아무래도 그렇긴하죠",
          "original_form": "아무래도 그렇긴하죠",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046170.1.7",
          "form": "생명을 책임져야 되니까",
          "original_form": "생명을 책임져야 되니까",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046170.1.8",
          "form": "맞아요 책임감 있어야하죠",
          "original_form": "맞아요 책임감 있어야하죠",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046170.1.9",
          "form": "동물 키우시나요?",
