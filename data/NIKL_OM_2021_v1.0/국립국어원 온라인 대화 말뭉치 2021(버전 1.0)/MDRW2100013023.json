        },
        {
          "id": "MDRW2100013023.1.2",
          "form": "게임 엄청 좋아했죠ㅋㅋ",
          "original_form": "게임 엄청 좋아했죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:11"
        },
        {
          "id": "MDRW2100013023.1.3",
          "form": "요즘엔 잘 안하는 것같아요",
          "original_form": "요즘엔 잘 안하는 것같아요",
          "speaker_id": "2",
          "time": "20210825 11:11"
        },
        {
          "id": "MDRW2100013023.1.4",
          "form": "아 그래요??? 무슨 게임 많이 하세요??ㅋㅋㅋ",
          "original_form": "아 그래요??? 무슨 게임 많이 하세요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100013023.1.5",
          "form": "요즘엔 플스4로 해요!",
          "original_form": "요즘엔 플스4로 해요!",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100013023.1.6",
          "form": "오 플스 4에 게임 많나요? 저는 그런 건 없어서요ㅎㅎ",
          "original_form": "오 플스 4에 게임 많나요? 저는 그런 건 없어서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:22"
        },
        {
          "id": "MDRW2100013023.1.7",
          "form": "게임은 많죠ㅋㅋ",
          "original_form": "게임은 많죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013023.1.8",
          "form": "그런데 거의 유료라서요ㅎㅎ",
          "original_form": "그런데 거의 유료라서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013023.1.9",
          "form": "그래서 전 집앞에 플스 시디 대여점에 자주가요",
          "original_form": "그래서 전 집앞에 플스 시디 대여점에 자주가요",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013023.1.10",
          "form": "아 대여도 해주시는군요! 약간 옛날 만화방 같네요ㅋㅋㅋ",
          "original_form": "아 대여도 해주시는군요! 약간 옛날 만화방 같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013023.1.11",
          "form": "저도 그런 느낌나서 자주 가요ㅎㅎ",
          "original_form": "저도 그런 느낌나서 자주 가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013023.1.12",
          "form": "name1님은 게임하세요?",
          "original_form": "&name1&님은 게임하세요?",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013023.1.13",
          "form": "전 주로 롤해요 쿠키런도 하고요!",
          "original_form": "전 주로 롤해요 쿠키런도 하고요!",
