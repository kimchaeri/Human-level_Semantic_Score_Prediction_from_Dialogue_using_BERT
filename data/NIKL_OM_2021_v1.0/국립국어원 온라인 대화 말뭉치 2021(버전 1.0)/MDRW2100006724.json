      "utterance": [
        {
          "id": "MDRW2100006724.1.1",
          "form": "반려동물 키워보신 적 있으신지요",
          "original_form": "반려동물 키워보신 적 있으신지요",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006724.1.2",
          "form": "예전에 어릴때 키워봤어요",
          "original_form": "예전에 어릴때 키워봤어요",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100006724.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100006724.1.4",
          "form": "저는 지금 구피라는 물고기 키우고 있답니다",
          "original_form": "저는 지금 구피라는 물고기 키우고 있답니다",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100006724.1.5",
          "form": "저도 구피 키웠었어요! 친구가 감당이 안되서 줬는데 저도 감당 안됐었죠 ㅠㅠ",
          "original_form": "저도 구피 키웠었어요! 친구가 감당이 안되서 줬는데 저도 감당 안됐었죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100006724.1.6",
          "form": "구피 예쁘잖아요",
          "original_form": "구피 예쁘잖아요",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100006724.1.7",
          "form": "몇머리나 있아요?",
          "original_form": "몇머리나 있아요?",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100006724.1.8",
          "form": "마리",
          "original_form": "마리",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100006724.1.9",
          "form": "처음에 10마리였는데 지금은 100마리쯤 됩니다",
          "original_form": "처음에 10마리였는데 지금은 100마리쯤 됩니다",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100006724.1.10",
          "form": "구피들은 조금 번식을 천천히 할 필요가 있다고 생각해요",
          "original_form": "구피들은 조금 번식을 천천히 할 필요가 있다고 생각해요",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006724.1.11",
          "form": "워낙 번식력이 좋은 생물이죠",
          "original_form": "워낙 번식력이 좋은 생물이죠",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006724.1.12",
          "form": "그래서 종종 가정분양도 합니다",
          "original_form": "그래서 종종 가정분양도 합니다",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006724.1.13",
          "form": "오래 키우셨나요?",
          "original_form": "오래 키우셨나요?",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006724.1.14",
          "form": "거의 10년 키운 듯 합니다",
          "original_form": "거의 10년 키운 듯 합니다",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006724.1.15",
          "form": "저는 털달린애들 좋아해요",
          "original_form": "저는 털달린애들 좋아해요",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006724.1.16",
          "form": "강아지 고양이 이런애들이요",
          "original_form": "강아지 고양이 이런애들이요",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006724.1.17",
          "form": "그런 동물들이 더 정이 가지요",
          "original_form": "그런 동물들이 더 정이 가지요",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100006724.1.18",
          "form": "저는 괸리에 자신이 없어서 물고기 키우기 시작했답니다",
          "original_form": "저는 괸리에 자신이 없어서 물고기 키우기 시작했답니다",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100006724.1.19",
          "form": "저는 책임질 준비가 안됐어요",
          "original_form": "저는 책임질 준비가 안됐어요",
          "speaker_id": "2",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100006724.1.20",
          "form": "아마 나이가 많이 들면 키울지도 모르겠어요",
          "original_form": "아마 나이가 많이 들면 키울지도 모르겠어요",
          "speaker_id": "2",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100006724.1.21",
          "form": "그래요 생명이니까 책임질 수 있을때 키우는 게 맞다고 봅니다",
          "original_form": "그래요 생명이니까 책임질 수 있을때 키우는 게 맞다고 봅니다",
          "speaker_id": "1",
          "time": "20210908 09:38"
        }
      ]
