      "utterance": [
        {
          "id": "MDRW2100033247.1.1",
          "form": "저녁은 뭐해드셨어요?",
          "original_form": "저녁은 뭐해드셨어요?",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100033247.1.2",
          "form": "아이들만 먹이고\r\n안먹었어요:)",
          "original_form": "아이들만 먹이고\r\n안먹었어요:)",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100033247.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100033247.1.4",
          "form": "먹고있어요",
          "original_form": "먹고있어요",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100033247.1.5",
          "form": "저녁은 궁중떡볶이",
          "original_form": "저녁은 궁중떡볶이",
          "speaker_id": "2",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100033247.1.6",
          "form": "왜 안드세요?",
          "original_form": "왜 안드세요?",
          "speaker_id": "2",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100033247.1.7",
          "form": "엄마표인가요? ㅋㅋㅋ",
          "original_form": "엄마표인가요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100033247.1.8",
          "form": "별로 생각이 없네영",
          "original_form": "별로 생각이 없네영",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100033247.1.9",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100033247.1.10",
          "form": "잘드셔야죠",
          "original_form": "잘드셔야죠",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100033247.1.11",
          "form": "그러게요ㅠ\r\n내일부터 잘먹는걸로!",
          "original_form": "그러게요ㅠ\r\n내일부터 잘먹는걸로!",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033247.1.12",
          "form": "입맛이 없으신가보군요",
          "original_form": "입맛이 없으신가보군요",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033247.1.13",
          "form": "킹크랩 먹고싶네여ㅋㅋ",
          "original_form": "킹크랩 먹고싶네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033247.1.14",
          "form": "입맛이 없는건 아닌듯ㅋㅋ",
          "original_form": "입맛이 없는건 아닌듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033247.1.15",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033247.1.16",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033247.1.17",
          "form": "킹크랩",
          "original_form": "킹크랩",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033247.1.18",
          "form": "ㅋㅋ연안부두 출똥해야겠어여",
          "original_form": "ㅋㅋ연안부두 출똥해야겠어여",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100033247.1.19",
          "form": "언제요?",
          "original_form": "언제요?",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033247.1.20",
          "form": "음 조만간?ㅋㅋ\r\n연안부두 아시는 곳 있나요?",
          "original_form": "음 조만간?ㅋㅋ\r\n연안부두 아시는 곳 있나요?",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100033247.1.21",
          "form": "연안부두에 꽃게사러 자주갔었는데",
          "original_form": "연안부두에 꽃게사러 자주갔었는데",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033247.1.22",
          "form": "아는곳은 없어요",
          "original_form": "아는곳은 없어요",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
