      "utterance": [
        {
          "id": "MDRW2100031397.1.1",
          "form": "이틀째가 많이 아플텐데 괜찮으세요?",
          "original_form": "이틀째가 많이 아플텐데 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100031397.1.2",
          "form": "네 백신 2차 맞고 24시간 지난 다음엔",
          "original_form": "네 백신 2차 맞고 24시간 지난 다음엔",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031397.1.3",
          "form": "몸살기가 있었는데",
          "original_form": "몸살기가 있었는데",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031397.1.4",
          "form": "(오늘 낮엔)",
          "original_form": "(오늘 낮엔)",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031397.1.5",
          "form": "지금은 한결 괜찮아졌어요",
          "original_form": "지금은 한결 괜찮아졌어요",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031397.1.6",
          "form": "백신 예약 하셨어요?",
          "original_form": "백신 예약 하셨어요?",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031397.1.7",
          "form": "2차까지 맞으셨군요 ㅠ",
          "original_form": "2차까지 맞으셨군요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031397.1.8",
          "form": "저는 화요일에 1차 맞았어요",
          "original_form": "저는 화요일에 1차 맞았어요",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031397.1.9",
          "form": "2차는 아직이구요",
          "original_form": "2차는 아직이구요",
          "speaker_id": "1",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031397.1.10",
          "form": "네 저는 10부제 전에 1차를 맞아서",
          "original_form": "네 저는 10부제 전에 1차를 맞아서",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031397.1.11",
          "form": "10부제라서 빨리 예약한다고 했는데도 날짜가 ㅠ",
          "original_form": "10부제라서 빨리 예약한다고 했는데도 날짜가 ㅠ",
          "speaker_id": "1",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031397.1.12",
          "form": "잔여백신을 못잡았거든요 ㅠㅠ",
          "original_form": "잔여백신을 못잡았거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031397.1.13",
          "form": "백신 예약 힘들진 않으셨어요?",
          "original_form": "백신 예약 힘들진 않으셨어요?",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031397.1.14",
          "form": "오히려 10부제 예약은 쉬운편이었어요",
          "original_form": "오히려 10부제 예약은 쉬운편이었어요",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031397.1.15",
          "form": "서버가 상당히 안정됐거든요",
          "original_form": "서버가 상당히 안정됐거든요",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031397.1.16",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031397.1.17",
          "form": "저는 그 전에 예약했는데",
          "original_form": "저는 그 전에 예약했는데",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031397.1.18",
          "form": "밤12시 땡하고 서버 다운돼서 새벽에 다시 일어나서 하고 그랬어요ㅜ",
          "original_form": "밤12시 땡하고 서버 다운돼서 새벽에 다시 일어나서 하고 그랬어요ㅜ",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031397.1.19",
          "form": "그.. 12시에 서버 땡 할때도 티켓팅 대리로 했었는데요",
          "original_form": "그.. 12시에 서버 땡 할때도 티켓팅 대리로 했었는데요",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031397.1.20",
          "form": "한시간만에 풀려서",
          "original_form": "한시간만에 풀려서",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031397.1.21",
          "form": "성공했어요 ㅎㅎ...",
          "original_form": "성공했어요 ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031397.1.22",
          "form": "그래도 10부제 전에 서버가 안정화되고",
          "original_form": "그래도 10부제 전에 서버가 안정화되고",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031397.1.23",
          "form": "진짜 그때 서버 생각하면.. 증말로..",
          "original_form": "진짜 그때 서버 생각하면.. 증말로..",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031397.1.24",
          "form": "신청시간도 당겨져서 다행이예요",
          "original_form": "신청시간도 당겨져서 다행이예요",
          "speaker_id": "2",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031397.1.25",
          "form": "하고난 뒤에 비행기모드라는 치트키가 있는걸 늦게 알았죠",
          "original_form": "하고난 뒤에 비행기모드라는 치트키가 있는걸 늦게 알았죠",
          "speaker_id": "1",
          "time": "20210904 21:24"
        }
      ]
