      "utterance": [
        {
          "id": "MDRW2100006881.1.1",
          "form": "우유는 소화가 안돼요",
          "original_form": "우유는 소화가 안돼요",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006881.1.2",
          "form": "유당불내증인가요? 동양인중에 유독 많대요",
          "original_form": "유당불내증인가요? 동양인중에 유독 많대요",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006881.1.3",
          "form": "네 속이 불편해요",
          "original_form": "네 속이 불편해요",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006881.1.4",
          "form": "동생도 우유만 먹으면 배가 아프대요",
          "original_form": "동생도 우유만 먹으면 배가 아프대요",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006881.1.5",
          "form": "근데 요거트는 괜찮구요",
          "original_form": "근데 요거트는 괜찮구요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006881.1.6",
          "form": "그래요? 요거트랑 뭐가 다르지요",
          "original_form": "그래요? 요거트랑 뭐가 다르지요",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006881.1.7",
          "form": "새우유가 아니라 그런가",
          "original_form": "새우유가 아니라 그런가",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006881.1.8",
          "form": "생우유랑 뭐가 다른가요",
          "original_form": "생우유랑 뭐가 다른가요",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006881.1.9",
          "form": "발효식품이라 그런가봐요",
          "original_form": "발효식품이라 그런가봐요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006881.1.10",
          "form": "딸기우유랑 바나나우유는요?",
          "original_form": "딸기우유랑 바나나우유는요?",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100006881.1.11",
          "form": "그건 괜찮아요",
          "original_form": "그건 괜찮아요",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100006881.1.12",
          "form": "가짜우유일까요 ㅋㅋ",
          "original_form": "가짜우유일까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100006881.1.13",
          "form": "가짜우유도 많대요",
          "original_form": "가짜우유도 많대요",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100006881.1.14",
          "form": "그럼 두유를 마셔봐요",
          "original_form": "그럼 두유를 마셔봐요",
          "speaker_id": "1",
          "time": "20210908 11:16"
