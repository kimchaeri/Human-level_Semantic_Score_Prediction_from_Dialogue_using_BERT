      "utterance": [
        {
          "id": "MDRW2100031300.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031300.1.2",
          "form": "안녕~~",
          "original_form": "안녕~~",
          "speaker_id": "2",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100031300.1.3",
          "form": "공연 좋아해??",
          "original_form": "공연 좋아해??",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100031300.1.4",
          "form": "전시회나ㅎㅎ",
          "original_form": "전시회나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100031300.1.5",
          "form": "응 좋아해!!",
          "original_form": "응 좋아해!!",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031300.1.6",
          "form": "최근에 뮤지컬 보러갔었어@",
          "original_form": "최근에 뮤지컬 보러갔었어@",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031300.1.7",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031300.1.8",
          "form": "뭐 봤어??",
          "original_form": "뭐 봤어??",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031300.1.9",
          "form": "나도 관심가긴 하는데",
          "original_form": "나도 관심가긴 하는데",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031300.1.10",
          "form": "뭐 볼지 모르겠더라",
          "original_form": "뭐 볼지 모르겠더라",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031300.1.11",
          "form": "마리앙투아네트!!",
          "original_form": "마리앙투아네트!!",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031300.1.12",
          "form": "사실 나도 뮤지컬 처음 보러 간 거라ㅋㅋㅋ 좋아하는 연예인 나와서 보러갔었어!",
          "original_form": "사실 나도 뮤지컬 처음 보러 간 거라ㅋㅋㅋ 좋아하는 연예인 나와서 보러갔었어!",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031300.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031300.1.14",
          "form": "누구 나오는데??",
          "original_form": "누구 나오는데??",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031300.1.15",
          "form": "도영 알엉 ㅎ?",
          "original_form": "도영 알엉 ㅎ?",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031300.1.16",
          "form": "뮤지컬 보러간 적이 없었지만 사람들이 왜 많이 보는지 알겠더라!",
          "original_form": "뮤지컬 보러간 적이 없었지만 사람들이 왜 많이 보는지 알겠더라!",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031300.1.17",
          "form": "영화보다 확실히 에너지 주는 게 많아가지구",
          "original_form": "영화보다 확실히 에너지 주는 게 많아가지구",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031300.1.18",
          "form": "오 나 연예인을 잘몰라서ㅜㅜ",
          "original_form": "오 나 연예인을 잘몰라서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031300.1.19",
          "form": "진짜 좋아하는 연예인 가까이서보면 좋겠다",
          "original_form": "진짜 좋아하는 연예인 가까이서보면 좋겠다",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031300.1.20",
          "form": "맞아 처음 보러 간 거라 완전 좋았어!ㅎㅎ",
          "original_form": "맞아 처음 보러 간 거라 완전 좋았어!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031300.1.21",
          "form": "아냐 자리는 가깝진 않았지만 그냥 같은 공간에 있는 거 자체가 좋은거짘ㅋㅋ",
          "original_form": "아냐 자리는 가깝진 않았지만 그냥 같은 공간에 있는 거 자체가 좋은거짘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100031300.1.22",
          "form": "혹시 뮤지컬 안 봤다면 한 번 보는것도 추천해!",
          "original_form": "혹시 뮤지컬 안 봤다면 한 번 보는것도 추천해!",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100031300.1.23",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031300.1.24",
          "form": "ㅋㅋㅋ 한번 찾아보고 나도 가볼게!",
          "original_form": "ㅋㅋㅋ 한번 찾아보고 나도 가볼게!",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031300.1.25",
          "form": "혹시 좋아하는 영화 있어??",
          "original_form": "혹시 좋아하는 영화 있어??",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031300.1.26",
          "form": "나",
          "original_form": "나",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031300.1.27",
          "form": "마블시리즈?좋아하고",
          "original_form": "마블시리즈?좋아하고",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031300.1.28",
          "form": "애니메이션도 좋아해ㅎㅎ",
          "original_form": "애니메이션도 좋아해ㅎㅎ",
