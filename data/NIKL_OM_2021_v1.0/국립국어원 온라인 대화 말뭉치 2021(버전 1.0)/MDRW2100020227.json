        },
        {
          "id": "MDRW2100020227.1.2",
          "form": "아웃프로",
          "original_form": "아웃프로",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.3",
          "form": "고프로",
          "original_form": "고프로",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.4",
          "form": "우리 뭔겜하지",
          "original_form": "우리 뭔겜하지",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.5",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.6",
          "form": "그거 이야기하고있었지",
          "original_form": "그거 이야기하고있었지",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.7",
          "form": "할 게임이 없어..",
          "original_form": "할 게임이 없어..",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.8",
          "form": "왜 라테일은 게임 아닌 거 같지",
          "original_form": "왜 라테일은 게임 아닌 거 같지",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.9",
          "form": "구몬이지",
          "original_form": "구몬이지",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.10",
          "form": "재밌는 숙제같은거",
          "original_form": "재밌는 숙제같은거",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.11",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.12",
          "form": "재미는있나?",
          "original_form": "재미는있나?",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.13",
          "form": "재밌으셨나요?",
          "original_form": "재밌으셨나요?",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.14",
          "form": "나 어릴때 남의 구몬 풀어주는거 좋아함",
          "original_form": "나 어릴때 남의 구몬 풀어주는거 좋아함",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020227.1.15",
          "form": "나는 구몬 안했어요",
          "original_form": "나는 구몬 안했어요",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.16",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ저는",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.18",
          "form": "눈높이 잠깐했었음",
          "original_form": "눈높이 잠깐했었음",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.19",
          "form": "오 대박이다",
          "original_form": "오 대박이다",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.20",
          "form": "어댔어여",
          "original_form": "어댔어여",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.21",
          "form": "노잼이였음",
          "original_form": "노잼이였음",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.22",
          "form": "아슂다..",
          "original_form": "아슂다..",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.23",
          "form": "수학했던가",
          "original_form": "수학했던가",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.24",
          "form": "난 맨날 틴구들 하고있아먄",
          "original_form": "난 맨날 틴구들 하고있아먄",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.25",
          "form": "나 해도돼?",
          "original_form": "나 해도돼?",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.26",
          "form": "하면 좋아하더라",
          "original_form": "하면 좋아하더라",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.27",
          "form": "응 해",
          "original_form": "응 해",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.28",
          "form": "근데 수학 그렇게햐도",
          "original_form": "근데 수학 그렇게햐도",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.29",
          "form": "그럼 나는 재밋게 푸는거지",
          "original_form": "그럼 나는 재밋게 푸는거지",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.30",
          "form": "지금 name1임",
          "original_form": "지금 &name1&임",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ 얼마나 했는데요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 얼마나 했는데요",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020227.1.32",
          "form": "수학 어떻게 하는데",
          "original_form": "수학 어떻게 하는데",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.33",
          "form": "몰라요 그랴도",
          "original_form": "몰라요 그랴도",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.34",
          "form": "수학은 중학교 이후가 중여한거같음",
          "original_form": "수학은 중학교 이후가 중여한거같음",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.35",
          "form": "초1땐가 눈높이하고",
          "original_form": "초1땐가 눈높이하고",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.36",
          "form": "초 3-5학년땬",
          "original_form": "초 3-5학년땬",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.37",
          "form": "공부학원다녓었는데",
          "original_form": "공부학원다녓었는데",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.38",
          "form": "걍 든게없어",
          "original_form": "걍 든게없어",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.39",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.40",
          "form": "나 뭔말핵다고 욕이래",
          "original_form": "나 뭔말핵다고 욕이래",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020227.1.41",
          "form": "초등학교 수학은 암튼",
          "original_form": "초등학교 수학은 암튼",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.42",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.43",
          "form": "큰 의미가 없는거같다고 말함",
          "original_form": "큰 의미가 없는거같다고 말함",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.44",
          "form": "욕좀고마해~",
          "original_form": "욕좀고마해~",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.45",
          "form": "나 초등학생땐 수학 반해서 일드잉엇는데",
          "original_form": "나 초등학생땐 수학 반해서 일드잉엇는데",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.46",
          "form": "나그러고 중딩이후로 완전 놓았다",
          "original_form": "나그러고 중딩이후로 완전 놓았다",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.47",
          "form": "중학교에서 놓으니가 걍 name1됨",
          "original_form": "중학교에서 놓으니가 걍 &name1&됨",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.49",
          "form": "아니 반해서는 뭐야",
          "original_form": "아니 반해서는 뭐야",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.50",
          "form": "반에서",
          "original_form": "반에서",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.51",
          "form": "나 수학경시대회에서 상도탓건만",
          "original_form": "나 수학경시대회에서 상도탓건만",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.52",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.53",
          "form": "끈기가 사라지니",
          "original_form": "끈기가 사라지니",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.54",
          "form": "그런걸 하다니",
          "original_form": "그런걸 하다니",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.55",
          "form": "아무의미가없더라",
          "original_form": "아무의미가없더라",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.56",
          "form": "걍 학교에서 하지 않나요",
          "original_form": "걍 학교에서 하지 않나요",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020227.1.57",
          "form": "전학년",
          "original_form": "전학년",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020227.1.58",
          "form": "나는 안 했는데",
          "original_form": "나는 안 했는데",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020227.1.59",
          "form": "ㅎㄹ 저희학교는",
          "original_form": "ㅎㄹ 저희학교는",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020227.1.60",
          "form": "어케하누",
          "original_form": "어케하누",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020227.1.61",
          "form": "걍 시험 전체다봐서",
          "original_form": "걍 시험 전체다봐서",
