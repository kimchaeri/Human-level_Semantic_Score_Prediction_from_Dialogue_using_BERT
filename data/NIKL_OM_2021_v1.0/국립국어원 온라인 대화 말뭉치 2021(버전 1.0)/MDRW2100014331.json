      "utterance": [
        {
          "id": "MDRW2100014331.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014331.1.2",
          "form": "아뇨 무교입니다",
          "original_form": "아뇨 무교입니다",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014331.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014331.1.4",
          "form": "네 전 기독교에요:)",
          "original_form": "네 전 기독교에요:)",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014331.1.5",
          "form": "혹시 종교 관련 궁금한 부분 있으세요?",
          "original_form": "혹시 종교 관련 궁금한 부분 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014331.1.6",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014331.1.7",
          "form": "교회다니세요?",
          "original_form": "교회다니세요?",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014331.1.8",
          "form": "네 다니고 있어요~\n요즘은 못 나가고 비대면으로 해요ㅠ",
          "original_form": "네 다니고 있어요~\n요즘은 못 나가고 비대면으로 해요ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014331.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014331.1.10",
          "form": "저희서방님이 둘째요",
          "original_form": "저희서방님이 둘째요",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014331.1.11",
          "form": "목사세요",
          "original_form": "목사세요",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014331.1.12",
          "form": "비대면 1년 넘었어요",
          "original_form": "비대면 1년 넘었어요",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014331.1.13",
          "form": "고생이 많으세요",
          "original_form": "고생이 많으세요",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014331.1.14",
          "form": "나가서 듣고 싶으실텐데",
          "original_form": "나가서 듣고 싶으실텐데",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014331.1.15",
          "form": "요새 힘드시죠",
          "original_form": "요새 힘드시죠",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014331.1.16",
          "form": "비대면 하시고 수칙 잘지키시고 보기좋아요 ㅎㅎ",
          "original_form": "비대면 하시고 수칙 잘지키시고 보기좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014331.1.17",
          "form": "목사님이시구나!\n그니까요..ㅠ 아이들도 교회학교 못 가고 줌으로 하네요;;ㅎㅎ",
          "original_form": "목사님이시구나!\n그니까요..ㅠ 아이들도 교회학교 못 가고 줌으로 하네요;;ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014331.1.18",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014331.1.19",
          "form": "저는 얘기만 들었지 직접 보지는 안았어요",
          "original_form": "저는 얘기만 들었지 직접 보지는 안았어요",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014331.1.20",
          "form": "학교처럼 하는거죠",
          "original_form": "학교처럼 하는거죠",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014331.1.21",
          "form": "네네~찬양하고 말씀듣고 해요ㅎㅎ",
          "original_form": "네네~찬양하고 말씀듣고 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014331.1.22",
          "form": "똑샅네요",
          "original_form": "똑샅네요",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014331.1.23",
          "form": "같네요",
          "original_form": "같네요",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014331.1.24",
          "form": "저희 서방님도 잘하시는지 물어보긴 해야겠러요",
          "original_form": "저희 서방님도 잘하시는지 물어보긴 해야겠러요",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014331.1.25",
          "form": "예배 못하셔서 교회가 비어있으세요 작은교획ㅎ",
          "original_form": "예배 못하셔서 교회가 비어있으세요 작은교획ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014331.1.26",
          "form": "교회고",
          "original_form": "교회고",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014331.1.27",
          "form": "아이고 ㅠㅠ 그렇군요 ㅠㅠ",
          "original_form": "아이고 ㅠㅠ 그렇군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014331.1.28",
          "form": "네 동서가 돈버라 고생해요",
          "original_form": "네 동서가 돈버라 고생해요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014331.1.29",
          "form": "서방님 도와주느라",
          "original_form": "서방님 도와주느라",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014331.1.30",
          "form": "개척이 힘들어요",
          "original_form": "개척이 힘들어요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014331.1.31",
          "form": "그쵸 개척이 힘들어요..힘내시길!!",
          "original_form": "그쵸 개척이 힘들어요..힘내시길!!",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014331.1.32",
          "form": "네 전 잘몰라요 도움 이 안돼요",
          "original_form": "네 전 잘몰라요 도움 이 안돼요",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014331.1.33",
          "form": "근처면 도와줄텐대",
          "original_form": "근처면 도와줄텐대",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014331.1.34",
          "form": "도움주기가 어렵죵 ㅠㅠ",
          "original_form": "도움주기가 어렵죵 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:02"
        }
      ]
