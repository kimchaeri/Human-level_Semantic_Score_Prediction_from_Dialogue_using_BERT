        },
        {
          "id": "MDRW2100029044.1.2",
          "form": "모든 학생이",
          "original_form": "모든 학생이",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.3",
          "form": "똑같은 시간에 쳐야 한다고",
          "original_form": "똑같은 시간에 쳐야 한다고",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.5",
          "form": "처음에 월요일 저녁 6시에 치겠습니다",
          "original_form": "처음에 월요일 저녁 6시에 치겠습니다",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.6",
          "form": "이랬는데 누가 알바간다 해서",
          "original_form": "이랬는데 누가 알바간다 해서",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.8",
          "form": "목요일 6시에 치겠습니다",
          "original_form": "목요일 6시에 치겠습니다",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.9",
          "form": "또 알바 나오고",
          "original_form": "또 알바 나오고",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.10",
          "form": "금요일 아침부터 알바를 하지는 않겠죠",
          "original_form": "금요일 아침부터 알바를 하지는 않겠죠",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029044.1.12",
          "form": "금요일 아침 9시에 치도록 하겠습니다 9월 2일 오후 5시까지 안되는 학생은",
          "original_form": "금요일 아침 9시에 치도록 하겠습니다 9월 2일 오후 5시까지 안되는 학생은",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.13",
          "form": "메일을 보내주세요 이랬는데",
          "original_form": "메일을 보내주세요 이랬는데",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.14",
          "form": "또안됏음?",
          "original_form": "또안됏음?",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.15",
          "form": "그 당일날",
          "original_form": "그 당일날",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.16",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.17",
          "form": "벌써 금요일 아침 알바가 있다는 학생이 연락 왔습니다",
          "original_form": "벌써 금요일 아침 알바가 있다는 학생이 연락 왔습니다",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.19",
          "form": "마음 같아서는 알바를 제치고 퀴즈를 치라고 하고 싶지만",
          "original_form": "마음 같아서는 알바를 제치고 퀴즈를 치라고 하고 싶지만",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029044.1.20",
          "form": "그럼 너무 꼰대 같이 되어버릴테니",
          "original_form": "그럼 너무 꼰대 같이 되어버릴테니",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029044.1.21",
          "form": "각자 수업시간에 퀴즈를 진행하도록 하겠습니다",
          "original_form": "각자 수업시간에 퀴즈를 진행하도록 하겠습니다",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029044.1.22",
          "form": "교수가 재치있네",
          "original_form": "교수가 재치있네",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029044.1.23",
          "form": "100명에 맞추려니 너무 힘이 드네요",
          "original_form": "100명에 맞추려니 너무 힘이 드네요",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029044.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029044.1.25",
          "form": "수강생이 100명이었구나",
          "original_form": "수강생이 100명이었구나",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029044.1.26",
          "form": "그럼 그럴만하다",
          "original_form": "그럼 그럴만하다",
          "speaker_id": "1",
          "time": "20210902 23:28"
        }
