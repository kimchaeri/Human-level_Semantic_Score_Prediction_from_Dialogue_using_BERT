        },
        {
          "id": "MDRW2100008530.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008530.1.4",
          "form": "일해여",
          "original_form": "일해여",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008530.1.5",
          "form": "어떤일하새요?",
          "original_form": "어떤일하새요?",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008530.1.6",
          "form": "많이 힘들어요?",
          "original_form": "많이 힘들어요?",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008530.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008530.1.8",
          "form": "이직 준비중",
          "original_form": "이직 준비중",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008530.1.9",
          "form": "어유 뭐가 제일 힘드셨나요",
          "original_form": "어유 뭐가 제일 힘드셨나요",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008530.1.10",
          "form": "사람이뵤",
          "original_form": "사람이뵤",
          "speaker_id": "1",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008530.1.11",
          "form": "대인관계힘들죠",
          "original_form": "대인관계힘들죠",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008530.1.12",
          "form": "다른건 괜찮았어요?",
          "original_form": "다른건 괜찮았어요?",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008530.1.13",
          "form": "다른것도 문제지만",
          "original_form": "다른것도 문제지만",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008530.1.14",
          "form": "사람이 제일",
          "original_form": "사람이 제일",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008530.1.15",
          "form": "안좋은 일 있었나요?",
          "original_form": "안좋은 일 있었나요?",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008530.1.16",
          "form": "참는대 한계가",
          "original_form": "참는대 한계가",
