      "utterance": [
        {
          "id": "MDRW2100017148.1.1",
          "form": "기억에 남는 고객있었나요",
          "original_form": "기억에 남는 고객있었나요",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.2",
          "form": "고객님...",
          "original_form": "고객님...",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.3",
          "form": "좋았던 고객님들?",
          "original_form": "좋았던 고객님들?",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.4",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.5",
          "form": "아무나요",
          "original_form": "아무나요",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.6",
          "form": "자주 웃으셨던 고객님들",
          "original_form": "자주 웃으셨던 고객님들",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.7",
          "form": "제가 실수했는데",
          "original_form": "제가 실수했는데",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.8",
          "form": "괜찮다고 넘어가주신 분들",
          "original_form": "괜찮다고 넘어가주신 분들",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.9",
          "form": "아 그런 분들 되게 감사하죠",
          "original_form": "아 그런 분들 되게 감사하죠",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.10",
          "form": "힘내라고 해주신 분들",
          "original_form": "힘내라고 해주신 분들",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017148.1.11",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.12",
          "form": "제 고객님은 아니었는데",
          "original_form": "제 고객님은 아니었는데",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.13",
          "form": "힘이 많이 났겠어요",
          "original_form": "힘이 많이 났겠어요",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.14",
          "form": "제 동기가 와인을 바지에 쏟은 거에요",
          "original_form": "제 동기가 와인을 바지에 쏟은 거에요",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.15",
          "form": "네 말씀하세요",
          "original_form": "네 말씀하세요",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.16",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.17",
          "form": "그런일이",
          "original_form": "그런일이",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.18",
          "form": "고객님 양복바지에요",
          "original_form": "고객님 양복바지에요",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.19",
          "form": "동기 잘못이라기에는",
          "original_form": "동기 잘못이라기에는",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.20",
          "form": "식겁하셨겠다",
          "original_form": "식겁하셨겠다",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.21",
          "form": "기내가 흔들려서요",
          "original_form": "기내가 흔들려서요",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.22",
          "form": "근데 또 정말 프로페셔널하면",
          "original_form": "근데 또 정말 프로페셔널하면",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017148.1.23",
          "form": "아.. 고의치않은 사고였구나",
          "original_form": "아.. 고의치않은 사고였구나",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.24",
          "form": "그런 실수 없기도하니깐...",
          "original_form": "그런 실수 없기도하니깐...",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.25",
          "form": "근데 그 승객분이 웃으면서 넘어가주시더라구요",
          "original_form": "근데 그 승객분이 웃으면서 넘어가주시더라구요",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.26",
          "form": "사람은 누구나 다 실수해요 완벽할 수 없으니까",
          "original_form": "사람은 누구나 다 실수해요 완벽할 수 없으니까",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.27",
          "form": "젊은 사람들 고생한다면서 ㅎㅎ",
          "original_form": "젊은 사람들 고생한다면서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.28",
          "form": "그래도 그분 되게 감사했겠어요",
          "original_form": "그래도 그분 되게 감사했겠어요",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.29",
          "form": "친구가 무릎 꿇었는데",
          "original_form": "친구가 무릎 꿇었는데",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.30",
          "form": "괜찮다고 일어나라구",
          "original_form": "괜찮다고 일어나라구",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.31",
          "form": "헉.. 무릎까지",
          "original_form": "헉.. 무릎까지",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017148.1.32",
          "form": "그래주셔서 진짜 눈물났어요",
          "original_form": "그래주셔서 진짜 눈물났어요",
          "speaker_id": "2",
          "time": "20210826 17:39"
        }
      ]
    }
