      "utterance": [
        {
          "id": "MDRW2100029790.1.1",
          "form": "현장직사람인데월화출근했다그러더라고",
          "original_form": "현장직사람인데월화출근했다그러더라고",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.2",
          "form": "격리해야하는거아닌가",
          "original_form": "격리해야하는거아닌가",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.3",
          "form": "ㅇㅇ대화도없었고",
          "original_form": "ㅇㅇ대화도없었고",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.4",
          "form": "아니 상호마스크쓰면",
          "original_form": "아니 상호마스크쓰면",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.5",
          "form": "근데일단검사받으러가는중",
          "original_form": "근데일단검사받으러가는중",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.6",
          "form": "괜찮은거아닐가.....",
          "original_form": "괜찮은거아닐가.....",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.7",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.8",
          "form": "그래도모르는거니",
          "original_form": "그래도모르는거니",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029790.1.9",
          "form": "동생그러면 집에서 어케격리해",
          "original_form": "동생그러면 집에서 어케격리해",
          "speaker_id": "1",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029790.1.10",
          "form": "방에읶는거지뭐",
          "original_form": "방에읶는거지뭐",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029790.1.11",
          "form": "ㅠㅠ힘들겠네 으으",
          "original_form": "ㅠㅠ힘들겠네 으으",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029790.1.12",
          "form": "뭐내일검사결과나올때까지만통조림하면될테니까....",
          "original_form": "뭐내일검사결과나올때까지만통조림하면될테니까....",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100029790.1.13",
          "form": "그것도갑갑하긴한데",
          "original_form": "그것도갑갑하긴한데",
          "speaker_id": "2",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029790.1.14",
          "form": "ㅜㅜ고생이네",
          "original_form": "ㅜㅜ고생이네",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029790.1.15",
          "form": "밀접아니라2주안하는게다행이지....",
          "original_form": "밀접아니라2주안하는게다행이지....",
          "speaker_id": "2",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029790.1.16",
          "form": "어머님 또 신경쓰이시겠군",
          "original_form": "어머님 또 신경쓰이시겠군",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029790.1.17",
          "form": "2주어케버티냐",
          "original_form": "2주어케버티냐",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029790.1.18",
          "form": "하루도갑갑한데",
          "original_form": "하루도갑갑한데",
          "speaker_id": "2",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029790.1.19",
          "form": "2주....개지옥",
          "original_form": "2주....개지옥",
          "speaker_id": "2",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029790.1.20",
          "form": "ㄹㅇ 저하루하는것ㄷ괴로왔오",
          "original_form": "ㄹㅇ 저하루하는것ㄷ괴로왔오",
          "speaker_id": "1",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029790.1.21",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
