      "utterance": [
        {
          "id": "MDRW2100023435.1.1",
          "form": "그쳐ㅋㅋㅋ",
          "original_form": "그쳐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:26"
        },
        {
          "id": "MDRW2100023435.1.2",
          "form": "여행 다녀오신적 있으세용?",
          "original_form": "여행 다녀오신적 있으세용?",
          "speaker_id": "1",
          "time": "20210830 02:26"
        },
        {
          "id": "MDRW2100023435.1.3",
          "form": "이번 여름에 속초 갔다왔어요",
          "original_form": "이번 여름에 속초 갔다왔어요",
          "speaker_id": "2",
          "time": "20210830 02:27"
        },
        {
          "id": "MDRW2100023435.1.4",
          "form": "우와아아",
          "original_form": "우와아아",
          "speaker_id": "1",
          "time": "20210830 02:27"
        },
        {
          "id": "MDRW2100023435.1.5",
          "form": "가서 뭐 드셨어요?",
          "original_form": "가서 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 02:27"
        },
        {
          "id": "MDRW2100023435.1.6",
          "form": "물회??",
          "original_form": "물회??",
          "speaker_id": "1",
          "time": "20210830 02:27"
        },
        {
          "id": "MDRW2100023435.1.7",
          "form": "헉 맞아요!",
          "original_form": "헉 맞아요!",
          "speaker_id": "2",
          "time": "20210830 02:27"
        },
        {
          "id": "MDRW2100023435.1.8",
          "form": "거기 유명한곳 있더라구요",
          "original_form": "거기 유명한곳 있더라구요",
          "speaker_id": "2",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.9",
          "form": "속초에 물회로 유명한 집 있는데",
          "original_form": "속초에 물회로 유명한 집 있는데",
          "speaker_id": "1",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.10",
          "form": "헉헉",
          "original_form": "헉헉",
          "speaker_id": "2",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.11",
          "form": "건물하나가물회집이져?",
          "original_form": "건물하나가물회집이져?",
          "speaker_id": "1",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.12",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.13",
          "form": "근처인가 옆에 또 무슨 유명한 빵집있구",
          "original_form": "근처인가 옆에 또 무슨 유명한 빵집있구",
          "speaker_id": "1",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.14",
          "form": "대기 겁나 길었는데 쑥쑥 빠져나가서 좋았어요",
          "original_form": "대기 겁나 길었는데 쑥쑥 빠져나가서 좋았어요",
          "speaker_id": "2",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.15",
          "form": "기억이 가물가물하네요ㅋㅋㅋ",
          "original_form": "기억이 가물가물하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.16",
          "form": "저도 거기가봤어요!",
          "original_form": "저도 거기가봤어요!",
          "speaker_id": "1",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.17",
          "form": "오ㅎㅎ역시 맛집이네요",
          "original_form": "오ㅎㅎ역시 맛집이네요",
          "speaker_id": "2",
          "time": "20210830 02:28"
        },
        {
          "id": "MDRW2100023435.1.18",
          "form": "속초에서 신나게 놀고",
          "original_form": "속초에서 신나게 놀고",
          "speaker_id": "1",
          "time": "20210830 02:29"
        },
        {
          "id": "MDRW2100023435.1.19",
          "form": "마지막날에ㅋㅋㅋ",
          "original_form": "마지막날에ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:29"
        },
        {
          "id": "MDRW2100023435.1.20",
          "form": "해외여행은 가보셨어용??",
          "original_form": "해외여행은 가보셨어용??",
          "speaker_id": "1",
          "time": "20210830 02:29"
        },
        {
          "id": "MDRW2100023435.1.21",
          "form": "네 재작년?에 일본 갔다왓어요",
          "original_form": "네 재작년?에 일본 갔다왓어요",
          "speaker_id": "2",
          "time": "20210830 02:29"
        },
        {
          "id": "MDRW2100023435.1.22",
          "form": "저도 19년도에 일본 다녀왔어요!ㅎㅎ",
          "original_form": "저도 19년도에 일본 다녀왔어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:30"
        },
        {
          "id": "MDRW2100023435.1.23",
          "form": "어디가셨어용?",
          "original_form": "어디가셨어용?",
          "speaker_id": "1",
          "time": "20210830 02:30"
        }
      ]
    }
