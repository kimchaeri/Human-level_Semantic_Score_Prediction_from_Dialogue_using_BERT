      "utterance": [
        {
          "id": "MDRW2100015771.1.1",
          "form": "오늘 뭐 드셨나용",
          "original_form": "오늘 뭐 드셨나용",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015771.1.2",
          "form": "오늘 저녁에 치킨 먹었습니당 ㅎㅎ",
          "original_form": "오늘 저녁에 치킨 먹었습니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015771.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015771.1.4",
          "form": "맛있으셨겠당 ㅎㅎ",
          "original_form": "맛있으셨겠당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015771.1.5",
          "form": "치킨 좋아하시나요?",
          "original_form": "치킨 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015771.1.6",
          "form": "네네네 당연하죠",
          "original_form": "네네네 당연하죠",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015771.1.7",
          "form": "어느 브랜드 좋아하세요?",
          "original_form": "어느 브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015771.1.8",
          "form": "제 bhc 뿌링클이요",
          "original_form": "제 bhc 뿌링클이요",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015771.1.9",
          "form": "진리죠",
          "original_form": "진리죠",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015771.1.10",
          "form": "크 맞아요 ㅋㅋㅋㅋ근데 비에이치씨 후라이드도 진짜 맛있어요",
          "original_form": "크 맞아요 ㅋㅋㅋㅋ근데 비에이치씨 후라이드도 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015771.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015771.1.12",
          "form": "기본의 소중함을 아시는 분이군요",
          "original_form": "기본의 소중함을 아시는 분이군요",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015771.1.13",
          "form": "바삭...바삭...",
          "original_form": "바삭...바삭...",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015771.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ네 튀김을 잘하더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네 튀김을 잘하더라구요",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015771.1.15",
          "form": "바른치킨 드셔보셧어요?",
          "original_form": "바른치킨 드셔보셧어요?",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015771.1.16",
          "form": "매운거좋아하시면 추천...",
          "original_form": "매운거좋아하시면 추천...",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015771.1.17",
          "form": "오 처음 들어요",
          "original_form": "오 처음 들어요",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015771.1.18",
          "form": "바른..치킨...메모.....",
          "original_form": "바른..치킨...메모.....",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015771.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ피피엘하는거 보고 비쥬얼땜에 궁금해서 먹어봣어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ피피엘하는거 보고 비쥬얼땜에 궁금해서 먹어봣어요",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015771.1.20",
          "form": "요즘 피피엘만 봐도 그냥 먹고 싶어지는..",
          "original_form": "요즘 피피엘만 봐도 그냥 먹고 싶어지는..",
          "speaker_id": "1",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100015771.1.21",
          "form": "그냥 제가 식욕이 폭발한 거겠죠",
          "original_form": "그냥 제가 식욕이 폭발한 거겠죠",
          "speaker_id": "1",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100015771.1.22",
          "form": "ㅋㅋㅋㅋㅋ원래 그런거 아니겟어요",
          "original_form": "ㅋㅋㅋㅋㅋ원래 그런거 아니겟어요",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015771.1.23",
          "form": "아 배달음식중에 젤 좋아하는게 뭐에여",
          "original_form": "아 배달음식중에 젤 좋아하는게 뭐에여",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015771.1.24",
          "form": "역시 치킨이죠",
          "original_form": "역시 치킨이죠",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015771.1.25",
          "form": "혹은 맥도날드 or 버거킹~?",
          "original_form": "혹은 맥도날드 or 버거킹~?",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015771.1.26",
          "form": "오 맘터는 안좋아하세요?",
          "original_form": "오 맘터는 안좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015771.1.27",
          "form": "맘터 좋아하죠!",
          "original_form": "맘터 좋아하죠!",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015771.1.28",
          "form": "근데 딜리버리가 더 잘되어있어서",
          "original_form": "근데 딜리버리가 더 잘되어있어서",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015771.1.29",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015771.1.30",
          "form": "싸이버거가 아주 치킨 같고 좋습니다",
          "original_form": "싸이버거가 아주 치킨 같고 좋습니다",
          "speaker_id": "1",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015771.1.31",
          "form": "ㅋㅋㅋㅋ맞아요 딜리버리는 쟤들이 잘되잇어용",
          "original_form": "ㅋㅋㅋㅋ맞아요 딜리버리는 쟤들이 잘되잇어용",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015771.1.32",
          "form": "저는 써브웨이 좋아해요",
          "original_form": "저는 써브웨이 좋아해요",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015771.1.33",
          "form": "오 레시피 추천 가능하신가요????",
          "original_form": "오 레시피 추천 가능하신가요????",
          "speaker_id": "1",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015771.1.34",
          "form": "음..저는 좋아하는 소스조합으로 먹어서",
          "original_form": "음..저는 좋아하는 소스조합으로 먹어서",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015771.1.35",
          "form": "스위트칠리 스위트어니언 랜치 조합 좋아요!",
          "original_form": "스위트칠리 스위트어니언 랜치 조합 좋아요!",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015771.1.36",
          "form": "오 스위트 어니언 저도 좋아하는데 랜치랑 조합 좋죠 ㅋㅋㅋ",
          "original_form": "오 스위트 어니언 저도 좋아하는데 랜치랑 조합 좋죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015771.1.37",
          "form": "네 맞아요 ㅋㅋㅋㅋ가끔 다여트할땐 올리브오일 후추 스위트어니언으로 먹어여",
          "original_form": "네 맞아요 ㅋㅋㅋㅋ가끔 다여트할땐 올리브오일 후추 스위트어니언으로 먹어여",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015771.1.38",
          "form": "전 후추 하면 뭔가 아쉬울 것 같아서 후추로 한번도 안먹어봤어요!! 어때요?",
          "original_form": "전 후추 하면 뭔가 아쉬울 것 같아서 후추로 한번도 안먹어봤어요!! 어때요?",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015771.1.39",
          "form": "저도 그럴거같앗는데 생각보다 후추맛집이더라구요..? ㅋㅋㅋㅋ잘어울렸어요",
          "original_form": "저도 그럴거같앗는데 생각보다 후추맛집이더라구요..? ㅋㅋㅋㅋ잘어울렸어요",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015771.1.40",
          "form": "오 후추 맛집이구나.....!!!!",
          "original_form": "오 후추 맛집이구나.....!!!!",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015771.1.41",
          "form": "네 맛이 괜찮앗어요 ㅋㅋㅋㅋ",
          "original_form": "네 맛이 괜찮앗어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015771.1.42",
          "form": "서브웨이 뭔가 건강하면서 내 입맛도 채워줘서 좋아요",
          "original_form": "서브웨이 뭔가 건강하면서 내 입맛도 채워줘서 좋아요",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015771.1.43",
          "form": "맞아요 ㅠㅠ종류도 다양하고",
          "original_form": "맞아요 ㅠㅠ종류도 다양하고",
