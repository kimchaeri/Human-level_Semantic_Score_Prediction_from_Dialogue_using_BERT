      "utterance": [
        {
          "id": "MDRW2100043383.1.1",
          "form": "연애는 몇번 해봤어?",
          "original_form": "연애는 몇번 해봤어?",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100043383.1.2",
          "form": "잘 기억이 안 나 한 세네번 정도...?",
          "original_form": "잘 기억이 안 나 한 세네번 정도...?",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100043383.1.3",
          "form": "첫번째 애는 누구였는데",
          "original_form": "첫번째 애는 누구였는데",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043383.1.4",
          "form": "걔는 고등학교때 친구",
          "original_form": "걔는 고등학교때 친구",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043383.1.5",
          "form": "괜찮은 애였어?",
          "original_form": "괜찮은 애였어?",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043383.1.6",
          "form": "공부를 잘했었지 운동도 좋아하고",
          "original_form": "공부를 잘했었지 운동도 좋아하고",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043383.1.7",
          "form": "착했어?",
          "original_form": "착했어?",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043383.1.8",
          "form": "완전 범생이였어",
          "original_form": "완전 범생이였어",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043383.1.9",
          "form": "그럼 재미없었겄네",
          "original_form": "그럼 재미없었겄네",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043383.1.10",
          "form": "아니야 걔가 말솜씨가 좋아가지고",
          "original_form": "아니야 걔가 말솜씨가 좋아가지고",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043383.1.11",
          "form": "어쩌다 헤어졌어?",
          "original_form": "어쩌다 헤어졌어?",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043383.1.12",
          "form": "ㅋㅋ 걔가 미국으로 유학갔어",
          "original_form": "ㅋㅋ 걔가 미국으로 유학갔어",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043383.1.13",
          "form": "이야 지금도 거기 있대?",
          "original_form": "이야 지금도 거기 있대?",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043383.1.14",
          "form": "몰라 이제 연락안해",
          "original_form": "몰라 이제 연락안해",
          "speaker_id": "1",
          "time": "20210907 18:38"
        }
      ]
