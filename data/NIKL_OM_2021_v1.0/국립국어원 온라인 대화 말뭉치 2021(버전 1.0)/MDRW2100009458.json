      "utterance": [
        {
          "id": "MDRW2100009458.1.1",
          "form": "가사 및 가족이래",
          "original_form": "가사 및 가족이래",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009458.1.2",
          "form": "우리 행복한 육퇴 이후 시간",
          "original_form": "우리 행복한 육퇴 이후 시간",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009458.1.3",
          "form": "소중...",
          "original_form": "소중...",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009458.1.4",
          "form": "소중한 육퇴시간인데 가사를 되돌아봐야한다니",
          "original_form": "소중한 육퇴시간인데 가사를 되돌아봐야한다니",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009458.1.5",
          "form": "난 오늘 가사는 1도 안하였다",
          "original_form": "난 오늘 가사는 1도 안하였다",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009458.1.6",
          "form": "흑흑 애들 열시반에 잔거야?",
          "original_form": "흑흑 애들 열시반에 잔거야?",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009458.1.7",
          "form": "보통 열시반 열한시 이쯤 자",
          "original_form": "보통 열시반 열한시 이쯤 자",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009458.1.8",
          "form": "헌나라의 어린이네. 남편님 오늘 가사당번이었나",
          "original_form": "헌나라의 어린이네. 남편님 오늘 가사당번이었나",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009458.1.9",
          "form": "저녁먹을때 8시쯤 피크로 졸려하다가 저녁먹구 나면 에너지 풀충전해서 두시간 난리남",
          "original_form": "저녁먹을때 8시쯤 피크로 졸려하다가 저녁먹구 나면 에너지 풀충전해서 두시간 난리남",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009458.1.10",
          "form": "아니왜 ㅋㅋㅋㅋ",
          "original_form": "아니왜 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009458.1.11",
          "form": "졸리면 잘 것이지",
          "original_form": "졸리면 잘 것이지",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009458.1.12",
          "form": "뭐랄까.... 자기 아쉬운 마음을 온몸으로 표출하는게 보임",
          "original_form": "뭐랄까.... 자기 아쉬운 마음을 온몸으로 표출하는게 보임",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009458.1.13",
          "form": "나 진짜 8월에 하루도 못쉼",
          "original_form": "나 진짜 8월에 하루도 못쉼",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009458.1.14",
          "form": "어빠가 14~16 다 출근했거덩",
          "original_form": "어빠가 14~16 다 출근했거덩",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009458.1.15",
          "form": "아 눈물..... 여름에 더 바쁜가??",
          "original_form": "아 눈물..... 여름에 더 바쁜가??",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009458.1.16",
          "form": "둘다 너무 힘들었겠다",
          "original_form": "둘다 너무 힘들었겠다",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009458.1.17",
          "form": "7월엔 한가하더니 8월엔 몰아치고",
          "original_form": "7월엔 한가하더니 8월엔 몰아치고",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009458.1.18",
          "form": "그래도 오빠는 7월엔 좀 쉬었단말여",
          "original_form": "그래도 오빠는 7월엔 좀 쉬었단말여",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009458.1.19",
          "form": "난 진짜 6월부터 하루도 못쉼",
          "original_form": "난 진짜 6월부터 하루도 못쉼",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009458.1.20",
          "form": "워킹맘은 진짜 고단하네",
          "original_form": "워킹맘은 진짜 고단하네",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009458.1.21",
          "form": "언제 쉬냐",
          "original_form": "언제 쉬냐",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009458.1.22",
          "form": "평일에 연가내고 쉬어야되는데 아직 눈칫밥먹을 짬밥",
          "original_form": "평일에 연가내고 쉬어야되는데 아직 눈칫밥먹을 짬밥",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009458.1.23",
          "form": "아 맞어 너 연가내고 놀러오겠다 그랬었는데 ㅋㅋㅋㅋ",
          "original_form": "아 맞어 너 연가내고 놀러오겠다 그랬었는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009458.1.24",
          "form": "흑흑 ㅠㅠㅠㅠ",
          "original_form": "흑흑 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009458.1.25",
          "form": "재산점검만 끝마치고 ㅠㅠㅠ",
