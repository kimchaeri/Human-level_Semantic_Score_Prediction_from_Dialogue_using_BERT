      "utterance": [
        {
          "id": "MDRW2100038807.1.1",
          "form": "헉 호불호 갈리나요?",
          "original_form": "헉 호불호 갈리나요?",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038807.1.2",
          "form": "네 ! 싫어하는 사람들도 많아요",
          "original_form": "네 ! 싫어하는 사람들도 많아요",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038807.1.3",
          "form": "너무 상큼하다구..",
          "original_form": "너무 상큼하다구..",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038807.1.4",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038807.1.5",
          "form": "역시 입맛은 다 다른가봐요...",
          "original_form": "역시 입맛은 다 다른가봐요...",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038807.1.6",
          "form": "맞죠..",
          "original_form": "맞죠..",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038807.1.7",
          "form": "민초처럼 취급하던걸요 ?!",
          "original_form": "민초처럼 취급하던걸요 ?!",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038807.1.8",
          "form": "막 민초단 레샤단",
          "original_form": "막 민초단 레샤단",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038807.1.9",
          "form": "헐 그정도에요??!!",
          "original_form": "헐 그정도에요??!!",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038807.1.10",
          "form": "충격...",
          "original_form": "충격...",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.11",
          "form": "레샤가...",
          "original_form": "레샤가...",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.12",
          "form": "맞죠...",
          "original_form": "맞죠...",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.13",
          "form": "전 체리쥬빌레는",
          "original_form": "전 체리쥬빌레는",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.14",
          "form": "이해하는덴",
          "original_form": "이해하는덴",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.15",
          "form": "레샤는 ! 뭔가 다 좋아할 맛이여서 ..",
          "original_form": "레샤는 ! 뭔가 다 좋아할 맛이여서 ..",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.16",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.17",
          "form": "민초는 좋아하시나요??",
          "original_form": "민초는 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038807.1.18",
          "form": "네 ! 전 가끔 끼워서 사먹어요 ㅎㅎㅎ",
          "original_form": "네 ! 전 가끔 끼워서 사먹어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100038807.1.19",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100038807.1.20",
          "form": "저는 민초는 안먹어요ㅎㅎ",
          "original_form": "저는 민초는 안먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038807.1.21",
          "form": "헐 진짜요 ??",
          "original_form": "헐 진짜요 ??",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038807.1.22",
          "form": "나쁘진않은데...",
          "original_form": "나쁘진않은데...",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038807.1.23",
          "form": "굳이 사먹고싶지가 않아서ㅎㅎ",
          "original_form": "굳이 사먹고싶지가 않아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038807.1.24",
          "form": "아 하긴 ㅎㅎ 그리고 민초는",
          "original_form": "아 하긴 ㅎㅎ 그리고 민초는",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038807.1.25",
          "form": "같이 담으면",
          "original_form": "같이 담으면",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038807.1.26",
          "form": "바로 옆 아스크림에도 민초맛 나욬ㅋㅋ",
          "original_form": "바로 옆 아스크림에도 민초맛 나욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038807.1.27",
          "form": "맛이 잘기억 안나서 담에 친구가 먹으면 뺏어먹어야겠어요ㅎㅎ",
          "original_form": "맛이 잘기억 안나서 담에 친구가 먹으면 뺏어먹어야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:42"
        }
      ]
