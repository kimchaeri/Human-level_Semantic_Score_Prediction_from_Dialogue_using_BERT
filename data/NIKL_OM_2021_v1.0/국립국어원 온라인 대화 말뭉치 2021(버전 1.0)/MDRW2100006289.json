      "utterance": [
        {
          "id": "MDRW2100006289.1.1",
          "form": "즐겨보는 프로그램 있으신가요",
          "original_form": "즐겨보는 프로그램 있으신가요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006289.1.2",
          "form": "예전에",
          "original_form": "예전에",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006289.1.3",
          "form": "무한도전은",
          "original_form": "무한도전은",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006289.1.4",
          "form": "누차례 돌려보고 그랬는데",
          "original_form": "누차례 돌려보고 그랬는데",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006289.1.5",
          "form": "요즘은 따로 없네요",
          "original_form": "요즘은 따로 없네요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006289.1.6",
          "form": "놀면뭐하니는 안보시나요",
          "original_form": "놀면뭐하니는 안보시나요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006289.1.7",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006289.1.8",
          "form": "다만",
          "original_form": "다만",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006289.1.9",
          "form": "본방 챙겨보진 않아요",
          "original_form": "본방 챙겨보진 않아요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006289.1.10",
          "form": "유튜브로는 보는데",
          "original_form": "유튜브로는 보는데",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006289.1.11",
          "form": "그렇군요 저도 드문드문만 보네요\r\n유투브는요?",
          "original_form": "그렇군요 저도 드문드문만 보네요\r\n유투브는요?",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006289.1.12",
          "form": "승우 아빠라던가",
          "original_form": "승우 아빠라던가",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006289.1.13",
          "form": "백종원이라던가",
          "original_form": "백종원이라던가",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006289.1.14",
          "form": "이런거 보고",
          "original_form": "이런거 보고",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006289.1.15",
          "form": "밥 먹을땐",
          "original_form": "밥 먹을땐",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006289.1.16",
          "form": "무도 옛날 하이라이트...보네요",
          "original_form": "무도 옛날 하이라이트...보네요",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006289.1.17",
          "form": "승우아빠는 채널 이름인가요?",
          "original_form": "승우아빠는 채널 이름인가요?",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006289.1.18",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006289.1.19",
          "form": "채널명이에요",
          "original_form": "채널명이에요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006289.1.20",
          "form": "어떤 내용 채널인가요?",
          "original_form": "어떤 내용 채널인가요?",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006289.1.21",
          "form": "요리사에요",
          "original_form": "요리사에요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006289.1.22",
          "form": "요리사",
          "original_form": "요리사",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006289.1.23",
          "form": "아 전직 요리사",
          "original_form": "아 전직 요리사",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006289.1.24",
          "form": "막 새우깡을 새우에서부터 만들고 그러는 채널 있어요",
          "original_form": "막 새우깡을 새우에서부터 만들고 그러는 채널 있어요",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006289.1.25",
          "form": "헉...새우부터요??대박이네요...",
          "original_form": "헉...새우부터요??대박이네요...",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006289.1.26",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006289.1.27",
          "form": "그러다보니",
          "original_form": "그러다보니",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006289.1.28",
          "form": "요리 보려 봤던건데",
          "original_form": "요리 보려 봤던건데",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006289.1.29",
          "form": "그냥 재밌게...",
          "original_form": "그냥 재밌게...",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006289.1.30",
          "form": "ㅎㅎ 해보긴 어렵겠어요",
          "original_form": "ㅎㅎ 해보긴 어렵겠어요",
          "speaker_id": "1",
          "time": "20210907 22:49"
        }
      ]
    }
  ]
