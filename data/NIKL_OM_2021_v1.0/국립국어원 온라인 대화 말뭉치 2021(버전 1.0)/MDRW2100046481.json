      "utterance": [
        {
          "id": "MDRW2100046481.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211025 17:10"
        },
        {
          "id": "MDRW2100046481.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211025 17:11"
        },
        {
          "id": "MDRW2100046481.1.3",
          "form": "혹시 글램핑 가보셨나요?",
          "original_form": "혹시 글램핑 가보셨나요?",
          "speaker_id": "2",
          "time": "20211025 17:12"
        },
        {
          "id": "MDRW2100046481.1.4",
          "form": "네 가봤습니다",
          "original_form": "네 가봤습니다",
          "speaker_id": "1",
          "time": "20211025 17:12"
        },
        {
          "id": "MDRW2100046481.1.5",
          "form": "지금 날씨는 조금 춥지만 글램핑 분위기 있죠",
          "original_form": "지금 날씨는 조금 춥지만 글램핑 분위기 있죠",
          "speaker_id": "1",
          "time": "20211025 17:12"
        },
        {
          "id": "MDRW2100046481.1.6",
          "form": "호오",
          "original_form": "호오",
          "speaker_id": "2",
          "time": "20211025 17:12"
        },
        {
          "id": "MDRW2100046481.1.7",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20211025 17:12"
        },
        {
          "id": "MDRW2100046481.1.8",
          "form": "글램핑은 춥고 비올 때 가는게",
          "original_form": "글램핑은 춥고 비올 때 가는게",
          "speaker_id": "2",
          "time": "20211025 17:12"
        },
        {
          "id": "MDRW2100046481.1.9",
          "form": "제일 좋은거같아요...",
          "original_form": "제일 좋은거같아요...",
          "speaker_id": "2",
          "time": "20211025 17:12"
        },
        {
          "id": "MDRW2100046481.1.10",
          "form": "그 분위기가 진짜 ㅜㅜ",
          "original_form": "그 분위기가 진짜 ㅜㅜ",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.11",
          "form": "맞아요 그맛에 술한잔 하면서..",
          "original_form": "맞아요 그맛에 술한잔 하면서..",
          "speaker_id": "1",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.12",
          "form": "크아아아",
          "original_form": "크아아아",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.13",
          "form": "캠핑은 솔직히",
          "original_form": "캠핑은 솔직히",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.14",
          "form": "좀 귀찮은데",
          "original_form": "좀 귀찮은데",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.15",
          "form": "글램핑은 몸은 편하게",
          "original_form": "글램핑은 몸은 편하게",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.16",
          "form": "캠핑 분위기 낼 수 있어서",
          "original_form": "캠핑 분위기 낼 수 있어서",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.17",
          "form": "빗소리 들으며 술한잔 먹다보면 취하는줄도 모르겠구요",
          "original_form": "빗소리 들으며 술한잔 먹다보면 취하는줄도 모르겠구요",
          "speaker_id": "1",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.18",
          "form": "너무 좋은거같아요",
          "original_form": "너무 좋은거같아요",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.19",
          "form": "와 진짜요!",
          "original_form": "와 진짜요!",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.20",
          "form": "글램핑가서",
          "original_form": "글램핑가서",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.21",
          "form": "술안먹는사람들",
          "original_form": "술안먹는사람들",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.22",
          "form": "유죄",
          "original_form": "유죄",
          "speaker_id": "2",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.23",
          "form": "맞아요 캠핑은 정리하는데만..",
          "original_form": "맞아요 캠핑은 정리하는데만..",
          "speaker_id": "1",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.24",
          "form": "ㅍㅎㅎ그렇죠",
          "original_form": "ㅍㅎㅎ그렇죠",
          "speaker_id": "1",
          "time": "20211025 17:13"
        },
        {
          "id": "MDRW2100046481.1.25",
          "form": "ㅎㅎㅎㅎ 맞아요",
          "original_form": "ㅎㅎㅎㅎ 맞아요",
          "speaker_id": "2",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.26",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.27",
          "form": "술 못먹는 사람도 한잔씩 쭉쭉 들어가구요",
          "original_form": "술 못먹는 사람도 한잔씩 쭉쭉 들어가구요",
          "speaker_id": "1",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.28",
          "form": "글램핑 명소 추천해주실수있나용",
          "original_form": "글램핑 명소 추천해주실수있나용",
          "speaker_id": "2",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.29",
          "form": "ㅎㅎㅎ마자요",
          "original_form": "ㅎㅎㅎ마자요",
          "speaker_id": "2",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.30",
          "form": "힘들면 옆에서 자면되니까",
          "original_form": "힘들면 옆에서 자면되니까",
          "speaker_id": "2",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.31",
          "form": "챙길필요두없구",
          "original_form": "챙길필요두없구",
          "speaker_id": "2",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.32",
          "form": "굿입니다",
          "original_form": "굿입니다",
          "speaker_id": "2",
          "time": "20211025 17:14"
        },
        {
          "id": "MDRW2100046481.1.33",
          "form": "가평 명지산 카라반 글램핑 가보셨나요?",
          "original_form": "가평 명지산 카라반 글램핑 가보셨나요?",
          "speaker_id": "1",
          "time": "20211025 17:15"
        },
        {
          "id": "MDRW2100046481.1.34",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20211025 17:15"
        },
        {
          "id": "MDRW2100046481.1.35",
          "form": "아니요ㅠ",
          "original_form": "아니요ㅠ",
          "speaker_id": "2",
          "time": "20211025 17:15"
        },
        {
          "id": "MDRW2100046481.1.36",
          "form": "분위기가 너무좋아요",
          "original_form": "분위기가 너무좋아요",
          "speaker_id": "1",
          "time": "20211025 17:15"
        },
        {
          "id": "MDRW2100046481.1.37",
          "form": "꼭 가볼게요..... 메모메모",
          "original_form": "꼭 가볼게요..... 메모메모",
          "speaker_id": "2",
          "time": "20211025 17:15"
        },
        {
          "id": "MDRW2100046481.1.38",
          "form": "추천 감사합니당 ㅎㅎ\r\n",
          "original_form": "추천 감사합니당 ㅎㅎ\r\n",
          "speaker_id": "2",
          "time": "20211025 17:15"
        }
      ]
