      "utterance": [
        {
          "id": "MDRW2100037783.1.1",
          "form": "안녕하세요! 아직요 ㅎㅎ 저녁식사 하셨나요?",
          "original_form": "안녕하세요! 아직요 ㅎㅎ 저녁식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100037783.1.2",
          "form": "저도아직안했어요ㅠ",
          "original_form": "저도아직안했어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037783.1.3",
          "form": "뭐드실예정이세요?!",
          "original_form": "뭐드실예정이세요?!",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037783.1.4",
          "form": "점심을 넘 늦게 먹어서 아직 생각이 없어요 ㅎㅎ 점심은 뭐 드셨어요?",
          "original_form": "점심을 넘 늦게 먹어서 아직 생각이 없어요 ㅎㅎ 점심은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037783.1.5",
          "form": "점심에 오동통면먹었어요!",
          "original_form": "점심에 오동통면먹었어요!",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037783.1.6",
          "form": "라면좋아하세요?!",
          "original_form": "라면좋아하세요?!",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037783.1.7",
          "form": "앗 전 짬뽕 먹었어요!! ㅋㅋ",
          "original_form": "앗 전 짬뽕 먹었어요!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037783.1.8",
          "form": "라면 좋아합니다 ㅎㅎ 요즘은 진라면 많이 먹어요",
          "original_form": "라면 좋아합니다 ㅎㅎ 요즘은 진라면 많이 먹어요",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037783.1.9",
          "form": "아 짬뽕 최고죠",
          "original_form": "아 짬뽕 최고죠",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037783.1.10",
          "form": "꿔바로우먹고싶네요,,,",
          "original_form": "꿔바로우먹고싶네요,,,",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037783.1.11",
          "form": "진매드세요? 진순드세요?",
          "original_form": "진매드세요? 진순드세요?",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037783.1.12",
          "form": "헉 꿔바로우 땡기네오 ㅠㅠ",
          "original_form": "헉 꿔바로우 땡기네오 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037783.1.13",
          "form": "역시!!",
          "original_form": "역시!!",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037783.1.14",
          "form": "진순이요!! ㅎㅎㅎ 맵찔이라서...",
          "original_form": "진순이요!! ㅎㅎㅎ 맵찔이라서...",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037783.1.15",
          "form": "저도맵찔!!",
          "original_form": "저도맵찔!!",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037783.1.16",
          "form": "전 오늘 비와서 곱창전골도 땡겨요",
          "original_form": "전 오늘 비와서 곱창전골도 땡겨요",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037783.1.17",
          "form": "최고죠 곱전골",
          "original_form": "최고죠 곱전골",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037783.1.18",
          "form": "아 얼큰",
          "original_form": "아 얼큰",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037783.1.19",
          "form": "너무비싸요근데",
          "original_form": "너무비싸요근데",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037783.1.20",
          "form": "맞아요...진짜 맘먹고 먹어야하는...",
