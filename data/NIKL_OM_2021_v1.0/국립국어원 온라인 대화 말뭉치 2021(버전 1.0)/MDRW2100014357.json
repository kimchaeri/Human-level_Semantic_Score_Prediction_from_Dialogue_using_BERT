      "utterance": [
        {
          "id": "MDRW2100014357.1.1",
          "form": "샤인머스켓 좋아하시나봐요",
          "original_form": "샤인머스켓 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100014357.1.2",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100014357.1.3",
          "form": "맛있더라구요 ㄹㄹ",
          "original_form": "맛있더라구요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100014357.1.4",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100014357.1.5",
          "form": "ㅋㅋㅋ 마자요 근데 넘 비싸요ㅜㅜ",
          "original_form": "ㅋㅋㅋ 마자요 근데 넘 비싸요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100014357.1.6",
          "form": "네 가격이 후덜덜하더라구요 십년쯤 있으면 싸지려나요",
          "original_form": "네 가격이 후덜덜하더라구요 십년쯤 있으면 싸지려나요",
          "speaker_id": "1",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100014357.1.7",
          "form": "ㅋㅋㅋㅋ 구래도 예전보단 싸졋어요 예전에는 한송이에 삼만원씩햇던것같은데..",
          "original_form": "ㅋㅋㅋㅋ 구래도 예전보단 싸졋어요 예전에는 한송이에 삼만원씩햇던것같은데..",
          "speaker_id": "2",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100014357.1.8",
          "form": "그렇군요 그나마 싸진거네요",
          "original_form": "그렇군요 그나마 싸진거네요",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100014357.1.9",
          "form": "수입포도 신기한거 많더라구요",
          "original_form": "수입포도 신기한거 많더라구요",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100014357.1.10",
          "form": "그래요? 뭐가 잇을가오?",
          "original_form": "그래요? 뭐가 잇을가오?",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100014357.1.11",
          "form": "이름까먹었는데",
          "original_form": "이름까먹었는데",
          "speaker_id": "1",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100014357.1.12",
          "form": "까맣고 가지처럼 길쭉한 모양도있고",
          "original_form": "까맣고 가지처럼 길쭉한 모양도있고",
          "speaker_id": "1",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100014357.1.13",
          "form": "캔디하트포도인가 하는거 봤어요",
          "original_form": "캔디하트포도인가 하는거 봤어요",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100014357.1.14",
          "form": "헐... 캔디하트포도는 첨들업하요 ㅋㅋㅋㅋ",
          "original_form": "헐... 캔디하트포도는 첨들업하요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100014357.1.15",
          "form": "블랙사파이어포도라고 길쭉한 모영은 많이 보이더라구요",
          "original_form": "블랙사파이어포도라고 길쭉한 모영은 많이 보이더라구요",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100014357.1.16",
          "form": "아 블랙사파이어포도인가봐요",
          "original_form": "아 블랙사파이어포도인가봐요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100014357.1.17",
          "form": "길쭉한거요",
          "original_form": "길쭉한거요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100014357.1.18",
          "form": "그런곳같아요 ㅎㅎ",
          "original_form": "그런곳같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100014357.1.19",
          "form": "블랙~ 드셔보셨어요?",
          "original_form": "블랙~ 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:18"
        }
