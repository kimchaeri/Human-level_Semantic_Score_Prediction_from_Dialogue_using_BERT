      "utterance": [
        {
          "id": "MDRW2100006359.1.1",
          "form": "지금 티비보시나요?",
          "original_form": "지금 티비보시나요?",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006359.1.2",
          "form": "지금 해방타운보는데",
          "original_form": "지금 해방타운보는데",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006359.1.3",
          "form": "최영재? 멋있네요ㅎㅎ",
          "original_form": "최영재? 멋있네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.4",
          "form": "전 옥탑방이요",
          "original_form": "전 옥탑방이요",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.5",
          "form": "마스터? 그사람이죠?",
          "original_form": "마스터? 그사람이죠?",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.6",
          "form": "잘생기고 몸도 좋고ㅎ 결혼안할줄알았는데",
          "original_form": "잘생기고 몸도 좋고ㅎ 결혼안할줄알았는데",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.7",
          "form": "지금 미용하더라구요",
          "original_form": "지금 미용하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.8",
          "form": "10년연애하고 결혼해서 딸둘",
          "original_form": "10년연애하고 결혼해서 딸둘",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.9",
          "form": "진짜 의외였어요",
          "original_form": "진짜 의외였어요",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.10",
          "form": "별거다하던데요ㅎ",
          "original_form": "별거다하던데요ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.11",
          "form": "딸 머리해줄려고 땄대요",
          "original_form": "딸 머리해줄려고 땄대요",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006359.1.12",
          "form": "ㅎ  멋있는 아빠네요",
          "original_form": "ㅎ  멋있는 아빠네요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006359.1.13",
          "form": "지금 키즈카페도 운영하고 미용실도 하는건지",
          "original_form": "지금 키즈카페도 운영하고 미용실도 하는건지",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006359.1.14",
          "form": "와~~ 키즈카페도하는지는 몰랐네요",
          "original_form": "와~~ 키즈카페도하는지는 몰랐네요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006359.1.15",
          "form": "옥탑방 누구나와요?",
          "original_form": "옥탑방 누구나와요?",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006359.1.16",
          "form": "지석진이요",
          "original_form": "지석진이요",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006359.1.17",
          "form": "옥탑방도 재밌죠?",
          "original_form": "옥탑방도 재밌죠?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006359.1.18",
          "form": "네.  자주보는 프로중하나요",
          "original_form": "네.  자주보는 프로중하나요",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006359.1.19",
          "form": "요즘 뭉쳐야찬다도 잼나요ㅎㅎ",
          "original_form": "요즘 뭉쳐야찬다도 잼나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006359.1.20",
          "form": "그것도 잘봐요",
          "original_form": "그것도 잘봐요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        }
      ]
