      "utterance": [
        {
          "id": "MDRW2100006149.1.1",
          "form": "다이어트하시면서 생각나는 음식 있나요",
          "original_form": "다이어트하시면서 생각나는 음식 있나요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006149.1.2",
          "form": "요리 하시나요?",
          "original_form": "요리 하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006149.1.3",
          "form": "요리 좋아해영 name1님은요?",
          "original_form": "요리 좋아해영 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006149.1.4",
          "form": "저는 가끔요...즐겨하는 레시피있나요",
          "original_form": "저는 가끔요...즐겨하는 레시피있나요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006149.1.5",
          "form": "거의 조리수준이긴한데...",
          "original_form": "거의 조리수준이긴한데...",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006149.1.6",
          "form": "요즘엔 당근라페 자주 만들어먹었네요",
          "original_form": "요즘엔 당근라페 자주 만들어먹었네요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006149.1.7",
          "form": "와이프가 최근 몇 번 만들던데 유행인가요..?ㅎㅎ",
          "original_form": "와이프가 최근 몇 번 만들던데 유행인가요..?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006149.1.8",
          "form": "ㅋㅋㅋ네 살짝 유행했던거같아요",
          "original_form": "ㅋㅋㅋ네 살짝 유행했던거같아요",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006149.1.9",
          "form": "맛있지않나요..!?",
          "original_form": "맛있지않나요..!?",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006149.1.10",
          "form": "반찬으론 못먹겠고 샌드위치는 맛있었어요 ㅎㅎ",
          "original_form": "반찬으론 못먹겠고 샌드위치는 맛있었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006149.1.11",
          "form": "샌드위치도 해먹고 김밥도 해먹고",
          "original_form": "샌드위치도 해먹고 김밥도 해먹고",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006149.1.12",
          "form": "은근 여기저기 잘 어울리더라구요ㅋㅋ",
          "original_form": "은근 여기저기 잘 어울리더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006149.1.13",
          "form": "아 김밥은 안먹어봤어요 괜찮던가요?",
          "original_form": "아 김밥은 안먹어봤어요 괜찮던가요?",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006149.1.14",
          "form": "크래미랑 넣고 한번 말아보세요 맛 괜찮아요",
          "original_form": "크래미랑 넣고 한번 말아보세요 맛 괜찮아요",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006149.1.15",
          "form": "조합이 궁금해요 크래미.당근라페 외에 또 넣은것 있나요?",
          "original_form": "조합이 궁금해요 크래미.당근라페 외에 또 넣은것 있나요?",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006149.1.16",
          "form": "깻잎도 넣구 계란지단도 부쳐서 넣었어요ㅎㅎ",
          "original_form": "깻잎도 넣구 계란지단도 부쳐서 넣었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006149.1.17",
          "form": "크래미는 마요에 비벼서 넣으면 더 맛나구영",
          "original_form": "크래미는 마요에 비벼서 넣으면 더 맛나구영",
