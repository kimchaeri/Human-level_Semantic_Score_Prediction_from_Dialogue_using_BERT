      "utterance": [
        {
          "id": "MDRW2100034130.1.1",
          "form": "헉 맞아요 충분한 휴식이 필요하댔어요",
          "original_form": "헉 맞아요 충분한 휴식이 필요하댔어요",
          "speaker_id": "2",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100034130.1.2",
          "form": "전 가벼운 산책까지 괜찮았어요",
          "original_form": "전 가벼운 산책까지 괜찮았어요",
          "speaker_id": "2",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100034130.1.3",
          "form": "모더나 맞으시나요?",
          "original_form": "모더나 맞으시나요?",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100034130.1.4",
          "form": "저는 화이자요",
          "original_form": "저는 화이자요",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100034130.1.5",
          "form": "아하 제 친구는 어제 맞았는데 모더나 맞아서",
          "original_form": "아하 제 친구는 어제 맞았는데 모더나 맞아서",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100034130.1.6",
          "form": "이제 모더나 놓나 싶었어요ㅎㅎ",
          "original_form": "이제 모더나 놓나 싶었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100034130.1.7",
          "form": "아하 이제 모더나도 맞는군요!",
          "original_form": "아하 이제 모더나도 맞는군요!",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034130.1.8",
          "form": "2차는 언제 맞으시나요?",
          "original_form": "2차는 언제 맞으시나요?",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034130.1.9",
          "form": "전 10월 1일이요!",
          "original_form": "전 10월 1일이요!",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034130.1.10",
          "form": "원래 더 전이었는데 늘어났어요ㅠ",
          "original_form": "원래 더 전이었는데 늘어났어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034130.1.11",
          "form": "아 그 물량모지라서 이주정도 늘어났다는데 그것인가요ㅠㅠ",
          "original_form": "아 그 물량모지라서 이주정도 늘어났다는데 그것인가요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034130.1.12",
          "form": "네네 맞아요ㅠ 어차피 그 물량 모더나 물량이던데",
          "original_form": "네네 맞아요ㅠ 어차피 그 물량 모더나 물량이던데",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100034130.1.13",
          "form": "전 모더나 못맞는데 왜 늘어났는지 모르겠어요ㅠ",
          "original_form": "전 모더나 못맞는데 왜 늘어났는지 모르겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100034130.1.14",
          "form": "그래도 그때 맞으라니 맞아야죠 모",
          "original_form": "그래도 그때 맞으라니 맞아야죠 모",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100034130.1.15",
          "form": "있던 화이자라도 나누어야해서 그런가...",
          "original_form": "있던 화이자라도 나누어야해서 그런가...",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100034130.1.16",
          "form": "어쩔수없죠ㅠ",
          "original_form": "어쩔수없죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100034130.1.17",
          "form": "전화해서 일정 조절할수도 있다고는 해요",
          "original_form": "전화해서 일정 조절할수도 있다고는 해요",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100034130.1.18",
          "form": "아 맞아요 근데 5~6주인건 못바꿀 것같기도 하도",
          "original_form": "아 맞아요 근데 5~6주인건 못바꿀 것같기도 하도",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100034130.1.19",
          "form": "어차피 그 날에 아무 일정없어서 괜찮아요ㅎㅎ",
          "original_form": "어차피 그 날에 아무 일정없어서 괜찮아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100034130.1.20",
          "form": "그러시구나 다행이네요!",
          "original_form": "그러시구나 다행이네요!",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100034130.1.21",
          "form": "네넹! 2차는 언제세요?",
          "original_form": "네넹! 2차는 언제세요?",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100034130.1.22",
          "form": "저는 10월12일이요",
          "original_form": "저는 10월12일이요",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100034130.1.23",
          "form": "아항 그러시구나",
          "original_form": "아항 그러시구나",
