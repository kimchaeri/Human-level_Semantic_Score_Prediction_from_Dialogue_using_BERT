      "utterance": [
        {
          "id": "MDRW2100040931.1.1",
          "form": "근데 그렇게 잘 갈려요? 저희 집꺼는 입자들이 크게 갈려요ㅠ",
          "original_form": "근데 그렇게 잘 갈려요? 저희 집꺼는 입자들이 크게 갈려요ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.2",
          "form": "10년 넘은.. 골동품이랍니다,,,,",
          "original_form": "10년 넘은.. 골동품이랍니다,,,,",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.3",
          "form": "좋은건가봐요",
          "original_form": "좋은건가봐요",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.4",
          "form": "어디꺼 쓰시는데요?!",
          "original_form": "어디꺼 쓰시는데요?!",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.5",
          "form": "호오",
          "original_form": "호오",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.6",
          "form": "앗 아닌뎈ㅋㅋ 진짜진짜",
          "original_form": "앗 아닌뎈ㅋㅋ 진짜진짜",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.7",
          "form": "투박하고 오래되고",
          "original_form": "투박하고 오래되고",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.8",
          "form": "갈다보면 뚜껑이 열려서",
          "original_form": "갈다보면 뚜껑이 열려서",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.9",
          "form": "잘 모르겠어요ㅋㅋㅋㅋ 그 써진 부분이 지워져가지구 ㅋㅋㅋ",
          "original_form": "잘 모르겠어요ㅋㅋㅋㅋ 그 써진 부분이 지워져가지구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.10",
          "form": "내용물도 새고 그래요.....ㅠㅠ",
          "original_form": "내용물도 새고 그래요.....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.11",
          "form": "아아 저도 심지어 어디껀지도 모르는",
          "original_form": "아아 저도 심지어 어디껀지도 모르는",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.13",
          "form": "골동품이에요 흐흐",
          "original_form": "골동품이에요 흐흐",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040931.1.14",
          "form": "오래되면 다 그런가봐요",
          "original_form": "오래되면 다 그런가봐요",
          "speaker_id": "2",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040931.1.15",
          "form": "백화점 가면 진짜",
          "original_form": "백화점 가면 진짜",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040931.1.16",
          "form": "좋은 믹서기랑.... 착즙기",
          "original_form": "좋은 믹서기랑.... 착즙기",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040931.1.17",
          "form": "많던데.... 진짜 탐나지 않나요",
          "original_form": "많던데.... 진짜 탐나지 않나요",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040931.1.18",
          "form": "맞아요 근데 착즙기는 진짜 엄청 크더라구요 보셨어요?",
          "original_form": "맞아요 근데 착즙기는 진짜 엄청 크더라구요 보셨어요?",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040931.1.19",
          "form": "홈카페 개장 완전 가능",
          "original_form": "홈카페 개장 완전 가능",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040931.1.20",
          "form": "네네네!!!",
          "original_form": "네네네!!!",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040931.1.21",
          "form": "오바 좀 보태서 거의 밥솥만큼 자리 차지하던데요",
          "original_form": "오바 좀 보태서 거의 밥솥만큼 자리 차지하던데요",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040931.1.22",
          "form": "완전 멋지게 생기긴(?) 했는데",
          "original_form": "완전 멋지게 생기긴(?) 했는데",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040931.1.23",
