      "utterance": [
        {
          "id": "MDRW2100029313.1.1",
          "form": "ㅋ 전 고기 특히 좋아해요 ㅎ",
          "original_form": "ㅋ 전 고기 특히 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029313.1.2",
          "form": "마라탕이랑 파스타도 좋아요!",
          "original_form": "마라탕이랑 파스타도 좋아요!",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029313.1.3",
          "form": "고기!!!!",
          "original_form": "고기!!!!",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029313.1.4",
          "form": "파스타도 엄청 좋아하는데 ㅎㅎㅎㅎ",
          "original_form": "파스타도 엄청 좋아하는데 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029313.1.5",
          "form": "고기는 항상옳죠ㅎ",
          "original_form": "고기는 항상옳죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029313.1.6",
          "form": "마라탕은 안먹어봤네요",
          "original_form": "마라탕은 안먹어봤네요",
          "speaker_id": "2",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029313.1.7",
          "form": "꼭드셔보세요",
          "original_form": "꼭드셔보세요",
          "speaker_id": "1",
          "time": "20210903 01:23"
        },
        {
          "id": "MDRW2100029313.1.8",
          "form": "많이 맵진 안나요?",
          "original_form": "많이 맵진 안나요?",
          "speaker_id": "2",
          "time": "20210903 01:23"
        },
        {
          "id": "MDRW2100029313.1.9",
          "form": "맵기를 조절할수있어서 괜찮아요!",
          "original_form": "맵기를 조절할수있어서 괜찮아요!",
          "speaker_id": "1",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029313.1.10",
          "form": "ㅎ 제가 맵찔이라 맵게 먹어야 맛있을거 같아서 아직 못먹어봤어요 ㅎ",
          "original_form": "ㅎ 제가 맵찔이라 맵게 먹어야 맛있을거 같아서 아직 못먹어봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029313.1.11",
          "form": "예전에 매운 쭈꾸미유행할때도 ㅎ 매워서 ㅎㅎㅎ",
          "original_form": "예전에 매운 쭈꾸미유행할때도 ㅎ 매워서 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:25"
        },
        {
          "id": "MDRW2100029313.1.12",
          "form": "저도 맵찔이라 1단계로만 먹어요ㅋㅋㅋㅋ",
          "original_form": "저도 맵찔이라 1단계로만 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:25"
        },
        {
          "id": "MDRW2100029313.1.13",
          "form": "ㅎㅎㅎ 그럼 한번 먹어봐야겠네요",
          "original_form": "ㅎㅎㅎ 그럼 한번 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029313.1.14",
          "form": "먹는걸 좋아하는데 못먹어본게 너무 많네요 ㅎㅎㅎㅎ",
          "original_form": "먹는걸 좋아하는데 못먹어본게 너무 많네요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029313.1.15",
          "form": "쭈꾸미는 덜맵게 해도 밖에선 너무 맵더라고요ㅋㅋ",
          "original_form": "쭈꾸미는 덜맵게 해도 밖에선 너무 맵더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029313.1.16",
          "form": "마라탕 진짜 강추에요ㅋㅋㅋ",
          "original_form": "마라탕 진짜 강추에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:27"
        },
        {
          "id": "MDRW2100029313.1.17",
          "form": "넵 ㅎㅎㅎ 저만 그런게 아니었네요",
          "original_form": "넵 ㅎㅎㅎ 저만 그런게 아니었네요",
          "speaker_id": "2",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100029313.1.18",
          "form": "쭈꾸민 안맵게 하면 또 맛이 덜해서요 ㅜ",
          "original_form": "쭈꾸민 안맵게 하면 또 맛이 덜해서요 ㅜ",
          "speaker_id": "2",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100029313.1.19",
          "form": "마자요ㅠ",
          "original_form": "마자요ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:29"
        },
        {
          "id": "MDRW2100029313.1.20",
          "form": "그나마 덜맵거 하고 콩나물 많이먹어야해요ㅋㅋㅋ",
          "original_form": "그나마 덜맵거 하고 콩나물 많이먹어야해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029313.1.21",
          "form": "파스타는 어떤거 좋아하세요?",
          "original_form": "파스타는 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 01:30"
        }
      ]
    }
