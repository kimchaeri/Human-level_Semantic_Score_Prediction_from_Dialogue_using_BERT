      "utterance": [
        {
          "id": "MDRW2100029431.1.1",
          "form": "아까 영화얘기하다 말았잖아요",
          "original_form": "아까 영화얘기하다 말았잖아요",
          "speaker_id": "1",
          "time": "20210903 07:53"
        },
        {
          "id": "MDRW2100029431.1.2",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029431.1.3",
          "form": "보고 싶었는데 못 본 영화는 없나요?",
          "original_form": "보고 싶었는데 못 본 영화는 없나요?",
          "speaker_id": "1",
          "time": "20210903 08:05"
        },
        {
          "id": "MDRW2100029431.1.4",
          "form": "저 있어요",
          "original_form": "저 있어요",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029431.1.5",
          "form": "지금 개봉중인",
          "original_form": "지금 개봉중인",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029431.1.6",
          "form": "모가디슈 인질 싱크홀",
          "original_form": "모가디슈 인질 싱크홀",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029431.1.7",
          "form": "광고 보면 재미있어 보이던데",
          "original_form": "광고 보면 재미있어 보이던데",
          "speaker_id": "1",
          "time": "20210903 08:15"
        },
        {
          "id": "MDRW2100029431.1.8",
          "form": "저도 그렇지 않아도 관심이 좀 있었거든요",
          "original_form": "저도 그렇지 않아도 관심이 좀 있었거든요",
          "speaker_id": "1",
          "time": "20210903 08:16"
        },
        {
          "id": "MDRW2100029431.1.9",
          "form": "그러셨군요 저는 모가디슈 같은 경우는 프리프로덕션 단계에서부터 엄청 관심이 있어서",
          "original_form": "그러셨군요 저는 모가디슈 같은 경우는 프리프로덕션 단계에서부터 엄청 관심이 있어서",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029431.1.10",
          "form": "드디어 개봉하나 언제 개봉하나",
          "original_form": "드디어 개봉하나 언제 개봉하나",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029431.1.11",
          "form": "오매불망 기다리던 와중에",
          "original_form": "오매불망 기다리던 와중에",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029431.1.12",
          "form": "코로나때문에 아직도 못 봤어요",
          "original_form": "코로나때문에 아직도 못 봤어요",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029431.1.13",
          "form": "정말 오래 기다렸는데 아쉽겠어요",
          "original_form": "정말 오래 기다렸는데 아쉽겠어요",
          "speaker_id": "1",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029431.1.14",
          "form": "네네넹",
          "original_form": "네네넹",
          "speaker_id": "2",
          "time": "20210903 08:18"
        },
        {
          "id": "MDRW2100029431.1.15",
          "form": "이번에 아프가니스탄 사태하고 겹쳐서",
          "original_form": "이번에 아프가니스탄 사태하고 겹쳐서",
          "speaker_id": "1",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100029431.1.16",
          "form": "신문 지면에 오르내리는 걸 그렇지 않아도 봤는데 작품성 좋아 보이더라구요",
          "original_form": "신문 지면에 오르내리는 걸 그렇지 않아도 봤는데 작품성 좋아 보이더라구요",
          "speaker_id": "1",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100029431.1.17",
          "form": "류승완 감독이 준비를 오래 했대요",
          "original_form": "류승완 감독이 준비를 오래 했대요",
          "speaker_id": "2",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100029431.1.18",
          "form": "그랬군요",
          "original_form": "그랬군요",
          "speaker_id": "1",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100029431.1.19",
          "form": "현지 로케 하느라 고생도 많이 했고",
          "original_form": "현지 로케 하느라 고생도 많이 했고",
          "speaker_id": "2",
          "time": "20210903 08:20"
        },
        {
          "id": "MDRW2100029431.1.20",
          "form": "해외 촬영 힘들죠",
          "original_form": "해외 촬영 힘들죠",
          "speaker_id": "1",
          "time": "20210903 08:20"
        },
        {
          "id": "MDRW2100029431.1.21",
          "form": "베를린 하고 베테랑처럼 대박 터져야 할 텐데",
          "original_form": "베를린 하고 베테랑처럼 대박 터져야 할 텐데",
          "speaker_id": "2",
          "time": "20210903 08:20"
        },
        {
          "id": "MDRW2100029431.1.22",
          "form": "진정한 영화 팬 다워요",
          "original_form": "진정한 영화 팬 다워요",
          "speaker_id": "1",
          "time": "20210903 08:20"
        },
        {
          "id": "MDRW2100029431.1.23",
          "form": "그렇게 말하긴 좀 쑥스럽네요",
          "original_form": "그렇게 말하긴 좀 쑥스럽네요",
          "speaker_id": "2",
          "time": "20210903 08:20"
        },
        {
          "id": "MDRW2100029431.1.24",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 10:12"
        }
      ]
