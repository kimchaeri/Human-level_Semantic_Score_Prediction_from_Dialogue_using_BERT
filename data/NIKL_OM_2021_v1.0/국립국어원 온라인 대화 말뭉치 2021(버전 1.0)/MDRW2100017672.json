      "utterance": [
        {
          "id": "MDRW2100017672.1.1",
          "form": "싸이월드 세대신가요?",
          "original_form": "싸이월드 세대신가요?",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017672.1.2",
          "form": "싸이 좋아하셨어요?",
          "original_form": "싸이 좋아하셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017672.1.3",
          "form": "네ㅎ엄청열심히 했었네요",
          "original_form": "네ㅎ엄청열심히 했었네요",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017672.1.4",
          "form": "이번에 부활 반가우셨겠어요!",
          "original_form": "이번에 부활 반가우셨겠어요!",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017672.1.5",
          "form": "네 오래 기다렸는데 자꾸미뤄지네요",
          "original_form": "네 오래 기다렸는데 자꾸미뤄지네요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017672.1.6",
          "form": "ㅋㅋ흑역사는 어쩌죠...",
          "original_form": "ㅋㅋ흑역사는 어쩌죠...",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017672.1.7",
          "form": "재빨리 비공개로ㅋㅋ",
          "original_form": "재빨리 비공개로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017672.1.8",
          "form": "음 사실 구남친 생각날 듯요ㅠㅋㅋ",
          "original_form": "음 사실 구남친 생각날 듯요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017672.1.9",
          "form": "악ㅋㅋ그때 구남친이면 추억이죠ㅜㅠ",
          "original_form": "악ㅋㅋ그때 구남친이면 추억이죠ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017672.1.10",
          "form": "맞아요ㅋㅋ10년전이네요",
          "original_form": "맞아요ㅋㅋ10년전이네요",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017672.1.11",
          "form": "미화된 기억들...",
          "original_form": "미화된 기억들...",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017672.1.12",
          "form": "ㅋㅋ시간지나니 좋은기억만 남네요",
          "original_form": "ㅋㅋ시간지나니 좋은기억만 남네요",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100017672.1.13",
          "form": "좋지만은 않았는데 말이죠",
          "original_form": "좋지만은 않았는데 말이죠",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100017672.1.14",
          "form": "그게 좋은 것 같아요 미화되니 행복하죠",
          "original_form": "그게 좋은 것 같아요 미화되니 행복하죠",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017672.1.15",
          "form": "지인들도 기다리더라고요 여행사진같은거 때문에요",
          "original_form": "지인들도 기다리더라고요 여행사진같은거 때문에요",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100017672.1.16",
          "form": "네ㅋ 어렸을때 사진 제대로 백업 안해놨는데",
          "original_form": "네ㅋ 어렸을때 사진 제대로 백업 안해놨는데",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100017672.1.17",
          "form": "넘좋네요",
          "original_form": "넘좋네요",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100017672.1.18",
          "form": "ㅋㅋ저는 싸이월드를 잘안해서ㅋㅋ 그래도 같은 세대라 공감이 많이 돼요",
          "original_form": "ㅋㅋ저는 싸이월드를 잘안해서ㅋㅋ 그래도 같은 세대라 공감이 많이 돼요",
