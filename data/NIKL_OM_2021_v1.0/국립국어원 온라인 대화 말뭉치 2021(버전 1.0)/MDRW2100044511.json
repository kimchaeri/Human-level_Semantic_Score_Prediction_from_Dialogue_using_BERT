      "utterance": [
        {
          "id": "MDRW2100044511.1.1",
          "form": "요즘 쇼핑한거 있나",
          "original_form": "요즘 쇼핑한거 있나",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044511.1.2",
          "form": "꿀떡 삼키는거는 포도물맛이고ㅎ",
          "original_form": "꿀떡 삼키는거는 포도물맛이고ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044511.1.3",
          "form": "아니 슬리퍼랑 핑크 겉옷 얘기했었지?",
          "original_form": "아니 슬리퍼랑 핑크 겉옷 얘기했었지?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044511.1.4",
          "form": "핑크 겉옷 처음들어",
          "original_form": "핑크 겉옷 처음들어",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044511.1.5",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044511.1.6",
          "form": "ㅋㅋㅋㅋ엄청 얇은 폴리 찰랑한 소재이고 모양은 트레이닝 비슷",
          "original_form": "ㅋㅋㅋㅋ엄청 얇은 폴리 찰랑한 소재이고 모양은 트레이닝 비슷",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044511.1.7",
          "form": "요즘 날이 그래서 겉옷으로 이틀 들고 다녔는데\n한번도 안입음ㅋㅋ",
          "original_form": "요즘 날이 그래서 겉옷으로 이틀 들고 다녔는데\n한번도 안입음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044511.1.8",
          "form": "덥드나 ㅋㅋ",
          "original_form": "덥드나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044511.1.9",
          "form": "낼 부터 안 들고 가기로 생각했다 짐이여ㅋㅋ",
          "original_form": "낼 부터 안 들고 가기로 생각했다 짐이여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044511.1.10",
          "form": "ㅋㅋ 아직 덥드라 ㅋㅋ",
          "original_form": "ㅋㅋ 아직 덥드라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044511.1.11",
          "form": "나 반바지 입어서 저녁퇴근 추울까 갖고다녔는데 회사서도 글코 더워",
          "original_form": "나 반바지 입어서 저녁퇴근 추울까 갖고다녔는데 회사서도 글코 더워",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044511.1.12",
          "form": "춥지가 않아ㅋㅋㅋ",
          "original_form": "춥지가 않아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:00"
