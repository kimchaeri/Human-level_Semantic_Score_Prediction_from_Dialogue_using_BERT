      "utterance": [
        {
          "id": "MDRW2100001061.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210722 22:28"
        },
        {
          "id": "MDRW2100001061.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210722 22:28"
        },
        {
          "id": "MDRW2100001061.1.3",
          "form": "혹시 쇼핑 좋아하세요?",
          "original_form": "혹시 쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210722 22:29"
        },
        {
          "id": "MDRW2100001061.1.4",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210722 22:29"
        },
        {
          "id": "MDRW2100001061.1.5",
          "form": "오오오! 주로 쇼핑하는건 뭔가요?",
          "original_form": "오오오! 주로 쇼핑하는건 뭔가요?",
          "speaker_id": "1",
          "time": "20210722 22:30"
        },
        {
          "id": "MDRW2100001061.1.6",
          "form": "옷 같은거 인터넷으로 많이사요!",
          "original_form": "옷 같은거 인터넷으로 많이사요!",
          "speaker_id": "2",
          "time": "20210722 22:30"
        },
        {
          "id": "MDRW2100001061.1.7",
          "form": "오옹 그렇군요 저는 옷 보단 그냥 생활 용품 많이 사는 것 같아요",
          "original_form": "오옹 그렇군요 저는 옷 보단 그냥 생활 용품 많이 사는 것 같아요",
          "speaker_id": "1",
          "time": "20210722 22:31"
        },
        {
          "id": "MDRW2100001061.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210722 22:31"
        },
        {
          "id": "MDRW2100001061.1.9",
          "form": "요즘은 직접쇼핑몰가서 사는 경우는 적어진거같아요",
          "original_form": "요즘은 직접쇼핑몰가서 사는 경우는 적어진거같아요",
          "speaker_id": "2",
          "time": "20210722 22:31"
        },
        {
          "id": "MDRW2100001061.1.10",
          "form": "코로나 때문에...ㅠ",
          "original_form": "코로나 때문에...ㅠ",
          "speaker_id": "2",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001061.1.11",
          "form": "아 맞아요ㅠㅠ 아무래도 코로나도 있고 그러다보니까ㅠㅠ",
          "original_form": "아 맞아요ㅠㅠ 아무래도 코로나도 있고 그러다보니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001061.1.12",
          "form": "저는 원래 아이쇼핑하는거 되게 좋아했는데 요즘 그러질 못해서 너무 아쉬워요..",
          "original_form": "저는 원래 아이쇼핑하는거 되게 좋아했는데 요즘 그러질 못해서 너무 아쉬워요..",
          "speaker_id": "1",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001061.1.13",
          "form": "아 진짜 ㅠㅠ",
          "original_form": "아 진짜 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001061.1.14",
          "form": "옷 사는것도 솔직히 입어보고 사는게 좀더 나은데",
          "original_form": "옷 사는것도 솔직히 입어보고 사는게 좀더 나은데",
          "speaker_id": "2",
          "time": "20210722 22:32"
        },
        {
          "id": "MDRW2100001061.1.15",
          "form": "아쉬워요 ㅠㅠ",
          "original_form": "아쉬워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 22:33"
        },
        {
          "id": "MDRW2100001061.1.16",
          "form": "맞아요 특히 옷같은 경우는 더 직접 입어보고 사야 맞는 것 같아요",
          "original_form": "맞아요 특히 옷같은 경우는 더 직접 입어보고 사야 맞는 것 같아요",
          "speaker_id": "1",
          "time": "20210722 22:33"
        },
        {
          "id": "MDRW2100001061.1.17",
          "form": "맞죠..",
          "original_form": "맞죠..",
          "speaker_id": "2",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001061.1.18",
          "form": "당근마켓 하세요??",
          "original_form": "당근마켓 하세요??",
          "speaker_id": "2",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001061.1.19",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001061.1.20",
          "form": "직거래 장인 다 됐어요ㅋㅋㅋㅋ",
          "original_form": "직거래 장인 다 됐어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001061.1.21",
          "form": "전 직접적으로 거랜한적은 없는데 뭔가 신기한게",
          "original_form": "전 직접적으로 거랜한적은 없는데 뭔가 신기한게",
          "speaker_id": "2",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001061.1.22",
          "form": "많이보이더라구요 ㅋㅋㅋ",
          "original_form": "많이보이더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001061.1.23",
          "form": "앜ㅋㅋㅋㅋ 맞아요",
          "original_form": "앜ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001061.1.24",
          "form": "전 최근에 무료나눔으로 교재들 받았는데 다 너무 상태가 좋아서 진짜 이득봤어요..",
          "original_form": "전 최근에 무료나눔으로 교재들 받았는데 다 너무 상태가 좋아서 진짜 이득봤어요..",
          "speaker_id": "1",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001061.1.25",
          "form": "어떤거 팔아보셨어요??",
          "original_form": "어떤거 팔아보셨어요??",
          "speaker_id": "2",
          "time": "20210722 22:42"
        },
        {
          "id": "MDRW2100001061.1.26",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210722 22:42"
        },
        {
          "id": "MDRW2100001061.1.27",
          "form": "저는 방탄 앨범이랑 옷 팔아봤어요!",
          "original_form": "저는 방탄 앨범이랑 옷 팔아봤어요!",
          "speaker_id": "1",
          "time": "20210722 22:43"
        },
        {
          "id": "MDRW2100001061.1.28",
          "form": "아하!!",
          "original_form": "아하!!",
          "speaker_id": "2",
          "time": "20210722 22:44"
        }
      ]
    }
