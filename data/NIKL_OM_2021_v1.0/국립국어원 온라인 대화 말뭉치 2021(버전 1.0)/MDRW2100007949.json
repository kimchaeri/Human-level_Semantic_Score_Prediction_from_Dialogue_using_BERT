      "utterance": [
        {
          "id": "MDRW2100007949.1.1",
          "form": "포장해와서 먹으려구요",
          "original_form": "포장해와서 먹으려구요",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007949.1.2",
          "form": "오늘 출근해서 ㅎㅎ",
          "original_form": "오늘 출근해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007949.1.3",
          "form": "오늘 일욜인데 출근해요?",
          "original_form": "오늘 일욜인데 출근해요?",
          "speaker_id": "2",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007949.1.4",
          "form": "네ㅎㅎ 교대근무라서!",
          "original_form": "네ㅎㅎ 교대근무라서!",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007949.1.5",
          "form": "아 무슨일하시는데 교대근무를~",
          "original_form": "아 무슨일하시는데 교대근무를~",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007949.1.6",
          "form": "저는 지하철이요ㅎㅎ 무슨 일 하세요?",
          "original_form": "저는 지하철이요ㅎㅎ 무슨 일 하세요?",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007949.1.7",
          "form": "오ㅎ 지하철이면 기관사? 그일인가요?",
          "original_form": "오ㅎ 지하철이면 기관사? 그일인가요?",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007949.1.8",
          "form": "아ㅎㅎ 역에 있어요~~",
          "original_form": "아ㅎㅎ 역에 있어요~~",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007949.1.9",
          "form": "전 그냥 사무직이요ㅎ",
          "original_form": "전 그냥 사무직이요ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007949.1.10",
          "form": "ㅎㅎ 저도 직렬은 사무에요",
          "original_form": "ㅎㅎ 저도 직렬은 사무에요",
          "speaker_id": "1",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007949.1.11",
          "form": "교대근무하면 어때요? 남들쉴때 일하고",
          "original_form": "교대근무하면 어때요? 남들쉴때 일하고",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007949.1.12",
          "form": "남들일할때 쉬고ㅎ",
          "original_form": "남들일할때 쉬고ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007949.1.13",
          "form": "장점이 해외여행인데",
          "original_form": "장점이 해외여행인데",
          "speaker_id": "1",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007949.1.14",
          "form": "요새는 못가니까",
          "original_form": "요새는 못가니까",
          "speaker_id": "1",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007949.1.15",
          "form": "장점이 퇴색된듯요",
          "original_form": "장점이 퇴색된듯요",
          "speaker_id": "1",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007949.1.16",
          "form": "격일로 교대가 아닌가보네요",
          "original_form": "격일로 교대가 아닌가보네요",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007949.1.17",
          "form": "업무 스트레스는 없으세요?",
          "original_form": "업무 스트레스는 없으세요?",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007949.1.18",
          "form": "질이 낮은 승객이 좀 ㅎㅎ",
          "original_form": "질이 낮은 승객이 좀 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007949.1.19",
          "form": "뭐 그거랑 회사 답답하죠ㅎㅎ",
          "original_form": "뭐 그거랑 회사 답답하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007949.1.20",
          "form": "지하철에 그런사람 많죠",
          "original_form": "지하철에 그런사람 많죠",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007949.1.21",
          "form": "야근 이런건 없으신거죠?",
          "original_form": "야근 이런건 없으신거죠?",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007949.1.22",
          "form": "음 야근은 없는데",
          "original_form": "음 야근은 없는데",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007949.1.23",
          "form": "야간에도 일해요ㅎㅎ",
          "original_form": "야간에도 일해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007949.1.24",
          "form": "지하철이 늦게까지 운행하니 그렇겠네요",
          "original_form": "지하철이 늦게까지 운행하니 그렇겠네요",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007949.1.25",
          "form": "야간에 일하면 수당도 나와요?ㅎ",
          "original_form": "야간에 일하면 수당도 나와요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007949.1.26",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007949.1.27",
          "form": "월급에 야간 수당",
          "original_form": "월급에 야간 수당",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007949.1.28",
          "form": "포함이에요",
          "original_form": "포함이에요",
          "speaker_id": "1",
          "time": "20210822 12:26"
        }
      ]
    }
