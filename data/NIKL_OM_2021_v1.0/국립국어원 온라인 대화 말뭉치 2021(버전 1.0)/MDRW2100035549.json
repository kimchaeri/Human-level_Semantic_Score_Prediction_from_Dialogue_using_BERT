      "utterance": [
        {
          "id": "MDRW2100035549.1.1",
          "form": "오늘 출렁다리 갔다옴 ㅋㅋ",
          "original_form": "오늘 출렁다리 갔다옴 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100035549.1.2",
          "form": "그게머야? 흔들다리?",
          "original_form": "그게머야? 흔들다리?",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100035549.1.3",
          "form": "ㅇㅇ흔들다리임 국내 최장이래요 ㅎㅎ",
          "original_form": "ㅇㅇ흔들다리임 국내 최장이래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100035549.1.4",
          "form": "와 재밌었음? 어땠어",
          "original_form": "와 재밌었음? 어땠어",
          "speaker_id": "2",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100035549.1.5",
          "form": "음 나는 사실 높은곳을 좀 무서워하는편임",
          "original_form": "음 나는 사실 높은곳을 좀 무서워하는편임",
          "speaker_id": "1",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035549.1.6",
          "form": "고소공포증 정도는 아니지만",
          "original_form": "고소공포증 정도는 아니지만",
          "speaker_id": "1",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035549.1.7",
          "form": "나도 무서움ㅋㅋ",
          "original_form": "나도 무서움ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035549.1.8",
          "form": "근데 8/31까지만 공짜래서",
          "original_form": "근데 8/31까지만 공짜래서",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100035549.1.9",
          "form": "정말 쪼잔한 소시민의 마인드로 숙제하듯 갔다옴",
          "original_form": "정말 쪼잔한 소시민의 마인드로 숙제하듯 갔다옴",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100035549.1.10",
          "form": "그것도 좋은 동기지",
          "original_form": "그것도 좋은 동기지",
          "speaker_id": "2",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100035549.1.11",
          "form": "안그럼 안볼거아님 ㅋㅋ",
          "original_form": "안그럼 안볼거아님 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100035549.1.12",
          "form": "생각보다 무서우면서 안무서움",
          "original_form": "생각보다 무서우면서 안무서움",
          "speaker_id": "1",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100035549.1.13",
          "form": "그게뭐여ㅋㅋ",
          "original_form": "그게뭐여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100035549.1.14",
          "form": "무서운데 아무렇지도않다고 마인드컨트롤하면 안무서울정도고",
          "original_form": "무서운데 아무렇지도않다고 마인드컨트롤하면 안무서울정도고",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035549.1.15",
          "form": "정신승리네",
          "original_form": "정신승리네",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035549.1.16",
          "form": "무섭다고생각하면 계속무서운정도",
          "original_form": "무섭다고생각하면 계속무서운정도",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035549.1.17",
          "form": "단지 대부분의 사람들은 괜찮아보였음",
          "original_form": "단지 대부분의 사람들은 괜찮아보였음",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035549.1.18",
          "form": "남들속이야 모르제ㅜ",
          "original_form": "남들속이야 모르제ㅜ",
          "speaker_id": "2",
          "time": "20210829 20:59"
        },
        {
          "id": "MDRW2100035549.1.19",
          "form": "좀더 풍경이나 바닷바람 못즐긴게 아쉬움",
          "original_form": "좀더 풍경이나 바닷바람 못즐긴게 아쉬움",
          "speaker_id": "1",
          "time": "20210829 21:00"
        },
        {
          "id": "MDRW2100035549.1.20",
          "form": "여러번가면",
          "original_form": "여러번가면",
          "speaker_id": "2",
          "time": "20210829 21:01"
        },
        {
          "id": "MDRW2100035549.1.21",
          "form": "시야가 넓어지겠다",
          "original_form": "시야가 넓어지겠다",
          "speaker_id": "2",
          "time": "20210829 21:01"
        },
        {
          "id": "MDRW2100035549.1.22",
          "form": "아니 이제 유료야ㅜ",
          "original_form": "아니 이제 유료야ㅜ",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035549.1.23",
          "form": "아쉬우니 30일 오전이랑 31일 오전에도 가볼까나",
          "original_form": "아쉬우니 30일 오전이랑 31일 오전에도 가볼까나",
          "speaker_id": "1",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035549.1.24",
          "form": "ㅋㅋㅇㅇ",
          "original_form": "ㅋㅋㅇㅇ",
          "speaker_id": "2",
