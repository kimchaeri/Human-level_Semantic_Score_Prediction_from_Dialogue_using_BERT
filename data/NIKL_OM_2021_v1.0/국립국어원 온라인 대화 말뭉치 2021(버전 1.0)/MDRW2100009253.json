      "utterance": [
        {
          "id": "MDRW2100009253.1.1",
          "form": "영화좋아요?",
          "original_form": "영화좋아요?",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009253.1.2",
          "form": "좋아해요 뮤지컬이나 연극도",
          "original_form": "좋아해요 뮤지컬이나 연극도",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009253.1.3",
          "form": "전 한번도 못봤는데",
          "original_form": "전 한번도 못봤는데",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009253.1.4",
          "form": "연극 한번보면 신세계예요 꼭 가보세요",
          "original_form": "연극 한번보면 신세계예요 꼭 가보세요",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009253.1.5",
          "form": "내용이?",
          "original_form": "내용이?",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009253.1.6",
          "form": "아무거나요 ㅎ",
          "original_form": "아무거나요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009253.1.7",
          "form": "현장감이 느껴지니 달라요",
          "original_form": "현장감이 느껴지니 달라요",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009253.1.8",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009253.1.9",
          "form": "가봐야겠다",
          "original_form": "가봐야겠다",
          "speaker_id": "1",
          "time": "20210823 07:24"
        },
        {
          "id": "MDRW2100009253.1.10",
          "form": "네 코믹도 있고 유명한거 많으니 꼭 봐보세요",
          "original_form": "네 코믹도 있고 유명한거 많으니 꼭 봐보세요",
          "speaker_id": "2",
          "time": "20210823 07:27"
        },
        {
          "id": "MDRW2100009253.1.11",
          "form": "거리두기라ㅜ아직 안하죠?",
          "original_form": "거리두기라ㅜ아직 안하죠?",
          "speaker_id": "1",
          "time": "20210823 07:33"
        },
        {
          "id": "MDRW2100009253.1.12",
          "form": "하긴해요",
          "original_form": "하긴해요",
          "speaker_id": "2",
          "time": "20210823 07:44"
        },
        {
          "id": "MDRW2100009253.1.13",
          "form": "지금은 위험하니 나중에 가세요",
          "original_form": "지금은 위험하니 나중에 가세요",
          "speaker_id": "2",
          "time": "20210823 07:45"
        },
        {
          "id": "MDRW2100009253.1.14",
          "form": "네 시간이 지나면 가야죠",
          "original_form": "네 시간이 지나면 가야죠",
          "speaker_id": "1",
          "time": "20210823 07:46"
        },
        {
          "id": "MDRW2100009253.1.15",
          "form": "코로나 끝나면 갈곳도 많아지겠어요",
          "original_form": "코로나 끝나면 갈곳도 많아지겠어요",
          "speaker_id": "2",
          "time": "20210823 07:52"
        },
        {
          "id": "MDRW2100009253.1.16",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210823 08:01"
        }
