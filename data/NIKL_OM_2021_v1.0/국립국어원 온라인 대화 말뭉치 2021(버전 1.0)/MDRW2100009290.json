          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009290.1.4",
          "form": "난 인터넷 쇼핑 좋아",
          "original_form": "난 인터넷 쇼핑 좋아",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009290.1.5",
          "form": "왜냐면 가기 귀찮거든",
          "original_form": "왜냐면 가기 귀찮거든",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009290.1.6",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009290.1.7",
          "form": "나 40마넌씩은 쓰는듯 ㅠㅠ",
          "original_form": "나 40마넌씩은 쓰는듯 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009290.1.8",
          "form": "뭘 ㅈㅔ일 많이 사?",
          "original_form": "뭘 ㅈㅔ일 많이 사?",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009290.1.9",
          "form": "나 옷!너는?",
          "original_form": "나 옷!너는?",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009290.1.10",
          "form": "나는 문구류... 맨날 다이어리 사",
          "original_form": "나는 문구류... 맨날 다이어리 사",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009290.1.11",
          "form": "다이어리좋지",
          "original_form": "다이어리좋지",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009290.1.12",
          "form": "이번해에만 5개 샀어",
          "original_form": "이번해에만 5개 샀어",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
