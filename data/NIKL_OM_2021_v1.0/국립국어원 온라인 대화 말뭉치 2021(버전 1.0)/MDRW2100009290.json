          "form": "키키",
          "original_form": "키키",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009290.1.14",
          "form": "언제써..?",
          "original_form": "언제써..?",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009290.1.15",
          "form": "나 최근에 갤럭시워치 구매했는대",
          "original_form": "나 최근에 갤럭시워치 구매했는대",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009290.1.16",
          "form": "기대도9",
          "original_form": "기대도9",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009290.1.17",
          "form": "갤워치4??!!??!?!?!?!??!??!?!?!?",
          "original_form": "갤워치4??!!??!?!?!?!??!??!?!?!?",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
