      "utterance": [
        {
          "id": "MDRW2100027808.1.1",
          "form": "올해에 태풍이 더 오겠죠?",
          "original_form": "올해에 태풍이 더 오겠죠?",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027808.1.2",
          "form": "그럴거 같아요ㅠ 태풍오면 안좋은데",
          "original_form": "그럴거 같아요ㅠ 태풍오면 안좋은데",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027808.1.3",
          "form": "태풍 오면 물가 오르는게 너무 싫어요",
          "original_form": "태풍 오면 물가 오르는게 너무 싫어요",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027808.1.4",
          "form": "맞아요 특히 채소들 금값이죠",
          "original_form": "맞아요 특히 채소들 금값이죠",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027808.1.5",
          "form": "그럴때는 어쩔수 없이 레토르트 사게되는거 같아요",
          "original_form": "그럴때는 어쩔수 없이 레토르트 사게되는거 같아요",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027808.1.6",
          "form": "맞아요 간편하게 돌려먹는게 편하죠",
          "original_form": "맞아요 간편하게 돌려먹는게 편하죠",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027808.1.7",
          "form": "그게 더 싸기도 하구요",
          "original_form": "그게 더 싸기도 하구요",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027808.1.8",
          "form": "맞아요 추워지면 과일도 비싸지고요",
          "original_form": "맞아요 추워지면 과일도 비싸지고요",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027808.1.9",
          "form": "그런점에선 여름이 좋군요",
          "original_form": "그런점에선 여름이 좋군요",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027808.1.10",
          "form": "여름이면 수박이죠!",
          "original_form": "여름이면 수박이죠!",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027808.1.11",
          "form": "맞아요 수박!",
          "original_form": "맞아요 수박!",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027808.1.12",
          "form": "올해에는 몇번 못먹은거 같아요",
          "original_form": "올해에는 몇번 못먹은거 같아요",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027808.1.13",
          "form": "맞아요ㅠ 유독 올해꺼는 달지않았어용",
          "original_form": "맞아요ㅠ 유독 올해꺼는 달지않았어용",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100027808.1.14",
          "form": "왜 그런걸까요... 폭염때문인가",
          "original_form": "왜 그런걸까요... 폭염때문인가",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100027808.1.15",
          "form": "폭염에다가 비가 너무 안와서 그런가바요",
          "original_form": "폭염에다가 비가 너무 안와서 그런가바요",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100027808.1.16",
          "form": "ㅠ슬프네요",
          "original_form": "ㅠ슬프네요",
