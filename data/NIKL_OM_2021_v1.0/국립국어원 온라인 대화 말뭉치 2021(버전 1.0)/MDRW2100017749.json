      "utterance": [
        {
          "id": "MDRW2100017749.1.1",
          "form": "요즘 날씨 미친거 같아",
          "original_form": "요즘 날씨 미친거 같아",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017749.1.2",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017749.1.3",
          "form": "오늘은 또 완전 여름이더라",
          "original_form": "오늘은 또 완전 여름이더라",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017749.1.4",
          "form": "어제 비내리던게 꿈같음",
          "original_form": "어제 비내리던게 꿈같음",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017749.1.5",
          "form": "어제 연산동 거제리쪽",
          "original_form": "어제 연산동 거제리쪽",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017749.1.6",
          "form": "ㄹㅇ ㅋㅋㅋㅋㅋㅋㅋ 거짓말처럼 맑더라",
          "original_form": "ㄹㅇ ㅋㅋㅋㅋㅋㅋㅋ 거짓말처럼 맑더라",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017749.1.7",
          "form": "침수됐었다며?",
          "original_form": "침수됐었다며?",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017749.1.8",
          "form": "헐...난 몰랐오",
          "original_form": "헐...난 몰랐오",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017749.1.9",
          "form": "그랬구나...나 퇴근할때 멀쩡하게 했는데...?",
          "original_form": "그랬구나...나 퇴근할때 멀쩡하게 했는데...?",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017749.1.10",
          "form": "새벽에 그랬나?",
          "original_form": "새벽에 그랬나?",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017749.1.11",
          "form": "그랬나봐",
          "original_form": "그랬나봐",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017749.1.12",
          "form": "차타고 퇴근하신분이 말씀해주시더라",
          "original_form": "차타고 퇴근하신분이 말씀해주시더라",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017749.1.13",
          "form": "피해는없었지?",
          "original_form": "피해는없었지?",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017749.1.14",
          "form": "나 오늘 완전 꿀잠자서 몰랐어...",
          "original_form": "나 오늘 완전 꿀잠자서 몰랐어...",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017749.1.15",
          "form": "ㅇㅇㅇㅇ 완전 멀쩡",
          "original_form": "ㅇㅇㅇㅇ 완전 멀쩡",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017749.1.16",
          "form": "지구가",
          "original_form": "지구가",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017749.1.17",
          "form": "인간을 싫어하는거같아",
          "original_form": "인간을 싫어하는거같아",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017749.1.18",
          "form": "근데 담주까지 비소식 있는거 같던데...",
          "original_form": "근데 담주까지 비소식 있는거 같던데...",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017749.1.19",
          "form": "마자..지구가 인간 죽이려고 애쓰는데",
          "original_form": "마자..지구가 인간 죽이려고 애쓰는데",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017749.1.20",
          "form": "살아있어서 미안합니다ㅜ",
          "original_form": "살아있어서 미안합니다ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017749.1.21",
          "form": "인간이 눈치없이 살고있는거 같은 기분 ㅠㅋㅋㅋ",
          "original_form": "인간이 눈치없이 살고있는거 같은 기분 ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017749.1.22",
          "form": "갑자기 계절이 가을같아졌어",
          "original_form": "갑자기 계절이 가을같아졌어",
          "speaker_id": "2",
          "time": "20210906 09:20"
