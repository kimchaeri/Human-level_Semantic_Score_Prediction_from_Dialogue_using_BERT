      "utterance": [
        {
          "id": "MDRW2100041330.1.1",
          "form": "그게 요즘 최고 관심사인데!",
          "original_form": "그게 요즘 최고 관심사인데!",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041330.1.2",
          "form": "앱테크 뭐하세요?",
          "original_form": "앱테크 뭐하세요?",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.3",
          "form": "저 진짜 여러가지 하는데 일단",
          "original_form": "저 진짜 여러가지 하는데 일단",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.4",
          "form": "블로그 애드포스트랑",
          "original_form": "블로그 애드포스트랑",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.5",
          "form": "이게 요즘 제일 많이 들어오는거구요",
          "original_form": "이게 요즘 제일 많이 들어오는거구요",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.6",
          "form": "토스 만보기로 하루에 만 걸음 채워서 100원 벌고...",
          "original_form": "토스 만보기로 하루에 만 걸음 채워서 100원 벌고...",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.7",
          "form": "ㅋㅋㅋㅋ아 저그거 맨날 5000보 겨우채워서",
          "original_form": "ㅋㅋㅋㅋ아 저그거 맨날 5000보 겨우채워서",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.8",
          "form": "10원받는제 ㅠ",
          "original_form": "10원받는제 ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.9",
          "form": "엠브레인 패널파워라고 설문조사하는거",
          "original_form": "엠브레인 패널파워라고 설문조사하는거",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.10",
          "form": "그걸로 한달에 만원정도 벌고",
          "original_form": "그걸로 한달에 만원정도 벌고",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041330.1.11",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.13",
          "form": "엠브레인 패널파워랑 오베이",
          "original_form": "엠브레인 패널파워랑 오베이",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.14",
          "form": "이것도 쫌쯈따리하면",
          "original_form": "이것도 쫌쯈따리하면",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.15",
          "form": "두 달동안 3만원 정도는 모으는 거 같아요",
          "original_form": "두 달동안 3만원 정도는 모으는 거 같아요",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.16",
          "form": "그리고 캐시워크도 하구요,,,",
          "original_form": "그리고 캐시워크도 하구요,,,",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.17",
          "form": "저도 캐시워크 ㅎㅎ",
          "original_form": "저도 캐시워크 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.18",
          "form": "엠브레인 오베이",
          "original_form": "엠브레인 오베이",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.19",
          "form": "기억..",
          "original_form": "기억..",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.20",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041330.1.21",
          "form": "엠브레인이랑 오베이 가입하실 때",
          "original_form": "엠브레인이랑 오베이 가입하실 때",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.22",
          "form": "제 추천번호... 입력해주시면,,, ",
          "original_form": "제 추천번호... 입력해주시면,,, {emoji:♡}",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.23",
          "form": "그럼요 ㅋㅋㅋㅋ 제 폰이 찐 똥폰이라",
          "original_form": "그럼요 ㅋㅋㅋㅋ 제 폰이 찐 똥폰이라",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.24",
          "form": "뭘 깔기가 미안한 정도거든요",
          "original_form": "뭘 깔기가 미안한 정도거든요",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.25",
          "form": "이번달에 폰 바꿀예정이라",
          "original_form": "이번달에 폰 바꿀예정이라",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.26",
          "form": "헐헐 그러시구나",
          "original_form": "헐헐 그러시구나",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.27",
          "form": "바꾸면 바로 하겟음다",
          "original_form": "바꾸면 바로 하겟음다",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.28",
          "form": "저도 그랬어요",
          "original_form": "저도 그랬어요",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.29",
          "form": "진짜 그 심정 알아요",
          "original_form": "진짜 그 심정 알아요",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.30",
          "form": "저도 똥폰이어서 캐시워크를 못랬어요",
          "original_form": "저도 똥폰이어서 캐시워크를 못랬어요",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.31",
          "form": "아 근데 그래서 좀 슬픈 소식이 제가 아이폰으로 갈아탈 ㄱㅖ획이라..",
          "original_form": "아 근데 그래서 좀 슬픈 소식이 제가 아이폰으로 갈아탈 ㄱㅖ획이라..",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.32",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041330.1.33",
          "form": "아 저 캐시워크도 하고 타임스프레드도 해욬ㅋㅋㅋ",
          "original_form": "아 저 캐시워크도 하고 타임스프레드도 해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041330.1.34",
          "form": "헐 그러신가요!!!",
          "original_form": "헐 그러신가요!!!",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041330.1.35",
          "form": "폰바꾸기 전까지 대화 많이 쌓아요..",
          "original_form": "폰바꾸기 전까지 대화 많이 쌓아요..",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041330.1.36",
          "form": "네네네ㅠㅠㅠ",
          "original_form": "네네네ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041330.1.37",
          "form": "잠깐 디엠 드릴게요!!!",
          "original_form": "잠깐 디엠 드릴게요!!!",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041330.1.38",
          "form": "앗 밑에 못봣어요!",
          "original_form": "앗 밑에 못봣어요!",
          "speaker_id": "2",
          "time": "20210905 22:15"
        }
      ]
    }
