      "utterance": [
        {
          "id": "MDRW2100046003.1.1",
          "form": "퇴근하고싶네요ㅎㅎ",
          "original_form": "퇴근하고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100046003.1.2",
          "form": "저도 벌써 퇴근하고 싶네요!!",
          "original_form": "저도 벌써 퇴근하고 싶네요!!",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046003.1.3",
          "form": "아직 도착 안했어요 근데 ㅋㅋㅋㅋ",
          "original_form": "아직 도착 안했어요 근데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046003.1.4",
          "form": "ㅋㅋ앜ㅋㅋㅋ아직도요?",
          "original_form": "ㅋㅋ앜ㅋㅋㅋ아직도요?",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046003.1.5",
          "form": "네 ㅋㅋㅋㅋ 곧 도착이에요",
          "original_form": "네 ㅋㅋㅋㅋ 곧 도착이에요",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046003.1.6",
          "form": "몇시에 퇴근하세요??",
          "original_form": "몇시에 퇴근하세요??",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046003.1.7",
          "form": "저는5시반이요",
          "original_form": "저는5시반이요",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046003.1.8",
          "form": "ㅠㅜ아쉽",
          "original_form": "ㅠㅜ아쉽",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046003.1.9",
          "form": "오! 퇴근 빠르신 편이네요??",
          "original_form": "오! 퇴근 빠르신 편이네요??",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046003.1.10",
          "form": "칼퇴하시나요??",
          "original_form": "칼퇴하시나요??",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046003.1.11",
          "form": "ㅎㅎ어제당직섰어서칼퇴해야해용",
          "original_form": "ㅎㅎ어제당직섰어서칼퇴해야해용",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046003.1.12",
          "form": "아아 병원에서 일하시나요??",
          "original_form": "아아 병원에서 일하시나요??",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046003.1.13",
          "form": "멋있으세요!!",
          "original_form": "멋있으세요!!",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046003.1.14",
          "form": "그럼 얼른가서 쉬셔야죠!!",
          "original_form": "그럼 얼른가서 쉬셔야죠!!",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046003.1.15",
          "form": "ㅎㅎㅎ넹넹 존버중이에요",
          "original_form": "ㅎㅎㅎ넹넹 존버중이에요",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046003.1.16",
          "form": "어제 그럼 잠을 못주무신거에요??",
          "original_form": "어제 그럼 잠을 못주무신거에요??",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046003.1.17",
          "form": "ㅠㅠㅠ 너무 피곤하시겠어요...",
          "original_form": "ㅠㅠㅠ 너무 피곤하시겠어요...",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046003.1.18",
          "form": "자면서콜받았어요..",
          "original_form": "자면서콜받았어요..",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046003.1.19",
          "form": "어휴ㅠㅠ 저는 병원생활은 잘 모르지만...",
          "original_form": "어휴ㅠㅠ 저는 병원생활은 잘 모르지만...",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046003.1.20",
          "form": "슬의생 보긴 했는데 ㅋㅋㅋㅋ 너무 판타지에 가까운거 같긴해요",
          "original_form": "슬의생 보긴 했는데 ㅋㅋㅋㅋ 너무 판타지에 가까운거 같긴해요",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046003.1.21",
          "form": "그렇게 바쁜데 밴드라니??!!",
          "original_form": "그렇게 바쁜데 밴드라니??!!",
