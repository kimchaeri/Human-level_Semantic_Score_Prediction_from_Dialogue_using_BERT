      "utterance": [
        {
          "id": "MDRW2100023655.1.1",
          "form": "쇼핑 주로 어디서 하세용?",
          "original_form": "쇼핑 주로 어디서 하세용?",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023655.1.2",
          "form": "생필품은 마트",
          "original_form": "생필품은 마트",
          "speaker_id": "2",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023655.1.3",
          "form": "나머지는 인터넷이요",
          "original_form": "나머지는 인터넷이요",
          "speaker_id": "2",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023655.1.4",
          "form": "저는 생필품은 옥션이나 지마켓에서 사는데",
          "original_form": "저는 생필품은 옥션이나 지마켓에서 사는데",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023655.1.5",
          "form": "빅스마일데이라고 아세요?",
          "original_form": "빅스마일데이라고 아세요?",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023655.1.6",
          "form": "들어는 봤어요",
          "original_form": "들어는 봤어요",
          "speaker_id": "2",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023655.1.7",
          "form": "써본적은 없지만요",
          "original_form": "써본적은 없지만요",
          "speaker_id": "2",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023655.1.8",
          "form": "일년에 3번인가 하는것같은데 그때 왕창 사요a",
          "original_form": "일년에 3번인가 하는것같은데 그때 왕창 사요a",
          "speaker_id": "1",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023655.1.9",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023655.1.10",
          "form": "전 그 네이버 멤버쉽해서",
          "original_form": "전 그 네이버 멤버쉽해서",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023655.1.11",
          "form": "걍 네이버 검색해서 사는편이에요",
          "original_form": "걍 네이버 검색해서 사는편이에요",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023655.1.12",
          "form": "네이버 멤버십으로 구매하면",
          "original_form": "네이버 멤버십으로 구매하면",
          "speaker_id": "1",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023655.1.13",
          "form": "포인트가 은근 잘 쌓이더라구요",
          "original_form": "포인트가 은근 잘 쌓이더라구요",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100023655.1.14",
          "form": "그렇게 쌓인 포인트로 또 구매해요ㅋㅋㅋ",
          "original_form": "그렇게 쌓인 포인트로 또 구매해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100023655.1.15",
          "form": "맞아요, 전 그 쌓인 포인트가 딱 멤버쉽 비용으로 나가더라고요",
          "original_form": "맞아요, 전 그 쌓인 포인트가 딱 멤버쉽 비용으로 나가더라고요",
          "speaker_id": "2",
          "time": "20210830 13:55"
        },
        {
          "id": "MDRW2100023655.1.16",
          "form": "인쇼 좀 하면 그닥 손해보진 않아요 ㅋㅋ",
          "original_form": "인쇼 좀 하면 그닥 손해보진 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023655.1.17",
          "form": "혹시 계란 필요하시면",
          "original_form": "혹시 계란 필요하시면",
          "speaker_id": "1",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023655.1.18",
          "form": "하나로마트에서",
          "original_form": "하나로마트에서",
          "speaker_id": "1",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023655.1.19",
          "form": "한판에 4900원에 세일한대요",
          "original_form": "한판에 4900원에 세일한대요",
          "speaker_id": "1",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023655.1.20",
          "form": "헉! 싸다",
          "original_form": "헉! 싸다",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023655.1.21",
          "form": "요즘 메추리알만한게 6천원 넘는데",
          "original_form": "요즘 메추리알만한게 6천원 넘는데",
          "speaker_id": "2",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023655.1.22",
          "form": "그춍ㅋㅋㅋ 농협에서 하는 특가세일인가봐요",
          "original_form": "그춍ㅋㅋㅋ 농협에서 하는 특가세일인가봐요",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023655.1.23",
          "form": "좋은 정보 감사합니다 ㅋㅋ",
          "original_form": "좋은 정보 감사합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023655.1.24",
          "form": "하루에 100한 한정이라 오픈때 가세용!ㅋㅋㅋ",
          "original_form": "하루에 100한 한정이라 오픈때 가세용!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023655.1.25",
          "form": "곧 추석 다가오면 계란값오르니",
          "original_form": "곧 추석 다가오면 계란값오르니",
          "speaker_id": "1",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023655.1.26",
          "form": "미리미리 구매하셔요",
          "original_form": "미리미리 구매하셔요",
          "speaker_id": "1",
          "time": "20210830 13:59"
        }
      ]
    }
