      "utterance": [
        {
          "id": "MDRW2100008141.1.1",
          "form": "새우튀김도 좋아",
          "original_form": "새우튀김도 좋아",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008141.1.2",
          "form": "오징어튀김 요즘 비싸서 보기도 힘들어",
          "original_form": "오징어튀김 요즘 비싸서 보기도 힘들어",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008141.1.3",
          "form": "새우튀김 저 진짜 좋아해요",
          "original_form": "새우튀김 저 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008141.1.4",
          "form": "오징어 튀김도...",
          "original_form": "오징어 튀김도...",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008141.1.5",
          "form": "오징어 튀김 그거 아예 냉동식품으로 사서 에어프라이기 돌리는게 더 싸게 치여요",
          "original_form": "오징어 튀김 그거 아예 냉동식품으로 사서 에어프라이기 돌리는게 더 싸게 치여요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008141.1.6",
          "form": "진짜 거기 또 가고싶다",
          "original_form": "진짜 거기 또 가고싶다",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008141.1.7",
          "form": "식단조절하다가 간거라서 많이 못먹은것 같아.. 대전이후 양이 다시 늘었어",
          "original_form": "식단조절하다가 간거라서 많이 못먹은것 같아.. 대전이후 양이 다시 늘었어",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008141.1.8",
          "form": "아나 ㅋㅋㅋㅋㅋ언니 안그래도",
          "original_form": "아나 ㅋㅋㅋㅋㅋ언니 안그래도",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008141.1.9",
          "form": "별로 못드시더라구여 실망이었어요",
          "original_form": "별로 못드시더라구여 실망이었어요",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008141.1.10",
          "form": "너두 자랑할 정도는 아니였는데",
          "original_form": "너두 자랑할 정도는 아니였는데",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008141.1.11",
          "form": "저 언니랑 소곱창이었나 그것두 다시 먹으러 가보고 싶어용 ㅇㅅㅇ 그때 맛났어",
          "original_form": "저 언니랑 소곱창이었나 그것두 다시 먹으러 가보고 싶어용 ㅇㅅㅇ 그때 맛났어",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008141.1.12",
          "form": "뭐 먹었었지?",
          "original_form": "뭐 먹었었지?",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008141.1.13",
          "form": "그때 그거 소곱창이랑 대창이랑...막창이랑 해서 무한리필집이었자나여",
          "original_form": "그때 그거 소곱창이랑 대창이랑...막창이랑 해서 무한리필집이었자나여",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008141.1.14",
          "form": "어디에서 먹었는지 기억이 안난다 그치만 다른 집이라도 가자",
          "original_form": "어디에서 먹었는지 기억이 안난다 그치만 다른 집이라도 가자",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008141.1.15",
          "form": "곱창은 넘 비싸...",
          "original_form": "곱창은 넘 비싸...",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008141.1.16",
          "form": "ㅇㅈ...근데 저 솔직히 소곱창류보다는 소도 막창이 취향이었어요",
          "original_form": "ㅇㅈ...근데 저 솔직히 소곱창류보다는 소도 막창이 취향이었어요",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008141.1.17",
          "form": "그냥 전 돼지막창이 더 취향인듯여",
          "original_form": "그냥 전 돼지막창이 더 취향인듯여",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008141.1.18",
          "form": "돼지막창 무한리필이런데 없나 찾아바여 ㅇㅅㅇ",
          "original_form": "돼지막창 무한리필이런데 없나 찾아바여 ㅇㅅㅇ",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008141.1.19",
          "form": "그런곳은 몰라 ㅋㅋ",
          "original_form": "그런곳은 몰라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008141.1.20",
          "form": "소막창은 별루야?",
          "original_form": "소막창은 별루야?",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008141.1.21",
          "form": "언니들이랑 먹은게 처음이자 마지막이었는데",
          "original_form": "언니들이랑 먹은게 처음이자 마지막이었는데",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008141.1.22",
          "form": "돼지막창 비슷한 맛나서 전 맛있게 잘먹었어용",
          "original_form": "돼지막창 비슷한 맛나서 전 맛있게 잘먹었어용",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008141.1.23",
          "form": "그럼 또 먹어봐",
          "original_form": "그럼 또 먹어봐",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008141.1.24",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008141.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아욧",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아욧",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008141.1.26",
          "form": "맛난거 먹을때",
          "original_form": "맛난거 먹을때",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008141.1.27",
          "form": "저두 델꼬다녀주세욧",
          "original_form": "저두 델꼬다녀주세욧",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008141.1.28",
          "form": "서울와...",
          "original_form": "서울와...",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008141.1.29",
          "form": "서울...4단계잖아요...",
          "original_form": "서울...4단계잖아요...",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008141.1.30",
          "form": "거긴 위험지역이예요...",
          "original_form": "거긴 위험지역이예요...",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.31",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.32",
          "form": "나중에 코로나 끝나믄 생각해볼게욧",
          "original_form": "나중에 코로나 끝나믄 생각해볼게욧",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.33",
          "form": "너무 일상이라서",
          "original_form": "너무 일상이라서",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.34",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ돈많은 졸부가 되어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ돈많은 졸부가 되어서",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.36",
          "form": "맛난음식들 잔뜩 먹으면서 다니고 싶어요 ㅠ",
          "original_form": "맛난음식들 잔뜩 먹으면서 다니고 싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.37",
          "form": "졸부되고싶다",
          "original_form": "졸부되고싶다",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008141.1.38",
          "form": "갑부는 일이 많아",
          "original_form": "갑부는 일이 많아",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008141.1.39",
          "form": "서울 단계 낮아지면 놀러와",
          "original_form": "서울 단계 낮아지면 놀러와",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008141.1.40",
          "form": "같이 명륜진사갈비 먹고",
          "original_form": "같이 명륜진사갈비 먹고",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008141.1.41",
          "form": "이것저것 먹자",
          "original_form": "이것저것 먹자",
