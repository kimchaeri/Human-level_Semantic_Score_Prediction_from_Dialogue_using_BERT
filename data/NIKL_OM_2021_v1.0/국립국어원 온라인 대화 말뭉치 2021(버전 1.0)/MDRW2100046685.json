      "utterance": [
        {
          "id": "MDRW2100046685.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 11:56"
        },
        {
          "id": "MDRW2100046685.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211104 11:56"
        },
        {
          "id": "MDRW2100046685.1.3",
          "form": "요즘 즐겨하시는 취미가 있으신가요?",
          "original_form": "요즘 즐겨하시는 취미가 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 11:56"
        },
        {
          "id": "MDRW2100046685.1.4",
          "form": "요즘 여행 다닙니다",
          "original_form": "요즘 여행 다닙니다",
          "speaker_id": "1",
          "time": "20211104 11:57"
        },
        {
          "id": "MDRW2100046685.1.5",
          "form": "우와!",
          "original_form": "우와!",
          "speaker_id": "2",
          "time": "20211104 11:58"
        },
        {
          "id": "MDRW2100046685.1.6",
          "form": "제일 최근에는",
          "original_form": "제일 최근에는",
          "speaker_id": "2",
          "time": "20211104 11:58"
        },
        {
          "id": "MDRW2100046685.1.7",
          "form": "어디 갔다오셨어요?",
          "original_form": "어디 갔다오셨어요?",
          "speaker_id": "2",
          "time": "20211104 11:58"
        },
        {
          "id": "MDRW2100046685.1.8",
          "form": "2주전에는 서울에 다녀왔습니다",
          "original_form": "2주전에는 서울에 다녀왔습니다",
          "speaker_id": "1",
          "time": "20211104 11:58"
        },
        {
          "id": "MDRW2100046685.1.9",
          "form": "오 서울여행!",
          "original_form": "오 서울여행!",
          "speaker_id": "2",
          "time": "20211104 11:58"
        },
        {
          "id": "MDRW2100046685.1.10",
          "form": "어디가 제일 기억에 남으셨나요 ㅎ.ㅎ",
          "original_form": "어디가 제일 기억에 남으셨나요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20211104 11:59"
        },
        {
          "id": "MDRW2100046685.1.11",
          "form": "아이들과 남산에 갔다왔는데 좋았네요",
          "original_form": "아이들과 남산에 갔다왔는데 좋았네요",
          "speaker_id": "1",
          "time": "20211104 11:59"
        },
        {
          "id": "MDRW2100046685.1.12",
          "form": "남산 ㅠㅠ 제가 진짜 진짜 좋아하는 장소인데",
          "original_form": "남산 ㅠㅠ 제가 진짜 진짜 좋아하는 장소인데",
          "speaker_id": "2",
          "time": "20211104 11:59"
        },
        {
          "id": "MDRW2100046685.1.13",
          "form": "그 바로 밑에 해방촌이라는 곳도",
          "original_form": "그 바로 밑에 해방촌이라는 곳도",
          "speaker_id": "2",
          "time": "20211104 11:59"
        },
        {
          "id": "MDRW2100046685.1.14",
          "form": "갔다오셨나요 혹시?!",
          "original_form": "갔다오셨나요 혹시?!",
          "speaker_id": "2",
          "time": "20211104 11:59"
        },
        {
          "id": "MDRW2100046685.1.15",
          "form": "거기는 안갔네요",
          "original_form": "거기는 안갔네요",
          "speaker_id": "1",
          "time": "20211105 00:01"
        },
        {
          "id": "MDRW2100046685.1.16",
          "form": "다음에는 해방촌도 가보세요",
          "original_form": "다음에는 해방촌도 가보세요",
          "speaker_id": "2",
          "time": "20211105 00:02"
        },
        {
          "id": "MDRW2100046685.1.17",
          "form": "제가 서울에서",
          "original_form": "제가 서울에서",
          "speaker_id": "2",
          "time": "20211105 00:02"
        },
        {
          "id": "MDRW2100046685.1.18",
          "form": "제일 좋아하는 동네에요 ㅎㅎ",
          "original_form": "제일 좋아하는 동네에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:02"
        },
        {
          "id": "MDRW2100046685.1.19",
          "form": "남산에서 걸어내려오시면 바로 시작일거에요!",
          "original_form": "남산에서 걸어내려오시면 바로 시작일거에요!",
          "speaker_id": "2",
          "time": "20211105 00:02"
        },
        {
          "id": "MDRW2100046685.1.20",
          "form": "그렇군요가고 싶네요",
          "original_form": "그렇군요가고 싶네요",
          "speaker_id": "1",
          "time": "20211105 00:04"
        },
        {
          "id": "MDRW2100046685.1.21",
          "form": "네네 맛집도 많고 와인이나 칵테일 한 잔하기 좋은 바도 많으니까 다음에는 꼭 가보세욤 ㅎㅎ",
          "original_form": "네네 맛집도 많고 와인이나 칵테일 한 잔하기 좋은 바도 많으니까 다음에는 꼭 가보세욤 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:05"
        }
      ]
