      "utterance": [
        {
          "id": "MDRW2100000867.1.1",
          "form": "오늘 바빴어?",
          "original_form": "오늘 바빴어?",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000867.1.2",
          "form": "엄청 바빴지롱~",
          "original_form": "엄청 바빴지롱~",
          "speaker_id": "2",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000867.1.3",
          "form": "외근 했어 아님 계속 사무실?",
          "original_form": "외근 했어 아님 계속 사무실?",
          "speaker_id": "1",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000867.1.4",
          "form": "밖에서 일하는데 날도 덥고 습해서 힘들었지롱",
          "original_form": "밖에서 일하는데 날도 덥고 습해서 힘들었지롱",
          "speaker_id": "2",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000867.1.5",
          "form": "고생많았겠네..",
          "original_form": "고생많았겠네..",
          "speaker_id": "1",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000867.1.6",
          "form": "괜찮아 ~\r\n더워서 아이스커피도 사먹고 쉬엄쉬엄했어",
          "original_form": "괜찮아 ~\r\n더워서 아이스커피도 사먹고 쉬엄쉬엄했어",
          "speaker_id": "2",
          "time": "20210819 20:14"
        },
        {
          "id": "MDRW2100000867.1.7",
          "form": "이렇게 고생하는데 사람들이 알아주나",
          "original_form": "이렇게 고생하는데 사람들이 알아주나",
          "speaker_id": "1",
          "time": "20210819 20:14"
        },
        {
          "id": "MDRW2100000867.1.8",
          "form": "그냥 열심히 하는거지 뭐",
          "original_form": "그냥 열심히 하는거지 뭐",
          "speaker_id": "2",
          "time": "20210819 20:14"
        },
        {
          "id": "MDRW2100000867.1.9",
          "form": "열심히 하면 복 받는데",
          "original_form": "열심히 하면 복 받는데",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000867.1.10",
          "form": "이번에 급여가 오르나",
          "original_form": "이번에 급여가 오르나",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000867.1.11",
          "form": "연봉계약할때 조금 올려 줄듯 함",
          "original_form": "연봉계약할때 조금 올려 줄듯 함",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000867.1.12",
          "form": "오~~ 나이스네",
          "original_form": "오~~ 나이스네",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000867.1.13",
          "form": "사장이 맨날 회사가 힘들다고 하네",
          "original_form": "사장이 맨날 회사가 힘들다고 하네",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000867.1.14",
          "form": "내가 더 힘든데",
          "original_form": "내가 더 힘든데",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000867.1.15",
          "form": "사장은 외제차 이번에 바꿨어",
          "original_form": "사장은 외제차 이번에 바꿨어",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000867.1.16",
          "form": "사장들은 자기들이 제일 힘들다 생각하겠지",
          "original_form": "사장들은 자기들이 제일 힘들다 생각하겠지",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000867.1.17",
          "form": "월급 올려줄때 되면 맨날 힘들다고 해 ㅋ",
          "original_form": "월급 올려줄때 되면 맨날 힘들다고 해 ㅋ",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000867.1.18",
          "form": "사장도 월급이 오르나",
          "original_form": "사장도 월급이 오르나",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000867.1.19",
          "form": "아니 남는돈 나 사장꺼야",
          "original_form": "아니 남는돈 나 사장꺼야",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000867.1.20",
          "form": "오 부럽다",
          "original_form": "오 부럽다",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000867.1.21",
          "form": "직원들도 고생 많이 하는데 월급 좀 많이 올려주면 좋겠다",
          "original_form": "직원들도 고생 많이 하는데 월급 좀 많이 올려주면 좋겠다",
          "speaker_id": "2",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000867.1.22",
          "form": "그니까... 고생하는거 알기나 할까",
          "original_form": "그니까... 고생하는거 알기나 할까",
          "speaker_id": "1",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000867.1.23",
          "form": "알아주기만 해도 감지덕지~",
          "original_form": "알아주기만 해도 감지덕지~",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000867.1.24",
          "form": "오늘은 무슨일 했어?",
          "original_form": "오늘은 무슨일 했어?",
          "speaker_id": "1",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000867.1.25",
          "form": "오늘은 현장에서 일했지",
          "original_form": "오늘은 현장에서 일했지",
          "speaker_id": "2",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000867.1.26",
          "form": "현장에서는 계속 밖에서 있었어?  쉴곳은 있나",
          "original_form": "현장에서는 계속 밖에서 있었어?  쉴곳은 있나",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000867.1.27",
          "form": "더워서 그늘에 피해 있었어",
          "original_form": "더워서 그늘에 피해 있었어",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000867.1.28",
          "form": "혼자갔어?",
          "original_form": "혼자갔어?",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000867.1.29",
          "form": "나는 관리자라 중간중간 진행사항 체크만 하면돼",
          "original_form": "나는 관리자라 중간중간 진행사항 체크만 하면돼",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000867.1.30",
          "form": "부하직원이랑 같이 같지",
          "original_form": "부하직원이랑 같이 같지",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000867.1.31",
          "form": "고생이 많네 우리 남편",
          "original_form": "고생이 많네 우리 남편",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000867.1.32",
          "form": "괜찮아~ 여봉이가 있으니깐 ~",
          "original_form": "괜찮아~ 여봉이가 있으니깐 ~",
          "speaker_id": "2",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000867.1.33",
          "form": "나도 오늘 일한다고 힘들었단다",
          "original_form": "나도 오늘 일한다고 힘들었단다",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000867.1.34",
          "form": "오늘은 컴퓨터작업 많이 했어1",
          "original_form": "오늘은 컴퓨터작업 많이 했어1",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000867.1.35",
          "form": "컴퓨터로 일하니까 눈이랑 허리가 너무 아프네...",
          "original_form": "컴퓨터로 일하니까 눈이랑 허리가 너무 아프네...",
          "speaker_id": "1",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000867.1.36",
          "form": "고생 많네 우리 여봉이\r\n눈이 금방 아프니깐 자주 쉬어야됑",
          "original_form": "고생 많네 우리 여봉이\r\n눈이 금방 아프니깐 자주 쉬어야됑",
          "speaker_id": "2",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000867.1.37",
          "form": "그래도 이렇게 힘든 시국에 일할수 있다는게 어디야",
          "original_form": "그래도 이렇게 힘든 시국에 일할수 있다는게 어디야",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000867.1.38",
          "form": "맞다 코로나 때문에 일자리 잃은 사람도 많다",
          "original_form": "맞다 코로나 때문에 일자리 잃은 사람도 많다",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000867.1.39",
          "form": "그니까... 4대보험도 들어주고 감사하지",
          "original_form": "그니까... 4대보험도 들어주고 감사하지",
          "speaker_id": "1",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000867.1.40",
          "form": "맞아 그래도 요즘은 작은회사라도 거의다 4대보험 가입 하더라구\r\n많이 바뀐거 같아",
          "original_form": "맞아 그래도 요즘은 작은회사라도 거의다 4대보험 가입 하더라구\r\n많이 바뀐거 같아",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000867.1.41",
          "form": "나는 혼자서 재택근무 하니까 스트레스받는건 없는데.자기는 직장동료 스트레스 없어?",
          "original_form": "나는 혼자서 재택근무 하니까 스트레스받는건 없는데.자기는 직장동료 스트레스 없어?",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000867.1.42",
          "form": "다른사람은 다 좋은데 위에 상사가 영~ 맘이 안맞아",
          "original_form": "다른사람은 다 좋은데 위에 상사가 영~ 맘이 안맞아",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000867.1.43",
          "form": "아 그 상사??  아직도야?",
          "original_form": "아 그 상사??  아직도야?",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000867.1.44",
          "form": "너무 이기적인 스타일이라 다른 사람들과도 유대관계가 잘 안돼",
          "original_form": "너무 이기적인 스타일이라 다른 사람들과도 유대관계가 잘 안돼",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000867.1.45",
          "form": "힘들겠다.. 가족보다 더 오래 봐야 하는 사람인데.. 고생한다",
          "original_form": "힘들겠다.. 가족보다 더 오래 봐야 하는 사람인데.. 고생한다",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000867.1.46",
          "form": "그러게 사무실에 그 상사만 있으면 분위기가 조용해져~",
          "original_form": "그러게 사무실에 그 상사만 있으면 분위기가 조용해져~",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000867.1.47",
          "form": "어떡하니..  분위기 망치는 사람이구나",
          "original_form": "어떡하니..  분위기 망치는 사람이구나",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000867.1.48",
          "form": "이기적인가",
          "original_form": "이기적인가",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000867.1.49",
          "form": "엄청 이기적인데 말투도 듣기 거북해서 다들 피해다녀",
          "original_form": "엄청 이기적인데 말투도 듣기 거북해서 다들 피해다녀",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000867.1.50",
          "form": "다들 싫어하는데 본인은 알까?",
          "original_form": "다들 싫어하는데 본인은 알까?",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000867.1.51",
          "form": "전혀 그런거 신경 안쓰던데\r\n일도 잘 못해서 올 해 계약 끝나면 짤릴거래",
          "original_form": "전혀 그런거 신경 안쓰던데\r\n일도 잘 못해서 올 해 계약 끝나면 짤릴거래",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000867.1.52",
          "form": "음 약간 안됐네",
          "original_form": "음 약간 안됐네",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000867.1.53",
          "form": "잘해드려라",
          "original_form": "잘해드려라",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000867.1.54",
          "form": "다 자업자득이지 뭐",
          "original_form": "다 자업자득이지 뭐",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000867.1.55",
          "form": "인과응보랄까?",
          "original_form": "인과응보랄까?",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000867.1.56",
          "form": "그러게..",
          "original_form": "그러게..",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000867.1.57",
          "form": "우리는 착하게 더불어 살자",
          "original_form": "우리는 착하게 더불어 살자",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
