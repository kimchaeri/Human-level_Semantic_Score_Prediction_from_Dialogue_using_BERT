      "utterance": [
        {
          "id": "MDRW2100038031.1.1",
          "form": "저희 애도 10kg가 넘어서 걱정했는데",
          "original_form": "저희 애도 10kg가 넘어서 걱정했는데",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038031.1.2",
          "form": "애가 거묘라서 다행히 근육이 많대요 ㅎㅎ",
          "original_form": "애가 거묘라서 다행히 근육이 많대요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038031.1.3",
          "form": "어모 튼튼해라",
          "original_form": "어모 튼튼해라",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038031.1.4",
          "form": "튼튼 유전자 있나봐요",
          "original_form": "튼튼 유전자 있나봐요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038031.1.5",
          "form": "진짜 큰고양이 유전자 ㅎㅎ",
          "original_form": "진짜 큰고양이 유전자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038031.1.6",
          "form": "우리애들은 7키로 넘었는데 어찌 그리 무거운지",
          "original_form": "우리애들은 7키로 넘었는데 어찌 그리 무거운지",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100038031.1.7",
          "form": "그럼 다이어트 사료 먹나요?",
          "original_form": "그럼 다이어트 사료 먹나요?",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100038031.1.8",
          "form": "일반 사료 먹어요!",
          "original_form": "일반 사료 먹어요!",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100038031.1.9",
          "form": "크기에 비해서 평균체중이라고 하셔서요 ㅎㅎ",
          "original_form": "크기에 비해서 평균체중이라고 하셔서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100038031.1.10",
          "form": "우와 뼈대가 큰가봐요",
          "original_form": "우와 뼈대가 큰가봐요",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100038031.1.11",
          "form": "맞아요ㅠ 중형견 같아요 무슨 ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅠ 중형견 같아요 무슨 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038031.1.12",
          "form": "ㅋㅋㅋㅋ 한번 병원 갈라치면 힘들겠어요",
          "original_form": "ㅋㅋㅋㅋ 한번 병원 갈라치면 힘들겠어요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038031.1.13",
          "form": "네 너무 무겁습니다..",
          "original_form": "네 너무 무겁습니다..",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100038031.1.14",
          "form": "진짜 중형견 보다는 작은 편인데도 이런데, 다른 분들은 어떻게 데려가시는 걸까요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 중형견 보다는 작은 편인데도 이런데, 다른 분들은 어떻게 데려가시는 걸까요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100038031.1.15",
          "form": "자차라도 있음 좋을텐데 ㅠㅠ",
          "original_form": "자차라도 있음 좋을텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038031.1.16",
          "form": "저는 두마리라 이고지고가요 ㅋㅋㅋㅋ",
          "original_form": "저는 두마리라 이고지고가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038031.1.17",
          "form": "하나는 들고, 하나는 메고 가시나요 ㅎㅎ",
          "original_form": "하나는 들고, 하나는 메고 가시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038031.1.18",
          "form": "애기들 둘이라 갈 때 더 힘드시겠어요ㅠㅠ",
          "original_form": "애기들 둘이라 갈 때 더 힘드시겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038031.1.19",
          "form": "애기들 병원을 잘 가나요??",
          "original_form": "애기들 병원을 잘 가나요??",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038031.1.20",
          "form": "네 메고 약간 끄는 캐리어같은 가방이랑요 ㅎㅎ",
          "original_form": "네 메고 약간 끄는 캐리어같은 가방이랑요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038031.1.21",
          "form": "턱밑에 뭐가나서",
          "original_form": "턱밑에 뭐가나서",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038031.1.22",
          "form": "첫째를 메고 버스타고 갔는데",
          "original_form": "첫째를 메고 버스타고 갔는데",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038031.1.23",
          "form": "울어서 너무 당황했어요 ㅎㅎ",
          "original_form": "울어서 너무 당황했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038031.1.24",
          "form": "아이고ㅠㅠ 잘못된 건 아니지만 난감하셨겠어요ㅠㅠ",
          "original_form": "아이고ㅠㅠ 잘못된 건 아니지만 난감하셨겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038031.1.25",
          "form": "애기가 많이 무서웠나봐요ㅠㅠ",
          "original_form": "애기가 많이 무서웠나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038031.1.26",
          "form": "그러게요 ㅠㅠ 집에서도 말많은 애라 그런지",
          "original_form": "그러게요 ㅠㅠ 집에서도 말많은 애라 그런지",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038031.1.27",
          "form": "갑자기 말문 트임 ㅋㅋㅋㅋ",
          "original_form": "갑자기 말문 트임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038031.1.28",
          "form": "버스안에서 ㅜㅜ",
          "original_form": "버스안에서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038031.1.29",
          "form": "짜증나서 화내는 거였을까요 ㅋㅋㅋㅋ",
          "original_form": "짜증나서 화내는 거였을까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038031.1.30",
          "form": "저희 애는 심기가 불편한게 얼굴에 보여서 웃긴에 귀여워요 ㅋㅋㅋㅋㅋ",
