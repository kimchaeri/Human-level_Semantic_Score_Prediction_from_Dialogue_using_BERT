      "utterance": [
        {
          "id": "MDRW2100026377.1.1",
          "form": "금손 어머님 옆에있으셔서 좋으시겠다",
          "original_form": "금손 어머님 옆에있으셔서 좋으시겠다",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026377.1.2",
          "form": "왠지 name2도 잘하실거 같아요",
          "original_form": "왠지 &name2&도 잘하실거 같아요",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026377.1.3",
          "form": "ㅎㅎㅎㅎ덕분에 건강한 돼지가 되어 가고 있답니다",
          "original_form": "ㅎㅎㅎㅎ덕분에 건강한 돼지가 되어 가고 있답니다",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026377.1.4",
          "form": "요리하는 걸 좋아하긴 하는데 저는 양식위주인 것 같아요!",
          "original_form": "요리하는 걸 좋아하긴 하는데 저는 양식위주인 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026377.1.5",
          "form": "name1은 요리 좋아하시나요?",
          "original_form": "&name1&은 요리 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026377.1.6",
          "form": "좋아해요 잘하지는 못하지만요",
          "original_form": "좋아해요 잘하지는 못하지만요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026377.1.7",
          "form": "어떤 요리를 주로 하시나요?",
          "original_form": "어떤 요리를 주로 하시나요?",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026377.1.8",
          "form": "제일 자주하는건 닭도리탕이예요",
          "original_form": "제일 자주하는건 닭도리탕이예요",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026377.1.9",
          "form": "자취생의 소중한 단백질...",
          "original_form": "자취생의 소중한 단백질...",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026377.1.10",
          "form": "우와 닭도리탕이라니",
          "original_form": "우와 닭도리탕이라니",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026377.1.11",
          "form": "멋있어요!!",
          "original_form": "멋있어요!!",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026377.1.12",
          "form": "한식을 자주해요 역시 한식이 한끼먹기좋아서요",
          "original_form": "한식을 자주해요 역시 한식이 한끼먹기좋아서요",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026377.1.13",
          "form": "그쵸 한식이 든든하고 속편하게 한끼 먹기 너무 좋죠",
          "original_form": "그쵸 한식이 든든하고 속편하게 한끼 먹기 너무 좋죠",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026377.1.14",
          "form": "콩불이나 김볶 자주먹어요",
          "original_form": "콩불이나 김볶 자주먹어요",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026377.1.15",
          "form": "냉장고 야채청소할때",
          "original_form": "냉장고 야채청소할때",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026377.1.16",
          "form": "샌드위치나 월남쌍도 먹어요",
          "original_form": "샌드위치나 월남쌍도 먹어요",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026377.1.17",
          "form": "김볶이라니.. 이 밤에 넘 맛있을 것 같네요!!",
          "original_form": "김볶이라니.. 이 밤에 넘 맛있을 것 같네요!!",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026377.1.18",
          "form": "충김볶 아시나요?",
          "original_form": "충김볶 아시나요?",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026377.1.19",
          "form": "최고예요",
          "original_form": "최고예요",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026377.1.20",
          "form": "충김볶은 뭔가요??",
          "original_form": "충김볶은 뭔가요??",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026377.1.21",
          "form": "충격의 김치볶음밥 이라고 인터넷에서 유명한 메뉴가있는데",
          "original_form": "충격의 김치볶음밥 이라고 인터넷에서 유명한 메뉴가있는데",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026377.1.22",
          "form": "언제먹어도 맛있어요",
          "original_form": "언제먹어도 맛있어요",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026377.1.23",
          "form": "오오 한번 찾아봐야겠네요!! 저는 그 강식당에 김치밥이 피었습니다? 이게 최고였는데",
          "original_form": "오오 한번 찾아봐야겠네요!! 저는 그 강식당에 김치밥이 피었습니다? 이게 최고였는데",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026377.1.24",
          "form": "한번 시도해봐야겠네요",
          "original_form": "한번 시도해봐야겠네요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026377.1.25",
          "form": "그것도 너무 맛있죠!",
          "original_form": "그것도 너무 맛있죠!",
          "speaker_id": "1",
          "time": "20210901 22:08"
        }
      ]
    }
