          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031630.1.5",
          "form": "신청해야하는데",
          "original_form": "신청해야하는데",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031630.1.6",
          "form": "쩐다",
          "original_form": "쩐다",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031630.1.7",
          "form": "너는 얼마 받아",
          "original_form": "너는 얼마 받아",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031630.1.8",
          "form": "난 못받지",
          "original_form": "난 못받지",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031630.1.9",
          "form": "근로자가 아니잖아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근로자가 아니잖아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031630.1.10",
          "form": "아 그렇네",
          "original_form": "아 그렇네",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031630.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031630.1.12",
          "form": "근데 생각보다 엄청많이받네",
          "original_form": "근데 생각보다 엄청많이받네",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031630.1.13",
          "form": "난 한 10만원 주는줄",
          "original_form": "난 한 10만원 주는줄",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031630.1.14",
          "form": "다음주엔 우리 그거도 받잖아",
          "original_form": "다음주엔 우리 그거도 받잖아",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031630.1.15",
          "form": "상생지원금 25만원",
          "original_form": "상생지원금 25만원",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031630.1.16",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031630.1.17",
          "form": "근데 어떤사람",
          "original_form": "근데 어떤사람",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031630.1.18",
          "form": "알바하는데",
          "original_form": "알바하는데",
          "speaker_id": "2",
