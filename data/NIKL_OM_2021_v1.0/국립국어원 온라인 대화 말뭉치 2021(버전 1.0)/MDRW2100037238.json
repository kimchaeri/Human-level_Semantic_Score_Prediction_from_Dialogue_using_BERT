      "utterance": [
        {
          "id": "MDRW2100037238.1.1",
          "form": "요즘은 20대 초반 커플들은",
          "original_form": "요즘은 20대 초반 커플들은",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037238.1.2",
          "form": "거의 다 반반 내고 데이트 한대요",
          "original_form": "거의 다 반반 내고 데이트 한대요",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037238.1.3",
          "form": "그런데 이상한 게",
          "original_form": "그런데 이상한 게",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037238.1.4",
          "form": "괜찮다는게 그냥적당히 직장만 있으면 다 괜찮대요",
          "original_form": "괜찮다는게 그냥적당히 직장만 있으면 다 괜찮대요",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037238.1.5",
          "form": "아니그니까요",
          "original_form": "아니그니까요",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037238.1.6",
          "form": "여자가 꾸미는 돈도많이들고 손해에요",
          "original_form": "여자가 꾸미는 돈도많이들고 손해에요",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037238.1.7",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037238.1.8",
          "form": "성병 걱정이라든지",
          "original_form": "성병 걱정이라든지",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037238.1.9",
          "form": "저는 예전에 연애할 때",
          "original_form": "저는 예전에 연애할 때",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037238.1.10",
          "form": "생리가 2주 넘게 늦어져서",
          "original_form": "생리가 2주 넘게 늦어져서",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037238.1.11",
          "form": "엄청 걱정했거든요?",
          "original_form": "엄청 걱정했거든요?",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037238.1.12",
          "form": "근데 전남친이 하는 말이",
          "original_form": "근데 전남친이 하는 말이",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.13",
          "form": "네 그래서요? 남자가 또 뻘짓하나요",
          "original_form": "네 그래서요? 남자가 또 뻘짓하나요",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.15",
          "form": "너가 걱정하니까 더 늦는거야",
          "original_form": "너가 걱정하니까 더 늦는거야",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.16",
          "form": "그렇게 걱정한다고 달라지는건 없어..",
          "original_form": "그렇게 걱정한다고 달라지는건 없어..",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.17",
          "form": "뭔 헛소리를 열심히 하네요",
          "original_form": "뭔 헛소리를 열심히 하네요",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.18",
          "form": "아니 왜 또 나쁜 말이래요",
          "original_form": "아니 왜 또 나쁜 말이래요",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.19",
          "form": "진짜 pregnant면 자기가 책임지겠대요",
          "original_form": "진짜 pregnant면 자기가 책임지겠대요",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.20",
          "form": "아니근데",
          "original_form": "아니근데",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037238.1.21",
          "form": "출산 누가하나요",
          "original_form": "출산 누가하나요",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.22",
          "form": "여자가 목숨걸고 하는건데",
          "original_form": "여자가 목숨걸고 하는건데",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.23",
          "form": "그런거도 그냥 무마하는 말일거라 생각해요",
          "original_form": "그런거도 그냥 무마하는 말일거라 생각해요",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.24",
          "form": "제말이요",
          "original_form": "제말이요",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.25",
          "form": "아니그니까요",
          "original_form": "아니그니까요",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.26",
          "form": "그리고 웃긴게",
          "original_form": "그리고 웃긴게",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.27",
          "form": "그남자랑 낳기싫은데 ㅋㅋㅋ",
          "original_form": "그남자랑 낳기싫은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.28",
          "form": "목숨 걸어서 낳아도 남편 성씨 따르잖아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "목숨 걸어서 낳아도 남편 성씨 따르잖아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037238.1.29",
          "form": "그니까요 우월하지도 않은 유전자면서",
          "original_form": "그니까요 우월하지도 않은 유전자면서",
          "speaker_id": "2",
          "time": "20210831 21:50"
        }
      ]
