      "utterance": [
        {
          "id": "MDRW2100022289.1.1",
          "form": "저 마라샹궈 좋아해요",
          "original_form": "저 마라샹궈 좋아해요",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022289.1.2",
          "form": "그리고 꿔바로우도 같이~",
          "original_form": "그리고 꿔바로우도 같이~",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022289.1.3",
          "form": "마라어때요",
          "original_form": "마라어때요",
          "speaker_id": "1",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022289.1.4",
          "form": "마라 핵매워요",
          "original_form": "마라 핵매워요",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022289.1.5",
          "form": "저 마라안먹어봤어쇼",
          "original_form": "저 마라안먹어봤어쇼",
          "speaker_id": "1",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022289.1.6",
          "form": "그거 먹으면 배에서 난리가 나요",
          "original_form": "그거 먹으면 배에서 난리가 나요",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022289.1.7",
          "form": "향신료 향은 안강한가요",
          "original_form": "향신료 향은 안강한가요",
          "speaker_id": "1",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022289.1.8",
          "form": "근데 알싸하게 매워서 인위적이진 않아요",
          "original_form": "근데 알싸하게 매워서 인위적이진 않아요",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.9",
          "form": "향신료 향이 강한편이에요 ㅠ",
          "original_form": "향신료 향이 강한편이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.10",
          "form": "앗 그럼 전 못 먹게써요",
          "original_form": "앗 그럼 전 못 먹게써요",
          "speaker_id": "1",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.11",
          "form": "향 강한거 잘 못먹어요?",
          "original_form": "향 강한거 잘 못먹어요?",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.12",
          "form": "전 향신료 강한걸 못먹습니다",
          "original_form": "전 향신료 강한걸 못먹습니다",
          "speaker_id": "1",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.13",
          "form": "그럼 고수 같은 것도 싫어하겠네요",
          "original_form": "그럼 고수 같은 것도 싫어하겠네요",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.14",
          "form": "고수 쯔란 못먹어요",
          "original_form": "고수 쯔란 못먹어요",
          "speaker_id": "1",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.15",
          "form": "고수드세요?",
          "original_form": "고수드세요?",
          "speaker_id": "1",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022289.1.16",
          "form": "허걱... 쯔란도요 ?ㅠㅠㅠ",
          "original_form": "허걱... 쯔란도요 ?ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:10"
        }
