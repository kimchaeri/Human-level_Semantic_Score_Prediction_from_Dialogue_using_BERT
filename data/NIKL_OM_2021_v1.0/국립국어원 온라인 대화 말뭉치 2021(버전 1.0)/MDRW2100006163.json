      "utterance": [
        {
          "id": "MDRW2100006163.1.1",
          "form": "가을엔 전어가 아니라 꽃게지",
          "original_form": "가을엔 전어가 아니라 꽃게지",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006163.1.2",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006163.1.3",
          "form": "꽃게 얘기하니까 아주 반응이 엄청나구만",
          "original_form": "꽃게 얘기하니까 아주 반응이 엄청나구만",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006163.1.4",
          "form": "꽃게탕 먹고파",
          "original_form": "꽃게탕 먹고파",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006163.1.5",
          "form": "난 게장이여 ㅎㅎ",
          "original_form": "난 게장이여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006163.1.6",
          "form": "게장먹은지도 진짜 오래됐다",
          "original_form": "게장먹은지도 진짜 오래됐다",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006163.1.7",
          "form": "코로나땜에 진짜ㅜㅜ",
          "original_form": "코로나땜에 진짜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006163.1.8",
          "form": "안산에 그 게장집 자주갔었는데",
          "original_form": "안산에 그 게장집 자주갔었는데",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006163.1.9",
          "form": "진짜 코로나 터지고 안산에서 데이트를 거의 안한거 같은데",
          "original_form": "진짜 코로나 터지고 안산에서 데이트를 거의 안한거 같은데",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006163.1.10",
          "form": "응 ㅜㅜ",
          "original_form": "응 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006163.1.11",
          "form": "안산에 게장 맛집 많은데",
          "original_form": "안산에 게장 맛집 많은데",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006163.1.12",
          "form": "거기 아직 다 그대로 있을라나",
          "original_form": "거기 아직 다 그대로 있을라나",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006163.1.13",
          "form": "게장 먹고파여",
          "original_form": "게장 먹고파여",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006163.1.14",
          "form": "이제 날도 선선해져서",
          "original_form": "이제 날도 선선해져서",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006163.1.15",
          "form": "게장 먹어도 되니까",
          "original_form": "게장 먹어도 되니까",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006163.1.16",
          "form": "가장",
          "original_form": "가장",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006163.1.17",
          "form": "앙 조아여",
          "original_form": "앙 조아여",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006163.1.18",
          "form": "그리고 마카롱이랑 커피 마시러가장",
          "original_form": "그리고 마카롱이랑 커피 마시러가장",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006163.1.19",
          "form": "앙 조아여",
          "original_form": "앙 조아여",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006163.1.20",
          "form": "옛날엔 회도 많이 먹으러 다녔는데",
          "original_form": "옛날엔 회도 많이 먹으러 다녔는데",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006163.1.21",
          "form": "요즘은 안간지 엄청 오래됐네",
          "original_form": "요즘은 안간지 엄청 오래됐네",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006163.1.22",
          "form": "그러게",
          "original_form": "그러게",
