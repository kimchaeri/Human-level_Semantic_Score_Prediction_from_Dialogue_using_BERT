      "utterance": [
        {
          "id": "MDRW2100021895.1.1",
          "form": "즐거운 데이트 하셨나여 ㅋㅋㅋ",
          "original_form": "즐거운 데이트 하셨나여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021895.1.2",
          "form": "어찌아셨어요?ㅋㅋ",
          "original_form": "어찌아셨어요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021895.1.3",
          "form": "넹? 아 아까 오늘 만나러 간다고 하셔서",
          "original_form": "넹? 아 아까 오늘 만나러 간다고 하셔서",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021895.1.4",
          "form": "아 ㅋㅋㅋㅋ 정신없네여",
          "original_form": "아 ㅋㅋㅋㅋ 정신없네여",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021895.1.5",
          "form": "백신맞은후라 여친이 운전기사 해줬어요 ㅋㅋㅋ",
          "original_form": "백신맞은후라 여친이 운전기사 해줬어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021895.1.6",
          "form": "오ㅋㅋㅋ 백신맞으셨음 쉬셔야겠네여 ㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋ 백신맞으셨음 쉬셔야겠네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021895.1.7",
          "form": "이제 집이니까 침대위에서 뒹굴거리며 대화하다가 자야죠 ㅋㅋㅋ",
          "original_form": "이제 집이니까 침대위에서 뒹굴거리며 대화하다가 자야죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021895.1.8",
          "form": "낼 데이트시죠?",
          "original_form": "낼 데이트시죠?",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021895.1.9",
          "form": "오 네 맞아요! 드디어 데이트날!",
          "original_form": "오 네 맞아요! 드디어 데이트날!",
          "speaker_id": "1",
          "time": "20210829 08:20"
        },
        {
          "id": "MDRW2100021895.1.10",
          "form": "데이트 잘하시구 이따가 말걸어주셔요 ㅎㅎ",
          "original_form": "데이트 잘하시구 이따가 말걸어주셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100021895.1.11",
          "form": "네ㅋㅋㅋㅋ어차피 저녁때 데이트라서",
          "original_form": "네ㅋㅋㅋㅋ어차피 저녁때 데이트라서",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021895.1.12",
          "form": "지금부터 준비하고 계시는거 아니에요?ㅋㅋㅋ",
          "original_form": "지금부터 준비하고 계시는거 아니에요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100021895.1.13",
          "form": "ㅋㅋㅋㅋㅋ저녁인데 벌써 준비하면 진빠져요",
          "original_form": "ㅋㅋㅋㅋㅋ저녁인데 벌써 준비하면 진빠져요",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100021895.1.14",
          "form": "준비 두시간정도?",
          "original_form": "준비 두시간정도?",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100021895.1.15",
          "form": "꽃단장하셔야하니",
          "original_form": "꽃단장하셔야하니",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100021895.1.16",
          "form": "ㅋㅋㅋㅋㅋ네 두시간 준비 시간으로 잡고 좀 뒹굴거릴려고요",
          "original_form": "ㅋㅋㅋㅋㅋ네 두시간 준비 시간으로 잡고 좀 뒹굴거릴려고요",
          "speaker_id": "1",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100021895.1.17",
          "form": "역시 ㅋㅋ",
          "original_form": "역시 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100021895.1.18",
          "form": "제동생 남친생기면 바로 알아차려요",
          "original_form": "제동생 남친생기면 바로 알아차려요",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100021895.1.19",
          "form": "뭔 준비를 그렇게 오래하는지",
          "original_form": "뭔 준비를 그렇게 오래하는지",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100021895.1.20",
          "form": "나 남친생겼어 라고 말하는거 같은 준비시간",
          "original_form": "나 남친생겼어 라고 말하는거 같은 준비시간",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100021895.1.21",
          "form": "저희집 식구들고 바로 알아차리더라규요",
          "original_form": "저희집 식구들고 바로 알아차리더라규요",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100021895.1.22",
          "form": "평소엔 30분컷으로 후다닥 나갈수도 있는데",
          "original_form": "평소엔 30분컷으로 후다닥 나갈수도 있는데",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100021895.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100021895.1.24",
          "form": "들켰어요?",
          "original_form": "들켰어요?",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100021895.1.25",
          "form": "일주일인데?",
          "original_form": "일주일인데?",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100021895.1.26",
          "form": "부모님 소개로 만난거라",
          "original_form": "부모님 소개로 만난거라",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100021895.1.27",
          "form": "뭔가 시작부터 다 알고있는게 너무 부담이지만ㅋㅋㅋㅋ",
          "original_form": "뭔가 시작부터 다 알고있는게 너무 부담이지만ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100021895.1.28",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100021895.1.29",
          "form": "부모님 소개였어요?",
          "original_form": "부모님 소개였어요?",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100021895.1.30",
          "form": "그럼 선...?",
          "original_form": "그럼 선...?",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100021895.1.31",
          "form": "넼ㅋㅋㅋ 선개팅 같은 느낌?",
          "original_form": "넼ㅋㅋㅋ 선개팅 같은 느낌?",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100021895.1.32",
          "form": "중간에 중매해준 분이 계세영!!",
          "original_form": "중간에 중매해준 분이 계세영!!",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100021895.1.33",
          "form": "헛 그랬구나",
          "original_form": "헛 그랬구나",
          "speaker_id": "2",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100021895.1.34",
          "form": "결혼으로 더 빨리 골인하시겠는데요?",
          "original_form": "결혼으로 더 빨리 골인하시겠는데요?",
