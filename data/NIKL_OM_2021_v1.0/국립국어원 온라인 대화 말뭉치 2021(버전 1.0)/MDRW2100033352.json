        },
        {
          "id": "MDRW2100033352.1.2",
          "form": "와 name1다 하이하이~~",
          "original_form": "와 &name1&다 하이하이~~",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033352.1.3",
          "form": "나 요즘 뜻하지않은 육아중",
          "original_form": "나 요즘 뜻하지않은 육아중",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033352.1.4",
          "form": "갱얼쥐육아",
          "original_form": "갱얼쥐육아",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033352.1.5",
          "form": "아 그치 그것도 육아지",
          "original_form": "아 그치 그것도 육아지",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033352.1.6",
          "form": "강아지 많이 컸나요",
          "original_form": "강아지 많이 컸나요",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033352.1.7",
          "form": "예...",
          "original_form": "예...",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033352.1.8",
          "form": "아니 이게 진짜웃긴게",
          "original_form": "아니 이게 진짜웃긴게",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033352.1.9",
          "form": "애기들 한눈팔면 사고친다하잖아",
          "original_form": "애기들 한눈팔면 사고친다하잖아",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033352.1.10",
          "form": "아기갱얼쥐도 똑같애...",
          "original_form": "아기갱얼쥐도 똑같애...",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033352.1.11",
          "form": "아니근데 이거 언제제출하지 나 타이밍을 모르겠는데",
          "original_form": "아니근데 이거 언제제출하지 나 타이밍을 모르겠는데",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033352.1.12",
          "form": "아무튼 방금도 강아지 사고쳐서 수습하고왔다^-^",
          "original_form": "아무튼 방금도 강아지 사고쳐서 수습하고왔다^-^",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033352.1.13",
          "form": "강아지 키우는 것도 걍 애 키우는 거구나...",
          "original_form": "강아지 키우는 것도 걍 애 키우는 거구나...",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033352.1.14",
          "form": "난 애도 강아지도 키워본 적 없어서 잘 몰겠지만",
          "original_form": "난 애도 강아지도 키워본 적 없어서 잘 몰겠지만",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100033352.1.15",
          "form": "일단설거지는 뜨뜻한물에 해야하고",
          "original_form": "일단설거지는 뜨뜻한물에 해야하고",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100033352.1.16",
          "form": "난.. 청소하는것보다 설거지하는게 적성에맞아",
          "original_form": "난.. 청소하는것보다 설거지하는게 적성에맞아",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100033352.1.17",
          "form": "진심..티엠아이네",
          "original_form": "진심..티엠아이네",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100033352.1.18",
          "form": "난 옛날에는 청소가 더 좋았는데",
          "original_form": "난 옛날에는 청소가 더 좋았는데",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100033352.1.19",
          "form": "요즘엔 움직이기 귀찮아서",
          "original_form": "요즘엔 움직이기 귀찮아서",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100033352.1.20",
          "form": "설거지가 더 좋은것같애",
          "original_form": "설거지가 더 좋은것같애",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100033352.1.21",
          "form": "나도~!^-^",
          "original_form": "나도~!^-^",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100033352.1.22",
          "form": "청소는남자가해",
          "original_form": "청소는남자가해",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100033352.1.23",
          "form": "설거지도해.",
          "original_form": "설거지도해.",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100033352.1.24",
          "form": "여자는쉬고싶어",
          "original_form": "여자는쉬고싶어",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100033352.1.25",
          "form": "맞아맞아 귀찮으니까 그냥 남자들이 다 해줬으면^^",
          "original_form": "맞아맞아 귀찮으니까 그냥 남자들이 다 해줬으면^^",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100033352.1.26",
          "form": "요리하는건 재밋지만",
          "original_form": "요리하는건 재밋지만",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100033352.1.27",
          "form": "설거지...?",
          "original_form": "설거지...?",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100033352.1.28",
          "form": "전혀 재밋지않은",
          "original_form": "전혀 재밋지않은",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100033352.1.29",
          "form": "사실 나는 요리도 잘 안하지만...",
          "original_form": "사실 나는 요리도 잘 안하지만...",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100033352.1.30",
          "form": "설거지 진짜 노잼이야",
          "original_form": "설거지 진짜 노잼이야",
