      "utterance": [
        {
          "id": "MDRW2100012144.1.1",
          "form": "운동 하시는 거 있으신가요?",
          "original_form": "운동 하시는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012144.1.2",
          "form": "홈트해요!",
          "original_form": "홈트해요!",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012144.1.3",
          "form": "홈트 누구 보세요??",
          "original_form": "홈트 누구 보세요??",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100012144.1.4",
          "form": "땅끄부부님이요",
          "original_form": "땅끄부부님이요",
          "speaker_id": "2",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012144.1.5",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100012144.1.6",
          "form": "완전 매운맛 그거 어려워",
          "original_form": "완전 매운맛 그거 어려워",
          "speaker_id": "1",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100012144.1.7",
          "form": "어려워요ㅋㅋㅋㅋ",
          "original_form": "어려워요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100012144.1.8",
          "form": "ㅋㅋㅋ그래서 저는 쉬워보이는것만 해요ㅋㅋ",
          "original_form": "ㅋㅋㅋ그래서 저는 쉬워보이는것만 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100012144.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 그거하면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그거하면",
          "speaker_id": "1",
          "time": "20210824 19:58"
        },
        {
          "id": "MDRW2100012144.1.10",
          "form": "허벅지 엄청 불타요",
          "original_form": "허벅지 엄청 불타요",
          "speaker_id": "1",
          "time": "20210824 19:58"
        },
        {
          "id": "MDRW2100012144.1.11",
          "form": "ㅋㅋ그래도 효과는 있지않나요?",
          "original_form": "ㅋㅋ그래도 효과는 있지않나요?",
          "speaker_id": "2",
          "time": "20210824 20:04"
        },
        {
          "id": "MDRW2100012144.1.12",
          "form": "아직 주말만 하느라",
          "original_form": "아직 주말만 하느라",
          "speaker_id": "1",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012144.1.13",
          "form": "못보고 있는데",
          "original_form": "못보고 있는데",
          "speaker_id": "1",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012144.1.14",
          "form": "효과 보셨어요?",
          "original_form": "효과 보셨어요?",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012144.1.15",
          "form": "저도 시간 날 때만 해서 몇개월하긴 했는데",
          "original_form": "저도 시간 날 때만 해서 몇개월하긴 했는데",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012144.1.16",
          "form": "4키로 빠졌어요ㅋㅋ",
          "original_form": "4키로 빠졌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012144.1.17",
          "form": "집밥위주로 먹다가 먹고싶은것도 종종먹으면서요",
          "original_form": "집밥위주로 먹다가 먹고싶은것도 종종먹으면서요",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012144.1.18",
          "form": "헐 와우 대박",
          "original_form": "헐 와우 대박",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012144.1.19",
          "form": "4키로라니 효과 빠빰이네요",
          "original_form": "4키로라니 효과 빠빰이네요",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012144.1.20",
          "form": "저녁먹고왔어요ㅎ",
          "original_form": "저녁먹고왔어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012144.1.21",
          "form": "살찐게 8키로라...아직 4키로남았네요ㅜ",
          "original_form": "살찐게 8키로라...아직 4키로남았네요ㅜ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012144.1.22",
          "form": "저도 빼야하는데 어려워요 흑",
          "original_form": "저도 빼야하는데 어려워요 흑",
          "speaker_id": "1",
          "time": "20210824 21:17"
        }
