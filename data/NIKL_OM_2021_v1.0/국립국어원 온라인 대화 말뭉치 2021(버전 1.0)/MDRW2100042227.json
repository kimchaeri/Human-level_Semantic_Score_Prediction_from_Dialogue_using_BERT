      "utterance": [
        {
          "id": "MDRW2100042227.1.1",
          "form": "진짜 멋쟁이",
          "original_form": "진짜 멋쟁이",
          "speaker_id": "2",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042227.1.2",
          "form": "ㅎㅎ그런가요",
          "original_form": "ㅎㅎ그런가요",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042227.1.3",
          "form": "네!! 전 그냥 검정색 흰색 롱패딩 번갈아입기....ㅎㅎ",
          "original_form": "네!! 전 그냥 검정색 흰색 롱패딩 번갈아입기....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042227.1.4",
          "form": "저도 롱패딩 입어요!",
          "original_form": "저도 롱패딩 입어요!",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042227.1.5",
          "form": "지금 날씨는 어떤가요?",
          "original_form": "지금 날씨는 어떤가요?",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042227.1.6",
          "form": "여기는 비가 부슬부슬오네요",
          "original_form": "여기는 비가 부슬부슬오네요",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042227.1.7",
          "form": "여기도 비가 와요!",
          "original_form": "여기도 비가 와요!",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042227.1.8",
          "form": "이번주도 비가 올건가봐요",
          "original_form": "이번주도 비가 올건가봐요",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042227.1.9",
          "form": "가을 장마가 오래 가네요",
          "original_form": "가을 장마가 오래 가네요",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042227.1.10",
          "form": "비 좀 그만왔으면 좋겠어요",
          "original_form": "비 좀 그만왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042227.1.11",
          "form": "비 오는 날 좋아하시나요?",
          "original_form": "비 오는 날 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042227.1.12",
          "form": "전 그냥 집 안에서 보는 비는 좋지만..",
          "original_form": "전 그냥 집 안에서 보는 비는 좋지만..",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042227.1.13",
          "form": "비오는날을 좋아하진 않아요.",
          "original_form": "비오는날을 좋아하진 않아요.",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042227.1.14",
          "form": "저도 해가 쨍쨍한 맑은 날이 좋아요!",
          "original_form": "저도 해가 쨍쨍한 맑은 날이 좋아요!",
          "speaker_id": "2",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042227.1.15",
          "form": "비오는 날은 이동하기도 힘들고 질퍽거리고 춥고 ㅠ",
          "original_form": "비오는 날은 이동하기도 힘들고 질퍽거리고 춥고 ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042227.1.16",
          "form": "ㅎㅎ맞아요 이동하기 너무 힘들죠",
          "original_form": "ㅎㅎ맞아요 이동하기 너무 힘들죠",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042227.1.17",
          "form": "빨리 가을 장마 지나갔으면...!",
          "original_form": "빨리 가을 장마 지나갔으면...!",
          "speaker_id": "2",
          "time": "20210906 20:57"
        }
