        {
          "id": "MDRW2100033649.1.1",
          "form": "햄버거 좋아하세요?",
          "original_form": "햄버거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100033649.1.2",
          "form": "네 햄버거 좋죠! name1 님은요?",
          "original_form": "네 햄버거 좋죠! &name1& 님은요?",
          "speaker_id": "2",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100033649.1.3",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100033649.1.4",
          "form": "어디 햄버거 좋아하세요?",
          "original_form": "어디 햄버거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100033649.1.5",
          "form": "저는 버거킹 제일 좋아해요!",
          "original_form": "저는 버거킹 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100033649.1.6",
          "form": "오 저도 버거킹 좋아해요",
          "original_form": "오 저도 버거킹 좋아해요",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100033649.1.7",
          "form": "어떤 버거 좋아하세요?",
          "original_form": "어떤 버거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100033649.1.8",
          "form": "콰트로 치즈 와퍼를 제일 좋아해요ㅋㅋㅋ name1 님은요?",
          "original_form": "콰트로 치즈 와퍼를 제일 좋아해요ㅋㅋㅋ &name1& 님은요?",
          "speaker_id": "2",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100033649.1.9",
          "form": "저도 그게 최애였는데",
          "original_form": "저도 그게 최애였는데",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100033649.1.10",
          "form": "요새는 통새우로",
          "original_form": "요새는 통새우로",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100033649.1.11",
          "form": "바꼈어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "바꼈어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100033649.1.12",
          "form": "오ㅋㅋㅋ 통새우 맛있게 먹는 법 있나요?",
          "original_form": "오ㅋㅋㅋ 통새우 맛있게 먹는 법 있나요?",
          "speaker_id": "2",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100033649.1.13",
          "form": "그냥 통새우만 먹어도 맛있어요",
          "original_form": "그냥 통새우만 먹어도 맛있어요",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100033649.1.14",
          "form": "새우추가도 좋구요",
          "original_form": "새우추가도 좋구요",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100033649.1.15",
          "form": "좋네요! 버거킹 사이드는 뭘 시키시나요?",
          "original_form": "좋네요! 버거킹 사이드는 뭘 시키시나요?",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100033649.1.16",
          "form": "저도 먹어보진 않았지만",
          "original_form": "저도 먹어보진 않았지만",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100033649.1.17",
          "form": "거기 파는 약간 튀김새우,,?",
          "original_form": "거기 파는 약간 튀김새우,,?",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100033649.1.18",
          "form": "그게 그렇게 맛있대요",
          "original_form": "그게 그렇게 맛있대요",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100033649.1.19",
          "form": "저는 버거킹 가면 치즈 감튀로 바꾸고 아이스크림을 무조건 먹어요ㅋㅋㅋ",
          "original_form": "저는 버거킹 가면 치즈 감튀로 바꾸고 아이스크림을 무조건 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100033649.1.20",
          "form": "오 저도 다음에",
          "original_form": "오 저도 다음에",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100033649.1.21",
          "form": "그렇게 먹어야겠어요ㅋㅋㅋㅋㅋ",
