      "utterance": [
        {
          "id": "MDRW2100024501.1.1",
          "form": "젤좋아하는 디저트는 무엇인가요!",
          "original_form": "젤좋아하는 디저트는 무엇인가요!",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024501.1.2",
          "form": "저는 아이스크림요! 근데 군것질 다 좋아해요ㅎㅎㅎ",
          "original_form": "저는 아이스크림요! 근데 군것질 다 좋아해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100024501.1.3",
          "form": "파 자두요 ㅠㅠ",
          "original_form": "파 자두요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100024501.1.4",
          "form": "요즘은 어떤거 좋아해요~?",
          "original_form": "요즘은 어떤거 좋아해요~?",
          "speaker_id": "1",
          "time": "20210831 14:33"
        },
        {
          "id": "MDRW2100024501.1.5",
          "form": "음....베라 민초요!!",
          "original_form": "음....베라 민초요!!",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100024501.1.6",
          "form": "민초 맛있죠ㅋㅋㅋ 저는 베라에서 이상한 나라의 솜사탕 좋아해요",
          "original_form": "민초 맛있죠ㅋㅋㅋ 저는 베라에서 이상한 나라의 솜사탕 좋아해요",
          "speaker_id": "1",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024501.1.7",
          "form": "북극곰도요!",
          "original_form": "북극곰도요!",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100024501.1.8",
          "form": "북극곰맛도 있나여?",
          "original_form": "북극곰맛도 있나여?",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100024501.1.9",
          "form": "북극곰폴라베어요!",
          "original_form": "북극곰폴라베어요!",
          "speaker_id": "2",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100024501.1.10",
          "form": "오 그건 처음 들었어요!",
          "original_form": "오 그건 처음 들었어요!",
          "speaker_id": "1",
          "time": "20210831 18:42"
