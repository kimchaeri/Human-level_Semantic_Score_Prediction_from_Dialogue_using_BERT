      "utterance": [
        {
          "id": "MDRW2100036363.1.1",
          "form": "흐윽.. 진짜 그러실거같아요..",
          "original_form": "흐윽.. 진짜 그러실거같아요..",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036363.1.2",
          "form": "그건 말잊못..",
          "original_form": "그건 말잊못..",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036363.1.3",
          "form": "막 파바 케이크도 드시나요?",
          "original_form": "막 파바 케이크도 드시나요?",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100036363.1.4",
          "form": "넹! 이번에 6가지맛 있는거 사서 먹었드랬죵",
          "original_form": "넹! 이번에 6가지맛 있는거 사서 먹었드랬죵",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100036363.1.5",
          "form": "앗 그거 마이넘버원2인가요 ㅎㅎ",
          "original_form": "앗 그거 마이넘버원2인가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036363.1.6",
          "form": "앗 맞을거에요!!",
          "original_form": "앗 맞을거에요!!",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036363.1.7",
          "form": "친구들이랑 나눠먹었어용",
          "original_form": "친구들이랑 나눠먹었어용",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036363.1.8",
          "form": "오 ! 친구분들이랑 보통 어떤거 시켜드세요?",
          "original_form": "오 ! 친구분들이랑 보통 어떤거 시켜드세요?",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036363.1.9",
          "form": "떡볶이를 자주먹었어용!",
          "original_form": "떡볶이를 자주먹었어용!",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036363.1.10",
          "form": "떡볶이 메이트 있거등요ㅋㅋㅋㅋㅋ",
          "original_form": "떡볶이 메이트 있거등요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036363.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036363.1.12",
          "form": "저두 보통 떡볶이이에욬ㅋㅋ 엽떡에 허콤!",
          "original_form": "저두 보통 떡볶이이에욬ㅋㅋ 엽떡에 허콤!",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036363.1.13",
          "form": "떡볶이 메이트 부러워요 ㅜㅜㅜ",
          "original_form": "떡볶이 메이트 부러워요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036363.1.14",
          "form": "우와 진짜 최고 조합이자나여!",
          "original_form": "우와 진짜 최고 조합이자나여!",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036363.1.15",
          "form": "허콤 아시는분 드문데",
          "original_form": "허콤 아시는분 드문데",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036363.1.16",
          "form": "흐흐 요즘 못먹었더니 좀 땡기네요",
          "original_form": "흐흐 요즘 못먹었더니 좀 땡기네요",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036363.1.17",
          "form": "로제도 좋아해서 만나면",
          "original_form": "로제도 좋아해서 만나면",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100036363.1.18",
          "form": "서로 싸워요! 어디꺼 시킬지! ㅋㅋ",
          "original_form": "서로 싸워요! 어디꺼 시킬지! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100036363.1.19",
          "form": "옼ㅋㅋㅋㅋ",
          "original_form": "옼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036363.1.20",
          "form": "저희 동네 로제는 별루라ㅜㅠ 맛집 좀 내줬음 좋겠어요",
          "original_form": "저희 동네 로제는 별루라ㅜㅠ 맛집 좀 내줬음 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036363.1.21",
          "form": "태리로제는 드셔보셨어요??!",
          "original_form": "태리로제는 드셔보셨어요??!",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036363.1.22",
          "form": "앗 아뇨!",
          "original_form": "앗 아뇨!",
          "speaker_id": "2",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100036363.1.23",
          "form": "가게명이 태리인가요?",
          "original_form": "가게명이 태리인가요?",
          "speaker_id": "2",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100036363.1.24",
          "form": "네네 ! 태리로제떡볶이가 가게명이에요!",
          "original_form": "네네 ! 태리로제떡볶이가 가게명이에요!",
