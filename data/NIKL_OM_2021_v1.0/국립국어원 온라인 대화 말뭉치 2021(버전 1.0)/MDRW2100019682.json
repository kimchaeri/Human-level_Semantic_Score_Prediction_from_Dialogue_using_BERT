        {
          "id": "MDRW2100019682.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019682.1.2",
          "form": "필요한만큼요",
          "original_form": "필요한만큼요",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019682.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019682.1.4",
          "form": "어디를청소하시나요",
          "original_form": "어디를청소하시나요",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019682.1.5",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019682.1.6",
          "form": "아 글쵸 화장실청소 맡아하시구 훌륭합니다",
          "original_form": "아 글쵸 화장실청소 맡아하시구 훌륭합니다",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100019682.1.7",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210828 07:39"
        },
        {
          "id": "MDRW2100019682.1.8",
          "form": "환기 잘하시면 될거같아요",
          "original_form": "환기 잘하시면 될거같아요",
          "speaker_id": "2",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100019682.1.9",
          "form": "네 젤 중요한건죠",
          "original_form": "네 젤 중요한건죠",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100019682.1.10",
          "form": "선선해져서 좀 쉬엄쉬엄해도 곰팡이 덜슬지않을까요",
          "original_form": "선선해져서 좀 쉬엄쉬엄해도 곰팡이 덜슬지않을까요",
          "speaker_id": "2",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100019682.1.11",
          "form": "그래도 아직 걱정이에요",
          "original_form": "그래도 아직 걱정이에요",
          "speaker_id": "1",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100019682.1.12",
          "form": "습기때문인가요",
