      "utterance": [
        {
          "id": "MDRW2100017612.1.1",
          "form": "집에 키우는 동물 있어요?",
          "original_form": "집에 키우는 동물 있어요?",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017612.1.2",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017612.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017612.1.4",
          "form": "저도 지금은 안키우는데",
          "original_form": "저도 지금은 안키우는데",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017612.1.5",
          "form": "파충류 키우는 친구 보니까 신기하더라구요 ㅎㅎ",
          "original_form": "파충류 키우는 친구 보니까 신기하더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017612.1.6",
          "form": "오.. 파충류는 한번도 생각해본적 없는,,",
          "original_form": "오.. 파충류는 한번도 생각해본적 없는,,",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017612.1.7",
          "form": "좋아하는 동물이 있으신가요?",
          "original_form": "좋아하는 동물이 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017612.1.8",
          "form": "저는 고양이요",
          "original_form": "저는 고양이요",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017612.1.9",
          "form": "고양이랑 살아보고싶네요,,",
          "original_form": "고양이랑 살아보고싶네요,,",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017612.1.10",
          "form": "저 강아지 좋아해요~",
          "original_form": "저 강아지 좋아해요~",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017612.1.11",
          "form": "고양이도 개냥이 성격이면 키워보고 싶어요..",
          "original_form": "고양이도 개냥이 성격이면 키워보고 싶어요..",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017612.1.12",
          "form": "강아지도 귀엽죠ㅠㅠ",
          "original_form": "강아지도 귀엽죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017612.1.13",
          "form": "지인분 중에 완전 개냥이랑 사시는 분 계셔서",
          "original_form": "지인분 중에 완전 개냥이랑 사시는 분 계셔서",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017612.1.14",
          "form": "몇번 놀러갔었는데",
          "original_form": "몇번 놀러갔었는데",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017612.1.15",
          "form": "원랜 고양이보단 강아지파였거든요",
          "original_form": "원랜 고양이보단 강아지파였거든요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017612.1.16",
          "form": "근데 너무,, 사랑스럽다고 생각했어요ㅠㅠㅠ",
          "original_form": "근데 너무,, 사랑스럽다고 생각했어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017612.1.17",
          "form": "고양이의 매력을 알게된,,",
          "original_form": "고양이의 매력을 알게된,,",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017612.1.18",
          "form": "그쵸 ㅜㅜ 진짜 귀여워요.",
          "original_form": "그쵸 ㅜㅜ 진짜 귀여워요.",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017612.1.19",
          "form": "장난감으로 놀아줄 때가 제일 재밌는 것 같아요 ㅎㅎ",
          "original_form": "장난감으로 놀아줄 때가 제일 재밌는 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017612.1.20",
          "form": "저는 그.. 말랑말랑한 젤리가",
          "original_form": "저는 그.. 말랑말랑한 젤리가",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017612.1.21",
          "form": "너무 귀여워서",
          "original_form": "너무 귀여워서",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017612.1.22",
          "form": "열심히 사진 왕창 찍고ㅋㅋㅋ",
          "original_form": "열심히 사진 왕창 찍고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017612.1.23",
          "form": "젤리가 그 분홍 발바닥이요? ㅎㅎ",
          "original_form": "젤리가 그 분홍 발바닥이요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017612.1.24",
          "form": "전 얼굴 부비적 거리는게 심쿵이더라구요 ㅜㅜ",
          "original_form": "전 얼굴 부비적 거리는게 심쿵이더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017612.1.25",
          "form": "네네 발바닥이요",
          "original_form": "네네 발바닥이요",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017612.1.26",
          "form": "흑흑 털이 많이 빠진다고는 하지만",
          "original_form": "흑흑 털이 많이 빠진다고는 하지만",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017612.1.27",
          "form": "같이 살고싶은",
          "original_form": "같이 살고싶은",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017612.1.28",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017612.1.29",
          "form": "부비적대는걸 엄청 부러워하면서 봤어요",
          "original_form": "부비적대는걸 엄청 부러워하면서 봤어요",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017612.1.30",
          "form": "역시 보호자를 이길순없는",
          "original_form": "역시 보호자를 이길순없는",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017612.1.31",
          "form": "ㅋㅋㅋ근데 지금 고양이 키울 여건은 안되시나봐요?",
          "original_form": "ㅋㅋㅋ근데 지금 고양이 키울 여건은 안되시나봐요?",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017612.1.32",
          "form": "사실 동물과 함께 한다는건",
          "original_form": "사실 동물과 함께 한다는건",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017612.1.33",
          "form": "털이나 배변이라거나",
          "original_form": "털이나 배변이라거나",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017612.1.34",
          "form": "여러가지를 감수해야하는데",
          "original_form": "여러가지를 감수해야하는데",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017612.1.35",
          "form": "좋은 보호자가 되어줄수있을까 싶기도 하고..",
          "original_form": "좋은 보호자가 되어줄수있을까 싶기도 하고..",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017612.1.36",
          "form": "지금은 그냥 랜선집사로 만족하고 살고있어요ㅎㅎ",
          "original_form": "지금은 그냥 랜선집사로 만족하고 살고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017612.1.37",
          "form": "저도요 ㅎㅎ 근데 부모님이 개 키우셔서 본가 갈때마다 예뻐해주고 와요",
          "original_form": "저도요 ㅎㅎ 근데 부모님이 개 키우셔서 본가 갈때마다 예뻐해주고 와요",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017612.1.38",
          "form": "와~",
          "original_form": "와~",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017612.1.39",
          "form": "귀엽겠네요",
          "original_form": "귀엽겠네요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017612.1.40",
          "form": "혹시 강아지 몇살이에요?",
          "original_form": "혹시 강아지 몇살이에요?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017612.1.41",
          "form": "10살 정도 됐어요!",
          "original_form": "10살 정도 됐어요!",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017612.1.42",
          "form": "귀여운데 좀 너무 무겁고 커요 ㅋㅋㅋㅋ",
          "original_form": "귀여운데 좀 너무 무겁고 커요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017612.1.43",
          "form": "ㅋㅋㅋㅋㅋ대형견인가요?",
          "original_form": "ㅋㅋㅋㅋㅋ대형견인가요?",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017612.1.44",
          "form": "귀엽겠다ㅠㅠ",
          "original_form": "귀엽겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:50"
        }
      ]
    }
