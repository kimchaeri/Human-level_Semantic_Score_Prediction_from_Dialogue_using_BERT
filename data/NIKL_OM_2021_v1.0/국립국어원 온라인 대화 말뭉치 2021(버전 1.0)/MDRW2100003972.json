      "utterance": [
        {
          "id": "MDRW2100003972.1.1",
          "form": "집안일 하시나요?",
          "original_form": "집안일 하시나요?",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003972.1.2",
          "form": "저희는 대가족이어서 각자 역할 분담해 하는 편이에요",
          "original_form": "저희는 대가족이어서 각자 역할 분담해 하는 편이에요",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003972.1.3",
          "form": "주로 어떤 일을 맡으시나요?",
          "original_form": "주로 어떤 일을 맡으시나요?",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003972.1.4",
          "form": "저는 보통 제가 다 하거든요",
          "original_form": "저는 보통 제가 다 하거든요",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003972.1.5",
          "form": "저는 장보기, 빨래건조, 재활용 분리수거, 종종 설거지나 청소기 해요",
          "original_form": "저는 장보기, 빨래건조, 재활용 분리수거, 종종 설거지나 청소기 해요",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003972.1.6",
          "form": "많이 하시네요",
          "original_form": "많이 하시네요",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003972.1.7",
          "form": "아 1인가구신가요?",
          "original_form": "아 1인가구신가요?",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003972.1.8",
          "form": "네 1인가구에요",
          "original_form": "네 1인가구에요",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003972.1.9",
          "form": "저는 저것들을 혼자하지 않고 늘 같이 해서ㅎㅎ",
          "original_form": "저는 저것들을 혼자하지 않고 늘 같이 해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003972.1.10",
          "form": "그 중 제일 귀찮은건 뭔가요",
          "original_form": "그 중 제일 귀찮은건 뭔가요",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003972.1.11",
          "form": "1인가구는 진짜 집안일 부담이 크죠ㅜㅠ",
          "original_form": "1인가구는 진짜 집안일 부담이 크죠ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003972.1.12",
          "form": "저는 분리수거요ㅎ",
          "original_form": "저는 분리수거요ㅎ",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003972.1.13",
          "form": "저는 설거지요ㅎㅎㅎ",
          "original_form": "저는 설거지요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003972.1.14",
          "form": "그것도 인정합니다",
          "original_form": "그것도 인정합니다",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003972.1.15",
          "form": "뭔가 먹고 바로 해야하는거 아는데도 미루게되잖아요",
          "original_form": "뭔가 먹고 바로 해야하는거 아는데도 미루게되잖아요",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003972.1.16",
          "form": "하고나면 아무것도 아닌데 그 아무것도 아닌게 왜 그리 귀찮은건지",
          "original_form": "하고나면 아무것도 아닌데 그 아무것도 아닌게 왜 그리 귀찮은건지",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003972.1.17",
          "form": "맞아요\r\n집안일은 미루는 순간 더 큰 짐이 되는 것 같아요",
          "original_form": "맞아요\r\n집안일은 미루는 순간 더 큰 짐이 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003972.1.18",
          "form": "대가족이라 한 번 설거지할 때 분량도 엄청나고요",
          "original_form": "대가족이라 한 번 설거지할 때 분량도 엄청나고요",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003972.1.19",
          "form": "아 그러겠네요",
          "original_form": "아 그러겠네요",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003972.1.20",
          "form": "제가 너무 제 위주로 생각을 했어요",
          "original_form": "제가 너무 제 위주로 생각을 했어요",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003972.1.21",
          "form": "편협적이었어요",
          "original_form": "편협적이었어요",
          "speaker_id": "1",
          "time": "20210821 14:23"
        }
      ]
