      "utterance": [
        {
          "id": "MDRW2100013359.1.1",
          "form": "넌 코로나긑나면",
          "original_form": "넌 코로나긑나면",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013359.1.2",
          "form": "어느나라 여행가구싶어?",
          "original_form": "어느나라 여행가구싶어?",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013359.1.3",
          "form": "나는 파리!",
          "original_form": "나는 파리!",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013359.1.4",
          "form": "프랑스 가보고깊어",
          "original_form": "프랑스 가보고깊어",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013359.1.5",
          "form": "에펠탑 앞에서사진 직고싶다",
          "original_form": "에펠탑 앞에서사진 직고싶다",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013359.1.6",
          "form": "나두나두",
          "original_form": "나두나두",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013359.1.7",
          "form": "글고 나는",
          "original_form": "글고 나는",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.8",
          "form": "미국도 가보고파",
          "original_form": "미국도 가보고파",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.9",
          "form": "뉴욕 조아",
          "original_form": "뉴욕 조아",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.10",
          "form": "로스앤젤레스",
          "original_form": "로스앤젤레스",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.11",
          "form": "글고 디즈니랜드도 가보고싶다",
          "original_form": "글고 디즈니랜드도 가보고싶다",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.12",
          "form": "아맞엏",
          "original_form": "아맞엏",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.13",
          "form": "홍콩 디즈니랜드나",
          "original_form": "홍콩 디즈니랜드나",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.14",
          "form": "아니면 도쿄빼고 다른 디즈니랜드도",
          "original_form": "아니면 도쿄빼고 다른 디즈니랜드도",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.15",
          "form": "가보고싶음..",
          "original_form": "가보고싶음..",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.16",
          "form": "한번도 모가봣어",
          "original_form": "한번도 모가봣어",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.17",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.18",
          "form": "진짜 디즈니랜드가면",
          "original_form": "진짜 디즈니랜드가면",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.19",
          "form": "환상의나라로 간것같을듯",
          "original_form": "환상의나라로 간것같을듯",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.20",
          "form": "그 캐릭터들도",
          "original_form": "그 캐릭터들도",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.21",
          "form": "다 잇잖아",
          "original_form": "다 잇잖아",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.22",
          "form": "진짜 재밌을거같애",
          "original_form": "진짜 재밌을거같애",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.23",
          "form": "나두ㅜㅜ",
          "original_form": "나두ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013359.1.24",
          "form": "언제가보나",
          "original_form": "언제가보나",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013359.1.25",
          "form": "코로나는 언제끝나",
          "original_form": "코로나는 언제끝나",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013359.1.26",
          "form": "지겨워죽겠어...",
          "original_form": "지겨워죽겠어...",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013359.1.27",
          "form": "비행기도 못타보고 죽을수없다",
          "original_form": "비행기도 못타보고 죽을수없다",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013359.1.28",
          "form": "공항가고싶다 !!",
          "original_form": "공항가고싶다 !!",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013359.1.29",
          "form": "공항가보자고",
          "original_form": "공항가보자고",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.30",
          "form": "글고 기내식두 먹어야지",
          "original_form": "글고 기내식두 먹어야지",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.31",
          "form": "아마자",
          "original_form": "아마자",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.32",
          "form": "비빔밥 먹어보고깊어",
          "original_form": "비빔밥 먹어보고깊어",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.33",
          "form": "하늘에서 먹는 비빔밥 ㄹㅇ",
          "original_form": "하늘에서 먹는 비빔밥 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.34",
          "form": "꿀맛이겟다리",
          "original_form": "꿀맛이겟다리",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.35",
          "form": "나도 비빔밥 먹어봤어",
          "original_form": "나도 비빔밥 먹어봤어",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.36",
          "form": "???",
          "original_form": "???",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.37",
          "form": "어땠어?",
          "original_form": "어땠어?",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013359.1.38",
          "form": "진짜존맛",
          "original_form": "진짜존맛",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013359.1.39",
          "form": "완전 짱짱",
          "original_form": "완전 짱짱",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013359.1.40",
          "form": "부산가보고싶다",
          "original_form": "부산가보고싶다",
          "speaker_id": "2",
          "time": "20210827 11:17"
        }
      ]
    }
