      "utterance": [
        {
          "id": "MDRW2100013334.1.1",
          "form": "우산은 챙겻지",
          "original_form": "우산은 챙겻지",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.2",
          "form": "당근",
          "original_form": "당근",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.3",
          "form": "요즘 날씨가",
          "original_form": "요즘 날씨가",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.4",
          "form": "스콜같애",
          "original_form": "스콜같애",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.5",
          "form": "동남아기후",
          "original_form": "동남아기후",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.6",
          "form": "마저",
          "original_form": "마저",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.7",
          "form": "걍 태풍도",
          "original_form": "걍 태풍도",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.8",
          "form": "자주오고 장마도 자주와",
          "original_form": "자주오고 장마도 자주와",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.9",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.10",
          "form": "점점 나라망할거같아",
          "original_form": "점점 나라망할거같아",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.11",
          "form": "아니 세계가",
          "original_form": "아니 세계가",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.13",
          "form": "대구는 이런적 없었는데",
          "original_form": "대구는 이런적 없었는데",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.14",
          "form": "자꾸 이상해지는느낌...",
          "original_form": "자꾸 이상해지는느낌...",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.15",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.16",
          "form": "지구온난화가",
          "original_form": "지구온난화가",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.17",
          "form": "이렇게 가깝게 느껴지는건 첨이야",
          "original_form": "이렇게 가깝게 느껴지는건 첨이야",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.18",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013334.1.19",
          "form": "너무 환경생각을 안했나뷰ㅏ",
          "original_form": "너무 환경생각을 안했나뷰ㅏ",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.20",
          "form": "그러니깐 날씨가 이모양이지",
          "original_form": "그러니깐 날씨가 이모양이지",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.21",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.22",
          "form": "일회용품 안쓰고 했는데도",
          "original_form": "일회용품 안쓰고 했는데도",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.23",
          "form": "바꿀수없나봐",
          "original_form": "바꿀수없나봐",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.24",
          "form": "지금도 천둥친다",
          "original_form": "지금도 천둥친다",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.25",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.26",
          "form": "무서워 천둥",
          "original_form": "무서워 천둥",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.27",
          "form": "글고 비도 너무많이와",
          "original_form": "글고 비도 너무많이와",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.28",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.29",
          "form": "나는 그냥 집에있긴 했는데",
          "original_form": "나는 그냥 집에있긴 했는데",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.30",
          "form": "그래도 소리로 다들림",
          "original_form": "그래도 소리로 다들림",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.31",
          "form": "양말 1개 더들고가야해",
          "original_form": "양말 1개 더들고가야해",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.32",
          "form": "양말이 물에 젖어서",
          "original_form": "양말이 물에 젖어서",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.33",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013334.1.34",
          "form": "딘짜",
          "original_form": "딘짜",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013334.1.35",
          "form": "찝찝하고싫어",
          "original_form": "찝찝하고싫어",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013334.1.36",
          "form": "너무 축축해",
          "original_form": "너무 축축해",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013334.1.37",
          "form": "인정...",
          "original_form": "인정...",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013334.1.38",
          "form": "이제 비좀 안오고",
          "original_form": "이제 비좀 안오고",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013334.1.39",
          "form": "가을됐으면",
          "original_form": "가을됐으면",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013334.1.40",
          "form": "얼른 겨울됐으면 !!!",
          "original_form": "얼른 겨울됐으면 !!!",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013334.1.41",
          "form": "눈싸움하고 눈사람 만들구",
          "original_form": "눈싸움하고 눈사람 만들구",
          "speaker_id": "1",
          "time": "20210825 14:07"
        }
      ]
