      "utterance": [
        {
          "id": "MDRW2100002871.1.1",
          "form": "여행가고싶어요?",
          "original_form": "여행가고싶어요?",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002871.1.2",
          "form": "네ㅜ코로나끝나면 해외가고싶어요",
          "original_form": "네ㅜ코로나끝나면 해외가고싶어요",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002871.1.3",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002871.1.4",
          "form": "저는 옆동네만 자유롭게 가도 좋겠어요 ㅠ",
          "original_form": "저는 옆동네만 자유롭게 가도 좋겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002871.1.5",
          "form": "해외까지는 바라지도 않을테니 ㅠ",
          "original_form": "해외까지는 바라지도 않을테니 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002871.1.6",
          "form": "옆동네 다녀오면 학교에서 애들 코 쑤시고 오래요 ㅠ",
          "original_form": "옆동네 다녀오면 학교에서 애들 코 쑤시고 오래요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002871.1.7",
          "form": "헉 ㅜ 검사를 그럼 갈때마다 해야되는건가요",
          "original_form": "헉 ㅜ 검사를 그럼 갈때마다 해야되는건가요",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002871.1.8",
          "form": "빡세네요",
          "original_form": "빡세네요",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002871.1.9",
          "form": "이제 얼마나 진실되느냐 차이죠",
          "original_form": "이제 얼마나 진실되느냐 차이죠",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002871.1.10",
          "form": "안갓다왓다ㅜ하면 어쩔거에요",
          "original_form": "안갓다왓다ㅜ하면 어쩔거에요",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002871.1.11",
          "form": "근데 만일 확진자 동선 겹쳐서 연락오면 난리나니까",
          "original_form": "근데 만일 확진자 동선 겹쳐서 연락오면 난리나니까",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002871.1.12",
          "form": "에고 애들도 고생이네여ㅜㅜ",
          "original_form": "에고 애들도 고생이네여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002871.1.13",
          "form": "학교가서 밥도 친구들이랑 편하게못먹는것같던데",
          "original_form": "학교가서 밥도 친구들이랑 편하게못먹는것같던데",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002871.1.14",
          "form": "양치도 하지말래요",
          "original_form": "양치도 하지말래요",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002871.1.15",
          "form": "이러니 집밖에 나가는게 이젠 꺼려져요",
          "original_form": "이러니 집밖에 나가는게 이젠 꺼려져요",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002871.1.16",
          "form": "종식되면 전국일주 할판이에요",
          "original_form": "종식되면 전국일주 할판이에요",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002871.1.17",
          "form": "ㅜㅜ진짜 답답하네요 줄어들만하면 다시늘고그러니까ㅜ",
          "original_form": "ㅜㅜ진짜 답답하네요 줄어들만하면 다시늘고그러니까ㅜ",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002871.1.18",
          "form": "여행가실때 아이들위주로 가시겠어요",
          "original_form": "여행가실때 아이들위주로 가시겠어요",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002871.1.19",
          "form": "맞아요 어쩔수 없더라고요",
          "original_form": "맞아요 어쩔수 없더라고요",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002871.1.20",
          "form": "애들이 여행지에서 지루해하면 이도저도ㅠ아니니까",
          "original_form": "애들이 여행지에서 지루해하면 이도저도ㅠ아니니까",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002871.1.21",
          "form": "혼자 여행 다니고 친구랑 많이 다닐걸 그걸 못해봤어요",
          "original_form": "혼자 여행 다니고 친구랑 많이 다닐걸 그걸 못해봤어요",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002871.1.22",
          "form": "그쳐 그래서 저도 지금 혼자여행많이다니고싶은데",
          "original_form": "그쳐 그래서 저도 지금 혼자여행많이다니고싶은데",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002871.1.23",
          "form": "시국이안따라주네요ㅜㅜㅜ",
          "original_form": "시국이안따라주네요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002871.1.24",
          "form": "잠잠해지면 실컷 돌아다니자구요 ㅠ",
          "original_form": "잠잠해지면 실컷 돌아다니자구요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002871.1.25",
          "form": "지금 여행 다니는 사람들 많긴 하던데",
          "original_form": "지금 여행 다니는 사람들 많긴 하던데",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002871.1.26",
          "form": "진짜 맘껏 편하게 다니고싶어요 지금은 진짜 불안해서ㅜㅜㅜ",
          "original_form": "진짜 맘껏 편하게 다니고싶어요 지금은 진짜 불안해서ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 16:44"
        }
      ]
    }
