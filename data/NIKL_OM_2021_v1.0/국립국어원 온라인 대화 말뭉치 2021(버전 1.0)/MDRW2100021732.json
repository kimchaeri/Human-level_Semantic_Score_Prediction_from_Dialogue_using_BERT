      "utterance": [
        {
          "id": "MDRW2100021732.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021732.1.2",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021732.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021732.1.4",
          "form": "네네 좋아해요",
          "original_form": "네네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021732.1.5",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021732.1.6",
          "form": "좋아하는 편이에요",
          "original_form": "좋아하는 편이에요",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021732.1.7",
          "form": "좋았던 여행지 있으세요?",
          "original_form": "좋았던 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021732.1.8",
          "form": "음 국내에서는 낙산사랑 우도를 좋아해요",
          "original_form": "음 국내에서는 낙산사랑 우도를 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021732.1.9",
          "form": "오 둘 다 안가본 곳이에요!",
          "original_form": "오 둘 다 안가본 곳이에요!",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021732.1.10",
          "form": "해외는 가본 곳 중에서는 홍콩을 좋아해요",
          "original_form": "해외는 가본 곳 중에서는 홍콩을 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021732.1.11",
          "form": "홍콩영화를 좋아해서",
          "original_form": "홍콩영화를 좋아해서",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021732.1.12",
          "form": "홍콩 너무 가보고싶었어요",
          "original_form": "홍콩 너무 가보고싶었어요",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021732.1.13",
          "form": "저는 해외를 한번밖에 안가봤거든요",
          "original_form": "저는 해외를 한번밖에 안가봤거든요",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021732.1.14",
          "form": "어디어디 다녀와보셨어요 해외?",
          "original_form": "어디어디 다녀와보셨어요 해외?",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021732.1.15",
          "form": "저는 가까운 데 위주로 일본 몇군데 중국 몇군데 대만 홍콩 베트남 싱가포르 말레이시아 동유럽몇군데 가봤어요",
          "original_form": "저는 가까운 데 위주로 일본 몇군데 중국 몇군데 대만 홍콩 베트남 싱가포르 말레이시아 동유럽몇군데 가봤어요",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021732.1.16",
          "form": "저두 막 멀리 오래 가본 적은 없어용",
          "original_form": "저두 막 멀리 오래 가본 적은 없어용",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021732.1.17",
          "form": "추천해주실만한 여행지 있으신가요",
          "original_form": "추천해주실만한 여행지 있으신가요",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021732.1.18",
          "form": "우와 진짜 많이 다녀오셨네요",
          "original_form": "우와 진짜 많이 다녀오셨네요",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021732.1.19",
          "form": "저는 국내로만 돌아다녀서",
          "original_form": "저는 국내로만 돌아다녀서",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021732.1.20",
          "form": "강릉을 너무 좋아해서 매년 가요",
          "original_form": "강릉을 너무 좋아해서 매년 가요",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021732.1.21",
          "form": "저도 국내여행도 정말 좋아해요",
          "original_form": "저도 국내여행도 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021732.1.22",
          "form": "어릴 때부터 가족끼리 많이 다녀서요",
          "original_form": "어릴 때부터 가족끼리 많이 다녀서요",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021732.1.23",
          "form": "통영도 좋았고, 제주도의 가파도도 정말 좋았어요",
          "original_form": "통영도 좋았고, 제주도의 가파도도 정말 좋았어요",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021732.1.24",
          "form": "강릉 어디 좋아하시나요?",
          "original_form": "강릉 어디 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021732.1.25",
          "form": "정말 좋은 가족분들이네요",
          "original_form": "정말 좋은 가족분들이네요",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021732.1.26",
          "form": "통영이랑 가파도 좋아요!!",
          "original_form": "통영이랑 가파도 좋아요!!",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021732.1.27",
          "form": "저는 그냥 강릉 바다 구경하고, 카페가고하는걸 좋아해요",
          "original_form": "저는 그냥 강릉 바다 구경하고, 카페가고하는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021732.1.28",
          "form": "가파도 가보셨군요!",
          "original_form": "가파도 가보셨군요!",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021732.1.29",
          "form": "지인이 제주도에 사시는데 추천해주셔서요",
          "original_form": "지인이 제주도에 사시는데 추천해주셔서요",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021732.1.30",
          "form": "아님 몰랐을 뻔했는데 몰랐음 아쉬웠겠죠",
          "original_form": "아님 몰랐을 뻔했는데 몰랐음 아쉬웠겠죠",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021732.1.31",
          "form": "제주도를 딱 한번밖에 못가봤는데, 즉흥적으로 여행 계획을 변경해서 가파도에 갔다가",
          "original_form": "제주도를 딱 한번밖에 못가봤는데, 즉흥적으로 여행 계획을 변경해서 가파도에 갔다가",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021732.1.32",
          "form": "너무 좋더라구요",
          "original_form": "너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021732.1.33",
          "form": "청보리밭이 바람에 흩날리는데 정말..",
          "original_form": "청보리밭이 바람에 흩날리는데 정말..",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021732.1.34",
          "form": "ㅠㅠㅠ 다시가고싶네요",
          "original_form": "ㅠㅠㅠ 다시가고싶네요",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021732.1.35",
          "form": "청보리밭 너무 예쁘죠",
          "original_form": "청보리밭 너무 예쁘죠",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021732.1.36",
          "form": "고창의 청보리밭도 예쁘답니다",
          "original_form": "고창의 청보리밭도 예쁘답니다",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021732.1.37",
          "form": "아직도 가끔 그때 찍어둔 청보리밭 영상을 봐요",
          "original_form": "아직도 가끔 그때 찍어둔 청보리밭 영상을 봐요",
          "speaker_id": "2",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021732.1.38",
          "form": "헉 좋은정보 감사해요",
          "original_form": "헉 좋은정보 감사해요",
          "speaker_id": "2",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021732.1.39",
          "form": "혹시 또 강릉 가시게 되면 쫌만 더 올라가셔서 낙산사도 추천할게요",
          "original_form": "혹시 또 강릉 가시게 되면 쫌만 더 올라가셔서 낙산사도 추천할게요",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021732.1.40",
          "form": "절 뒤로 바다가 보이는데 진짜진짜 말로 다 표현못해요",
          "original_form": "절 뒤로 바다가 보이는데 진짜진짜 말로 다 표현못해요",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021732.1.41",
          "form": "헉 꼭 가볼게요",
          "original_form": "헉 꼭 가볼게요",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021732.1.42",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021732.1.43",
          "form": "저 강릉에 소나무길도 너무 좋아해요",
          "original_form": "저 강릉에 소나무길도 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021732.1.44",
          "form": "카페거리 가는 길 한시간 넘게 걸었던거같은데",
          "original_form": "카페거리 가는 길 한시간 넘게 걸었던거같은데",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021732.1.45",
          "form": "저는 거긴 안가봤어요",
          "original_form": "저는 거긴 안가봤어요",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021732.1.46",
          "form": "너무 예쁘고 행복했어요",
          "original_form": "너무 예쁘고 행복했어요",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021732.1.47",
          "form": "카페거리쪽에 있나요?",
          "original_form": "카페거리쪽에 있나요?",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021732.1.48",
          "form": "소나무길을 걷다보면 카페거리가 나와요",
          "original_form": "소나무길을 걷다보면 카페거리가 나와요",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021732.1.49",
          "form": "소나무길을 걷다보면 옆에 바다가 보이고 너무 힐링돼요",
          "original_form": "소나무길을 걷다보면 옆에 바다가 보이고 너무 힐링돼요",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021732.1.50",
          "form": "꼭 한번 걸어보시길 추천할게요!",
          "original_form": "꼭 한번 걸어보시길 추천할게요!",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021732.1.51",
          "form": "저도 꼭 가봐야겠어요",
          "original_form": "저도 꼭 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021732.1.52",
          "form": "좋은 정보 고맙습니다",
          "original_form": "좋은 정보 고맙습니다",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021732.1.53",
          "form": "저는 블라디보스톡을 가려고 하다가",
          "original_form": "저는 블라디보스톡을 가려고 하다가",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021732.1.54",
          "form": "코로나가 터지는 바람에 못가게 됐어요",
          "original_form": "코로나가 터지는 바람에 못가게 됐어요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021732.1.55",
          "form": "블라디보스톡 가서 발레공연을 봐보고싶었거든용 ㅋㅋㅋㅋ",
          "original_form": "블라디보스톡 가서 발레공연을 봐보고싶었거든용 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021732.1.56",
          "form": "아ㅜㅜ",
          "original_form": "아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021732.1.57",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021732.1.58",
          "form": "블라디보스톡에서 게랑 맥주 먹으면 그렇게 좋다더라구요..",
          "original_form": "블라디보스톡에서 게랑 맥주 먹으면 그렇게 좋다더라구요..",
          "speaker_id": "1",
          "time": "20210828 22:19"
