      "utterance": [
        {
          "id": "MDRW2100046928.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211108 11:08"
        },
        {
          "id": "MDRW2100046928.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211108 11:30"
        },
        {
          "id": "MDRW2100046928.1.3",
          "form": "혹시 클럽하우스 아시나요?",
          "original_form": "혹시 클럽하우스 아시나요?",
          "speaker_id": "2",
          "time": "20211108 11:30"
        },
        {
          "id": "MDRW2100046928.1.4",
          "form": "클럽하우스 들어본적 있어요",
          "original_form": "클럽하우스 들어본적 있어요",
          "speaker_id": "1",
          "time": "20211108 11:37"
        },
        {
          "id": "MDRW2100046928.1.5",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20211109 00:14"
        },
        {
          "id": "MDRW2100046928.1.6",
          "form": "해보셨나요 ㅎㅎ",
          "original_form": "해보셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:14"
        },
        {
          "id": "MDRW2100046928.1.7",
          "form": "아니요 저도 해보지는 못했어요",
          "original_form": "아니요 저도 해보지는 못했어요",
          "speaker_id": "1",
          "time": "20211109 00:21"
        },
        {
          "id": "MDRW2100046928.1.8",
          "form": "아아 혹시 안드로이드 유저신가요 ㅎㅎㅎ",
          "original_form": "아아 혹시 안드로이드 유저신가요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046928.1.9",
          "form": "네 안드로이드 사용해요",
          "original_form": "네 안드로이드 사용해요",
          "speaker_id": "1",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046928.1.10",
          "form": "아하 저두",
          "original_form": "아하 저두",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046928.1.11",
          "form": "안드로이드 유저여서 못했거든요 ㅋㅋㅋ",
          "original_form": "안드로이드 유저여서 못했거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046928.1.12",
          "form": "해보고싶긴했는데",
          "original_form": "해보고싶긴했는데",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046928.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046928.1.14",
          "form": "근데 지금 검색해보니까",
          "original_form": "근데 지금 검색해보니까",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046928.1.15",
          "form": "최근에 안드로이드 유저들도 사용할 수 있게됐다네요",
          "original_form": "최근에 안드로이드 유저들도 사용할 수 있게됐다네요",
          "speaker_id": "2",
          "time": "20211109 00:27"
        },
        {
          "id": "MDRW2100046928.1.16",
          "form": "정말요 !",
          "original_form": "정말요 !",
          "speaker_id": "1",
          "time": "20211109 00:34"
        },
        {
          "id": "MDRW2100046928.1.17",
          "form": "클럽하우스 재미있나요?",
          "original_form": "클럽하우스 재미있나요?",
          "speaker_id": "1",
          "time": "20211109 00:34"
        },
        {
          "id": "MDRW2100046928.1.18",
          "form": "엇 사실 저도 안해봤어요 ㅠ",
          "original_form": "엇 사실 저도 안해봤어요 ㅠ",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046928.1.19",
          "form": "요즘은",
          "original_form": "요즘은",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046928.1.20",
          "form": "사람들이",
          "original_form": "사람들이",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046928.1.21",
          "form": "클럽하우스 많이 안하는거같더라구요",
          "original_form": "클럽하우스 많이 안하는거같더라구요",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046928.1.22",
          "form": "작년에 잠깐 유행처럼",
          "original_form": "작년에 잠깐 유행처럼",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046928.1.23",
          "form": "전 클럽하우스같은거보다 트위치나 아프리카같은",
          "original_form": "전 클럽하우스같은거보다 트위치나 아프리카같은",
          "speaker_id": "1",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046928.1.24",
          "form": "지나간 느낌",
          "original_form": "지나간 느낌",
          "speaker_id": "2",
          "time": "20211109 00:37"
        },
        {
          "id": "MDRW2100046928.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20211109 00:37"
        },
        {
          "id": "MDRW2100046928.1.26",
          "form": "개인방송듣는게 더 좋아서요",
          "original_form": "개인방송듣는게 더 좋아서요",
          "speaker_id": "1",
          "time": "20211109 00:37"
        },
        {
          "id": "MDRW2100046928.1.27",
          "form": "그게 나은 거 같아요 저두 ㅋㅋㅋ",
          "original_form": "그게 나은 거 같아요 저두 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:37"
        },
        {
          "id": "MDRW2100046928.1.28",
          "form": "개인방송",
          "original_form": "개인방송",
          "speaker_id": "2",
          "time": "20211109 00:37"
        },
        {
          "id": "MDRW2100046928.1.29",
          "form": "보시는거 뭐 있으세요?",
          "original_form": "보시는거 뭐 있으세요?",
          "speaker_id": "2",
          "time": "20211109 00:37"
        },
        {
          "id": "MDRW2100046928.1.30",
          "form": "딱히 정해놓고 보는 건 없고 그때그때",
          "original_form": "딱히 정해놓고 보는 건 없고 그때그때",
          "speaker_id": "1",
          "time": "20211109 00:38"
        },
        {
          "id": "MDRW2100046928.1.31",
          "form": "인기방송 시간때우기로 봐요",
          "original_form": "인기방송 시간때우기로 봐요",
          "speaker_id": "1",
          "time": "20211109 00:38"
        },
        {
          "id": "MDRW2100046928.1.32",
          "form": "아하 그게 편하고 좋죠 ㅋㅋㅋ\r\n",
          "original_form": "아하 그게 편하고 좋죠 ㅋㅋㅋ\r\n",
          "speaker_id": "2",
          "time": "20211109 00:39"
        }
      ]
    }
