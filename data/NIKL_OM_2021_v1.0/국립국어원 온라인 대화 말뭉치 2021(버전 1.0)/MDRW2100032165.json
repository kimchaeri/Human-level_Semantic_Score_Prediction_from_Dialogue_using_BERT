      "utterance": [
        {
          "id": "MDRW2100032165.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032165.1.2",
          "form": "네네 고양이 키워요 ㅋㅋ",
          "original_form": "네네 고양이 키워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032165.1.3",
          "form": "키우시나요",
          "original_form": "키우시나요",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032165.1.4",
          "form": "와 저는 안 키우는데",
          "original_form": "와 저는 안 키우는데",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032165.1.5",
          "form": "고양이 너무 예쁘겠어요",
          "original_form": "고양이 너무 예쁘겠어요",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032165.1.6",
          "form": "ㅋㅋㅋ네예뻐요 ㅋㅋ 귀찮을때도 가끔 있지만ㅜㅜㅋ",
          "original_form": "ㅋㅋㅋ네예뻐요 ㅋㅋ 귀찮을때도 가끔 있지만ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032165.1.7",
          "form": "털이 많이 날려서 비염인이라 힘들때가 있어요 ㅋㅋㅋ",
          "original_form": "털이 많이 날려서 비염인이라 힘들때가 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032165.1.8",
          "form": "와 비염인이신데 고양이 키우시는 거면 엄청 대단하시네요",
          "original_form": "와 비염인이신데 고양이 키우시는 거면 엄청 대단하시네요",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032165.1.9",
          "form": "어떻게 키우게 되신 거예요?",
          "original_form": "어떻게 키우게 되신 거예요?",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032165.1.10",
          "form": "앗 저는 선택권이 없었고 ㅋㅋㅋ 혈육이 어느순간 데려왔어요 ㅠㅠㅋㅋ",
          "original_form": "앗 저는 선택권이 없었고 ㅋㅋㅋ 혈육이 어느순간 데려왔어요 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032165.1.11",
          "form": "아이고ㅋㅋㅋㅋ그러셨구나",
          "original_form": "아이고ㅋㅋㅋㅋ그러셨구나",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032165.1.12",
          "form": "고양이 이름 물어봐도 돼요?",
          "original_form": "고양이 이름 물어봐도 돼요?",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032165.1.13",
          "form": "ㅋㅋㅋ네네 먼지에요",
          "original_form": "ㅋㅋㅋ네네 먼지에요",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032165.1.14",
          "form": "와 너무 귀엽다",
          "original_form": "와 너무 귀엽다",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032165.1.15",
          "form": "ㅋㅋㅋㅋ근데 정말 먼지같은 털이 마니빠져요ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ근데 정말 먼지같은 털이 마니빠져요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032165.1.16",
          "form": "그래도 예뻐여 사람 워낙조아하고 따르고ㅋㅋ",
          "original_form": "그래도 예뻐여 사람 워낙조아하고 따르고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032165.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무 귀여울 것 같아요 와",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무 귀여울 것 같아요 와",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032165.1.18",
          "form": "고양이들은 사람 안 좋아한다는 이미지가 있었는데 너무 귀엽네요ㅜ",
          "original_form": "고양이들은 사람 안 좋아한다는 이미지가 있었는데 너무 귀엽네요ㅜ",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032165.1.19",
          "form": "아니에오 사람 엄청 조아하고 ㅋㅋㅋ 쓰다듬어달라해요",
          "original_form": "아니에오 사람 엄청 조아하고 ㅋㅋㅋ 쓰다듬어달라해요",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032165.1.20",
          "form": "저도 키우기전까진 몰랐어요 ㅎㅎ",
          "original_form": "저도 키우기전까진 몰랐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032165.1.21",
          "form": "와 너무 귀엽겠다",
          "original_form": "와 너무 귀엽겠다",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032165.1.22",
          "form": "쓰다듬어달라그래요?? 너무 예쁘다 진짜",
          "original_form": "쓰다듬어달라그래요?? 너무 예쁘다 진짜",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032165.1.23",
          "form": "네네 근데 안해주면 살짝 물긴해욬ㅋㅋㅋ 후...자기맘대로 안해주면 물건떨어뜨리기도하고 ㅋㅋㅋ",
          "original_form": "네네 근데 안해주면 살짝 물긴해욬ㅋㅋㅋ 후...자기맘대로 안해주면 물건떨어뜨리기도하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032165.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그런 것도 다 너무 귀여울 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그런 것도 다 너무 귀여울 것 같아요",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032165.1.25",
          "form": "제 친구는 털 빠짐 때문에 고생하던데",
          "original_form": "제 친구는 털 빠짐 때문에 고생하던데",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032165.1.26",
          "form": "그건맞아요 저도 단모종인데 엄청 빠져서 ㅋㅋㅋ",
          "original_form": "그건맞아요 저도 단모종인데 엄청 빠져서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032165.1.27",
          "form": "덕분에 건조기를 샀답니다...ㅎㅎ",
          "original_form": "덕분에 건조기를 샀답니다...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032165.1.28",
          "form": "ㅋㅋㅋㅋ와 건조기 걔도 맨날 돌돌이 하더라구요..",
          "original_form": "ㅋㅋㅋㅋ와 건조기 걔도 맨날 돌돌이 하더라구요..",
