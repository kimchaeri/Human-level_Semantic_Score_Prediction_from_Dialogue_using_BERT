      "utterance": [
        {
          "id": "MDRW2100039974.1.1",
          "form": "저녁은뭐드셨나요?",
          "original_form": "저녁은뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039974.1.2",
          "form": "저 오리주물럭이요!!",
          "original_form": "저 오리주물럭이요!!",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039974.1.3",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039974.1.4",
          "form": "와~직접요리하셨나요",
          "original_form": "와~직접요리하셨나요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039974.1.5",
          "form": "전 저녁에 카레먹었어요",
          "original_form": "전 저녁에 카레먹었어요",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039974.1.6",
          "form": "넵ㅋㅋ 소스좀넣고 야채 몽땅",
          "original_form": "넵ㅋㅋ 소스좀넣고 야채 몽땅",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039974.1.7",
          "form": "카레 만드셨나요?",
          "original_form": "카레 만드셨나요?",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039974.1.8",
          "form": "아뇨 ㅎㅎ 전 구냥데워먹었어요 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ 전 구냥데워먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039974.1.9",
          "form": "오리주물럭은 한번도안해봒네요!",
          "original_form": "오리주물럭은 한번도안해봒네요!",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039974.1.10",
          "form": "저도 오늘 처음! ㅋㅋ 제육볶음이랑 비슷하게 볶았어요ㅎㅎ",
          "original_form": "저도 오늘 처음! ㅋㅋ 제육볶음이랑 비슷하게 볶았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039974.1.11",
          "form": "아~ 돼지고기대신 오리로 양념해서 하면되겠군요~",
          "original_form": "아~ 돼지고기대신 오리로 양념해서 하면되겠군요~",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039974.1.12",
          "form": "깻잎쌈이랑 무쌈같이먹으면 먓있겠어요!",
          "original_form": "깻잎쌈이랑 무쌈같이먹으면 먓있겠어요!",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039974.1.13",
          "form": "네 버섯이랑 양파랑 깻잎!",
          "original_form": "네 버섯이랑 양파랑 깻잎!",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039974.1.14",
          "form": "요즘 오리는 다 손질되어 팔더라고요",
          "original_form": "요즘 오리는 다 손질되어 팔더라고요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039974.1.15",
          "form": "순살인가요??",
          "original_form": "순살인가요??",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039974.1.16",
          "form": "네 살만 발라서 한입크기로 주더라고요!",
          "original_form": "네 살만 발라서 한입크기로 주더라고요!",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039974.1.17",
          "form": "와~ 그렇게나오는균요! 오리는 집에서 해볼생각을 못했어요!",
          "original_form": "와~ 그렇게나오는균요! 오리는 집에서 해볼생각을 못했어요!",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100039974.1.18",
          "form": "네 저도 올해 처음 알았어요!!!",
          "original_form": "네 저도 올해 처음 알았어요!!!",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100039974.1.19",
          "form": "오리기름 괜찮으시다면 추천이요!",
          "original_form": "오리기름 괜찮으시다면 추천이요!",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100039974.1.20",
          "form": "진짜 생오리 요리를 언제먹었는지 기억도안나네요 ㅎㅎ",
          "original_form": "진짜 생오리 요리를 언제먹었는지 기억도안나네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100039974.1.21",
          "form": "요리잘하시나봐요ㅠㅠ",
          "original_form": "요리잘하시나봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039974.1.22",
          "form": "그냥 먹고살라구ㅋㅋ",
          "original_form": "그냥 먹고살라구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100039974.1.23",
          "form": "해외가서 마트장봐서 한식해먹고요",
          "original_form": "해외가서 마트장봐서 한식해먹고요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        }
      ]
    }
  ]
