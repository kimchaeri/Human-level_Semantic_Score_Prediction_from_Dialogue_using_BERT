      "utterance": [
        {
          "id": "MDRW2100021963.1.1",
          "form": "주말에 일이야기라니",
          "original_form": "주말에 일이야기라니",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021963.1.2",
          "form": "미안ㅋㅋㅋㅋ",
          "original_form": "미안ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021963.1.3",
          "form": "인생직업은 아닌데 안락",
          "original_form": "인생직업은 아닌데 안락",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021963.1.4",
          "form": "발전은 없어도 되는데 안정적이얌 니는",
          "original_form": "발전은 없어도 되는데 안정적이얌 니는",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021963.1.5",
          "form": "나는 전회사보다 연봉이줄어서",
          "original_form": "나는 전회사보다 연봉이줄어서",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021963.1.6",
          "form": "투잡하고싶다",
          "original_form": "투잡하고싶다",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021963.1.7",
          "form": "일단 나이드니깐 뭔가 하고자하는게 없네..",
          "original_form": "일단 나이드니깐 뭔가 하고자하는게 없네..",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021963.1.8",
          "form": "나도...",
          "original_form": "나도...",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021963.1.9",
          "form": "투잡은 머?",
          "original_form": "투잡은 머?",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021963.1.10",
          "form": "뭐..그냥 이모티콘파는거라던가..그런거?",
          "original_form": "뭐..그냥 이모티콘파는거라던가..그런거?",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021963.1.11",
          "form": "수익이 괜찮나?",
          "original_form": "수익이 괜찮나?",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021963.1.12",
          "form": "커미션이나",
          "original_form": "커미션이나",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021963.1.13",
          "form": "외주작업 커미션이런거는 괜찮은듯",
          "original_form": "외주작업 커미션이런거는 괜찮은듯",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021963.1.14",
          "form": "투잡하면 안되서 하려면 몰래해야되",
          "original_form": "투잡하면 안되서 하려면 몰래해야되",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021963.1.15",
          "form": "아이디어스 같은거보면",
          "original_form": "아이디어스 같은거보면",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021963.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021963.1.17",
          "form": "막 그림도 그려주고 그런던데 강아지..",
          "original_form": "막 그림도 그려주고 그런던데 강아지..",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021963.1.18",
          "form": "아이디어스 등록해볼까",
          "original_form": "아이디어스 등록해볼까",
          "speaker_id": "1",
          "time": "20210829 00:17"
        }
      ]
