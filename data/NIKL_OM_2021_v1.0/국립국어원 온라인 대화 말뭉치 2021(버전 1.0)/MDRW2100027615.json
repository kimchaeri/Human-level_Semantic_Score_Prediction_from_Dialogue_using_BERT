      "utterance": [
        {
          "id": "MDRW2100027615.1.1",
          "form": "소불고기~~",
          "original_form": "소불고기~~",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027615.1.2",
          "form": "백신맞은 아가씨 주무시나요",
          "original_form": "백신맞은 아가씨 주무시나요",
          "speaker_id": "2",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100027615.1.3",
          "form": "ㅎㅎ 와 정신없이 졸았네요",
          "original_form": "ㅎㅎ 와 정신없이 졸았네요",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100027615.1.4",
          "form": "백신에 수면제 성분도 있나요 ㄷㄷ",
          "original_form": "백신에 수면제 성분도 있나요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100027615.1.5",
          "form": "저녁 시간이 다가왔네요",
          "original_form": "저녁 시간이 다가왔네요",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100027615.1.6",
          "form": "ㅎㅎ 오늘 저녁 메뉴는 뭔가요",
          "original_form": "ㅎㅎ 오늘 저녁 메뉴는 뭔가요",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100027615.1.7",
          "form": "소고기 이마트에서 털어왔습니다",
          "original_form": "소고기 이마트에서 털어왔습니다",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100027615.1.8",
          "form": "유통기한 모레까지인데",
          "original_form": "유통기한 모레까지인데",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100027615.1.9",
          "form": "40퍼 할인해서 ㅋㅋ",
          "original_form": "40퍼 할인해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100027615.1.10",
          "form": "헉 대박사건",
          "original_form": "헉 대박사건",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100027615.1.11",
          "form": "부르주아의 상징인 소고기를 드시는 건가요!!",
          "original_form": "부르주아의 상징인 소고기를 드시는 건가요!!",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100027615.1.12",
          "form": "똑똑똑 집앞이에요",
          "original_form": "똑똑똑 집앞이에요",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100027615.1.13",
          "form": "40퍼 할인한 소고기입니다만",
          "original_form": "40퍼 할인한 소고기입니다만",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100027615.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100027615.1.15",
          "form": "지금 2인 위로는 집합 금지에요",
          "original_form": "지금 2인 위로는 집합 금지에요",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100027615.1.16",
          "form": "돌아가세요~~~",
          "original_form": "돌아가세요~~~",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100027615.1.17",
          "form": "name2이 나가고 제가 들어가면 안될까욤",
          "original_form": "&name2&이 나가고 제가 들어가면 안될까욤",
          "speaker_id": "1",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100027615.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 소고기 먹겠다고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 소고기 먹겠다고",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100027615.1.19",
          "form": "저를 쫓으십니까!!",
          "original_form": "저를 쫓으십니까!!",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100027615.1.20",
          "form": "근데 이미 다 먹었어요",
          "original_form": "근데 이미 다 먹었어요",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100027615.1.21",
          "form": "어떻게 40퍼 할인 소고기를 한끼만 살 수 있죠?",
          "original_form": "어떻게 40퍼 할인 소고기를 한끼만 살 수 있죠?",
