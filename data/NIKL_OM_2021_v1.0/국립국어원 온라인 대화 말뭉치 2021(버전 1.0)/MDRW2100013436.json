      "utterance": [
        {
          "id": "MDRW2100013436.1.1",
          "form": "혹시 키우는 동물 있으세요?",
          "original_form": "혹시 키우는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013436.1.2",
          "form": "저는 따로 없어요",
          "original_form": "저는 따로 없어요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013436.1.3",
          "form": "동물키우시나요?",
          "original_form": "동물키우시나요?",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013436.1.4",
          "form": "저는 강아지 키우고있어요",
          "original_form": "저는 강아지 키우고있어요",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013436.1.5",
          "form": "강아지 좋아하세요?",
          "original_form": "강아지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013436.1.6",
          "form": "네 좋아해요 저는 작고 복슬거리고 하얀아이들 특히 좋아해요",
          "original_form": "네 좋아해요 저는 작고 복슬거리고 하얀아이들 특히 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013436.1.7",
          "form": "제일 중요한거는 복슬복슬",
          "original_form": "제일 중요한거는 복슬복슬",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013436.1.8",
          "form": "어떤 종이 제일 좋은거같아요?",
          "original_form": "어떤 종이 제일 좋은거같아요?",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013436.1.9",
          "form": "비숑? 말티즈? ㅎㅎㅎ",
          "original_form": "비숑? 말티즈? ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013436.1.10",
          "form": "푸들도 좋아요",
          "original_form": "푸들도 좋아요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013436.1.11",
          "form": "사실 강아지들은 다 귀엽죠",
          "original_form": "사실 강아지들은 다 귀엽죠",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013436.1.12",
          "form": "어떤 동물이 제일 좋은거같아요?",
          "original_form": "어떤 동물이 제일 좋은거같아요?",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013436.1.13",
          "form": "전 최근에 고양이요 그 고양이 특유의",
          "original_form": "전 최근에 고양이요 그 고양이 특유의",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013436.1.14",
          "form": "도도댕청미가 너무 귀여워요",
          "original_form": "도도댕청미가 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013436.1.15",
          "form": "혹시 올빼미나 그런거도 좋아하세요?",
          "original_form": "혹시 올빼미나 그런거도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100013436.1.16",
          "form": "엇 생각도 못해봤네요",
          "original_form": "엇 생각도 못해봤네요",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013436.1.17",
          "form": "혹시 키우시나요?",
          "original_form": "혹시 키우시나요?",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013436.1.18",
          "form": "아뇨 키우진 않는데 유튜브로 보면 되게 귀엽더라고요",
          "original_form": "아뇨 키우진 않는데 유튜브로 보면 되게 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013436.1.19",
          "form": "오리키우는분은 봤는데 올빼미... 한번 찾아봐야겠어요.",
          "original_form": "오리키우는분은 봤는데 올빼미... 한번 찾아봐야겠어요.",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013436.1.20",
          "form": "조류쪽에 귀여운게 의외로 많더라구요 ㅎㅎ",
          "original_form": "조류쪽에 귀여운게 의외로 많더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:47"
        }
      ]
