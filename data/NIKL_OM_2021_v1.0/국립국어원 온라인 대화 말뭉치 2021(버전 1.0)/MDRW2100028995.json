      "utterance": [
        {
          "id": "MDRW2100028995.1.1",
          "form": "안녕하세영!",
          "original_form": "안녕하세영!",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028995.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028995.1.3",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028995.1.4",
          "form": "앗 디카페인으로는 자주 마셔요",
          "original_form": "앗 디카페인으로는 자주 마셔요",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028995.1.5",
          "form": "커피 좋아하시나여?",
          "original_form": "커피 좋아하시나여?",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028995.1.6",
          "form": "네 전 좋아해요 ㅋㅋㅋ 인스턴트 좋아하시면 보통 카누 드시나요??",
          "original_form": "네 전 좋아해요 ㅋㅋㅋ 인스턴트 좋아하시면 보통 카누 드시나요??",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028995.1.7",
          "form": "앗 아뇨 카누 디카페인 한 번 먹어봤는데",
          "original_form": "앗 아뇨 카누 디카페인 한 번 먹어봤는데",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028995.1.8",
          "form": "뭔가 탄맛이 나더라구요ㅠㅠㅠㅠ",
          "original_form": "뭔가 탄맛이 나더라구요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028995.1.9",
          "form": "디카페인 아닌 것도 탄맛나나요?",
          "original_form": "디카페인 아닌 것도 탄맛나나요?",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028995.1.10",
          "form": "아 맞아요 쓰고 탄맛나요 ㅋㅋㅋ",
          "original_form": "아 맞아요 쓰고 탄맛나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028995.1.11",
          "form": "인스턴트 중에 디카페인 괜찮은 제품 있나요?",
          "original_form": "인스턴트 중에 디카페인 괜찮은 제품 있나요?",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028995.1.12",
          "form": "아뇨 인스턴트중에 디카페인은..",
          "original_form": "아뇨 인스턴트중에 디카페인은..",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028995.1.13",
          "form": "카누밖에 몰라요 흑흑...",
          "original_form": "카누밖에 몰라요 흑흑...",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028995.1.14",
          "form": "ㅠㅠㅠㅠㅠ역시 카누가 대세는 대세인 거 같아요ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ역시 카누가 대세는 대세인 거 같아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028995.1.15",
          "form": "디카페인은 보통 카페에서 드시겠군요ㅠㅠ",
          "original_form": "디카페인은 보통 카페에서 드시겠군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028995.1.16",
          "form": "맞아여ㅠㅠㅠ 카페도 프랜차이즈 몇개밖에 없어서..",
          "original_form": "맞아여ㅠㅠㅠ 카페도 프랜차이즈 몇개밖에 없어서..",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028995.1.17",
          "form": "어떤커피 제일 좋아하세요?",
          "original_form": "어떤커피 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028995.1.18",
          "form": "저는 역시 스타벅스요  ㅋㅋㅋㅋ",
          "original_form": "저는 역시 스타벅스요  ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028995.1.19",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028995.1.20",
          "form": "저도 스벅 돌체라떼!!",
          "original_form": "저도 스벅 돌체라떼!!",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028995.1.21",
          "form": "디카페인으로 마셔요 ㅠㅠㅠㅠ",
          "original_form": "디카페인으로 마셔요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028995.1.22",
          "form": "아 돌체라떼 맛나죠!!",
          "original_form": "아 돌체라떼 맛나죠!!",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028995.1.23",
          "form": "시럽 몇 번 펌핑하세요??",
          "original_form": "시럽 몇 번 펌핑하세요??",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028995.1.24",
          "form": "후후 무조건 두번추가합니다",
          "original_form": "후후 무조건 두번추가합니다",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028995.1.25",
          "form": "어떤 메뉴가 젤 좋으세요?",
          "original_form": "어떤 메뉴가 젤 좋으세요?",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100028995.1.26",
          "form": "저는 바닐라라떼요!",
          "original_form": "저는 바닐라라떼요!",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100028995.1.27",
          "form": "죠리퐁맛 나더라구요",
          "original_form": "죠리퐁맛 나더라구요",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100028995.1.28",
          "form": "아아 맞아요 구수하죠 뭔가..",
          "original_form": "아아 맞아요 구수하죠 뭔가..",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028995.1.29",
          "form": "바닐라라떼가",
          "original_form": "바닐라라떼가",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028995.1.30",
          "form": "은근 칼로리가 낮았던 것 같은데....",
          "original_form": "은근 칼로리가 낮았던 것 같은데....",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028995.1.31",
          "form": "맞아요!!ㅋㅋㅋㅋ 구수한 맛이 좋더라구요",
          "original_form": "맞아요!!ㅋㅋㅋㅋ 구수한 맛이 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028995.1.32",
          "form": "칼로리가....근데 전 시럽 많이 넣는 편이라 소용이 없더라구요",
          "original_form": "칼로리가....근데 전 시럽 많이 넣는 편이라 소용이 없더라구요",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028995.1.33",
          "form": "시즌 메뉴 좋아하세요???",
          "original_form": "시즌 메뉴 좋아하세요???",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028995.1.34",
          "form": "시즌메뉴 음..",
          "original_form": "시즌메뉴 음..",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100028995.1.35",
          "form": "맛잇는 건 좋아해요!",
          "original_form": "맛잇는 건 좋아해요!",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100028995.1.36",
          "form": "토피넛이나 벚꽃메뉴...",
          "original_form": "토피넛이나 벚꽃메뉴...",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100028995.1.37",
          "form": "전 난리났던 슈크림은 그저그렇더라고요ㅠㅠ",
          "original_form": "전 난리났던 슈크림은 그저그렇더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100028995.1.38",
          "form": "아 저도 슈크림은 별로였어요",
          "original_form": "아 저도 슈크림은 별로였어요",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100028995.1.39",
          "form": "한동안 더워서 그런지 피지오 계열이 나은 거 같아요",
          "original_form": "한동안 더워서 그런지 피지오 계열이 나은 거 같아요",
