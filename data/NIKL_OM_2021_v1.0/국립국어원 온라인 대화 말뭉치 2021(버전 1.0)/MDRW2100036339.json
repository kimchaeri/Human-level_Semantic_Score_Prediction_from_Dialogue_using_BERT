      "utterance": [
        {
          "id": "MDRW2100036339.1.1",
          "form": "이러고 그날 두병을 마셨어욬ㅋㅋㅋㅋ",
          "original_form": "이러고 그날 두병을 마셨어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.2",
          "form": "ㅋㅋㅋ훅가죠",
          "original_form": "ㅋㅋㅋ훅가죠",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.3",
          "form": "근데 다음날 머리가 너무 깨질것같이 아프고",
          "original_form": "근데 다음날 머리가 너무 깨질것같이 아프고",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.4",
          "form": "달달달 ㅋㅋㅋ 전 자몽좋아햇는데",
          "original_form": "달달달 ㅋㅋㅋ 전 자몽좋아햇는데",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.5",
          "form": "하루 온종일 그러는거에요ㅠㅠ",
          "original_form": "하루 온종일 그러는거에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.6",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.7",
          "form": "블루베리랑",
          "original_form": "블루베리랑",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.8",
          "form": "자몽도 맛있겠다...",
          "original_form": "자몽도 맛있겠다...",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.9",
          "form": "이젠 안마셔서 그런건 ㅠ",
          "original_form": "이젠 안마셔서 그런건 ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.10",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.11",
          "form": "숙취가 너무 더러워섴ㅋㅋㅋ",
          "original_form": "숙취가 너무 더러워섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.12",
          "form": "달아서 ㅋㅋ물리더라구요",
          "original_form": "달아서 ㅋㅋ물리더라구요",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036339.1.13",
          "form": "그때 21살이었는데",
          "original_form": "그때 21살이었는데",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036339.1.14",
          "form": "많이들어가서 그런거겟죠?",
          "original_form": "많이들어가서 그런거겟죠?",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036339.1.15",
          "form": "한창 술 제일 잘 마실때였고 간도 팔팔했는데",
          "original_form": "한창 술 제일 잘 마실때였고 간도 팔팔했는데",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036339.1.16",
          "form": "하학 ㅠㅠ",
          "original_form": "하학 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036339.1.17",
          "form": "아 제 간이 못당하더라구요",
          "original_form": "아 제 간이 못당하더라구요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036339.1.18",
          "form": "숙취가ㅠㅠㅠㅋㅋㅋ",
          "original_form": "숙취가ㅠㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036339.1.19",
          "form": "이거 실제로 설탕? 많이 들어간대요!",
          "original_form": "이거 실제로 설탕? 많이 들어간대요!",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036339.1.20",
          "form": "그래서 달달~한 맛에 훅가는거라고ㅠㅠ 하더라구요",
          "original_form": "그래서 달달~한 맛에 훅가는거라고ㅠㅠ 하더라구요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        }
      ]
