      "utterance": [
        {
          "id": "MDRW2100043861.1.1",
          "form": "나 가면",
          "original_form": "나 가면",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043861.1.2",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043861.1.3",
          "form": "사오고싶은거 너무 많아",
          "original_form": "사오고싶은거 너무 많아",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043861.1.4",
          "form": "나 신칸센도 타보고싶어!",
          "original_form": "나 신칸센도 타보고싶어!",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043861.1.5",
          "form": "신칸센이 뭐였지??",
          "original_form": "신칸센이 뭐였지??",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.6",
          "form": "기차?",
          "original_form": "기차?",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.7",
          "form": "지하철!",
          "original_form": "지하철!",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.9",
          "form": "마자 그리고 일본에 코타츠 열차도",
          "original_form": "마자 그리고 일본에 코타츠 열차도",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.10",
          "form": "일본 지하철 엄청 복잡하대",
          "original_form": "일본 지하철 엄청 복잡하대",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.11",
          "form": "있다던데",
          "original_form": "있다던데",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.12",
          "form": "마저..",
          "original_form": "마저..",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.13",
          "form": "근데 궁금혀!",
          "original_form": "근데 궁금혀!",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.14",
          "form": "헐 그게 뭐야",
          "original_form": "헐 그게 뭐야",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043861.1.15",
          "form": "열차에 코타츠 있는 거야?",
          "original_form": "열차에 코타츠 있는 거야?",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.16",
          "form": "엉엉!",
          "original_form": "엉엉!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.17",
          "form": "좌석이",
          "original_form": "좌석이",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.18",
          "form": "코타츠래!",
          "original_form": "코타츠래!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.19",
          "form": "헉 대박",
          "original_form": "헉 대박",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.20",
          "form": "겨울에 타면 좋겠네ㅋㅋㅋ",
          "original_form": "겨울에 타면 좋겠네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.21",
          "form": "그지 댜박이지!",
          "original_form": "그지 댜박이지!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.22",
          "form": "마자마자!",
          "original_form": "마자마자!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.23",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.24",
          "form": "여행 가고 싶어ㅜㅜ",
          "original_form": "여행 가고 싶어ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043861.1.25",
          "form": "나두...",
          "original_form": "나두...",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043861.1.26",
          "form": "풀빌라..",
          "original_form": "풀빌라..",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043861.1.27",
          "form": "가고싶다",
          "original_form": "가고싶다",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043861.1.28",
          "form": "동남아 가면",
          "original_form": "동남아 가면",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043861.1.29",
          "form": "무조건 풀빌라",
          "original_form": "무조건 풀빌라",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043861.1.30",
          "form": "과일 왕창사먹고",
          "original_form": "과일 왕창사먹고",
          "speaker_id": "2",
