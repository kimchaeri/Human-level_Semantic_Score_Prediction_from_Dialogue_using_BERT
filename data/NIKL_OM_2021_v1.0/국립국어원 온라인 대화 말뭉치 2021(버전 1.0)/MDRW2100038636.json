      "utterance": [
        {
          "id": "MDRW2100038636.1.1",
          "form": "많은사람들과 대화중이구나?",
          "original_form": "많은사람들과 대화중이구나?",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100038636.1.2",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100038636.1.3",
          "form": "그럼 도서로 할 걸 그랬네",
          "original_form": "그럼 도서로 할 걸 그랬네",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038636.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038636.1.5",
          "form": "바꿔",
          "original_form": "바꿔",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038636.1.6",
          "form": "엥 아까워 그냥 해",
          "original_form": "엥 아까워 그냥 해",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038636.1.7",
          "form": "그랰ㅋㅋ",
          "original_form": "그랰ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038636.1.8",
          "form": "오늘은 꼭 상견니 마지막 화를 볼 것이다",
          "original_form": "오늘은 꼭 상견니 마지막 화를 볼 것이다",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038636.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038636.1.10",
          "form": "대만? 태국?어디 드라마 랬나",
          "original_form": "대만? 태국?어디 드라마 랬나",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038636.1.11",
          "form": "대만!",
          "original_form": "대만!",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038636.1.12",
          "form": "재밌음?ㅋㅋㅋㅋ",
          "original_form": "재밌음?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038636.1.13",
          "form": "응 처음에 타임슬립 전까지만 버티면 괜찮음",
          "original_form": "응 처음에 타임슬립 전까지만 버티면 괜찮음",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100038636.1.14",
          "form": "중국드라마도 그렇지만",
          "original_form": "중국드라마도 그렇지만",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100038636.1.15",
          "form": "다른데거도 기묘하던데ㅋㅋㅋ",
          "original_form": "다른데거도 기묘하던데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100038636.1.16",
          "form": "어떤 기묘함?",
          "original_form": "어떤 기묘함?",
          "speaker_id": "1",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100038636.1.17",
          "form": "설정이 조잡한데 밀고가는 느낌",
          "original_form": "설정이 조잡한데 밀고가는 느낌",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100038636.1.18",
          "form": "시청자의 이해와는 상관없이 흘러가는 스토리",
          "original_form": "시청자의 이해와는 상관없이 흘러가는 스토리",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100038636.1.19",
          "form": "아...그거 상견니도 조금 있엌ㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "아...그거 상견니도 조금 있엌ㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100038636.1.20",
          "form": "이렇게 편견이 추가됨",
          "original_form": "이렇게 편견이 추가됨",
          "speaker_id": "2",
          "time": "20210902 18:22"
        },
        {
