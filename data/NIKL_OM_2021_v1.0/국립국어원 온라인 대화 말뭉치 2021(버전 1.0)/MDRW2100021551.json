      "utterance": [
        {
          "id": "MDRW2100021551.1.1",
          "form": "안녕하세용 티비 뭐 보세용?",
          "original_form": "안녕하세용 티비 뭐 보세용?",
          "speaker_id": "1",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100021551.1.2",
          "form": "저 지금 광자매 보고있어요",
          "original_form": "저 지금 광자매 보고있어요",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100021551.1.3",
          "form": "아 지금 온에어로 하나요?",
          "original_form": "아 지금 온에어로 하나요?",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100021551.1.4",
          "form": "이거 인기많더라구요",
          "original_form": "이거 인기많더라구요",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100021551.1.5",
          "form": "온에어로 하지않을까요? ㅎㅎ",
          "original_form": "온에어로 하지않을까요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100021551.1.6",
          "form": "혹시 유튜브 보세요?",
          "original_form": "혹시 유튜브 보세요?",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100021551.1.7",
          "form": "가끔 보는데 즐겨보진않아요",
          "original_form": "가끔 보는데 즐겨보진않아요",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021551.1.8",
          "form": "넘 한도 끝도없이 보는것같아 일부러 안보려고 노력중이예요",
          "original_form": "넘 한도 끝도없이 보는것같아 일부러 안보려고 노력중이예요",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021551.1.9",
          "form": "오호ㅋㅋㅋ 저는 동물농장같은 영상 주로 즐겨봐요",
          "original_form": "오호ㅋㅋㅋ 저는 동물농장같은 영상 주로 즐겨봐요",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100021551.1.10",
          "form": "ㅎㅎ 낼하겠네요",
          "original_form": "ㅎㅎ 낼하겠네요",
          "speaker_id": "2",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100021551.1.11",
          "form": "유튜브에도 클립영상으로 주로 올라와용",
          "original_form": "유튜브에도 클립영상으로 주로 올라와용",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021551.1.12",
          "form": "애니멀바 라는 채널인데",
          "original_form": "애니멀바 라는 채널인데",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021551.1.13",
          "form": "추천드립니다",
          "original_form": "추천드립니다",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021551.1.14",
          "form": "아 그거 본 적 있어요 ㅎㅎ",
          "original_form": "아 그거 본 적 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021551.1.15",
          "form": "짧게짧게 핵심부분만 요약해서 틀어주는데",
          "original_form": "짧게짧게 핵심부분만 요약해서 틀어주는데",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021551.1.16",
          "form": "집중도 잘되고 동물들이 넘 귀여워서ㅋㅋㅋㅋㅋ",
          "original_form": "집중도 잘되고 동물들이 넘 귀여워서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021551.1.17",
          "form": "유튜브 제 최애 프로그램이에요",
          "original_form": "유튜브 제 최애 프로그램이에요",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021551.1.18",
          "form": "ㅋㅋㅋ 동물 나오는 영상은 사랑이쥬",
          "original_form": "ㅋㅋㅋ 동물 나오는 영상은 사랑이쥬",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021551.1.19",
          "form": "요즘 음악은 뭐 들으세요?",
          "original_form": "요즘 음악은 뭐 들으세요?",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021551.1.20",
          "form": "저 요즘 비가올때 디즈니 재즈 듣는데 좋아요",
          "original_form": "저 요즘 비가올때 디즈니 재즈 듣는데 좋아요",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100021551.1.21",
          "form": "디즈니가 재즈도 만들어요??",
          "original_form": "디즈니가 재즈도 만들어요??",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100021551.1.22",
          "form": "재즈버전으로 나온게 있어요",
          "original_form": "재즈버전으로 나온게 있어요",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100021551.1.23",
          "form": "헐 처음 알았는데 감사해요",
          "original_form": "헐 처음 알았는데 감사해요",
          "speaker_id": "1",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100021551.1.24",
          "form": "비오늘날이랑 잘 어울리겠어요",
          "original_form": "비오늘날이랑 잘 어울리겠어요",
