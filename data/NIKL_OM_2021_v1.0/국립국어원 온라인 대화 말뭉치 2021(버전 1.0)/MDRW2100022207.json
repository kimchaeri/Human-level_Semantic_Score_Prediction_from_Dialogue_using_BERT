          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022207.1.2",
          "form": "비밀",
          "original_form": "비밀",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022207.1.3",
          "form": "시크릿",
          "original_form": "시크릿",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022207.1.4",
          "form": "주주",
          "original_form": "주주",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022207.1.5",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022207.1.6",
          "form": "맞춰보세요",
          "original_form": "맞춰보세요",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022207.1.7",
          "form": "여친없을듯",
          "original_form": "여친없을듯",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022207.1.8",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022207.1.9",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022207.1.10",
          "form": "잇져",
          "original_form": "잇져",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022207.1.11",
          "form": "이상형이먼가여",
          "original_form": "이상형이먼가여",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022207.1.12",
          "form": "잠 잘자는 사람요",
          "original_form": "잠 잘자는 사람요",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022207.1.13",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022207.1.14",
          "form": "소개팅 많이해봤어요?",
          "original_form": "소개팅 많이해봤어요?",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022207.1.15",
          "form": "저는 못생긴 귀요미욤",
          "original_form": "저는 못생긴 귀요미욤",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100022207.1.16",
          "form": "비오는거같네요",
          "original_form": "비오는거같네요",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100022207.1.17",
          "form": "남자친구가 우산챙겨라했나요",
          "original_form": "남자친구가 우산챙겨라했나요",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100022207.1.18",
          "form": "안챙겨주떠요",
          "original_form": "안챙겨주떠요",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022207.1.19",
          "form": "흥",
          "original_form": "흥",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022207.1.20",
          "form": "누가데리러오나요?",
          "original_form": "누가데리러오나요?",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022207.1.21",
          "form": "조카와욤",
          "original_form": "조카와욤",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022207.1.22",
          "form": "애낳고싶은가요",
          "original_form": "애낳고싶은가요",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022207.1.23",
          "form": "네 여자친구 닮은 딸",
          "original_form": "네 여자친구 닮은 딸",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022207.1.24",
          "form": "근데 딸은 불안해서",
          "original_form": "근데 딸은 불안해서",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022207.1.25",
          "form": "하늘이 정해주겠죠",
          "original_form": "하늘이 정해주겠죠",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022207.1.26",
          "form": "주시는대로 키워야죠",
          "original_form": "주시는대로 키워야죠",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022207.1.27",
          "form": "미자도키우고",
          "original_form": "미자도키우고",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022207.1.28",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022207.1.29",
          "form": "ㅋㅋㅋㅋ저는 딩크하고싶어요",
          "original_form": "ㅋㅋㅋㅋ저는 딩크하고싶어요",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022207.1.30",
          "form": "개한마리키우고살면",
          "original_form": "개한마리키우고살면",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022207.1.31",
          "form": "딱좋을거같아요",
          "original_form": "딱좋을거같아요",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022207.1.32",
          "form": "그것도좋죠",
          "original_form": "그것도좋죠",
          "speaker_id": "2",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022207.1.33",
          "form": "바빠요?",
          "original_form": "바빠요?",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022207.1.34",
          "form": "안바빠요",
          "original_form": "안바빠요",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.35",
          "form": "틈틈히",
          "original_form": "틈틈히",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.36",
          "form": "사진찍고왓어욤",
          "original_form": "사진찍고왓어욤",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.37",
          "form": "일하고잇는데",
          "original_form": "일하고잇는데",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.38",
          "form": "남친이",
          "original_form": "남친이",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.39",
          "form": "너무",
          "original_form": "너무",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.40",
          "form": "보고싶어요",
          "original_form": "보고싶어요",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.41",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.42",
          "form": "큰일이네요",
          "original_form": "큰일이네요",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.43",
          "form": "조카보러간다했는데",
          "original_form": "조카보러간다했는데",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022207.1.44",
          "form": "진차큰일이에요",
          "original_form": "진차큰일이에요",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022207.1.45",
          "form": "너무 보고싶어요",
          "original_form": "너무 보고싶어요",
          "speaker_id": "1",
          "time": "20210829 11:27"
        }
      ]
    }
