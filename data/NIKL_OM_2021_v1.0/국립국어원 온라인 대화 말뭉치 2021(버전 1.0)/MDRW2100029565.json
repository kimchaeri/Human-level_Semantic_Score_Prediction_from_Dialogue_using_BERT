      "utterance": [
        {
          "id": "MDRW2100029565.1.1",
          "form": "점심은 뭐드십니까",
          "original_form": "점심은 뭐드십니까",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029565.1.2",
          "form": "name1님",
          "original_form": "&name1&님",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029565.1.3",
          "form": "몸이 멀쩡한데 정녕 회에 소주는 안될까용",
          "original_form": "몸이 멀쩡한데 정녕 회에 소주는 안될까용",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029565.1.4",
          "form": "전 먹었었음",
          "original_form": "전 먹었었음",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100029565.1.5",
          "form": "이틀만 몸사릴까 했는데 점점 그럴 필요가 있나 싶음ㅎ",
          "original_form": "이틀만 몸사릴까 했는데 점점 그럴 필요가 있나 싶음ㅎ",
          "speaker_id": "1",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100029565.1.6",
          "form": "그래서 와이프가 구박했는데",
          "original_form": "그래서 와이프가 구박했는데",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100029565.1.7",
          "form": "아니 아팠지 않나요..?",
          "original_form": "아니 아팠지 않나요..?",
          "speaker_id": "1",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100029565.1.8",
          "form": "아빠한테 이르는데",
          "original_form": "아빠한테 이르는데",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100029565.1.9",
          "form": "아빠는 당일에 먹어서",
          "original_form": "아빠는 당일에 먹어서",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100029565.1.10",
          "form": "침 묵",
          "original_form": "침 묵",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100029565.1.11",
          "form": "앓고 일어나자 마신 겁니꽈ㅎㅎ",
          "original_form": "앓고 일어나자 마신 겁니꽈ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100029565.1.12",
          "form": "아님 마시고 아픈 겁니꽈",
          "original_form": "아님 마시고 아픈 겁니꽈",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100029565.1.13",
          "form": "안 아팠는데요 저는!",
          "original_form": "안 아팠는데요 저는!",
          "speaker_id": "2",
          "time": "20210903 10:28"
        },
        {
          "id": "MDRW2100029565.1.14",
          "form": "그럼 맞고 언제부터 마셨어요?!!",
          "original_form": "그럼 맞고 언제부터 마셨어요?!!",
          "speaker_id": "1",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029565.1.15",
          "form": "다음날이오",
          "original_form": "다음날이오",
          "speaker_id": "2",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029565.1.16",
          "form": "담날부터라고 해주세요ㅜ",
          "original_form": "담날부터라고 해주세요ㅜ",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029565.1.17",
          "form": "굿 굿!",
          "original_form": "굿 굿!",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029565.1.18",
          "form": "오늘 점심은 회에 소주입니다!",
          "original_form": "오늘 점심은 회에 소주입니다!",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029565.1.19",
          "form": "점심이요?",
          "original_form": "점심이요?",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029565.1.20",
          "form": "넹넹ㅎㅎ",
          "original_form": "넹넹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029565.1.21",
          "form": "낮술쟁이시군요",
          "original_form": "낮술쟁이시군요",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029565.1.22",
          "form": "아프게 되면 name2님께 소송하쥬",
          "original_form": "아프게 되면 &name2&님께 소송하쥬",
