      "utterance": [
        {
          "id": "MDRW2100037107.1.1",
          "form": "저두여 !!! 만동제과꺼 드셔보셨나요 !",
          "original_form": "저두여 !!! 만동제과꺼 드셔보셨나요 !",
          "speaker_id": "1",
          "time": "20210831 19:44"
        },
        {
          "id": "MDRW2100037107.1.2",
          "form": "앗.. 아뇨!",
          "original_form": "앗.. 아뇨!",
          "speaker_id": "2",
          "time": "20210831 19:45"
        },
        {
          "id": "MDRW2100037107.1.3",
          "form": "동네 제과점 출신임닷ㅜ",
          "original_form": "동네 제과점 출신임닷ㅜ",
          "speaker_id": "2",
          "time": "20210831 19:45"
        },
        {
          "id": "MDRW2100037107.1.4",
          "form": "앗 사실 저두 동네에서 사먹지만 ㅋㅋㅋ",
          "original_form": "앗 사실 저두 동네에서 사먹지만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:45"
        },
        {
          "id": "MDRW2100037107.1.5",
          "form": "서울 갔을때 먹었는데",
          "original_form": "서울 갔을때 먹었는데",
          "speaker_id": "1",
          "time": "20210831 19:46"
        },
        {
          "id": "MDRW2100037107.1.6",
          "form": "짱맛이였어요 ...",
          "original_form": "짱맛이였어요 ...",
          "speaker_id": "1",
          "time": "20210831 19:46"
        },
        {
          "id": "MDRW2100037107.1.7",
          "form": "아 서율!!",
          "original_form": "아 서율!!",
          "speaker_id": "2",
          "time": "20210831 19:46"
        },
        {
          "id": "MDRW2100037107.1.8",
          "form": "세상에 그렇게 맛난게 존재하고 있었다니요..",
          "original_form": "세상에 그렇게 맛난게 존재하고 있었다니요..",
          "speaker_id": "2",
          "time": "20210831 19:46"
        },
        {
          "id": "MDRW2100037107.1.9",
          "form": "마자요 ... 지방인은 서럽습니다 ...",
          "original_form": "마자요 ... 지방인은 서럽습니다 ...",
          "speaker_id": "1",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100037107.1.10",
          "form": "맞아요ㅜㅠ",
          "original_form": "맞아요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037107.1.11",
          "form": "하지만 전 동네껄로도 만족해요 ..# 흑흑...",
          "original_form": "하지만 전 동네껄로도 만족해요 ..# 흑흑...",
          "speaker_id": "1",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037107.1.12",
          "form": "맛있는거면 좀 체인점 좀..",
          "original_form": "맛있는거면 좀 체인점 좀..",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037107.1.13",
          "form": "맞아여 ㅜㅜㅜ",
          "original_form": "맞아여 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037107.1.14",
          "form": "저두 그렇긴해용ㅋㅋㅋㅋ",
          "original_form": "저두 그렇긴해용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037107.1.15",
          "form": "빵집이 특히 체인이 없는거같아요 ㅋㅋ",
          "original_form": "빵집이 특히 체인이 없는거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037107.1.16",
          "form": "파바뚜쥬 때문인가...",
          "original_form": "파바뚜쥬 때문인가...",
          "speaker_id": "1",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037107.1.17",
          "form": "그른가봐여..",
          "original_form": "그른가봐여..",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037107.1.18",
          "form": "젤 가까운 곳 하나 사라졌어요ㅠ",
          "original_form": "젤 가까운 곳 하나 사라졌어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037107.1.19",
          "form": "헐 진짜여 ???",
          "original_form": "헐 진짜여 ???",
          "speaker_id": "1",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037107.1.20",
          "form": "코로나때문인가요 설마ㅜㅜ",
          "original_form": "코로나때문인가요 설마ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037107.1.21",
          "form": "그런거같아요..ㅜㅜ",
          "original_form": "그런거같아요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 19:53"
        },
