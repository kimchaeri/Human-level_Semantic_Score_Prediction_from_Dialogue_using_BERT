      "utterance": [
        {
          "id": "MDRW2100037115.1.1",
          "form": "공연 뷰는 거",
          "original_form": "공연 뷰는 거",
          "speaker_id": "1",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037115.1.2",
          "form": "좋아하시나요",
          "original_form": "좋아하시나요",
          "speaker_id": "1",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037115.1.3",
          "form": "음 다양하게 본적은 없어요! 많이능 못봤지만 뮤지컬은 좋아해요!",
          "original_form": "음 다양하게 본적은 없어요! 많이능 못봤지만 뮤지컬은 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100037115.1.4",
          "form": "뮤지컬 뭐 보셨어요?",
          "original_form": "뮤지컬 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100037115.1.5",
          "form": "젤 기억에 남는건 레미제라블 재밌게봤어요!",
          "original_form": "젤 기억에 남는건 레미제라블 재밌게봤어요!",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100037115.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210831 19:59"
        },
        {
          "id": "MDRW2100037115.1.7",
          "form": "전 뮤지컬에 요즘 관심 생겼거든요",
          "original_form": "전 뮤지컬에 요즘 관심 생겼거든요",
          "speaker_id": "1",
          "time": "20210831 19:59"
        },
        {
          "id": "MDRW2100037115.1.8",
          "form": "오 그렇군요 어쩌다가 관심이 생기셧어요?",
          "original_form": "오 그렇군요 어쩌다가 관심이 생기셧어요?",
          "speaker_id": "2",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100037115.1.9",
          "form": "그냥.. 재밌어보여서요",
          "original_form": "그냥.. 재밌어보여서요",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100037115.1.10",
          "form": "노래로 감정을 표현하고",
          "original_form": "노래로 감정을 표현하고",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100037115.1.11",
          "form": "그쵸 보고듣는재미가있는거같아요",
          "original_form": "그쵸 보고듣는재미가있는거같아요",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100037115.1.12",
          "form": "가수콘서트는 가보셨어요?",
          "original_form": "가수콘서트는 가보셨어요?",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100037115.1.13",
          "form": "네 가봤어요",
          "original_form": "네 가봤어요",
          "speaker_id": "1",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100037115.1.14",
          "form": "가보셨나여?",
          "original_form": "가보셨나여?",
          "speaker_id": "1",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100037115.1.15",
          "form": "아뇨 아직 한번도 앙가봤ㅇ어요..!",
          "original_form": "아뇨 아직 한번도 앙가봤ㅇ어요..!",
          "speaker_id": "2",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100037115.1.16",
          "form": "누구콘서트 다녀오셧어요?",
          "original_form": "누구콘서트 다녀오셧어요?",
          "speaker_id": "2",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100037115.1.17",
          "form": "저 아이돌이여ㅋㅋ",
          "original_form": "저 아이돌이여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100037115.1.18",
          "form": "가보고 싶은 사람",
          "original_form": "가보고 싶은 사람",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100037115.1.19",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100037115.1.20",
          "form": "오 네 저 많은것같아요 ㅋㅋㅋㅋ",
          "original_form": "오 네 저 많은것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:17"
        },
        {
          "id": "MDRW2100037115.1.21",
          "form": "아이돌 누구 콘서트인지 물어봐두되나요!?",
          "original_form": "아이돌 누구 콘서트인지 물어봐두되나요!?",
          "speaker_id": "2",
          "time": "20210831 20:17"
        },
        {
          "id": "MDRW2100037115.1.22",
          "form": "저 엔시티랑 엑소요ㅋㅋ",
          "original_form": "저 엔시티랑 엑소요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037115.1.23",
          "form": "누구 가고싶으세요?! 저도",
          "original_form": "누구 가고싶으세요?! 저도",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037115.1.24",
          "form": "가고싶은 사람 있거든요",
          "original_form": "가고싶은 사람 있거든요",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037115.1.25",
          "form": "오 글쿤요 아이돌은 저는 샤이니랑 위너 좋아해서 가고싶더라구요",
          "original_form": "오 글쿤요 아이돌은 저는 샤이니랑 위너 좋아해서 가고싶더라구요",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037115.1.26",
          "form": "아이유랑 성시경콘서트도 가고싶어요",
          "original_form": "아이유랑 성시경콘서트도 가고싶어요",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037115.1.27",
          "form": "오 저도 아이유 콘서트여 ㅠㅠ",
          "original_form": "오 저도 아이유 콘서트여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037115.1.28",
          "form": "노래 들으면 정말",
          "original_form": "노래 들으면 정말",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037115.1.29",
          "form": "행복항 거 같아요...",
          "original_form": "행복항 거 같아요...",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037115.1.30",
          "form": "맞아요 노래를 어떻게 그렇게 부르죠 ㅠㅠㅠ",
          "original_form": "맞아요 노래를 어떻게 그렇게 부르죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:28"
        }
      ]
    }
