      "utterance": [
        {
          "id": "MDRW2100039226.1.1",
          "form": "저랑 꼭 닮았군욪ㅎㅎ",
          "original_form": "저랑 꼭 닮았군욪ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100039226.1.2",
          "form": "다음엔 아무개말고 김네티로 지어야겠어요",
          "original_form": "다음엔 아무개말고 김네티로 지어야겠어요",
          "speaker_id": "2",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100039226.1.3",
          "form": "네티는 요리 잘할거예요",
          "original_form": "네티는 요리 잘할거예요",
          "speaker_id": "1",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100039226.1.4",
          "form": "님은 요리못하니까 탈락이에여",
          "original_form": "님은 요리못하니까 탈락이에여",
          "speaker_id": "1",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100039226.1.5",
          "form": "??? 소피스트신가요?",
          "original_form": "??? 소피스트신가요?",
          "speaker_id": "2",
          "time": "20210903 14:26"
        },
        {
          "id": "MDRW2100039226.1.6",
          "form": "멍멍이소리 잘하시네욧",
          "original_form": "멍멍이소리 잘하시네욧",
          "speaker_id": "2",
          "time": "20210903 14:26"
        },
        {
          "id": "MDRW2100039226.1.7",
          "form": "소피스트는 투움바 파스타를 잘 만들었군요ㅎㅎ",
          "original_form": "소피스트는 투움바 파스타를 잘 만들었군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:27"
        },
        {
          "id": "MDRW2100039226.1.8",
          "form": "근데 타르트로 끼니가 되나요",
          "original_form": "근데 타르트로 끼니가 되나요",
          "speaker_id": "2",
          "time": "20210903 14:27"
        },
        {
          "id": "MDRW2100039226.1.9",
          "form": "여러개 먹으면 가능해요 홀홀홀",
          "original_form": "여러개 먹으면 가능해요 홀홀홀",
          "speaker_id": "1",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039226.1.10",
          "form": "회 언제 도착 예정인가여?",
          "original_form": "회 언제 도착 예정인가여?",
          "speaker_id": "1",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039226.1.11",
          "form": "ㅎㅎ1시 40분에 재조리재배달 가게에 전달한댔는데 왜 안 오죠",
          "original_form": "ㅎㅎ1시 40분에 재조리재배달 가게에 전달한댔는데 왜 안 오죠",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039226.1.12",
          "form": "배민1주문 20분만에 오배송되더니",
          "original_form": "배민1주문 20분만에 오배송되더니",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039226.1.13",
          "form": "재주문 들어간건 세월아네월아",
          "original_form": "재주문 들어간건 세월아네월아",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039226.1.14",
          "form": "전화를 해보시는 게...!!!",
          "original_form": "전화를 해보시는 게...!!!",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039226.1.15",
          "form": "저혈압이 치료되네요ㅎㅎ",
          "original_form": "저혈압이 치료되네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039226.1.16",
          "form": "넘 늦는데여...?",
          "original_form": "넘 늦는데여...?",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039226.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039226.1.18",
          "form": "매운탕도 추가하셨나여",
          "original_form": "매운탕도 추가하셨나여",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039226.1.19",
          "form": "말도 없이 집앞에 두고 갔네요",
          "original_form": "말도 없이 집앞에 두고 갔네요",
          "speaker_id": "2",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100039226.1.20",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100039226.1.21",
          "form": "아뇨 간단히 먹으려고요ㅎ",
          "original_form": "아뇨 간단히 먹으려고요ㅎ",
          "speaker_id": "2",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100039226.1.22",
          "form": "크흡.. 맛잇게 드시어요",
          "original_form": "크흡.. 맛잇게 드시어요",
          "speaker_id": "1",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100039226.1.23",
          "form": "넘 부럽네용...",
          "original_form": "넘 부럽네용...",
          "speaker_id": "1",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100039226.1.24",
          "form": "ㅎㅎ  그래도 회님...",
          "original_form": "ㅎㅎ  그래도 회님...",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039226.1.25",
          "form": "행복합니다",
          "original_form": "행복합니다",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039226.1.26",
          "form": "님도 저녁엔 회 드세요!!ㅎㅎ",
          "original_form": "님도 저녁엔 회 드세요!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039226.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 되게 행복해보이세여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 되게 행복해보이세여",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039226.1.28",
          "form": "맛있게 드시구.. 저는.... 이따 밤에 다시 오겠슴니다..",
          "original_form": "맛있게 드시구.. 저는.... 이따 밤에 다시 오겠슴니다..",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039226.1.29",
          "form": "ㅎㅎㅎ 넹",
          "original_form": "ㅎㅎㅎ 넹",
          "speaker_id": "2",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039226.1.30",
          "form": "세상서 가장 행복해진 단세포가 인사드림ㅎ",
          "original_form": "세상서 가장 행복해진 단세포가 인사드림ㅎ",
