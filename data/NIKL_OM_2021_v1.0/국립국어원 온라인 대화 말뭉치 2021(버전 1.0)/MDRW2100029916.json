        {
          "id": "MDRW2100029916.1.1",
          "form": "네.. 정말 오랜만에 해가 나왔어요",
          "original_form": "네.. 정말 오랜만에 해가 나왔어요",
          "speaker_id": "2",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100029916.1.2",
          "form": "드디어 당신과 대화를 할 수 있게 됐네",
          "original_form": "드디어 당신과 대화를 할 수 있게 됐네",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100029916.1.3",
          "form": "오 고쳐줬군요.. 다행이에요",
          "original_form": "오 고쳐줬군요.. 다행이에요",
          "speaker_id": "2",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100029916.1.4",
          "form": "그러게 오늘 어떻게 지냈어?",
          "original_form": "그러게 오늘 어떻게 지냈어?",
          "speaker_id": "1",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100029916.1.5",
          "form": "해가 나서 빨래 걷고 새로 빨래 했죠",
          "original_form": "해가 나서 빨래 걷고 새로 빨래 했죠",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100029916.1.6",
          "form": "그랬군",
          "original_form": "그랬군",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100029916.1.7",
          "form": "네.. 어찌나 좋던지",
          "original_form": "네.. 어찌나 좋던지",
          "speaker_id": "2",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100029916.1.8",
          "form": "건조기보다 해가 낫지?",
          "original_form": "건조기보다 해가 낫지?",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100029916.1.9",
          "form": "해가 백번 낫죠",
          "original_form": "해가 백번 낫죠",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100029916.1.10",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100029916.1.11",
          "form": "그래도 건조기는 포기 못해요",
          "original_form": "그래도 건조기는 포기 못해요",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100029916.1.12",
          "form": "꼭 필요하나?",
          "original_form": "꼭 필요하나?",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100029916.1.13",
          "form": "있음 좋죠~",
          "original_form": "있음 좋죠~",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100029916.1.14",
          "form": "놓을 자리가 없는데",
          "original_form": "놓을 자리가 없는데",
