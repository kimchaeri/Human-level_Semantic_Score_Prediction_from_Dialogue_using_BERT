      "utterance": [
        {
          "id": "MDRW2100027619.1.1",
          "form": "오늘 날씨 정말 상큼하고 좋던데요",
          "original_form": "오늘 날씨 정말 상큼하고 좋던데요",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027619.1.2",
          "form": "날씨가 어떤가요?",
          "original_form": "날씨가 어떤가요?",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027619.1.3",
          "form": "딱 가을이라는 느낌이에요",
          "original_form": "딱 가을이라는 느낌이에요",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027619.1.4",
          "form": "날씨 너무 좋아서 내일 낚시 가보려고요",
          "original_form": "날씨 너무 좋아서 내일 낚시 가보려고요",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027619.1.5",
          "form": "와 낚시 좋아하세요?",
          "original_form": "와 낚시 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027619.1.6",
          "form": "네 얼마전부터 하기 시작했는데 재미있더라고요",
          "original_form": "네 얼마전부터 하기 시작했는데 재미있더라고요",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027619.1.7",
          "form": "먹는거 좋아하는데 직접 낚은게 싱싱하고 맛있기도 하고요",
          "original_form": "먹는거 좋아하는데 직접 낚은게 싱싱하고 맛있기도 하고요",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027619.1.8",
          "form": "앗 그래요?",
          "original_form": "앗 그래요?",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027619.1.9",
          "form": "날씨가 딱이에요",
          "original_form": "날씨가 딱이에요",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027619.1.10",
          "form": "네~ 9월부터 쭈꾸미 갑오징어철인데",
          "original_form": "네~ 9월부터 쭈꾸미 갑오징어철인데",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027619.1.11",
          "form": "초보자도 하기 좋아서 가족단위로도 체험배 많이들 타시더라고요",
          "original_form": "초보자도 하기 좋아서 가족단위로도 체험배 많이들 타시더라고요",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027619.1.12",
          "form": "아 그래모?",
          "original_form": "아 그래모?",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027619.1.13",
          "form": "서해 멀지 않으시면 추천드립니다",
          "original_form": "서해 멀지 않으시면 추천드립니다",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027619.1.14",
          "form": "신청하면 되나요?",
          "original_form": "신청하면 되나요?",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027619.1.15",
          "form": "어린이들도 곧잘 낚더라고요",
          "original_form": "어린이들도 곧잘 낚더라고요",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027619.1.16",
          "form": "네네 인터넷에서 가실 지역 낚시배 검색해보시면 홈페이지에서 예약하실 수 있어요!",
          "original_form": "네네 인터넷에서 가실 지역 낚시배 검색해보시면 홈페이지에서 예약하실 수 있어요!",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027619.1.17",
          "form": "아 고급정보예요",
          "original_form": "아 고급정보예요",
          "speaker_id": "1",
          "time": "20210902 14:18"
        }
      ]
