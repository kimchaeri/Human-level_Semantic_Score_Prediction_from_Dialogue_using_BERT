      "utterance": [
        {
          "id": "MDRW2100042166.1.1",
          "form": "저는 매운제육볶음 매운짬뽕 같이",
          "original_form": "저는 매운제육볶음 매운짬뽕 같이",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042166.1.2",
          "form": "캡사이신 매운거보다 고.추로 매운맛 낸거 좋아해요ㅋㅋㅋ",
          "original_form": "캡사이신 매운거보다 고.추로 매운맛 낸거 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042166.1.3",
          "form": "아아!! 캡사이신은 매운게 아니라 아파요 ㅠㅠㅠ",
          "original_form": "아아!! 캡사이신은 매운게 아니라 아파요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042166.1.4",
          "form": "맞아요 그건 거의 고문음식..ㅠㅠㅠ",
          "original_form": "맞아요 그건 거의 고문음식..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042166.1.5",
          "form": "하니님은 싫어하는 음식 있으세요??",
          "original_form": "하니님은 싫어하는 음식 있으세요??",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042166.1.6",
          "form": "저는 매운걸 못먹어요 ㅠㅠ",
          "original_form": "저는 매운걸 못먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042166.1.7",
          "form": "싫어하진 않는데 속이 안좋아서",
          "original_form": "싫어하진 않는데 속이 안좋아서",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042166.1.8",
          "form": "느끼한거 좋아해요",
          "original_form": "느끼한거 좋아해요",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042166.1.9",
          "form": "아하 맞아요 속이 난리날때가 있죠ㅠㅠ 저도 자제하면서 먹어요ㅜㅜ",
          "original_form": "아하 맞아요 속이 난리날때가 있죠ㅠㅠ 저도 자제하면서 먹어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042166.1.10",
          "form": "오옷 느끼한거라면 어떤거 좋아하세요??",
          "original_form": "오옷 느끼한거라면 어떤거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042166.1.11",
          "form": "저는 꾸덕한 파스타나 피자요..!!!",
          "original_form": "저는 꾸덕한 파스타나 피자요..!!!",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042166.1.12",
          "form": "오오오..!!!! 저두 좋아해요!!",
          "original_form": "오오오..!!!! 저두 좋아해요!!",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042166.1.13",
          "form": "느끼한 피자 뭐가 있나요!?!??",
          "original_form": "느끼한 피자 뭐가 있나요!?!??",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042166.1.14",
          "form": "치즈 잔뜩 들어간거요!! 치즈크러스트랑 ㅠㅠ",
          "original_form": "치즈 잔뜩 들어간거요!! 치즈크러스트랑 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042166.1.15",
          "form": "파인 애플 들어간거 좋아하세요?",
          "original_form": "파인 애플 들어간거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042166.1.16",
          "form": "오오옹ㅜㅜㅜ 치즈크러스트는 진리죠ㅠㅠ 시카고 피자 먹고싶네요,,,",
          "original_form": "오오옹ㅜㅜㅜ 치즈크러스트는 진리죠ㅠㅠ 시카고 피자 먹고싶네요,,,",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042166.1.17",
          "form": "네네 전 파인애플을 워낙 좋아해서요!!",
          "original_form": "네네 전 파인애플을 워낙 좋아해서요!!",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042166.1.18",
          "form": "우아 시카고 피자!!!!!!",
          "original_form": "우아 시카고 피자!!!!!!",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042166.1.19",
          "form": "따뜻한 파인애플 시러하는 분들 있드라구요 ㅋㅋ",
          "original_form": "따뜻한 파인애플 시러하는 분들 있드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042166.1.20",
          "form": "맞아요..ㅠㅠ 파인애플 is 뭔들 아닙니까ㅜㅜㅜ",
          "original_form": "맞아요..ㅠㅠ 파인애플 is 뭔들 아닙니까ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042166.1.21",
          "form": "저는 따뜻한 파인애플 민초 다 좋아해요ㅋㅋㅋ",
          "original_form": "저는 따뜻한 파인애플 민초 다 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042166.1.22",
          "form": "우아 ㅜㅜ 저도 민초는 아니지만 ㅎㅎㅎ",
          "original_form": "우아 ㅜㅜ 저도 민초는 아니지만 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042166.1.23",
          "form": "헉 반민초단이시군요ㅜㅜ 우리민초..",
          "original_form": "헉 반민초단이시군요ㅜㅜ 우리민초..",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042166.1.24",
          "form": "베라에서 좋아하는 맛 있으신가요??",
          "original_form": "베라에서 좋아하는 맛 있으신가요??",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042166.1.25",
          "form": "저는 녹차 초코 치즈 맛 좋아해요!!",
