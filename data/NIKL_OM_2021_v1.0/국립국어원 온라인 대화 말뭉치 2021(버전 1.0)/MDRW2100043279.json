      "utterance": [
        {
          "id": "MDRW2100043279.1.1",
          "form": "가고싶은 여행 지가 있나요",
          "original_form": "가고싶은 여행 지가 있나요",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100043279.1.2",
          "form": "음 저는 제주도요 name1님은요?",
          "original_form": "음 저는 제주도요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100043279.1.3",
          "form": "제주도 좋으네요",
          "original_form": "제주도 좋으네요",
          "speaker_id": "1",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100043279.1.4",
          "form": "전 남해도 가보고 싶어요",
          "original_form": "전 남해도 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100043279.1.5",
          "form": "경상남도 남해 말씀하시는건가요?!",
          "original_form": "경상남도 남해 말씀하시는건가요?!",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100043279.1.6",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100043279.1.7",
          "form": "오오 남해에 가볼 만한 곳 있나요?",
          "original_form": "오오 남해에 가볼 만한 곳 있나요?",
          "speaker_id": "2",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043279.1.8",
          "form": "남해 금산 도 좋아요",
          "original_form": "남해 금산 도 좋아요",
          "speaker_id": "1",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100043279.1.9",
          "form": "오오오 산 좋죠 저도 나중에 가보고 싶네요",
          "original_form": "오오오 산 좋죠 저도 나중에 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100043279.1.10",
          "form": "여행 많이 다녀보셨나요?",
          "original_form": "여행 많이 다녀보셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100043279.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043279.1.12",
          "form": "자주 다니지 못하네요",
          "original_form": "자주 다니지 못하네요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043279.1.13",
          "form": "아 그렇군요 그럼 제일 기억나는 여행지가 있나요?",
          "original_form": "아 그렇군요 그럼 제일 기억나는 여행지가 있나요?",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100043279.1.14",
          "form": "제주도도 기억에 남죠",
          "original_form": "제주도도 기억에 남죠",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043279.1.15",
          "form": "친구들과에 추억이 생긴곳 이니깐요",
          "original_form": "친구들과에 추억이 생긴곳 이니깐요",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100043279.1.16",
          "form": "오 그렇군요! 즐거우셨겠어요",
          "original_form": "오 그렇군요! 즐거우셨겠어요",
          "speaker_id": "2",
          "time": "20210908 00:16"
