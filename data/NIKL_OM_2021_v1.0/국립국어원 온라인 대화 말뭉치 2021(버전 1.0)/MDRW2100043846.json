      "utterance": [
        {
          "id": "MDRW2100043846.1.1",
          "form": "근데 엄청 직육면체처럼 되었었어요",
          "original_form": "근데 엄청 직육면체처럼 되었었어요",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043846.1.2",
          "form": "계란말이 말기 쉽지 않은데",
          "original_form": "계란말이 말기 쉽지 않은데",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043846.1.3",
          "form": "핳",
          "original_form": "핳",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043846.1.4",
          "form": "직육면체면 대단하시네요",
          "original_form": "직육면체면 대단하시네요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043846.1.5",
          "form": "치즈까지넣어서",
          "original_form": "치즈까지넣어서",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043846.1.6",
          "form": "이쁘게해서먹었죠",
          "original_form": "이쁘게해서먹었죠",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043846.1.7",
          "form": "요리를 잘하시나봐요",
          "original_form": "요리를 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.8",
          "form": "아뇨...",
          "original_form": "아뇨...",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.9",
          "form": "라먄이랑",
          "original_form": "라먄이랑",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.10",
          "form": "라면",
          "original_form": "라면",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.11",
          "form": "잘합니다",
          "original_form": "잘합니다",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.12",
          "form": "딱 계랑까지하고",
          "original_form": "딱 계랑까지하고",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.13",
          "form": "타이머랑해서",
          "original_form": "타이머랑해서",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.14",
          "form": "정석이최고죠",
          "original_form": "정석이최고죠",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.15",
          "form": "name1님은",
          "original_form": "&name1&님은",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.16",
          "form": "잘하시나요",
          "original_form": "잘하시나요",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043846.1.17",
          "form": "저도 딱히 못해요ㅋㅋㅋ",
          "original_form": "저도 딱히 못해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043846.1.18",
          "form": "카페 가는거 좋아하시나요?",
          "original_form": "카페 가는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043846.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043846.1.20",
          "form": "돈만있으면",
          "original_form": "돈만있으면",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043846.1.21",
          "form": "맨날카페갑니다",
          "original_form": "맨날카페갑니다",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043846.1.22",
          "form": "제가 또 카페 알바경험도있어서",
          "original_form": "제가 또 카페 알바경험도있어서",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043846.1.23",
          "form": "커피좋아하게되었네요",
          "original_form": "커피좋아하게되었네요",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043846.1.24",
          "form": "커피 그럼 잘 만드시겠넹ㅅ",
          "original_form": "커피 그럼 잘 만드시겠넹ㅅ",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043846.1.25",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043846.1.26",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043846.1.27",
          "form": "진짜잘만들어요",
          "original_form": "진짜잘만들어요",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043846.1.28",
          "form": "사장님도그렇고",
          "original_form": "사장님도그렇고",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043846.1.29",
