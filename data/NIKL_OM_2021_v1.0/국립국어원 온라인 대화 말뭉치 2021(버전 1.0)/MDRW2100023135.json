      "utterance": [
        {
          "id": "MDRW2100023135.1.1",
          "form": "name3과 name4 자랑좀 해주세요",
          "original_form": "&name3&과 &name4& 자랑좀 해주세요",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023135.1.2",
          "form": "애기때부터 키우셨나요?",
          "original_form": "애기때부터 키우셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023135.1.3",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023135.1.4",
          "form": "name3은 애기때부터,name4은 4개월 때부터 키웠어요ㅎ",
          "original_form": "&name3&은 애기때부터,&name4&은 4개월 때부터 키웠어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023135.1.5",
          "form": "엄청 귀여웠죠ㅎ name3사진은 지금도 봐요ㅎ 너무좋아요ㅎㅎ",
          "original_form": "엄청 귀여웠죠ㅎ &name3&사진은 지금도 봐요ㅎ 너무좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023135.1.6",
          "form": "name3은 곰같애요ㅎㅎㅎ 미니곰ㅎㅎ",
          "original_form": "&name3&은 곰같애요ㅎㅎㅎ 미니곰ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023135.1.7",
          "form": "근데 name4은 여자애라 새침때기ㅎㅎ 그리고 4개월에 데리고온거라 절 주인으로 받아들이기까지 좀 시간이 걸렸어요ㅎ",
          "original_form": "근데 &name4&은 여자애라 새침때기ㅎㅎ 그리고 4개월에 데리고온거라 절 주인으로 받아들이기까지 좀 시간이 걸렸어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023135.1.8",
          "form": "4개월이면 아직 애기인데도 전주인 다 기억하는군요",
          "original_form": "4개월이면 아직 애기인데도 전주인 다 기억하는군요",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023135.1.9",
          "form": "맞아요ㅎ 사람하고 똑같애요ㅎㅎ",
          "original_form": "맞아요ㅎ 사람하고 똑같애요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023135.1.10",
          "form": "오고나서 계속 적응못했어요ㅎ",
          "original_form": "오고나서 계속 적응못했어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023135.1.11",
          "form": "근데 나중에는 없으면 낑낑대고 난리였어요ㅎ",
          "original_form": "근데 나중에는 없으면 낑낑대고 난리였어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023135.1.12",
          "form": "주인이 눈앞에있어도 낑낑대는 애들있더라고요 ㅋㅋ",
          "original_form": "주인이 눈앞에있어도 낑낑대는 애들있더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023135.1.13",
          "form": "시장 지나가는데 가게분 지금한참바쁜데",
          "original_form": "시장 지나가는데 가게분 지금한참바쁜데",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023135.1.14",
          "form": "강아지가 낑낑대니까",
          "original_form": "강아지가 낑낑대니까",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023135.1.15",
          "form": "귀엽긴 진짜귀여운데",
          "original_form": "귀엽긴 진짜귀여운데",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023135.1.16",
          "form": "한번 용기내보세요ㅎ",
          "original_form": "한번 용기내보세요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023135.1.17",
          "form": "주인분이 name5 안돼 지금은 진짜바빠 그러면안돼 막 이렇게 말거시는것도 ㅋㅋ",
          "original_form": "주인분이 &name5& 안돼 지금은 진짜바빠 그러면안돼 막 이렇게 말거시는것도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023135.1.18",
          "form": "정말 행복하실꺼에요ㅎ",
          "original_form": "정말 행복하실꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023135.1.19",
          "form": "아참 결혼하셨으면 패쓰ㅎㅎ",
          "original_form": "아참 결혼하셨으면 패쓰ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023135.1.20",
          "form": "지금은 엄마가 너무싫어하셔서 무리고",
          "original_form": "지금은 엄마가 너무싫어하셔서 무리고",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023135.1.21",
          "form": "음 1인가구면 너무 외로울거같아요 강아지들이 ㅋ",
          "original_form": "음 1인가구면 너무 외로울거같아요 강아지들이 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023135.1.22",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023135.1.23",
          "form": "주인없으면,엄청 울어요ㅠ",
          "original_form": "주인없으면,엄청 울어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023135.1.24",
          "form": "음 이 모든건 일단 여러가지 조건이 필요하네요",
          "original_form": "음 이 모든건 일단 여러가지 조건이 필요하네요",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023135.1.25",
          "form": "글죠ㅎ 근데 한번키우보는것도 좋아요ㅎ",
          "original_form": "글죠ㅎ 근데 한번키우보는것도 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023135.1.26",
          "form": "저도 일하면서 두마리 다 집에 냅뒀어요ㅎ",
          "original_form": "저도 일하면서 두마리 다 집에 냅뒀어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023135.1.27",
          "form": "일단 개를 정말 좋아하는 남자를 찾아야겠네요",
          "original_form": "일단 개를 정말 좋아하는 남자를 찾아야겠네요",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023135.1.28",
          "form": "잘 훈련시키면 가능해요ㅎ",
          "original_form": "잘 훈련시키면 가능해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:17"
        }
      ]
