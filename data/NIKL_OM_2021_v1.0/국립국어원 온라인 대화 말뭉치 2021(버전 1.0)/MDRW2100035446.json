      "utterance": [
        {
          "id": "MDRW2100035446.1.1",
          "form": "저녁.. 밥이 좋을까요?",
          "original_form": "저녁.. 밥이 좋을까요?",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100035446.1.2",
          "form": "너무.. 좋은데요?",
          "original_form": "너무.. 좋은데요?",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035446.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 저는 파전 부쳐 먹고싶어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저는 파전 부쳐 먹고싶어요",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035446.1.4",
          "form": "오 그것도 좋네요!",
          "original_form": "오 그것도 좋네요!",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100035446.1.5",
          "form": "반죽가루를 못 찾아서 파전은 못 먹고 치킨텐더 튀겨 먹었어요 ㅋㅋㅋ",
          "original_form": "반죽가루를 못 찾아서 파전은 못 먹고 치킨텐더 튀겨 먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100035446.1.6",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100035446.1.7",
          "form": "케이크 먹었어요ㅋㅋㄱㄱ",
          "original_form": "케이크 먹었어요ㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100035446.1.8",
          "form": "치킨텐더 맛있겠는걸요?",
          "original_form": "치킨텐더 맛있겠는걸요?",
          "speaker_id": "1",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100035446.1.9",
          "form": "치킨텐더 먹고 케이크 먹었으면 완벽했을 것 같아요 ㅋㅋㅋ",
          "original_form": "치킨텐더 먹고 케이크 먹었으면 완벽했을 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100035446.1.10",
          "form": "디저트 못 먹어서 아쉽던 차에 케이크도 배달을 해주려나 싶네용",
          "original_form": "디저트 못 먹어서 아쉽던 차에 케이크도 배달을 해주려나 싶네용",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100035446.1.11",
          "form": "크으 케이크 배달 괜찮죠",
          "original_form": "크으 케이크 배달 괜찮죠",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035446.1.12",
          "form": "요즘엔 디저트도 배달되니 좋더라구요~",
          "original_form": "요즘엔 디저트도 배달되니 좋더라구요~",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035446.1.13",
          "form": "성원에 힘 입어 ㅋㅋㅋㅋ 우유 케이크를 시켰습니다 ㅋㅋㅋㅋ",
          "original_form": "성원에 힘 입어 ㅋㅋㅋㅋ 우유 케이크를 시켰습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035446.1.14",
          "form": "오오 좋죠좋죠!",
          "original_form": "오오 좋죠좋죠!",
          "speaker_id": "1",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100035446.1.15",
          "form": "음료는 어떤건가욥?!",
          "original_form": "음료는 어떤건가욥?!",
          "speaker_id": "1",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100035446.1.16",
          "form": "꿀막걸리인데용...",
          "original_form": "꿀막걸리인데용...",
          "speaker_id": "2",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100035446.1.17",
          "form": "크으 최고인걸요",
          "original_form": "크으 최고인걸요",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035446.1.18",
          "form": "마무리가 깔끔하시네용",
          "original_form": "마무리가 깔끔하시네용",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035446.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 제가 지금 커피를 마시면 잠을 못자서 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 제가 지금 커피를 마시면 잠을 못자서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035446.1.20",
          "form": "집에 우유는 없고.. 비슷한 색으로...",
          "original_form": "집에 우유는 없고.. 비슷한 색으로...",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035446.1.21",
          "form": "저희집에 우유만 있어서",
          "original_form": "저희집에 우유만 있어서",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035446.1.22",
          "form": "울어요",
          "original_form": "울어요",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035446.1.23",
          "form": "부럽습니닷",
          "original_form": "부럽습니닷",
          "speaker_id": "1",
