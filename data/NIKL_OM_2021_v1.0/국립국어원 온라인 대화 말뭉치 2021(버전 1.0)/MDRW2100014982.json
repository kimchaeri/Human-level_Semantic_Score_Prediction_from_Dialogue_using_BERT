      "utterance": [
        {
          "id": "MDRW2100014982.1.1",
          "form": "머리가 간지럽다",
          "original_form": "머리가 간지럽다",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014982.1.2",
          "form": "안 감았나",
          "original_form": "안 감았나",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014982.1.3",
          "form": "감았는뎅 ㅠㅠ요새 좀 간지러움",
          "original_form": "감았는뎅 ㅠㅠ요새 좀 간지러움",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014982.1.4",
          "form": "지루성 두피라 그렇다",
          "original_form": "지루성 두피라 그렇다",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014982.1.5",
          "form": "헐 그런가 ㅠㅠ 짜증나는군",
          "original_form": "헐 그런가 ㅠㅠ 짜증나는군",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014982.1.6",
          "form": "식초로 행궈봐라",
          "original_form": "식초로 행궈봐라",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014982.1.7",
          "form": "식초가 효과 좋은가?!",
          "original_form": "식초가 효과 좋은가?!",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014982.1.8",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014982.1.9",
          "form": "근데 냄새가 넘심해서 ㅠㅠ",
          "original_form": "근데 냄새가 넘심해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014982.1.10",
          "form": "참아야지",
          "original_form": "참아야지",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014982.1.11",
          "form": "글쿤 흡.. 그래 그거해서 나아지면 다행이니깡",
          "original_form": "글쿤 흡.. 그래 그거해서 나아지면 다행이니깡",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014982.1.12",
          "form": "그래 이따가 해봐라",
          "original_form": "그래 이따가 해봐라",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
