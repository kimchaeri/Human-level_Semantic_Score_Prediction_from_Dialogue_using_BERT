      "utterance": [
        {
          "id": "MDRW2100038349.1.1",
          "form": "기름기가 어느정도 있는건 좋아해요",
          "original_form": "기름기가 어느정도 있는건 좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038349.1.2",
          "form": "손에 좀 뭍기는하지만....소스가 또 있어가지고",
          "original_form": "손에 좀 뭍기는하지만....소스가 또 있어가지고",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038349.1.3",
          "form": "후라이드로먹어도되고",
          "original_form": "후라이드로먹어도되고",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038349.1.4",
          "form": "소스에찍어먹어되고",
          "original_form": "소스에찍어먹어되고",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038349.1.5",
          "form": "좋네요ㅎㅎ",
          "original_form": "좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038349.1.6",
          "form": "나중에 한번 먹어보겠습니다",
          "original_form": "나중에 한번 먹어보겠습니다",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038349.1.7",
          "form": "먹을만할꺼에요",
          "original_form": "먹을만할꺼에요",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038349.1.8",
          "form": "추천 감사해요",
          "original_form": "추천 감사해요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038349.1.9",
          "form": "나중에 기회되면 먹어보세요",
          "original_form": "나중에 기회되면 먹어보세요",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100038349.1.10",
          "form": "넵ㅎㅎ",
          "original_form": "넵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100038349.1.11",
          "form": "점심먹었나요?",
          "original_form": "점심먹었나요?",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038349.1.12",
          "form": "곧 먹으려구요",
          "original_form": "곧 먹으려구요",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038349.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038349.1.14",
          "form": "저는 점심먹었어요 일반 평범한 반찬메뉴나왔는데",
          "original_form": "저는 점심먹었어요 일반 평범한 반찬메뉴나왔는데",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100038349.1.15",
          "form": "후식으로 바나나  나와서",
          "original_form": "후식으로 바나나  나와서",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100038349.1.16",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100038349.1.17",
          "form": "바나나 좋네요",
          "original_form": "바나나 좋네요",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100038349.1.18",
          "form": "사실 바나나만 먹어도 든든한데",
          "original_form": "사실 바나나만 먹어도 든든한데",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100038349.1.19",
          "form": "그건 맞아요ㄱㅋㅋ",
          "original_form": "그건 맞아요ㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100038349.1.20",
          "form": "그래도 점심은 더 든든하게 먹는게 맞죠",
          "original_form": "그래도 점심은 더 든든하게 먹는게 맞죠",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100038349.1.21",
          "form": "점심을 든든하게먹으니깐 잠와요",
          "original_form": "점심을 든든하게먹으니깐 잠와요",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100038349.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100038349.1.23",
