      "utterance": [
        {
          "id": "MDRW2100022016.1.1",
          "form": "나름 일은 재미있게 일하고있어요ㅎㅎ",
          "original_form": "나름 일은 재미있게 일하고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022016.1.2",
          "form": "소소한 재미를 찾아서ㅎ",
          "original_form": "소소한 재미를 찾아서ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022016.1.3",
          "form": "그래서 누가 그러더라구요ㅎ 너무 자료가 많은거 아니냐?",
          "original_form": "그래서 누가 그러더라구요ㅎ 너무 자료가 많은거 아니냐?",
          "speaker_id": "2",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022016.1.4",
          "form": "ㅎㅎㅎ 그래서 뭐라 했어요",
          "original_form": "ㅎㅎㅎ 그래서 뭐라 했어요",
          "speaker_id": "1",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100022016.1.5",
          "form": "ㅎㅎ 자료를많이 만들어야 대충보신다고ㅎ",
          "original_form": "ㅎㅎ 자료를많이 만들어야 대충보신다고ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022016.1.6",
          "form": "간략하게 만들면 분명 리마크 달꺼라고ㅎㅎㅎ",
          "original_form": "간략하게 만들면 분명 리마크 달꺼라고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022016.1.7",
          "form": "풉 ㅋㅋㅋ 정답이네여 ㅋ",
          "original_form": "풉 ㅋㅋㅋ 정답이네여 ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022016.1.8",
          "form": "대충보셔야 저도 일하죠ㅎㅎㅎ",
          "original_form": "대충보셔야 저도 일하죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022016.1.9",
          "form": "똑똑 하신데~~",
          "original_form": "똑똑 하신데~~",
          "speaker_id": "1",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100022016.1.10",
          "form": "그렇게 살아가야죠ㅎ",
          "original_form": "그렇게 살아가야죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100022016.1.11",
          "form": "근데 이게 장점도있는게",
          "original_form": "근데 이게 장점도있는게",
          "speaker_id": "2",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100022016.1.12",
          "form": "얍얍~",
          "original_form": "얍얍~",
          "speaker_id": "1",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100022016.1.13",
          "form": "자료가 방대하고 또 확실한 정보를 제공해주면",
          "original_form": "자료가 방대하고 또 확실한 정보를 제공해주면",
          "speaker_id": "2",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100022016.1.14",
          "form": "내생각과 다르게 좋아하세요 ㅎㅎㅎㅎㅎㅎㄹㄹㄹㄹ",
          "original_form": "내생각과 다르게 좋아하세요 ㅎㅎㅎㅎㅎㅎㄹㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022016.1.15",
          "form": "난 호박엿드시라고 만든건데 ㅎㅎㅎㄹㄹㄹ",
          "original_form": "난 호박엿드시라고 만든건데 ㅎㅎㅎㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022016.1.16",
          "form": "뭔가 한거처럼 보이자나여 ㅋㅋㅋ",
          "original_form": "뭔가 한거처럼 보이자나여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022016.1.17",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:00"
        },
        {
          "id": "MDRW2100022016.1.18",
          "form": "꾸미는거 전문이라ㅎㅎㅎ",
          "original_form": "꾸미는거 전문이라ㅎㅎㅎ",
