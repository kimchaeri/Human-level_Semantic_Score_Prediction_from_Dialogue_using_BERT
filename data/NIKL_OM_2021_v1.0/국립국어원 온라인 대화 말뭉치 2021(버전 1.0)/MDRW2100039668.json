      "utterance": [
        {
          "id": "MDRW2100039668.1.1",
          "form": "머라구 하셨는지 안 보여여",
          "original_form": "머라구 하셨는지 안 보여여",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039668.1.2",
          "form": "디저트도 드시나여?",
          "original_form": "디저트도 드시나여?",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039668.1.3",
          "form": "에프에 하면 시간 오래 걸리지 않아요?",
          "original_form": "에프에 하면 시간 오래 걸리지 않아요?",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039668.1.4",
          "form": "아직 식사중이라ㅎ",
          "original_form": "아직 식사중이라ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039668.1.5",
          "form": "아주 넉넉하게",
          "original_form": "아주 넉넉하게",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039668.1.6",
          "form": "20분이면 돼영",
          "original_form": "20분이면 돼영",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039668.1.7",
          "form": "기회되면 해드셔보세여 아주 간단하고 맛있어여",
          "original_form": "기회되면 해드셔보세여 아주 간단하고 맛있어여",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039668.1.8",
          "form": "생각보다 짧네요!",
          "original_form": "생각보다 짧네요!",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039668.1.9",
          "form": "오오 수육용으로 두꺼운거 그대로 넣는 건가요",
          "original_form": "오오 수육용으로 두꺼운거 그대로 넣는 건가요",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039668.1.10",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039668.1.11",
          "form": "진짜 겉이 바삭해서",
          "original_form": "진짜 겉이 바삭해서",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039668.1.12",
          "form": "수육이랑은 다른 맛이에요!",
          "original_form": "수육이랑은 다른 맛이에요!",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039668.1.13",
          "form": "ㄷㄱㄷㄱ",
          "original_form": "ㄷㄱㄷㄱ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039668.1.14",
          "form": "수육과 구이의 사이 어드메인가요",
          "original_form": "수육과 구이의 사이 어드메인가요",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039668.1.15",
          "form": "넘나 궁금한것!",
          "original_form": "넘나 궁금한것!",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039668.1.16",
          "form": "혹시 집에 에프 있으세여?",
          "original_form": "혹시 집에 에프 있으세여?",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039668.1.17",
          "form": "에프 있으면 넣기만 해도 먹을 수 있는 요리가 무궁무진하답니다...",
          "original_form": "에프 있으면 넣기만 해도 먹을 수 있는 요리가 무궁무진하답니다...",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039668.1.18",
          "form": "ㅎ있는데 잘 안써요",
          "original_form": "ㅎ있는데 잘 안써요",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039668.1.19",
          "form": "최근에 김말이 먹었네요ㅎ",
          "original_form": "최근에 김말이 먹었네요ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039668.1.20",
          "form": "고기를 구워드셔요....",
          "original_form": "고기를 구워드셔요....",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039668.1.21",
          "form": "고기...",
          "original_form": "고기...",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039668.1.22",
          "form": "아ㅋㅋ 너무 아련하게 권하시는데요ㅋㅋㅋ",
          "original_form": "아ㅋㅋ 너무 아련하게 권하시는데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039668.1.23",
          "form": "또 권할 에프 레시피 있나요?!",
          "original_form": "또 권할 에프 레시피 있나요?!",
          "speaker_id": "1",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039668.1.24",
          "form": "어유 많죠많죠",
          "original_form": "어유 많죠많죠",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039668.1.25",
          "form": "버섯!!!",
          "original_form": "버섯!!!",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039668.1.26",
          "form": "버섯이요?",
          "original_form": "버섯이요?",
          "speaker_id": "1",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039668.1.27",
          "form": "새송이버섯 통째로 넣어서 구워먹어도 진짜 맛있고",
          "original_form": "새송이버섯 통째로 넣어서 구워먹어도 진짜 맛있고",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039668.1.28",
          "form": "고구마 조각내서 구워먹어도 진짜 맛있어요",
          "original_form": "고구마 조각내서 구워먹어도 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210903 20:31"
        }
      ]
    }
  ]
