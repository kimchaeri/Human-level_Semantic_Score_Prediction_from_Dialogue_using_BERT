      "utterance": [
        {
          "id": "MDRW2100021951.1.1",
          "form": "다이어트?",
          "original_form": "다이어트?",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100021951.1.2",
          "form": "계속하세요?",
          "original_form": "계속하세요?",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100021951.1.3",
          "form": "평생 아닌가요?",
          "original_form": "평생 아닌가요?",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100021951.1.4",
          "form": "아... 그렇죠ㅠ",
          "original_form": "아... 그렇죠ㅠ",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100021951.1.5",
          "form": "근데 살은 왜안빠질까요?",
          "original_form": "근데 살은 왜안빠질까요?",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100021951.1.6",
          "form": "ㅜㅠ운동 식이 둘다하세요?",
          "original_form": "ㅜㅠ운동 식이 둘다하세요?",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100021951.1.7",
          "form": "둘다 항상 생각만하고있습니다ㅎㅎ",
          "original_form": "둘다 항상 생각만하고있습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100021951.1.8",
          "form": "생각보다 잘안되는게 다이어트죠ㅠㅠ",
          "original_form": "생각보다 잘안되는게 다이어트죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100021951.1.9",
          "form": "닭가슴살 맨날 먹고았는데도 안빠지면 소질이없는거겟죠?",
          "original_form": "닭가슴살 맨날 먹고았는데도 안빠지면 소질이없는거겟죠?",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100021951.1.10",
          "form": "근데 살보단 건강때문에 음식도 조절하고 운동도 하는거같아요",
          "original_form": "근데 살보단 건강때문에 음식도 조절하고 운동도 하는거같아요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100021951.1.11",
          "form": "운동은 안하시고 닭만 드시나요?",
          "original_form": "운동은 안하시고 닭만 드시나요?",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100021951.1.12",
          "form": "운동은 조만간 시작하려구요",
          "original_form": "운동은 조만간 시작하려구요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100021951.1.13",
          "form": "동생이 동네에 사는데 살이많이쪄서 같이 운동하기로햌ㅅ어요",
          "original_form": "동생이 동네에 사는데 살이많이쪄서 같이 운동하기로햌ㅅ어요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100021951.1.14",
          "form": "런닝이 최곤거같아요",
          "original_form": "런닝이 최곤거같아요",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100021951.1.15",
          "form": "런닝머신이요?ㅎㅎ",
          "original_form": "런닝머신이요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100021951.1.16",
          "form": "아니면 실제로?",
          "original_form": "아니면 실제로?",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100021951.1.17",
          "form": "뭐든?",
          "original_form": "뭐든?",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100021951.1.18",
          "form": "전 런닝머신은 재미없더라고요",
          "original_form": "전 런닝머신은 재미없더라고요",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100021951.1.19",
          "form": "걷기와 런닝ㅎㅎ",
          "original_form": "걷기와 런닝ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100021951.1.20",
          "form": "그게 가장 살이많이 빠짐다고했어요ㅎ",
          "original_form": "그게 가장 살이많이 빠짐다고했어요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100021951.1.21",
          "form": "1만보 걷기 챌린지 도전해보새요ㅎ",
          "original_form": "1만보 걷기 챌린지 도전해보새요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100021951.1.22",
          "form": "아 저 잘 걸어다녀요ㅋㅋ",
          "original_form": "아 저 잘 걸어다녀요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100021951.1.23",
          "form": "시간 괜찮으시면 배민 뚜벅이 해보세요ㅋㅋ 진짜 살 많이 빠져요ㅋㅋ",
          "original_form": "시간 괜찮으시면 배민 뚜벅이 해보세요ㅋㅋ 진짜 살 많이 빠져요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100021951.1.24",
          "form": "배민뚜벅이가 알바인가요?ㅎ",
          "original_form": "배민뚜벅이가 알바인가요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 03:30"
        },
        {
          "id": "MDRW2100021951.1.25",
          "form": "네네ㅋㅋ 그걸로 살 엄청 빼요ㅋㅋ",
          "original_form": "네네ㅋㅋ 그걸로 살 엄청 빼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:28"
        }
      ]
    }
