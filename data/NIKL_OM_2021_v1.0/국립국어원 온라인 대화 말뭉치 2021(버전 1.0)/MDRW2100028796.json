      "utterance": [
        {
          "id": "MDRW2100028796.1.1",
          "form": "좋아하시는 음료 있으세요?",
          "original_form": "좋아하시는 음료 있으세요?",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028796.1.2",
          "form": "저는 식ㅎ혜 좋아해요",
          "original_form": "저는 식ㅎ혜 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028796.1.3",
          "form": "어떤 음료 좋아하세요",
          "original_form": "어떤 음료 좋아하세요",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028796.1.4",
          "form": "저는 갈아만든 배 음료 좋아해요ㅎ",
          "original_form": "저는 갈아만든 배 음료 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028796.1.5",
          "form": "저희 입맛이 조금 옛스럽네요ㅋㅋㅌㅋ",
          "original_form": "저희 입맛이 조금 옛스럽네요ㅋㅋㅌㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028796.1.6",
          "form": "저도 그거 좋아합니다ㅋㅋ",
          "original_form": "저도 그거 좋아합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028796.1.7",
          "form": "식혜에 갈배ㅎㅎ 두사람 독특해요ㅎㅎ",
          "original_form": "식혜에 갈배ㅎㅎ 두사람 독특해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028796.1.8",
          "form": "카페가면 뭐 드세요?",
          "original_form": "카페가면 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028796.1.9",
          "form": "전 오곡라떼 먹어요#",
          "original_form": "전 오곡라떼 먹어요#",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028796.1.10",
          "form": "어떤 거 드세요?",
          "original_form": "어떤 거 드세요?",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028796.1.11",
          "form": "저는 카라멜 마끼야또",
          "original_form": "저는 카라멜 마끼야또",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028796.1.12",
          "form": "달달한거 좋아합니다ㅎ",
          "original_form": "달달한거 좋아합니다ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028796.1.13",
          "form": "저도 단 거",
          "original_form": "저도 단 거",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028796.1.14",
          "form": "더 단거  좋아합니다",
          "original_form": "더 단거  좋아합니다",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028796.1.15",
          "form": "살은 언제 빼 죠?ㅋㅋ",
          "original_form": "살은 언제 빼 죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028796.1.16",
          "form": "저는 탄산도 좋아해서",
          "original_form": "저는 탄산도 좋아해서",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028796.1.17",
          "form": "클났어요ㅠㅠㅋㅋㅋ",
          "original_form": "클났어요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028796.1.18",
          "form": "저도 탄산 좋아합니다ㅋㅋㅋㅋ",
          "original_form": "저도 탄산 좋아합니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028796.1.19",
          "form": "안 먹으면 소화가 안 되죠ㅋㅋㅋ",
          "original_form": "안 먹으면 소화가 안 되죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028796.1.20",
          "form": "앗 인정",
          "original_form": "앗 인정",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028796.1.21",
          "form": "식후 탄산은 기본이죠...",
          "original_form": "식후 탄산은 기본이죠...",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028796.1.22",
          "form": "소화제 같은 거죠ㅋㅋㅋㅋ",
          "original_form": "소화제 같은 거죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028796.1.23",
          "form": "휴...저희는 영양제 잘 챙겨 먹읍시다...",
          "original_form": "휴...저희는 영양제 잘 챙겨 먹읍시다...",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028796.1.24",
          "form": "그렇죠??",
          "original_form": "그렇죠??",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028796.1.25",
          "form": "약물로 버 팁 시다ㅋㅋ",
          "original_form": "약물로 버 팁 시다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:13"
        }
      ]
