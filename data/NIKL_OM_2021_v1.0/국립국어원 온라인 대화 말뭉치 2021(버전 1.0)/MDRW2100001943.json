      "utterance": [
        {
          "id": "MDRW2100001943.1.1",
          "form": "오늘 먹은 저녁!",
          "original_form": "오늘 먹은 저녁!",
          "speaker_id": "1",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001943.1.2",
          "form": "오늘은 평범하게 김치찌개랑 미역국! 먹었네요",
          "original_form": "오늘은 평범하게 김치찌개랑 미역국! 먹었네요",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001943.1.3",
          "form": "저는 삼겹살을 먹었습니다",
          "original_form": "저는 삼겹살을 먹었습니다",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001943.1.4",
          "form": "김치찌개랑 미역국을 같이 먹다니",
          "original_form": "김치찌개랑 미역국을 같이 먹다니",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001943.1.5",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001943.1.6",
          "form": "한식사에 두개의 국",
          "original_form": "한식사에 두개의 국",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001943.1.7",
          "form": "ㅋㅋㅋㅋㅋ 찌개 잖아요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 찌개 잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001943.1.8",
          "form": "삼겹살..",
          "original_form": "삼겹살..",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001943.1.9",
          "form": "부럽다..",
          "original_form": "부럽다..",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001943.1.10",
          "form": "요리 잘해요??",
          "original_form": "요리 잘해요??",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001943.1.11",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001943.1.12",
          "form": "저는 겁나 못합니다",
          "original_form": "저는 겁나 못합니다",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001943.1.13",
          "form": "저번에 카레가 있어서",
          "original_form": "저번에 카레가 있어서",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001943.1.14",
          "form": "데필다가 다 태웠거든요..?",
          "original_form": "데필다가 다 태웠거든요..?",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001943.1.15",
          "form": "남은거 어떻개든 살릴려고.. 냄비 옆에 담아두고",
          "original_form": "남은거 어떻개든 살릴려고.. 냄비 옆에 담아두고",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001943.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ와우",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ와우",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001943.1.17",
          "form": "물이랑 카레가루를 넣었는데..",
          "original_form": "물이랑 카레가루를 넣었는데..",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001943.1.18",
          "form": "카레가루를 저는 당연히 듬뿍 넣는줄 알았더니..",
          "original_form": "카레가루를 저는 당연히 듬뿍 넣는줄 알았더니..",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001943.1.19",
          "form": "얼마 넣으면 안되더라구요..",
          "original_form": "얼마 넣으면 안되더라구요..",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001943.1.20",
          "form": "전 진짜 듬뿍 넣어서..",
          "original_form": "전 진짜 듬뿍 넣어서..",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001943.1.21",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001943.1.22",
          "form": "원래 있던 카레를",
          "original_form": "원래 있던 카레를",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001943.1.23",
          "form": "살리는데에도 실패하셨군요",
          "original_form": "살리는데에도 실패하셨군요",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001943.1.24",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001943.1.25",
          "form": "맞습니다.. 하핳",
          "original_form": "맞습니다.. 하핳",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001943.1.26",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001943.1.27",
          "form": "저는 근 10년을 혼자 살아서",
          "original_form": "저는 근 10년을 혼자 살아서",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001943.1.28",
          "form": "먹고살 정도는 해요",
          "original_form": "먹고살 정도는 해요",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001943.1.29",
          "form": "왜냐면 우리의 영원한 선생님 백아저씨가 있으니까요",
          "original_form": "왜냐면 우리의 영원한 선생님 백아저씨가 있으니까요",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001943.1.30",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001943.1.31",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001943.1.32",
          "form": "레시피만 따라하면",
          "original_form": "레시피만 따라하면",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001943.1.33",
          "form": "잘 돼죠!!",
          "original_form": "잘 돼죠!!",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001943.1.34",
          "form": "근데 혼자 10년.. 대박",
          "original_form": "근데 혼자 10년.. 대박",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001943.1.35",
          "form": "집 구하셨어요..?",
          "original_form": "집 구하셨어요..?",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001943.1.36",
          "form": "뭘 주로 먹어요?",
          "original_form": "뭘 주로 먹어요?",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001943.1.37",
          "form": "만들기 귀찮을거 같은데..",
          "original_form": "만들기 귀찮을거 같은데..",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001943.1.38",
          "form": "허닭볶음밥이 최애구요",
          "original_form": "허닭볶음밥이 최애구요",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001943.1.39",
          "form": "마켓컬리나 쿠팡에서 잔뜩 시켜서 냉동실이 꽉차있습니다",
          "original_form": "마켓컬리나 쿠팡에서 잔뜩 시켜서 냉동실이 꽉차있습니다",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001943.1.40",
          "form": "근데 배달도 많이 먹어요 플라스틱이 너무 나와서 죄책감이 좀 들지만..",
          "original_form": "근데 배달도 많이 먹어요 플라스틱이 너무 나와서 죄책감이 좀 들지만..",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001943.1.41",
          "form": "최애 음식이 뭔가요?",
          "original_form": "최애 음식이 뭔가요?",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001943.1.42",
          "form": "와우 메모 해놓을게요!",
          "original_form": "와우 메모 해놓을게요!",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001943.1.43",
          "form": "제 최애 음식은 계란찜!!",
          "original_form": "제 최애 음식은 계란찜!!",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001943.1.44",
          "form": "진짜 사랑합니다..",
          "original_form": "진짜 사랑합니다..",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001943.1.45",
          "form": "물론 제가 해본적은 없지만..",
          "original_form": "물론 제가 해본적은 없지만..",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001943.1.46",
          "form": "3일에 한번꼴로 먹고 있어요.. ㅗㅎ",
          "original_form": "3일에 한번꼴로 먹고 있어요.. ㅗㅎ",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001943.1.47",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001943.1.48",
          "form": "계란찜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "계란찜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001943.1.49",
          "form": "부럽네요 어머니가 해주시는 밥",
          "original_form": "부럽네요 어머니가 해주시는 밥",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001943.1.50",
          "form": "광주에 맛집은 어딘가요",
          "original_form": "광주에 맛집은 어딘가요",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001943.1.51",
          "form": "서울 곱창! 이랑 중국집은 칠봉이 짬뽕!  글구 은행나무갈매기 갈비집",
          "original_form": "서울 곱창! 이랑 중국집은 칠봉이 짬뽕!  글구 은행나무갈매기 갈비집",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001943.1.52",
          "form": "사실은 잘 몰라요.. 가본 곳중에는 이것들 ㅎㅎ",
          "original_form": "사실은 잘 몰라요.. 가본 곳중에는 이것들 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001943.1.53",
          "form": "올 저 광주 한번도 안가봤는데 나중에 가면 먹어볼게요 칠봉이 짬뽕 이름이 맘에 드네요",
          "original_form": "올 저 광주 한번도 안가봤는데 나중에 가면 먹어볼게요 칠봉이 짬뽕 이름이 맘에 드네요",
          "speaker_id": "1",
          "time": "20210819 22:43"
        }
      ]
    }
