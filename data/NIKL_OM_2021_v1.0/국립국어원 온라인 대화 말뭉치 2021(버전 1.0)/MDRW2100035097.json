        {
          "id": "MDRW2100035097.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035097.1.2",
          "form": "나 내일 친구들이랑 제주도 가",
          "original_form": "나 내일 친구들이랑 제주도 가",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035097.1.3",
          "form": "비행기 타고 가니",
          "original_form": "비행기 타고 가니",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035097.1.4",
          "form": "당연하지 배는 오래걸려",
          "original_form": "당연하지 배는 오래걸려",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035097.1.5",
          "form": "가서는 렌트할 거야?",
          "original_form": "가서는 렌트할 거야?",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035097.1.6",
          "form": "내가 면허가 있긴하지만 그렇게 잘하지는 않다보니",
          "original_form": "내가 면허가 있긴하지만 그렇게 잘하지는 않다보니",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035097.1.7",
          "form": "대중교통 타고 다니기엔 불편하지 않나",
          "original_form": "대중교통 타고 다니기엔 불편하지 않나",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035097.1.8",
          "form": "그건 그래 택시를 탈수도 있고",
          "original_form": "그건 그래 택시를 탈수도 있고",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035097.1.9",
          "form": "제주도 가서 뭐 먹을 거야",
          "original_form": "제주도 가서 뭐 먹을 거야",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035097.1.10",
          "form": "일단 도착하자마자 돼지고기 먹어야지",
          "original_form": "일단 도착하자마자 돼지고기 먹어야지",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035097.1.11",
          "form": "제주도 흑돼지는 정말 최고지",
          "original_form": "제주도 흑돼지는 정말 최고지",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035097.1.12",
          "form": "맞아 그리고선 귤 카페를 갈거야",
          "original_form": "맞아 그리고선 귤 카페를 갈거야",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035097.1.13",
          "form": "귤 카페라는 게 있구나",
          "original_form": "귤 카페라는 게 있구나",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035097.1.14",
          "form": "제주도하면 귤이니까",
          "original_form": "제주도하면 귤이니까",
          "speaker_id": "1",
          "time": "20210829 02:36"
        }
