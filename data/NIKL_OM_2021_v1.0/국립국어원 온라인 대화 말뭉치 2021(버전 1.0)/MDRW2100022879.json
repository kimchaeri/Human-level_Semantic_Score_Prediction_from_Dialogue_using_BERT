      "utterance": [
        {
          "id": "MDRW2100022879.1.1",
          "form": "우리 내일 다시 학교가네ㅠㅠ",
          "original_form": "우리 내일 다시 학교가네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022879.1.2",
          "form": "아 방학 너무 짧은 거 같다",
          "original_form": "아 방학 너무 짧은 거 같다",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022879.1.3",
          "form": "반학기 정도는 쉬고 싶은뎈ㅋㅋㅋ",
          "original_form": "반학기 정도는 쉬고 싶은뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022879.1.4",
          "form": "그니까 방학 너무 짧아",
          "original_form": "그니까 방학 너무 짧아",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022879.1.5",
          "form": "내일 학교간다는게 믿기지가 않는다",
          "original_form": "내일 학교간다는게 믿기지가 않는다",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022879.1.6",
          "form": "수강신청은 잘 끝냈어??",
          "original_form": "수강신청은 잘 끝냈어??",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022879.1.7",
          "form": "당연히 올클했지",
          "original_form": "당연히 올클했지",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022879.1.8",
          "form": "넌 성공했어?",
          "original_form": "넌 성공했어?",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022879.1.9",
          "form": "완전 망했어....",
          "original_form": "완전 망했어....",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022879.1.10",
          "form": "왜?? 뭐 놓쳤어?",
          "original_form": "왜?? 뭐 놓쳤어?",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022879.1.11",
          "form": "PC방 갔어야 했는데 날짜를 착각해서ㅠㅠ",
          "original_form": "PC방 갔어야 했는데 날짜를 착각해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022879.1.12",
          "form": "헐 그럼 수강정정기간때 주운거야?",
          "original_form": "헐 그럼 수강정정기간때 주운거야?",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022879.1.13",
          "form": "ㅇㅇ 교수님한테 전화도 드리고...",
          "original_form": "ㅇㅇ 교수님한테 전화도 드리고...",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022879.1.14",
          "form": "고생많았네ㅜㅜㅠ 그래서 수업에 받아주셨어?",
          "original_form": "고생많았네ㅜㅜㅠ 그래서 수업에 받아주셨어?",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022879.1.15",
          "form": "전공 수업이야?",
          "original_form": "전공 수업이야?",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022879.1.16",
          "form": "응 근데 그래도 학점 모자라서 이번에 졸업하려면 계절하기도 들어야 해",
          "original_form": "응 근데 그래도 학점 모자라서 이번에 졸업하려면 계절하기도 들어야 해",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022879.1.17",
          "form": "하기가 아니라 학기",
          "original_form": "하기가 아니라 학기",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022879.1.18",
          "form": "벌써 졸업학년이구나 난 아직 2년이나 남았네ㅠㅠ",
          "original_form": "벌써 졸업학년이구나 난 아직 2년이나 남았네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022879.1.19",
          "form": "막학기에 계절학기 듣는 바보가 있다?!",
          "original_form": "막학기에 계절학기 듣는 바보가 있다?!",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022879.1.20",
          "form": "바로 나",
          "original_form": "바로 나",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022879.1.21",
          "form": "졸업학년의 위엄을 보여주네",
          "original_form": "졸업학년의 위엄을 보여주네",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022879.1.22",
          "form": "아악 내일부터 학교갈 거 생각하면진짜 끔찍하다...",
          "original_form": "아악 내일부터 학교갈 거 생각하면진짜 끔찍하다...",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022879.1.23",
          "form": "돈도 더들고..ㅋㅋㅋㅋ국가장학금은 신청했니?",
          "original_form": "돈도 더들고..ㅋㅋㅋㅋ국가장학금은 신청했니?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022879.1.24",
          "form": "헉 잊고있었다!",
          "original_form": "헉 잊고있었다!",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022879.1.25",
          "form": "니가 말 안해줬으면 이것도 놓칠뻔ㅋㅋㅋㅋ",
          "original_form": "니가 말 안해줬으면 이것도 놓칠뻔ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022879.1.26",
          "form": "ㅋㅋㅋ내덕이지 만나면 너네 학교 학식 한번 같이 먹자ㅋㅋ",
          "original_form": "ㅋㅋㅋ내덕이지 만나면 너네 학교 학식 한번 같이 먹자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022879.1.27",
          "form": "학식 맛있니?",
          "original_form": "학식 맛있니?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022879.1.28",
          "form": "우리는 진짜...싸서 먹는 맛...",
          "original_form": "우리는 진짜...싸서 먹는 맛...",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022879.1.29",
          "form": "너희는?",
          "original_form": "너희는?",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022879.1.30",
          "form": "나 한번도 안먹어봤어",
          "original_form": "나 한번도 안먹어봤어",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022879.1.31",
          "form": "맨날 밖에서 사먹었어",
          "original_form": "맨날 밖에서 사먹었어",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022879.1.32",
          "form": "맛없기로 악명이 높거든",
          "original_form": "맛없기로 악명이 높거든",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022879.1.33",
          "form": "아 그러면 힘든데",
          "original_form": "아 그러면 힘든데",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022879.1.34",
          "form": "그래서 우리도 자주 나가긴 해ㅋㅋㅋㅋ",
          "original_form": "그래서 우리도 자주 나가긴 해ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022879.1.35",
          "form": "그래도 그럼 주변 맛집 잘 알겠네?",
          "original_form": "그래도 그럼 주변 맛집 잘 알겠네?",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022879.1.36",
          "form": "그치 우리 학교 놀러와 맛집 소개해줄게",
          "original_form": "그치 우리 학교 놀러와 맛집 소개해줄게",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022879.1.37",
          "form": "공강날 있어?",
          "original_form": "공강날 있어?",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022879.1.38",
          "form": "시간표 망해서 그딴 거 없다",
          "original_form": "시간표 망해서 그딴 거 없다",
          "speaker_id": "2",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022879.1.39",
          "form": "내게 있는 건... 우주공강뿐....",
          "original_form": "내게 있는 건... 우주공강뿐....",
          "speaker_id": "2",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022879.1.40",
          "form": "공강시간동안 같이 있을 친구는 있니?",
          "original_form": "공강시간동안 같이 있을 친구는 있니?",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022879.1.41",
          "form": "동아리실에 박혀있어야지 뭐...",
          "original_form": "동아리실에 박혀있어야지 뭐...",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022879.1.42",
          "form": "오 너 무슨 동아린데??",
          "original_form": "오 너 무슨 동아린데??",
          "speaker_id": "1",
          "time": "20210829 19:00"
