      "utterance": [
        {
          "id": "MDRW2100041990.1.1",
          "form": "이제 진짜 밥하러 가야되겟다",
          "original_form": "이제 진짜 밥하러 가야되겟다",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041990.1.2",
          "form": "나중에 밥하고 톡할게 ㅋㅋ 어여 라면 먹거랍",
          "original_form": "나중에 밥하고 톡할게 ㅋㅋ 어여 라면 먹거랍",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041990.1.3",
          "form": "그려 그려",
          "original_form": "그려 그려",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041990.1.4",
          "form": "밥 맛있게 먹고 와",
          "original_form": "밥 맛있게 먹고 와",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041990.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041990.1.6",
          "form": "이제 책읽어주고 재운다ㅋ",
          "original_form": "이제 책읽어주고 재운다ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041990.1.7",
          "form": "육퇴를 위해ㅋㅋ",
          "original_form": "육퇴를 위해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041990.1.8",
          "form": "난 육퇴 하고 지코바 시켜놨지",
          "original_form": "난 육퇴 하고 지코바 시켜놨지",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041990.1.9",
          "form": "오늘 다 끝내고 깨운하게 자 소",
          "original_form": "오늘 다 끝내고 깨운하게 자 소",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041990.1.10",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041990.1.11",
          "form": "지코바",
          "original_form": "지코바",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041990.1.12",
          "form": "땡기노ㅋㅋ",
          "original_form": "땡기노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041990.1.13",
          "form": "맛있는거먹는대이ㅋ",
          "original_form": "맛있는거먹는대이ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041990.1.14",
          "form": "오늘은 모든 걸 놓겠다",
          "original_form": "오늘은 모든 걸 놓겠다",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100041990.1.15",
          "form": "내일부터 다이어트 해야지ㅋㅋㅋ",
          "original_form": "내일부터 다이어트 해야지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100041990.1.16",
          "form": "어제 애들재우다가 잠들었다ㅋ",
          "original_form": "어제 애들재우다가 잠들었다ㅋ",
          "speaker_id": "2",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100041990.1.17",
          "form": "못 끝냈네ㅋㅋㅋㅋㅋ",
          "original_form": "못 끝냈네ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100041990.1.18",
          "form": "나도 먹다가 지쳐 잠들었어ㅋㅋㅋㅋ",
          "original_form": "나도 먹다가 지쳐 잠들었어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100041990.1.19",
          "form": "아 웃기노ㅋㅋㄱㅋㅋㄱㅋㅋ",
          "original_form": "아 웃기노ㅋㅋㄱㅋㅋㄱㅋㅋ",
