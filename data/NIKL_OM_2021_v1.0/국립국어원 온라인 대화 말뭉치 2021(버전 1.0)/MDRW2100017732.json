      "utterance": [
        {
          "id": "MDRW2100017732.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017732.1.2",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017732.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017732.1.4",
          "form": "요리는 딱히 못해요 ㅠㅠ",
          "original_form": "요리는 딱히 못해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017732.1.5",
          "form": "하지만 먹는건 엄청 잘해요 ㅎㅎ",
          "original_form": "하지만 먹는건 엄청 잘해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017732.1.6",
          "form": "그럼 밥은 주로 어떻게 해결하세요?",
          "original_form": "그럼 밥은 주로 어떻게 해결하세요?",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017732.1.7",
          "form": "집에서 어머니가 해주시거나",
          "original_form": "집에서 어머니가 해주시거나",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017732.1.8",
          "form": "아니면 배달시켜서 먹어용",
          "original_form": "아니면 배달시켜서 먹어용",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017732.1.9",
          "form": "오~ 주로 배달 시키는 시간대가 어떻게 돼요?",
          "original_form": "오~ 주로 배달 시키는 시간대가 어떻게 돼요?",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017732.1.10",
          "form": "주로 저녁시간대요!",
          "original_form": "주로 저녁시간대요!",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017732.1.11",
          "form": "그럼 자주 시켜먹는 음식은 뭐에요?",
          "original_form": "그럼 자주 시켜먹는 음식은 뭐에요?",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017732.1.12",
          "form": "치킨을 주로 시켜먹죠!",
          "original_form": "치킨을 주로 시켜먹죠!",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017732.1.13",
          "form": "우왕 ㅎㅎ 치킨 요즘 다양하게 나오던데 젤 좋아하는 치킨은 뭐에요?",
          "original_form": "우왕 ㅎㅎ 치킨 요즘 다양하게 나오던데 젤 좋아하는 치킨은 뭐에요?",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017732.1.14",
          "form": "전 BBQ 자메이카랑 교촌 허니콤보",
          "original_form": "전 BBQ 자메이카랑 교촌 허니콤보",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017732.1.15",
          "form": "그리고 지코바 좋아해요 히히",
          "original_form": "그리고 지코바 좋아해요 히히",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017732.1.16",
          "form": "오호 ㅎㅎ 근데 그거 아세요?",
          "original_form": "오호 ㅎㅎ 근데 그거 아세요?",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017732.1.17",
          "form": "지코바가 모든 지점이 숯불인건 아니래요....",
          "original_form": "지코바가 모든 지점이 숯불인건 아니래요....",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017732.1.18",
          "form": "아 알고있어용",
          "original_form": "아 알고있어용",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017732.1.19",
          "form": "저희 점은 다행히 숯불인지라",
          "original_form": "저희 점은 다행히 숯불인지라",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017732.1.20",
          "form": "안도했던 기억도 나네요 ㅎㅎ",
          "original_form": "안도했던 기억도 나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017732.1.21",
          "form": "어 알고 계셨구나 ㅋㅋㅋ그거 좀 충격이었어요.",
          "original_form": "어 알고 계셨구나 ㅋㅋㅋ그거 좀 충격이었어요.",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017732.1.22",
          "form": "어쩐지 지코바 지점마다 맛이 조금씩 다르더라구요!!",
          "original_form": "어쩐지 지코바 지점마다 맛이 조금씩 다르더라구요!!",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017732.1.23",
          "form": "맞아요 그 풍미한 맛도 있고 뭔가 밍밍한 맛도 나는데",
          "original_form": "맞아요 그 풍미한 맛도 있고 뭔가 밍밍한 맛도 나는데",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017732.1.24",
          "form": "각각 보니까 숯불 차이더군요",
          "original_form": "각각 보니까 숯불 차이더군요",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017732.1.25",
          "form": "맞아요.. 숯불 차이가 이렇게 크다니..",
          "original_form": "맞아요.. 숯불 차이가 이렇게 크다니..",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017732.1.26",
          "form": "하긴 삼겹살도 숯불에 먹으면 더 꿀맛이죠 ㅎㅎ",
          "original_form": "하긴 삼겹살도 숯불에 먹으면 더 꿀맛이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017732.1.27",
          "form": "맞아요! 숯불 삼겹살은 진짜 넘사..",
          "original_form": "맞아요! 숯불 삼겹살은 진짜 넘사..",
          "speaker_id": "2",
          "time": "20210826 21:26"
        }
      ]
