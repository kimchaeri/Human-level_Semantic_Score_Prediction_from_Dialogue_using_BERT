      "utterance": [
        {
          "id": "MDRW2100010862.1.1",
          "form": "아직 비오나요?",
          "original_form": "아직 비오나요?",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010862.1.2",
          "form": "아직 비 짱 많이 와요ㅠㅠ",
          "original_form": "아직 비 짱 많이 와요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010862.1.3",
          "form": "어.. 경기도 서부는 안오는데...",
          "original_form": "어.. 경기도 서부는 안오는데...",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010862.1.4",
          "form": "경상도는 짱 많이 와요",
          "original_form": "경상도는 짱 많이 와요",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010862.1.5",
          "form": "태풍 때문에",
          "original_form": "태풍 때문에",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010862.1.6",
          "form": "많이 오는 거 같아요!",
          "original_form": "많이 오는 거 같아요!",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010862.1.7",
          "form": "아. 태풍 아직 안나갓구나..",
          "original_form": "아. 태풍 아직 안나갓구나..",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010862.1.8",
          "form": "올해는 아랫지방이 비 많이 오네요",
          "original_form": "올해는 아랫지방이 비 많이 오네요",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010862.1.9",
          "form": "그런 거 같아요ㅠㅠ",
          "original_form": "그런 거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010862.1.10",
          "form": "경기도는 춥나요??",
          "original_form": "경기도는 춥나요??",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010862.1.11",
          "form": "아니요.. 여전히 축축하고 더움...",
          "original_form": "아니요.. 여전히 축축하고 더움...",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010862.1.12",
          "form": "이제는 가을장마가 정착되나바용..",
          "original_form": "이제는 가을장마가 정착되나바용..",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010862.1.13",
          "form": "거긴 추워여?",
          "original_form": "거긴 추워여?",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010862.1.14",
          "form": "춥지는 않아요!",
          "original_form": "춥지는 않아요!",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010862.1.15",
          "form": "그래도 완전 덥지도 않아요!",
          "original_form": "그래도 완전 덥지도 않아요!",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010862.1.16",
          "form": "대신 경상도는 겨울에 따뜻해서 부렂",
          "original_form": "대신 경상도는 겨울에 따뜻해서 부렂",
          "speaker_id": "1",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010862.1.17",
          "form": "부럽",
          "original_form": "부럽",
          "speaker_id": "1",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010862.1.18",
          "form": "전 경기도가 더 부러워요",
          "original_form": "전 경기도가 더 부러워요",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010862.1.19",
          "form": "눈을 좋아하는데",
          "original_form": "눈을 좋아하는데",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010862.1.20",
          "form": "눈이 많이 오잖아요!",
          "original_form": "눈이 많이 오잖아요!",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010862.1.21",
          "form": "아직 학생이신가바요...\r\n눈오면 출근하기 귀찮... ㅋㅋㅋㅋ",
          "original_form": "아직 학생이신가바요...\r\n눈오면 출근하기 귀찮... ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010862.1.22",
          "form": "부산은 1년에 눈한번도 안올때도잇죠?",
          "original_form": "부산은 1년에 눈한번도 안올때도잇죠?",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010862.1.23",
          "form": "네ㅠㅠㅠㅠ",
          "original_form": "네ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010862.1.24",
          "form": "눈만 완전 열심히 기다리고 있어요",
          "original_form": "눈만 완전 열심히 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010862.1.25",
          "form": "최근에 눈언제 봤어여ㅡ",
          "original_form": "최근에 눈언제 봤어여ㅡ",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010862.1.26",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010862.1.27",
          "form": "21년도 1월?2월? 그때요!",
          "original_form": "21년도 1월?2월? 그때요!",
