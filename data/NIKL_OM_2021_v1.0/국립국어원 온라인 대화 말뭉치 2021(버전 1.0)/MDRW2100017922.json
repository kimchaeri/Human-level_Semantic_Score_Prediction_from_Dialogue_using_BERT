      "utterance": [
        {
          "id": "MDRW2100017922.1.1",
          "form": "가장 최근에 구매하신 용품이 뭐예요?",
          "original_form": "가장 최근에 구매하신 용품이 뭐예요?",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017922.1.2",
          "form": "최근에 구매한거라면",
          "original_form": "최근에 구매한거라면",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.3",
          "form": "옷!",
          "original_form": "옷!",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.4",
          "form": "옷샀죠",
          "original_form": "옷샀죠",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.5",
          "form": "오 가을옷?",
          "original_form": "오 가을옷?",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.6",
          "form": "슬슬 가을철 올라고 하는거같아서",
          "original_form": "슬슬 가을철 올라고 하는거같아서",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.7",
          "form": "가디건 하나샀어요",
          "original_form": "가디건 하나샀어요",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.8",
          "form": "패션에 관심이 많으세요?",
          "original_form": "패션에 관심이 많으세요?",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.9",
          "form": "예전엔 없었는ㄷ0",
          "original_form": "예전엔 없었는ㄷ0",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.10",
          "form": "성인되고 나니까 뭔가 옷에 관심이 생겨버렸어요 ㅋㅋ",
          "original_form": "성인되고 나니까 뭔가 옷에 관심이 생겨버렸어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.11",
          "form": "오 ㅎㅎ 관심이 생기셨군요 ㅎㅎ 전 그냥 작년치 입으려구요",
          "original_form": "오 ㅎㅎ 관심이 생기셨군요 ㅎㅎ 전 그냥 작년치 입으려구요",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.12",
          "form": "아하 그렇군용",
          "original_form": "아하 그렇군용",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.13",
          "form": "앗 이제 막 성인 되신 거에요?",
          "original_form": "앗 이제 막 성인 되신 거에요?",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017922.1.14",
          "form": "아뇨! 성인된진 몇년됬어용",
          "original_form": "아뇨! 성인된진 몇년됬어용",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017922.1.15",
          "form": "오 몇년이어도 초반이네요 ㅎㅎ",
          "original_form": "오 몇년이어도 초반이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017922.1.16",
          "form": "한창 패션감성 충만할 때네요",
          "original_form": "한창 패션감성 충만할 때네요",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017922.1.17",
          "form": "네넹 ㅎㅎ",
          "original_form": "네넹 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017922.1.18",
          "form": "또 뭐 사셨어요?",
          "original_form": "또 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017922.1.19",
          "form": "전동칫솔이랑 면도기 구매한 기억도 나네요!",
          "original_form": "전동칫솔이랑 면도기 구매한 기억도 나네요!",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017922.1.20",
          "form": "오 필수품!",
          "original_form": "오 필수품!",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017922.1.21",
          "form": "만족하세요?",
          "original_form": "만족하세요?",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017922.1.22",
          "form": "도루코 면도기 왜쓰나 싶었는데",
          "original_form": "도루코 면도기 왜쓰나 싶었는데",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017922.1.23",
          "form": "너무 늦게 안거같기도해요 ㅠㅠ",
          "original_form": "너무 늦게 안거같기도해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017922.1.24",
          "form": "오 도루코!",
          "original_form": "오 도루코!",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017922.1.25",
          "form": "전동칫솔도 그렇고요",
          "original_form": "전동칫솔도 그렇고요",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017922.1.26",
          "form": "저도 들어봤어요!",
          "original_form": "저도 들어봤어요!",
          "speaker_id": "1",
          "time": "20210826 21:44"
        }
      ]
    }
  ]
