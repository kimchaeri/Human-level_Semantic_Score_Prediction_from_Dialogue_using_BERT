      "utterance": [
        {
          "id": "MDRW2100022590.1.1",
          "form": "아니요 ㅋㅋㅋㅋ예전에 식빵피자 정도는 해봣어요",
          "original_form": "아니요 ㅋㅋㅋㅋ예전에 식빵피자 정도는 해봣어요",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100022590.1.2",
          "form": "아하 식빵피자는 오븐에 구웠나요?",
          "original_form": "아하 식빵피자는 오븐에 구웠나요?",
          "speaker_id": "1",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100022590.1.3",
          "form": "아니요 레인지요!",
          "original_form": "아니요 레인지요!",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022590.1.4",
          "form": "집에 오븐없어요 ㅠㅠ",
          "original_form": "집에 오븐없어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022590.1.5",
          "form": "안드래도 요즘 사고싶더라구요",
          "original_form": "안드래도 요즘 사고싶더라구요",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022590.1.6",
          "form": "우와 전자레인지에도 가능해요?",
          "original_form": "우와 전자레인지에도 가능해요?",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022590.1.7",
          "form": "그럼 재료는 뭐가 필요해요?",
          "original_form": "그럼 재료는 뭐가 필요해요?",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022590.1.8",
          "form": "토마토소스?버섯양파 햄 치즈?",
          "original_form": "토마토소스?버섯양파 햄 치즈?",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022590.1.9",
          "form": "그것만 있으면 돼요?",
          "original_form": "그것만 있으면 돼요?",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022590.1.10",
          "form": "식빵은 없어도 돼요?ㅋㅋㅋㅋㅋ",
          "original_form": "식빵은 없어도 돼요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.11",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ있어야죸ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ있어야죸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.12",
          "form": "토마토소스는 시판소스 써도 돼죠?",
          "original_form": "토마토소스는 시판소스 써도 돼죠?",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.13",
          "form": "근데 다이어트피자는 빵없이하도라구요",
          "original_form": "근데 다이어트피자는 빵없이하도라구요",
          "speaker_id": "2",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.14",
          "form": "네네 그게훨맛잇죠",
          "original_form": "네네 그게훨맛잇죠",
          "speaker_id": "2",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.15",
          "form": "와 다이어트피자는 처음 들어봤어요",
          "original_form": "와 다이어트피자는 처음 들어봤어요",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.16",
          "form": "빵 없이 어떻게 만들어요 그걸?",
          "original_form": "빵 없이 어떻게 만들어요 그걸?",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.17",
          "form": "ㅋㅋㅋㅋㅋ야채정말많이넣구 피자맛만내는용이에요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ야채정말많이넣구 피자맛만내는용이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.18",
          "form": "치즈랑 토마토소스로 맛만내깈ㅋㅋㅋ",
          "original_form": "치즈랑 토마토소스로 맛만내깈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022590.1.19",
          "form": "거기에 파스타면 넣으면 완전 토마토파스타인데 ㅋㅋ",
          "original_form": "거기에 파스타면 넣으면 완전 토마토파스타인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022590.1.20",
          "form": "치즈는 무슨 치즈 넣어야해요?",
          "original_form": "치즈는 무슨 치즈 넣어야해요?",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022590.1.21",
          "form": "모짜렐라치즈요!",
          "original_form": "모짜렐라치즈요!",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022590.1.22",
          "form": "체다랑 같이넣어도되긴해요! 취향따라",
          "original_form": "체다랑 같이넣어도되긴해요! 취향따라",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022590.1.23",
          "form": "슬라이스 치즈느 ㄴ안돼요?ㅠㅠ",
          "original_form": "슬라이스 치즈느 ㄴ안돼요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022590.1.24",
          "form": "히히 맛있겠다 식빵 구매각이네요",
          "original_form": "히히 맛있겠다 식빵 구매각이네요",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022590.1.25",
          "form": "슬라이스 주로 체다치즈잉데 좀 짤거에요 ㅠ",
          "original_form": "슬라이스 주로 체다치즈잉데 좀 짤거에요 ㅠ",
