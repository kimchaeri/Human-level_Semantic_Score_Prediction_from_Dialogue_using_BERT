        },
        {
          "id": "MDRW2100037776.1.2",
          "form": "도착",
          "original_form": "도착",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037776.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ애들이 더 바쁘네여 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ애들이 더 바쁘네여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037776.1.4",
          "form": "연예인스케줄이잖아ㅋㅋ",
          "original_form": "연예인스케줄이잖아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037776.1.5",
          "form": "이제 숙제한다ㅋ",
          "original_form": "이제 숙제한다ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037776.1.6",
          "form": "진짜 연옌 스케줄 ㅋㅋㅋㅋ",
          "original_form": "진짜 연옌 스케줄 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037776.1.7",
          "form": "우린 육퇴여 ㅋㅋㅋㅋ",
          "original_form": "우린 육퇴여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037776.1.8",
          "form": "신이납니다 ㅋㅋㅋ",
          "original_form": "신이납니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037776.1.9",
          "form": "벌써?대박",
          "original_form": "벌써?대박",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037776.1.10",
          "form": "우리는9시넘어야될듯ㅋ",
          "original_form": "우리는9시넘어야될듯ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037776.1.11",
          "form": "축하한다",
          "original_form": "축하한다",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037776.1.12",
          "form": "언니..",
          "original_form": "언니..",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037776.1.13",
          "form": "언니네 애들은",
          "original_form": "언니네 애들은",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037776.1.14",
          "form": "깨있어도 언니 귀찮게ㅡ안하잖아요 ㅋㅋㅋㅋ",
          "original_form": "깨있어도 언니 귀찮게ㅡ안하잖아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037776.1.15",
          "form": "아;;; 그래도 자주면 좋지",
          "original_form": "아;;; 그래도 자주면 좋지",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037776.1.16",
          "form": "육퇴하면 상콤함",
          "original_form": "육퇴하면 상콤함",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037776.1.17",
          "form": "개운하게 나만의 시간ㅋㅋㅋ",
          "original_form": "개운하게 나만의 시간ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037776.1.18",
          "form": "이제 숙제 끝냈어요?",
          "original_form": "이제 숙제 끝냈어요?",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037776.1.19",
          "form": "어ㅋㅋ",
          "original_form": "어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037776.1.20",
          "form": "다끝ㅋ",
          "original_form": "다끝ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037776.1.21",
          "form": "오늘 시원하네ㅋㅋ",
          "original_form": "오늘 시원하네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037776.1.22",
          "form": "그럼 이제 얼른 재워요ㅋㅋ",
          "original_form": "그럼 이제 얼른 재워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037776.1.23",
          "form": "나도 창문 열어 놓고 있어요",
          "original_form": "나도 창문 열어 놓고 있어요",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037776.1.24",
          "form": "자러갔다ㅋ",
          "original_form": "자러갔다ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037776.1.25",
          "form": "이월 언니 멋지네요",
          "original_form": "이월 언니 멋지네요",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037776.1.26",
          "form": "자러 가라 하면 둘이 자요?",
          "original_form": "자러 가라 하면 둘이 자요?",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037776.1.27",
          "form": "스스로해야재ㅋ 초딩들인데ㅋ",
          "original_form": "스스로해야재ㅋ 초딩들인데ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037776.1.28",
          "form": "어ㅋㅋ",
          "original_form": "어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037776.1.29",
          "form": "그치 안잔다고 해도 해줄 언니가 아니지 뭐ㅋㅋㅋ",
          "original_form": "그치 안잔다고 해도 해줄 언니가 아니지 뭐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037776.1.30",
          "form": "재워줄순 없잖아ㅋㅋㅋㅋ",
          "original_form": "재워줄순 없잖아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037776.1.31",
          "form": "자장가 불러줘야죠",
          "original_form": "자장가 불러줘야죠",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037776.1.32",
          "form": "알아서자야재ㅋㅋ",
          "original_form": "알아서자야재ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037776.1.33",
