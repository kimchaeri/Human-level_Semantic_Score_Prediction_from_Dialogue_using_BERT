        {
          "id": "MDRW2100045946.1.1",
          "form": "식사 하셧나뇨",
          "original_form": "식사 하셧나뇨",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045946.1.2",
          "form": "네 먹었어요 name1님은요?",
          "original_form": "네 먹었어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045946.1.3",
          "form": "저는 아직이요",
          "original_form": "저는 아직이요",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045946.1.4",
          "form": "어떠넉 드셧어용",
          "original_form": "어떠넉 드셧어용",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045946.1.5",
          "form": "집밥이요 ㅎㅎ 평소 아침 챙겨 드세요?",
          "original_form": "집밥이요 ㅎㅎ 평소 아침 챙겨 드세요?",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045946.1.6",
          "form": "아뇨 ㅠㅠ 점심 먹을려구용",
          "original_form": "아뇨 ㅠㅠ 점심 먹을려구용",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045946.1.7",
          "form": "반찬 맛있나용??",
          "original_form": "반찬 맛있나용??",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045946.1.8",
          "form": "그냥 먹을만 했어요 ㅜㅋㅋ",
          "original_form": "그냥 먹을만 했어요 ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045946.1.9",
          "form": "점심 맛난 거 먹으려구요",
          "original_form": "점심 맛난 거 먹으려구요",
          "speaker_id": "2",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100045946.1.10",
          "form": "배달 자주 시켜드세요?",
          "original_form": "배달 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100045946.1.11",
          "form": "배달 잘 안 시켜요 포장 주로해요",
          "original_form": "배달 잘 안 시켜요 포장 주로해요",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045946.1.12",
          "form": "name1님은 어떠세요",
          "original_form": "&name1&님은 어떠세요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045946.1.13",
          "form": "오 저는 귀찮아서 배달 잘 먹어요 ㅠㅠ",
          "original_form": "오 저는 귀찮아서 배달 잘 먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045946.1.14",
          "form": "어떤거 좋으흐새요",
          "original_form": "어떤거 좋으흐새요",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045946.1.15",
          "form": "맛있으면 다 좋아요 요즘은 초밥먹고 싶네요",
