      "utterance": [
        {
          "id": "MDRW2100029008.1.1",
          "form": "곰표 맥주는 편의점에서 많이 팔더라",
          "original_form": "곰표 맥주는 편의점에서 많이 팔더라",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029008.1.2",
          "form": "아 그거 맛있음",
          "original_form": "아 그거 맛있음",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029008.1.3",
          "form": "요전에 쥬시후레쉬 맥주도 나온거 봤는데",
          "original_form": "요전에 쥬시후레쉬 맥주도 나온거 봤는데",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029008.1.4",
          "form": "대체 곰표와 무슨 연관인진 모르겠지만...",
          "original_form": "대체 곰표와 무슨 연관인진 모르겠지만...",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029008.1.5",
          "form": "맥주로???",
          "original_form": "맥주로???",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029008.1.6",
          "form": "응 맥주로",
          "original_form": "응 맥주로",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029008.1.7",
          "form": "아이스크림은 봤는데ㅋㅋㅋ",
          "original_form": "아이스크림은 봤는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029008.1.8",
          "form": "이젠 연관성도 찾지않고 콜라보 하는 듯",
          "original_form": "이젠 연관성도 찾지않고 콜라보 하는 듯",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029008.1.9",
          "form": "걍 옛날 브랜드기만 하면ㅋㅋㅋ",
          "original_form": "걍 옛날 브랜드기만 하면ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029008.1.10",
          "form": "참고로 후라보노 맥주도 있었다",
          "original_form": "참고로 후라보노 맥주도 있었다",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029008.1.11",
          "form": "세상에ㅋㅋㅋ 무슨 맛인지 상상도 안감",
          "original_form": "세상에ㅋㅋㅋ 무슨 맛인지 상상도 안감",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029008.1.12",
          "form": "금성이랑 무슨 과자도 콜라보 있던데",
          "original_form": "금성이랑 무슨 과자도 콜라보 있던데",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029008.1.13",
          "form": "와 끝이 없구나",
          "original_form": "와 끝이 없구나",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029008.1.14",
          "form": "워낙 맥락이 없으니까 뭐뭐 있었는지 잘 떠오르지도 않네",
          "original_form": "워낙 맥락이 없으니까 뭐뭐 있었는지 잘 떠오르지도 않네",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029008.1.15",
          "form": "볼때 신기하긴 한데",
          "original_form": "볼때 신기하긴 한데",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029008.1.16",
          "form": "맞아 이런거랑도 하내 라는 느낌",
          "original_form": "맞아 이런거랑도 하내 라는 느낌",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029008.1.17",
          "form": "맛이 보장되는 콜라보를 해주면 좋겠는데 말이지",
          "original_form": "맛이 보장되는 콜라보를 해주면 좋겠는데 말이지",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029008.1.18",
          "form": "파리바게트랑 팔도비빔면 콜라보는 좋았어",
          "original_form": "파리바게트랑 팔도비빔면 콜라보는 좋았어",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029008.1.19",
          "form": "아 그거 맛있었어?",
          "original_form": "아 그거 맛있었어?",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029008.1.20",
          "form": "응 약간 매콤한 고로케 맛이었어",
          "original_form": "응 약간 매콤한 고로케 맛이었어",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029008.1.21",
          "form": "베스킨라빈스도 맥심커피랑 콜라보하던데",
          "original_form": "베스킨라빈스도 맥심커피랑 콜라보하던데",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029008.1.22",
          "form": "그건 더위사냥 맛일것 같아 기대 안되더라",
          "original_form": "그건 더위사냥 맛일것 같아 기대 안되더라",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029008.1.23",
          "form": "아 나 그거 먹어봤어",
          "original_form": "아 나 그거 먹어봤어",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029008.1.24",
          "form": "되게 충실한 맥심커피 맛이야",
          "original_form": "되게 충실한 맥심커피 맛이야",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029008.1.25",
          "form": "더위사냥이랑 다름",
          "original_form": "더위사냥이랑 다름",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029008.1.26",
          "form": "오 달라? 근데  충실한 맥심 커피 맛도 별로 안땡기는데",
          "original_form": "오 달라? 근데  충실한 맥심 커피 맛도 별로 안땡기는데",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029008.1.27",
          "form": "맥심 커피 얼리면 되는거 아냐?",
          "original_form": "맥심 커피 얼리면 되는거 아냐?",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029008.1.28",
          "form": "사실 그렇긴 함ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 그렇긴 함ㅋㅋㅋㅋㅋㅋ",
