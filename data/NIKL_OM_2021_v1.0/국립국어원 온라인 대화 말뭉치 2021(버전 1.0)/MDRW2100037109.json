        },
        {
          "id": "MDRW2100037109.1.2",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "2",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100037109.1.3",
          "form": "네 name2님은 드셨나요?",
          "original_form": "네 &name2&님은 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100037109.1.4",
          "form": "네 어떤거 드셨어요?",
          "original_form": "네 어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 19:58"
        },
        {
          "id": "MDRW2100037109.1.5",
          "form": "저는 고기 먹었어요",
          "original_form": "저는 고기 먹었어요",
          "speaker_id": "1",
          "time": "20210831 19:58"
        },
        {
          "id": "MDRW2100037109.1.6",
          "form": "name2님은 뭐 드셨나요?",
          "original_form": "&name2&님은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:59"
        },
        {
          "id": "MDRW2100037109.1.7",
          "form": "저는 소고기무국이랑 오징어볶음이요 ㅎㅎ",
          "original_form": "저는 소고기무국이랑 오징어볶음이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100037109.1.8",
          "form": "맛있었겠네요",
          "original_form": "맛있었겠네요",
          "speaker_id": "1",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100037109.1.9",
          "form": "직접 요리하신건가요?",
          "original_form": "직접 요리하신건가요?",
          "speaker_id": "1",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100037109.1.10",
          "form": "넵..ㅋㅋ 좌충우돌 요리초보입니다",
          "original_form": "넵..ㅋㅋ 좌충우돌 요리초보입니다",
          "speaker_id": "2",
          "time": "20210831 20:01"
        },
        {
          "id": "MDRW2100037109.1.11",
          "form": "저도 똥손이예요",
          "original_form": "저도 똥손이예요",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100037109.1.12",
          "form": "요리 잘하고싶어요ㅜ",
          "original_form": "요리 잘하고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100037109.1.13",
          "form": "저도 ㄸ손이예요..ㅋㅋ 애들 먹이려고 어찌어찌 생존요리를..",
          "original_form": "저도 ㄸ손이예요..ㅋㅋ 애들 먹이려고 어찌어찌 생존요리를..",
          "speaker_id": "2",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100037109.1.14",
          "form": "그래도 여러가지 하시는게 대단하세요",
          "original_form": "그래도 여러가지 하시는게 대단하세요",
          "speaker_id": "1",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100037109.1.15",
          "form": "만개의레시피 어플 아세요? 도움 많이 받아용",
          "original_form": "만개의레시피 어플 아세요? 도움 많이 받아용",
          "speaker_id": "2",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100037109.1.16",
          "form": "아 본거같아요",
          "original_form": "아 본거같아요",
          "speaker_id": "1",
          "time": "20210831 20:06"
        },
        {
