      "utterance": [
        {
          "id": "MDRW2100040460.1.1",
          "form": "ㅎㅎ전국민이 다 보는 프로그램이었죠",
          "original_form": "ㅎㅎ전국민이 다 보는 프로그램이었죠",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040460.1.2",
          "form": "추억이 새록새록 떠오르네요",
          "original_form": "추억이 새록새록 떠오르네요",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040460.1.3",
          "form": "맞아요 ㅋㅋ 그리구 지오디는 뭔가",
          "original_form": "맞아요 ㅋㅋ 그리구 지오디는 뭔가",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040460.1.4",
          "form": "집도 가정집같고 막 엄청 잘사는것같지않아서 위화감도 없었구 ㅋㅋㅋㅋㅋ",
          "original_form": "집도 가정집같고 막 엄청 잘사는것같지않아서 위화감도 없었구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040460.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100040460.1.6",
          "form": "서민그룹 느낌ㅎㅎ",
          "original_form": "서민그룹 느낌ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100040460.1.7",
          "form": "다들 성격도 무난하고 ㅋㅋㅋ",
          "original_form": "다들 성격도 무난하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100040460.1.8",
          "form": "막 잘생기기보단",
          "original_form": "막 잘생기기보단",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100040460.1.9",
          "form": "마자요 ㅋㅋ그래서진짜 옆집오빠들이 애보는 느낌이라 더 재밌었던것같아요 ㅋㅋㅋ친근",
          "original_form": "마자요 ㅋㅋ그래서진짜 옆집오빠들이 애보는 느낌이라 더 재밌었던것같아요 ㅋㅋㅋ친근",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100040460.1.10",
          "form": "동네 건실한 청년들 같았죠",
          "original_form": "동네 건실한 청년들 같았죠",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100040460.1.11",
          "form": "재민이도 이젠 성인이겠네요",
          "original_form": "재민이도 이젠 성인이겠네요",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100040460.1.12",
          "form": "와아아 그러게요 재민이가 2001년생인가 2002년생이었나 그랬던것같은데 ㅋㅋ",
          "original_form": "와아아 그러게요 재민이가 2001년생인가 2002년생이었나 그랬던것같은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100040460.1.13",
          "form": "대학갔겠어요ㅋㅋㅋ",
          "original_form": "대학갔겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100040460.1.14",
          "form": "ㅎㅎ티비에 나온 아가들이 성인된 것 보면",
          "original_form": "ㅎㅎ티비에 나온 아가들이 성인된 것 보면",
          "speaker_id": "2",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100040460.1.15",
          "form": "진짜 놀라워요",
          "original_form": "진짜 놀라워요",
          "speaker_id": "2",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100040460.1.16",
          "form": "맞아요 약간 내가 나이먹는거는 잘안느껴지는데",
          "original_form": "맞아요 약간 내가 나이먹는거는 잘안느껴지는데",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040460.1.17",
          "form": "막 그런거있져",
          "original_form": "막 그런거있져",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040460.1.18",
          "form": "아이유가 28살이래~ 하면",
          "original_form": "아이유가 28살이래~ 하면",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040460.1.19",
          "form": "세상에나......",
          "original_form": "세상에나......",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040460.1.20",
          "form": "세월이...",
          "original_form": "세월이...",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040460.1.21",
          "form": "ㅋㅋㅋㅋ이러는거",
          "original_form": "ㅋㅋㅋㅋ이러는거",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040460.1.22",
          "form": "헐 말하고보니 29인가요 올해는? ㅋㅋㅋ",
          "original_form": "헐 말하고보니 29인가요 올해는? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100040460.1.23",
          "form": "ㅎㅎㅎ맞아요",
          "original_form": "ㅎㅎㅎ맞아요",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100040460.1.24",
          "form": "아역들이 성인된 것 보면 놀라고",
          "original_form": "아역들이 성인된 것 보면 놀라고",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100040460.1.25",
          "form": "맞아요... 여진구오빠라던지.........",
          "original_form": "맞아요... 여진구오빠라던지.........",
