      "utterance": [
        {
          "id": "MDRW2100041154.1.1",
          "form": "컵라면 좋아하시는 거 있어요?",
          "original_form": "컵라면 좋아하시는 거 있어요?",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041154.1.2",
          "form": "컵라면은 무조건 육개장이요!",
          "original_form": "컵라면은 무조건 육개장이요!",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041154.1.3",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100041154.1.4",
          "form": "저는 튀김우동 좋아해요",
          "original_form": "저는 튀김우동 좋아해요",
          "speaker_id": "2",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100041154.1.5",
          "form": "저도하얀국물로는 튀김우동!",
          "original_form": "저도하얀국물로는 튀김우동!",
          "speaker_id": "1",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100041154.1.6",
          "form": "그러시군요 튀김이 맛있어요",
          "original_form": "그러시군요 튀김이 맛있어요",
          "speaker_id": "2",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100041154.1.7",
          "form": "맞아요 더많이넣어줬으면,,",
          "original_form": "맞아요 더많이넣어줬으면,,",
          "speaker_id": "1",
          "time": "20210905 18:39"
        },
        {
          "id": "MDRW2100041154.1.8",
          "form": "튀김 두 배 상품 같은 거 나오면 좋겠어요",
          "original_form": "튀김 두 배 상품 같은 거 나오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 18:39"
        },
        {
          "id": "MDRW2100041154.1.9",
          "form": "오동통면 다시마2개들어있듯이ㅋㅋ",
          "original_form": "오동통면 다시마2개들어있듯이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:39"
        },
        {
          "id": "MDRW2100041154.1.10",
          "form": "네 ㅋㅋㅋ그거죠 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ그거죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:39"
        },
        {
          "id": "MDRW2100041154.1.11",
          "form": "오동통면처럼요",
          "original_form": "오동통면처럼요",
          "speaker_id": "2",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100041154.1.12",
          "form": "라면 자주드세요?",
          "original_form": "라면 자주드세요?",
          "speaker_id": "1",
          "time": "20210905 18:45"
        },
        {
          "id": "MDRW2100041154.1.13",
          "form": "요즘엔 잘 안먹어요",
          "original_form": "요즘엔 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100041154.1.14",
          "form": "봉투라면보다 컵라면 좋아하세요?!",
          "original_form": "봉투라면보다 컵라면 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041154.1.15",
          "form": "봉투라면이 더 좋아요 ㅎㅎ",
          "original_form": "봉투라면이 더 좋아요 ㅎㅎ",
