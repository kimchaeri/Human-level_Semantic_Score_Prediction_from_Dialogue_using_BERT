      "utterance": [
        {
          "id": "MDRW2100023416.1.1",
          "form": "그것도 꼭 먹어볼게용!ㅎㅎㅎ",
          "original_form": "그것도 꼭 먹어볼게용!ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023416.1.2",
          "form": "반려동물 키워보신적 있으세요?",
          "original_form": "반려동물 키워보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023416.1.3",
          "form": "한번 있는데 끝이 안 좋았어요..ㅠㅠ 님은요?",
          "original_form": "한번 있는데 끝이 안 좋았어요..ㅠㅠ 님은요?",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023416.1.4",
          "form": "강아지? 키우시나요?",
          "original_form": "강아지? 키우시나요?",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.5",
          "form": "아아ㅠㅜㅠ",
          "original_form": "아아ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.6",
          "form": "저는 동물을 너무 좋아하는데",
          "original_form": "저는 동물을 너무 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.7",
          "form": "그중에서도 오리를 좋아해서",
          "original_form": "그중에서도 오리를 좋아해서",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.8",
          "form": "오리 키우고싶어요!",
          "original_form": "오리 키우고싶어요!",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.9",
          "form": "오옹 집에서 오리를 키울수가 있나요?",
          "original_form": "오옹 집에서 오리를 키울수가 있나요?",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.10",
          "form": "네넹ㅎㅎ",
          "original_form": "네넹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.11",
          "form": "@.@ 처음 들었어요",
          "original_form": "@.@ 처음 들었어요",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.12",
          "form": "오리도 종이 어려가지인데",
          "original_form": "오리도 종이 어려가지인데",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.13",
          "form": "오리키우고 싶다는 분은 처음이라 신선하네요",
          "original_form": "오리키우고 싶다는 분은 처음이라 신선하네요",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.14",
          "form": "집오리나 콜덕같은 종은 집에서도",
          "original_form": "집오리나 콜덕같은 종은 집에서도",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.15",
          "form": "키울수있어용!",
          "original_form": "키울수있어용!",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.16",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023416.1.17",
          "form": "너무 키우고싶은데",
          "original_form": "너무 키우고싶은데",
          "speaker_id": "1",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023416.1.18",
          "form": "오리의 어떤점에 끌리신거에요?",
          "original_form": "오리의 어떤점에 끌리신거에요?",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023416.1.19",
          "form": "가족중에 비염이 심한사람이 있어서ㅠㅠㅋㅋㅋ",
          "original_form": "가족중에 비염이 심한사람이 있어서ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023416.1.20",
          "form": "부리가 너무 귀여워요",
          "original_form": "부리가 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023416.1.21",
          "form": "아하 ㅠㅠ 아이구",
          "original_form": "아하 ㅠㅠ 아이구",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023416.1.22",
          "form": "이빨이 없어서 공격이 하찮은것도 귀엽구",
          "original_form": "이빨이 없어서 공격이 하찮은것도 귀엽구",
          "speaker_id": "1",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023416.1.23",
          "form": "특유의 바보같은 표정이 너무 귀여워욬ㅋㅋㅋ",
          "original_form": "특유의 바보같은 표정이 너무 귀여워욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:52"
        }
      ]
