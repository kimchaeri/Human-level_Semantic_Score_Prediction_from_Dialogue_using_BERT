      "utterance": [
        {
          "id": "MDRW2100001684.1.1",
          "form": "조성진 리사이틀 공연한다는 거 봤나",
          "original_form": "조성진 리사이틀 공연한다는 거 봤나",
          "speaker_id": "2",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100001684.1.2",
          "form": "엥 어디서",
          "original_form": "엥 어디서",
          "speaker_id": "1",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100001684.1.3",
          "form": "대구??",
          "original_form": "대구??",
          "speaker_id": "1",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100001684.1.4",
          "form": "대구!!",
          "original_form": "대구!!",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001684.1.5",
          "form": "최근에 티켓팅있었던데 어젠가..?",
          "original_form": "최근에 티켓팅있었던데 어젠가..?",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001684.1.6",
          "form": "헐 경쟁률 쎄겠다",
          "original_form": "헐 경쟁률 쎄겠다",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001684.1.7",
          "form": "조성진이 대구까지오네,,,",
          "original_form": "조성진이 대구까지오네,,,",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001684.1.8",
          "form": "근데 예상 티켓팅 매진시간?? 30초였는데",
          "original_form": "근데 예상 티켓팅 매진시간?? 30초였는데",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001684.1.9",
          "form": "더 힘들었나봐",
          "original_form": "더 힘들었나봐",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001684.1.10",
          "form": "어제 커뮤니티 보니까 못잡은 사람 수두룩",
          "original_form": "어제 커뮤니티 보니까 못잡은 사람 수두룩",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001684.1.11",
          "form": "엥 대박...진짜 사람들 문화생활 열심히하네",
          "original_form": "엥 대박...진짜 사람들 문화생활 열심히하네",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001684.1.12",
          "form": "왜 나는 안알려줬어",
          "original_form": "왜 나는 안알려줬어",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.13",
          "form": "근데 조성진은 음악 문외한한테도 알려져있잖아",
          "original_form": "근데 조성진은 음악 문외한한테도 알려져있잖아",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.14",
          "form": "오잉 당연 아는줄",
          "original_form": "오잉 당연 아는줄",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.15",
          "form": "알았으면 갔나?",
          "original_form": "알았으면 갔나?",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.16",
          "form": "아니 돈없지.",
          "original_form": "아니 돈없지.",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.17",
          "form": "콘서트하우스에서 하는가?",
          "original_form": "콘서트하우스에서 하는가?",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 얼만지 궁금하다 티켓",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 얼만지 궁금하다 티켓",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.19",
          "form": "그런거까지는..모르는데",
          "original_form": "그런거까지는..모르는데",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.20",
          "form": "어셔 출장 가보고싶다",
          "original_form": "어셔 출장 가보고싶다",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001684.1.21",
          "form": "어셔 출장도 가나?",
          "original_form": "어셔 출장도 가나?",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001684.1.22",
          "form": "아니 안가지 ㅎ",
          "original_form": "아니 안가지 ㅎ",
          "speaker_id": "1",
          "time": "20210819 18:16"
        }
