      "utterance": [
        {
          "id": "MDRW2100004726.1.1",
          "form": "혹시 반려동물 키우시나요?",
          "original_form": "혹시 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004726.1.2",
          "form": "네 고양이키워요",
          "original_form": "네 고양이키워요",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004726.1.3",
          "form": "스키틀즈님은요?",
          "original_form": "스키틀즈님은요?",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004726.1.4",
          "form": "저는 그냥 사진이나 영상으로만 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 그냥 사진이나 영상으로만 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100004726.1.5",
          "form": "어릴때만 강아지 키워봤어요",
          "original_form": "어릴때만 강아지 키워봤어요",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100004726.1.6",
          "form": "ㅋㅋㅋ그쳐 영상으로 저도 랜선으로키우는애들많아여",
          "original_form": "ㅋㅋㅋ그쳐 영상으로 저도 랜선으로키우는애들많아여",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100004726.1.7",
          "form": "영상으로 좋아하는 동물 어떤동물인가여",
          "original_form": "영상으로 좋아하는 동물 어떤동물인가여",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100004726.1.8",
          "form": "강아지와 고양이 유튜브 구독중입니다 ㅎㅎ",
          "original_form": "강아지와 고양이 유튜브 구독중입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100004726.1.9",
          "form": "주는거없이 좋은거같아요",
          "original_form": "주는거없이 좋은거같아요",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100004726.1.10",
          "form": "맞아요 저도 고양이 유투버들구독중인데",
          "original_form": "맞아요 저도 고양이 유투버들구독중인데",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100004726.1.11",
          "form": "하하하네 보고있어요 ㅋㅋㅋ",
          "original_form": "하하하네 보고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100004726.1.12",
          "form": "저도 하하하네 구독중입니다 ㅋㅋㅋㅋ",
          "original_form": "저도 하하하네 구독중입니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100004726.1.13",
          "form": "통통한 고양이들 너무 귀여워요 ㅜㅠ",
          "original_form": "통통한 고양이들 너무 귀여워요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100004726.1.14",
          "form": "털찐 ㅋㅋㅋ",
          "original_form": "털찐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100004726.1.15",
          "form": "ㅋㅋㅋㅋㅋ맞아요 너무귀여워요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 너무귀여워요",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100004726.1.16",
          "form": "하삼 냥이들도 어찌그리 귀여운지",
          "original_form": "하삼 냥이들도 어찌그리 귀여운지",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100004726.1.17",
          "form": "진짜 순둥하고 ㅋㅋㅋㅋ",
          "original_form": "진짜 순둥하고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100004726.1.18",
          "form": "데려오고싶어요......",
          "original_form": "데려오고싶어요......",
          "speaker_id": "1",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100004726.1.19",
          "form": "ㅋㅋㅋ맞아여 순둥순둥이들",
          "original_form": "ㅋㅋㅋ맞아여 순둥순둥이들",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100004726.1.20",
          "form": "나중에 혹시 반려동물키우실생각 있으신가여",
          "original_form": "나중에 혹시 반려동물키우실생각 있으신가여",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100004726.1.21",
          "form": "뭔가 혼자 살게되면 키워보고싶긴해요",
          "original_form": "뭔가 혼자 살게되면 키워보고싶긴해요",
          "speaker_id": "1",
          "time": "20210823 18:54"
        },
        {
          "id": "MDRW2100004726.1.22",
          "form": "사실 지금도 늘 키우고 싶은 마음은 있긴하지만....",
          "original_form": "사실 지금도 늘 키우고 싶은 마음은 있긴하지만....",
          "speaker_id": "1",
          "time": "20210823 18:54"
        },
        {
          "id": "MDRW2100004726.1.23",
          "form": "책임이 따르는지라 ㅜㅜ 늘 집에 있어줄수도 없구요",
          "original_form": "책임이 따르는지라 ㅜㅜ 늘 집에 있어줄수도 없구요",
          "speaker_id": "1",
          "time": "20210823 18:54"
        },
        {
          "id": "MDRW2100004726.1.24",
          "form": "맞아요ㅜ책임이 뒤따르니까ㅜ",
          "original_form": "맞아요ㅜ책임이 뒤따르니까ㅜ",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100004726.1.25",
          "form": "얘들은 저없으면 살아갈수가없다는게 항상 책임감을 느끼고있어요",
          "original_form": "얘들은 저없으면 살아갈수가없다는게 항상 책임감을 느끼고있어요",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100004726.1.26",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100004726.1.27",
          "form": "사실 가족인거잖아요 반려동물도",
