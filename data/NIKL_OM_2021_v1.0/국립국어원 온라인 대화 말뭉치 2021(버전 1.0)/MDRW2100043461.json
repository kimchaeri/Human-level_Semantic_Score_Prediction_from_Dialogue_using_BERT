      "utterance": [
        {
          "id": "MDRW2100043461.1.1",
          "form": "무슨음악을",
          "original_form": "무슨음악을",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043461.1.2",
          "form": "좋아하시나용",
          "original_form": "좋아하시나용",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043461.1.3",
          "form": "저는 거의 신나는 아이돌노래요! name1님은요?",
          "original_form": "저는 거의 신나는 아이돌노래요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043461.1.4",
          "form": "저는 알앤비 좋아해요!",
          "original_form": "저는 알앤비 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043461.1.5",
          "form": "오 제일 좋아하는 노래는요?",
          "original_form": "오 제일 좋아하는 노래는요?",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043461.1.6",
          "form": "보트좋아해요!",
          "original_form": "보트좋아해요!",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043461.1.7",
          "form": "어? 처음들어요 ㅋㅋㅋ 들어봐야겠어요",
          "original_form": "어? 처음들어요 ㅋㅋㅋ 들어봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043461.1.8",
          "form": "들어보세영!!좋아여!",
          "original_form": "들어보세영!!좋아여!",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043461.1.9",
          "form": "오 추천 감사합니다 또 있나요",
          "original_form": "오 추천 감사합니다 또 있나요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043461.1.10",
          "form": "어음... 레이니데이?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어음... 레이니데이?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043461.1.11",
          "form": "제기준좋은거라",
          "original_form": "제기준좋은거라",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043461.1.12",
          "form": "잘머르겠네용",
          "original_form": "잘머르겠네용",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043461.1.13",
          "form": "추천해주신 거 다 들어볼게요 ㅋㅋㅋ 저는 벅차는 노래도 좋아해요",
          "original_form": "추천해주신 거 다 들어볼게요 ㅋㅋㅋ 저는 벅차는 노래도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100043461.1.14",
          "form": "완전 신나는 노랴말씀하시는건가용??",
          "original_form": "완전 신나는 노랴말씀하시는건가용??",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100043461.1.15",
          "form": "추천해주세여!",
          "original_form": "추천해주세여!",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100043461.1.16",
          "form": "주로..아이돌노래라 우주소녀나 여자친구 러블리즈 이런거 들어요 ㅋㅋㅋ",
          "original_form": "주로..아이돌노래라 우주소녀나 여자친구 러블리즈 이런거 들어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100043461.1.17",
          "form": "오 저는 오마이걸좋아해요",
          "original_form": "오 저는 오마이걸좋아해요",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100043461.1.18",
          "form": "어 저도요 ㅋㅋ",
