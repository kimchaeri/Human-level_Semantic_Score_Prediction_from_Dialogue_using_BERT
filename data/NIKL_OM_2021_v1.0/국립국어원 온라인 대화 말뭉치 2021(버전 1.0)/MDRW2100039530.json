      "utterance": [
        {
          "id": "MDRW2100039530.1.1",
          "form": "동물 키우면 ..",
          "original_form": "동물 키우면 ..",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039530.1.2",
          "form": "짓고싶은 이름 있나요",
          "original_form": "짓고싶은 이름 있나요",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039530.1.3",
          "form": "글쎄요ㅋㅋ 네이밍센스가 안좋아서...",
          "original_form": "글쎄요ㅋㅋ 네이밍센스가 안좋아서...",
          "speaker_id": "1",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039530.1.4",
          "form": "동물털색으로 간단하게 짓지 않을까요?",
          "original_form": "동물털색으로 간단하게 짓지 않을까요?",
          "speaker_id": "1",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039530.1.5",
          "form": "ㅋㅋㅋㅋ아 초코.. 이런거요?",
          "original_form": "ㅋㅋㅋㅋ아 초코.. 이런거요?",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039530.1.6",
          "form": "아마두요?ㅋㅋㅋ",
          "original_form": "아마두요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039530.1.7",
          "form": "음식이름으로 짓는 사람도 많더라구요",
          "original_form": "음식이름으로 짓는 사람도 많더라구요",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039530.1.8",
          "form": "맞아여 ㅋㅋ 특이한 이름",
          "original_form": "맞아여 ㅋㅋ 특이한 이름",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039530.1.9",
          "form": "되게 많더라구요",
          "original_form": "되게 많더라구요",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039530.1.10",
          "form": "이름이 인절미인 강아지도 봤어요ㅋㅋㅋㅋ",
          "original_form": "이름이 인절미인 강아지도 봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100039530.1.11",
          "form": "ㅠㅠㅠㅠ 아 그 강아지",
          "original_form": "ㅠㅠㅠㅠ 아 그 강아지",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039530.1.12",
          "form": "완전 귀여워요",
          "original_form": "완전 귀여워요",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039530.1.13",
          "form": "완전 인기스타에요ㅋㅋㅋ",
          "original_form": "완전 인기스타에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039530.1.14",
          "form": "쓰담아보고싶어요",
          "original_form": "쓰담아보고싶어요",
          "speaker_id": "1",
          "time": "20210903 18:58"
        },
        {
          "id": "MDRW2100039530.1.15",
          "form": "저두요...",
          "original_form": "저두요...",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039530.1.16",
          "form": "웃을 때 완전 힐링 ㅠ",
          "original_form": "웃을 때 완전 힐링 ㅠ",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039530.1.17",
          "form": "디폴트가 웃는표정 같긴해요ㅋㅋ",
          "original_form": "디폴트가 웃는표정 같긴해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039530.1.18",
          "form": "활짝 웃는거 같아서 귀여워요",
          "original_form": "활짝 웃는거 같아서 귀여워요",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039530.1.19",
