      "utterance": [
        {
          "id": "MDRW2100031433.1.1",
          "form": "갤럭시 폴더폰ㅎㅁㅎ",
          "original_form": "갤럭시 폴더폰ㅎㅁㅎ",
          "speaker_id": "1",
