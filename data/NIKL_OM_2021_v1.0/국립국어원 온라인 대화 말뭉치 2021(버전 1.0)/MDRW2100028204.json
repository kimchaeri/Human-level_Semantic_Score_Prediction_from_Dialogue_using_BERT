      "utterance": [
        {
          "id": "MDRW2100028204.1.1",
          "form": "알아요 빨래를 하겠어요~",
          "original_form": "알아요 빨래를 하겠어요~",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028204.1.2",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028204.1.3",
          "form": "같이 가자고 했던 분이 있었는데 소식이 없어요",
          "original_form": "같이 가자고 했던 분이 있었는데 소식이 없어요",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028204.1.4",
          "form": "그거 소극장 공연인데 좋아해요",
          "original_form": "그거 소극장 공연인데 좋아해요",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028204.1.5",
          "form": "이번엔 같이 보자고 하겠죠",
          "original_form": "이번엔 같이 보자고 하겠죠",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.6",
          "form": "그렇군요 차이셨나보네요",
          "original_form": "그렇군요 차이셨나보네요",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.7",
          "form": "소극장이면 생생하겠네요",
          "original_form": "소극장이면 생생하겠네요",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.8",
          "form": "그냥 혼자 보러 가세요.",
          "original_form": "그냥 혼자 보러 가세요.",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.9",
          "form": "연극도 봐보고싶어요",
          "original_form": "연극도 봐보고싶어요",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.10",
          "form": "소극장에서 보는 연극이 진리죠",
          "original_form": "소극장에서 보는 연극이 진리죠",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.11",
          "form": "뮤지컬도 좋지만요",
          "original_form": "뮤지컬도 좋지만요",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.12",
          "form": "연극을 안 봤어요? 이런 원시인",
          "original_form": "연극을 안 봤어요? 이런 원시인",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.13",
          "form": "그림은 보러 가시나요",
          "original_form": "그림은 보러 가시나요",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.14",
          "form": "뮤지컬도 나쁘지않죠",
          "original_form": "뮤지컬도 나쁘지않죠",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028204.1.15",
          "form": "조승우 좋아래요",
          "original_form": "조승우 좋아래요",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028204.1.16",
          "form": "그림은 피카소전 본 적 있어요 대학생때",
          "original_form": "그림은 피카소전 본 적 있어요 대학생때",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028204.1.17",
          "form": "오 그러세요",
          "original_form": "오 그러세요",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028204.1.18",
          "form": "조지킬이죠",
          "original_form": "조지킬이죠",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028204.1.19",
          "form": "저도 고흐요",
          "original_form": "저도 고흐요",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028204.1.20",
          "form": "이번에 이문세 콘 있어요",
          "original_form": "이번에 이문세 콘 있어요",
          "speaker_id": "1",
