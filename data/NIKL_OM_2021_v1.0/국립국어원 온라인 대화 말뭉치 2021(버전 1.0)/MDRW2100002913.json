      "utterance": [
        {
          "id": "MDRW2100002913.1.1",
          "form": "주단태가 죽을수도 있을것 같아요",
          "original_form": "주단태가 죽을수도 있을것 같아요",
          "speaker_id": "1",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002913.1.2",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002913.1.3",
          "form": "제일 나쁜놈을 보내는군요",
          "original_form": "제일 나쁜놈을 보내는군요",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002913.1.4",
          "form": "그냥 제 추측이요 ㅎㅎ",
          "original_form": "그냥 제 추측이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002913.1.5",
          "form": "네ㅋㅋ 워낙 예상이 안대는 드라마긴하져",
          "original_form": "네ㅋㅋ 워낙 예상이 안대는 드라마긴하져",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002913.1.6",
          "form": "네 ㅎㅎ 반전이 많아요",
          "original_form": "네 ㅎㅎ 반전이 많아요",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002913.1.7",
          "form": "네 그래서 제가 보다가 끊었어요ㅋㅋ",
          "original_form": "네 그래서 제가 보다가 끊었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002913.1.8",
          "form": "ㅋㅋㅋ 저는 결말이 궁금해서 계속 보고 있어요",
          "original_form": "ㅋㅋㅋ 저는 결말이 궁금해서 계속 보고 있어요",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002913.1.9",
          "form": "두근거려서 볼수가 없더라고요",
          "original_form": "두근거려서 볼수가 없더라고요",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002913.1.10",
          "form": "그리고 네이버 기사로 줄거리만 보져ㅋㅋ",
          "original_form": "그리고 네이버 기사로 줄거리만 보져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002913.1.11",
          "form": "저는 시즌 1에 1화 보고 심장내려 앉는줄 알았어요",
          "original_form": "저는 시즌 1에 1화 보고 심장내려 앉는줄 알았어요",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002913.1.12",
          "form": "시작부터 너무 자극적이긴 했어요",
          "original_form": "시작부터 너무 자극적이긴 했어요",
          "speaker_id": "2",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002913.1.13",
          "form": "혹시 보셨었어요?민설아 죽는 장면요",
          "original_form": "혹시 보셨었어요?민설아 죽는 장면요",
          "speaker_id": "1",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002913.1.14",
          "form": "넘잔인해요 진짜",
          "original_form": "넘잔인해요 진짜",
          "speaker_id": "2",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002913.1.15",
          "form": "네~~너무 적나라하게 다 보여주더라고요",
          "original_form": "네~~너무 적나라하게 다 보여주더라고요",
