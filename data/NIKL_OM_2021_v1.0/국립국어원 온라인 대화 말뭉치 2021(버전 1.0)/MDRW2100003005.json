      "utterance": [
        {
          "id": "MDRW2100003005.1.1",
          "form": "저는 포카리좋아해요",
          "original_form": "저는 포카리좋아해요",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003005.1.2",
          "form": "운동마니아다우세요 ㅎㅎ",
          "original_form": "운동마니아다우세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003005.1.3",
          "form": "저는 커피요 ㅎㅎ",
          "original_form": "저는 커피요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003005.1.4",
          "form": "배달음식 자주 시키시나요?",
          "original_form": "배달음식 자주 시키시나요?",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003005.1.5",
          "form": "배달음식은 잘 안먹고",
          "original_form": "배달음식은 잘 안먹고",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003005.1.6",
          "form": "주로 가서 먹는편이에요",
          "original_form": "주로 가서 먹는편이에요",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003005.1.7",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003005.1.8",
          "form": "점심은 구내식당에서 주는거 먹어요",
          "original_form": "점심은 구내식당에서 주는거 먹어요",
          "speaker_id": "1",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100003005.1.9",
          "form": "오늘은 불고기랑 쌈채소나와서 좋았네요",
          "original_form": "오늘은 불고기랑 쌈채소나와서 좋았네요",
          "speaker_id": "1",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100003005.1.10",
          "form": "좋은 회사네요!",
          "original_form": "좋은 회사네요!",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003005.1.11",
          "form": "저도 오늘 불고기 먹었는데",
          "original_form": "저도 오늘 불고기 먹었는데",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003005.1.12",
          "form": "설마 사우님?",
          "original_form": "설마 사우님?",
          "speaker_id": "2",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003005.1.13",
          "form": "한번 직원들한테 안좋은소리듣고는 엄청 신경쓰더라구요",
          "original_form": "한번 직원들한테 안좋은소리듣고는 엄청 신경쓰더라구요",
          "speaker_id": "1",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003005.1.14",
          "form": "설마요 ㅎㅎ",
          "original_form": "설마요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003005.1.15",
          "form": "음식이 맛잇어야해요",
          "original_form": "음식이 맛잇어야해요",
          "speaker_id": "2",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003005.1.16",
          "form": "다 먹고 살자고 하는건데",
          "original_form": "다 먹고 살자고 하는건데",
          "speaker_id": "2",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100003005.1.17",
          "form": "그렇죠 우렁쌈장 나왔는데 맛있더라구요\r\n호박잎이랑 먹으니 더 맛나더라구요",
          "original_form": "그렇죠 우렁쌈장 나왔는데 맛있더라구요\r\n호박잎이랑 먹으니 더 맛나더라구요",
          "speaker_id": "1",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100003005.1.18",
          "form": "호박잎 아주맛있는데",
          "original_form": "호박잎 아주맛있는데",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003005.1.19",
          "form": "강된장도 맛있고",
          "original_form": "강된장도 맛있고",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003005.1.20",
          "form": "미역줄기도 같이",
          "original_form": "미역줄기도 같이",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003005.1.21",
          "form": "원래 양식을 좋아해서 식습관 바꾸려고 노력 많이하고 있어요",
          "original_form": "원래 양식을 좋아해서 식습관 바꾸려고 노력 많이하고 있어요",
          "speaker_id": "2",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003005.1.22",
          "form": "저도 고기파라 ㅎㅎ 채소 안좋아하는데 요새 호박잎이 입에 맞고 있어요 ㅎ",
          "original_form": "저도 고기파라 ㅎㅎ 채소 안좋아하는데 요새 호박잎이 입에 맞고 있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003005.1.23",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003005.1.24",
          "form": "나이가 어떻게되세요?",
          "original_form": "나이가 어떻게되세요?",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003005.1.25",
          "form": "입맛이 완전 연륜있으신ㅋㅋ",
          "original_form": "입맛이 완전 연륜있으신ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003005.1.26",
          "form": "그래도 고기파인거는 완전삭제는 안되더라구요 ㅎ",
          "original_form": "그래도 고기파인거는 완전삭제는 안되더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003005.1.27",
          "form": "얼마전에 여행가서 고기말고로 사먹었더니 속은 편하던데 고기생각은 계속나더라구요",
          "original_form": "얼마전에 여행가서 고기말고로 사먹었더니 속은 편하던데 고기생각은 계속나더라구요",
          "speaker_id": "1",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003005.1.28",
          "form": "회먹고싶네요",
          "original_form": "회먹고싶네요",
          "speaker_id": "2",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003005.1.29",
          "form": "화에 쏘주한잔 캬",
          "original_form": "화에 쏘주한잔 캬",
          "speaker_id": "2",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003005.1.30",
          "form": "저는 날것을 못먹어서 ㅎㅎ \r\n회를 안먹어요 ㅎㅎ",
          "original_form": "저는 날것을 못먹어서 ㅎㅎ \r\n회를 안먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:18"
        }
      ]
    }
