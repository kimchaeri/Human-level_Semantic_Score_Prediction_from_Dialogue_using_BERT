      "utterance": [
        {
          "id": "MDRW2100023294.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100023294.1.2",
          "form": "아까도 말했듯이 걷기만요",
          "original_form": "아까도 말했듯이 걷기만요",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100023294.1.3",
          "form": "다이어트는하시나요",
          "original_form": "다이어트는하시나요",
          "speaker_id": "2",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100023294.1.4",
          "form": "다이어트 하신지 얼마나 되셨어요?",
          "original_form": "다이어트 하신지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100023294.1.5",
          "form": "저는 6개월된것같아요 얼마나되셨어요?",
          "original_form": "저는 6개월된것같아요 얼마나되셨어요?",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023294.1.6",
          "form": "저도 육개월정도요",
          "original_form": "저도 육개월정도요",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023294.1.7",
          "form": "효과는 못봤지만요ㅎ",
          "original_form": "효과는 못봤지만요ㅎ",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023294.1.8",
          "form": "홈트는 어느 채널 보세요?",
          "original_form": "홈트는 어느 채널 보세요?",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023294.1.9",
          "form": "저는 땅끄부부잘보는데 추천해줄건강홈트채널있나요?",
          "original_form": "저는 땅끄부부잘보는데 추천해줄건강홈트채널있나요?",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023294.1.10",
          "form": "알려주실수있나요?",
          "original_form": "알려주실수있나요?",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023294.1.11",
          "form": "저는 빅씨스님이 좋더라구요",
          "original_form": "저는 빅씨스님이 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023294.1.12",
          "form": "땅끄 저도 해봤어요 ㅎㅎ",
          "original_form": "땅끄 저도 해봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023294.1.13",
          "form": "내일은그거보고 홈트해볼게요",
          "original_form": "내일은그거보고 홈트해볼게요",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023294.1.14",
          "form": "반복되는 동작이 없어서 좋더라구요",
          "original_form": "반복되는 동작이 없어서 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023294.1.15",
          "form": "ㅇㅇ맛 팍팍한번하면땀이흘러요",
          "original_form": "ㅇㅇ맛 팍팍한번하면땀이흘러요",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023294.1.16",
          "form": "추천감사합니다 피부는 좋으세요?",
          "original_form": "추천감사합니다 피부는 좋으세요?",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023294.1.17",
          "form": "아뇨 피부 완전 안좋아요",
          "original_form": "아뇨 피부 완전 안좋아요",
          "speaker_id": "1",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023294.1.18",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023294.1.19",
          "form": "다이어트하면서 피부도 챙기게되더라구요",
          "original_form": "다이어트하면서 피부도 챙기게되더라구요",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023294.1.20",
          "form": "저는매일 1일1팩하는데",
          "original_form": "저는매일 1일1팩하는데",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023294.1.21",
          "form": "오 피부도 좋아지셨어요?",
          "original_form": "오 피부도 좋아지셨어요?",
          "speaker_id": "1",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023294.1.22",
