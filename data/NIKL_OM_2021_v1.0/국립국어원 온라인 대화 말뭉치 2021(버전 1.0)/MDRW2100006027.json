        },
        {
          "id": "MDRW2100006027.1.2",
          "form": "8명인가?? 백신완료자 포함해서요",
          "original_form": "8명인가?? 백신완료자 포함해서요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006027.1.3",
          "form": "완료자가 없으면요?",
          "original_form": "완료자가 없으면요?",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006027.1.4",
          "form": "그대로 4명일거에요!",
          "original_form": "그대로 4명일거에요!",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006027.1.5",
          "form": "우리집에서 저만 완료자니까 5인까지 겠군요",
          "original_form": "우리집에서 저만 완료자니까 5인까지 겠군요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100006027.1.6",
          "form": "그건 다 다른거같아요",
          "original_form": "그건 다 다른거같아요",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006027.1.7",
          "form": "제대로 안봐서ㅋㅋ",
          "original_form": "제대로 안봐서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006027.1.8",
          "form": "친구들이 추석때 안본다고ㅜㅋㅋ",
          "original_form": "친구들이 추석때 안본다고ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006027.1.9",
          "form": "아.. 전 너무 뭐가 많으니까 이제 뭐가 뭔지 모르겠어요 ㅋㅋ",
          "original_form": "아.. 전 너무 뭐가 많으니까 이제 뭐가 뭔지 모르겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006027.1.10",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006027.1.11",
          "form": "그냥 안만나는게 베스튼데 또 아쉽고",
          "original_form": "그냥 안만나는게 베스튼데 또 아쉽고",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006027.1.12",
          "form": "맞아요. 그때가 만날 기회인데요",
          "original_form": "맞아요. 그때가 만날 기회인데요",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006027.1.13",
          "form": "사실 그때가 서울은 텅빌거같아요",
          "original_form": "사실 그때가 서울은 텅빌거같아요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006027.1.14",
          "form": "그럼 그때 서울에 놀러가야 할까요? 전 지방민이라.. ㅋ",
          "original_form": "그럼 그때 서울에 놀러가야 할까요? 전 지방민이라.. ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006027.1.15",
          "form": "ㅋㅋ 서로 체인지ㅋㅋ",
          "original_form": "ㅋㅋ 서로 체인지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006027.1.16",
          "form": "서울 여의도같은덴 진짜 텅비었어요",
          "original_form": "서울 여의도같은덴 진짜 텅비었어요",
