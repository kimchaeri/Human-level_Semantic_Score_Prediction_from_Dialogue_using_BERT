      "utterance": [
        {
          "id": "MDRW2100008609.1.1",
          "form": "춘천도 오늘 비왔어?",
          "original_form": "춘천도 오늘 비왔어?",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008609.1.2",
          "form": "아니! 오늘은 안왔지!!",
          "original_form": "아니! 오늘은 안왔지!!",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008609.1.3",
          "form": "서울은 아침에 비 엄청 쏟더니 지금은 잠잠",
          "original_form": "서울은 아침에 비 엄청 쏟더니 지금은 잠잠",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008609.1.4",
          "form": "오 이제 태풍이 올라온다고 하더라ㅋㅋㅋ",
          "original_form": "오 이제 태풍이 올라온다고 하더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008609.1.5",
          "form": "그래서 그런지 바람도 많이불고 서늘해!!",
          "original_form": "그래서 그런지 바람도 많이불고 서늘해!!",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008609.1.6",
          "form": "아~~~태풍 귀찮",
          "original_form": "아~~~태풍 귀찮",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008609.1.7",
          "form": "서늘한데 뭔가 습함",
          "original_form": "서늘한데 뭔가 습함",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008609.1.8",
          "form": "그니까 ...아침 저녁으로 서늘한데 낮에는 또 엄청더움",
          "original_form": "그니까 ...아침 저녁으로 서늘한데 낮에는 또 엄청더움",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008609.1.9",
          "form": "이제 슬슬 가을옷 사야하는데",
          "original_form": "이제 슬슬 가을옷 사야하는데",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008609.1.10",
          "form": "텅장이죠~~",
          "original_form": "텅장이죠~~",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008609.1.11",
          "form": "ㅋㅋㅋ근데 이러다가 가을 순삭되고 급 추워져서 겨울됌^^ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ근데 이러다가 가을 순삭되고 급 추워져서 겨울됌^^ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008609.1.12",
          "form": "버티다가 겨울옷 가야하나",
          "original_form": "버티다가 겨울옷 가야하나",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008609.1.13",
          "form": "그래도 이제 마스크끼는건 편하겠다",
          "original_form": "그래도 이제 마스크끼는건 편하겠다",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008609.1.14",
          "form": "여름에 마스크 너무 힘들었어",
          "original_form": "여름에 마스크 너무 힘들었어",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008609.1.15",
          "form": "그니까 마스크쓰니까 그 더운게 더 심해졌어",
          "original_form": "그니까 마스크쓰니까 그 더운게 더 심해졌어",
          "speaker_id": "2",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008609.1.16",
          "form": "근데 추우면 일단 이불에서 나오기가 싫어..",
          "original_form": "근데 추우면 일단 이불에서 나오기가 싫어..",
          "speaker_id": "2",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008609.1.17",
          "form": "여름은 그래도 집이 더우니까 카페라도 가려고하는데",
          "original_form": "여름은 그래도 집이 더우니까 카페라도 가려고하는데",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008609.1.18",
          "form": "겨울은 집밖을 아얘 안나감",
          "original_form": "겨울은 집밖을 아얘 안나감",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008609.1.19",
          "form": "ㅋㅋㅋㅋ게으름 최고",
          "original_form": "ㅋㅋㅋㅋ게으름 최고",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008609.1.20",
          "form": "내말이ㅋㅋㅋㅋㅋ",
          "original_form": "내말이ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008609.1.21",
          "form": "겨울에 일어나서 찬물로 세수하는것도 너무싫어ㅋㅋㅋㅋ",
          "original_form": "겨울에 일어나서 찬물로 세수하는것도 너무싫어ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008609.1.22",
          "form": "근데 여름은 덥고...",
          "original_form": "근데 여름은 덥고...",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
