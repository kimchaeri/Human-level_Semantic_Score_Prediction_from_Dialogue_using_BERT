      "utterance": [
        {
          "id": "MDRW2100008084.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008084.1.2",
          "form": "네 헉 질문이 네번 ㅋㅋ",
          "original_form": "네 헉 질문이 네번 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008084.1.3",
          "form": "도마뱀 2마리요",
          "original_form": "도마뱀 2마리요",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008084.1.4",
          "form": "뭐 기르시나요?",
          "original_form": "뭐 기르시나요?",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008084.1.5",
          "form": "전 아직",
          "original_form": "전 아직",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008084.1.6",
          "form": "책임이 따라서 부담감이 있어요",
          "original_form": "책임이 따라서 부담감이 있어요",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008084.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008084.1.8",
          "form": "관리가",
          "original_form": "관리가",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008084.1.9",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008084.1.10",
          "form": "목욕에 먹이주기가 젤 힘들어요",
          "original_form": "목욕에 먹이주기가 젤 힘들어요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008084.1.11",
          "form": "안먹으려해서",
          "original_form": "안먹으려해서",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008084.1.12",
          "form": "케어가 힘들어요",
          "original_form": "케어가 힘들어요",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008084.1.13",
          "form": "네 입벌혀서 겨우 먹여요 ㅜㅜ",
          "original_form": "네 입벌혀서 겨우 먹여요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008084.1.14",
          "form": "속상하네요",
          "original_form": "속상하네요",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008084.1.15",
          "form": "잘 안먹죠",
          "original_form": "잘 안먹죠",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008084.1.16",
          "form": "요새 입맛이 없나 크면 덜먹긴 한대요",
          "original_form": "요새 입맛이 없나 크면 덜먹긴 한대요",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008084.1.17",
          "form": "크면 더먹는거 아녜요",
          "original_form": "크면 더먹는거 아녜요",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008084.1.18",
          "form": "아니래요",
          "original_form": "아니래요",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008084.1.19",
          "form": "도마뱀은 어릴때 막주면 다먹어요 ㅋㅋ",
          "original_form": "도마뱀은 어릴때 막주면 다먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008084.1.20",
          "form": "그러다 토하고 ㅜㅜ",
          "original_form": "그러다 토하고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008084.1.21",
          "form": "맞아요 토하면 속상하죠",
          "original_form": "맞아요 토하면 속상하죠",
          "speaker_id": "1",
          "time": "20210822 13:28"
        }
      ]
