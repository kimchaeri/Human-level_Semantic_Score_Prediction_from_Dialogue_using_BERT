      "utterance": [
        {
          "id": "MDRW2100028349.1.1",
          "form": "토욜 오전에 코스트코 가자",
          "original_form": "토욜 오전에 코스트코 가자",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028349.1.2",
          "form": "살 거 있어?",
          "original_form": "살 거 있어?",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028349.1.3",
          "form": "니 생일이잖아",
          "original_form": "니 생일이잖아",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028349.1.4",
          "form": "장 좀 보게",
          "original_form": "장 좀 보게",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028349.1.5",
          "form": "오오올 머 사게?",
          "original_form": "오오올 머 사게?",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028349.1.6",
          "form": "맛있눈고 해줄거야?",
          "original_form": "맛있눈고 해줄거야?",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028349.1.7",
          "form": "전복 사다 미역국 끓여줄게",
          "original_form": "전복 사다 미역국 끓여줄게",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028349.1.8",
          "form": "헐 전복 좋지!",
          "original_form": "헐 전복 좋지!",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028349.1.9",
          "form": "근데 손질 넘 힘들던데ㅠㅜ",
          "original_form": "근데 손질 넘 힘들던데ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028349.1.10",
          "form": "한번해봐서 나을 것 같다",
          "original_form": "한번해봐서 나을 것 같다",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028349.1.11",
          "form": "과연 그럴깤ㅋㅋ",
          "original_form": "과연 그럴깤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028349.1.12",
          "form": "야 니가 해봐 그럼",
          "original_form": "야 니가 해봐 그럼",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028349.1.13",
          "form": "거기 갔다 커피빈들렸다왔음 좋겠다ㅠㅜ",
          "original_form": "거기 갔다 커피빈들렸다왔음 좋겠다ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028349.1.14",
          "form": "니 생일케이크 부산대 앞에서 한다며",
          "original_form": "니 생일케이크 부산대 앞에서 한다며",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028349.1.15",
          "form": "아니 홀케이크 아니라도 조각 케이크라도 먹게",
          "original_form": "아니 홀케이크 아니라도 조각 케이크라도 먹게",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028349.1.16",
          "form": "거기 커피도 맛있음",
          "original_form": "거기 커피도 맛있음",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028349.1.17",
          "form": "스벅도 가야된다",
          "original_form": "스벅도 가야된다",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028349.1.18",
          "form": "스벅 가서 먹어",
          "original_form": "스벅 가서 먹어",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028349.1.19",
          "form": "아 왜 스벅만 가 맨날",
          "original_form": "아 왜 스벅만 가 맨날",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028349.1.20",
          "form": "난 커피빈이 좋은데",
          "original_form": "난 커피빈이 좋은데",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028349.1.21",
          "form": "스벅 바닐라크림콜드블루가 짱이야",
          "original_form": "스벅 바닐라크림콜드블루가 짱이야",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028349.1.22",
          "form": "니 덕에 알았지",
          "original_form": "니 덕에 알았지",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028349.1.23",
          "form": "내가 잘못했네 잘못했어",
          "original_form": "내가 잘못했네 잘못했어",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028349.1.24",
          "form": "지혼자 맛있는 거 많이 먹고 다니더라",
          "original_form": "지혼자 맛있는 거 많이 먹고 다니더라",
          "speaker_id": "2",
          "time": "20210902 20:19"
        }
      ]
