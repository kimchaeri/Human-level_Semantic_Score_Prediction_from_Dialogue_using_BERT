        {
          "id": "MDRW2100015376.1.1",
          "form": "스포츠 자주 보세요?",
          "original_form": "스포츠 자주 보세요?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015376.1.2",
          "form": "네좋아해요!",
          "original_form": "네좋아해요!",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015376.1.3",
          "form": "주로 어떤 종목이요?",
          "original_form": "주로 어떤 종목이요?",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015376.1.4",
          "form": "축구 좋아해요",
          "original_form": "축구 좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015376.1.5",
          "form": "케이리그? 아니면 해외축구 보시나요?",
          "original_form": "케이리그? 아니면 해외축구 보시나요?",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015376.1.6",
          "form": "해축봅니다",
          "original_form": "해축봅니다",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015376.1.7",
          "form": "혹시 epl?",
          "original_form": "혹시 epl?",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015376.1.8",
